{"textgrid.poem.64805": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "3. An Se. K\u00f6nigl. Hoheit Herrn Eduard August Herzog zu York etc.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Prinz!", "tokens": ["Prinz", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Hier, wo stiller Flei\u00df, von Hof und Welt entfernt,", "tokens": ["Hier", ",", "wo", "stil\u00b7ler", "Flei\u00df", ",", "von", "Hof", "und", "Welt", "ent\u00b7fernt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ADJA", "NN", "$,", "APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Aus Ruf und B\u00fcchern nur die Gro\u00dfen kennen lernt,", "tokens": ["Aus", "Ruf", "und", "B\u00fc\u00b7chern", "nur", "die", "Gro\u00b7\u00dfen", "ken\u00b7nen", "lernt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch Witz und Einsicht sch\u00e4rft, bald ihrem Dienst zu leben,", "tokens": ["Doch", "Witz", "und", "Ein\u00b7sicht", "sch\u00e4rft", ",", "bald", "ih\u00b7rem", "Dienst", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und einer sp\u00e4tern Zeit ihr wahres Bild zu geben:", "tokens": ["Und", "ei\u00b7ner", "sp\u00e4\u00b7tern", "Zeit", "ihr", "wah\u00b7res", "Bild", "zu", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Hat froher als der Tag, den uns ", "tokens": ["Hat", "fro\u00b7her", "als", "der", "Tag", ",", "den", "uns"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon siebzehn lange Jahr' kein andrer sonst gelacht,", "tokens": ["Schon", "sieb\u00b7zehn", "lan\u00b7ge", "Jahr'", "kein", "an\u00b7drer", "sonst", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJA", "NN", "PIAT", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Als jener gro\u00dfe Tag! da gingen Vaterblicke", "tokens": ["Als", "je\u00b7ner", "gro\u00b7\u00dfe", "Tag", "!", "da", "gin\u00b7gen", "Va\u00b7ter\u00b7bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "$.", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wie einst der Sch\u00f6pfer sich zur Welt herabgeneigt,", "tokens": ["Wie", "einst", "der", "Sch\u00f6p\u00b7fer", "sich", "zur", "Welt", "her\u00b7ab\u00b7ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gebilligt was er sah, und Menschen Huld erzeigt.", "tokens": ["Ge\u00b7bil\u00b7ligt", "was", "er", "sah", ",", "und", "Men\u00b7schen", "Huld", "er\u00b7zeigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PWS", "PPER", "VVFIN", "$,", "KON", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch auf den goldnen Tag folgt' eine Zeit von Eisen,", "tokens": ["Doch", "auf", "den", "gold\u00b7nen", "Tag", "folgt'", "ei\u00b7ne", "Zeit", "von", "Ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Feinde mu\u00dfte man hier als Besch\u00fctzer preisen:", "tokens": ["Und", "Fein\u00b7de", "mu\u00df\u00b7te", "man", "hier", "als", "Be\u00b7sch\u00fct\u00b7zer", "prei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "PIS", "ADV", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo Pflicht zur Freude wird, Gehorsam da zu weyhn,", "tokens": ["Wo", "Pflicht", "zur", "Freu\u00b7de", "wird", ",", "Ge\u00b7hor\u00b7sam", "da", "zu", "weyhn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "VAFIN", "$,", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So gl\u00fccklich l\u00e4ssest Du uns jetzt, o Herzog, seyn,", "tokens": ["So", "gl\u00fcck\u00b7lich", "l\u00e4s\u00b7sest", "Du", "uns", "jetzt", ",", "o", "Her\u00b7zog", ",", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PRF", "ADV", "$,", "FM", "NE", "$,", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und Deiner Gnade Werth ein fr\u00f6hlich Land erkennen,", "tokens": ["Und", "Dei\u00b7ner", "Gna\u00b7de", "Werth", "ein", "fr\u00f6h\u00b7lich", "Land", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "ART", "ADJD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Stolz, wie Britannien, sich auch Dein Land zu nennen.", "tokens": ["Stolz", ",", "wie", "Bri\u00b7tan\u00b7ni\u00b7en", ",", "sich", "auch", "Dein", "Land", "zu", "nen\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "NE", "$,", "PRF", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Verzeih' ihm diesen Stolz! w\u00e4r Welschland noch so sch\u00f6n,", "tokens": ["Ver\u00b7zeih'", "ihm", "die\u00b7sen", "Stolz", "!", "w\u00e4r", "Wel\u00b7schland", "noch", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "NN", "$.", "VAFIN", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es lie\u00df Dich gr\u00f6\u00dfre Pracht, doch nicht Gehorsam sehn:", "tokens": ["Es", "lie\u00df", "Dich", "gr\u00f6\u00df\u00b7re", "Pracht", ",", "doch", "nicht", "Ge\u00b7hor\u00b7sam", "sehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,", "ADV", "PTKNEG", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da mu\u00dft' als Kenner Dich manch ewig Werk entz\u00fccken;", "tokens": ["Da", "mu\u00dft'", "als", "Ken\u00b7ner", "Dich", "manch", "e\u00b7wig", "Werk", "ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KOUS", "NN", "PPER", "PIAT", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verfallner Gr\u00f6\u00dfe Rest zeigt es des F\u00fcrsten Blicken:", "tokens": ["Ver\u00b7fall\u00b7ner", "Gr\u00f6\u00b7\u00dfe", "Rest", "zeigt", "es", "des", "F\u00fcrs\u00b7ten", "Bli\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Europens Barbarey ward erst aus ihm gelehrt,", "tokens": ["Eu\u00b7ro\u00b7pens", "Bar\u00b7ba\u00b7rey", "ward", "erst", "aus", "ihm", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Eh' ein belebter Stahl in fernes Meer geleitet,", "tokens": ["Eh'", "ein", "be\u00b7leb\u00b7ter", "Stahl", "in", "fer\u00b7nes", "Meer", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat es durch unsre Welt des Ostens Gluth verbreitet;", "tokens": ["Hat", "es", "durch", "uns\u00b7re", "Welt", "des", "Os\u00b7tens", "Gluth", "ver\u00b7brei\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun ist sein Handel das, was die Galeere bleibt,", "tokens": ["Nun", "ist", "sein", "Han\u00b7del", "das", ",", "was", "die", "Ga\u00b7lee\u00b7re", "bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die matter Sclaven Arm an nahen Ufern treibt:", "tokens": ["Die", "mat\u00b7ter", "Scla\u00b7ven", "Arm", "an", "na\u00b7hen", "U\u00b7fern", "treibt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn, wo das Wallro\u00df geht, wo Fische fliegend zagen,", "tokens": ["Wenn", ",", "wo", "das", "Wall\u00b7ro\u00df", "geht", ",", "wo", "Fi\u00b7sche", "flie\u00b7gend", "za\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "VVFIN", "$,", "PWAV", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Segel Albions Neptun und Aeol tragen,", "tokens": ["Die", "Se\u00b7gel", "Al\u00b7bions", "Nep\u00b7tun", "und", "A\u00b7e\u00b7ol", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Und von dem Ganges an, den Ph\u00f6bus fr\u00fch begr\u00fc\u00dft,", "tokens": ["Und", "von", "dem", "Gan\u00b7ges", "an", ",", "den", "Ph\u00f6\u00b7bus", "fr\u00fch", "be\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKVZ", "$,", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Bis wo in ewig Eis sich Quebecs Strom ergie\u00dft,", "tokens": ["Bis", "wo", "in", "e\u00b7wig", "Eis", "sich", "Que\u00b7becs", "Strom", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "APPR", "ADJD", "NN", "PRF", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gef\u00fcrchtet und verehrt vom Mogel zum Huronen,", "tokens": ["Ge\u00b7f\u00fcrch\u00b7tet", "und", "ver\u00b7ehrt", "vom", "Mo\u00b7gel", "zum", "Hu\u00b7ro\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und auf dem Ocean die reichen Briten wohnen.", "tokens": ["Und", "auf", "dem", "O\u00b7cean", "die", "rei\u00b7chen", "Bri\u00b7ten", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Herr, weil Dein Muth und Geist f\u00fcr ihre Schifffahrt wacht,", "tokens": ["Herr", ",", "weil", "Dein", "Muth", "und", "Geist", "f\u00fcr", "ih\