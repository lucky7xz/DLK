{"dta.poem.12052": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was in der Schule du gelernt, ists wol vergebens,", "tokens": ["Was", "in", "der", "Schu\u00b7le", "du", "ge\u00b7lernt", ",", "ists", "wol", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVPP", "$,", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil du gebrauchen es nicht kannst im Lauf des Lebens?", "tokens": ["Weil", "du", "ge\u00b7brau\u00b7chen", "es", "nicht", "kannst", "im", "Lauf", "des", "Le\u00b7bens", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "PPER", "PTKNEG", "VMFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O nein, den Acker hat zum Anbau es entwildet,", "tokens": ["O", "nein", ",", "den", "A\u00b7cker", "hat", "zum", "An\u00b7bau", "es", "ent\u00b7wil\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$,", "ART", "NN", "VAFIN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zum Wesentlichen hats dich f\u00f6rmlich vorgebildet.", "tokens": ["Zum", "We\u00b7sent\u00b7li\u00b7chen", "hats", "dich", "f\u00f6rm\u00b7lich", "vor\u00b7ge\u00b7bil\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So was im Leben selbst, der gro\u00dfen Schule, du", "tokens": ["So", "was", "im", "Le\u00b7ben", "selbst", ",", "der", "gro\u00b7\u00dfen", "Schu\u00b7le", ",", "du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PRELS", "APPRART", "NN", "ADV", "$,", "ART", "ADJA", "NN", "$,", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gelernt hast, bringst du nicht umsonst dem Himmel zu.", "tokens": ["Ge\u00b7lernt", "hast", ",", "bringst", "du", "nicht", "um\u00b7sonst", "dem", "Him\u00b7mel", "zu", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Du mu\u00dft die irdischen Aufgaben recht nur treiben,", "tokens": ["Du", "mu\u00dft", "die", "ir\u00b7di\u00b7schen", "Auf\u00b7ga\u00b7ben", "recht", "nur", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ewig wird davon die Segenswirkung bleiben.", "tokens": ["Und", "e\u00b7wig", "wird", "da\u00b7von", "die", "Se\u00b7gens\u00b7wir\u00b7kung", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}