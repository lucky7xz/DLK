{"dta.poem.21705": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Sunium.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein, o Sunium, denk' ich, und deiner romanti-\nschen Fluren.", "tokens": ["Dein", ",", "o", "Su\u00b7ni\u00b7um", ",", "denk'", "ich", ",", "und", "dei\u00b7ner", "ro\u00b7man\u00b7ti", "schen", "Flu\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "$,", "FM", "FM", "$,", "VVFIN", "PPER", "$,", "KON", "PPOSAT", "TRUNC", "ADJA", "NN", "$."], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Manchen lebendigern Tag hast du den J\u00fcng-", "tokens": ["Man\u00b7chen", "le\u00b7ben\u00b7di\u00b7gern", "Tag", "hast", "du", "den", "J\u00fcng"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Mancher Abend verfloss ihm in deinen gastlichen", "tokens": ["Man\u00b7cher", "A\u00b7bend", "ver\u00b7floss", "ihm", "in", "dei\u00b7nen", "gast\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Hallen,", "tokens": ["Hal\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Unter ernstem Gespr\u00e4ch, unter vertraulichem", "tokens": ["Un\u00b7ter", "erns\u00b7tem", "Ge\u00b7spr\u00e4ch", ",", "un\u00b7ter", "ver\u00b7trau\u00b7li\u00b7chem"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "--+---+--+--", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Scherz.", "tokens": ["Scherz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Deine Fluren sind sch\u00f6n, o Sunium. Deine Ge-", "tokens": ["Dei\u00b7ne", "Flu\u00b7ren", "sind", "sch\u00f6n", ",", "o", "Su\u00b7ni\u00b7um", ".", "Dei\u00b7ne", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "FM", "FM", "$.", "PPOSAT", "TRUNC"], "meter": "--+--+--+-+--", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Schatten so k\u00fchlend, so frisch duftet der Kal-", "tokens": ["Schat\u00b7ten", "so", "k\u00fch\u00b7lend", ",", "so", "frisch", "duf\u00b7tet", "der", "Ka\u00b7l"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "ART", "TRUNC"], "meter": "+--+-+++-+-+", "measure": "iambic.septa.invert"}, "line.9": {"text": "Horch, wie die Nachtigall schl\u00e4gt in der bl\u00fcthen-", "tokens": ["Horch", ",", "wie", "die", "Nach\u00b7ti\u00b7gall", "schl\u00e4gt", "in", "der", "bl\u00fcthen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Schau wie die g\u00fcldene Saat wogt das Gel\u00e4nde hinan.", "tokens": ["Schau", "wie", "die", "g\u00fcl\u00b7de\u00b7ne", "Saat", "wogt", "das", "Ge\u00b7l\u00e4n\u00b7de", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+-+-+-+-", "measure": "elegiambus"}, "line.11": {"text": "Gellend erschallt aus dem goldenen Bette die Fl\u00f6te", "tokens": ["Gel\u00b7lend", "er\u00b7schallt", "aus", "dem", "gol\u00b7de\u00b7nen", "Bet\u00b7te", "die", "Fl\u00f6\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.12": {"text": "der Wachtel,", "tokens": ["der", "Wach\u00b7tel", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Dumpfer des Rohrspatz Ruf aus dem Ger\u00f6h-", "tokens": ["Dum\u00b7pfer", "des", "Rohrs\u00b7patz", "Ruf", "aus", "dem", "Ge\u00b7r\u00f6h"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dein o Sunium denk' ich und deines verg\u00f6tterten", "tokens": ["Dein", "o", "Su\u00b7ni\u00b7um", "denk'", "ich", "und", "dei\u00b7nes", "ver\u00b7g\u00f6t\u00b7ter\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "FM", "FM", "VVFIN", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Weisen,", "tokens": ["Wei\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Welcher entschleyerten Blicks jetzund die Wahr-", "tokens": ["Wel\u00b7cher", "ent\u00b7schley\u00b7er\u00b7ten", "Blicks", "je\u00b7tzund", "die", "Wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "ADJA", "NN", "ADV", "ART", "TRUNC"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mancher gesellige Abend, und manche der n\u00e4chtli-", "tokens": ["Man\u00b7cher", "ge\u00b7sel\u00b7li\u00b7ge", "A\u00b7bend", ",", "und", "man\u00b7che", "der", "n\u00e4cht\u00b7li"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "KON", "PIS", "ART", "TRUNC"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.5": {"text": "Hold ist dem Denken die Nacht, hab ich dem", "tokens": ["Hold", "ist", "dem", "Den\u00b7ken", "die", "Nacht", ",", "hab", "ich", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VAFIN", "ART", "NN", "ART", "NN", "$,", "VAFIN", "PPER", "ART"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Denker gelauscht,", "tokens": ["Den\u00b7ker", "ge\u00b7lauscht", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "\u00dcber das Leiden und Thun, und \u00fcber Verh\u00e4ngniss", "tokens": ["\u00dc\u00b7ber", "das", "Lei\u00b7den", "und", "Thun", ",", "und", "\u00fc\u00b7ber", "Ver\u00b7h\u00e4ng\u00b7niss"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$,", "KON", "APPR", "NN"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.8": {"text": "und Freyheit,", "tokens": ["und", "Frey\u00b7heit", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.9": {"text": "\u00dcber des Endlichen Kampf mit dem unendli-", "tokens": ["\u00dc\u00b7ber", "des", "End\u00b7li\u00b7chen", "Kampf", "mit", "dem", "un\u00b7end\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "APPR", "ART", "TRUNC"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Aber dem Denker voran flog immer die Ahnung des", "tokens": ["A\u00b7ber", "dem", "Den\u00b7ker", "vo\u00b7ran", "flog", "im\u00b7mer", "die", "Ah\u00b7nung", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKVZ", "VVFIN", "ADV", "ART", "NN", "ART"], "meter": "+--+--+--+-+--", "measure": "dactylic.tri.plus"}, "line.11": {"text": "Dichters.", "tokens": ["Dich\u00b7ters", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Z\u00fcrnend dem z\u00f6gernden Gang, sprengt' ich der", "tokens": ["Z\u00fcr\u00b7nend", "dem", "z\u00f6\u00b7gern\u00b7den", "Gang", ",", "sprengt'", "ich", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Schl\u00fcsse Gespinnst,", "tokens": ["Schl\u00fcs\u00b7se", "Ge\u00b7spinnst", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.14": {"text": "St\u00fcrmte hinaus in die Nacht, in die heilige. \u00dcber", "tokens": ["St\u00fcrm\u00b7te", "hin\u00b7aus", "in", "die", "Nacht", ",", "in", "die", "hei\u00b7li\u00b7ge", ".", "\u00dc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NN", "APZR", "APPR", "ART", "NN", "$,", "APPR", "ART", "ADJA", "$.", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.15": {"text": "der Scheitel", "tokens": ["der", "Schei\u00b7tel"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Rolleten Leyer und Schwan, blitzten Arktur", "tokens": ["Rol\u00b7le\u00b7ten", "Le\u00b7yer", "und", "Schwan", ",", "blitz\u00b7ten", "Ark\u00b7tur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "ADJA", "NN"], "meter": "+--++-++--+", "measure": "iambic.hexa.chol"}, "line.17": {"text": "und Centaur,", "tokens": ["und", "Cen\u00b7taur", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Jeglicher funkelnde Stern und jegliche rollende Sonne,", "tokens": ["Jeg\u00b7li\u00b7cher", "fun\u00b7keln\u00b7de", "Stern", "und", "jeg\u00b7li\u00b7che", "rol\u00b7len\u00b7de", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.19": {"text": "Jegliches Rauschen im Busch, jedes Gefl\u00fcster", "tokens": ["Jeg\u00b7li\u00b7ches", "Rau\u00b7schen", "im", "Busch", ",", "je\u00b7des", "Ge\u00b7fl\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "NN", "APPRART", "NN", "$,", "PIAT", "NN"], "meter": "+--+--+---+-", "measure": "dactylic.tri.plus"}, "line.20": {"text": "des Schilfs,", "tokens": ["des", "Schilfs", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.21": {"text": "Jegliches Echo der Nacht, die Stimm' aus andern", "tokens": ["Jeg\u00b7li\u00b7ches", "E\u00b7cho", "der", "Nacht", ",", "die", "Stimm'", "aus", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "ADJA"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.22": {"text": "Welten,", "tokens": ["Wel\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.23": {"text": "Haucht in dem lechzenden Geist Ahnung des", "tokens": ["Haucht", "in", "dem", "lech\u00b7zen\u00b7den", "Geist", "Ah\u00b7nung", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "NN", "ART"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.24": {"text": "Bessern empor.", "tokens": ["Bes\u00b7sern", "em\u00b7por", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "PTKVZ", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}