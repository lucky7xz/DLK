{"dta.poem.17709": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "331.  Was GOtt nicht thut/ gef\u00e4llt ihm  \n nicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott mu\u00df der Anfang seyn/ das Mittel und das Ende/", "tokens": ["Gott", "mu\u00df", "der", "An\u00b7fang", "seyn", "/", "das", "Mit\u00b7tel", "und", "das", "En\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VAINF", "$(", "ART", "NN", "KON", "ART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Wo Jhm gefallen solln die Werke deiner H\u00e4nde.", "tokens": ["Wo", "Jhm", "ge\u00b7fal\u00b7len", "solln", "die", "Wer\u00b7ke", "dei\u00b7ner", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVINF", "VMFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}