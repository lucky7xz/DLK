 "line.3": {"text": "Da\u00df sanft im Regen und Sonnenschein", "tokens": ["Da\u00df", "sanft", "im", "Re\u00b7gen", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zur Frucht das Kn\u00f6splein werde.", "tokens": ["Zur", "Frucht", "das", "Kn\u00f6s\u00b7plein", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Die Fr\u00fcchte lachen, es nagt der Wurm,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "la\u00b7chen", ",", "es", "nagt", "der", "Wurm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo die Fr\u00fccht' am Sch\u00f6nsten lachen,", "tokens": ["Wo", "die", "Fr\u00fccht'", "am", "Sch\u00f6ns\u00b7ten", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, voller Baum, Dich peitscht der Sturm,", "tokens": ["Und", ",", "vol\u00b7ler", "Baum", ",", "Dich", "peitscht", "der", "Sturm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum nackten Streif zu machen.", "tokens": ["Zum", "nack\u00b7ten", "Streif", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF