{"textgrid.poem.67708": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Herbstlied", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Winter kommt, der Wind ist kalt,", "tokens": ["Der", "Win\u00b7ter", "kommt", ",", "der", "Wind", "ist", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Laub beginnt zu fallen;", "tokens": ["Das", "Laub", "be\u00b7ginnt", "zu", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach, wie's Dir gehet, liebes Laub,", "tokens": ["Ach", ",", "wie's", "Dir", "ge\u00b7het", ",", "lie\u00b7bes", "Laub", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df es gehn uns Allen.", "tokens": ["So", "mu\u00df", "es", "gehn", "uns", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sind geflochten, roll'n umher,", "tokens": ["Wir", "sind", "ge\u00b7floch\u00b7ten", ",", "roll'n", "um\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umher im Rad der Zeiten,", "tokens": ["Um\u00b7her", "im", "Rad", "der", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wie sie rollen Jahr ins Jahr,", "tokens": ["Und", "wie", "sie", "rol\u00b7len", "Jahr", "ins", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So geht's zu Ewigkeiten.", "tokens": ["So", "geht's", "zu", "E\u00b7wig\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich stand einst jung, ich schwebt' umher", "tokens": ["Ich", "stand", "einst", "jung", ",", "ich", "schwebt'", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Hauch der Fr\u00fchlingsweste;", "tokens": ["Im", "Hauch", "der", "Fr\u00fch\u00b7lings\u00b7wes\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es spr\u00fchte frisch, es trieb der Saft,", "tokens": ["Es", "spr\u00fch\u00b7te", "frisch", ",", "es", "trieb", "der", "Saft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So ward das B\u00e4umlein feste.", "tokens": ["So", "ward", "das", "B\u00e4um\u00b7lein", "fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die Bl\u00fcthen weben die Bl\u00e4tter herab,", "tokens": ["Die", "Bl\u00fc\u00b7then", "we\u00b7ben", "die", "Bl\u00e4t\u00b7ter", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie spreiten wei\u00df die Erde,", "tokens": ["Sie", "sprei\u00b7ten", "wei\u00df", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df sanft im Regen und Sonnenschein", "tokens": ["Da\u00df", "sanft", "im", "Re\u00b7gen", "und", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "KON", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zur Frucht das Kn\u00f6splein werde.", "tokens": ["Zur", "Frucht", "das", "Kn\u00f6s\u00b7plein", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Fr\u00fcchte lachen, es nagt der Wurm,", "tokens": ["Die", "Fr\u00fcch\u00b7te", "la\u00b7chen", ",", "es", "nagt", "der", "Wurm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wo die Fr\u00fccht' am Sch\u00f6nsten lachen,", "tokens": ["Wo", "die", "Fr\u00fccht'", "am", "Sch\u00f6ns\u00b7ten", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, voller Baum, Dich peitscht der Sturm,", "tokens": ["Und", ",", "vol\u00b7ler", "Baum", ",", "Dich", "peitscht", "der", "Sturm", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum nackten Streif zu machen.", "tokens": ["Zum", "nack\u00b7ten", "Streif", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie zeucht uns an, sie zeucht uns aus,", "tokens": ["Sie", "zeucht", "uns", "an", ",", "sie", "zeucht", "uns", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt nackt uns nieder zur Bahre,", "tokens": ["Legt", "nackt", "uns", "nie\u00b7der", "zur", "Bah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "O grause Mutter, Mutter Zeit!", "tokens": ["O", "grau\u00b7se", "Mut\u00b7ter", ",", "Mut\u00b7ter", "Zeit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00e4rbt und falbt die Haare.", "tokens": ["Und", "f\u00e4rbt", "und", "falbt", "die", "Haa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wirf ab! die Bl\u00fcthe dauert nicht,", "tokens": ["Wirf", "ab", "!", "die", "Bl\u00fc\u00b7the", "dau\u00b7ert", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df reif die Knospe werde.", "tokens": ["Da\u00df", "reif", "die", "Knos\u00b7pe", "wer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wirf ab! die Bl\u00e4tter falben schon", "tokens": ["Wirf", "ab", "!", "die", "Bl\u00e4t\u00b7ter", "fal\u00b7ben", "schon"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wallen nieder zur Erde.", "tokens": ["Und", "wal\u00b7len", "nie\u00b7der", "zur", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Da rauscht's von Leichen: \u00bbBrich, o Nord,", "tokens": ["Da", "rauscht's", "von", "Lei\u00b7chen", ":", "\u00bb", "Brich", ",", "o", "Nord", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$.", "$(", "NE", "$,", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das D\u00fcrre auch danieder!", "tokens": ["Das", "D\u00fcr\u00b7re", "auch", "da\u00b7nie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Rauscht, Bl\u00e4tter! d\u00fcrren Aeste, flammt!", "tokens": ["Rauscht", ",", "Bl\u00e4t\u00b7ter", "!", "d\u00fcr\u00b7ren", "A\u00b7es\u00b7te", ",", "flammt", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Es sind nicht meine Glieder.\u00ab", "tokens": ["Es", "sind", "nicht", "mei\u00b7ne", "Glie\u00b7der", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ha, neuer, neuer Fr\u00fchlingswind,", "tokens": ["Ha", ",", "neu\u00b7er", ",", "neu\u00b7er", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wann wirst, wann wirst Du wehen,", "tokens": ["Wann", "wirst", ",", "wann", "wirst", "Du", "we\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "$,", "PWAV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da Laub und Bl\u00fcth' und Frucht ersteht", "tokens": ["Da", "Laub", "und", "Bl\u00fcth'", "und", "Frucht", "er\u00b7steht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nimmer wird vergehen?", "tokens": ["Und", "nim\u00b7mer", "wird", "ver\u00b7ge\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ha, neuer, neuer Fr\u00fchlingswind,", "tokens": ["Ha", ",", "neu\u00b7er", ",", "neu\u00b7er", "Fr\u00fch\u00b7lings\u00b7wind", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du w\u00e4rmst mein Mark verborgen;", "tokens": ["Du", "w\u00e4rmst", "mein", "Mark", "ver\u00b7bor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch in der Wurzel lebt mein Saft,", "tokens": ["Noch", "in", "der", "Wur\u00b7zel", "lebt", "mein", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und frisch ersteh' ich morgen.", "tokens": ["Und", "frisch", "er\u00b7steh'", "ich", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der Winter kommt, der Wind ist kalt,", "tokens": ["Der", "Win\u00b7ter", "kommt", ",", "der", "Wind", "ist", "kalt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Laub beginnt zu fallen;", "tokens": ["Das", "Laub", "be\u00b7ginnt", "zu", "fal\u00b7len", ";"], "token_info": ["word", "word", "word", 