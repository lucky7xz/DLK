{"textgrid.poem.40566": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: In des Waldes Kathedrale", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Waldes Kathedrale", "tokens": ["In", "des", "Wal\u00b7des", "Ka\u00b7thed\u00b7ra\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rauscht das Laub als Sonntagsglocken,", "tokens": ["Rauscht", "das", "Laub", "als", "Sonn\u00b7tags\u00b7glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gl\u00fchn als goldne Ampelstrahle", "tokens": ["Gl\u00fchn", "als", "gold\u00b7ne", "Am\u00b7pel\u00b7strah\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hell der Sonne Lichterflocken.", "tokens": ["Hell", "der", "Son\u00b7ne", "Lich\u00b7ter\u00b7flo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und die gl\u00e4ub'gen V\u00f6glein wallen,", "tokens": ["Und", "die", "gl\u00e4ub'\u00b7gen", "V\u00f6\u00b7glein", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sonntaglich an Leib und Feder,", "tokens": ["Sonn\u00b7tag\u00b7lich", "an", "Leib", "und", "Fe\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu des Buchbaums gr\u00fcnen Hallen,", "tokens": ["Zu", "des", "Buch\u00b7baums", "gr\u00fc\u00b7nen", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo ein Ast ragt als Katheder.", "tokens": ["Wo", "ein", "Ast", "ragt", "als", "Ka\u00b7the\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "KOUS", "NN", "$."], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Dompfaff Gimpel predigt dorten,", "tokens": ["Dom\u00b7pfaff", "Gim\u00b7pel", "pre\u00b7digt", "dor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die Frau'n und Herrn begeistert,", "tokens": ["Der", "die", "Frau'n", "und", "Herrn", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er klug mit Salbungsworten", "tokens": ["Weil", "er", "klug", "mit", "Sal\u00b7bungs\u00b7wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene r\u00fchrt und diese meistert.", "tokens": ["Je\u00b7ne", "r\u00fchrt", "und", "die\u00b7se", "meis\u00b7tert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KON", "PDS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "L\u00e4\u00dft nicht gut von schwarzem Sammet", "tokens": ["L\u00e4\u00dft", "nicht", "gut", "von", "schwar\u00b7zem", "Sam\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm das Soli-deo-k\u00e4ppchen?", "tokens": ["Ihm", "das", "So\u00b7li\u00b7deo\u00b7k\u00e4pp\u00b7chen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Roth die Domherrnweste flammet,", "tokens": ["Roth", "die", "Dom\u00b7herrn\u00b7wes\u00b7te", "flam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zierlich f\u00e4llt das schwarze Schleppchen.", "tokens": ["Zier\u00b7lich", "f\u00e4llt", "das", "schwar\u00b7ze", "Schlepp\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seine engbestrumpften Beine", "tokens": ["Sei\u00b7ne", "eng\u00b7bes\u00b7trumpf\u00b7ten", "Bei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wei\u00df er anstandsvoll zu stellen,", "tokens": ["Wei\u00df", "er", "an\u00b7stands\u00b7voll", "zu", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem Asketeneifer feine", "tokens": ["Dem", "As\u00b7ke\u00b7ten\u00b7ei\u00b7fer", "fei\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Weltmanieren zu gesellen.", "tokens": ["Welt\u00b7ma\u00b7nie\u00b7ren", "zu", "ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbo ihr S\u00fcnder, unbu\u00dffertig,", "tokens": ["\u00bb", "o", "ihr", "S\u00fcn\u00b7der", ",", "un\u00b7bu\u00df\u00b7fer\u00b7tig", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "FM", "PPOSAT", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wandelnd auf des Irrfals Wegen,", "tokens": ["Wan\u00b7delnd", "auf", "des", "Irr\u00b7fals", "We\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid des G\u00f6tterzorns gew\u00e4rtig,", "tokens": ["Seid", "des", "G\u00f6t\u00b7ter\u00b7zorns", "ge\u00b7w\u00e4r\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der euch allw\u00e4rts droht entgegen.", "tokens": ["Der", "euch", "all\u00b7w\u00e4rts", "droht", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Meidet die Gewohnheits\u00fcnden", "tokens": ["Mei\u00b7det", "die", "Ge\u00b7wohn\u00b7hei\u00b7ts\u00fcn\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kirschen, Hanfkorn, Weizen\u00e4hren,", "tokens": ["Kir\u00b7schen", ",", "Hanf\u00b7korn", ",", "Wei\u00b7ze\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft euch nicht von Lust entz\u00fcnden", "tokens": ["La\u00dft", "euch", "nicht", "von", "Lust", "ent\u00b7z\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu Wachholders schn\u00f6den Beeren!", "tokens": ["Zu", "Wach\u00b7hol\u00b7ders", "schn\u00f6\u00b7den", "Bee\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.8": {"line.1": {"text": "Denn Leimruthen, Netze, Kloben", "tokens": ["Denn", "Leim\u00b7ru\u00b7then", ",", "Net\u00b7ze", ",", "Klo\u00b7ben"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "NN", "$,", "NN", "$,", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Drohn euch dort als Fegefeuer,", "tokens": ["Drohn", "euch", "dort", "als", "Fe\u00b7ge\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Drin in Qual ihr werdet toben,", "tokens": ["Drin", "in", "Qual", "ihr", "wer\u00b7det", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "PPER", "VAFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und aus dem Befreiung theuer.", "tokens": ["Und", "aus", "dem", "Be\u00b7frei\u00b7ung", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJD", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.9": {"line.1": {"text": "Wehe! Den verstockten B\u00f6sen", "tokens": ["We\u00b7he", "!", "Den", "ver\u00b7stock\u00b7ten", "B\u00f6\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$.", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00e4hnt die H\u00f6lle Vogelbauer,", "tokens": ["G\u00e4hnt", "die", "H\u00f6l\u00b7le", "Vo\u00b7gel\u00b7bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Daraus nimmer ein Erl\u00f6sen,", "tokens": ["Da\u00b7raus", "nim\u00b7mer", "ein", "Er\u00b7l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Drin der Pips und ew'ge Trauer!", "tokens": ["Drin", "der", "Pips", "und", "ew'\u00b7ge", "Trau\u00b7er", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun geht heim und unbeth\u00f6ret", "tokens": ["Nun", "geht", "heim", "und", "un\u00b7be\u00b7th\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Weiter am Wachholderhage;", "tokens": ["Wei\u00b7ter", "am", "Wach\u00b7hol\u00b7der\u00b7ha\u00b7ge", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Denkt der Predigt, bis ihr h\u00f6ret", "tokens": ["Denkt", "der", "Pre\u00b7digt", ",", "bis", "ihr", "h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deren Ende heut acht Tage.\u00ab", "tokens": ["De\u00b7ren", "En\u00b7de", "heut", "acht", "Ta\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "NN", "ADV", "CARD", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Doch am n\u00e4chsten Festesmorgen", "tokens": ["Doch", "am", "n\u00e4chs\u00b7ten", "Fes\u00b7tes\u00b7mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unbesetzt ragt der Katheder;", "tokens": ["Un\u00b7be\u00b7setzt", "ragt", "der", "Ka\u00b7the\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wo der Pred'ger sich verborgen,", "tokens": ["Wo", "der", "Pre\u00b7d'\u00b7ger", "sich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sucht mit Angst und Neugier Jeder.", "tokens": ["Sucht", "mit", "Angst", "und", "Neu\u00b7gier", "Je\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Am Wachholder d\u00fcstre Reste!", "tokens": ["Am", "Wach\u00b7hol\u00b7der", "d\u00fcst\u00b7re", "Res\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "An de