{"dta.poem.745": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxXI.  An  Furium.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dv falscher b\u00f6ser Mensch/ au\u00df dessen krummen Rachen ", "tokens": ["Dv", "fal\u00b7scher", "b\u00f6\u00b7ser", "Mensch", "/", "au\u00df", "des\u00b7sen", "krum\u00b7men", "Ra\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "ADJA", "NN", "$(", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die gr\u00fcne Natter pfeifft/ aus dessen schlimmen Mund", "tokens": ["Die", "gr\u00fc\u00b7ne", "Nat\u00b7ter", "pfeifft", "/", "aus", "des\u00b7sen", "schlim\u00b7men", "Mund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$(", "APPR", "PRELAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die schwartzen Schlangen sehn/ du mehr de&#241; tober Hund. ", "tokens": ["Die", "schwart\u00b7zen", "Schlan\u00b7gen", "sehn", "/", "du", "mehr", "de", "&#241;", "to\u00b7ber", "Hund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "XML_entity", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$(", "PPER", "ADV", "NE", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du gantz verschalckter Fuchs! du Hau\u00df der grimmen Dra- ", "tokens": ["Du", "gantz", "ver\u00b7schalck\u00b7ter", "Fuchs", "!", "du", "Hau\u00df", "der", "grim\u00b7men", "Dra"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADJA", "NE", "$.", "PPER", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "chen.", "tokens": ["chen", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Wil dir denn nicht einmahl der Leib vor gifft zerkrachen?", "tokens": ["Wil", "dir", "denn", "nicht", "ein\u00b7mahl", "der", "Leib", "vor", "gifft", "zer\u00b7kra\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist ja nichts an dir (wie jung vnd alten kundt) ", "tokens": ["Es", "ist", "ja", "nichts", "an", "dir", "(", "wie", "jung", "vnd", "al\u00b7ten", "kundt", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIS", "APPR", "PPER", "$(", "PWAV", "ADJD", "KON", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von deinem Sch\u00e4del ab bi\u00df auff den fus gesundt.", "tokens": ["Von", "dei\u00b7nem", "Sch\u00e4\u00b7del", "ab", "bi\u00df", "auff", "den", "fus", "ge\u00b7sundt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du bist so teufel schwartz als du dich weis kanst machen.", "tokens": ["Du", "bist", "so", "teu\u00b7fel", "schwartz", "als", "du", "dich", "weis", "kanst", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "ADJD", "KOKOM", "PPER", "PRF", "PTKVZ", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dein Aug ist flam\u2019 vnd pest/ die Zung ein schneidend", "tokens": ["Dein", "Aug", "ist", "flam'", "vnd", "pest", "/", "die", "Zung", "ein", "schnei\u00b7dend"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "VVFIN", "$(", "ART", "NN", "ART", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schwerdt.", "tokens": ["Schwerdt", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Du leichter ehren dieb/ bist raad vnd fewers wehrt.", "tokens": ["Du", "leich\u00b7ter", "eh\u00b7ren", "dieb", "/", "bist", "raad", "vnd", "fe\u00b7wers", "wehrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVFIN", "$(", "VAFIN", "ADJD", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie da\u00df die rache dein/ O schaum der Laster schonet?", "tokens": ["Wie", "da\u00df", "die", "ra\u00b7che", "dein", "/", "O", "schaum", "der", "Las\u00b7ter", "scho\u00b7net", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "ART", "NN", "PPOSAT", "$(", "NE", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie straft dich durch dich selbst/ du kanst nicht mehr ent-", "tokens": ["Sie", "straft", "dich", "durch", "dich", "selbst", "/", "du", "kanst", "nicht", "mehr", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "ADV", "$(", "PPER", "VMFIN", "PTKNEG", "ADV", "TRUNC"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "gehn/", "tokens": ["gehn", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+", "measure": "single.up"}}, "stanza.5": {"line.1": {"text": "Weil jeder/ der dich sieht mit schrecken mu\u00df gestehn", "tokens": ["Weil", "je\u00b7der", "/", "der", "dich", "sieht", "mit", "schre\u00b7cken", "mu\u00df", "ge\u00b7stehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "$(", "PRELS", "PPER", "VVFIN", "APPR", "VVINF", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wesentlich in dir die gantze H\u00f6lle wohnet. ", "tokens": ["Das", "we\u00b7sent\u00b7lich", "in", "dir", "die", "gant\u00b7ze", "H\u00f6l\u00b7le", "woh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}