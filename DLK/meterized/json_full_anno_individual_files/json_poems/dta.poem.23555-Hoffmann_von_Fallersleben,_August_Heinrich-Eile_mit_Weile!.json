{"dta.poem.23555": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eile mit Weile!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ja, immer gr\u00f6\u00dfer wird die Eile:", "tokens": ["Ja", ",", "im\u00b7mer", "gr\u00f6\u00b7\u00dfer", "wird", "die", "Ei\u00b7le", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Man sucht Gewinn, man will Genu\u00df,", "tokens": ["Man", "sucht", "Ge\u00b7winn", ",", "man", "will", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NN", "$,", "PIS", "VMFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch bleibet uns an Langerweile", "tokens": ["Doch", "blei\u00b7bet", "uns", "an", "Lan\u00b7ger\u00b7wei\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch immer gro\u00dfer Ueberflu\u00df.", "tokens": ["Noch", "im\u00b7mer", "gro\u00b7\u00dfer", "Ue\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und fliegst du wie ein Vogel, fliege!", "tokens": ["Und", "fliegst", "du", "wie", "ein", "Vo\u00b7gel", ",", "flie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Langeweile l\u00e4sst nicht ab:", "tokens": ["Die", "Lan\u00b7ge\u00b7wei\u00b7le", "l\u00e4sst", "nicht", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lag mit dir schon in der Wiege,", "tokens": ["Sie", "lag", "mit", "dir", "schon", "in", "der", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sie geht mit dir auch in das Grab.", "tokens": ["Sie", "geht", "mit", "dir", "auch", "in", "das", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}