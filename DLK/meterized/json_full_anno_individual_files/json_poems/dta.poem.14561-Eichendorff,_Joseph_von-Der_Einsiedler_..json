{"dta.poem.14561": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Einsiedler .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Komm, Trost der Welt, Du stille Nacht!", "tokens": ["Komm", ",", "Trost", "der", "Welt", ",", "Du", "stil\u00b7le", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie steigst Du von den Bergen sacht,", "tokens": ["Wie", "steigst", "Du", "von", "den", "Ber\u00b7gen", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die L\u00fcfte alle schlafen,", "tokens": ["Die", "L\u00fcf\u00b7te", "al\u00b7le", "schla\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Schiffer nur noch, wanderm\u00fcd,", "tokens": ["Ein", "Schif\u00b7fer", "nur", "noch", ",", "wan\u00b7der\u00b7m\u00fcd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "$,", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Singt \u00fcber's Meer sein Abendlied", "tokens": ["Singt", "\u00fc\u00b7ber's", "Meer", "sein", "A\u00b7ben\u00b7dlied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu Gottes Lob im Hafen.", "tokens": ["Zu", "Got\u00b7tes", "Lob", "im", "Ha\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}