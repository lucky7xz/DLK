{"textgrid.poem.61273": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Frohe Botschaft", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nach langem, bangem Winterschweigen", "tokens": ["Nach", "lan\u00b7gem", ",", "ban\u00b7gem", "Win\u00b7ter\u00b7schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Willkommen, heller Fr\u00fchlingsklang!", "tokens": ["Will\u00b7kom\u00b7men", ",", "hel\u00b7ler", "Fr\u00fch\u00b7lings\u00b7klang", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nun r\u00fchrt der Saft sich in den Zweigen,", "tokens": ["Nun", "r\u00fchrt", "der", "Saft", "sich", "in", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und in der Seele der Gesang.", "tokens": ["Und", "in", "der", "See\u00b7le", "der", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wandelt unter Bl\u00fctenb\u00e4umen", "tokens": ["Es", "wan\u00b7delt", "un\u00b7ter", "Bl\u00fc\u00b7ten\u00b7b\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Hoffnung \u00fcbers gr\u00fcne Feld;", "tokens": ["Die", "Hoff\u00b7nung", "\u00fc\u00b7bers", "gr\u00fc\