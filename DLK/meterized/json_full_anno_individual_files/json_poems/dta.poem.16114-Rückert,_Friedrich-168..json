{"dta.poem.16114": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "168.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Sprichwort sagt, darauf magst du dein Gl\u00fccke bauen:", "tokens": ["Ein", "Sprich\u00b7wort", "sagt", ",", "da\u00b7rauf", "magst", "du", "dein", "Gl\u00fc\u00b7cke", "bau\u00b7en", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Feinde soll man selbst zur Flucht die Br\u00fccke bauen.", "tokens": ["Dem", "Fein\u00b7de", "soll", "man", "selbst", "zur", "Flucht", "die", "Br\u00fc\u00b7cke", "bau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "ADV", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Im Feld des Krieges zwar ist manches auch dawider;", "tokens": ["Im", "Feld", "des", "Krie\u00b7ges", "zwar", "ist", "man\u00b7ches", "auch", "da\u00b7wi\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "VAFIN", "PIS", "ADV", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df heut den Feind entfliehn, so kommt er morgen wieder.", "tokens": ["La\u00df", "heut", "den", "Feind", "ent\u00b7fliehn", ",", "so", "kommt", "er", "mor\u00b7gen", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "VVINF", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hingegen unbedingt gilts in des Lebens Krieg:", "tokens": ["Hin\u00b7ge\u00b7gen", "un\u00b7be\u00b7dingt", "gilts", "in", "des", "Le\u00b7bens", "Krieg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Verfolge nicht zu weit den Feind und deinen Sieg.", "tokens": ["Ver\u00b7fol\u00b7ge", "nicht", "zu", "weit", "den", "Feind", "und", "dei\u00b7nen", "Sieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKA", "ADJD", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}