{"textgrid.poem.54222": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Soltst du, mein Engel sehn, wie kranck ich mich befinde,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soltst du, mein Engel sehn, wie kranck ich mich befinde,", "tokens": ["Soltst", "du", ",", "mein", "En\u00b7gel", "sehn", ",", "wie", "kranck", "ich", "mich", "be\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "NN", "VVINF", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor Melancholey die matten H\u00e4nde winde;", "tokens": ["Und", "vor", "Me\u00b7lan\u00b7cho\u00b7ley", "die", "mat\u00b7ten", "H\u00e4n\u00b7de", "win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bliebe wahrlich nicht dein Hertz von Stahl und Stein,", "tokens": ["So", "blie\u00b7be", "wahr\u00b7lich", "nicht", "dein", "Hertz", "von", "Stahl", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du w\u00fcrdest gegen mich nicht mehr tyrannisch seyn.", "tokens": ["Du", "w\u00fcr\u00b7dest", "ge\u00b7gen", "mich", "nicht", "mehr", "ty\u00b7ran\u00b7nisch", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Zunge lechtzt vor Durst, sie wei\u00df kaum mehr zu lallen,", "tokens": ["Die", "Zun\u00b7ge", "lechtzt", "vor", "Durst", ",", "sie", "wei\u00df", "kaum", "mehr", "zu", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Auge bricht, mein Leib ist schwach und so verfallen,", "tokens": ["Das", "Au\u00b7ge", "bricht", ",", "mein", "Leib", "ist", "schwach", "und", "so", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df noch ein schmahler Schritt nach meinen Grabmahl ist,", "tokens": ["Da\u00df", "noch", "ein", "schmah\u00b7ler", "Schritt", "nach", "mei\u00b7nen", "Grab\u00b7mahl", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Worzu mir deine Hand die Steine selber list.", "tokens": ["Wor\u00b7zu", "mir", "dei\u00b7ne", "Hand", "die", "Stei\u00b7ne", "sel\u00b7ber", "list", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Engel, kanst du noch an mir so grausam handeln?", "tokens": ["Mein", "En\u00b7gel", ",", "kanst", "du", "noch", "an", "mir", "so", "grau\u00b7sam", "han\u00b7deln", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "L\u00e4\u00dft du mich sonder Trost nach meiner Grube wandeln?", "tokens": ["L\u00e4\u00dft", "du", "mich", "son\u00b7der", "Trost", "nach", "mei\u00b7ner", "Gru\u00b7be", "wan\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ists m\u00f6glich, da\u00df dich so nach meinen Blute d\u00fcrst?", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "dich", "so", "nach", "mei\u00b7nen", "Blu\u00b7te", "d\u00fcrst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bedencke, da\u00df du nun an mir zum M\u00f6rder wirst.", "tokens": ["Be\u00b7den\u00b7cke", ",", "da\u00df", "du", "nun", "an", "mir", "zum", "M\u00f6r\u00b7der", "wirst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist bis nunmehr der Danck vor meine Lieb' und Flammen,", "tokens": ["Ist", "bis", "nun\u00b7mehr", "der", "Danck", "vor", "mei\u00b7ne", "Lieb'", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df du zum Opfer-Heerd wilst selbsten mich verdammen,", "tokens": ["Da\u00df", "du", "zum", "Op\u00b7fer\u00b7He\u00b7erd", "wilst", "selbs\u00b7ten", "mich", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VMFIN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Und r\u00e4chend mich vertilgst, was hab ich denn gethan,", "tokens": ["Und", "r\u00e4\u00b7chend", "mich", "ver\u00b7tilgst", ",", "was", "hab", "ich", "denn", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das einen solchen Groll und Ha\u00df erregen kan?", "tokens": ["Das", "ei\u00b7nen", "sol\u00b7chen", "Groll", "und", "Ha\u00df", "er\u00b7re\u00b7gen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich bin dir allezeit mit Ehrfurcht nachgegangen,", "tokens": ["Ich", "bin", "dir", "al\u00b7le\u00b7zeit", "mit", "Ehr\u00b7furcht", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Frechheit hatte nie das Wappen ausgehangen;", "tokens": ["Die", "Frech\u00b7heit", "hat\u00b7te", "nie", "das", "Wap\u00b7pen", "aus\u00b7ge\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du warst mein andres ich, mein Angel-Stern und Licht,", "tokens": ["Du", "warst", "mein", "and\u00b7res", "ich", ",", "mein", "An\u00b7gel\u00b7S\u00b7tern", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.20": {"text": "Mein Auge suchte dich, und sonsten keine nicht.", "tokens": ["Mein", "Au\u00b7ge", "such\u00b7te", "dich", ",", "und", "sons\u00b7ten", "kei\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KON", "ADV", "PIAT", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch alles dieses will bey dir gar wenig taugen,", "tokens": ["Doch", "al\u00b7les", "die\u00b7ses", "will", "bey", "dir", "gar", "we\u00b7nig", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "VMFIN", "APPR", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es machet mich vielmehr in deinen sch\u00f6nen Augen", "tokens": ["Es", "ma\u00b7chet", "mich", "viel\u00b7mehr", "in", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Zum Greuel, sch\u00f6nstes Kind, dein fest verstopftes Ohr", "tokens": ["Zum", "Greu\u00b7el", ",", "sch\u00f6ns\u00b7tes", "Kind", ",", "dein", "fest", "ver\u00b7stopf\u00b7tes", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bleibt bey den Klagen taub noch immer, wie zuvor.", "tokens": ["Bleibt", "bey", "den", "Kla\u00b7gen", "taub", "noch", "im\u00b7mer", ",", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dis ist was meinem Leib auf Sieche-Betten leget,", "tokens": ["Dis", "ist", "was", "mei\u00b7nem", "Leib", "auf", "Sie\u00b7che\u00b7Bet\u00b7ten", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und auf die Stirne schon des Todes Merckmahl pr\u00e4get.", "tokens": ["Und", "auf", "die", "Stir\u00b7ne", "schon", "des", "To\u00b7des", "Merck\u00b7mahl", "pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist dir mit meinen Blut gedient, so nimm es hin,", "tokens": ["Ist", "dir", "mit", "mei\u00b7nen", "Blut", "ge\u00b7dient", ",", "so", "nimm", "es", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Damit ich dir auch noch zuletzt gehorsam bin.", "tokens": ["Da\u00b7mit", "ich", "dir", "auch", "noch", "zu\u00b7letzt", "ge\u00b7hor\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Tyrannin! nimm das Hertz, zerfleische Marck und Knochen,", "tokens": ["Ty\u00b7ran\u00b7nin", "!", "nimm", "das", "Hertz", ",", "zer\u00b7flei\u00b7sche", "Marck", "und", "Kno\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "ART", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dein kalter Sinn hat mir das Leben abgesprochen;", "tokens": ["Dein", "kal\u00b7ter", "Sinn", "hat", "mir", "das", "Le\u00b7ben", "ab\u00b7ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Komm, komm und s\u00e4ume nicht, ich bl\u00f6se schon die Brust,", "tokens": ["Komm", ",", "komm", "und", "s\u00e4u\u00b7me", "nicht", ",", "ich", "bl\u00f6\u00b7se", "schon", "die", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und wart auf meinen Tod mit gantz besondrer Lust.", "tokens": ["Und", "wart", "auf", "mei\u00b7nen", "Tod", "mit", "gantz", "be\u00b7sond\u00b7rer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kan mein entseelter Leib dir Freud und Lust erwecken,", "tokens": ["Kan", "mein", "ent\u00b7seel\u00b7ter", "Leib", "dir", "Freud", "und", "Lust", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So weyde dich daran, ich will mich willig strecken,", "tokens": ["So", "wey\u00b7de", "dich", "da\u00b7ran", ",", "ich", "will", "mich", "wil\u00b7lig", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mein Geist empfindt vielleicht, h\u00f6rt gleich der C\u00f6rper nicht,", "tokens": ["Mein", "Geist", "emp\u00b7findt", "viel\u00b7leicht", ",", "h\u00f6rt", "gleich", "der", "C\u00f6r\u00b7per", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was dein verstockter Mund zu meinen Leichnam spricht.", "tokens": ["Was", "dein", "ver\u00b7stock\u00b7ter", "Mund", "zu", "mei\u00b7nen", "Leich\u00b7nam", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "O! solte, Sch\u00f6nste, dir die Haut darbey nicht schauren;", "tokens": ["O", "!", "sol\u00b7te", ",", "Sch\u00f6ns\u00b7te", ",", "dir", "die", "Haut", "dar\u00b7bey", "nicht", "schau\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "$,", "NN", "$,", "PPER", "ART", "NN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dich wird, wiewohl zu sp\u00e4th, mein armes Leben dauren.", "tokens": ["Dich", "wird", ",", "wie\u00b7wohl", "zu", "sp\u00e4th", ",", "mein", "ar\u00b7mes", "Le\u00b7ben", "dau\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PTKA", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Jedoch was hilfft es mir? mir n\u00fctzt es nichts im Tod,", "tokens": ["Je\u00b7doch", "was", "hilfft", "es", "mir", "?", "mir", "n\u00fctzt", "es", "nichts", "im", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Gnug, mein beklemmter Geist entrei\u00dft sich aller Noth.", "tokens": ["Gnug", ",", "mein", "be\u00b7klemm\u00b7ter", "Geist", "ent\u00b7rei\u00dft", "sich", "al\u00b7ler", "Noth", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Fesseln springen weg, die Band und Ketten reissen,", "tokens": ["Die", "Fes\u00b7seln", "sprin\u00b7gen", "weg", ",", "die", "Band", "und", "Ket\u00b7ten", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der Tod erbarmet sich, so grausam du geheissen.", "tokens": ["Der", "Tod", "er\u00b7bar\u00b7met", "sich", ",", "so", "grau\u00b7sam", "du", "ge\u00b7heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die letzte Stunde schl\u00e4gt, ich f\u00fchle schon den Sto\u00df,", "tokens": ["Die", "letz\u00b7te", "Stun\u00b7de", "schl\u00e4gt", ",", "ich", "f\u00fch\u00b7le", "schon", "den", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mein Engel! lebe wohl! du wirst mich nunmehr lo\u00df.", "tokens": ["Mein", "En\u00b7gel", "!", "le\u00b7be", "wohl", "!", "du", "wirst", "mich", "nun\u00b7mehr", "lo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Soltst du, mein Engel sehn, wie kranck ich mich befinde,", "tokens": ["Soltst", "du", ",", "mein", "En\u00b7gel", "sehn", ",", "wie", "kranck", "ich", "mich", "be\u00b7fin\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "NN", "VVINF", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und vor Melancholey die matten H\u00e4nde winde;", "tokens": ["Und", "vor", "Me\u00b7lan\u00b7cho\u00b7ley", "die", "mat\u00b7ten", "H\u00e4n\u00b7de", "win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So bliebe wahrlich nicht dein Hertz von Stahl und Stein,", "tokens": ["So", "blie\u00b7be", "wahr\u00b7lich", "nicht", "dein", "Hertz", "von", "Stahl", "und", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du w\u00fcrdest gegen mich nicht mehr tyrannisch seyn.", "tokens": ["Du", "w\u00fcr\u00b7dest", "ge\u00b7gen", "mich", "nicht", "mehr", "ty\u00b7ran\u00b7nisch", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "PTKNEG", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Zunge lechtzt vor Durst, sie wei\u00df kaum mehr zu lallen,", "tokens": ["Die", "Zun\u00b7ge", "lechtzt", "vor", "Durst", ",", "sie", "wei\u00df", "kaum", "mehr", "zu", "lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,", "PPER", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Auge bricht, mein Leib ist schwach und so verfallen,", "tokens": ["Das", "Au\u00b7ge", "bricht", ",", "mein", "Leib", "ist", "schwach", "und", "so", "ver\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df noch ein schmahler Schritt nach meinen Grabmahl ist,", "tokens": ["Da\u00df", "noch", "ein", "schmah\u00b7ler", "Schritt", "nach", "mei\u00b7nen", "Grab\u00b7mahl", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Worzu mir deine Hand die Steine selber list.", "tokens": ["Wor\u00b7zu", "mir", "dei\u00b7ne", "Hand", "die", "Stei\u00b7ne", "sel\u00b7ber", "list", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Engel, kanst du noch an mir so grausam handeln?", "tokens": ["Mein", "En\u00b7gel", ",", "kanst", "du", "noch", "an", "mir", "so", "grau\u00b7sam", "han\u00b7deln", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VMFIN", "PPER", "ADV", "APPR", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "L\u00e4\u00dft du mich sonder Trost nach meiner Grube wandeln?", "tokens": ["L\u00e4\u00dft", "du", "mich", "son\u00b7der", "Trost", "nach", "mei\u00b7ner", "Gru\u00b7be", "wan\u00b7deln", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ists m\u00f6glich, da\u00df dich so nach meinen Blute d\u00fcrst?", "tokens": ["Ists", "m\u00f6g\u00b7lich", ",", "da\u00df", "dich", "so", "nach", "mei\u00b7nen", "Blu\u00b7te", "d\u00fcrst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bedencke, da\u00df du nun an mir zum M\u00f6rder wirst.", "tokens": ["Be\u00b7den\u00b7cke", ",", "da\u00df", "du", "nun", "an", "mir", "zum", "M\u00f6r\u00b7der", "wirst", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ADV", "APPR", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ist bis nunmehr der Danck vor meine Lieb' und Flammen,", "tokens": ["Ist", "bis", "nun\u00b7mehr", "der", "Danck", "vor", "mei\u00b7ne", "Lieb'", "und", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df du zum Opfer-Heerd wilst selbsten mich verdammen,", "tokens": ["Da\u00df", "du", "zum", "Op\u00b7fer\u00b7He\u00b7erd", "wilst", "selbs\u00b7ten", "mich", "ver\u00b7dam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VMFIN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Und r\u00e4chend mich vertilgst, was hab ich denn gethan,", "tokens": ["Und", "r\u00e4\u00b7chend", "mich", "ver\u00b7tilgst", ",", "was", "hab", "ich", "denn", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$,", "PWS", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Das einen solchen Groll und Ha\u00df erregen kan?", "tokens": ["Das", "ei\u00b7nen", "sol\u00b7chen", "Groll", "und", "Ha\u00df", "er\u00b7re\u00b7gen", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "PIAT", "NN", "KON", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich bin dir allezeit mit Ehrfurcht nachgegangen,", "tokens": ["Ich", "bin", "dir", "al\u00b7le\u00b7zeit", "mit", "Ehr\u00b7furcht", "nach\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Frechheit hatte nie das Wappen ausgehangen;", "tokens": ["Die", "Frech\u00b7heit", "hat\u00b7te", "nie", "das", "Wap\u00b7pen", "aus\u00b7ge\u00b7han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du warst mein andres ich, mein Angel-Stern und Licht,", "tokens": ["Du", "warst", "mein", "and\u00b7res", "ich", ",", "mein", "An\u00b7gel\u00b7S\u00b7tern", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "PPER", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.20": {"text": "Mein Auge suchte dich, und sonsten keine nicht.", "tokens": ["Mein", "Au\u00b7ge", "such\u00b7te", "dich", ",", "und", "sons\u00b7ten", "kei\u00b7ne", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "$,", "KON", "ADV", "PIAT", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Doch alles dieses will bey dir gar wenig taugen,", "tokens": ["Doch", "al\u00b7les", "die\u00b7ses", "will", "bey", "dir", "gar", "we\u00b7nig", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "VMFIN", "APPR", "PPER", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Es machet mich vielmehr in deinen sch\u00f6nen Augen", "tokens": ["Es", "ma\u00b7chet", "mich", "viel\u00b7mehr", "in", "dei\u00b7nen", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Zum Greuel, sch\u00f6nstes Kind, dein fest verstopftes Ohr", "tokens": ["Zum", "Greu\u00b7el", ",", "sch\u00f6ns\u00b7tes", "Kind", ",", "dein", "fest", "ver\u00b7stopf\u00b7tes", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "ADJA", "NN", "$,", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Bleibt bey den Klagen taub noch immer, wie zuvor.", "tokens": ["Bleibt", "bey", "den", "Kla\u00b7gen", "taub", "noch", "im\u00b7mer", ",", "wie", "zu\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVFIN", "ADV", "ADV", "$,", "PWAV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Dis ist was meinem Leib auf Sieche-Betten leget,", "tokens": ["Dis", "ist", "was", "mei\u00b7nem", "Leib", "auf", "Sie\u00b7che\u00b7Bet\u00b7ten", "le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und auf die Stirne schon des Todes Merckmahl pr\u00e4get.", "tokens": ["Und", "auf", "die", "Stir\u00b7ne", "schon", "des", "To\u00b7des", "Merck\u00b7mahl", "pr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Ist dir mit meinen Blut gedient, so nimm es hin,", "tokens": ["Ist", "dir", "mit", "mei\u00b7nen", "Blut", "ge\u00b7dient", ",", "so", "nimm", "es", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "ADV", "VVIMP", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Damit ich dir auch noch zuletzt gehorsam bin.", "tokens": ["Da\u00b7mit", "ich", "dir", "auch", "noch", "zu\u00b7letzt", "ge\u00b7hor\u00b7sam", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Tyrannin! nimm das Hertz, zerfleische Marck und Knochen,", "tokens": ["Ty\u00b7ran\u00b7nin", "!", "nimm", "das", "Hertz", ",", "zer\u00b7flei\u00b7sche", "Marck", "und", "Kno\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "ART", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dein kalter Sinn hat mir das Leben abgesprochen;", "tokens": ["Dein", "kal\u00b7ter", "Sinn", "hat", "mir", "das", "Le\u00b7ben", "ab\u00b7ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Komm, komm und s\u00e4ume nicht, ich bl\u00f6se schon die Brust,", "tokens": ["Komm", ",", "komm", "und", "s\u00e4u\u00b7me", "nicht", ",", "ich", "bl\u00f6\u00b7se", "schon", "die", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und wart auf meinen Tod mit gantz besondrer Lust.", "tokens": ["Und", "wart", "auf", "mei\u00b7nen", "Tod", "mit", "gantz", "be\u00b7sond\u00b7rer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Kan mein entseelter Leib dir Freud und Lust erwecken,", "tokens": ["Kan", "mein", "ent\u00b7seel\u00b7ter", "Leib", "dir", "Freud", "und", "Lust", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "PPER", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So weyde dich daran, ich will mich willig strecken,", "tokens": ["So", "wey\u00b7de", "dich", "da\u00b7ran", ",", "ich", "will", "mich", "wil\u00b7lig", "stre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "$,", "PPER", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Mein Geist empfindt vielleicht, h\u00f6rt gleich der C\u00f6rper nicht,", "tokens": ["Mein", "Geist", "emp\u00b7findt", "viel\u00b7leicht", ",", "h\u00f6rt", "gleich", "der", "C\u00f6r\u00b7per", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$,", "VVFIN", "ADV", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Was dein verstockter Mund zu meinen Leichnam spricht.", "tokens": ["Was", "dein", "ver\u00b7stock\u00b7ter", "Mund", "zu", "mei\u00b7nen", "Leich\u00b7nam", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "O! solte, Sch\u00f6nste, dir die Haut darbey nicht schauren;", "tokens": ["O", "!", "sol\u00b7te", ",", "Sch\u00f6ns\u00b7te", ",", "dir", "die", "Haut", "dar\u00b7bey", "nicht", "schau\u00b7ren", ";"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "$,", "NN", "$,", "PPER", "ART", "NN", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dich wird, wiewohl zu sp\u00e4th, mein armes Leben dauren.", "tokens": ["Dich", "wird", ",", "wie\u00b7wohl", "zu", "sp\u00e4th", ",", "mein", "ar\u00b7mes", "Le\u00b7ben", "dau\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PTKA", "ADJD", "$,", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Jedoch was hilfft es mir? mir n\u00fctzt es nichts im Tod,", "tokens": ["Je\u00b7doch", "was", "hilfft", "es", "mir", "?", "mir", "n\u00fctzt", "es", "nichts", "im", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPER", "PPER", "$.", "PPER", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Gnug, mein beklemmter Geist entrei\u00dft sich aller Noth.", "tokens": ["Gnug", ",", "mein", "be\u00b7klemm\u00b7ter", "Geist", "ent\u00b7rei\u00dft", "sich", "al\u00b7ler", "Noth", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Fesseln springen weg, die Band und Ketten reissen,", "tokens": ["Die", "Fes\u00b7seln", "sprin\u00b7gen", "weg", ",", "die", "Band", "und", "Ket\u00b7ten", "reis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der Tod erbarmet sich, so grausam du geheissen.", "tokens": ["Der", "Tod", "er\u00b7bar\u00b7met", "sich", ",", "so", "grau\u00b7sam", "du", "ge\u00b7heis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die letzte Stunde schl\u00e4gt, ich f\u00fchle schon den Sto\u00df,", "tokens": ["Die", "letz\u00b7te", "Stun\u00b7de", "schl\u00e4gt", ",", "ich", "f\u00fch\u00b7le", "schon", "den", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Mein Engel! lebe wohl! du wirst mich nunmehr lo\u00df.", "tokens": ["Mein", "En\u00b7gel", "!", "le\u00b7be", "wohl", "!", "du", "wirst", "mich", "nun\u00b7mehr", "lo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "ADV", "$.", "PPER", "VAFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}