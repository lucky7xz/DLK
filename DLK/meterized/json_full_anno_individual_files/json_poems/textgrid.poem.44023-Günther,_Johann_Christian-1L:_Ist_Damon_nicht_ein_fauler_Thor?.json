{"textgrid.poem.44023": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ist Damon nicht ein fauler Thor?", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Damon nicht ein fauler Thor?", "tokens": ["Ist", "Da\u00b7mon", "nicht", "ein", "fau\u00b7ler", "Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So sprechen viel der reichen Br\u00fcder,", "tokens": ["So", "spre\u00b7chen", "viel", "der", "rei\u00b7chen", "Br\u00fc\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er bringt kein gro\u00dfes Werck hervor,", "tokens": ["Er", "bringt", "kein", "gro\u00b7\u00dfes", "Werck", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und was er macht, das sind nur Lieder;", "tokens": ["Und", "was", "er", "macht", ",", "das", "sind", "nur", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PDS", "VAFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Er scherzt mit G\u00e4rthen, Ku\u00df und Hayn,", "tokens": ["Er", "scherzt", "mit", "G\u00e4r\u00b7then", ",", "Ku\u00df", "und", "Hayn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Will ruhig und verborgen seyn", "tokens": ["Will", "ru\u00b7hig", "und", "ver\u00b7bor\u00b7gen", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "KON", "VVPP", "VAINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und weder Schaz noch Staat erwerben;", "tokens": ["Und", "we\u00b7der", "Schaz", "noch", "Staat", "er\u00b7wer\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KON", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sagt, ist wohl Damon weis und klug?", "tokens": ["Sagt", ",", "ist", "wohl", "Da\u00b7mon", "weis", "und", "klug", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "ADV", "NE", "PTKVZ", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr Narren, thut er nicht genug?", "tokens": ["Ihr", "Nar\u00b7ren", ",", "thut", "er", "nicht", "ge\u00b7nug", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Er lernt ja leben und auch sterben.", "tokens": ["Er", "lernt", "ja", "le\u00b7ben", "und", "auch", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ist Damon nicht ein fauler Thor?", "tokens": ["Ist", "Da\u00b7mon", "nicht", "ein", "fau\u00b7ler", "Thor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ART", "ADJA", "NN", 