{"dta.poem.2048": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Fuchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nebst dem Balg ist auch am Fuchs seine Lunge, Fett", "tokens": ["Nebst", "dem", "Balg", "ist", "auch", "am", "Fuchs", "sei\u00b7ne", "Lun\u00b7ge", ",", "Fett"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADV", "APPRART", "NE", "PPOSAT", "NN", "$,", "NN"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "und Blut", "tokens": ["und", "Blut"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Gegen Schwindsucht, Krampfbeschwerden, Blas\u2019- und", "tokens": ["Ge\u00b7gen", "Schwind\u00b7sucht", ",", "Krampf\u00b7be\u00b7schwer\u00b7den", ",", "Blas'", "und"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "TRUNC", "KON"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Nierenschmerzen gut.", "tokens": ["Nie\u00b7ren\u00b7schmer\u00b7zen", "gut", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wird man also abermal auch in F\u00fcchsen offen-", "tokens": ["Wird", "man", "al\u00b7so", "a\u00b7ber\u00b7mal", "auch", "in", "F\u00fcch\u00b7sen", "of\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "ADV", "ADV", "ADV", "APPR", "NN", "TRUNC"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "bar", "tokens": ["bar"], "token_info": ["word"], "pos": ["ADJD"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Einer Ordnung, Weisheit, Absicht, wenn man es er-", "tokens": ["Ei\u00b7ner", "Ord\u00b7nung", ",", "Weis\u00b7heit", ",", "Ab\u00b7sicht", ",", "wenn", "man", "es", "er"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "$,", "KOUS", "PIS", "PPER", "TRUNC"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "w\u00e4gt, gewahr!", "tokens": ["w\u00e4gt", ",", "ge\u00b7wahr", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}