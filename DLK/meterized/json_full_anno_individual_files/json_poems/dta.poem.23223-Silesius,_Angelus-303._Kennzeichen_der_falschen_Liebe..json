{"dta.poem.23223": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "303. Kennzeichen der falschen Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wiltu die falsche Lieb von wahrer unterscheiden/", "tokens": ["Wil\u00b7tu", "die", "fal\u00b7sche", "Lieb", "von", "wah\u00b7rer", "un\u00b7ter\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "---+-+-+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "So schau sie sucht sich selbst/ und f\u00e4llet ab inn Leiden.", "tokens": ["So", "schau", "sie", "sucht", "sich", "selbst", "/", "und", "f\u00e4l\u00b7let", "ab", "inn", "Lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PRF", "ADV", "$(", "KON", "VVFIN", "PTKVZ", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}