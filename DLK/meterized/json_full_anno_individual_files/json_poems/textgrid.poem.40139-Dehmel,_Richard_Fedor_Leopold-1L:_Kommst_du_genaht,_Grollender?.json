{"textgrid.poem.40139": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kommst du genaht, Grollender?", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommst du genaht, Grollender?", "tokens": ["Kommst", "du", "ge\u00b7naht", ",", "Grol\u00b7len\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "VVPP", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "tief von Unten?", "tokens": ["tief", "von", "Un\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "schwarzgekr\u00f6nter Wetter riese,", "tokens": ["schwarz\u00b7ge\u00b7kr\u00f6n\u00b7ter", "Wet\u00b7ter", "rie\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "mit der bleiernen Wolkenstirne?", "tokens": ["mit", "der", "blei\u00b7er\u00b7nen", "Wol\u00b7kens\u00b7tir\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "H\u00f6her doch! n\u00e4her! ", "tokens": ["H\u00f6\u00b7her", "doch", "!", "n\u00e4\u00b7her", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$.", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Mir und meiner Sonne,", "tokens": ["Mir", "und", "mei\u00b7ner", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "die ich erst\u00fcrmt", "tokens": ["die", "ich", "er\u00b7st\u00fcrmt"], "token_info": ["word", "word", "word"], "pos": ["PRELS", "PPER", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "mit meiner Kraft,", "tokens": ["mit", "mei\u00b7ner", "Kraft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "die ", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.10": {"text": "Sie meine Seele, ihr Leben Ich,", "tokens": ["Sie", "mei\u00b7ne", "See\u00b7le", ",", "ihr", "Le\u00b7ben", "Ich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "zusammengeschmolzen in Eine gro\u00dfe", "tokens": ["zu\u00b7sam\u00b7men\u00b7ge\u00b7schmol\u00b7zen", "in", "Ei\u00b7ne", "gro\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "einige einzige Flammenwelt! \u2013", "tokens": ["ei\u00b7ni\u00b7ge", "ein\u00b7zi\u00b7ge", "Flam\u00b7men\u00b7welt", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "$("], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ja, du suchest uns:", "tokens": ["Ja", ",", "du", "su\u00b7chest", "uns", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "du willst uns ", "tokens": ["du", "willst", "uns"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "sehnest dich nach ", "tokens": ["seh\u00b7nest", "dich", "nach"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Flammender!", "tokens": ["Flam\u00b7men\u00b7der", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}, "line.5": {"text": "sehnest dich, zu blicken", "tokens": ["seh\u00b7nest", "dich", ",", "zu", "bli\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PTKZU", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "tief in unser leuchtend", "tokens": ["tief", "in", "un\u00b7ser", "leuch\u00b7tend"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "allumfangend Gl\u00fcck,", "tokens": ["al\u00b7lum\u00b7fan\u00b7gend", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "allumkreisender", "tokens": ["al\u00b7lum\u00b7krei\u00b7sen\u00b7der"], "token_info": ["word"], "pos": ["ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ersch\u00fcttrer Du!", "tokens": ["Er\u00b7sch\u00fctt\u00b7rer", "Du", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ja! ich ", "tokens": ["Ja", "!", "ich"], "token_info": ["word", "punct", "word"], "pos": ["PTKANT", "$.", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "du bist mein ", "tokens": ["du", "bist", "mein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Tief schaue ich dir", "tokens": ["Tief", "schau\u00b7e", "ich", "dir"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "durch das n\u00e4chtige Auge", "tokens": ["durch", "das", "n\u00e4ch\u00b7ti\u00b7ge", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "in das hei\u00dfe zuckende Herz:", "tokens": ["in", "das", "hei\u00b7\u00dfe", "zu\u00b7cken\u00b7de", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "du wirkest ", "tokens": ["du", "wir\u00b7kest"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "auf die lechzenden Fluren herab,", "tokens": ["auf", "die", "lech\u00b7zen\u00b7den", "Flu\u00b7ren", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "wenn du mit wuchtender Faust", "tokens": ["wenn", "du", "mit", "wuch\u00b7ten\u00b7der", "Faust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "krachend zerrei\u00dfest", "tokens": ["kra\u00b7chend", "zer\u00b7rei\u00b7\u00dfest"], "token_info": ["word", "word"], "pos": ["ADJD", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "die schwer hangende Luftlast.", "tokens": ["die", "schwer", "han\u00b7gen\u00b7de", "Luft\u00b7last", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "die schatten umbr\u00fcteten Wimpern!", "tokens": ["die", "schat\u00b7ten", "um\u00b7br\u00fc\u00b7te\u00b7ten", "Wim\u00b7pern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "ewigkeit erschlie\u00dfend Auge,", "tokens": ["e\u00b7wig\u00b7keit", "er\u00b7schlie\u00b7\u00dfend", "Au\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich ", "tokens": ["da\u00df", "ich"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "an dieser funkelnden Unendlichkeit \u2013!", "tokens": ["an", "die\u00b7ser", "fun\u00b7keln\u00b7den", "Un\u00b7end\u00b7lich\u00b7keit", "\u2013", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Oeffnet, ihr schmetternden Lippen, euch!", "tokens": ["Oeff\u00b7net", ",", "ihr", "schmet\u00b7tern\u00b7den", "Lip\u00b7pen", ",", "euch", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "aus eurem rollenden Donnersang rauscht mir", "tokens": ["aus", "eu\u00b7rem", "rol\u00b7len\u00b7den", "Don\u00b7ner\u00b7sang", "rauscht", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "das ewige Lied vom Ringen der Wesen,", "tokens": ["das", "e\u00b7wi\u00b7ge", "Lied", "vom", "Rin\u00b7gen", "der", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "vom Krieg des Lebens \u2013 der Atem der Welt!", "tokens": ["vom", "Krieg", "des", "Le\u00b7bens", "\u2013", "der", "A\u00b7tem", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$(", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Sonne, meine Sonne!", "tokens": ["Son\u00b7ne", ",", "mei\u00b7ne", "Son\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "siehe: er ", "tokens": ["sie\u00b7he", ":", "er"], "token_info": ["word", "punct", "word"], "pos": ["VVIMP", "$.", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Licht sein Blick,", "tokens": ["Licht", "sein", "Blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "wir selber licht,", "tokens": ["wir", "sel\u00b7ber", "licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "stehn wir umlodert,", "tokens": ["stehn", "wir", "um\u00b7lo\u00b7dert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "stehn wir und zagen nicht:", "tokens": ["stehn", "wir", "und", "za\u00b7gen", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "ADJA", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "zittern vor ", "tokens": ["zit\u00b7tern", "vor"], "token_info": ["word", "word"], "pos": ["VVFIN", "APPR"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Ob auch berstend der Baum zerspellt,", "tokens": ["Ob", "auch", "bers\u00b7tend", "der", "Baum", "zer\u00b7spellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VVPP", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "in Tr\u00fcmmer zerprasselt ein Menschenwerk", "tokens": ["in", "Tr\u00fcm\u00b7mer", "zer\u00b7pras\u00b7selt", "ein", "Men\u00b7schen\u00b7werk"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "unter der br\u00fcllenden Lohe:", "tokens": ["un\u00b7ter", "der", "br\u00fcl\u00b7len\u00b7den", "Lo\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.11": {"text": "nicht vergehen sie,", "tokens": ["nicht", "ver\u00b7ge\u00b7hen", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.12": {"text": "neu erstehen sie,", "tokens": ["neu", "er\u00b7ste\u00b7hen", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "neue Gebilde:", "tokens": ["neu\u00b7e", "Ge\u00b7bil\u00b7de", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.14": {"text": "Eines stirbt, das Tausend n\u00e4hrt!", "tokens": ["Ei\u00b7nes", "stirbt", ",", "das", "Tau\u00b7send", "n\u00e4hrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "St\u00fcrzen die Starken, wachsen die Schwachen;", "tokens": ["St\u00fcr\u00b7zen", "die", "Star\u00b7ken", ",", "wach\u00b7sen", "die", "Schwa\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Tausend wachsen, Einer ragt!", "tokens": ["Tau\u00b7send", "wach\u00b7sen", ",", "Ei\u00b7ner", "ragt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Tod und Leben \u2013 stammelt die Lippe;", "tokens": ["Tod", "und", "Le\u00b7ben", "\u2013", "stam\u00b7melt", "die", "Lip\u00b7pe", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.18": {"text": "was ", "tokens": ["was"], "token_info": ["word"], "pos": ["PWS"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Greller doch, Blitze!", "tokens": ["Grel\u00b7ler", "doch", ",", "Blit\u00b7ze", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NE", "ADV", "$,", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "grolle nur, Donner du!", "tokens": ["grol\u00b7le", "nur", ",", "Don\u00b7ner", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "ADV", "$,", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "triff, zermalme", "tokens": ["triff", ",", "zer\u00b7mal\u00b7me"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "die ", "tokens": ["die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Uns schonest du,", "tokens": ["Uns", "scho\u00b7nest", "du", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "in deiner Flammentaufe ahnten wir's!", "tokens": ["in", "dei\u00b7ner", "Flam\u00b7men\u00b7tau\u00b7fe", "ahn\u00b7ten", "wir's", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "sind gefeit", "tokens": ["sind", "ge\u00b7feit"], "token_info": ["word", "word"], "pos": ["VAFIN", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "durch die Liebe,", "tokens": ["durch", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "durch die sonnenselige Liebe,", "tokens": ["durch", "die", "son\u00b7nen\u00b7se\u00b7li\u00b7ge", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "durch die Kraft der Einigen Glut!", "tokens": ["durch", "die", "Kraft", "der", "Ei\u00b7ni\u00b7gen", "Glut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.7": {"line.1": {"text": "Oh! und triffst du auch ", "tokens": ["Oh", "!", "und", "triffst", "du", "auch"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KON", "VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "suchst ein ", "tokens": ["suchst", "ein"