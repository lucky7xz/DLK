{"dta.poem.15833": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "38.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm, la\u00df uns gehn aufs Feld, das lang wir nicht besuchten;", "tokens": ["Komm", ",", "la\u00df", "uns", "gehn", "aufs", "Feld", ",", "das", "lang", "wir", "nicht", "be\u00b7such\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVIMP", "PPER", "VVFIN", "APPRART", "NN", "$,", "PRELS", "ADJD", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Hauch des Maien soll unser Gem\u00fcth befruchten.", "tokens": ["Der", "Hauch", "des", "Mai\u00b7en", "soll", "un\u00b7ser", "Ge\u00b7m\u00fcth", "be\u00b7fruch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VMFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Maienregen spr\u00fcht, la\u00df uns den Maiensegen", "tokens": ["Der", "Mai\u00b7en\u00b7re\u00b7gen", "spr\u00fcht", ",", "la\u00df", "uns", "den", "Mai\u00b7en\u00b7se\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVIMP", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empfahn! ein Sprichwort sagt, fruchtbar macht Maienregen.", "tokens": ["E\u00b7mpfahn", "!", "ein", "Sprich\u00b7wort", "sagt", ",", "frucht\u00b7bar", "macht", "Mai\u00b7en\u00b7re\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "NN", "VVFIN", "$,", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "F\u00e4llt er aufs Land, so schwillt von Fruchtbarkeit der Anger,", "tokens": ["F\u00e4llt", "er", "aufs", "Land", ",", "so", "schwillt", "von", "Frucht\u00b7bar\u00b7keit", "der", "An\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "ADV", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf H\u00fcrden, Herden sind von Zwillingsl\u00e4mmern schwanger.", "tokens": ["Auf", "H\u00fcr\u00b7den", ",", "Her\u00b7den", "sind", "von", "Zwil\u00b7lings\u00b7l\u00e4m\u00b7mern", "schwan\u00b7ger", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und wo er f\u00e4llt aufs Meer, da \u00f6ffnet ihren Scho\u00df", "tokens": ["Und", "wo", "er", "f\u00e4llt", "aufs", "Meer", ",", "da", "\u00f6ff\u00b7net", "ih\u00b7ren", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPRART", "NN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Muschel, und in ihr wird eine Perle gro\u00df.", "tokens": ["Die", "Mu\u00b7schel", ",", "und", "in", "ihr", "wird", "ei\u00b7ne", "Per\u00b7le", "gro\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "APPR", "PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}}}}