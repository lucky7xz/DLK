{"textgrid.poem.25709": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Schlaf du fest und ruhig vor den Dieben!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf du fest und ruhig vor den Dieben!", "tokens": ["Schlaf", "du", "fest", "und", "ru\u00b7hig", "vor", "den", "Die\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "KON", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Fenster ohne Laden blieben,", "tokens": ["Wo", "die", "Fens\u00b7ter", "oh\u00b7ne", "La\u00b7den", "blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie bei mir, da sucht man schwerlich Gold.", "tokens": ["Wie", "bei", "mir", ",", "da", "sucht", "man", "schwer\u00b7lich", "Gold", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PPER", "$,", "ADV", "VVFIN", "PIS", "ADJD", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Macht' ein Dieb sich dennoch das Vergn\u00fcgen:", "tokens": ["Macht'", "ein", "Dieb", "sich", "den\u00b7noch", "das", "Ver\u00b7gn\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Meinen Flaccus lie\u00df er sicher liegen,", "tokens": ["Mei\u00b7nen", "Flac\u00b7cus", "lie\u00df", "er", "si\u00b7cher", "lie\u00b7gen", ","], "token_info": 