{"textgrid.poem.54414": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "25. Gedichte", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Freund, o! thu dir nicht Gewalt;", "tokens": ["Mein", "Freund", ",", "o", "!", "thu", "dir", "nicht", "Ge\u00b7walt", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "FM", "$.", "VVFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kennst du mich gleich nicht von Gestalt,", "tokens": ["Kennst", "du", "mich", "gleich", "nicht", "von", "Ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "APPR", "NN", "$,"], "meter": "+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Deswegen fasse keine Grillen;", "tokens": ["Des\u00b7we\u00b7gen", "fas\u00b7se", "kei\u00b7ne", "Gril\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Kummer will ich dir bald stillen.", "tokens": ["Den", "Kum\u00b7mer", "will", "ich", "dir", "bald", "stil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich setze schon die Feder an.", "tokens": ["Ich", "set\u00b7ze", "schon", "die", "Fe\u00b7der", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit dieser wird dir kund gethan:", "tokens": ["Mit", "die\u00b7ser", "wird", "dir", "kund", "ge\u00b7than", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du sollst mein Bild in Reimen lesen,", "tokens": ["Du", "sollst", "mein", "Bild", "in", "Rei\u00b7men", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein Ansehn und mein ganzes Wesen.", "tokens": ["Mein", "An\u00b7sehn", "und", "mein", "gan\u00b7zes", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Ich bin nicht klein, ich bin nicht gro\u00df,", "tokens": ["Ich", "bin", "nicht", "klein", ",", "ich", "bin", "nicht", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich geh bedeckt und niemals blo\u00df.", "tokens": ["Ich", "geh", "be\u00b7deckt", "und", "nie\u00b7mals", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Mit aufger\u00e4umten frohen Minen", "tokens": ["Mit", "auf\u00b7ge\u00b7r\u00e4um\u00b7ten", "fro\u00b7hen", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "lin