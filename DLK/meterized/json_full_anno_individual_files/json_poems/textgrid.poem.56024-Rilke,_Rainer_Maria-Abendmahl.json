{"textgrid.poem.56024": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Abendmahl", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ewiges will zu uns. Wer hat die Wahl", "tokens": ["E\u00b7wi\u00b7ges", "will", "zu", "uns", ".", "Wer", "hat", "die", "Wahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "VMFIN", "APPR", "PPER", "$.", "PWS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "und trennt die gro\u00dfen und geringen Kr\u00e4fte?", "tokens": ["und", "trennt", "die", "gro\u00b7\u00dfen", "und", "ge\u00b7rin\u00b7gen", "Kr\u00e4f\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Erkennst du durch das D\u00e4mmern der Gesch\u00e4fte", "tokens": ["Er\u00b7kennst", "du", "durch", "das", "D\u00e4m\u00b7mern", "der", "Ge\u00b7sch\u00e4f\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "im klaren Hinterraum das Abendmahl:", "tokens": ["im", "kla\u00b7ren", "Hin\u00b7ter\u00b7raum", "das", "A\u00b7bend\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "wie sie sichs halten und wie sie sichs reichen", "tokens": ["wie", "sie", "sichs", "hal\u00b7ten", "und", "wie", "sie", "sichs", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "KON", "PWAV", "PPER", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und in der Handlung schlicht und schwer beruhn.", "tokens": ["und", "in", "der", "Hand\u00b7lung", "schlicht", "und", "schwer", "be\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus ihren H\u00e4nden heben sich die Zeichen;", "tokens": ["Aus", "ih\u00b7ren", "H\u00e4n\u00b7den", "he\u00b7ben", "sich", "die", "Zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sie wissen nicht, da\u00df sie sie tun", "tokens": ["sie", "wis\u00b7sen", "nicht", ",", "da\u00df", "sie", "sie", "tun"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "und immer neu mit irgendwelchen Worten", "tokens": ["und", "im\u00b7mer", "neu", "mit", "ir\u00b7gend\u00b7wel\u00b7chen", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "einsetzen, was man trinkt und was man teilt.", "tokens": ["ein\u00b7set\u00b7zen", ",", "was", "man", "trinkt", "und", "was", "man", "teilt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIZU", "$,", "PRELS", "PIS", "VVFIN", "KON", "PWS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.