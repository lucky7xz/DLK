{"textgrid.poem.53850": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Frieren und frieren lassen", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In England streiken die Bergarbeiter.", "tokens": ["In", "En\u00b7gland", "strei\u00b7ken", "die", "Ber\u00b7gar\u00b7bei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Na, was ist da weiter?", "tokens": ["Na", ",", "was", "ist", "da", "wei\u00b7ter", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da m\u00fcssen die Engl\u00e4nder ihre Kohlen", "tokens": ["Da", "m\u00fcs\u00b7sen", "die", "En\u00b7gl\u00e4n\u00b7der", "ih\u00b7re", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "eben bei uns bezohlen!", "tokens": ["e\u00b7ben", "bei", "uns", "be\u00b7zoh\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Das ist ein altes Gesetz in der Welt:", "tokens": ["Das", "ist", "ein", "al\u00b7tes", "Ge\u00b7setz", "in", "der", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Deutschland dient andern gerne. F\u00fcr Geld.", "tokens": ["Deutschland", "dient", "an\u00b7dern", "ger\u00b7ne", ".", "F\u00fcr", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "$.", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Was aber jene betrifft, die Briten:", "tokens": ["Was", "a\u00b7ber", "je\u00b7ne", "be\u00b7tr\u00b7ifft", ",", "die", "Bri\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "PDS", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "die sitzen inmitten", "tokens": ["die", "sit\u00b7zen", "in\u00b7mit\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "ihrer Zimmer an einem leeren Kamin,", "tokens": ["ih\u00b7rer", "Zim\u00b7mer", "an", "ei\u00b7nem", "lee\u00b7ren", "Ka\u00b7min", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.4": {"text": "lesen ein liebliches Magazin . . .", "tokens": ["le\u00b7sen", "ein", "lieb\u00b7li\u00b7ches", "Ma\u00b7ga\u00b7zin", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$.", "$.", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Nachgeben? Mit denen da parlamentieren?", "tokens": ["Nach\u00b7ge\u00b7ben", "?", "Mit", "de\u00b7nen", "da", "par\u00b7la\u00b7men\u00b7tie\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PRELS", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Lieber frieren.", "tokens": ["Lie\u00b7ber", "frie\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Die Minister, aus englischem Pfeifenholz,", "tokens": ["Die", "Mi\u00b7nis\u00b7ter", ",", "aus", "eng\u00b7li\u00b7schem", "Pfei\u00b7fen\u00b7holz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.2": {"text": "bleiben stolz,", "tokens": ["blei\u00b7ben", "stolz", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Erst der Staat. Dann die Wirtschaft. Dann der Profit.", "tokens": ["Erst", "der", "Staat", ".", "Dann", "die", "Wirt\u00b7schaft", ".", "Dann", "der", "Pro\u00b7fit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ADV", "ART", "NN", "$.", "ADV", "ART", "NN", "$."], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wer streikt, ist ein Kommunistenbandit.", "tokens": ["Wer", "streikt", ",", "ist", "ein", "Kom\u00b7mu\u00b7nis\u00b7ten\u00b7ban\u00b7dit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+---", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf die Bergherren dr\u00fccken? Kapitulieren?", "tokens": ["Auf", "die", "Berg\u00b7her\u00b7ren", "dr\u00fc\u00b7cken", "?", "Ka\u00b7pi\u00b7tu\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$.", "NN", "$."], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Lieber frieren.", "tokens": ["Lie\u00b7ber", "frie\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Die Arbeiter hungern. Kinder weinen.", "tokens": ["Die", "Ar\u00b7bei\u00b7ter", "hun\u00b7gern", ".", "Kin\u00b7der", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mutter hat Wasser f\u00fcr die Kleinen.", "tokens": ["Mut\u00b7ter", "hat", "Was\u00b7ser", "f\u00fcr", "die", "Klei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nagt selbst an den N\u00e4geln. Schnupft auf. Wartet geduldig.", "tokens": ["Nagt", "selbst", "an", "den", "N\u00e4\u00b7geln", ".", "Schnupft", "auf", ".", "War\u00b7tet", "ge\u00b7dul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$.", "NN", "PTKVZ", "$.", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Monat f\u00fcr Monat. Bleibt Miete schuldig.", "tokens": ["Mo\u00b7nat", "f\u00fcr", "Mo\u00b7nat", ".", "Bleibt", "Mie\u00b7te", "schul\u00b7dig", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$.", "VVFIN", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Liegt auf der Stra\u00dfe . . . \u00dcberlegt . . .", "tokens": ["Liegt", "auf", "der", "Stra\u00b7\u00dfe", ".", ".", ".", "\u00dc\u00b7ber\u00b7legt", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "$.", "$.", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da! Polizei kommt angefegt.", "tokens": ["Da", "!", "Po\u00b7li\u00b7zei", "kommt", "an\u00b7ge\u00b7fegt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "\u00bbstra\u00dfe frei!\u00ab \u00dcberlegung vorbei!", "tokens": ["\u00bb", "stra\u00b7\u00dfe", "frei", "!", "\u00ab", "\u00dc\u00b7ber\u00b7le\u00b7gung", "vor\u00b7bei", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$.", "$(", "NN", "PTKVZ", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.8": {"text": "Nieder mit der Streikbrecherei!", "tokens": ["Nie\u00b7der", "mit", "der", "Streik\u00b7bre\u00b7che\u00b7rei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.9": {"text": "Nachgeben? Zehn Stunden am Tag? Desertieren?", "tokens": ["Nach\u00b7ge\u00b7ben", "?", "Zehn", "Stun\u00b7den", "am", "Tag", "?", "De\u00b7ser\u00b7tie\u00b7ren", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "CARD", "NN", "APPRART", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Lieber hungern und frieren.", "tokens": ["Lie\u00b7ber", "hun\u00b7gern", "und", "frie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.5": {"line.1": {"text": "So geht das seit einem halben Jahr.", "tokens": ["So", "geht", "das", "seit", "ei\u00b7nem", "hal\u00b7ben", "Jahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jetzt wird es kalt. Dezember, Januar . . .", "tokens": ["Jetzt", "wird", "es", "kalt", ".", "De\u00b7zem\u00b7ber", ",", "Ja\u00b7nu\u00b7ar", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "NN", "$,", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn dabei Tausende zu Grunde gehn:", "tokens": ["Und", "wenn", "da\u00b7bei", "Tau\u00b7sen\u00b7de", "zu", "Grun\u00b7de", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PAV", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das ist Ordnung. Das mu\u00df bestehn.", "tokens": ["Das", "ist", "Ord\u00b7nung", ".", "Das", "mu\u00df", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$.", "PDS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Licht im Ofen und Licht in Kaminen", "tokens": ["Licht", "im", "O\u00b7fen", "und", "Licht", "in", "Ka\u00b7mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "KON", "NN", "APPR", "NN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "kann nur scheinen, wenn welche verdienen \u2013", "tokens": ["kann", "nur", "schei\u00b7nen", ",", "wenn", "wel\u00b7che", "ver\u00b7die\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVFIN", "$,", "KOUS", "PIS", "VVINF", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "W\u00e4rme in Stube und W\u00e4rme im Saal:", "tokens": ["W\u00e4r\u00b7me", "in", "Stu\u00b7be", "und", "W\u00e4r\u00b7me", "im", "Saal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "nur mit Prozenten f\u00fcrs Kapital!", "tokens": ["nur", "mit", "Pro\u00b7zen\u00b7ten", "f\u00fcrs", "Ka\u00b7pi\u00b7tal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Oben: Behaglich durchw\u00e4rmte Zimmer.", "tokens": ["O\u00b7ben", ":", "Be\u00b7hag\u00b7lich", "durch\u00b7w\u00e4rm\u00b7te", "Zim\u00b7mer", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJD", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Oben: Blitzender Lampenschimmer.", "tokens": ["O\u00b7ben", ":", "Blit\u00b7zen\u00b7der", "Lam\u00b7pen\u00b7schim\u00b7mer", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$.", "ADJA", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Toast-R\u00f6ster, Hei\u00dfluft-Ondulieren.", "tokens": ["Toast\u00b7R\u00f6s\u00b7ter", ",", "Hei\u00df\u00b7luft\u00b7On\u00b7du\u00b7lie\u00b7ren", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Unten:", "tokens": ["Un\u00b7ten", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Frieren.", "tokens": ["Frie\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "