{"dta.poem.19111": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "XcIII.   Auf eben demselben. Ubersetzung ei-  \n nes Schreibens der   XXIX.   Frau Gr\u00e4-  \n fin zu Ebersdorf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin ungemein erfreut, da\u00df es unser HErr gef\u00fcget,", "tokens": ["Ich", "bin", "un\u00b7ge\u00b7mein", "er\u00b7freut", ",", "da\u00df", "es", "un\u00b7ser", "Herr", "ge\u00b7f\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Da\u00df ich gegenw\u00e4rtig bin, da dich dieser Tag vergn\u00fcget.", "tokens": ["Da\u00df", "ich", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "bin", ",", "da", "dich", "die\u00b7ser", "Tag", "ver\u00b7gn\u00fc\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Allertheurste Hertzens-Schwester! drey\u00dfig Jahre sind\nzur\u00fcck;", "tokens": ["Al\u00b7ler\u00b7the\u00b7urs\u00b7te", "Hert\u00b7zens\u00b7Schwes\u00b7ter", "!", "drey\u00b7\u00dfig", "Jah\u00b7re", "sind", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "CARD", "NN", "VAFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und es strahlt noch diesen Abend Christi erster Gnaden-Blick.", "tokens": ["Und", "es", "strahlt", "noch", "die\u00b7sen", "A\u00b7bend", "Chris\u00b7ti", "ers\u00b7ter", "Gna\u00b7den\u00b7Blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PDAT", "NN", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Billig ruft dein froher Mund: Solt ich meinen GOtt nicht", "tokens": ["Bil\u00b7lig", "ruft", "dein", "fro\u00b7her", "Mund", ":", "Solt", "ich", "mei\u00b7nen", "Gott", "nicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "NN", "$.", "VMFIN", "PPER", "PPOSAT", "NN", "PTKNEG"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Denn ich sehe, wie ers meynt, er ist treu in all en Dingen:", "tokens": ["Denn", "ich", "se\u00b7he", ",", "wie", "ers", "meynt", ",", "er", "ist", "treu", "in", "all", "en", "Din\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "ADJD", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Meiner Seelen Wohlergehen hat er seliglich bedacht.", "tokens": ["Mei\u00b7ner", "See\u00b7len", "Woh\u00b7ler\u00b7ge\u00b7hen", "hat", "er", "se\u00b7lig\u00b7lich", "be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Will dem Leibe Noth zu stehen, nimmt ers gleichfalls gut in acht.", "tokens": ["Will", "dem", "Lei\u00b7be", "Noth", "zu", "ste\u00b7hen", ",", "nimmt", "ers", "gleich\u00b7falls", "gut", "in", "acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,", "VVFIN", "PIS", "ADV", "ADJD", "APPR", "CARD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Wenn ich nichts mehr machen kan, aus gewissen Unverm\u00f6gen,", "tokens": ["Wenn", "ich", "nichts", "mehr", "ma\u00b7chen", "kan", ",", "aus", "ge\u00b7wis\u00b7sen", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADV", "VVINF", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Kommt mein GOtt und hebt mir an sein Verm\u00f6gen bey zulegen:", "tokens": ["Kommt", "mein", "Gott", "und", "hebt", "mir", "an", "sein", "Ver\u00b7m\u00f6\u00b7gen", "bey", "zu\u00b7le\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "APPR", "VVINF", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Dieses hast du j\u00fcngst erfahren. O wohlan! erwecke dich,", "tokens": ["Die\u00b7ses", "hast", "du", "j\u00fcngst", "er\u00b7fah\u00b7ren", ".", "O", "wo\u00b7hlan", "!", "er\u00b7we\u00b7cke", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVINF", "$.", "NE", "ADV", "$.", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Lobe diesen m\u00e4chtgen Konig, diesen Guten, inniglich.", "tokens": ["Lo\u00b7be", "die\u00b7sen", "m\u00e4cht\u00b7gen", "Ko\u00b7nig", ",", "die\u00b7sen", "Gu\u00b7ten", ",", "in\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$,", "PDAT", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Ruhe! denn es w\u00e4re ja nicht erlaubet mehr zu sorgen,", "tokens": ["Ru\u00b7he", "!", "denn", "es", "w\u00e4\u00b7re", "ja", "nicht", "er\u00b7lau\u00b7bet", "mehr", "zu", "sor\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPER", "VAFIN", "ADV", "PTKNEG", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Und der Zweck der Pr\u00fcffungen ist dir selber unverborgen:", "tokens": ["Und", "der", "Zweck", "der", "Pr\u00fcf\u00b7fun\u00b7gen", "ist", "dir", "sel\u00b7ber", "un\u00b7ver\u00b7bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "Die in unsre treue Liebe sinckende Gelassenheit,", "tokens": ["Die", "in", "uns\u00b7re", "treu\u00b7e", "Lie\u00b7be", "sin\u00b7cken\u00b7de", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Ist die Absicht unsers K\u00f6nigs und des Raths der Ewigkeit.", "tokens": ["Ist", "die", "Ab\u00b7sicht", "un\u00b7sers", "K\u00f6\u00b7nigs", "und", "des", "Raths", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPOSAT", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Schwester! wirf denn hinter dich, zum Beschlu\u00df der drey\u00dfig\nJahre,", "tokens": ["Schwes\u00b7ter", "!", "wirf", "denn", "hin\u00b7ter", "dich", ",", "zum", "Be\u00b7schlu\u00df", "der", "drey\u00b7\u00dfig", "Jah\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVIMP", "ADV", "APPR", "PPER", "$,", "APPRART", "NN", "ART", "CARD", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.18": {"text": "All es, was dahinten ist, und zum Ziel der Freyheit fahre,", "tokens": ["All", "es", ",", "was", "da\u00b7hin\u00b7ten", "ist", ",", "und", "zum", "Ziel", "der", "Frey\u00b7heit", "fah\u00b7re", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PRELS", "PAV", "VAFIN", "$,", "KON", "APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.19": {"text": "Und der K\u00f6nig, dem wir dienen, ruffe dir nach seiner Treu.", "tokens": ["Und", "der", "K\u00f6\u00b7nig", ",", "dem", "wir", "die\u00b7nen", ",", "ruf\u00b7fe", "dir", "nach", "sei\u00b7ner", "Treu", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Und nach meinem Wunsch entgegen: Sieh! ich mache alles neu.", "tokens": ["Und", "nach", "mei\u00b7nem", "Wunsch", "ent\u00b7ge\u00b7gen", ":", "Sieh", "!", "ich", "ma\u00b7che", "al\u00b7les", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKVZ", "$.", "NE", "$.", "PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.21": {"text": "Dieses neue Lebens-Jahr stelle dich mit neuen Kr\u00e4ften", "tokens": ["Die\u00b7ses", "neu\u00b7e", "Le\u00b7bens\u00b7Jahr", "stel\u00b7le", "dich", "mit", "neu\u00b7en", "Kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Deinem gantzen Hause dar zu den st\u00e4rcksten Lichts-Gesch\u00e4ften,", "tokens": ["Dei\u00b7nem", "gant\u00b7zen", "Hau\u00b7se", "dar", "zu", "den", "st\u00e4rcks\u00b7ten", "Lichts\u00b7Ge\u00b7sch\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.23": {"text": "Da\u00df du unsers HErrn Gewerbe treibest in und ausser dir,", "tokens": ["Da\u00df", "du", "un\u00b7sers", "Herrn", "Ge\u00b7wer\u00b7be", "trei\u00b7best", "in", "und", "aus\u00b7ser", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "VVFIN", "APPR", "KON", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Und dem grossen Chor der Schwestern, als ein Stern-Licht,", "tokens": ["Und", "dem", "gros\u00b7sen", "Chor", "der", "Schwes\u00b7tern", ",", "als", "ein", "Stern\u00b7Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "ART", "NN", "$,"], "meter": "+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.25": {"text": "Welch ein unvergleichlich Lob wird durch alle Reigen dringen,", "tokens": ["Welch", "ein", "un\u00b7ver\u00b7gleich\u00b7lich", "Lob", "wird", "durch", "al\u00b7le", "Rei\u00b7gen", "drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "ADJD", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.26": {"text": "Wenn sie ihres Meisters Ruhm \u00fcber deinem Prei\u00df besingen!", "tokens": ["Wenn", "sie", "ih\u00b7res", "Meis\u00b7ters", "Ruhm", "\u00fc\u00b7ber", "dei\u00b7nem", "Prei\u00df", "be\u00b7sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Und wie gro\u00df wird deine Freude bey der Uberlegung seyn,", "tokens": ["Und", "wie", "gro\u00df", "wird", "dei\u00b7ne", "Freu\u00b7de", "bey", "der", "U\u00b7ber\u00b7le\u00b7gung", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Einer also hochbegnadigt-Apostolischen Gemein!", "tokens": ["Ei\u00b7ner", "al\u00b7so", "hoch\u00b7be\u00b7gna\u00b7dig\u00b7tApo\u00b7sto\u00b7li\u00b7schen", "Ge\u00b7mein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.29": {"text": "Wachs in tausend tausendmal, denn du bist ja unsre Schwester,", "tokens": ["Wachs", "in", "tau\u00b7send", "tau\u00b7send\u00b7mal", ",", "denn", "du", "bist", "ja", "uns\u00b7re", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "ADV", "$,", "KON", "PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Siege deinen Feinden ob, wie die dran gewagte Esther:", "tokens": ["Sie\u00b7ge", "dei\u00b7nen", "Fein\u00b7den", "ob", ",", "wie", "die", "dran", "ge\u00b7wag\u00b7te", "E\u00b7sther", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KOUS", "$,", "PWAV", "ART", "PAV", "ADJA", "NN", "$."], "meter": "--+-+------+--+", "measure": "iambic.tetra.chol"}, "line.31": {"text": "Weil die Schwester-Liebe brennet, o! so brich herf\u00fcr in Kraft,", "tokens": ["Weil", "die", "Schwes\u00b7ter\u00b7Lie\u00b7be", "bren\u00b7net", ",", "o", "!", "so", "brich", "her\u00b7f\u00fcr", "in", "Kraft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "FM", "$.", "ADV", "ADJD", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.32": {"text": "Weil dir Thor und Th\u00fcren offen, zeuge, was das Beugen schaft.", "tokens": ["Weil", "dir", "Thor", "und", "Th\u00fc\u00b7ren", "of\u00b7fen", ",", "zeu\u00b7ge", ",", "was", "das", "Beu\u00b7gen", "schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "PTKVZ", "$,", "VVFIN", "$,", "PRELS", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.33": {"text": "Ich gesteh es \u00f6ffentlich, da\u00df ich in der Creutz-Gemeine", "tokens": ["Ich", "ge\u00b7steh", "es", "\u00f6f\u00b7fent\u00b7lich", ",", "da\u00df", "ich", "in", "der", "Creutz\u00b7Ge\u00b7mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}, "line.34": {"text": "Gerne als die Niedrigste vor des Leibes Haupt erscheine,", "tokens": ["Ger\u00b7ne", "als", "die", "Nied\u00b7rigs\u00b7te", "vor", "des", "Lei\u00b7bes", "Haupt", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.35": {"text": "Da\u00df mir Leut und Land beherrschen lange nicht so n\u00f6thig thut.", "tokens": ["Da\u00df", "mir", "Leut", "und", "Land", "be\u00b7herr\u00b7schen", "lan\u00b7ge", "nicht", "so", "n\u00f6\u00b7thig", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "ADV", "PTKNEG", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.36": {"text": "Als der Gnaden-Zucht gehorchen in dem Kirchlein Herrenhut.", "tokens": ["Als", "der", "Gna\u00b7den\u00b7Zucht", "ge\u00b7hor\u00b7chen", "in", "dem", "Kirch\u00b7lein", "Her\u00b7ren\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.37": {"text":