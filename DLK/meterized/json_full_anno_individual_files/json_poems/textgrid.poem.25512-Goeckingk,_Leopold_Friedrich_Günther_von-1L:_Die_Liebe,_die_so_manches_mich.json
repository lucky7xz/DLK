{"textgrid.poem.25512": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Liebe, die so manches mich", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Liebe, die so manches mich", "tokens": ["Die", "Lie\u00b7be", ",", "die", "so", "man\u00b7ches", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gelehrt, lehrt andre wieder.", "tokens": ["Ge\u00b7lehrt", ",", "lehrt", "and\u00b7re", "wie\u00b7der", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu ganzen Stunden setz' ich mich", "tokens": ["Zu", "gan\u00b7zen", "Stun\u00b7den", "setz'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Papchens K\u00e4fich nieder,", "tokens": ["An", "Pap\u00b7chens", "K\u00e4\u00b7fich", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und ruf' ihm zu, als s\u00e4h' ich Land:", "tokens": ["Und", "ruf'", "ihm", "zu", ",", "als", "s\u00e4h'", "ich", "Land", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Amarant!", "tokens": ["A\u00b7ma\u00b7rant", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Zwei Sylben hat er erst gewagt,", "tokens": ["Zwei", "Syl\u00b7ben", "hat", "er", "erst", "ge\u00b7wagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So sehr ich ihn auch \u00fcbe;", "tokens": ["So", "sehr", "ich", "ihn", "auch", "\u00fc\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Doch da mein Vetter Fritz mir sagt:", "tokens": ["Doch", "da", "mein", "Vet\u00b7ter", "Fritz", "mir", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "NE", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.11": {"text": "So antwort' ich, von Lieb' entbrannt:", "tokens": ["So", "ant\u00b7wort'", "ich", ",", "von", "Lieb'", "ent\u00b7brannt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Amarant!", "tokens": ["A\u00b7ma\u00b7rant", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die Liebe, die so manches mich", "tokens": ["Die", "Lie\u00b7be", ",", "die", "so", "man\u00b7ches", "mich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIS", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.