{"dta.poem.20298": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Als Flavia auff das land reisete.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eilstu denn/ Flavia/ so schleunig auff das land?", "tokens": ["Eils\u00b7tu", "denn", "/", "Fla\u00b7via", "/", "so", "schleu\u00b7nig", "auff", "das", "land", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$(", "NE", "$(", "ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Wilstu zu dieser zeit dich in die rosen setzen?", "tokens": ["Wils\u00b7tu", "zu", "die\u00b7ser", "zeit", "dich", "in", "die", "ro\u00b7sen", "set\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PDAT", "NN", "PRF", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So h\u00fcte dich doch auch/ da\u00df deine sch\u00f6ne hand", "tokens": ["So", "h\u00fc\u00b7te", "dich", "doch", "auch", "/", "da\u00df", "dei\u00b7ne", "sch\u00f6\u00b7ne", "hand"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der rosen nachbarin/ die dornen/ nicht verletzen.", "tokens": ["Der", "ro\u00b7sen", "nach\u00b7ba\u00b7rin", "/", "die", "dor\u00b7nen", "/", "nicht", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "NN", "$(", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Flora/ die dich nicht genugsam r\u00fchmen kan/", "tokens": ["Die", "Flo\u00b7ra", "/", "die", "dich", "nicht", "ge\u00b7nug\u00b7sam", "r\u00fch\u00b7men", "kan", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "PPER", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wirfft ihre lieblichkeit zu deinen zarten f\u00fcssen/", "tokens": ["Wirfft", "ih\u00b7re", "lieb\u00b7lich\u00b7keit", "zu", "dei\u00b7nen", "zar\u00b7ten", "f\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie legt mit eigner hand dir einen s\u00e4ssel an/", "tokens": ["Sie", "legt", "mit", "eig\u00b7ner", "hand", "dir", "ei\u00b7nen", "s\u00e4s\u00b7sel", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und w\u00fcnschet nichts so sehr als deinen mund zu k\u00fcssen.", "tokens": ["Und", "w\u00fcn\u00b7schet", "nichts", "so", "sehr", "als", "dei\u00b7nen", "mund", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ADV", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie spricht: Ich bin besch\u00e4mt da\u00df mein bebl\u00fcmtes reich", "tokens": ["Sie", "spricht", ":", "Ich", "bin", "be\u00b7sch\u00e4mt", "da\u00df", "mein", "be\u00b7bl\u00fcm\u00b7tes", "reich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "KOUS", "PPOSAT", "ADJA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "F\u00fcr deiner glieder pracht mu\u00df auff die seite weichen;", "tokens": ["F\u00fcr", "dei\u00b7ner", "glie\u00b7der", "pracht", "mu\u00df", "auff", "die", "sei\u00b7te", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die rosen werden hier von deinen lippen bleich/", "tokens": ["Die", "ro\u00b7sen", "wer\u00b7den", "hier", "von", "dei\u00b7nen", "lip\u00b7pen", "bleich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dir mu\u00df die lilie den weissen scepter reichen.", "tokens": ["Dir", "mu\u00df", "die", "li\u00b7lie", "den", "weis\u00b7sen", "scep\u00b7ter", "rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.13": {"text": "Ja selber der Jasmin reucht nicht so gut wie du.", "tokens": ["Ja", "sel\u00b7ber", "der", "Jas\u00b7min", "reucht", "nicht", "so", "gut", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "VVFIN", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der weisse Isabell l\u00e4st sich dein haar vertreiben/", "tokens": ["Der", "weis\u00b7se", "I\u00b7sa\u00b7bell", "l\u00e4st", "sich", "dein", "haar", "ver\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PRF", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und iede blume rufft dir/ sch\u00f6nste blume/ zu:", "tokens": ["Und", "ie\u00b7de", "blu\u00b7me", "rufft", "dir", "/", "sch\u00f6ns\u00b7te", "blu\u00b7me", "/", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PPER", "$(", "ADJA", "NN", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Wir blumen/ wir vergehn/ du blume kanst verbleiben.", "tokens": ["Wir", "blu\u00b7men", "/", "wir", "ver\u00b7gehn", "/", "du", "blu\u00b7me", "kanst", "ver\u00b7blei\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVINF", "$(", "PPER", "VVFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}