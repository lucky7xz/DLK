{"dta.poem.5478": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Erkl\u00e4rung des Vater Unsers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wol der Anfang, als der Schlu\u00df", "tokens": ["So", "wol", "der", "An\u00b7fang", ",", "als", "der", "Schlu\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des ", "tokens": ["Des"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Da\u00df, auch im Beten, jedermann", "tokens": ["Da\u00df", ",", "auch", "im", "Be\u00b7ten", ",", "je\u00b7der\u00b7mann"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "$,", "ADV", "APPRART", "NN", "$,", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf die Verherrlichung des Sch\u00f6pfers gehen mu\u00df.", "tokens": ["Auf", "die", "Ver\u00b7herr\u00b7li\u00b7chung", "des", "Sch\u00f6p\u00b7fers", "ge\u00b7hen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es giebet Christus selbst den deutlichsten Bericht,", "tokens": ["Es", "gie\u00b7bet", "Chris\u00b7tus", "selbst", "den", "deut\u00b7lichs\u00b7ten", "Be\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Der je davon zu unsrer Kundschaft kame,", "tokens": ["Der", "je", "da\u00b7von", "zu", "uns\u00b7rer", "Kund\u00b7schaft", "ka\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PAV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn er zu Anfang: ", "tokens": ["Wenn", "er", "zu", "An\u00b7fang", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie kann nun GOttes Nahm\u2019 auf Erden", "tokens": ["Wie", "kann", "nun", "Got\u00b7tes", "Nahm'", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Von uns doch mehr geheiligt werden,", "tokens": ["Von", "uns", "doch", "mehr", "ge\u00b7hei\u00b7ligt", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wenn wir, wie sein Werck so sch\u00f6n,", "tokens": ["Als", "wenn", "wir", ",", "wie", "sein", "Werck", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "$,", "PWAV", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "In fr\u00f6licher Betrachtung, sehn?", "tokens": ["In", "fr\u00f6\u00b7li\u00b7cher", "Be\u00b7trach\u00b7tung", ",", "sehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn wir, in seinem Werck, mit Lust, sein Lob erh\u00f6hn.", "tokens": ["Wenn", "wir", ",", "in", "sei\u00b7nem", "Werck", ",", "mit", "Lust", ",", "sein", "Lob", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,", "APPR", "NN", "$,", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auch ", "tokens": ["Auch"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Und wird, aus weiser Huld, von ihm hervorgebracht.", "tokens": ["Und", "wird", ",", "aus", "wei\u00b7ser", "Huld", ",", "von", "ihm", "her\u00b7vor\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "APPR", "ADJA", "NN", "$,", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wenn Christus das Gebeth nun endlich schleu\u00dft;", "tokens": ["Wenn", "Chris\u00b7tus", "das", "Ge\u00b7beth", "nun", "end\u00b7lich", "schleu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "So finden wir noch mehr: indem es hei\u00dft:", "tokens": ["So", "fin\u00b7den", "wir", "noch", "mehr", ":", "in\u00b7dem", "es", "hei\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$.", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Der HErr noch weiter fort,", "tokens": ["Der", "Herr", "noch", "wei\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Und hei\u00dft in Lilien, da sie so sch\u00f6n,", "tokens": ["Und", "hei\u00dft", "in", "Li\u00b7li\u00b7en", ",", "da", "sie", "so", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "An V\u00f6geln und am Gras\u2019 uns GOttes Allmacht sehn.", "tokens": ["An", "V\u00f6\u00b7geln", "und", "am", "Gras'", "uns", "Got\u00b7tes", "All\u00b7macht", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPRART", "NN", "PPER", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach, warum nehmen wir denn nicht", "tokens": ["Ach", ",", "wa\u00b7rum", "neh\u00b7men", "wir", "denn", "nicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PWAV", "VVFIN", "PPER", "ADV", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die, durch Natur und Schrift uns eingepr\u00e4gte, Pflicht", "tokens": ["Die", ",", "durch", "Na\u00b7tur", "und", "Schrift", "uns", "ein\u00b7ge\u00b7pr\u00e4g\u00b7te", ",", "Pflicht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "$,", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit mehrerm Ernst in acht?", "tokens": ["Mit", "meh\u00b7rerm", "Ernst", "in", "acht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "APPR", "CARD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf, la\u00dft uns \u00fcberall in GOttes Wercken,", "tokens": ["Auf", ",", "la\u00dft", "uns", "\u00fc\u00b7be\u00b7rall", "in", "Got\u00b7tes", "Wer\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "VVIMP", "PPER", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Mit Andacht, Lust und Ehrfurcht, mercken", "tokens": ["Mit", "An\u00b7dacht", ",", "Lust", "und", "Ehr\u00b7furcht", ",", "mer\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Des Sch\u00f6pfers ", "tokens": ["Des", "Sch\u00f6p\u00b7fers"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}}}}}