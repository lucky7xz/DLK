V", "$.", "$(", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von deinem g\u00f6ttlichen Beethoven!", "tokens": ["Von", "dei\u00b7nem", "g\u00f6tt\u00b7li\u00b7chen", "Beet\u00b7ho\u00b7ven", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df ich auch Banner fliegen nicht,", "tokens": ["La\u00df", "ich", "auch", "Ban\u00b7ner", "flie\u00b7gen", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df ich doch fliegen zorn'ge Strophen!", "tokens": ["La\u00df", "ich", "doch", "flie\u00b7gen", "zorn'\u00b7ge", "Stro\u00b7phen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "VVINF", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das ist die rechte Feldmusik,", "tokens": ["Das", "ist", "die", "rech\u00b7te", "Feld\u00b7mu\u00b7sik", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Geht ein Poet der Welt zu Leibe:", "tokens": ["Geht", "ein", "Po\u00b7et", "der", "Welt", "zu", "Lei\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Am eignen Herd ein mutig St\u00fcck,", "tokens": ["Am", "eig\u00b7nen", "Herd", "ein", "mu\u00b7tig", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gespielt von seinem lieben Weibe!", "tokens": ["Ge\u00b7spielt", "von", "sei\u00b7nem", "lie\u00b7ben", "Wei\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcllt k\u00fchnes Klingen ihm das Haus,", "tokens": ["F\u00fcllt", "k\u00fch\u00b7nes", "Klin\u00b7gen", "ihm", "das", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dann singt er doppelt freud'gen Schalles", "tokens": ["Dann", "singt", "er", "dop\u00b7pelt", "freu\u00b7d'\u00b7gen", "Schal\u00b7les"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In Wetter und in Sturm hinaus:", "tokens": ["In", "Wet\u00b7ter", "und", "in", "Sturm", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Deutschland und Freiheit \u00fcber alles!", "tokens": ["Deutschland", "und", "Frei\u00b7heit", "\u00fc\u00b7ber", "al\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "APPR", "PIS", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}