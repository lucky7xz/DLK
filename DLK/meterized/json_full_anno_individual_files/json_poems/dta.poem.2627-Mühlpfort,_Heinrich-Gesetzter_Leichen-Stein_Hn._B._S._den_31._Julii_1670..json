{"dta.poem.2627": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gesetzter Leichen-Stein  \n Hn. B. S. den 31. Julii 1670.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Weil unser Fleisch und Blut sind Kleider die verwesen/", "tokens": ["Weil", "un\u00b7ser", "Fleisch", "und", "Blut", "sind", "Klei\u00b7der", "die", "ver\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "VAFIN", "NN", "ART", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieses irrdne Haus verf\u00e4llt in seinen Sand/", "tokens": ["Und", "die\u00b7ses", "irrd\u00b7ne", "Haus", "ver\u00b7f\u00e4llt", "in", "sei\u00b7nen", "Sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So lie\u00df die kluge Welt noch ihr Ged\u00e4chtn\u00fc\u00df lesen/", "tokens": ["So", "lie\u00df", "die", "klu\u00b7ge", "Welt", "noch", "ihr", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und hat den gr\u00f6sten Flei\u00df auff Gr\u00e4bern angewand.", "tokens": ["Und", "hat", "den", "gr\u00f6s\u00b7ten", "Flei\u00df", "auff", "Gr\u00e4\u00b7bern", "an\u00b7ge\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rom wolt\u2019 ein ewig Licht den Abgelebten brennen/", "tokens": ["Rom", "wolt'", "ein", "e\u00b7wig", "Licht", "den", "Ab\u00b7ge\u00b7leb\u00b7ten", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "ART", "ADJD", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aegypten f\u00fchrte Th\u00fcrm/ und hohe S\u00e4ulen auff;", "tokens": ["A\u00b7e\u00b7gyp\u00b7ten", "f\u00fchr\u00b7te", "Th\u00fcrm", "/", "und", "ho\u00b7he", "S\u00e4u\u00b7len", "auff", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$(", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Viel gaben die Begier durch Tempel zu erkennen/", "tokens": ["Viel", "ga\u00b7ben", "die", "Be\u00b7gier", "durch", "Tem\u00b7pel", "zu", "er\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In welchen einverleibt der Todten Lebens-Lauff.", "tokens": ["In", "wel\u00b7chen", "ein\u00b7ver\u00b7leibt", "der", "Tod\u00b7ten", "Le\u00b7bens\u00b7Lauff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es muste Morgenland den besten Balsam schicken/", "tokens": ["Es", "mus\u00b7te", "Mor\u00b7gen\u00b7land", "den", "bes\u00b7ten", "Bal\u00b7sam", "schi\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und angenehmes Oel/ sie mit zu salben ein.", "tokens": ["Und", "an\u00b7ge\u00b7neh\u00b7mes", "O\u00b7el", "/", "sie", "mit", "zu", "sal\u00b7ben", "ein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "PPER", "APPR", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Der letzte Wille hie\u00df das Grab mit Flei\u00df zu schm\u00fccken/", "tokens": ["Der", "letz\u00b7te", "Wil\u00b7le", "hie\u00df", "das", "Grab", "mit", "Flei\u00df", "zu", "schm\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und es mit Lilien und Rosen zu bestreun.", "tokens": ["Und", "es", "mit", "Li\u00b7li\u00b7en", "und", "Ro\u00b7sen", "zu", "be\u00b7streun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es stunden Redner auff/ die mit beredter Zungen", "tokens": ["Es", "stun\u00b7den", "Red\u00b7ner", "auff", "/", "die", "mit", "be\u00b7red\u00b7ter", "Zun\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "APPR", "$(", "ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der grimmen Sterbligkeit entgegen sich gesetzt/", "tokens": ["Der", "grim\u00b7men", "Ster\u00b7blig\u00b7keit", "ent\u00b7ge\u00b7gen", "sich", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Es ward des Todten Ruhm durch Lieder abgesungen/", "tokens": ["Es", "ward", "des", "Tod\u00b7ten", "Ruhm", "durch", "Lie\u00b7der", "ab\u00b7ge\u00b7sun\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der besten Thaten Ruff in Stein und Stahl geetzt.", "tokens": ["Der", "bes\u00b7ten", "Tha\u00b7ten", "Ruff", "in", "Stein", "und", "Stahl", "ge\u00b7etzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Ich wil den grauen Mund der alten Zeit nicht fragen/", "tokens": ["Ich", "wil", "den", "grau\u00b7en", "Mund", "der", "al\u00b7ten", "Zeit", "nicht", "fra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Noch frembder V\u00f6lcker Recht und Br\u00e4uche f\u00fchren an;", "tokens": ["Noch", "fremb\u00b7der", "V\u00f6l\u00b7cker", "Recht", "und", "Br\u00e4u\u00b7che", "f\u00fch\u00b7ren", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NN", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Denn wo wir mehr zu sehn nur ein Verlangen tragen/", "tokens": ["Denn", "wo", "wir", "mehr", "zu", "sehn", "nur", "ein", "Ver\u00b7lan\u00b7gen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "PTKZU", "VVINF", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So ist der ", "tokens": ["So", "ist", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Jhr Eifer/ der sie trieb die Gr\u00e4ber auffzubauen/", "tokens": ["Ihr", "Ei\u00b7fer", "/", "der", "sie", "trieb", "die", "Gr\u00e4\u00b7ber", "auff\u00b7zu\u00b7bau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Liebe so sie auch die Steine legen hie\u00df/", "tokens": ["Die", "Lie\u00b7be", "so", "sie", "auch", "die", "Stei\u00b7ne", "le\u00b7gen", "hie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "ADV", "ART", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und drein mit kluger Kunst ein Angedencken hauen/", "tokens": ["Und", "drein", "mit", "klu\u00b7ger", "Kunst", "ein", "An\u00b7ge\u00b7den\u00b7cken", "hau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "So voller Wei\u00dfheit war/ di\u00df macht uns schon gewi\u00df.", "tokens": ["So", "vol\u00b7ler", "Wei\u00df\u00b7heit", "war", "/", "di\u00df", "macht", "uns", "schon", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VAFIN", "$(", "PDS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und einen grossen Ruhm begriffen kurtze Zeilen/", "tokens": ["Und", "ei\u00b7nen", "gros\u00b7sen", "Ruhm", "be\u00b7grif\u00b7fen", "kurt\u00b7ze", "Zei\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVPP", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das Schwerdt wormit sie offt den stoltzen Feind erlegt/", "tokens": ["Das", "Schwerdt", "wor\u00b7mit", "sie", "offt", "den", "stolt\u00b7zen", "Feind", "er\u00b7legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAV", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Hing an dem Leichen-Stein/ als wie an Ehren-S\u00e4ulen/", "tokens": ["Hing", "an", "dem", "Lei\u00b7chen\u00b7Stein", "/", "als", "wie", "an", "Eh\u00b7ren\u00b7S\u00e4u\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$(", "KOUS", "KOKOM", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "In die der Helden Lob und Thaten eingepr\u00e4gt.", "tokens": ["In", "die", "der", "Hel\u00b7den", "Lob", "und", "Tha\u00b7ten", "ein\u00b7ge\u00b7pr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auff diese Weise nun blieb feurigen Gem\u00fcthern", "tokens": ["Auff", "die\u00b7se", "Wei\u00b7se", "nun", "blieb", "feu\u00b7ri\u00b7gen", "Ge\u00b7m\u00fc\u00b7thern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Des Namens Ewigkeit zu stifften eingepflantzt/", "tokens": ["Des", "Na\u00b7mens", "E\u00b7wig\u00b7keit", "zu", "stiff\u00b7ten", "ein\u00b7ge\u00b7pflantzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Sie sahn den Untergang von allen irrd\u2019 schen G\u00fctern/", "tokens": ["Sie", "sahn", "den", "Un\u00b7ter\u00b7gang", "von", "al\u00b7len", "irrd'", "schen", "G\u00fc\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und da\u00df des Menschen Thun mit Noth und Tod umbschantzt.", "tokens": ["Und", "da\u00df", "des", "Men\u00b7schen", "Thun", "mit", "Noth", "und", "Tod", "umbsc\u00b7hantzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Allein die Eitelkeit hat Meister hier gespielet/", "tokens": ["Al\u00b7lein", "die", "Ei\u00b7tel\u00b7keit", "hat", "Meis\u00b7ter", "hier", "ge\u00b7spie\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vergessenheit nnd Neid was reissen sie nicht ein?", "tokens": ["Ver\u00b7ges\u00b7sen\u00b7heit", "nnd", "Neid", "was", "reis\u00b7sen", "sie", "nicht", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PWS", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was hat die Tyranney der Zeiten nicht durchw\u00fchlet/", "tokens": ["Was", "hat", "die", "Ty\u00b7ran\u00b7ney", "der", "Zei\u00b7ten", "nicht", "durch\u00b7w\u00fch\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ART", "NN", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df solche Gr\u00e4ber nun ein Hauffen Steine seyn?", "tokens": ["Da\u00df", "sol\u00b7che", "Gr\u00e4\u00b7ber", "nun", "ein", "Hauf\u00b7fen", "Stei\u00b7ne", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sol denn ein Ehren-Mahl auff dieser Welt uns bleiben/", "tokens": ["Sol", "denn", "ein", "Eh\u00b7ren\u00b7Mahl", "auff", "die\u00b7ser", "Welt", "uns", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "PDAT", "NN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das nicht gebrechlich ist/ und so/ wie wir/ vergeht/", "tokens": ["Das", "nicht", "ge\u00b7brech\u00b7lich", "ist", "/", "und", "so", "/", "wie", "wir", "/", "ver\u00b7geht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADJD", "VAFIN", "$(", "KON", "ADV", "$(", "PWAV", "PPER", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "So mu\u00df die Tugend uns die Grabe-Zeilen schreiben/", "tokens": ["So", "mu\u00df", "die", "Tu\u00b7gend", "uns", "die", "Gra\u00b7be\u00b7Zei\u00b7len", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die ists/ wenn gleich die Welt wird brennen/ so besteht.", "tokens": ["Die", "ists", "/", "wenn", "gleich", "die", "Welt", "wird", "bren\u00b7nen", "/", "so", "be\u00b7steht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "$(", "KOUS", "ADV", "ART", "NN", "VAFIN", "VVINF", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Herr S", "tokens": ["Herr", "S"], "token_info": ["word", "word"], "pos": ["NN", "NE"], "meter": "+-", "measure": "trochaic.single"}, "line.42": {"text": "Und nach bekandtem Brauch der Erden anvertraun/", "tokens": ["Und", "nach", "be\u00b7kand\u00b7tem", "Brauch", "der", "Er\u00b7den", "an\u00b7ver\u00b7traun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Kan die gemeine Stadt/ was