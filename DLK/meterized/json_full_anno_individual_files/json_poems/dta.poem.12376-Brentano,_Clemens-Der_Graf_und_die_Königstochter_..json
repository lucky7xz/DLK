{"dta.poem.12376": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Graf und die K\u00f6nigstochter .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich k\u00f6nnt' von Herzen               ", "tokens": ["O", "da\u00df", "ich", "k\u00f6nnt'", "von", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "VMFIN", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Singen eine Tagewei\u00df,", "tokens": ["Sin\u00b7gen", "ei\u00b7ne", "Ta\u00b7ge\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von Lieb' und bittern Schmerzen!", "tokens": ["Von", "Lieb'", "und", "bit\u00b7tern", "Schmer\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Merkt auf, merkt auf mit Flei\u00df,", "tokens": ["Merkt", "auf", ",", "merkt", "auf", "mit", "Flei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie's einer K\u00f6nigstochter ging", "tokens": ["Wie's", "ei\u00b7ner", "K\u00f6\u00b7nig\u00b7stoch\u00b7ter", "ging"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit einem jungen Grafen!", "tokens": ["Mit", "ei\u00b7nem", "jun\u00b7gen", "Gra\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nun h\u00f6rt gro\u00df Wunderding!", "tokens": ["Nun", "h\u00f6rt", "gro\u00df", "Wun\u00b7der\u00b7ding", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "An ihres Vaters Tafel", "tokens": ["An", "ih\u00b7res", "Va\u00b7ters", "Ta\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sa\u00df mancher Ritter werth,", "tokens": ["Sa\u00df", "man\u00b7cher", "Rit\u00b7ter", "werth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch liebte sie den Grafen", "tokens": ["Doch", "lieb\u00b7te", "sie", "den", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor allem was auf Erd,", "tokens": ["Vor", "al\u00b7lem", "was", "auf", "Erd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PWS", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was Gott durch seine Weisheit schuf;", "tokens": ["Was", "Gott", "durch", "sei\u00b7ne", "Weis\u00b7heit", "schuf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Aus heimlichem bangem Herzen", "tokens": ["Aus", "heim\u00b7li\u00b7chem", "ban\u00b7gem", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Th\u00e4t sie so manchen Ruf.", "tokens": ["Th\u00e4t", "sie", "so", "man\u00b7chen", "Ruf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eherr Gott, send mir das Gl\u00fccke,", "tokens": ["\u201e", "herr", "Gott", ",", "send", "mir", "das", "Gl\u00fc\u00b7cke", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "KOUS", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201eda\u00df er mein Herz erkenn!", "tokens": ["\u201e", "da\u00df", "er", "mein", "Herz", "er\u00b7kenn", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u201el\u00f6s mir auf Band und Stricke", "tokens": ["\u201e", "l\u00f6s", "mir", "auf", "Band", "und", "Stri\u00b7cke"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "VVIMP", "PPER", "APPR", "NN", "KON", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "\u201eder edlen Venusin!\u201c", "tokens": ["\u201e", "der", "ed\u00b7len", "Ve\u00b7nu\u00b7sin", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und was ihr in dem Herzen lag,", "tokens": ["Und", "was", "ihr", "in", "dem", "Her\u00b7zen", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das lag wohl auch dem Grafen", "tokens": ["Das", "lag", "wohl", "auch", "dem", "Gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Im Sinn bei Nacht und Tag.", "tokens": ["Im", "Sinn", "bei", "Nacht", "und", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Keins klagt dem andern offen,", "tokens": ["Keins", "klagt", "dem", "an\u00b7dern", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was ihm am Herzen lag;", "tokens": ["Was", "ihm", "am", "Her\u00b7zen", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein jeder th\u00e4te hoffen", "tokens": ["Ein", "je\u00b7der", "th\u00e4\u00b7te", "hof\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Einen guten Freudentag,", "tokens": ["Ei\u00b7nen", "gu\u00b7ten", "Freu\u00b7den\u00b7tag", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der doch zuletzt mit Jammer kam,", "tokens": ["Der", "doch", "zu\u00b7letzt", "mit", "Jam\u00b7mer", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie schrieben sich Liebesbriefelein,", "tokens": ["Sie", "schrie\u00b7ben", "sich", "Lie\u00b7bes\u00b7brie\u00b7fel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ganz frei und ohne Scham.", "tokens": ["Ganz", "frei", "und", "oh\u00b7ne", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Darin sie sich gemeldet", "tokens": ["Da\u00b7rin", "sie", "sich", "ge\u00b7mel\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von einem Brunnen kalt,", "tokens": ["Von", "ei\u00b7nem", "Brun\u00b7nen", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der lag so weit im Felde,", "tokens": ["Der", "lag", "so", "weit", "im", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor einem gr\u00fcnen Wald,", "tokens": ["Vor", "ei\u00b7nem", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wer ehe k\u00e4m zu des Brunnens Flu\u00df,", "tokens": ["Wer", "e\u00b7he", "k\u00e4m", "zu", "des", "Brun\u00b7nens", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOUS", "NE", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der sollte des andern warten;", "tokens": ["Der", "soll\u00b7te", "des", "an\u00b7dern", "war\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Also war ihr Beschlu\u00df.", "tokens": ["Al\u00b7so", "war", "ihr", "Be\u00b7schlu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Die Jungfrau th\u00e4t sich zieren", "tokens": ["Die", "Jung\u00b7frau", "th\u00e4t", "sich", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In einen Mantel weis,", "tokens": ["In", "ei\u00b7nen", "Man\u00b7tel", "weis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihre Br\u00fcst' th\u00e4t sie einschn\u00fcren,", "tokens": ["Ih\u00b7re", "Br\u00fcst'", "th\u00e4t", "sie", "ein\u00b7schn\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vermacht mit allem Fleis", "tokens": ["Ver\u00b7macht", "mit", "al\u00b7lem", "Fleis"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PIS", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch sprach die edle Jungfrau schon:", "tokens": ["Auch", "sprach", "die", "ed\u00b7le", "Jung\u00b7frau", "schon", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201ekein Mann soll mir's aufrei\u00dfen,", "tokens": ["\u201e", "kein", "Mann", "soll", "mir's", "auf\u00b7rei\u00b7\u00dfen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201edenn eines Grafen Sohn!\u201c", "tokens": ["\u201e", "denn", "ei\u00b7nes", "Gra\u00b7fen", "Sohn", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Sie kam wohl zu dem Brunnen,", "tokens": ["Sie", "kam", "wohl", "zu", "dem", "Brun\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie fand viel Lust und Freud,", "tokens": ["Sie", "fand", "viel", "Lust", "und", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie dacht: \u201eIch hab gewonnen!", "tokens": ["Sie", "dacht", ":", "\u201e", "Ich", "hab", "ge\u00b7won\u00b7nen", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u201emein Trauern ist zerstreut,", "tokens": ["\u201e", "mein", "Trau\u00b7ern", "ist", "zer\u00b7streut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eaus aller Noth bin ich erl\u00f6\u00dft,", "tokens": ["\u201e", "aus", "al\u00b7ler", "Noth", "bin", "ich", "er\u00b7l\u00f6\u00dft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PIAT", "NN", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u201eo da\u00df ich s\u00e4h hertreten", "tokens": ["\u201e", "o", "da\u00df", "ich", "s\u00e4h", "her\u00b7tre\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "FM", "KOUS", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "\u201emein Hoffnung und mein Trost.\u201c", "tokens": ["\u201e", "mein", "Hoff\u00b7nung", "und", "mein", "Trost", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Zur Hand lief aus dem Walde,", "tokens": ["Zur", "Hand", "lief", "aus", "dem", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Eine grimme L\u00f6win her.", "tokens": ["Ei\u00b7ne", "grim\u00b7me", "L\u00f6\u00b7win", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Jungfrau sah sie balde,", "tokens": ["Die", "Jung\u00b7frau", "sah", "sie", "bal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie lief von dannen fern,", "tokens": ["Sie", "lief", "von", "dan\u00b7nen", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und kam nicht wieder denselben Tag;", "tokens": ["Und", "kam", "nicht", "wie\u00b7der", "den\u00b7sel\u00b7ben", "Tag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "PDAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ihren Mantel lie\u00df sie liegen,", "tokens": ["Ih\u00b7ren", "Man\u00b7tel", "lie\u00df", "sie", "lie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Daraus kam Noth und Klag.", "tokens": ["Da\u00b7raus", "kam", "Noth", "und", "Klag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Die L\u00f6win warf ihre Jungen", "tokens": ["Die", "L\u00f6\u00b7win", "warf", "ih\u00b7re", "Jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Wohl auf den Mantel gut,", "tokens": ["Wohl", "auf", "den", "Man\u00b7tel", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Mantel ward durchdrungen", "tokens": ["Der", "Man\u00b7tel", "ward", "durch\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Schwei\u00df und rothem Blut.", "tokens": ["Von", "Schwei\u00df", "und", "ro\u00b7them", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$."], "me