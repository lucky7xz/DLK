{"textgrid.poem.24434": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Uber Dessen Dunckelheit in vielen Uberschrifften", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bey deiner Finsterni\u00df/ die man fast greifen kan/", "tokens": ["Bey", "dei\u00b7ner", "Fins\u00b7ter\u00b7ni\u00df", "/", "die", "man", "fast", "grei\u00b7fen", "kan", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PRELS", "PIS", "ADV", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Klagst du den Leser selbst/ und seine Dummheit an.", "tokens": ["Klagst", "du", "den", "Le\u00b7ser", "selbst", "/", "und", "sei\u00b7ne", "Dumm\u00b7heit", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$(", "KON", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn dir zuweilen nun/ mein Freund/ was gutes tr\u00e4umet/", "tokens": ["Wenn", "dir", "zu\u00b7wei\u00b7len", "nun", "/", "mein", "Freund", "/", "was", "gu\u00b7tes", "tr\u00e4u\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "$(", "PPOSAT", "NN", "$(", "PWS", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "L\u00e4\u00dft man zur Erndten Zeit wohl gantze Garben stehn/", "tokens": ["L\u00e4\u00dft", "man", "zur", "Ernd\u00b7ten", "Zeit", "wohl", "gant\u00b7ze", "Gar\u00b7ben", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Um einen Halm von Korn/ so aus dem Unkraut k\u00e4umet?", "tokens": ["Um", "ei\u00b7nen", "Halm", "von", "Korn", "/", "so", "aus", "dem", "Un\u00b7kraut", "k\u00e4u\u00b7met", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "APPR", "NN", "$(", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein solcher Schnitter mu\u00df gar arm nach Hause gehn.", "tokens": ["Ein", "sol\u00b7cher", "Schnit\u00b7ter", "mu\u00df", "gar", "arm", "nach", "Hau\u00b7se", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VMFIN", "ADV", "ADJD", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus dunckeln Ver\u00dfen wird der Seegen hergestellt/", "tokens": ["Aus", "dun\u00b7ckeln", "Ver\u00b7\u00dfen", "wird", "der", "See\u00b7gen", "her\u00b7ge\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie Saamen/ der erstickt/ so unter Dornen f\u00e4llt.", "tokens": ["Wie", "Saa\u00b7men", "/", "der", "er\u00b7stickt", "/", "so", "un\u00b7ter", "Dor\u00b7nen", "f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$(", "ART", "VVPP", "$(", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bey de