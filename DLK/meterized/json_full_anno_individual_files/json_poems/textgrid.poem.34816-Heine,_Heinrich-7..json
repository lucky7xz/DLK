{"textgrid.poem.34816": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngstens tr\u00e4umte mir: spazieren", "tokens": ["J\u00fcng\u00b7stens", "tr\u00e4um\u00b7te", "mir", ":", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Himmelreiche ging ich,", "tokens": ["In", "dem", "Him\u00b7mel\u00b7rei\u00b7che", "ging", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mit dir \u2013 denn ohne dich", "tokens": ["Ich", "mit", "dir", "\u2013", "denn", "oh\u00b7ne", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "APPR", "PPER", "$(", "KON", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00e4r der Himmel eine H\u00f6lle.", "tokens": ["W\u00e4r", "der", "Him\u00b7mel", "ei\u00b7ne", "H\u00f6l\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dort sah ich die Auserw\u00e4hlten,", "tokens": ["Dort", "sah", "ich", "die", "Au\u00b7ser\u00b7w\u00e4hl\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Gerechten und die Frommen,", "tokens": ["Die", "Ge\u00b7rech\u00b7ten", "und", "die", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die auf Erden ihren Leib", "tokens": ["Die", "auf", "Er\u00b7den", "ih\u00b7ren", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fcr der Seele Heil gepeinigt:", "tokens": ["F\u00fcr", "der", "See\u00b7le", "Heil", "ge\u00b7pei\u00b7nigt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Kirchenv\u00e4ter und Apostel,", "tokens": ["Kir\u00b7chen\u00b7v\u00e4\u00b7ter", "und", "A\u00b7pos\u00b7tel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eremiten, Kapuziner,", "tokens": ["E\u00b7re\u00b7mi\u00b7ten", ",", "Ka\u00b7pu\u00b7zi\u00b7ner", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Alte K\u00e4uze, ein'ge junge \u2013", "tokens": ["Al\u00b7te", "K\u00e4u\u00b7ze", ",", "ein'\u00b7ge", "jun\u00b7ge", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "ADJA", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Letztre sahn noch schlechter aus!", "tokens": ["Letz\u00b7tre", "sahn", "noch", "schlech\u00b7ter", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Lange, heilige Gesichter,", "tokens": ["Lan\u00b7ge", ",", "hei\u00b7li\u00b7ge", "Ge\u00b7sich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breite Glatzen, graue B\u00e4rte,", "tokens": ["Brei\u00b7te", "Glat\u00b7zen", ",", "grau\u00b7e", "B\u00e4r\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "(drunter auch verschiedne Juden) \u2013", "tokens": ["(", "drun\u00b7ter", "auch", "ver\u00b7schied\u00b7ne", "Ju\u00b7den", ")", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PAV", "ADV", "ADJA", "NN", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gingen streng an uns vor\u00fcber,", "tokens": ["Gin\u00b7gen", "streng", "an", "uns", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Warfen keinen Blick nach dir,", "tokens": ["War\u00b7fen", "kei\u00b7nen", "Blick", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "APPR", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ob du gleich, mein sch\u00f6nes Liebchen,", "tokens": ["Ob", "du", "gleich", ",", "mein", "sch\u00f6\u00b7nes", "Lieb\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "T\u00e4ndelnd mir am Arme hingest,", "tokens": ["T\u00e4n\u00b7delnd", "mir", "am", "Ar\u00b7me", "hin\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00e4ndelnd, l\u00e4chelnd, kokettierend!", "tokens": ["T\u00e4n\u00b7delnd", ",", "l\u00e4\u00b7chelnd", ",", "ko\u00b7ket\u00b7tie\u00b7rend", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nur ein einz'ger sah dich an,", "tokens": ["Nur", "ein", "einz'\u00b7ger", "sah", "dich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und es war der einz'ge sch\u00f6ne,", "tokens": ["Und", "es", "war", "der", "einz'\u00b7ge", "sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "ADJA", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ne Mann in dieser Schar;", "tokens": ["Sch\u00f6\u00b7ne", "Mann", "in", "die\u00b7ser", "Schar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wunderherrlich war sein Antlitz.", "tokens": ["Wun\u00b7der\u00b7herr\u00b7lich", "war", "sein", "Ant\u00b7litz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Menscheng\u00fcte um die Lippen,", "tokens": ["Men\u00b7schen\u00b7g\u00fc\u00b7te", "um", "die", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "G\u00f6tterruhe in den Augen,", "tokens": ["G\u00f6t\u00b7ter\u00b7ru\u00b7he", "in", "den", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie auf Magdalenen einst", "tokens": ["Wie", "auf", "Mag\u00b7da\u00b7le\u00b7nen", "einst"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ADV"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Schaute jener auf dich nieder.", "tokens": ["Schau\u00b7te", "je\u00b7ner", "auf", "dich", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ach! ich wei\u00df, er meint es gut \u2013", "tokens": ["Ach", "!", "ich", "wei\u00df", ",", "er", "meint", "es", "gut", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Keiner ist so rein und edel \u2013", "tokens": ["Kei\u00b7ner", "ist", "so", "rein", "und", "e\u00b7del", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber ich, ich wurde dennoch", "tokens": ["A\u00b7ber", "ich", ",", "ich", "wur\u00b7de", "den\u00b7noch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPER", "VAFIN", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wie von Eifersucht ber\u00fchret \u2013", "tokens": ["Wie", "von", "Ei\u00b7fer\u00b7sucht", "be\u00b7r\u00fch\u00b7ret", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Und ich mu\u00df gestehn, es wurde", "tokens": ["Und", "ich", "mu\u00df", "ge\u00b7stehn", ",", "es", "wur\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "VVINF", "$,", "PPER", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir im Himmel unbehaglich \u2013", "tokens": ["Mir", "im", "Him\u00b7mel", "un\u00b7be\u00b7hag\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "ADJD", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott verzeih mir's! mich genierte", "tokens": ["Gott", "ver\u00b7zeih", "mir's", "!", "mich", "ge\u00b7nier\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NE", "NE", "$.", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Heiland, Jesus Christus.", "tokens": ["Un\u00b7ser", "Hei\u00b7land", ",", "Je\u00b7sus", "Chris\u00b7tus", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.10": {"line.1": {"text": "J\u00fcngstens tr\u00e4umte mir: spazieren", "tokens": ["J\u00fcng\u00b7stens", "tr\u00e4um\u00b7te", "mir", ":", "spa\u00b7zie\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Himmelreiche ging ich,", "tokens": ["In", "dem", "Him\u00b7mel\u00b7rei\u00b7che", "ging", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter