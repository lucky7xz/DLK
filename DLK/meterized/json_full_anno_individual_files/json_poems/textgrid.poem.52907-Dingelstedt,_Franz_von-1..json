{"textgrid.poem.52907": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dutzend-F\u00fcrsten, Taschen-H\u00f6flein,", "tokens": ["Dut\u00b7zen\u00b7dF\u00fcrs\u00b7ten", ",", "Ta\u00b7schen\u00b7H\u00f6f\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gl\u00fccklich, wer euch niemals kennt!", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wer", "euch", "nie\u00b7mals", "kennt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffouriers- und Kammerz\u00f6flein-", "tokens": ["Hof\u00b7fou\u00b7ri\u00b7er\u00b7s", "und", "Kam\u00b7mer\u00b7z\u00f6f\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "TRUNC"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Actricen-Regiment!", "tokens": ["Und", "Ac\u00b7tri\u00b7cen\u00b7Re\u00b7gi\u00b7ment", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Alles ein Intrigen-Kn\u00e4uel,", "tokens": ["Al\u00b7les", "ein", "In\u00b7tri\u00b7gen\u00b7Kn\u00e4u\u00b7el", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teegeklatsch und Weiberschnack, \u2013", "tokens": ["Tee\u00b7ge\u00b7klatsch", "und", "Wei\u00b7ber\u00b7schnack", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fctz Euch Gott vor solchem Greuel", "tokens": ["Sch\u00fctz", "Euch", "Gott", "vor", "sol\u00b7chem", "Greu\u00b7el"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vor seidnem Lumpenpack!", "tokens": ["Und", "vor", "seid\u00b7nem", "Lum\u00b7pen\u00b7pack", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Mittags spart man's ab am Essen,", "tokens": ["Mit\u00b7tags", "spart", "man's", "ab", "am", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trinkt Zichorien statt Kaffee,", "tokens": ["Trinkt", "Zi\u00b7cho\u00b7ri\u00b7en", "statt", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Wein wird karg gemessen,", "tokens": ["Und", "der", "Wein", "wird", "karg", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles f\u00fcr die Soir\u00e9e.", "tokens": ["Al\u00b7les", "f\u00fcr", "die", "Soir\u00e9e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ohne Hosen wird gesessen", "tokens": ["Oh\u00b7ne", "Ho\u00b7sen", "wird", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgens fr\u00fch bei dem Lever,", "tokens": ["Mor\u00b7gens", "fr\u00fch", "bei", "dem", "Le\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Denn der Schneider n\u00e4ht die Tressen", "tokens": ["Denn", "der", "Schnei\u00b7der", "n\u00e4ht", "die", "Tres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NE", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An zur heutgen Soir\u00e9e.", "tokens": ["An", "zur", "heut\u00b7gen", "Soir\u00e9e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Aber abends welcher L\u00fcstre,", "tokens": ["A\u00b7ber", "a\u00b7bends", "wel\u00b7cher", "L\u00fcst\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch Get\u00fcmmel, welcher Glanz,", "tokens": ["Welch", "Ge\u00b7t\u00fcm\u00b7mel", ",", "wel\u00b7cher", "Glanz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PWAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welch vornehmes Hofgefl\u00fcster,", "tokens": ["Welch", "vor\u00b7neh\u00b7mes", "Hof\u00b7ge\u00b7fl\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher reiche Damenkranz!", "tokens": ["Wel\u00b7cher", "rei\u00b7che", "Da\u00b7men\u00b7kranz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Eines Kammerherren Schl\u00fcssel", "tokens": ["Ei\u00b7nes", "Kam\u00b7mer\u00b7her\u00b7ren", "Schl\u00fcs\u00b7sel"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Reibt sich am Minister-Stern,", "tokens": ["Reibt", "sich", "am", "Mi\u00b7nis\u00b7ter\u00b7S\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und von einer leeren Sch\u00fcssel", "tokens": ["Und", "von", "ei\u00b7ner", "lee\u00b7ren", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4hmen alle beide gern.", "tokens": ["N\u00e4h\u00b7men", "al\u00b7le", "bei\u00b7de", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Generalen-Epauletten", "tokens": ["Ge\u00b7ne\u00b7ra\u00b7len\u00b7E\u00b7pau\u00b7let\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Werden rot, weil sie nicht echt,", "tokens": ["Wer\u00b7den", "rot", ",", "weil", "sie", "nicht", "echt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neben den massiven Ketten,", "tokens": ["Ne\u00b7ben", "den", "mas\u00b7si\u00b7ven", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die der Herr Hofbanquier tr\u00e4gt.", "tokens": ["Die", "der", "Herr", "Hof\u00b7ban\u00b7qui\u00b7er", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Pl\u00f6tzlich fliegen auf die T\u00fcren,", "tokens": ["Pl\u00f6tz\u00b7lich", "flie\u00b7gen", "auf", "die", "T\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bb!Ha, der HErr!\u00ab hei\u00dft's \u00fcberall:", "tokens": ["\u00bb", "!", "Ha", ",", "der", "Herr", "!", "\u00ab", "hei\u00dft's", "\u00fc\u00b7be\u00b7rall", ":"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "$.", "ITJ", "$,", "ART", "NN", "$.", "$(", "NE", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Durchlaucht sieht man f\u00fchren", "tokens": ["Sei\u00b7ne", "Durch\u00b7laucht", "sieht", "man", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihre Durchlaucht in den Saal!", "tokens": ["Ih\u00b7re", "Durch\u00b7laucht", "in", "den", "Saal", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+---+", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Und nach dem Adre\u00dfkalender", "tokens": ["Und", "nach", "dem", "A\u00b7dre\u00df\u00b7ka\u00b7len\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Reiht sich alles hoch und tief,", "tokens": ["Reiht", "sich", "al\u00b7les", "hoch", "und", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Herren stehn wie St\u00e4nder,", "tokens": ["Al\u00b7le", "Her\u00b7ren", "stehn", "wie", "St\u00e4n\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "KOKOM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle Damen knixen schief.", "tokens": ["Al\u00b7le", "Da\u00b7men", "kni\u00b7xen", "schief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Sieh, mit spanischer Grandezza", "tokens": ["Sieh", ",", "mit", "spa\u00b7ni\u00b7scher", "Gran\u00b7dez\u00b7za"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ADJA", "NN"], "meter": "+----+-+", "measure": "dactylic.init"}, "line.2": {"text": "Sieht der Herr durch ihre Reihn,", "tokens": ["Sieht", "der", "Herr", "durch", "ih\u00b7re", "Reihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er nur redet laut und mezza", "tokens": ["Er", "nur", "re\u00b7det", "laut", "und", "mez\u00b7za"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "KON", "NE"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Voce falln die andern ein.", "tokens": ["Vo\u00b7ce", "falln", "die", "an\u00b7dern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Hungern, Dursten, G\u00e4hnen, Frieren,", "tokens": ["Hun\u00b7gern", ",", "Durs\u00b7ten", ",", "G\u00e4h\u00b7nen", ",", "Frie\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Echo und Maschine sein,", "tokens": ["E\u00b7cho", "und", "Ma\u00b7schi\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Obendrein im Whist verlieren", "tokens": ["O\u00b7ben\u00b7drein", "im", "Whist", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Tanz sich abkastein \u2013", "tokens": ["Und", "im", "Tanz", "sich", "ab\u00b7ka\u00b7stein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PRF", "PTKVZ", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.12": {"line.1": {"text": "O der \u00fcbert\u00fcnchten Leere,", "tokens": ["O", "der", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7ten", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Draus die Armut allw\u00e4rts schielt,", "tokens": ["Draus", "die", "Ar\u00b7mut", "all\u00b7w\u00e4rts", "schielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Just als ob's ein Jahrmarkt w\u00e4re,", "tokens": ["Just", "als", "ob's", "ein", "Jahr\u00b7markt", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "KOUS", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man Volkstheater spielt!", "tokens": ["Wo", "man", "Volkst\u00b7he\u00b7a\u00b7ter", "spielt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Munter, munter, Marionetten,", "tokens": ["Mun\u00b7ter", ",", "mun\u00b7ter", ",", "Ma\u00b7ri\u00b7o\u00b7net\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Tanzt zu Seinem Zeitvertreib!", "tokens": ["Tanzt", "zu", "Sei\u00b7nem", "Zeit\u00b7ver\u00b7treib", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ha, wenn sie den Draht nicht h\u00e4tten,", "tokens": ["Ha", ",", "wenn", "sie", "den", "Draht", "nicht", "h\u00e4t\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ART", "NN", "PTKNEG", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00e4tten sie nichts in ", "tokens": ["H\u00e4t\u00b7ten", "sie", "nichts", "in"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "APPR"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Dutzend-F\u00fcrsten, Taschen-H\u00f6flein,", "tokens": ["Dut\u00b7zen\u00b7dF\u00fcrs\u00b7ten", ",", "Ta\u00b7schen\u00b7H\u00f6f\u00b7lein", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Gl\u00fccklich, wer euch niemals kennt!", "tokens": ["Gl\u00fcck\u00b7lich", ",", "wer", "euch", "nie\u00b7mals", "kennt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoffouriers- und Kammerz\u00f6flein-", "tokens": ["Hof\u00b7fou\u00b7ri\u00b7er\u00b7s", "und", "Kam\u00b7mer\u00b7z\u00f6f\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["TRUNC", "KON", "TRUNC"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und Actricen-Regiment!", "tokens": ["Und", "Ac\u00b7tri\u00b7cen\u00b7Re\u00b7gi\u00b7ment", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Alles ein Intrigen-Kn\u00e4uel,", "tokens": ["Al\u00b7les", "ein", "In\u00b7tri\u00b7gen\u00b7Kn\u00e4u\u00b7el", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teegeklatsch und Weiberschnack, \u2013", "tokens": ["Tee\u00b7ge\u00b7klatsch", "und", "Wei\u00b7ber\u00b7schnack", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fctz Euch Gott vor solchem Greuel", "tokens": ["Sch\u00fctz", "Euch", "Gott", "vor", "sol\u00b7chem", "Greu\u00b7el"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "NN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und vor seidnem Lumpenpack!", "tokens": ["Und", "vor", "seid\u00b7nem", "Lum\u00b7pen\u00b7pack", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Mittags spart man's ab am Essen,", "tokens": ["Mit\u00b7tags", "spart", "man's", "ab", "am", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trinkt Zichorien statt Kaffee,", "tokens": ["Trinkt", "Zi\u00b7cho\u00b7ri\u00b7en", "statt", "Kaf\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Wein wird karg gemessen,", "tokens": ["Und", "der", "Wein", "wird", "karg", "ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles f\u00fcr die Soir\u00e9e.", "tokens": ["Al\u00b7les", "f\u00fcr", "die", "Soir\u00e9e", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.17": {"line.1": {"text": "Ohne Hosen wird gesessen", "tokens": ["Oh\u00b7ne", "Ho\u00b7sen", "wird", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Morgens fr\u00fch bei dem Lever,", "tokens": ["Mor\u00b7gens", "fr\u00fch", "bei", "dem", "Le\u00b7ver", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text"