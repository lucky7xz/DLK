": {"text": "Reibt sich am Minister-Stern,", "tokens": ["Reibt", "sich", "am", "Mi\u00b7nis\u00b7ter\u00b7S\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und von einer leeren Sch\u00fcssel", "tokens": ["Und", "von", "ei\u00b7ner", "lee\u00b7ren", "Sch\u00fcs\u00b7sel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4hmen alle beide gern.", "tokens": ["N\u00e4h\u00b7men", "al\u00b7le", "bei\u00b7de", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Generalen-Epauletten", "tokens": ["Ge\u00b7ne\u00b7ra\u00b7len\u00b7E\u00b7pau\u00b7let\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Werden rot, weil sie nicht echt,", "tokens": ["Wer\u00b7den", "rot", ",", "weil", "sie", "nicht", "echt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Neben den massiven Ketten,", "tokens": ["Ne\u00b7ben", "den", "mas\u00b7si\u00b7ven", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Die der Herr Hofbanquier tr\u00e4gt.", "tokens": ["Die", "der", "Herr", "Hof\u00b7ban\u00b7qui\u00b7er", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stan