 "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Echo und Maschine sein,", "tokens": ["E\u00b7cho", "und", "Ma\u00b7schi\u00b7ne", "sein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Obendrein im Whist verlieren", "tokens": ["O\u00b7ben\u00b7drein", "im", "Whist", "ver\u00b7lie\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und im Tanz sich abkastein \u2013", "tokens": ["Und", "im", "Tanz", "sich", "ab\u00b7ka\u00b7stein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "PRF", "PTKVZ", "$("], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.25": {"line.1": {"text": "O der \u00fcbert\u00fcnchten Leere,", "tokens": ["O", "der", "\u00fc\u00b7ber\u00b7t\u00fcnch\u00b7ten", "Lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Draus die Armut allw\u00e4rts schielt,", "tokens": ["Draus", "die", "Ar\u00b7mut", "all\u00b7w\u00e4rts", "schielt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Just als ob's ein Jahrmarkt w\u00e4re,", "tokens": ["Just", "als", "ob's", "ein", "Jahr\u00b7markt", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "KOUS", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo man Volkstheater spielt!", "tokens": ["Wo", "man", "Volkst\u00b7he\u00b7