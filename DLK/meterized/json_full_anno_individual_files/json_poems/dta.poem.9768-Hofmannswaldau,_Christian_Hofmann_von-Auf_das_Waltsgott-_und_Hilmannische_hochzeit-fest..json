{"dta.poem.9768": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Waltsgott- und Hilmannische  \n hochzeit-fest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Njm werther bruder di\u00df/ so gut als meine liebe", "tokens": ["Njm", "wert\u00b7her", "bru\u00b7der", "di\u00df", "/", "so", "gut", "als", "mei\u00b7ne", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "PDS", "$(", "ADV", "ADJD", "KOKOM", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir in der einsamkeit von ferne dienen kan.", "tokens": ["Dir", "in", "der", "ein\u00b7sam\u00b7keit", "von", "fer\u00b7ne", "die\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "APPR", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es steht mein Helicon ietzunder etwas tr\u00fcbe:", "tokens": ["Es", "steht", "mein", "He\u00b7li\u00b7con", "ietz\u00b7un\u00b7der", "et\u00b7was", "tr\u00fc\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "ADV", "PIAT", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir bricht ein helles licht/ mir nacht und schatten an", "tokens": ["Dir", "bricht", "ein", "hel\u00b7les", "licht", "/", "mir", "nacht", "und", "schat\u00b7ten", "an"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "PPER", "NN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein himmel schm\u00fccket sich mit einer g\u00f6ldnen sonne/", "tokens": ["Dein", "him\u00b7mel", "schm\u00fc\u00b7cket", "sich", "mit", "ei\u00b7ner", "g\u00f6ld\u00b7nen", "son\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es d\u00fcnckt mich deine lust ein blosser traum zu seyn/", "tokens": ["Es", "d\u00fcnckt", "mich", "dei\u00b7ne", "lust", "ein", "blos\u00b7ser", "traum", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du lebst wie du verlangst in s\u00fcsser Oster-wonne;", "tokens": ["Du", "lebst", "wie", "du", "ver\u00b7langst", "in", "s\u00fcs\u00b7ser", "Os\u00b7ter\u00b7won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Allein bey mir allhier f\u00e4llt erst die fasten ein.", "tokens": ["Al\u00b7lein", "bey", "mir", "all\u00b7hier", "f\u00e4llt", "erst", "die", "fas\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Doch schau/ der ferne weg zeucht mich von deinen freuden", "tokens": ["Doch", "schau", "/", "der", "fer\u00b7ne", "weg", "zeucht", "mich", "von", "dei\u00b7nen", "freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$(", "ART", "ADJA", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nicht gar so g\u00e4ntzlich ab: es will mein bestes ich", "tokens": ["Nicht", "gar", "so", "g\u00e4ntz\u00b7lich", "ab", ":", "es", "will", "mein", "bes\u00b7tes", "ich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "PTKVZ", "$.", "PPER", "VMFIN", "PPOSAT", "ADJA", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "An deiner fr\u00f6ligkeit sich gegenw\u00e4rtig weiden;", "tokens": ["An", "dei\u00b7ner", "fr\u00f6\u00b7lig\u00b7keit", "sich", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig", "wei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Mich d\u00fcnckt/ ich seh bereits dich/ theurer bruder/ dich.", "tokens": ["Mich", "d\u00fcnckt", "/", "ich", "seh", "be\u00b7reits", "dich", "/", "theu\u00b7rer", "bru\u00b7der", "/", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "PPER", "$(", "ADJD", "NN", "$(", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich komme gantz zu dir/ und lasse nur die schaale/", "tokens": ["Ich", "kom\u00b7me", "gantz", "zu", "dir", "/", "und", "las\u00b7se", "nur", "die", "schaa\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "$(", "KON", "VVFIN", "ADV", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und das befleischte hau\u00df bey seinen musen stehn:", "tokens": ["Und", "das", "be\u00b7fleischte", "hau\u00df", "bey", "sei\u00b7nen", "mu\u00b7sen", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.15": {"text": "Ich komm und sehe dich in einem hochzeit-saale/", "tokens": ["Ich", "komm", "und", "se\u00b7he", "dich", "in", "ei\u00b7nem", "hoch\u00b7zeit\u00b7saa\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ein recht himmlisch kind an deiner seite gehn.", "tokens": ["Und", "ein", "recht", "himm\u00b7lisch", "kind", "an", "dei\u00b7ner", "sei\u00b7te", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "ADJD", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Entbrich dich doch so viel von deiner hochzeit-freude/", "tokens": ["Ent\u00b7brich", "dich", "doch", "so", "viel", "von", "dei\u00b7ner", "hoch\u00b7zeit\u00b7freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und schau noch einst zur\u00fcck/ wohin das gl\u00fccke zielt:", "tokens": ["Und", "schau", "noch", "einst", "zu\u00b7r\u00fcck", "/", "wo\u00b7hin", "das", "gl\u00fc\u00b7cke", "zielt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "ADV", "PTKVZ", "$(", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie selbtes dich nunmehr in lauter rosen weide/", "tokens": ["Wie", "selb\u00b7tes", "dich", "nun\u00b7mehr", "in", "lau\u00b7ter", "ro\u00b7sen", "wei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "PPER", "ADV", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Nachdem es manchen dorn in deine brust gespielt.", "tokens": ["Nach\u00b7dem", "es", "man\u00b7chen", "dorn", "in", "dei\u00b7ne", "brust", "ge\u00b7spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Dich ri\u00df ein fester schlu\u00df in abgelegne gr\u00e4ntzen/", "tokens": ["Dich", "ri\u00df", "ein", "fes\u00b7ter", "schlu\u00df", "in", "ab\u00b7ge\u00b7leg\u00b7ne", "gr\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Wo einst der erden-krei\u00df ein knecht und sclave hie\u00df;", "tokens": ["Wo", "einst", "der", "er\u00b7den\u00b7krei\u00df", "ein", "knecht", "und", "scla\u00b7ve", "hie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ART", "NN", "KON", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wo dich der himmel lie\u00df in neuer w\u00fcrde gl\u00e4ntzen/", "tokens": ["Wo", "dich", "der", "him\u00b7mel", "lie\u00df", "in", "neu\u00b7er", "w\u00fcr\u00b7de", "gl\u00e4nt\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "APPR", "ADJA", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Indem man dir den lohn der Hygiene wies.", "tokens": ["In\u00b7dem", "man", "dir", "den", "lohn", "der", "Hy\u00b7gi\u00b7e\u00b7ne", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zwar erstlich str\u00f6mte dir das gl\u00fccke nicht zu wider/", "tokens": ["Zwar", "erst\u00b7lich", "str\u00f6m\u00b7te", "dir", "das", "gl\u00fc\u00b7cke", "nicht", "zu", "wi\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "PDS", "VVFIN", "PTKNEG", "PTKZU", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Es schien als w\u00e4r es dir mehr als zu viel geneigt/", "tokens": ["Es", "schien", "als", "w\u00e4r", "es", "dir", "mehr", "als", "zu", "viel", "ge\u00b7neigt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "VAFIN", "PPER", "PPER", "PIAT", "KOKOM", "APPR", "PIS", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es satzte dich die fluth durch sanffte westen nieder/", "tokens": ["Es", "satz\u00b7te", "dich", "die", "fluth", "durch", "sanff\u00b7te", "wes\u00b7ten", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da wo die zier der welt aus Thetis busen steigt.", "tokens": ["Da", "wo", "die", "zier", "der", "welt", "aus", "The\u00b7tis", "bu\u00b7sen", "steigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWAV", "ART", "NN", "ART", "NN", "APPR", "NE", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und weil dir jederzeit die seele grosser st\u00e4dte", "tokens": ["Und", "weil", "dir", "je\u00b7der\u00b7zeit", "die", "see\u00b7le", "gros\u00b7ser", "st\u00e4d\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Mehr als der bau gefiel: so rieth Hygea dir", "tokens": ["Mehr", "als", "der", "bau", "ge\u00b7fiel", ":", "so", "rieth", "Hy\u00b7gea", "dir"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "VVFIN", "$.", "ADV", "VVFIN", "NE", "PPER"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.31": {"text": "Zu schaun/ was die natur vor k\u00fcnstliches ger\u00e4the", "tokens": ["Zu", "schaun", "/", "was", "die", "na\u00b7tur", "vor", "k\u00fcnst\u00b7li\u00b7ches", "ge\u00b7r\u00e4\u00b7the"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "$(", "PWS", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Stellt der verwunderung an Arnus ufern f\u00fcr.", "tokens": ["Stellt", "der", "ver\u00b7wun\u00b7de\u00b7rung", "an", "Ar\u00b7nus", "u\u00b7fern", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NE", "VVINF", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Drauff sahestu das aa\u00df der f\u00fcrstin aller reiche/", "tokens": ["Drauff", "sa\u00b7he\u00b7stu", "das", "aa\u00df", "der", "f\u00fcrs\u00b7tin", "al\u00b7ler", "rei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "PIAT", "ADJA", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.34": {"text": "Wo/ was die Donau ist/ vorhin die Tyber war:", "tokens": ["Wo", "/", "was", "die", "Do\u00b7nau", "ist", "/", "vor\u00b7hin", "die", "Ty\u00b7ber", "war", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "PWS", "ART", "NE", "VAFIN", "$(", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und mercktest: da\u00df die zeit/ die allgemeine seuche/", "tokens": ["Und", "merck\u00b7test", ":", "da\u00df", "die", "zeit", "/", "die", "all\u00b7ge\u00b7mei\u00b7ne", "seu\u00b7che", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der menschen ewigkeit selbst leget auff die bahr.", "tokens": ["Der", "men\u00b7schen", "e\u00b7wig\u00b7keit", "selbst", "le\u00b7get", "auff", "die", "bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-++--+-+", "measure": "iambic.hexa.invert"}, "line.37": {"text": "Hier schien dein schutz-gestirn sich nach und nach zu neigen/", "tokens": ["Hier", "schien", "dein", "schutz\u00b7ge\u00b7stirn", "sich", "nach", "und", "nach", "zu", "nei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "KON", "APPR", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Indem manch wilder sturm auff deine seegel drang/", "tokens": ["In\u00b7dem", "manch", "wil\u00b7der", "sturm", "auff", "dei\u00b7ne", "see\u00b7gel", "drang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es schiene dir die fluth ein nasses grab zu zeigen/", "tokens": ["Es", "schie\u00b7ne", "dir", "die", "fluth", "ein", "nas\u00b7ses", "grab", "zu", "zei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die fluth/ die Cajus einst in eine br\u00fccke zwang.", "tokens": ["Die", "fluth", "/", "die", "Ca\u00b7jus", "einst", "in", "ei\u00b7ne", "br\u00fc\u00b7cke", "zwang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NE", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Doch hat