{"textgrid.poem.55700": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mein Herz ist viel zu m\u00fcde, um zu toben,", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist viel zu m\u00fcde, um zu toben,", "tokens": ["Mein", "Herz", "ist", "viel", "zu", "m\u00fc\u00b7de", ",", "um", "zu", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PTKA", "ADJD", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In Ha\u00df und Groll, weil Du von mir geschieden", "tokens": ["In", "Ha\u00df", "und", "Groll", ",", "weil", "Du", "von", "mir", "ge\u00b7schie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und mit Dir nahmst das bi\u00dfchen Gl\u00fcck und Frieden,", "tokens": ["Und", "mit", "Dir", "nahmst", "das", "bi\u00df\u00b7chen", "Gl\u00fcck", "und", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ins tr\u00fcbe Dasein m\u00fchsam nur gewoben.", "tokens": ["Ins", "tr\u00fc\u00b7be", "Da\u00b7sein", "m\u00fch\u00b7sam", "nur", "ge\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Noch kann zu l\u00e4cheln ich die Lippen zwingen", "tokens": ["Noch", "kann", "zu", "l\u00e4\u00b7cheln", "ich", "die", "Lip\u00b7pen", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PTKZU", "VVFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kann auch noch f\u00fcr Andre warm empfinden,", "tokens": ["Und", "kann", "auch", "noch", "f\u00fcr", "And\u00b7re", "warm", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sie ermutigen zu \u00fcberwinden,", "tokens": ["Und", "sie", "er\u00b7mu\u00b7ti\u00b7gen", "zu", "\u00fc\u00b7berw\u00b7in\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur will's mir selber nicht mehr gut gelingen.", "tokens": ["Nur", "will's", "mir", "sel\u00b7ber", "nicht", "mehr", "gut", "ge\u00b7lin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn insgeheim die Ketten niederklirren", "tokens": ["Denn", "ins\u00b7ge\u00b7heim", "die", "Ket\u00b7ten", "nie\u00b7der\u00b7klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die mich ans Dasein bisher festgebunden,", "tokens": ["Die", "mich", "ans", "Da\u00b7sein", "bis\u00b7her", "fest\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["w