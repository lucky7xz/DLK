{"textgrid.poem.55102": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Hermann und Dorothea", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Also das w\u00e4re Verbrechen, da\u00df einst Properz mich begeistert,", "tokens": ["Al\u00b7so", "das", "w\u00e4\u00b7re", "Ver\u00b7bre\u00b7chen", ",", "da\u00df", "einst", "Pro\u00b7perz", "mich", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "NN", "$,", "KOUS", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+-++-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da\u00df Martial sich zu mir auch, der verwegne, gesellt?", "tokens": ["Da\u00df", "Mar\u00b7ti\u00b7al", "sich", "zu", "mir", "auch", ",", "der", "ver\u00b7weg\u00b7ne", ",", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "APPR", "PPER", "ADV", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df ich die Alten nicht hinter mir lie\u00df, die Schule zu h\u00fcten,", "tokens": ["Da\u00df", "ich", "die", "Al\u00b7ten", "nicht", "hin\u00b7ter", "mir", "lie\u00df", ",", "die", "Schu\u00b7le", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df sie nach Latium gern mir in das Leben gefolgt?", "tokens": ["Da\u00df", "sie", "nach", "La\u00b7ti\u00b7um", "gern", "mir", "in", "das", "Le\u00b7ben", "ge\u00b7folgt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ADV", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da\u00df ich Natur und Kunst zu schaun mich treulich bestrebe,", "tokens": ["Da\u00df", "ich", "Na\u00b7tur", "und", "Kunst", "zu", "schaun", "mich", "treu\u00b7lich", "be\u00b7stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da\u00df kein Name mich t\u00e4uscht, da\u00df mich kein Dogma beschr\u00e4nkt?", "tokens": ["Da\u00df", "kein", "Na\u00b7me", "mich", "t\u00e4uscht", ",", "da\u00df", "mich", "kein", "Dog\u00b7ma", "be\u00b7schr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Da\u00df nicht des Lebens bedingender Drang mich, den Menschen, ver\u00e4ndert,", "tokens": ["Da\u00df", "nicht", "des", "Le\u00b7bens", "be\u00b7din\u00b7gen\u00b7der", "Drang", "mich", ",", "den", "Men\u00b7schen", ",", "ver\u00b7\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJA", "NN", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "++-+--+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Da\u00df ich der Heuchelei d\u00fcrftige Maske verschm\u00e4ht?", "tokens": ["Da\u00df", "ich", "der", "Heu\u00b7che\u00b7lei", "d\u00fcrf\u00b7ti\u00b7ge", "Mas\u00b7ke", "ver\u00b7schm\u00e4ht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Solcher Fehler, die du, o Muse, so emsig gepfleget,", "tokens": ["Sol\u00b7cher", "Feh\u00b7ler", ",", "die", "du", ",", "o", "Mu\u00b7se", ",", "so", "em\u00b7sig", "ge\u00b7pfle\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "$,", "FM", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Zeihet der P\u00f6bel mich; P\u00f6bel nur sieht er in mir.", "tokens": ["Zei\u00b7het", "der", "P\u00f6\u00b7bel", "mich", ";", "P\u00f6\u00b7bel", "nur", "sieht", "er", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$.", "NN", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Ja, sogar der Bessere selbst, gutm\u00fctig und bieder,", "tokens": ["Ja", ",", "so\u00b7gar", "der", "Bes\u00b7se\u00b7re", "selbst", ",", "gut\u00b7m\u00fc\u00b7tig", "und", "bie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+----+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Will mich anders; doch du, Muse, befiehlst mir allein.", "tokens": ["Will", "mich", "an\u00b7ders", ";", "doch", "du", ",", "Mu\u00b7se", ",", "be\u00b7fiehlst", "mir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$.", "KON", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Denn du bist es allein, die noch mir die innere Jugend", "tokens": ["Denn", "du", "bist", "es", "al\u00b7lein", ",", "die", "noch", "mir", "die", "in\u00b7ne\u00b7re", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Frisch erneuest und sie mir bis zu Ende versprichst.", "tokens": ["Frisch", "er\u00b7neu\u00b7est", "und", "sie", "mir", "bis", "zu", "En\u00b7de", "ver\u00b7sprichst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Aber verdopple nunmehr, o G\u00f6ttin, die heilige Sorgfalt!", "tokens": ["A\u00b7ber", "ver\u00b7dopp\u00b7le", "nun\u00b7mehr", ",", "o", "G\u00f6t\u00b7tin", ",", "die", "hei\u00b7li\u00b7ge", "Sorg\u00b7falt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Ach! die Scheitel umwallt reichlich die Locke nicht mehr:", "tokens": ["Ach", "!", "die", "Schei\u00b7tel", "um\u00b7wallt", "reich\u00b7lich", "die", "Lo\u00b7cke", "nicht", "mehr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Da bedarf man der Kr\u00e4nze, sich selbst und andre zu t\u00e4uschen;", "tokens": ["Da", "be\u00b7darf", "man", "der", "Kr\u00e4n\u00b7ze", ",", "sich", "selbst", "und", "and\u00b7re", "zu", "t\u00e4u\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "PRF", "ADV", "KON", "PIS", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Kr\u00e4nzte doch C\u00e4sar selbst nur aus Bed\u00fcrfnis das Haupt.", "tokens": ["Kr\u00e4nz\u00b7te", "doch", "C\u00e4\u00b7sar", "selbst", "nur", "aus", "Be\u00b7d\u00fcrf\u00b7nis", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "ADV", "ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+---+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Hast du ein Lorbeerreis mir bestimmt, so la\u00df es am Zweige", "tokens": ["Hast", "du", "ein", "Lor\u00b7beer\u00b7reis", "mir", "be\u00b7stimmt", ",", "so", "la\u00df", "es", "am", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "VVPP", "$,", "ADV", "VVIMP", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Weiter gr\u00fcnen, und gib einst es dem W\u00fcrdigern hin;", "tokens": ["Wei\u00b7ter", "gr\u00fc\u00b7nen", ",", "und", "gib", "einst", "es", "dem", "W\u00fcr\u00b7di\u00b7gern", "hin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "VVIMP", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Aber Rosen winde genug zum h\u00e4uslichen Kranze;", "tokens": ["A\u00b7ber", "Ro\u00b7sen", "win\u00b7de", "ge\u00b7nug", "zum", "h\u00e4us\u00b7li\u00b7chen", "Kran\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Bald als Lilie schlingt silberne Locke sich durch.", "tokens": ["Bald", "als", "Li\u00b7lie", "schlingt", "sil\u00b7ber\u00b7ne", "Lo\u00b7cke", "sich", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.23": {"text": "Sch\u00fcre die Gattin das Feuer, auf reinlichem Herde zu kochen!", "tokens": ["Sch\u00fc\u00b7re", "die", "Gat\u00b7tin", "das", "Feu\u00b7er", ",", "auf", "rein\u00b7li\u00b7chem", "Her\u00b7de", "zu", "ko\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Werfe der Knabe das Reis, spielend, gesch\u00e4ftig dazu!", "tokens": ["Wer\u00b7fe", "der", "Kna\u00b7be", "das", "Reis", ",", "spie\u00b7lend", ",", "ge\u00b7sch\u00e4f\u00b7tig", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "PAV", "$."], "meter": "+--+--++--+-++", "measure": "dactylic.di.plus"}, "line.25": {"text": "La\u00df im Becher nicht fehlen den Wein! Gespr\u00e4chige Freunde,", "tokens": ["La\u00df", "im", "Be\u00b7cher", "nicht", "feh\u00b7len", "den", "Wein", "!", "Ge\u00b7spr\u00e4\u00b7chi\u00b7ge", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Gleichgesinnte, herein! Kr\u00e4nze, sie warten auf euch.", "tokens": ["Gleich\u00b7ge\u00b7sinn\u00b7te", ",", "her\u00b7ein", "!", "Kr\u00e4n\u00b7ze", ",", "sie", "war\u00b7ten", "auf", "euch", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$.", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Erst die Gesundheit des Mannes, der, endlich vom Namen Homeros", "tokens": ["Erst", "die", "Ge\u00b7sund\u00b7heit", "des", "Man\u00b7nes", ",", "der", ",", "end\u00b7lich", "vom", "Na\u00b7men", "Ho\u00b7me\u00b7ros"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "$,", "ADV", "APPRART", "NN", "NE"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.28": {"text": "K\u00fchn uns befreiend, uns auch ruft in die vollere Bahn.", "tokens": ["K\u00fchn", "uns", "be\u00b7frei\u00b7end", ",", "uns", "auch", "ruft", "in", "die", "vol\u00b7le\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$,", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Denn wer wagte mit G\u00f6ttern den Kampf? und wer mit dem Einen?", "tokens": ["Denn", "wer", "wag\u00b7te", "mit", "G\u00f6t\u00b7tern", "den", "Kampf", "?", "und", "wer", "mit", "dem", "Ei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "NN", "ART", "NN", "$.", "KON", "PWS", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Doch Homeride zu sein, auch nur als letzter, ist sch\u00f6n.", "tokens": ["Doch", "Ho\u00b7me\u00b7ri\u00b7de", "zu", "sein", ",", "auch", "nur", "als", "letz\u00b7ter", ",", "ist", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "PTKZU", "VAINF", "$,", "ADV", "ADV", "KOUS", "ADJA", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Darum h\u00f6ret das neuste Gedicht! Noch einmal getrunken!", "tokens": ["Da\u00b7rum", "h\u00f6\u00b7ret", "das", "neus\u00b7te", "Ge\u00b7dicht", "!", "Noch", "ein\u00b7mal", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "ADV", "VVPP", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Euch besteche der Wein, Freundschaft und Liebe das Ohr.", "tokens": ["Euch", "be\u00b7ste\u00b7che", "der", "Wein", ",", "Freund\u00b7schaft", "und", "Lie\u00b7be", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Deutschen selber f\u00fchr ich euch zu, in die stillere Wohnung,", "tokens": ["Deut\u00b7schen", "sel\u00b7ber", "f\u00fchr", "ich", "euch", "zu", ",", "in", "die", "stil\u00b7le\u00b7re", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Wo sich, nah der Natur, menschlich der Mensch noch erzieht.", "tokens": ["Wo", "sich", ",", "nah", "der", "Na\u00b7tur", ",", "menschlich", "der", "Mensch", "noch", "er\u00b7zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "ADJD", "ART", "NN", "$,", "ADJD", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.35": {"text": "Uns begleite des Dichters Geist, der seine Luise", "tokens": ["Uns", "be\u00b7glei\u00b7te", "des", "Dich\u00b7ters", "Geist", ",", "der", "sei\u00b7ne", "Lu\u00b7i\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Rasch dem w\u00fcrdigen Freund, uns zu entz\u00fccken, verband.", "tokens": ["Rasch", "dem", "w\u00fcr\u00b7di\u00b7gen", "Freund", ",", "uns", "zu", "ent\u00b7z\u00fc\u00b7cken", ",", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Auch die traurigen Bilder der Zeit, sie f\u00fchr ich vor\u00fcber;", "tokens": ["Auch", "die", "trau\u00b7ri\u00b7gen", "Bil\u00b7der", "der", "Zeit", ",", "sie", "f\u00fchr", "ich", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.38": {"text": "Aber es siege der Mut in dem gesunden Geschlecht.", "tokens": ["A\u00b7ber", "es", "sie\u00b7ge", "der", "Mut", "in", "dem", "ge\u00b7sun\u00b7den", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Hab ich euch Tr\u00e4nen ins Auge gelockt und Lust in die Seele", "tokens": ["Hab", "ich", "euch", "Tr\u00e4\u00b7nen", "ins", "Au\u00b7ge", "ge\u00b7lockt", "und", "Lust", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "NN", "APPRART", "NN", "VVPP", "KON", "NN", "APPR", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.40": {"text": "Singend gefl\u00f6\u00dft, so kommt, dr\u00fccket mich herzlich ans Herz!", "tokens": ["Sin\u00b7gend", "ge\u00b7fl\u00f6\u00dft", ",", "so", "kommt", ",", "dr\u00fc\u00b7cket", "mich", "herz\u00b7lich", "ans", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.41": {"text": "Weise denn sei das Gespr\u00e4ch ! Uns lehret Weisheit am Ende", "tokens": ["Wei\u00b7se", "denn", "sei", "das", "Ge\u00b7spr\u00e4ch", "!", "Uns", "leh\u00b7ret", "Weis\u00b7heit", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "APPRART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Das Jahrhundert; wen hat das Geschick nicht gepr\u00fcft?", "tokens": ["Das", "Jahr\u00b7hun\u00b7dert", ";", "wen", "hat", "das", "Ge\u00b7schick", "nicht", "ge\u00b7pr\u00fcft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Blicket heiterer nun auf jene Schmerzen zur\u00fccke,", "tokens": ["Bli\u00b7cket", "hei\u00b7te\u00b7rer", "nun", "auf", "je\u00b7ne", "Schmer\u00b7zen", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.44": {"text": "Wenn euch ein fr\u00f6hlicher Sinn manches entbehrlich erkl\u00e4rt.", "tokens": ["Wenn", "euch", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Sinn", "man\u00b7ches", "ent\u00b7behr\u00b7lich", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "Menschen lernten wir kennen und Nationen; so la\u00dft uns,", "tokens": ["Men\u00b7schen", "lern\u00b7ten", "wir", "ken\u00b7nen", "und", "Na\u00b7ti\u00b7o\u00b7nen", ";", "so", "la\u00dft", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "KON", "NN", "$.", "ADV", "VVIMP", "PPER", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.46": {"text": "Unser eigenes Herz kennend, uns dessen erfreun.", "tokens": ["Un\u00b7ser", "ei\u00b7ge\u00b7nes", "Herz", "ken\u00b7nend", ",", "uns", "des\u00b7sen", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "PPER", "PDS", "VVINF", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Also das w\u00e4re Verbrechen, da\u00df einst Properz mich begeistert,", "tokens": ["Al\u00b7so", "das", "w\u00e4\u00b7re", "Ver\u00b7bre\u00b7chen", ",", "da\u00df", "einst", "Pro\u00b7perz", "mich", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "VAFIN", "NN", "$,", "KOUS", "ADV", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+-++-+--+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Da\u00df Martial sich zu mir auch, der verwegne, gesellt?", "tokens": ["Da\u00df", "Mar\u00b7ti\u00b7al", "sich", "zu", "mir", "auch", ",", "der", "ver\u00b7weg\u00b7ne", ",", "ge\u00b7sellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "NE", "PRF", "APPR", "PPER", "ADV", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df ich die Alten nicht hinter mir lie\u00df, die Schule zu h\u00fcten,", "tokens": ["Da\u00df", "ich", "die", "Al\u00b7ten", "nicht", "hin\u00b7ter", "mir", "lie\u00df", ",", "die", "Schu\u00b7le", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPER", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Da\u00df sie nach Latium gern mir in das Leben gefolgt?", "tokens": ["Da\u00df", "sie", "nach", "La\u00b7ti\u00b7um", "gern", "mir", "in", "das", "Le\u00b7ben", "ge\u00b7folgt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "ADV", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "---+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da\u00df ich Natur und Kunst zu schaun mich treulich bestrebe,", "tokens": ["Da\u00df", "ich", "Na\u00b7tur", "und", "Kunst", "zu", "schaun", "mich", "treu\u00b7lich", "be\u00b7stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Da\u00df kein Name mich t\u00e4uscht, da\u00df mich kein Dogma beschr\u00e4nkt?", "tokens": ["Da\u00df", "kein", "Na\u00b7me", "mich", "t\u00e4uscht", ",", "da\u00df", "mich", "kein", "Dog\u00b7ma", "be\u00b7schr\u00e4nkt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Da\u00df nicht des Lebens bedingender Drang mich, den Menschen, ver\u00e4ndert,", "tokens": ["Da\u00df", "nicht", "des", "Le\u00b7bens", "be\u00b7din\u00b7gen\u00b7der", "Drang", "mich", ",", "den", "Men\u00b7schen", ",", "ver\u00b7\u00e4n\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJA", "NN", "PPER", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "++-+--+--+--+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Da\u00df ich der Heuchelei d\u00fcrftige Maske verschm\u00e4ht?", "tokens": ["Da\u00df", "ich", "der", "Heu\u00b7che\u00b7lei", "d\u00fcrf\u00b7ti\u00b7ge", "Mas\u00b7ke", "ver\u00b7schm\u00e4ht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Solcher Fehler, die du, o Muse, so emsig gepfleget,", "tokens": ["Sol\u00b7cher", "Feh\u00b7ler", ",", "die", "du", ",", "o", "Mu\u00b7se", ",", "so", "em\u00b7sig", "ge\u00b7pfle\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "PPER", "$,", "FM", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Zeihet der P\u00f6bel mich; P\u00f6bel nur sieht er in mir.", "tokens": ["Zei\u00b7het", "der", "P\u00f6\u00b7bel", "mich", ";", "P\u00f6\u00b7bel", "nur", "sieht", "er", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$.", "NN", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.11": {"text": "Ja, sogar der Bessere selbst, gutm\u00fctig und bieder,", "tokens": ["Ja", ",", "so\u00b7gar", "der", "Bes\u00b7se\u00b7re", "selbst", ",", "gut\u00b7m\u00fc\u00b7tig", "und", "bie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "ADV", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+----+--+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Will mich anders; doch du, Muse, befiehlst mir allein.", "tokens": ["Will", "mich", "an\u00b7ders", ";", "doch", "du", ",", "Mu\u00b7se", ",", "be\u00b7fiehlst", "mir", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$.", "KON", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Denn du bist es allein, die noch mir die innere Jugend", "tokens": ["Denn", "du", "bist", "es", "al\u00b7lein", ",", "die", "noch", "mir", "die", "in\u00b7ne\u00b7re", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Frisch erneuest und sie mir bis zu Ende versprichst.", "tokens": ["Frisch", "er\u00b7neu\u00b7est", "und", "sie", "mir", "bis", "zu", "En\u00b7de", "ver\u00b7sprichst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "PPER", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Aber verdopple nunmehr, o G\u00f6ttin, die heilige Sorgfalt!", "tokens": ["A\u00b7ber", "ver\u00b7dopp\u00b7le", "nun\u00b7mehr", ",", "o", "G\u00f6t\u00b7tin", ",", "die", "hei\u00b7li\u00b7ge", "Sorg\u00b7falt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Ach! die Scheitel umwallt reichlich die Locke nicht mehr:", "tokens": ["Ach", "!", "die", "Schei\u00b7tel", "um\u00b7wallt", "reich\u00b7lich", "die", "Lo\u00b7cke", "nicht", "mehr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "VVFIN", "ADJD", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Da bedarf man der Kr\u00e4nze, sich selbst und andre zu t\u00e4uschen;", "tokens": ["Da", "be\u00b7darf", "man", "der", "Kr\u00e4n\u00b7ze", ",", "sich", "selbst", "und", "and\u00b7re", "zu", "t\u00e4u\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN", "$,", "PRF", "ADV", "KON", "PIS", "PTKZU", "VVINF", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Kr\u00e4nzte doch C\u00e4sar selbst nur aus Bed\u00fcrfnis das Haupt.", "tokens": ["Kr\u00e4nz\u00b7te", "doch", "C\u00e4\u00b7sar", "selbst", "nur", "aus", "Be\u00b7d\u00fcrf\u00b7nis", "das", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "ADV", "ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+---+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Hast du ein Lorbeerreis mir bestimmt, so la\u00df es am Zweige", "tokens": ["Hast", "du", "ein", "Lor\u00b7beer\u00b7reis", "mir", "be\u00b7stimmt", ",", "so", "la\u00df", "es", "am", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPER", "VVPP", "$,", "ADV", "VVIMP", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "Weiter gr\u00fcnen, und gib einst es dem W\u00fcrdigern hin;", "tokens": ["Wei\u00b7ter", "gr\u00fc\u00b7nen", ",", "und", "gib", "einst", "es", "dem", "W\u00fcr\u00b7di\u00b7gern", "hin", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "KON", "VVIMP", "ADV", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Aber Rosen winde genug zum h\u00e4uslichen Kranze;", "tokens": ["A\u00b7ber", "Ro\u00b7sen", "win\u00b7de", "ge\u00b7nug", "zum", "h\u00e4us\u00b7li\u00b7chen", "Kran\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Bald als Lilie schlingt silberne Locke sich durch.", "tokens": ["Bald", "als", "Li\u00b7lie", "schlingt", "sil\u00b7ber\u00b7ne", "Lo\u00b7cke", "sich", "durch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "NN", "VVFIN", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.23": {"text": "Sch\u00fcre die Gattin das Feuer, auf reinlichem Herde zu kochen!", "tokens": ["Sch\u00fc\u00b7re", "die", "Gat\u00b7tin", "das", "Feu\u00b7er", ",", "auf", "rein\u00b7li\u00b7chem", "Her\u00b7de", "zu", "ko\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.24": {"text": "Werfe der Knabe das Reis, spielend, gesch\u00e4ftig dazu!", "tokens": ["Wer\u00b7fe", "der", "Kna\u00b7be", "das", "Reis", ",", "spie\u00b7lend", ",", "ge\u00b7sch\u00e4f\u00b7tig", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "ADJD", "$,", "ADJD", "PAV", "$."], "meter": "+--+--++--+-++", "measure": "dactylic.di.plus"}, "line.25": {"text": "La\u00df im Becher nicht fehlen den Wein! Gespr\u00e4chige Freunde,", "tokens": ["La\u00df", "im", "Be\u00b7cher", "nicht", "feh\u00b7len", "den", "Wein", "!", "Ge\u00b7spr\u00e4\u00b7chi\u00b7ge", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "APPRART", "NN", "PTKNEG", "VVFIN", "ART", "NN", "$.", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Gleichgesinnte, herein! Kr\u00e4nze, sie warten auf euch.", "tokens": ["Gleich\u00b7ge\u00b7sinn\u00b7te", ",", "her\u00b7ein", "!", "Kr\u00e4n\u00b7ze", ",", "sie", "war\u00b7ten", "auf", "euch", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$.", "NN", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.27": {"text": "Erst die Gesundheit des Mannes, der, endlich vom Namen Homeros", "tokens": ["Erst", "die", "Ge\u00b7sund\u00b7heit", "des", "Man\u00b7nes", ",", "der", ",", "end\u00b7lich", "vom", "Na\u00b7men", "Ho\u00b7me\u00b7ros"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "$,", "ADV", "APPRART", "NN", "NE"], "meter": "+--+--+--+--+-+-+", "measure": "dactylic.tetra.plus"}, "line.28": {"text": "K\u00fchn uns befreiend, uns auch ruft in die vollere Bahn.", "tokens": ["K\u00fchn", "uns", "be\u00b7frei\u00b7end", ",", "uns", "auch", "ruft", "in", "die", "vol\u00b7le\u00b7re", "Bahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VVPP", "$,", "PPER", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Denn wer wagte mit G\u00f6ttern den Kampf? und wer mit dem Einen?", "tokens": ["Denn", "wer", "wag\u00b7te", "mit", "G\u00f6t\u00b7tern", "den", "Kampf", "?", "und", "wer", "mit", "dem", "Ei\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "APPR", "NN", "ART", "NN", "$.", "KON", "PWS", "APPR", "ART", "NN", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.30": {"text": "Doch Homeride zu sein, auch nur als letzter, ist sch\u00f6n.", "tokens": ["Doch", "Ho\u00b7me\u00b7ri\u00b7de", "zu", "sein", ",", "auch", "nur", "als", "letz\u00b7ter", ",", "ist", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NE", "PTKZU", "VAINF", "$,", "ADV", "ADV", "KOUS", "ADJA", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Darum h\u00f6ret das neuste Gedicht! Noch einmal getrunken!", "tokens": ["Da\u00b7rum", "h\u00f6\u00b7ret", "das", "neus\u00b7te", "Ge\u00b7dicht", "!", "Noch", "ein\u00b7mal", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "$.", "ADV", "ADV", "VVPP", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.32": {"text": "Euch besteche der Wein, Freundschaft und Liebe das Ohr.", "tokens": ["Euch", "be\u00b7ste\u00b7che", "der", "Wein", ",", "Freund\u00b7schaft", "und", "Lie\u00b7be", "das", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.33": {"text": "Deutschen selber f\u00fchr ich euch zu, in die stillere Wohnung,", "tokens": ["Deut\u00b7schen", "sel\u00b7ber", "f\u00fchr", "ich", "euch", "zu", ",", "in", "die", "stil\u00b7le\u00b7re", "Woh\u00b7nung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.34": {"text": "Wo sich, nah der Natur, menschlich der Mensch noch erzieht.", "tokens": ["Wo", "sich", ",", "nah", "der", "Na\u00b7tur", ",", "menschlich", "der", "Mensch", "noch", "er\u00b7zieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "$,", "ADJD", "ART", "NN", "$,", "ADJD", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.35": {"text": "Uns begleite des Dichters Geist, der seine Luise", "tokens": ["Uns", "be\u00b7glei\u00b7te", "des", "Dich\u00b7ters", "Geist", ",", "der", "sei\u00b7ne", "Lu\u00b7i\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.36": {"text": "Rasch dem w\u00fcrdigen Freund, uns zu entz\u00fccken, verband.", "tokens": ["Rasch", "dem", "w\u00fcr\u00b7di\u00b7gen", "Freund", ",", "uns", "zu", "ent\u00b7z\u00fc\u00b7cken", ",", "ver\u00b7band", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.37": {"text": "Auch die traurigen Bilder der Zeit, sie f\u00fchr ich vor\u00fcber;", "tokens": ["Auch", "die", "trau\u00b7ri\u00b7gen", "Bil\u00b7der", "der", "Zeit", ",", "sie", "f\u00fchr", "ich", "vor\u00b7\u00fc\u00b7ber", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.38": {"text": "Aber es siege der Mut in dem gesunden Geschlecht.", "tokens": ["A\u00b7ber", "es", "sie\u00b7ge", "der", "Mut", "in", "dem", "ge\u00b7sun\u00b7den", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.39": {"text": "Hab ich euch Tr\u00e4nen ins Auge gelockt und Lust in die Seele", "tokens": ["Hab", "ich", "euch", "Tr\u00e4\u00b7nen", "ins", "Au\u00b7ge", "ge\u00b7lockt", "und", "Lust", "in", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "NN", "APPRART", "NN", "VVPP", "KON", "NN", "APPR", "ART", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.40": {"text": "Singend gefl\u00f6\u00dft, so kommt, dr\u00fccket mich herzlich ans Herz!", "tokens": ["Sin\u00b7gend", "ge\u00b7fl\u00f6\u00dft", ",", "so", "kommt", ",", "dr\u00fc\u00b7cket", "mich", "herz\u00b7lich", "ans", "Herz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,", "ADV", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.41": {"text": "Weise denn sei das Gespr\u00e4ch ! Uns lehret Weisheit am Ende", "tokens": ["Wei\u00b7se", "denn", "sei", "das", "Ge\u00b7spr\u00e4ch", "!", "Uns", "leh\u00b7ret", "Weis\u00b7heit", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "$.", "PPER", "VVFIN", "NN", "APPRART", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.42": {"text": "Das Jahrhundert; wen hat das Geschick nicht gepr\u00fcft?", "tokens": ["Das", "Jahr\u00b7hun\u00b7dert", ";", "wen", "hat", "das", "Ge\u00b7schick", "nicht", "ge\u00b7pr\u00fcft", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "VAFIN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Blicket heiterer nun auf jene Schmerzen zur\u00fccke,", "tokens": ["Bli\u00b7cket", "hei\u00b7te\u00b7rer", "nun", "auf", "je\u00b7ne", "Schmer\u00b7zen", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.44": {"text": "Wenn euch ein fr\u00f6hlicher Sinn manches entbehrlich erkl\u00e4rt.", "tokens": ["Wenn", "euch", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Sinn", "man\u00b7ches", "ent\u00b7behr\u00b7lich", "er\u00b7kl\u00e4rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PIS", "ADJD", "VVPP", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.45": {"text": "Menschen lernten wir kennen und Nationen; so la\u00dft uns,", "tokens": ["Men\u00b7schen", "lern\u00b7ten", "wir", "ken\u00b7nen", "und", "Na\u00b7ti\u00b7o\u00b7nen", ";", "so", "la\u00dft", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVINF", "KON", "NN", "$.", "ADV", "VVIMP", "PPER", "$,"], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.46": {"text": "Unser eigenes Herz kennend, uns dessen erfreun.", "tokens": ["Un\u00b7ser", "ei\u00b7ge\u00b7nes", "Herz", "ken\u00b7nend", ",", "uns", "des\u00b7sen", "er\u00b7freun", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$,", "PPER", "PDS", "VVINF", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}}}}}