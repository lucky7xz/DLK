{"textgrid.poem.46694": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[den du weder hast geboren]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den du weder hast geboren,", "tokens": ["Den", "du", "we\u00b7der", "hast", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ges\u00e4ugt an Mutterbrust,", "tokens": ["Noch", "ge\u00b7s\u00e4ugt", "an", "Mut\u00b7ter\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihn mit freier Lieb erkoren,", "tokens": ["Ihn", "mit", "frei\u00b7er", "Lieb", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eltern gleich an Schmerz und Lust;", "tokens": ["El\u00b7tern", "gleich", "an", "Schmerz", "und", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "H\u00e4ttest ihn wie wir verloren,", "tokens": ["H\u00e4t\u00b7test", "ihn", "wie", "wir", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ersparst uns den Verlust.", "tokens": ["Und", "er\u00b7sparst", "uns", "den", "Ver\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ja, er wird zur\u00fcckbeschworen", "tokens": ["Ja", ",", "er", "wird", "zu\u00b7r\u00fcck\u00b7be\u00b7schwo\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von des Todes dunklen Thoren,", "tokens": ["Von", "des", "To\u00b7des", "dunk\u00b7len", "Tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn zum Guten, das du thust,", "tokens": ["Wenn", "zum", "Gu\u00b7ten", ",", "das", "du", "thust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Thut der gute Gott das Beste!", "tokens": ["Thut", "der", "gu\u00b7te", "Gott", "das", "Bes\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dann zu seinem Wiegenfeste,", "tokens": ["Dann", "zu", "sei\u00b7nem", "Wie\u00b7gen\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Das sein Heimgang wollte werden,", "tokens": ["Das", "sein", "Heim\u00b7gang", "woll\u00b7te", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Soll, solang' er geht auf Erden,", "tokens": ["Soll", ",", "so\u00b7lang'", "er", "geht", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Er zuerst der Mutter danken,", "tokens": ["Er", "zu\u00b7erst", "der", "Mut\u00b7ter", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Die ihn einf\u00fchrt' in die Schranken,", "tokens": ["Die", "ihn", "ein\u00b7f\u00fchrt'", "in", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Und dann dir, die mit Beschwerden", "tokens": ["Und", "dann", "dir", ",", "die", "mit", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Uns zur\u00fcck gebar den Kranken.", "tokens": ["Uns", "zu\u00b7r\u00fcck", "ge\u00b7bar", "den", "Kran\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Den du weder hast geboren,", "tokens": ["Den", "du", "we\u00b7der", "hast", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "KON", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch ges\u00e4ugt an Mutterbrust,", "tokens": ["Noch", "ge\u00b7s\u00e4ugt", "an", "Mut\u00b7ter\u00b7brust", ","], "token_info":