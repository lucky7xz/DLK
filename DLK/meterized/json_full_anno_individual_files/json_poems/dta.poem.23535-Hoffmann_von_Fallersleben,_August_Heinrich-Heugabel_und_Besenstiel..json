{"dta.poem.23535": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heugabel und Besenstiel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heugabel und Besenstiel,", "tokens": ["Heu\u00b7ga\u00b7bel", "und", "Be\u00b7sen\u00b7stiel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die wollten sich verm\u00e4hlen;", "tokens": ["Die", "woll\u00b7ten", "sich", "ver\u00b7m\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da gab's im ganzen Land gar viel", "tokens": ["Da", "gab's", "im", "gan\u00b7zen", "Land", "gar", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ADV", "PIAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und mancherlei zu erz\u00e4hlen.", "tokens": ["Und", "man\u00b7cher\u00b7lei", "zu", "er\u00b7z\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Was ist das f\u00fcr ein Paar!", "tokens": ["Was", "ist", "das", "f\u00fcr", "ein", "Paar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Er ist zu Haus das ganze Jahr,", "tokens": ["Er", "ist", "zu", "Haus", "das", "gan\u00b7ze", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie drau\u00dfen auf Wegen und Gassen.", "tokens": ["Sie", "drau\u00b7\u00dfen", "auf", "We\u00b7gen", "und", "Gas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Er denkt an Stub' und Flur,", "tokens": ["Er", "denkt", "an", "Stub'", "und", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Sie strebet nach dem Hohen nur,", "tokens": ["Sie", "stre\u00b7bet", "nach", "dem", "Ho\u00b7hen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.4": {"line.1": {"text": "Bei Fr\u00fchlingssonnenschein", "tokens": ["Bei", "Fr\u00fch\u00b7lings\u00b7son\u00b7nen\u00b7schein"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Will ", "tokens": ["Will"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Er aber mu\u00df Jahr aus Jahr ein,", "tokens": ["Er", "a\u00b7ber", "mu\u00df", "Jahr", "aus", "Jahr", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Er muss sich immer r&#252;hren.", "tokens": ["Er", "muss", "sich", "im\u00b7mer", "r", "&#252;", "hren", "."], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADV", "ADJD", "$(", "VVINF", "$."], "meter": "-+-+---", "measure": "unknown.measure.di"}}, "stanza.5": {"line.1": {"text": "Doch als die Trauung war,", "tokens": ["Doch", "als", "die", "Trau\u00b7ung", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da wurden die M\u00e4uler stille;", "tokens": ["Da", "wur\u00b7den", "die", "M\u00e4u\u00b7ler", "stil\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Heugabel und Besenstiel blieb ein Paar,", "tokens": ["Heu\u00b7ga\u00b7bel", "und", "Be\u00b7sen\u00b7stiel", "blieb", "ein", "Paar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "I nun, es war ihr Wille.", "tokens": ["I", "nun", ",", "es", "war", "ihr", "Wil\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hei\u00dfa! das ganze Land", "tokens": ["Hei\u00b7\u00dfa", "!", "das", "gan\u00b7ze", "Land"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Zur Hochzeit war geladen,", "tokens": ["Zur", "Hoch\u00b7zeit", "war", "ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verwandt, bekannt und unbekannt,", "tokens": ["Ver\u00b7wandt", ",", "be\u00b7kannt", "und", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Krummen, die Schiefen, die Graden.", "tokens": ["Die", "Krum\u00b7men", ",", "die", "Schie\u00b7fen", ",", "die", "Gra\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.7": {"line.1": {"text": "Da tanzten munter und frisch", "tokens": ["Da", "tanz\u00b7ten", "mun\u00b7ter", "und", "frisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Die Schemel, die H\u00fctschen, die B\u00e4nke,", "tokens": ["Die", "Sche\u00b7mel", ",", "die", "H\u00fct\u00b7schen", ",", "die", "B\u00e4n\u00b7ke", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Die Kannen, die Mulden, die St\u00fchl' und die Tisch'", "tokens": ["Die", "Kan\u00b7nen", ",", "die", "Mul\u00b7den", ",", "die", "St\u00fchl'", "und", "die", "Tisch'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und Kisten und Kasten und Schr\u00e4nke.", "tokens": ["Und", "Kis\u00b7ten", "und", "Kas\u00b7ten", "und", "Schr\u00e4n\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Hei\u00dfa! nun wurden sie", "tokens": ["Hei\u00b7\u00dfa", "!", "nun", "wur\u00b7den", "sie"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VAFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Poetisch \u00fcber die Ma\u00dfen,", "tokens": ["Poe\u00b7tisch", "\u00fc\u00b7ber", "die", "Ma\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Da\u00df sie vor lauter Poesie", "tokens": ["Da\u00df", "sie", "vor", "lau\u00b7ter", "Poe\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Stand, Rang und W\u00fcrde verga\u00dfen:", "tokens": ["Stand", ",", "Rang", "und", "W\u00fcr\u00b7de", "ver\u00b7ga\u00b7\u00dfen", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Die Liebe macht uns gleich,", "tokens": ["Die", "Lie\u00b7be", "macht", "uns", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Frau Besenstiel, Herr Gabel!", "tokens": ["Frau", "Be\u00b7sen\u00b7stiel", ",", "Herr", "Ga\u00b7bel", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NN", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Will' ist unser Himmelreich \u2014", "tokens": ["Der", "Will'", "ist", "un\u00b7ser", "Him\u00b7mel\u00b7reich"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und das ist keine Fabel.", "tokens": ["Und", "das", "ist", "kei\u00b7ne", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}