{"dta.poem.532": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Battulus,  da  Sylvia  seine Gedichte  \n verbrannt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6r Battulus Po&#235;t, wenn dein verliebt Gedichte", "tokens": ["H\u00f6r", "Bat\u00b7tu\u00b7lus", "Po", "&#235;", "t", ",", "wenn", "dein", "ver\u00b7liebt", "Ge\u00b7dich\u00b7te"], "token_info": ["word", "word", "word", "XML_entity", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "$(", "VVFIN", "$,", "KOUS", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6nen ", "tokens": ["Der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "So tische ihr nicht auf verbuhlte Lust-Gedichte/", "tokens": ["So", "ti\u00b7sche", "ihr", "nicht", "auf", "ver\u00b7buhl\u00b7te", "Lust\u00b7Ge\u00b7dich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und schenck der Geilheit Most nicht in den Becher ein.", "tokens": ["Und", "schenck", "der", "Geil\u00b7heit", "Most", "nicht", "in", "den", "Be\u00b7cher", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr Auge liebet nicht was nach der Geilheit schmecket/", "tokens": ["Ihr", "Au\u00b7ge", "lie\u00b7bet", "nicht", "was", "nach", "der", "Geil\u00b7heit", "schme\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "PRELS", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verbuhlte Reden flieht sie \u00e4rger als das Gifft;", "tokens": ["Ver\u00b7buhl\u00b7te", "Re\u00b7den", "flieht", "sie", "\u00e4r\u00b7ger", "als", "das", "Gifft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Weil nun in deiner Schrifft die Geilheit war verstecket/", "tokens": ["Weil", "nun", "in", "dei\u00b7ner", "Schrifft", "die", "Geil\u00b7heit", "war", "ver\u00b7ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So klage nicht/ wenn dich gerechter Eyfer trifft.", "tokens": ["So", "kla\u00b7ge", "nicht", "/", "wenn", "dich", "ge\u00b7rech\u00b7ter", "Ey\u00b7fer", "trifft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Nahme ", "tokens": ["Der", "Nah\u00b7me"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Und dieses \u00e4rgert sie wenn man sie ", "tokens": ["Und", "die\u00b7ses", "\u00e4r\u00b7gert", "sie", "wenn", "man", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "PPER", "KOUS", "PIS", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Jhr Wesen wil sich nicht in ", "tokens": ["Ihr", "We\u00b7sen", "wil", "sich", "nicht", "in"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PRF", "PTKNEG", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Derselben Eigenschafft nicht aus den Augen glei\u00dft.", "tokens": ["Der\u00b7sel\u00b7ben", "Ei\u00b7gen\u00b7schafft", "nicht", "aus", "den", "Au\u00b7gen", "glei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die M\u00e4dgens/ die an Brunst die ", "tokens": ["Die", "M\u00e4d\u00b7gens", "/", "die", "an", "Brunst", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Die nehmen ein Gedicht von geilen Worten an/", "tokens": ["Die", "neh\u00b7men", "ein", "Ge\u00b7dicht", "von", "gei\u00b7len", "Wor\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bey ihnen kan man Gunst durch solche ", "tokens": ["Bey", "ih\u00b7nen", "kan", "man", "Gunst", "durch", "sol\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VMFIN", "PIS", "NN", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.17": {"text": "Jhr reiner Eyfer lie\u00df die garstgen ", "tokens": ["Ihr", "rei\u00b7ner", "Ey\u00b7fer", "lie\u00df", "die", "garst\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Hierinnen wolte sie der ", "tokens": ["Hie\u00b7rin\u00b7nen", "wol\u00b7te", "sie", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Sie sprach: Die ", "tokens": ["Sie", "sprach", ":", "Die"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Drum werffe ich sie jetzt in seine Flammen ein.", "tokens": ["Drum", "werf\u00b7fe", "ich", "sie", "jetzt", "in", "sei\u00b7ne", "Flam\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}