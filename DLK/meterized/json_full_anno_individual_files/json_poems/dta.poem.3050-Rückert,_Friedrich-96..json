{"dta.poem.3050": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "96.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nie auf den Gegenstand wird ganz ein Urtheil passen,", "tokens": ["Nie", "auf", "den", "Ge\u00b7gen\u00b7stand", "wird", "ganz", "ein", "Ur\u00b7theil", "pas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum lieber allgemein mag ich Urtheile fassen,", "tokens": ["Drum", "lie\u00b7ber", "all\u00b7ge\u00b7mein", "mag", "ich", "Ur\u00b7thei\u00b7le", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ADJD", "VMFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Damit sich, was davon nicht zu dem Falle passe,", "tokens": ["Da\u00b7mit", "sich", ",", "was", "da\u00b7von", "nicht", "zu", "dem", "Fal\u00b7le", "pas\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "$,", "PRELS", "PAV", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hier zu Grunde liegt, beziehn auf andre lasse;", "tokens": ["Der", "hier", "zu", "Grun\u00b7de", "liegt", ",", "be\u00b7ziehn", "auf", "and\u00b7re", "las\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,", "VVFIN", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn immer wahr wird seyn, was du als wahr erkannt,", "tokens": ["Denn", "im\u00b7mer", "wahr", "wird", "seyn", ",", "was", "du", "als", "wahr", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VAFIN", "VAINF", "$,", "PWS", "PPER", "KOUS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn du es auch villeicht auf Falsches angewandt.", "tokens": ["Wenn", "du", "es", "auch", "vil\u00b7leicht", "auf", "Fal\u00b7sches", "an\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}