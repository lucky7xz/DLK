{"textgrid.poem.60161": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Graf nur klein ist unsers Lebens Werth", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Graf nur klein ist unsers Lebens Werth", "tokens": ["O", "Graf", "nur", "klein", "ist", "un\u00b7sers", "Le\u00b7bens", "Werth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So bald in einem unsrer Glieder", "tokens": ["So", "bald", "in", "ei\u00b7nem", "uns\u00b7rer", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schmerz, wie ein Tyrann verf\u00e4hrt,", "tokens": ["Der", "Schmerz", ",", "wie", "ein", "Ty\u00b7rann", "ver\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4llt in des Menschen Bau der ganze Staat danieder;", "tokens": ["F\u00e4llt", "in", "des", "Men\u00b7schen", "Bau", "der", "gan\u00b7ze", "Staat", "da\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Kranckheit sa\u00df in meiner Stirne tief", "tokens": ["Die", "Kran\u00b7ck\u00b7heit", "sa\u00df", "in", "mei\u00b7ner", "Stir\u00b7ne", "tief"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ganz bet\u00e4ubt von ihrem Grimme,", "tokens": ["Und", "ganz", "be\u00b7t\u00e4ubt", "von", "ih\u00b7rem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ob ich Todesschlummer schlief,", "tokens": ["Als", "ob", "ich", "To\u00b7des\u00b7schlum\u00b7mer", "schlief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "H\u00f6rt ich noch kaum das Herz in eines Freundes Stimme.", "tokens": ["H\u00f6rt", "ich", "noch", "kaum", "das", "Herz", "in", "ei\u00b7nes", "Freun\u00b7des", "Stim\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "O welch ein Tand! wie wenig schmeichlerisch", "tokens": ["O", "welch", "ein", "Tand", "!", "wie", "we\u00b7nig", "schmeich\u00b7le\u00b7risch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PIAT", "ART", "NN", "$.", "PWAV", "PIS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ist diese Welt des Kranken Blicke!", "tokens": ["Ist", "die\u00b7se", "Welt", "des", "Kran\u00b7ken", "Bli\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Kranken, der des F\u00fcrsten Tisch", "tokens": ["Des", "Kran\u00b7ken", ",", "der", "des", "F\u00fcrs\u00b7ten", "Tisch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht w\u00fcnscht, nicht einen Sitz als G\u00fcnstling bey dem Gl\u00fccke!", "tokens": ["Nicht", "w\u00fcnscht", ",", "nicht", "ei\u00b7nen", "Sitz", "als", "G\u00fcnst\u00b7ling", "bey", "dem", "Gl\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PTKNEG", "ART", "NN", "KOKOM", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wenn vor dem Herscher einer halben Welt", "tokens": ["Wenn", "vor", "dem", "Her\u00b7scher", "ei\u00b7ner", "hal\u00b7ben", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leib-Aerzte stummen Zweifel sagen;", "tokens": ["Leib\u00b7Ae\u00b7rz\u00b7te", "stum\u00b7men", "Zwei\u00b7fel", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Dann reitzet, neben ihn gestellt,", "tokens": ["Dann", "reit\u00b7zet", ",", "ne\u00b7ben", "ihn", "ge\u00b7stellt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn nicht der Glanz vor dem die V\u00f6lker kniend lagen.", "tokens": ["Ihn", "nicht", "der", "Glanz", "vor", "dem", "die", "V\u00f6l\u00b7ker", "kni\u00b7end", "la\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "APPR", "ART", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nicht Geld, nicht Ruhm, verwachter N\u00e4chte Prei\u00df!", "tokens": ["Nicht", "Geld", ",", "nicht", "Ruhm", ",", "ver\u00b7wach\u00b7ter", "N\u00e4ch\u00b7te", "Prei\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PTKNEG", "NN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erkaufen uns noch Lust zu leben:", "tokens": ["Er\u00b7kau\u00b7fen", "uns", "noch", "Lust", "zu", "le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der J\u00fcngling zittert wie der Grei\u00df", "tokens": ["Der", "J\u00fcng\u00b7ling", "zit\u00b7tert", "wie", "der", "Grei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kein Erfinder kann sich Kraft zur Freude geben.", "tokens": ["Und", "kein", "Er\u00b7fin\u00b7der", "kann", "sich", "Kraft", "zur", "Freu\u00b7de", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PRF", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Des Kranken Ohr h\u00f6rt nicht auf Sayten-Klang;", "tokens": ["Des", "Kran\u00b7ken", "Ohr", "h\u00f6rt", "nicht", "auf", "Say\u00b7ten\u00b7Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er mag im Lenz auf Rosen liegen:", "tokens": ["Er", "mag", "im", "Lenz", "auf", "Ro\u00b7sen", "lie\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch singt der Nachtigal Gesang", "tokens": ["Doch", "singt", "der", "Nach\u00b7ti\u00b7gal", "Ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm keinen Schlaf ins Aug', ins Herze kein Vergn\u00fcgen.", "tokens": ["Ihm", "kei\u00b7nen", "Schlaf", "ins", "Aug'", ",", "ins", "Her\u00b7ze", "kein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "APPRART", "NN", "$,", "APPRART", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Von seinem \u00f6den Lager hei\u00dft er sich,", "tokens": ["Von", "sei\u00b7nem", "\u00f6\u00b7den", "La\u00b7ger", "hei\u00dft", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der L\u00fcste lockend Heer entfernen;", "tokens": ["Der", "L\u00fcs\u00b7te", "lo\u00b7ckend", "Heer", "ent\u00b7fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und Troz, der keinem Menschen wich,", "tokens": ["Und", "Troz", ",", "der", "kei\u00b7nem", "Men\u00b7schen", "wich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df sich vor kleinem Schmerz gehorsam beugen lernen.", "tokens": ["Mu\u00df", "sich", "vor", "klei\u00b7nem", "Schmerz", "ge\u00b7hor\u00b7sam", "beu\u00b7gen", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ADJA", "NN", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich nicht unbiegsam, nie zum Stolz gew\u00f6hnt,", "tokens": ["Ich", "nicht", "un\u00b7bieg\u00b7sam", ",", "nie", "zum", "Stolz", "ge\u00b7w\u00f6hnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ADJD", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Empfinde mehr der Krankheit Lasten,", "tokens": ["Emp\u00b7fin\u00b7de", "mehr", "der", "Krank\u00b7heit", "Las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als jene Noth da ungest\u00f6hnt,", "tokens": ["Als", "je\u00b7ne", "Noth", "da", "un\u00b7ge\u00b7st\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein K\u00f6rper Tage lang m\u00fchselig muste fasten.", "tokens": ["Mein", "K\u00f6r\u00b7per", "Ta\u00b7ge", "lang", "m\u00fch\u00b7se\u00b7lig", "mus\u00b7te", "fas\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADJD", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Graf, als ich fern von eines Reichen Saal", "tokens": ["Graf", ",", "als", "ich", "fern", "von", "ei\u00b7nes", "Rei\u00b7chen", "Saal"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Holz trug zu kleingebautem Heerde;", "tokens": ["Holz", "trug", "zu", "klein\u00b7ge\u00b7bau\u00b7tem", "Heer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Da war mein selbst bereitet Mahl", "tokens": ["Da", "war", "mein", "selbst", "be\u00b7rei\u00b7tet", "Mahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir k\u00f6stlicher, als jetzt wenn ich geladen werde.", "tokens": ["Mir", "k\u00f6st\u00b7li\u00b7cher", ",", "als", "jetzt", "wenn", "ich", "ge\u00b7la\u00b7den", "wer\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "KOUS", "ADV", "KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Bey schwarzem Brod und Wasser aus dem Quell", "tokens": ["Bey", "schwar\u00b7zem", "Brod", "und", "Was\u00b7ser", "aus", "dem", "Quell"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sa\u00df frisches Roth auf meiner Wange;", "tokens": ["Sa\u00df", "fri\u00b7sches", "Roth", "auf", "mei\u00b7ner", "Wan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Morgen fand mein Auge hell,", "tokens": ["Der", "Mor\u00b7gen", "fand", "mein", "Au\u00b7ge", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und munter meinen Geist zu fr\u00f6lichem Gesange.", "tokens": ["Und", "mun\u00b7ter", "mei\u00b7nen", "Geist", "zu", "fr\u00f6\u00b7li\u00b7chem", "Ge\u00b7san\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Nicht gr\u00f6\u00dfres Gl\u00fcck, nicht fern gehohlter Wein", "tokens": ["Nicht", "gr\u00f6\u00df\u00b7res", "Gl\u00fcck", ",", "nicht", "fern", "ge\u00b7hohl\u00b7ter", "Wein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJA", "NN", "$,", "PTKNEG", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Macht seeliger die Erden-G\u00e4ste", "tokens": ["Macht", "see\u00b7li\u00b7ger", "die", "Er\u00b7den\u00b7G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bleibt m\u00e4\u00dfige Gesundheit mein:", "tokens": ["Bleibt", "m\u00e4\u00b7\u00dfi\u00b7ge", "Ge\u00b7sund\u00b7heit", "mein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So feyret mein Gesang der Freundschaft Freudenfeste.", "tokens": ["So", "fey\u00b7ret", "mein", "Ge\u00b7sang", "der", "Freund\u00b7schaft", "Freu\u00b7den\u00b7fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "O Graf nur klein ist unsers Lebens Werth", "tokens": ["O", "Graf", "nur", "klein", "ist", "un\u00b7sers", "Le\u00b7bens", "Werth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So bald in einem unsrer Glieder", "tokens": ["So", "bald", "in", "ei\u00b7nem", "uns\u00b7rer", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schmerz, wie ein Tyrann verf\u00e4hrt,", "tokens": ["Der", "Schmerz", ",", "wie", "ein", "Ty\u00b7rann", "ver\u00b7f\u00e4hrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00e4llt in des Menschen Bau der ganze Staat danieder;", "tokens": ["F\u00e4llt", "in", "des", "Men\u00b7schen", "Bau", "der", "gan\u00b7ze", "Staat", "da\u00b7nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "ART", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "