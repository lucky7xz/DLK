{"textgrid.poem.25493": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erschrick nicht etwa vor den Falten", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erschrick nicht etwa vor den Falten", "tokens": ["Er\u00b7schrick", "nicht", "et\u00b7wa", "vor", "den", "Fal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem ernsten Angesicht'.", "tokens": ["In", "die\u00b7sem", "erns\u00b7ten", "An\u00b7ge\u00b7sicht'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl mu\u00df die Zeit ihr Recht behalten,", "tokens": ["Wohl", "mu\u00df", "die", "Zeit", "ihr", "Recht", "be\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Gl\u00fcck' behielt's die Sorge nicht.", "tokens": ["Zum", "Gl\u00fcck", "be\u00b7hielt's", "die", "Sor\u00b7ge", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht diese hat mit grauen Haaren", "tokens": ["Nicht", "die\u00b7se", "hat", "mit", "grau\u00b7en", "Haa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PDS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Kopf bestreut; die dank' ich blo\u00df den Jahren", "tokens": ["Den", "Kopf", "be\u00b7streut", ";", "die", "dank'", "ich", "blo\u00df", "den", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "ART", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und der Erf\u00fcllung meiner Pflicht.", "tokens": ["Und", "der", "Er\u00b7f\u00fcl\u00b7lung", "mei\u00b7ner", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jetzt bin ich in der Lebenszeit,", "tokens": ["Jetzt", "bin", "ich", "in", "der", "Le\u00b7bens\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wo seltner sich beim Scherz' der Sch\u00f6nen", "tokens": ["Wo", "selt\u00b7ner", "sich", "beim", "Scher\u00b7z'", "der", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Noch meine ernstre Seele freut;", "tokens": ["Noch", "mei\u00b7ne", "ernst\u00b7re", "See\u00b7le", "freut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Man mu\u00df an alles sich gew\u00f6hnen.", "tokens": ["Man", "mu\u00df", "an", "al\u00b7les", "sich", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ich g\u00e4be keinen Pappenstiel", "tokens": ["Ich", "g\u00e4\u00b7be", "kei\u00b7nen", "Pap\u00b7pen\u00b7stiel"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Darum, ein griechisches Profil", "tokens": ["Da\u00b7rum", ",", "ein", "grie\u00b7chi\u00b7sches", "Pro\u00b7fil"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PAV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "F\u00fcr mein Gesicht mir zu erkaufen;", "tokens": ["F\u00fcr", "mein", "Ge\u00b7sicht", "mir", "zu", "er\u00b7kau\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Zu dem jetzt vorgesteckten Ziel'", "tokens": ["Zu", "dem", "jetzt", "vor\u00b7ge\u00b7steck\u00b7ten", "Ziel'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "W\u00fcrd' ich damit nicht schneller laufen.", "tokens": ["W\u00fcrd'", "ich", "da\u00b7mit", "nicht", "schnel\u00b7ler", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "So wie ich bin, so will ich seyn,", "tokens": ["So", "wie", "ich", "bin", ",", "so", "will", "ich", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "$,", "ADV", "VMFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und so mich meinen Freunden geben.", "tokens": ["Und", "so", "mich", "mei\u00b7nen", "Freun\u00b7den", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Wahr dr\u00fcckte die\u00df dem Gyps sich ein.", "tokens": ["Wahr", "dr\u00fcck\u00b7te", "die\u00df", "dem", "Gyps", "sich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PDS", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Was fehlt dem Brustbild' noch, als Leben?", "tokens": ["Was", "fehlt", "dem", "Brust\u00b7bild'", "noch", ",", "als", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "O! la\u00df es, trotz den tiefen Falten,", "tokens": ["O", "!", "la\u00df", "es", ",", "trotz", "den", "tie\u00b7fen", "Fal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "(blieb's mit dem Herzen doch beim alten!)", "tokens": ["(", "blie\u00b7b's", "mit", "dem", "Her\u00b7zen", "doch", "beim", "al\u00b7ten", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "APPR", "ART", "NN", "ADV", "APPRART", "ADJA", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Dir jetzt auch noch willkommen seyn.", "tokens": ["Dir", "jetzt", "auch", "noch", "will\u00b7kom\u00b7men", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Es hat vor dem Originale", "tokens": ["Es", "hat", "vor", "dem", "O\u00b7rig\u00b7i\u00b7na\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.25": {"text": "Den gro\u00dfen Vorzug noch sogar:", "tokens": ["Den", "gro\u00b7\u00dfen", "Vor\u00b7zug", "noch", "so\u00b7gar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Es bleibet ein f\u00fcr allemale", "tokens": ["Es", "blei\u00b7bet", "ein", "f\u00fcr", "al\u00b7le\u00b7ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "So freundlich, als es gestern war.", "tokens": ["So", "freund\u00b7lich", ",", "als", "es", "ge\u00b7stern", "war", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "So nimm das Bild denn g\u00fctig auf,", "tokens": ["So", "nimm", "das", "Bild", "denn", "g\u00fc\u00b7tig", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "ART", "NN", "KON", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wirf dann und wann den Blick darauf,", "tokens": ["Wirf", "dann", "und", "wann", "den", "Blick", "da\u00b7rauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "PWAV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und bin ich einst dahin gegangen,", "tokens": ["Und", "bin", "ich", "einst", "da\u00b7hin", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Von wannen Niemand wiederkehrt,", "tokens": ["Von", "wan\u00b7nen", "Nie\u00b7mand", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "So gib den kalten, bleichen Wangen", "tokens": ["So", "gib", "den", "kal\u00b7ten", ",", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Noch einen letzten Abschiedsku\u00df,", "tokens": ["Noch", "ei\u00b7nen", "letz\u00b7ten", "Ab\u00b7schieds\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der meinen Schatten h\u00f6her ehrt,", "tokens": ["Der", "mei\u00b7nen", "Schat\u00b7ten", "h\u00f6\u00b7her", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Als, (nie erw\u00e4hnt in Flaccus Liede!)", "tokens": ["Als", ",", "(", "nie", "er\u00b7w\u00e4hnt", "in", "Flac\u00b7cus", "Lie\u00b7de", "!", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$,", "$(", "ADV", "VVPP", "APPR", "NE", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Zu Rom die stolze Pyramide", "