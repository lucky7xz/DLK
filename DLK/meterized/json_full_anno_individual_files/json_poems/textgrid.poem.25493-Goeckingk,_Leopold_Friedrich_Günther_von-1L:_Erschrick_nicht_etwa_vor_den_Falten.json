 "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KON", "PWAV", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Und bin ich einst dahin gegangen,", "tokens": ["Und", "bin", "ich", "einst", "da\u00b7hin", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Von wannen Niemand wiederkehrt,", "tokens": ["Von", "wan\u00b7nen", "Nie\u00b7mand", "wie\u00b7der\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "So gib den kalten, bleichen Wangen", "tokens": ["So", "gib", "den", "kal\u00b7ten", ",", "blei\u00b7chen", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Noch einen letzten Abschiedsku\u00df,", "tokens": ["Noch", "ei\u00b7nen", "letz\u00b7ten", "Ab\u00b7schieds\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Der meinen Schatten h\u00f6her ehrt,", "tokens": ["Der", "mei\u00b7nen", "Schat\u00b7ten", "h\u00f6\u00b7her", "ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Als, (nie erw\u00e4hnt in Flaccus Liede!)", "tokens": ["Als", ",", "(", "nie", "er\u00b7w\u00e4hnt", "in", "Flac\u00b7cus", "Lie\u00b7de", "!", ")"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "$,