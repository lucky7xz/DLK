{"dta.poem.6026": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Allein, vom Firmament gedr\u00fcckt, in Creis gebracht,", "tokens": ["Al\u00b7lein", ",", "vom", "Fir\u00b7ma\u00b7ment", "ge\u00b7dr\u00fcckt", ",", "in", "Creis", "ge\u00b7bracht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird sie durch eignen Stoff ern\u00e4hret.", "tokens": ["Wird", "sie", "durch", "eig\u00b7nen", "Stoff", "er\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das erstre Element, woraus sie selbst gemacht,", "tokens": ["Das", "er\u00b7stre", "E\u00b7le\u00b7ment", ",", "wo\u00b7raus", "sie", "selbst", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "ADV", "VVPP", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Das allenthalben fleugt, sich hurtig dreht und kehret,", "tokens": ["Das", "al\u00b7len\u00b7thal\u00b7ben", "fleugt", ",", "sich", "hur\u00b7tig", "dreht", "und", "keh\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VVFIN", "$,", "PRF", "ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Fl\u00f6st ihrem Creise, voller Schein,", "tokens": ["Fl\u00f6st", "ih\u00b7rem", "Crei\u00b7se", ",", "vol\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das, was aus ihrem Schoo\u00df hervor brach, wieder ein.", "tokens": ["Das", ",", "was", "aus", "ih\u00b7rem", "Schoo\u00df", "her\u00b7vor", "brach", ",", "wie\u00b7der", "ein", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "APPR", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}