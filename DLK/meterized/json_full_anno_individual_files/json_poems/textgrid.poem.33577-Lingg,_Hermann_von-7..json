{"textgrid.poem.33577": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verstummt sind nun die Wogen,", "tokens": ["Ver\u00b7stummt", "sind", "nun", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die l\u00e4rmend uns getrennt,", "tokens": ["Die", "l\u00e4r\u00b7mend", "uns", "ge\u00b7trennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wolken sind verzogen,", "tokens": ["Die", "Wol\u00b7ken", "sind", "ver\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Rein strahlt das Firmament,", "tokens": ["Rein", "strahlt", "das", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und wie in jenen R\u00e4umen", "tokens": ["Und", "wie", "in", "je\u00b7nen", "R\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird's hier im Busen still,", "tokens": ["Wird's", "hier", "im", "Bu\u00b7sen", "still", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich kann jetzt wieder tr\u00e4umen", "tokens": ["Ich", "kann", "jetzt", "wie\u00b7der", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und denken, was ich will.", "tokens": ["Und", "den\u00b7ken", ",", "was", "ich", "will", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich denk' an dich! Dein Wesen", "tokens": ["Ich", "denk'", "an", "dich", "!", "Dein", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tritt l\u00e4chelnd auf mich zu.", "tokens": ["Tritt", "l\u00e4\u00b7chelnd", "auf", "mich", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was hat dich mir erlesen,", "tokens": ["Was", "hat", "dich", "mir", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du seltsam R\u00e4tsel du?", "tokens": ["Du", "selt\u00b7sam", "R\u00e4t\u00b7sel", "du", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du seltne Blume, sage,", "tokens": ["Du", "selt\u00b7ne", "Blu\u00b7me", ",", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie kamest du herein", "tokens": ["Wie", "