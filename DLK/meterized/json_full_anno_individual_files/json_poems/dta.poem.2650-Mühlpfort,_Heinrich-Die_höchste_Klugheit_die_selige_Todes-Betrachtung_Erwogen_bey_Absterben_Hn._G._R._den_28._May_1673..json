ord", "punct"], "pos": ["KON", "PWS", "PIS", "ADV", "VVFIN", "$(", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.51": {"text": "Was sollen wir denn nun von unsrer Klugheit schliessen/", "tokens": ["Was", "sol\u00b7len", "wir", "denn", "nun", "von", "uns\u00b7rer", "Klug\u00b7heit", "schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.52": {"text": "Die uns f\u00fcr GOttes Thron zu nichts/ als Thoren macht?", "tokens": ["Die", "uns", "f\u00fcr", "Got\u00b7tes", "Thron", "zu", "nichts", "/", "als", "Tho\u00b7ren", "macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "NN", "APPR", "PIS", "$(", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "Mit recht rufft Salomo: Es ist nur eitel Jammer/", "tokens": ["Mit", "recht", "rufft", "Sa\u00b7lo\u00b7mo", ":", "Es", "ist", "nur", "ei\u00b7tel", "Jam\u00b7mer", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVFIN", "NE", "$.", "PPER", "VAFIN", "ADV", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.54": {"text": "So weit das lichte Rad der g\u00f6ldnen Sonnen geht/", "tokens": ["So", "weit", "das", "lich\u00b7te", "Rad", "der", "g\u00f6ld\u00b7nen", "Son\u00b7nen", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.55": {"text": "Angst/ Gr\u00e4men/ Leid und Qual nagt unsers Hertzens Kammer/", "tokens": ["Angst", "/", "Gr\u00e4\u00b7men", "/", "Leid", "und", "Qual", "nagt", "un\u00b7sers", "Hert\u00b7zens", "Kam\u00b7mer", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.56": {"text": "Da\u00df es zu Tag als Nacht in keiner Ruhe steht.", "tokens": ["Da\u00df", "es", "zu", "Tag", "als", "Nacht", "in", "kei\u00b7ner", "Ru\u00b7he", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KOUS", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.57": {"text": "Auch den/ so hochber\u00fchmt/ mu\u00df man zum Todten zehlen/", "tokens": ["Auch", "den", "/", "so", "hoch\u00b7be\u00b7r\u00fchmt", "/", "mu\u00df", "man", "zum", "Tod\u00b7ten", "zeh\u00b7len", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ADV", "ADJD", "$(", "VMFIN", "PIS", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.58": {"text": "Den klug- und th\u00f6richten scharrt man in einen Sand.", "tokens": ["Den", "klug", "und", "th\u00f6\u00b7rich\u00b7ten", "scharrt", "man", "in", "ei\u00b7nen", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.59": {"text": "Wir werden wol gewi\u00df des rechten Weges fehlen/", "tokens": ["Wir", "wer\u00b7den", "wol", "ge\u00b7wi\u00df", "des", "rech