{"dta.poem.2650": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die h\u00f6chste Klugheit die selige Todes-Betrachtung/  \n Erwogen bey Absterben Hn. G. R. den 28.  \n May 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Mensch/ das kl\u00fcgste Thier/ und Meister aller Sach&#275;/", "tokens": ["Der", "Mensch", "/", "das", "kl\u00fcgs\u00b7te", "Thier", "/", "und", "Meis\u00b7ter", "al\u00b7ler", "Sach", "&#275;", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$(", "KON", "NN", "PIAT", "NN", "$(", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So das gevierdt Rund der weiten Welt umschleust/", "tokens": ["So", "das", "ge\u00b7vierdt", "Rund", "der", "wei\u00b7ten", "Welt", "um\u00b7schleust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kan sich zum Wunderwerck durch Witz und Tugend", "tokens": ["Kan", "sich", "zum", "Wun\u00b7der\u00b7werck", "durch", "Witz", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPRART", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und was nur Athem hat/ beherrscht sein kluger Geist.", "tokens": ["Und", "was", "nur", "A\u00b7them", "hat", "/", "be\u00b7herrscht", "sein", "klu\u00b7ger", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NN", "VAFIN", "$(", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er hat nicht nur allein den Schauplatz dieser Erden", "tokens": ["Er", "hat", "nicht", "nur", "al\u00b7lein", "den", "Schau\u00b7platz", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADV", "ADV", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In Gr\u00e4ntzen abgesteckt/ die Felder angebaut/", "tokens": ["In", "Gr\u00e4nt\u00b7zen", "ab\u00b7ge\u00b7steckt", "/", "die", "Fel\u00b7der", "an\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$(", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es haben ihm die Thier\u2019 auch m\u00fcssen dienstbar werden/", "tokens": ["Es", "ha\u00b7ben", "ihm", "die", "Thier'", "auch", "m\u00fcs\u00b7sen", "dienst\u00b7bar", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "ADV", "VMFIN", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sein Verstand erlernt\u2019 jedweder Blum und Kraut.", "tokens": ["Und", "sein", "Ver\u00b7stand", "er\u00b7lernt'", "jed\u00b7we\u00b7der", "Blum", "und", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Jhm blieb der Sternen Reyh und Namen unverborgen/", "tokens": ["Jhm", "blieb", "der", "Ster\u00b7nen", "Reyh", "und", "Na\u00b7men", "un\u00b7ver\u00b7bor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er nahm des Meeres Fluth/ der Fl\u00fcsse Quell gewahr/", "tokens": ["Er", "nahm", "des", "Mee\u00b7res", "Fluth", "/", "der", "Fl\u00fcs\u00b7se", "Quell", "ge\u00b7wahr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und endlich must\u2019 er auch f\u00fcr die Verfassung sorgen/", "tokens": ["Und", "end\u00b7lich", "must'", "er", "auch", "f\u00fcr", "die", "Ver\u00b7fas\u00b7sung", "sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df er und sein Geschlecht sey sicher in Gefahr.", "tokens": ["Da\u00df", "er", "und", "sein", "Ge\u00b7schlecht", "sey", "si\u00b7cher", "in", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Den Zwang die Nothdurfft aus Gehorsam und Gesetze/", "tokens": ["Den", "Zwang", "die", "Noth\u00b7durfft", "aus", "Ge\u00b7hor\u00b7sam", "und", "Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zwey Seulen/ drauffberuht gemeine Policey.", "tokens": ["Zwey", "Seu\u00b7len", "/", "drauff\u00b7be\u00b7ruht", "ge\u00b7mei\u00b7ne", "Po\u00b7li\u00b7cey", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$(", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ja er wie\u00df sattsam aus durch seiner Klugheit Sch\u00e4tze/", "tokens": ["Ja", "er", "wie\u00df", "satt\u00b7sam", "aus", "durch", "sei\u00b7ner", "Klug\u00b7heit", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ADJD", "APPR", "APPR", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Da\u00df er des Landes Herr/ der Erden Herrscher sey.", "tokens": ["Da\u00df", "er", "des", "Lan\u00b7des", "Herr", "/", "der", "Er\u00b7den", "Herr\u00b7scher", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$(", "ART", "NN", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nun musten mit Vernunfft die ungez\u00e4hmten Sinnen", "tokens": ["Nun", "mus\u00b7ten", "mit", "Ver\u00b7nunfft", "die", "un\u00b7ge\u00b7z\u00e4hm\u00b7ten", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der w\u00fcsten Sterbligkeit noch werden ausgeziert/", "tokens": ["Der", "w\u00fcs\u00b7ten", "Ster\u00b7blig\u00b7keit", "noch", "wer\u00b7den", "aus\u00b7ge\u00b7ziert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Damit ihr gantzes Thun/ ihr Leben und Beginnen/", "tokens": ["Da\u00b7mit", "ihr", "gant\u00b7zes", "Thun", "/", "ihr", "Le\u00b7ben", "und", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "$(", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "W\u00fcrd\u2019 auff den rechten Zweck des Regiments gef\u00fchrt.", "tokens": ["W\u00fcrd'", "auff", "den", "rech\u00b7ten", "Zweck", "des", "Re\u00b7gi\u00b7ments", "ge\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Denn als die K\u00f6nigreich und V\u00f6lcker sich gebreitet/", "tokens": ["Denn", "als", "die", "K\u00f6\u00b7nig\u00b7reich", "und", "V\u00f6l\u00b7cker", "sich", "ge\u00b7brei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "KON", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Der Menschen M\u00e4nge wuchs in \u00fcberh\u00e4uffter Zahl/", "tokens": ["Der", "Men\u00b7schen", "M\u00e4n\u00b7ge", "wuchs", "in", "\u00fc\u00b7berh\u00b7\u00e4uff\u00b7ter", "Zahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hat sein erfahrner Witz die Rechte so geleitet/", "tokens": ["Hat", "sein", "er\u00b7fahr\u00b7ner", "Witz", "die", "Rech\u00b7te", "so", "ge\u00b7lei\u00b7tet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df Cron und Scepter stund bey jedes Volckes Wahl.", "tokens": ["Da\u00df", "Cron", "und", "Scep\u00b7ter", "stund", "bey", "je\u00b7des", "Vol\u00b7ckes", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wem nun die W\u00fcrdigkeit des Reiches auffgetragen/", "tokens": ["Wem", "nun", "die", "W\u00fcr\u00b7dig\u00b7keit", "des", "Rei\u00b7ches", "auff\u00b7ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der ging an Treffligkeit und Tugend andern f\u00fcr;", "tokens": ["Der", "ging", "an", "Tref\u00b7flig\u00b7keit", "und", "Tu\u00b7gend", "an\u00b7dern", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "KON", "NN", "ADJA", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Des Volckes Richter seyn/ Entschied und Urtheil sagen/", "tokens": ["Des", "Vol\u00b7ckes", "Rich\u00b7ter", "seyn", "/", "Ent\u00b7schied", "und", "Ur\u00b7theil", "sa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAINF", "$(", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und schlichten/ was verwirrt/ dis hie\u00df die h\u00f6chste Zier.", "tokens": ["Und", "schlich\u00b7ten", "/", "was", "ver\u00b7wirrt", "/", "dis", "hie\u00df", "die", "h\u00f6chs\u00b7te", "Zier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "PWS", "ADJD", "$(", "PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und solche Klugheit hat sich weiter ausgestrecket/", "tokens": ["Und", "sol\u00b7che", "Klug\u00b7heit", "hat", "sich", "wei\u00b7ter", "aus\u00b7ge\u00b7stre\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PRF", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bi\u00df sie die Monarchie zu vollem Wachstum bracht/", "tokens": ["Bi\u00df", "sie", "die", "Mon\u00b7ar\u00b7chie", "zu", "vol\u00b7lem", "Wachs\u00b7tum", "bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Denn ward mit ihr zugleich die Staatssucht ausgehecket/", "tokens": ["Denn", "ward", "mit", "ihr", "zu\u00b7gleich", "die", "Staats\u00b7sucht", "aus\u00b7ge\u00b7he\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "PPER", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die ihrer Eitelkeit Abg\u00f6tter hat gemacht.", "tokens": ["Die", "ih\u00b7rer", "Ei\u00b7tel\u00b7keit", "Ab\u00b7g\u00f6t\u00b7ter", "hat", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es blieb der alte Spruch: Da\u00df wei\u00dflich zu regieren", "tokens": ["Es", "blieb", "der", "al\u00b7te", "Spruch", ":", "Da\u00df", "wei\u00df\u00b7lich", "zu", "re\u00b7gie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "KOUS", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die gr\u00f6ste Klugheit sey und wenigen geschenckt.", "tokens": ["Die", "gr\u00f6s\u00b7te", "Klug\u00b7heit", "sey", "und", "we\u00b7ni\u00b7gen", "ge\u00b7schenckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "KON", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Alleine wie man lie\u00df sich die Begierden f\u00fchren/", "tokens": ["Al\u00b7lei\u00b7ne", "wie", "man", "lie\u00df", "sich", "die", "Be\u00b7gier\u00b7den", "f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "VVFIN", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Hat an die Stats-Kunst sich der Laster Schaar gehenckt.", "tokens": ["Hat", "an", "die", "Stats\u00b7Kunst", "sich", "der", "Las\u00b7ter", "Schaar", "ge\u00b7henckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "PRF", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "So da\u00df man jetzt nicht wei\u00df/ ob Thorheit und Gelucke/", "tokens": ["So", "da\u00df", "man", "jetzt", "nicht", "wei\u00df", "/", "ob", "Thor\u00b7heit", "und", "Ge\u00b7lu\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "ADV", "PTKNEG", "VVFIN", "$(", "KOUS", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "F\u00fcr Wei\u00dfheit und Verstand zu unterscheiden seyn.", "tokens": ["F\u00fcr", "Wei\u00df\u00b7heit", "und", "Ver\u00b7stand", "zu", "un\u00b7ter\u00b7schei\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn di\u00df nun so bestallt der Klugheit meistes St\u00fccke/", "tokens": ["Wenn", "di\u00df", "nun", "so", "be\u00b7stallt", "der", "Klug\u00b7heit", "meis\u00b7tes", "St\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ADV", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So geht die Klugheit auch in Wissenschafften ein.", "tokens": ["So", "geht", "die", "Klug\u00b7heit", "auch", "in", "Wis\u00b7sen\u00b7schaff\u00b7ten", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Das Auge sicht nicht satt/ das Ohr kan nicht satt h\u00f6ren/", "tokens": ["Das", "Au\u00b7ge", "sicht", "nicht", "satt", "/", "das", "Ohr", "kan", "nicht", "satt", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "wo