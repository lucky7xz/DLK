{"textgrid.poem.36608": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Amalia", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als noch Amalia in unsern Sch\u00e4ferh\u00fctten", "tokens": ["Als", "noch", "A\u00b7ma\u00b7lia", "in", "un\u00b7sern", "Sch\u00e4\u00b7fer\u00b7h\u00fct\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NE", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Unschuld selbst, das Muster frommer Sitten,", "tokens": ["Die", "Un\u00b7schuld", "selbst", ",", "das", "Mus\u00b7ter", "from\u00b7mer", "Sit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und aller Sch\u00e4fer Ehrfurcht war,", "tokens": ["Und", "al\u00b7ler", "Sch\u00e4\u00b7fer", "Ehr\u00b7furcht", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da schm\u00fcckte nur ein Kranz ihr lockigt Haar.", "tokens": ["Da", "schm\u00fcck\u00b7te", "nur", "ein", "Kranz", "ihr", "lo\u00b7ckigt", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als sie noch gern auf meine Weide trieb,", "tokens": ["Als", "sie", "noch", "gern", "auf", "mei\u00b7ne", "Wei\u00b7de", "trieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da waren ihr die kleinen Lieder lieb,", "tokens": ["Da", "wa\u00b7ren", "ihr", "die", "klei\u00b7nen", "Lie\u00b7der", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die ich von ihr und ihren frommen Sitten", "tokens": ["Die", "ich", "von", "ihr", "und", "ih\u00b7ren", "from\u00b7men", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Echo sang, oft wohl auf ihre Bitten.", "tokens": ["Dem", "E\u00b7cho", "sang", ",", "oft", "wohl", "auf", "ih\u00b7re", "Bit\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Itzt aber, da sie in der Stadt", "tokens": ["Itzt", "a\u00b7ber", ",", "da", "sie", "in", "der", "Stadt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Viel stolze Schmeichler um sich hat,", "tokens": ["Viel", "stol\u00b7ze", "Schmeich\u00b7ler", "um", "sich", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "PRF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Itzt liebet sie den schweren Pomp von Gold,", "tokens": ["Itzt", "lie\u00b7bet", "sie", "den", "s