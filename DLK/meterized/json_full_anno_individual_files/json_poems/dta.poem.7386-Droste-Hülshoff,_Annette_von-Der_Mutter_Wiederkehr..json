{"dta.poem.7386": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Der Mutter Wiederkehr.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Nie brachte wieder auf sein Geschick", "tokens": ["Nie", "brach\u00b7te", "wie\u00b7der", "auf", "sein", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die gute Marie den Mann,", "tokens": ["Die", "gu\u00b7te", "Ma\u00b7rie", "den", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Der seines Lebens einziges Gl\u00fcck", "tokens": ["Der", "sei\u00b7nes", "Le\u00b7bens", "ein\u00b7zi\u00b7ges", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In ihrer Liebe gewann.", "tokens": ["In", "ih\u00b7rer", "Lie\u00b7be", "ge\u00b7wann", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So mild und schonend bot sie die H\u00e4nde,", "tokens": ["So", "mild", "und", "scho\u00b7nend", "bot", "sie", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Bracht' ihm so manches bl\u00fchende Kind,", "tokens": ["Bracht'", "ihm", "so", "man\u00b7ches", "bl\u00fc\u00b7hen\u00b7de", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Da\u00df von der ehrlichen Stirn am Ende", "tokens": ["Da\u00df", "von", "der", "ehr\u00b7li\u00b7chen", "Stirn", "am", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die d\u00fcstern Falten gewichen sind.", "tokens": ["Die", "d\u00fcs\u00b7tern", "Fal\u00b7ten", "ge\u00b7wi\u00b7chen", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wohl f\u00fchrt' nach Jahren einmal sein Weg", "tokens": ["Wohl", "f\u00fchrt'", "nach", "Jah\u00b7ren", "ein\u00b7mal", "sein", "Weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "ADV", "PPOSAT", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihn dicht zur Heimath hinan,", "tokens": ["Ihn", "dicht", "zur", "Hei\u00b7math", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da lie\u00df er halten am M\u00fchlensteg,", "tokens": ["Da", "lie\u00df", "er", "hal\u00b7ten", "am", "M\u00fch\u00b7lens\u00b7teg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schaute die Th\u00fcrme sich an.", "tokens": ["Und", "schau\u00b7te", "die", "Th\u00fcr\u00b7me", "sich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Die H\u00e4nd' gefaltet, schien er zu beten,", "tokens": ["Die", "H\u00e4nd'", "ge\u00b7fal\u00b7tet", ",", "schien", "er", "zu", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ein Wink \u2014 die Kutsche rasselte fort;", "tokens": ["Ein", "Wink", "die", "Kut\u00b7sche", "ras\u00b7sel\u00b7te", "fort", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-++-+---+", "measure": "unknown.measure.tetra"}, "line.7": {"text": "Doch nimmer hat er den Ort betreten,", "tokens": ["Doch", "nim\u00b7mer", "hat", "er", "den", "Ort", "be\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Und keinen Trunk Wasser nahm er dort.", "tokens": ["Und", "kei\u00b7nen", "Trunk", "Was\u00b7ser", "nahm", "er", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}}}}