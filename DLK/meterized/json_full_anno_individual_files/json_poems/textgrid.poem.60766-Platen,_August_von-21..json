{"textgrid.poem.60766": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hab ich diesen Taumel \u00fcberwunden,", "tokens": ["Nun", "hab", "ich", "die\u00b7sen", "Tau\u00b7mel", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und irre nicht mehr hier und dort ins Weite,", "tokens": ["Und", "ir\u00b7re", "nicht", "mehr", "hier", "und", "dort", "ins", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADV", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Geist gewann ein sicheres Geleite,", "tokens": ["Mein", "Geist", "ge\u00b7wann", "ein", "si\u00b7che\u00b7res", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seitdem er endlich einen Freund gefunden.", "tokens": ["Seit\u00b7dem", "er", "end\u00b7lich", "ei\u00b7nen", "Freund", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dir nun, o Freund, geh\u00f6ren meine Stunden,", "tokens": ["Dir", "nun", ",", "o", "Freund", ",", "ge\u00b7h\u00f6\u00b7ren", "mei\u00b7ne", "Stun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "FM", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du gabst ein Ziel mir nun, wonach ich schreite,", "tokens": ["Du", "gabst", "ein", "Ziel", "mir", "nun", ",", "wo\u00b7nach", "ich", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nach dieser eil ich oder jener Seite,", "tokens": ["Nach", "die\u00b7ser", "eil", "ich", "o\u00b7der", "je\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PPER", "KON", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo ich, dich anzutreffen, kann erkunden.", "tokens": ["Wo", "ich", ",", "dich", "an\u00b7zu\u00b7tref\u00b7fen", ",", "kann", "er\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPER", "VVIZU", "$,", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du winkst mir zu von manchem Weihaltare,", "tokens": ["Du", "winkst", "mir", "zu", "von", "man\u00b7chem", "Wei\u00b7hal\u00b7ta\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Geist ist ein harmonisches Bestreben,", "tokens": ["Dein", "Geist", "ist", "ein", "har\u00b7mo\u00b7ni\u00b7sches", "Be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deine sanfte Seele liebt das Wahre.", "tokens": ["Und", "dei\u00b7ne", "sanf\u00b7te", "See\u00b7le", "liebt", "das", "Wah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "O welch ein Gl\u00fcck, sich ganz dir hinzugeben,", "tokens": ["O", "welch", "ein", "Gl\u00fcck", ",", "sich", "ganz", "dir", "hin\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$,", "PRF", "ADV", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und, wenn es m\u00f6glich w\u00e4re, Jahr um Jahre", "tokens": ["Und", ",", "wenn", "es", "m\u00f6g\u00b7lich", "w\u00e4\u00b7re", ",", "Jahr", "um", "Jah\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit deinen Engeln, Gian Bellin, zu leben!", "tokens": ["Mit", "dei\u00b7nen", "En\u00b7geln", ",", "Gi\u00b7an", "Bel\u00b7lin", ",", "zu", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NE", "NE", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Nun hab ich diesen Taumel \u00fcberwunden,", "tokens": ["Nun", "hab", "ich", "die\u00b7sen", "Tau\u00b7mel", "\u00fc\u00b7berw\u00b7un\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV"