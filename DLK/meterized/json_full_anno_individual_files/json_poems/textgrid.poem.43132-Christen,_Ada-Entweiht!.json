{"textgrid.poem.43132": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "Entweiht!", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern sei es mir, da\u00df spottend ich", "tokens": ["Fern", "sei", "es", "mir", ",", "da\u00df", "spot\u00b7tend", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "PPER", "$,", "KOUS", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach Dir, zerfall'ne Gottheit, zeige,", "tokens": ["Nach", "Dir", ",", "zer\u00b7fall'\u00b7ne", "Got\u00b7theit", ",", "zei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn ich auch nimmer gl\u00e4ubig mich", "tokens": ["Wenn", "ich", "auch", "nim\u00b7mer", "gl\u00e4u\u00b7big", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vor Deiner Macht in Demuth neige.", "tokens": ["Vor", "Dei\u00b7ner", "Macht", "in", "De\u00b7muth", "nei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du stehst mir schmerzvoll, menschlich-nah,", "tokens": ["Du", "stehst", "mir", "schmerz\u00b7voll", ",", "menschlich\u00b7nah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Stehst menschlich-schwach an meiner Seite.", "tokens": ["Stehst", "menschlich\u00b7schwach", "an", "mei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich schaue nun, was ich nicht sah,", "tokens": ["Ich", "schau\u00b7e", "nun", ",", "was", "ich", "nicht", "sah", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als Du in mystisch-ferner Weite!", "tokens": ["Als", "Du", "in", "mys\u00b7tischfer\u00b7ner", "Wei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Ern\u00fcchtert starr' ich zu Dir hin,", "tokens": ["Er\u00b7n\u00fcch\u00b7tert", "starr'", "ich", "zu", "Dir", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und such' die schmerzgefeiten Z\u00fcge,", "tokens": ["Und", "such'", "die", "schmerz\u00b7ge\u00b7fei\u00b7ten", "Z\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und schau: so elend wie ich bin,", "tokens": ["Und", "schau", ":", "so", "e\u00b7lend", "wie", "ich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKVZ", "$.", "ADV", "ADJD", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Bist Du \u2013 durc