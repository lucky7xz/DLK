 "tokens": ["T\u00f6nt", "er", "die", "Me\u00b7lo\u00b7die", "zu", "mei\u00b7nem", "Lied", "voll", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und die Natur ist \u00e4ngstlich still und trauernd,", "tokens": ["Und", "die", "Na\u00b7tur", "ist", "\u00e4ngst\u00b7lich", "still", "und", "trau\u00b7ernd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Doch hoffnungsvoller als mein Herz.", "tokens": ["Doch", "hoff\u00b7nungs\u00b7vol\u00b7ler", "als", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn sieh, bald gaukelt dir, mit Rosenkr\u00e4nzen", "tokens": ["Denn", "sieh", ",", "bald", "gau\u00b7kelt", "dir", ",", "mit", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVIMP", "$,", "ADV", "VVFIN", "PPER", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In runder Hand, du Sonnengott, das Zwillingspaar", "tokens": ["In", "run\u00b7der", "Hand", ",", "du", "Son\u00b7nen\u00b7gott", ",", "das", "Zwil\u00b7lings\u00b7paar"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "PPER", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit offnem blauen Aug, mit krausem goldnen Haar", "tokens": ["Mit", "o