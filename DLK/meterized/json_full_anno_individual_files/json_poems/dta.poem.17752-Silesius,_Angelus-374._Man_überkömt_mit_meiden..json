{"dta.poem.17752": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "374.  Man \u00fcberk\u00f6mt mit meiden.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Freund meide was dir Lieb/ fleuch was dein Sin\u0304 begehrt.", "tokens": ["Freund", "mei\u00b7de", "was", "dir", "Lieb", "/", "fleuch", "was", "dein", "Sin\u0304", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PWS", "PPER", "NN", "$(", "VVFIN", "PIS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du wirst sonst nimmermehr ges\u00e4ttigt vnd gewehrt.", "tokens": ["Du", "wirst", "sonst", "nim\u00b7mer\u00b7mehr", "ge\u00b7s\u00e4t\u00b7tigt", "vnd", "ge\u00b7wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Viel w\u00e4ren zum Genu\u00df der ewgen Wollust kommen/", "tokens": ["Viel", "w\u00e4\u00b7ren", "zum", "Ge\u00b7nu\u00df", "der", "ew\u00b7gen", "Wol\u00b7lust", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn sie mit Zeittlicher sich hier nicht \u00fcbernommen.", "tokens": ["Wenn", "sie", "mit", "Zeitt\u00b7li\u00b7cher", "sich", "hier", "nicht", "\u00fc\u00b7ber\u00b7nom\u00b7men", "."], "token_info": ["word", "wor