{"dta.poem.20786": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "1742 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Englisches Evchen, ach gieb dich zufrieden,                 ", "tokens": ["Eng\u00b7li\u00b7sches", "Ev\u00b7chen", ",", "ach", "gieb", "dich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "XY", "VVIMP", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Movire dich doch nicht, wenn man dich so nennt;", "tokens": ["Mo\u00b7vi\u00b7re", "dich", "doch", "nicht", ",", "wenn", "man", "dich", "so", "nennt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "PTKNEG", "$,", "KOUS", "PIS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ist dir der Name nun einmal beschieden,", "tokens": ["Ist", "dir", "der", "Na\u00b7me", "nun", "ein\u00b7mal", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So leid ihn geduldig und lebe content:", "tokens": ["So", "leid", "ihn", "ge\u00b7dul\u00b7dig", "und", "le\u00b7be", "con\u00b7tent", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "KON", "VVFIN", "NE", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Eva ward h\u00f6chst vergn\u00fcgt, da es die Vorsicht f\u00fcgt,", "tokens": ["E\u00b7va", "ward", "h\u00f6chst", "ver\u00b7gn\u00fcgt", ",", "da", "es", "die", "Vor\u00b7sicht", "f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie der Adam sein Sch\u00e4tzchen genennt.", "tokens": ["Da\u00df", "sie", "der", "A\u00b7dam", "sein", "Sch\u00e4tz\u00b7chen", "ge\u00b7nennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun denn, mein Evchen, so wird dirs auch gehen,", "tokens": ["Nun", "denn", ",", "mein", "Ev\u00b7chen", ",", "so", "wird", "dirs", "auch", "ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ob dich dein Name gleich itzo verdriest;", "tokens": ["Ob", "dich", "dein", "Na\u00b7me", "gleich", "it\u00b7zo", "ver\u00b7driest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich sichre, da wirst du schon freundlicher sehen,", "tokens": ["Ich", "sich\u00b7re", ",", "da", "wirst", "du", "schon", "freund\u00b7li\u00b7cher", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wenn dich ein Adam einst rufet und k\u00fc\u00dft:", "tokens": ["Wenn", "dich", "ein", "A\u00b7dam", "einst", "ru\u00b7fet", "und", "k\u00fc\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dann wird recht buchstabirt, gelesen und vexirt,", "tokens": ["Dann", "wird", "recht", "buch\u00b7sta\u00b7birt", ",", "ge\u00b7le\u00b7sen", "und", "ve\u00b7xirt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn uns die Liebe den Namen vers\u00fc\u00dft.", "tokens": ["Wenn", "uns", "die", "Lie\u00b7be", "den", "Na\u00b7men", "ver\u00b7s\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}