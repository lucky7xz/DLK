{"textgrid.poem.42909": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Dreiste Blicke", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Knie", "tokens": ["\u00dc\u00b7ber", "die", "Knie"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Unter ein R\u00f6ckchen zu schaun \u2013 \u2013", "tokens": ["Un\u00b7ter", "ein", "R\u00f6ck\u00b7chen", "zu", "schaun", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "PTKZU", "VVINF", "$(", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Wenn sie doch das und die", "tokens": ["Wenn", "sie", "doch", "das", "und", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "KON", "ART"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Haben, die sch\u00f6nen Fraun!", "tokens": ["Ha\u00b7ben", ",", "die", "sch\u00f6\u00b7nen", "Fraun", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "\u00dcber einen \u00f6ffnenden Saum", "tokens": ["\u00dc\u00b7ber", "ei\u00b7nen", "\u00f6ff\u00b7nen\u00b7den", "Saum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "In T\u00e4ler zwischen Br\u00fcstchen", "tokens": ["In", "T\u00e4\u00b7ler", "zwi\u00b7schen", "Br\u00fcst\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Darf Blick wie stiller Traum", "tokens": ["Darf", "Blick", "wie", "stil\u00b7ler", "Traum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "NN", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "St\u00fcrzen sein L\u00fcstchen.", "tokens": ["St\u00fcr\u00b7zen", "sein", "L\u00fcst\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sollen doch Frauen auch", "tokens": ["Sol\u00b7len", "doch", "Frau\u00b7en", "auch"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "So blicken, \u2013 nicht schielen \u2013", "tokens": ["So", "bli\u00b7cken", ",", "\u2013", "nicht", "schie\u00b7len", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "$(", "PTKNEG", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Wenn Arm, Popo und Bauch", "tokens": ["Wenn", "Arm", ",", "Po\u00b7po", "und", "Bauch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In F\u00e4ltchen spielen.", "tokens": ["In", "F\u00e4lt\u00b7chen", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Nimm, was der Blick dir gibt,", "tokens": ["Nimm", ",", "was", "der", "Blick", "dir", "gibt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sei es, was es sei.", "tokens": ["Sei", "es"