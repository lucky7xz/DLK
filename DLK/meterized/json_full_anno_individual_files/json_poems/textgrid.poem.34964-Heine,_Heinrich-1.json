 ["APPR", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4hrend gl\u00fccklich als ein Sieger", "tokens": ["W\u00e4h\u00b7rend", "gl\u00fcck\u00b7lich", "als", "ein", "Sie\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trabt auf hohem Ro\u00df der Schlechte?", "tokens": ["Trabt", "auf", "ho\u00b7hem", "Ro\u00df", "der", "Schlech\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Woran liegt die Schuld? Ist etwa", "tokens": ["Wo\u00b7ran", "liegt", "die", "Schuld", "?", "Ist", "et\u00b7wa"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "VAFIN", "ADV"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Unser Herr nicht ganz allm\u00e4chtig?", "tokens": ["Un\u00b7ser", "Herr", "nicht", "ganz", "all\u00b7m\u00e4ch\u00b7tig", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder treibt er selbst den Unfug?", "tokens": ["O\u00b7der", "treibt", "er", "selbst", "den", "Un\u00b7fug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, das w\u00e4re niedertr\u00e4chtig.", "tokens": ["Ach", ",", "das", "w\u00e4\u00b7re", "nie\u00b7der\u00b7tr\u00e4ch\u00b7tig", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PDS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure