{"textgrid.poem.55857": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Ende des Herbstes", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sehe seit einer Zeit,", "tokens": ["Ich", "se\u00b7he", "seit", "ei\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wie alles sich verwandelt.", "tokens": ["wie", "al\u00b7les", "sich", "ver\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Etwas steht auf und handelt", "tokens": ["Et\u00b7was", "steht", "auf", "und", "han\u00b7delt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "und t\u00f6tet und tut Leid.", "tokens": ["und", "t\u00f6\u00b7tet", "und", "tut", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von Mal zu Mal sind all", "tokens": ["Von", "Mal", "zu", "Mal", "sind", "all"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "VAFIN", "PIAT"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "die G\u00e4rten nicht dieselben;", "tokens": ["die", "G\u00e4r\u00b7ten", "nicht", "die\u00b7sel\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "von den gilbenden zu der gelben", "tokens": ["von", "den", "gil\u00b7ben\u00b7den", "zu", "der", "gel\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "langsamem Verfall:", "tokens": ["lang\u00b7sa\u00b7mem", "Ver\u00b7fall", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "wie war der Weg mir weit.", "tokens": ["wie", "war", "der", "Weg", "mir", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Jetzt bin ich bei den leeren", "tokens": ["Jetzt", "bin", "ich", "bei", "den", "lee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "und schaue durch alle Alleen.", "tokens": ["und", "schau\u00b7e", "durch", "al\u00b7le", "Al\u00b7leen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Fast bis zu den fernen Meeren", "tokens": ["Fast", "bis", "zu", "den", "fer\u00b7nen", "Mee\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "kann ich den ernsten schweren", "tokens": ["kann", "ich", "den", "erns\u00b7ten", "schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VVINF"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.5": {"text": "verwehrenden Himmel sehn.", "t