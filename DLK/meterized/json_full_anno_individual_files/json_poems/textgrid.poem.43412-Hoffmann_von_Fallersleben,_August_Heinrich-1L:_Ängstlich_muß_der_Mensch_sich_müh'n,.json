{"textgrid.poem.43412": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00c4ngstlich mu\u00df der Mensch sich m\u00fch'n,", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00c4ngstlich mu\u00df der Mensch sich m\u00fch'n,", "tokens": ["\u00c4ngst\u00b7lich", "mu\u00df", "der", "Mensch", "sich", "m\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VMFIN", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer nur bedacht auf morgen;", "tokens": ["Im\u00b7mer", "nur", "be\u00b7dacht", "auf", "mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "APPR", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht nichts gr\u00fcnen, nichts erbl\u00fch'n", "tokens": ["Sieht", "nichts", "gr\u00fc\u00b7nen", ",", "nichts", "er\u00b7bl\u00fch'n"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PIS", "VVINF", "$,", "PIS", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Furcht und ohne Sorgen.", "tokens": ["Oh\u00b7ne", "Furcht", "und", "oh\u00b7ne", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Immer ist er auf der Flucht;", "tokens": ["Im\u00b7mer", "ist", "er", "auf", "der", "Flucht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auch in sch\u00f6nen Herbstestag