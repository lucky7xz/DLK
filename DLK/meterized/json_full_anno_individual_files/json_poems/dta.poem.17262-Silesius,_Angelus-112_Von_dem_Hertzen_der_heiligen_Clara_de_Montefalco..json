{"dta.poem.17262": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "112  Von dem Hertzen der heiligen  \n  Clara  de Montefalco.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Hier ist der Speer und Schwamm/ die N\u00e4gel/ S\u00e4ul und", "tokens": ["Hier", "ist", "der", "Speer", "und", "Schwamm", "/", "die", "N\u00e4\u00b7gel", "/", "S\u00e4ul", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NN", "$(", "ART", "NN", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kron/ (Sohn:", "tokens": ["Kron", "/", "(", "Sohn", ":"], "token_info": ["word", "punct", "punct", "word", "punct"], "pos": ["NE", "$(", "$(", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Geisseln/ und auch gar das Creutz mit GOttes", "tokens": ["Die", "Geis\u00b7seln", "/", "und", "auch", "gar", "das", "Creutz", "mit", "Got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "KON", "ADV", "ADV", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drey Kugeln eines halts: Es kan nicht anderst seyn/", "tokens": ["Drey", "Ku\u00b7geln", "ei\u00b7nes", "halts", ":", "Es", "kan", "nicht", "an\u00b7derst", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ART", "NN", "$.", "PPER", "VMFIN", "PTKNEG", "ADV", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Di\u00df Hertz ist GOttesburg/ und seines Leydensschreyn.", "tokens": ["Di\u00df", "Hertz", "ist", "Got\u00b7tes\u00b7burg", "/", "und", "sei\u00b7nes", "Ley\u00b7dens\u00b7schreyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "NN", "$(", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}