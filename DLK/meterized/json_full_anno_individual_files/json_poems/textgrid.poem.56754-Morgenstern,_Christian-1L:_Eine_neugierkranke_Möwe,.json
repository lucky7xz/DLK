{"textgrid.poem.56754": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine neugierkranke M\u00f6we,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine neugierkranke M\u00f6we,", "tokens": ["Ei\u00b7ne", "neu\u00b7gier\u00b7kran\u00b7ke", "M\u00f6\u00b7we", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "kreiste ich zu H\u00e4upten eines", "tokens": ["kreis\u00b7te", "ich", "zu", "H\u00e4up\u00b7ten", "ei\u00b7nes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wesens, das in einen weiten", "tokens": ["We\u00b7sens", ",", "das", "in", "ei\u00b7nen", "wei\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "dunklen Mantel eingewickelt,", "tokens": ["dunk\u00b7len", "Man\u00b7tel", "ein\u00b7ge\u00b7wi\u00b7ckelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "von dem Kopfe einer Bune", "tokens": ["von", "dem", "Kop\u00b7fe", "ei\u00b7ner", "Bu\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "auf die gr\u00fcne See hinaussah.", "tokens": ["auf", "die", "gr\u00fc\u00b7ne", "See", "hin\u00b7aus\u00b7sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Und ich wu\u00dfte, da\u00df ich selber", "tokens": ["Und", "ich", "wu\u00df\u00b7te", ",", "da\u00df", "ich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "dieses Wesen sei, und war mir", "tokens": ["die\u00b7ses", "We\u00b7sen", "sei", ",", "und", "war", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDAT", "NN", "VAFIN", "$,", "KON", "VAFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "dennoch selbst so problematisch,", "tokens": ["den\u00b7noch", "selbst", "so", "prob\u00b7le\u00b7ma\u00b7tisch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "wie nur je dem klugen Sinne", "tokens": ["wie", "nur", "je", "dem", "klu\u00b7gen", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "einer M\u00f6we solch ein dunkler", "tokens": ["ei\u00b7ner", "M\u00f6\u00b7we", "solch", "ein", "dunk\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PIAT", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Mantelvogel, Mensch gehei\u00dfen.", "tokens": ["Man\u00b7tel\u00b7vo\u00b7gel", ",", "Mensch", "ge\u00b7hei\u00b7\u00dfen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Warum blickt dies gro\u00dfe, stumme,", "tokens": ["Wa\u00b7rum", "blickt", "dies", "gro\u00b7\u00dfe", ",", "stum\u00b7me", ","], "token