{"dta.poem.18767": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika Nameh .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gedachte in der Nacht", "tokens": ["Ich", "ge\u00b7dach\u00b7te", "in", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass ich den Mond s&#228;he im Schlaf;", "tokens": ["Dass", "ich", "den", "Mond", "s", "&#228;", "he", "im", "Schlaf", ";"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "FM", "FM", "FM", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich aber erwachte", "tokens": ["Als", "ich", "a\u00b7ber", "er\u00b7wach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ging unvermuthet die Sonne auf.", "tokens": ["Ging", "un\u00b7ver\u00b7mu\u00b7thet", "die", "Son\u00b7ne", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}