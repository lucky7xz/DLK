{"dta.poem.9871": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Von der letzten zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist ein tag/ ein jahr? die zeit von hundert jahren/", "tokens": ["Was", "ist", "ein", "tag", "/", "ein", "jahr", "?", "die", "zeit", "von", "hun\u00b7dert", "jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$(", "ART", "NN", "$.", "ART", "NN", "APPR", "CARD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In welcher aus der welt viel tausend seelen fahren/", "tokens": ["In", "wel\u00b7cher", "aus", "der", "welt", "viel", "tau\u00b7send", "see\u00b7len", "fah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "ART", "NN", "ADV", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein spiel das vielen kurtz/ und vielen lang mu\u00df seyn/", "tokens": ["Ein", "spiel", "das", "vie\u00b7len", "kurtz", "/", "und", "vie\u00b7len", "lang", "mu\u00df", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "PIAT", "NN", "$(", "KON", "PIAT", "ADJD", "VMFIN", "VAINF", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "O mensch nim alle zeit genau in augenschein!", "tokens": ["O", "mensch", "nim", "al\u00b7le", "zeit", "ge\u00b7nau", "in", "au\u00b7gen\u00b7schein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADV", "PIAT", "NN", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein tag kommt/ dessen macht die jahre wird verschlingen/", "tokens": ["Ein", "tag", "kommt", "/", "des\u00b7sen", "macht", "die", "jah\u00b7re", "wird", "ver\u00b7schlin\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PDS", "VVFIN", "ART", "NN", "VAFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein jahr kommt/ dessen lauff nicht mehr wird tage bringen/", "tokens": ["Ein", "jahr", "kommt", "/", "des\u00b7sen", "lauff", "nicht", "mehr", "wird", "ta\u00b7ge", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PRELAT", "NN", "PTKNEG", "ADV", "VAFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O wunderbares jahr! o wunderbarer tag!", "tokens": ["O", "wun\u00b7der\u00b7ba\u00b7res", "jahr", "!", "o", "wun\u00b7der\u00b7ba\u00b7rer", "tag", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "FM", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In welchem auffersteht/ was in den gr\u00e4bern lag/", "tokens": ["In", "wel\u00b7chem", "auf\u00b7fer\u00b7steht", "/", "was", "in", "den", "gr\u00e4\u00b7bern", "lag", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVFIN", "$(", "PWS", "APPR", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wer soll nicht alle tag an dieses jahr gedencken/", "tokens": ["Wer", "soll", "nicht", "al\u00b7le", "tag", "an", "die\u00b7ses", "jahr", "ge\u00b7den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "PIAT", "NN", "APPR", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das aller zeiten stand wird in zwey theil verschrencken/", "tokens": ["Das", "al\u00b7ler", "zei\u00b7ten", "stand", "wird", "in", "zwey", "theil", "ver\u00b7schren\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "VAFIN", "APPR", "CARD", "NN", "VVINF", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Wo stete trauer-nacht/ wo stetes freuden-licht/", "tokens": ["Wo", "ste\u00b7te", "trau\u00b7e\u00b7rnacht", "/", "wo", "ste\u00b7tes", "freu\u00b7den\u00b7licht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "$(", "PWAV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Des himmels wollust-strahl/ der h\u00f6lle marter sticht.", "tokens": ["Des", "him\u00b7mels", "wol\u00b7lust\u00b7strahl", "/", "der", "h\u00f6l\u00b7le", "mar\u00b7ter", "sticht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr freches laster-volck/ ach lasset alle tage", "tokens": ["Ihr", "fre\u00b7ches", "las\u00b7ter\u00b7volck", "/", "ach", "las\u00b7set", "al\u00b7le", "ta\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ADV", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Zu beyden ohren ein des letzten tages klage:", "tokens": ["Zu", "bey\u00b7den", "oh\u00b7ren", "ein", "des", "letz\u00b7ten", "ta\u00b7ges", "kla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Nun mu\u00df die gantze welt vor ihrem richter stehn/", "tokens": ["Nun", "mu\u00df", "die", "gant\u00b7ze", "welt", "vor", "ih\u00b7rem", "rich\u00b7ter", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der keiner s\u00fcnde zeit l\u00e4st ungestrafft hingehn.", "tokens": ["Der", "kei\u00b7ner", "s\u00fcn\u00b7de", "zeit", "l\u00e4st", "un\u00b7ge\u00b7strafft", "hin\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wolt ihr/ o sterblichen! nicht mit der zeit verderben/", "tokens": ["Wolt", "ihr", "/", "o", "sterb\u00b7li\u00b7chen", "!", "nicht", "mit", "der", "zeit", "ver\u00b7der\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$(", "FM", "VVINF", "$.", "PTKNEG", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "So legt die zeit wohl an im leben und im sterben;", "tokens": ["So", "legt", "die", "zeit", "wohl", "an", "im", "le\u00b7ben", "und", "im", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPR", "APPRART", "VVINF", "KON", "APPRART", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der ist klug/ der wird fromm/ wer stets denckt an die zeit/", "tokens": ["Der", "ist", "klug", "/", "der", "wird", "fromm", "/", "wer", "stets", "denckt", "an", "die", "zeit", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$(", "ART", "VAFIN", "ADJD", "$(", "PWS", "ADV", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-----+-+-+-+", "measure": "unknown.measure.tetra"}, "line.20": {"text": "Wenn ihn ein augenblick setzt in die ewigkeit.", "tokens": ["Wenn", "ihn", "ein", "au\u00b7gen\u00b7blick", "setzt", "in", "die", "e\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}