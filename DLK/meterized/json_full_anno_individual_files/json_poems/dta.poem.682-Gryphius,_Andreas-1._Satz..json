{"dta.poem.682": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "1. Satz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Was trawr' ich noch! was zittern meine glieder? ", "tokens": ["Was", "tra\u00b7wr'", "ich", "noch", "!", "was", "zit\u00b7tern", "mei\u00b7ne", "glie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wie das mein abgemattes hertz ", "tokens": ["Wie", "das", "mein", "ab\u00b7ge\u00b7mat\u00b7tes", "hertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sich will in angst verleiten/ ", "tokens": ["Sich", "will", "in", "angst", "ver\u00b7lei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein schmertz wird nuhmer ja zum schertz", "tokens": ["Mein", "schmertz", "wird", "nuh\u00b7mer", "ja", "zum", "schertz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "VAFIN", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In diesen frewden zeiten.", "tokens": ["In", "die\u00b7sen", "frew\u00b7den", "zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was wanckt mein sinn de\u00f1 trawrig hin vnd wider:", "tokens": ["Was", "wanckt", "mein", "sinn", "de\u00f1", "traw\u00b7rig", "hin", "vnd", "wi\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wie/ wenn die winde streitten/ ", "tokens": ["Wie", "/", "wenn", "die", "win\u00b7de", "streit\u00b7ten", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KOUS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Vnd Nordt vnd Ost raast wider seine br\u00fcder;", "tokens": ["Vnd", "Nordt", "vnd", "Ost", "raast", "wi\u00b7der", "sei\u00b7ne", "br\u00fc\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ein ast erbebt im rawen Mertz. ", "tokens": ["Ein", "ast", "er\u00b7bebt", "im", "ra\u00b7wen", "Mertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}