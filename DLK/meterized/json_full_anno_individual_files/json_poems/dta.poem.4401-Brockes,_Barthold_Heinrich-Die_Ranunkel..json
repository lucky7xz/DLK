{"dta.poem.4401": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Ranunkel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unersch\u00f6pflichs helles Meer", "tokens": ["Un\u00b7er\u00b7sch\u00f6pf\u00b7lichs", "hel\u00b7les", "Meer"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Aller Vollenkommenheiten,", "tokens": ["Al\u00b7ler", "Vol\u00b7len\u00b7kom\u00b7men\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6nheit und Vortrefflichkeiten!", "tokens": ["Sch\u00f6n\u00b7heit", "und", "Vor\u00b7treff\u00b7lich\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.4": {"text": "Deiner Creaturen Heer", "tokens": ["Dei\u00b7ner", "Crea\u00b7tu\u00b7ren", "Heer"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Zeiget, in dem Unterscheid", "tokens": ["Zei\u00b7get", ",", "in", "dem", "Un\u00b7ter\u00b7scheid"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhrer Form- und Farben Pracht,", "tokens": ["Ih\u00b7rer", "For\u00b7m", "und", "Far\u00b7ben", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "TRUNC", "KON", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Wie so herrlich Deiner Macht,", "tokens": ["Wie", "so", "herr\u00b7lich", "Dei\u00b7ner", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Deiner Weis heit, Deiner Liebe", "tokens": ["Dei\u00b7ner", "Weis", "heit", ",", "Dei\u00b7ner", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "$,", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und derselben holden Triebe", "tokens": ["Und", "der\u00b7sel\u00b7ben", "hol\u00b7den", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wahrheit und Unendlichkeit.", "tokens": ["Wahr\u00b7heit", "und", "Un\u00b7end\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}