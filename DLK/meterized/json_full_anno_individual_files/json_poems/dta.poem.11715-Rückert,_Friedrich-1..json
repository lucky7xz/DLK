{"dta.poem.11715": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du fassest selbst nur halb, was du im Herzen sagst;", "tokens": ["Du", "fas\u00b7sest", "selbst", "nur", "halb", ",", "was", "du", "im", "Her\u00b7zen", "sagst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn du in ein Wort es nun zu fassen wagst,", "tokens": ["Und", "wenn", "du", "in", "ein", "Wort", "es", "nun", "zu", "fas\u00b7sen", "wagst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "PPER", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wird es nur wieder halb darin sich fassen lassen;", "tokens": ["Wird", "es", "nur", "wie\u00b7der", "halb", "da\u00b7rin", "sich", "fas\u00b7sen", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADJD", "PAV", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie soll der H\u00f6rer ganz dies halbe Halbe fassen?", "tokens": ["Wie", "soll", "der", "H\u00f6\u00b7rer", "ganz", "dies", "hal\u00b7be", "Hal\u00b7be", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ART", "NN", "ADV", "PDS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er fa\u00dft soviel er mag, und macht es ganz in sich,", "tokens": ["Er", "fa\u00dft", "so\u00b7viel", "er", "mag", ",", "und", "macht", "es", "ganz", "in", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PPER", "VMFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "APPR", "PRF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Fa\u00dft dies auch halb, und glaubt nun ganz zu fassen dich.", "tokens": ["Fa\u00dft", "dies", "auch", "halb", ",", "und", "glaubt", "nun", "ganz", "zu", "fas\u00b7sen", "dich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "ADV", "ADJD", "$,", "KON", "VVFIN", "ADV", "ADV", "PTKZU", "VVINF", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}