{"textgrid.poem.40676": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "Einsam", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Sieg gewann das tapfre Heer gemeinsam,", "tokens": ["Den", "Sieg", "ge\u00b7wann", "das", "tapf\u00b7re", "Heer", "ge\u00b7mein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Schlachtenplan entwarf der Feldherr einsam;", "tokens": ["Den", "Schlach\u00b7ten\u00b7plan", "ent\u00b7warf", "der", "Feld\u00b7herr", "ein\u00b7sam", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Zum Garbenschnitt wetteifert die Gemeine,", "tokens": ["Zum", "Gar\u00b7ben\u00b7schnitt", "wett\u00b7ei\u00b7fert", "die", "Ge\u00b7mei\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der S\u00e4mann ging saatstreuend ganz alleine;", "tokens": ["Der", "S\u00e4\u00b7mann", "ging", "saat\u00b7streu\u00b7end", "ganz", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Den Dichtersang, vererbt von Mund zu Munde,", "tokens": ["Den", "Dich\u00b7ter\u00b7sang", ",", "ver\u00b7erbt", "von", "Mund", "zu", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gebar der Einsamkeit geweihte Stunde;", "tokens": ["Ge\u00b7bar", "der", "Ein\u00b7sam\u00b7keit", "ge\u00b7weih\u00b7te", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Leiden Quellen fluthen allerwegen,", "tokens": ["Der", "Lei\u00b7den", "Quel\u00b7len", "flut\u00b7hen", "al\u00b7ler\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Der Heilquell rieselt einsam, abgelegen;", "tokens": ["Der", "Heil\u00b7quell", "rie\u00b7selt", "ein\u00b7sam", ",", "ab\u00b7ge\u00b7le\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Genu\u00df und Leid des Alltags ist gemeinsam,", "tokens": ["Ge\u00b7nu\u00df", "und", "Leid", "des", "All\u00b7tags", "ist", "ge\u00b7mein\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Der h\u00f6chste Stolz, der tiefste Schmerz bleibt einsam.", "tokens": ["Der", "h\u00f6chs\u00b7te", "Stolz", ",", "der", "tiefs\u00b7te", "Schmerz", "bleibt", "ein\u00b7sam", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Den Sieg gewan