{"dta.poem.14525": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Umkehrende .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Hier bin ich, Herr! Gegr\u00fc\u00dft das Licht,", "tokens": ["Hier", "bin", "ich", ",", "Herr", "!", "Ge\u00b7gr\u00fc\u00dft", "das", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$.", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das durch die stille Schw\u00fcle", "tokens": ["Das", "durch", "die", "stil\u00b7le", "Schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der m\u00fcden Brust gewaltig bricht", "tokens": ["Der", "m\u00fc\u00b7den", "Brust", "ge\u00b7wal\u00b7tig", "bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seiner strengen K\u00fchle.", "tokens": ["Mit", "sei\u00b7ner", "stren\u00b7gen", "K\u00fch\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun bin ich frei! Ich taum'le noch", "tokens": ["Nun", "bin", "ich", "frei", "!", "Ich", "taum'\u00b7le", "noch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kann mich noch nicht fassen \u2014", "tokens": ["Und", "kann", "mich", "noch", "nicht", "fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O Vater, Du erkennst mich doch,", "tokens": ["O", "Va\u00b7ter", ",", "Du", "er\u00b7kennst", "mich", "doch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und wirst nicht von mir lassen!", "tokens": ["Und", "wirst", "nicht", "von", "mir", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}