{"dta.poem.3940": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "30) Schreiben an eine Freundin,  \n die sich den Tod ohne Ursache w\u00fcnschte.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "So wilt du von der Welt, und gantz von hinnen", "tokens": ["So", "wilt", "du", "von", "der", "Welt", ",", "und", "gantz", "von", "hin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "ADV", "APPR", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "scheiden?", "tokens": ["schei\u00b7den", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dich qu\u00e4lt ja keine Noth, dich qu\u00e4lt kein banges", "tokens": ["Dich", "qu\u00e4lt", "ja", "kei\u00b7ne", "Noth", ",", "dich", "qu\u00e4lt", "kein", "ban\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,", "PPER", "VVFIN", "PIAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Leiden!", "tokens": ["Lei\u00b7den", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Ach, ", "tokens": ["Ach", ","], "token_info": ["word", "punct"], "pos": ["ITJ", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Du suchest allzufr\u00fch des Grabes d\u00fcstre Ruh,", "tokens": ["Du", "su\u00b7chest", "all\u00b7zu\u00b7fr\u00fch", "des", "Gra\u00b7bes", "d\u00fcst\u00b7re", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du wilt von hier, warum? wohin aus dieser H\u00fctten?", "tokens": ["Du", "wilt", "von", "hier", ",", "wa\u00b7rum", "?", "wo\u00b7hin", "aus", "die\u00b7ser", "H\u00fct\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADV", "$,", "PWAV", "$.", "PWAV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das heisset allzufr\u00fch die Todes-Grufft beschritten;", "tokens": ["Das", "heis\u00b7set", "all\u00b7zu\u00b7fr\u00fch", "die", "To\u00b7des\u00b7G\u00b7rufft", "be\u00b7schrit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nein, lebe ferner noch! sag, was gebricht denn dir?", "tokens": ["Nein", ",", "le\u00b7be", "fer\u00b7ner", "noch", "!", "sag", ",", "was", "ge\u00b7bricht", "denn", "dir", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "ADV", "ADV", "$.", "VVFIN", "$,", "PWS", "VVFIN", "KON", "PPER", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Nein, ", "tokens": ["Nein", ","], "token_info": ["word", "punct"], "pos": ["PTKANT", "$,"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Du warst ja sonsten nur zur Fr\u00f6hlichkeit gebohren,", "tokens": ["Du", "warst", "ja", "sons\u00b7ten", "nur", "zur", "Fr\u00f6h\u00b7lich\u00b7keit", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hast du den\u0303 allen Muth nun auf einmahl verlohren?", "tokens": ["Hast", "du", "de\u00f1", "al\u00b7len", "Muth", "nun", "auf", "ein\u00b7mahl", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "ADV", "APPR", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gewi\u00df, es w\u00e4re Schad um deine fr\u00fche Leiche,", "tokens": ["Ge\u00b7wi\u00df", ",", "es", "w\u00e4\u00b7re", "Schad", "um", "dei\u00b7ne", "fr\u00fc\u00b7he", "Lei\u00b7che", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Was machst du in dem Sarg, und in der Todten", "tokens": ["Was", "machst", "du", "in", "dem", "Sarg", ",", "und", "in", "der", "Tod\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Reiche?", "tokens": ["Rei\u00b7che", "?"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Ich glaub es kaum von dir, die Reden sind nur", "tokens": ["Ich", "glaub", "es", "kaum", "von", "dir", ",", "die", "Re\u00b7den", "sind", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,", "ART", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Schertz,", "tokens": ["Schertz", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Ein anders redt dein Mund, ein anders denckt dein", "tokens": ["Ein", "an\u00b7ders", "redt", "dein", "Mund", ",", "ein", "an\u00b7ders", "denckt", "dein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "ART", "ADV", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Hertz.", "tokens": ["Hertz", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.20": {"text": "Du w\u00fcnschest nicht den Tod, vielmehr ein l\u00e4nger", "tokens": ["Du", "w\u00fcn\u00b7schest", "nicht", "den", "Tod", ",", "viel\u00b7mehr", "ein", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Leben,", "tokens": ["Le\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.22": {"text": "Ich w\u00fcnsche dir es auch, der Himmel wird es geben.", "tokens": ["Ich", "w\u00fcn\u00b7sche", "dir", "es", "auch", ",", "der", "Him\u00b7mel", "wird", "es", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADV", "$,", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Gesetzt, du st\u00fcrbest denn, so hie\u00df dein Leichen-Stein:", "tokens": ["Ge\u00b7setzt", ",", "du", "st\u00fcr\u00b7best", "denn", ",", "so", "hie\u00df", "dein", "Lei\u00b7chen\u00b7Stein", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}}}}}