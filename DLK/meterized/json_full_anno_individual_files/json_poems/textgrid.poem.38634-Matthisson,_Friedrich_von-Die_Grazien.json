{"textgrid.poem.38634": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Die Grazien", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fccklich ist der und hochgesinnt wie G\u00f6tter,", "tokens": ["Gl\u00fcck\u00b7lich", "ist", "der", "und", "hoch\u00b7ge\u00b7sinnt", "wie", "G\u00f6t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "KON", "ADJD", "KOKOM", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der den Grazien opfert! seine Tage", "tokens": ["Der", "den", "Gra\u00b7zi\u00b7en", "op\u00b7fert", "!", "sei\u00b7ne", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "$.", "PPOSAT", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Fliessen hell wie Tage des Bl\u00fctenmondes,", "tokens": ["Flies\u00b7sen", "hell", "wie", "Ta\u00b7ge", "des", "Bl\u00fc\u00b7ten\u00b7mon\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Lieblicher S\u00e4nger!", "tokens": ["Lieb\u00b7li\u00b7cher", "S\u00e4n\u00b7ger", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Unser Pokal, geweiht von M\u00e4dchenlippen,", "tokens": ["Un\u00b7ser", "Po\u00b7kal", ",", "ge\u00b7weiht", "von", "M\u00e4d\u00b7chen\u00b7lip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "me