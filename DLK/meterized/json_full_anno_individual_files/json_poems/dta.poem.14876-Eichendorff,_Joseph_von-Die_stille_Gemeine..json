{"dta.poem.14876": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die stille Gemeine.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die st\u00fcrzten sich in den Krieg so weit,", "tokens": ["Die", "st\u00fcrz\u00b7ten", "sich", "in", "den", "Krieg", "so", "weit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie sind verweht und zerstoben,", "tokens": ["Sie", "sind", "ver\u00b7weht", "und", "zer\u00b7sto\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Kirchlein aber steht noch heut", "tokens": ["Das", "Kirch\u00b7lein", "a\u00b7ber", "steht", "noch", "heut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Unter den Linden droben.", "tokens": ["Un\u00b7ter", "den", "Lin\u00b7den", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "ADV", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}}}}