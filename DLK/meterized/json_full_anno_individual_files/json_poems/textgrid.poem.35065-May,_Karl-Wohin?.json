{"textgrid.poem.35065": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Wohin?", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frag doch einmal, und la\u00df dir endlich zeigen,", "tokens": ["Frag", "doch", "ein\u00b7mal", ",", "und", "la\u00df", "dir", "end\u00b7lich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,", "KON", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wohin du kommst, wenn du so weitergehst.", "tokens": ["Wo\u00b7hin", "du", "kommst", ",", "wenn", "du", "so", "wei\u00b7ter\u00b7gehst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du sollst nicht abw\u00e4rts sondern aufw\u00e4rts steigen;", "tokens": ["Du", "sollst", "nicht", "ab\u00b7w\u00e4rts", "son\u00b7dern", "auf\u00b7w\u00e4rts", "stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Drum halte ein, und siehe, wo du stehst!", "tokens": ["Drum", "hal\u00b7te", "ein", ",", "und", "sie\u00b7he", ",", "wo", "du", "stehst", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PTKVZ", "$,", "KON", "VVIMP", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Frag nicht die Welt, nicht sterbliche Propheten;", "tokens": ["Frag", "nicht", "die", "Welt", ",", "nicht", "sterb\u00b7li\u00b7che", "Pro\u00b7phe\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon Mancher, Mancher frug sie und beklagts.", "tokens": ["Schon", "Man\u00b7cher", ",", "Man\u00b7cher", "frug", "sie", "und", "be\u00b7klagts", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PIS", "VVFIN", "PPER", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Frag nur die Wahrheit, und sie wird dann reden;", "tokens": ["Frag", "nur", "die", "Wahr\u00b7heit", ",", "und", "sie", "wird", "dann", "re\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "KON", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Frag nur den Himmel, und der Himmel sagts!", "tokens": ["Frag", "nur", "den", "Him\u00b7mel", ",", "und", "der", "Him\u00b7mel", "sagts", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wei\u00dft du, wo du diese Wahrheit findest?", "tokens": ["Und", "wei\u00dft", "du", ",", "wo", "du", "die\u00b7se", "Wahr\u00b7heit", "fin\u00b7dest", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wei\u00dft du auch, wo d