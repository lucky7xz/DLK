{"textgrid.poem.51211": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Klage der Magd", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun ist der Lenz gekommen,", "tokens": ["Nun", "ist", "der", "Lenz", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun bl\u00fchen alle Wiesen,", "tokens": ["Nun", "bl\u00fc\u00b7hen", "al\u00b7le", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun herrschen Glanz und Freude", "tokens": ["Nun", "herr\u00b7schen", "Glanz", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Erden weit und breit;", "tokens": ["Auf", "Er\u00b7den", "weit", "und", "breit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nur meine b\u00f6se Herrin,", "tokens": ["Nur", "mei\u00b7ne", "b\u00f6\u00b7se", "Her\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+---", "measure": "unknown.measure.di"}, "line.6": {"text": "Sie keift und zetert immer", "tokens": ["Sie", "keift", "und", "ze\u00b7tert", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Noch wie in der betr\u00fcbten", "tokens": ["Noch", "wie", "in", "der", "be\u00b7tr\u00fcb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und kalten Winterzeit!", "tokens": ["Und", "kal\u00b7ten", "Win\u00b7ter\u00b7zeit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wenn ich am fr\u00fchen Morgen", "tokens": ["Wenn", "ich", "am", "fr\u00fc\u00b7hen", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit aufgewachtem Herzen", "tokens": ["Mit", "auf\u00b7ge\u00b7wach\u00b7tem", "Her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Garten grab und singe,", "tokens": ["Im", "Gar\u00b7ten", "grab", "und", "sin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Welt mir freundlich blickt,", "tokens": ["Die", "Welt", "mir", "freund\u00b7lich", "blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wirft sie mir aus dem Fenster", "tokens": ["Wirft", "sie", "mir", "aus", "dem", "Fens\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Die ungef\u00fcgen Worte,", "tokens": ["Die", "un\u00b7ge\u00b7f\u00fc\u00b7gen", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df rasch in meiner Kehle", "tokens": ["Da\u00df", "rasch", "in", "mei\u00b7ner", "Keh\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das kleine Lied erstickt.", "tokens": ["Das", "klei\u00b7ne", "Lied", "er\u00b7stickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und wenn mein Vielgeliebter", "tokens": ["Und", "wenn", "mein", "Viel\u00b7ge\u00b7lieb\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Hag vor\u00fcberwandelt", "tokens": ["Am", "Hag", "vor\u00b7\u00fc\u00b7ber\u00b7wan\u00b7delt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und ein paar warme Blicke", "tokens": ["Und", "ein", "paar", "war\u00b7me", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir in die Seele warf,", "tokens": ["Mir", "in", "die", "See\u00b7le", "warf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "H\u00f6hnt sie am Mittagsmahle,", "tokens": ["H\u00f6hnt", "sie", "am", "Mit\u00b7tags\u00b7mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df ich am untern Ende", "tokens": ["Da\u00df", "ich", "am", "un\u00b7tern", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das Auge nicht erheben", "tokens": ["Das", "Au\u00b7ge", "nicht", "er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und mich nicht r\u00fchren darf,", "tokens": ["Und", "mich", "nicht", "r\u00fch\u00b7ren", "darf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df hungernd ich, mit Tr\u00e4nen,", "tokens": ["Da\u00df", "hun\u00b7gernd", "ich", ",", "mit", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "PPER", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Essen stehenlassen", "tokens": ["Das", "Es\u00b7sen", "ste\u00b7hen\u00b7las\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mich hinweg mu\u00df wenden", "tokens": ["Und", "mich", "hin\u00b7weg", "mu\u00df", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APZR", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll Scham und voll Verdru\u00df", "tokens": ["Voll", "Scham", "und", "voll", "Ver\u00b7dru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weinend im Verborgnen", "tokens": ["Und", "wei\u00b7nend", "im", "Ver\u00b7borg\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Rinde harten Brotes", "tokens": ["Die", "Rin\u00b7de", "har\u00b7ten", "Bro\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit all den harten Reden", "tokens": ["Mit", "all", "den", "har\u00b7ten", "Re\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Hinunterw\u00fcrgen mu\u00df.", "tokens": ["Hin\u00b7un\u00b7ter\u00b7w\u00fcr\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sogar wenn ich am Sonntag", "tokens": ["So\u00b7gar", "wenn", "ich", "am", "Sonn\u00b7tag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will in die Kirche gehen", "tokens": ["Will", "in", "die", "Kir\u00b7che", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mir ein armes B\u00e4ndchen", "tokens": ["Und", "mir", "ein", "ar\u00b7mes", "B\u00e4nd\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Am Hals nicht \u00fcbel steht,", "tokens": ["Am", "Hals", "nicht", "\u00fc\u00b7bel", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vergiftet sie mir neidisch", "tokens": ["Ver\u00b7gif\u00b7tet", "sie", "mir", "nei\u00b7disch"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit ungerechtem Tadel", "tokens": ["Mit", "un\u00b7ge\u00b7rech\u00b7tem", "Ta\u00b7del"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die wochenm\u00fcde Seele,", "tokens": ["Die", "wo\u00b7chen\u00b7m\u00fc\u00b7de", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das tr\u00f6stliche Gebet.", "tokens": ["Das", "tr\u00f6st\u00b7li\u00b7che", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}}, "stanza.6": {"line.1": {"text": "Mag selber sie nur beten,", "tokens": ["Mag", "sel\u00b7ber", "sie", "nur", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ihre eignen Kinder", "tokens": ["Da\u00df", "ih\u00b7re", "eig\u00b7nen", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nicht einmal dienen m\u00fcssen,", "tokens": ["Nicht", "ein\u00b7mal", "die\u00b7nen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn ihr das Gl\u00fcck entschwand", "tokens": ["Wenn", "ihr", "das", "Gl\u00fcck", "ent\u00b7schwand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und sie als arme Mutter", "tokens": ["Und", "sie", "als", "ar\u00b7me", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wird um die H\u00e4user schleichen,", "tokens": ["Wird", "um", "die", "H\u00e4u\u00b7ser", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wo jene sind geschlagen", "tokens": ["Wo", "je\u00b7ne", "sind", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PDS", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Von b\u00f6ser Herrenhand!", "tokens": ["Von", "b\u00f6\u00b7ser", "Her\u00b7ren\u00b7hand", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nun ist der Lenz gekommen,", "tokens": ["Nun", "ist", "der", "Lenz", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Nun bl\u00fchen alle Wiesen,", "tokens": ["Nun", "bl\u00fc\u00b7hen", "al\u00b7le", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun herrschen Glanz und Freude", "tokens": ["Nun", "herr\u00b7schen", "Glanz", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Erden weit und brei