{"textgrid.poem.39372": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dort, wo sich an des Mittelmeeres Wogen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dort, wo sich an des Mittelmeeres Wogen", "tokens": ["Dort", ",", "wo", "sich", "an", "des", "Mit\u00b7tel\u00b7mee\u00b7res", "Wo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gaeta's schroffer Felsenwall erhebt,", "tokens": ["Gae\u00b7ta's", "schrof\u00b7fer", "Fel\u00b7sen\u00b7wall", "er\u00b7hebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo, rechts und links am Ufer hingezogen,", "tokens": ["Wo", ",", "rechts", "und", "links", "am", "U\u00b7fer", "hin\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADV", "KON", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der freie Blick zwo Buchten \u00fcberschwebt,", "tokens": ["Der", "frei\u00b7e", "Blick", "zwo", "Buch\u00b7ten", "\u00fc\u00b7bersc\u00b7hwebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "CARD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wo noch nicht in Verge\u00dfenheit entflogen", "tokens": ["Wo", "noch", "nicht", "in", "Ver\u00b7ge\u00b7\u00dfen\u00b7heit", "ent\u00b7flo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PTKNEG", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So manch' uralte Heldensage lebt,", "tokens": ["So", "man\u00b7ch'", "ur\u00b7al\u00b7te", "Hel\u00b7den\u00b7sa\u00b7ge", "lebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Wie seiner Amme hier zum Angedenken", "tokens": ["Wie", "sei\u00b7ner", "Am\u00b7me", "hier", "zum", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aeneas Grab und Namen wollte schenken:", "tokens": ["A\u00b7e\u00b7neas", "Grab", "und", "Na\u00b7men", "woll\u00b7te", "schen\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da siehest du der Bergesh\u00f6hen eine", "tokens": ["Da", "sie\u00b7hest", "du", "der", "Ber\u00b7ges\u00b7h\u00f6\u00b7hen", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gespalten ganz durch eine selt'ne Kluft.", "tokens": ["Ge\u00b7spal\u00b7ten", "ganz", "durch", "ei\u00b7ne", "selt'\u00b7ne", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es geht der Ri\u00df hinab im harten Steine", "tokens": ["Es", "geht", "der", "Ri\u00df", "hin\u00b7ab", "im", "har\u00b7ten", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Gipfel an bis in die tiefste Gruft,", "tokens": ["Vom", "Gip\u00b7fel", "an", "bis", "in", "die", "tiefs\u00b7te", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch hemmt die beiden W\u00e4nde vom Vereine", "tokens": ["Doch", "hemmt", "die", "bei\u00b7den", "W\u00e4n\u00b7de", "vom", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur enger Raum und wenig Himmelsluft,", "tokens": ["Nur", "en\u00b7ger", "Raum", "und", "we\u00b7nig", "Him\u00b7mel\u00b7sluft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Also, da\u00df Einer nur und nach dem Andern", "tokens": ["Al\u00b7so", ",", "da\u00df", "Ei\u00b7ner", "nur", "und", "nach", "dem", "An\u00b7dern"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADV", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den dunkeln Fu\u00dfsteig mag hinunter wandern.", "tokens": ["Den", "dun\u00b7keln", "Fu\u00df\u00b7steig", "mag", "hin\u00b7un\u00b7ter", "wan\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und es berichten uns die heil'gen Sagen:", "tokens": ["Und", "es", "be\u00b7rich\u00b7ten", "uns", "die", "heil'\u00b7gen", "Sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Derselbe Berg war vormals fest und dicht.", "tokens": ["Der\u00b7sel\u00b7be", "Berg", "war", "vor\u00b7mals", "fest", "und", "dicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch als der Heiland, an das Kreuz geschlagen,", "tokens": ["Doch", "als", "der", "Hei\u00b7land", ",", "an", "das", "Kreuz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr unsre S\u00fcnd' erlitten das Gericht,", "tokens": ["F\u00fcr", "uns\u00b7re", "S\u00fcnd'", "er\u00b7lit\u00b7ten", "das", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und als der Schrei von seinem letzten Zagen,", "tokens": ["Und", "als", "der", "Schrei", "von", "sei\u00b7nem", "letz\u00b7ten", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der durch der Erde Schoo\u00df ersch\u00fctternd bricht,", "tokens": ["Der", "durch", "der", "Er\u00b7de", "Schoo\u00df", "er\u00b7sch\u00fct\u00b7ternd", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die H\u00fcll' am Allerheiligsten zerri\u00dfen;", "tokens": ["Die", "H\u00fcll'", "am", "Al\u00b7ler\u00b7hei\u00b7ligs\u00b7ten", "zer\u00b7ri\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Da hat sich auch des Felsen Brust zerspli\u00dfen.", "tokens": ["Da", "hat", "sich", "auch", "des", "Fel\u00b7sen", "Brust", "zer\u00b7spli\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Kirche, von der Andacht Trieb geleitet,", "tokens": ["Die", "Kir\u00b7che", ",", "von", "der", "An\u00b7dacht", "Trieb", "ge\u00b7lei\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hat dieses Wunderzeichen wohl gehegt.", "tokens": ["Hat", "die\u00b7ses", "Wun\u00b7der\u00b7zei\u00b7chen", "wohl", "ge\u00b7hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier ist ein Weg zur Pilgerfahrt bereitet,", "tokens": ["Hier", "ist", "ein", "Weg", "zur", "Pil\u00b7ger\u00b7fahrt", "be\u00b7rei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wobei die Seele Christi Leid erw\u00e4gt;", "tokens": ["Wo\u00b7bei", "die", "See\u00b7le", "Chris\u00b7ti", "Leid", "er\u00b7w\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An dreizehn Stellen, wie man nieder schreitet,", "tokens": ["An", "drei\u00b7zehn", "Stel\u00b7len", ",", "wie", "man", "nie\u00b7der", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "PWAV", "PIS", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Erscheint das Bild des Kreuzes eingepr\u00e4gt;", "tokens": ["Er\u00b7scheint", "das", "Bild", "des", "Kreu\u00b7zes", "ein\u00b7ge\u00b7pr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bald murmelnd, bald ger\u00e4uschig, sp\u00fclt die Welle", "tokens": ["Bald", "mur\u00b7melnd", ",", "bald", "ge\u00b7r\u00e4u\u00b7schig", ",", "sp\u00fclt", "die", "Wel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVPP", "$,", "ADV", "ADJD", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Am Ausgang um die heimliche Kapelle.", "tokens": ["Am", "Aus\u00b7gang", "um", "die", "heim\u00b7li\u00b7che", "Ka\u00b7pel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": 