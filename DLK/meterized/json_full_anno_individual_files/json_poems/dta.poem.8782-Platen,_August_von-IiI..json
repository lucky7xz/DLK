{"dta.poem.8782": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "IiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Auge lispelts leise mir;", "tokens": ["Dein", "Au\u00b7ge", "lis\u00b7pelts", "lei\u00b7se", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist ein Gastfreund ohne Hehl", "tokens": ["Du", "bist", "ein", "Gast\u00b7freund", "oh\u00b7ne", "Hehl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dieser langen Reise mir;", "tokens": ["Auf", "die\u00b7ser", "lan\u00b7gen", "Rei\u00b7se", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein Leben wird, da\u00df Liebe noch", "tokens": ["Dein", "Le\u00b7ben", "wird", ",", "da\u00df", "Lie\u00b7be", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lebendig, zum Beweise mir;", "tokens": ["Le\u00b7ben\u00b7dig", ",", "zum", "Be\u00b7wei\u00b7se", "mir", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPRART", "NN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Du bringst der Liebe Moschusduft,", "tokens": ["Du", "bringst", "der", "Lie\u00b7be", "Mo\u00b7schus\u00b7duft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du bringst der Wahrheit Speise mir;", "tokens": ["Du", "bringst", "der", "Wahr\u00b7heit", "Spei\u00b7se", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es wird so licht, es wird so warm", "tokens": ["Es", "wird", "so", "licht", ",", "es", "wird", "so", "warm"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In deinem lieben Kreise mir;", "tokens": ["In", "dei\u00b7nem", "lie\u00b7ben", "Krei\u00b7se", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du bist die Perle, deren Werth", "tokens": ["Du", "bist", "die", "Per\u00b7le", ",", "de\u00b7ren", "Werth"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Hoch \u00fcber jedem Preise mir!", "tokens": ["Hoch", "\u00fc\u00b7ber", "je\u00b7dem", "Prei\u00b7se", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIAT", "NN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}