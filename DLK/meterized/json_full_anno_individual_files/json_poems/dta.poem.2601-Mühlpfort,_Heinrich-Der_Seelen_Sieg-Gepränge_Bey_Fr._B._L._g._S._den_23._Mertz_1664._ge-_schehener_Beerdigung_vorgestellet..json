{"dta.poem.2601": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Seelen Sieg-Gepr\u00e4nge/  \n  Bey Fr. B. L. g. S. den 23. Mertz 1664. ge-  \n schehener Beerdigung vorgestellet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00fcck zu! Erlauchte Seel' in diesem Hoheits-Stande!", "tokens": ["Gl\u00fcck", "zu", "!", "Er\u00b7lauch\u00b7te", "Seel'", "in", "die\u00b7sem", "Ho\u00b7heits\u00b7Stan\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADJA", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein herrlich Sieggepr\u00e4ng\u2019 entz\u00fcndet unsern Geist/", "tokens": ["Dein", "herr\u00b7lich", "Sieg\u00b7ge\u00b7pr\u00e4ng'", "ent\u00b7z\u00fcn\u00b7det", "un\u00b7sern", "Geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df er mit gleicher Lust die Welt verg\u00e4nglich heist/", "tokens": ["Da\u00df", "er", "mit", "glei\u00b7cher", "Lust", "die", "Welt", "ver\u00b7g\u00e4ng\u00b7lich", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "ART", "NN", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und deine Kronen lieb\u2019t/ hast aber seine Bande/", "tokens": ["Und", "dei\u00b7ne", "Kro\u00b7nen", "lie\u00b7b't", "/", "hast", "a\u00b7ber", "sei\u00b7ne", "Ban\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$(", "VAFIN", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Freyheit kleidet dich mit weissen Lilgen an/", "tokens": ["Die", "Frey\u00b7heit", "klei\u00b7det", "dich", "mit", "weis\u00b7sen", "Lil\u00b7gen", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die weder Noth noch Tod hinfort verw\u00fcsten kan.", "tokens": ["Die", "we\u00b7der", "Noth", "noch", "Tod", "hin\u00b7fort", "ver\u00b7w\u00fcs\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KON", "NN", "ADV", "NN", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Leib lieg\u2019t hier entseel\u2019t/ ein\u2019 au\u00dfgedorrte Schaale/", "tokens": ["Der", "Leib", "lieg't", "hier", "ent\u00b7seel't", "/", "ein'", "au\u00df\u00b7ge\u00b7dorr\u00b7te", "Schaa\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVFIN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Fessel sind zermalmt/ so dich so hart bestrick\u2019t/", "tokens": ["Die", "Fes\u00b7sel", "sind", "zer\u00b7malmt", "/", "so", "dich", "so", "hart", "be\u00b7strick't", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$(", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das m\u00fcrbe Wohnhaus ist bi\u00df auf den Grund zerst\u00fcck\u2019t/", "tokens": ["Das", "m\u00fcr\u00b7be", "Wohn\u00b7haus", "ist", "bi\u00df", "auf", "den", "Grund", "zer\u00b7st\u00fc\u00b7ck't", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Du prangest unverletz\u2019t im g\u00fcldnen Sternen-Saale/", "tokens": ["Du", "pran\u00b7gest", "un\u00b7ver\u00b7letz't", "im", "g\u00fcld\u00b7nen", "Ster\u00b7nen\u00b7Saa\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "In GOtt verkl\u00e4rte Seel/ di\u00df Zimmer ist mehr sch\u00f6n", "tokens": ["In", "Gott", "ver\u00b7kl\u00e4r\u00b7te", "Seel", "/", "di\u00df", "Zim\u00b7mer", "ist", "mehr", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "$(", "PDS", "NN", "VAFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Als di\u00df/ wo Schmertzen nur/ des Lebens Hencker geh\u2019n.", "tokens": ["Als", "di\u00df", "/", "wo", "Schmert\u00b7zen", "nur", "/", "des", "Le\u00b7bens", "Hen\u00b7cker", "geh'", "n."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PDS", "$(", "PWAV", "NN", "ADV", "$(", "ART", "NN", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch m\u00fcssen wir zuvor die Folterbanck betrachten/", "tokens": ["Doch", "m\u00fcs\u00b7sen", "wir", "zu\u00b7vor", "die", "Fol\u00b7ter\u00b7banck", "be\u00b7trach\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die dich im Fleisch und Blut erb\u00e4rmlich hat gekl\u00e4mt/", "tokens": ["Die", "dich", "im", "Fleisch", "und", "Blut", "er\u00b7b\u00e4rm\u00b7lich", "hat", "ge\u00b7kl\u00e4mt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "NN", "ADJD", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Thr\u00e4nen/ die die Bach der Augen \u00fcberschw\u00e4mt/", "tokens": ["Die", "Thr\u00e4\u00b7nen", "/", "die", "die", "Bach", "der", "Au\u00b7gen", "\u00fc\u00b7bersc\u00b7hw\u00e4mt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die Angst/ in der das Hertz\u2019 unendlich muste schmachten/", "tokens": ["Die", "Angst", "/", "in", "der", "das", "Hertz'", "un\u00b7end\u00b7lich", "mus\u00b7te", "schmach\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "APPR", "ART", "ART", "NN", "ADJD", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Bezeugen wie der Leib gantz ausgemergelt lag/", "tokens": ["Be\u00b7zeu\u00b7gen", "wie", "der", "Leib", "gantz", "aus\u00b7ge\u00b7mer\u00b7gelt", "lag", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADV", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie sehnlich du gew\u00fcnsch\u2019t den s\u00fcssen L\u00f6sungs-Tag.", "tokens": ["Wie", "sehn\u00b7lich", "du", "ge\u00b7w\u00fcn\u00b7sch't", "den", "s\u00fcs\u00b7sen", "L\u00f6\u00b7sungs\u00b7Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Bey solches Alters Last/ bey so verlebten Jahren/", "tokens": ["Bey", "sol\u00b7ches", "Al\u00b7ters", "Last", "/", "bey", "so", "ver\u00b7leb\u00b7ten", "Jah\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NE", "$(", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Bey so verkehrter Zeit/ da T\u00fcrck\u2019 und Teufel raast/", "tokens": ["Bey", "so", "ver\u00b7kehr\u00b7ter", "Zeit", "/", "da", "T\u00fcrck'", "und", "Teu\u00b7fel", "raast", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Als abgesagter Freund nach unsrem Kopff uns graast/", "tokens": ["Als", "ab\u00b7ge\u00b7sag\u00b7ter", "Freund", "nach", "uns\u00b7rem", "Kopff", "uns", "graast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Bestand der Hoffnung Schlu\u00df von hinnen weg zu fahren:", "tokens": ["Be\u00b7stand", "der", "Hoff\u00b7nung", "Schlu\u00df", "von", "hin\u00b7nen", "weg", "zu", "fah\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "APPR", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Die Lippen wurden stum/ das Auge sanck in Nacht", "tokens": ["Die", "Lip\u00b7pen", "wur\u00b7den", "stum", "/", "das", "Au\u00b7ge", "san\u00b7ck", "in", "Nacht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.24": {"text": "Die Beine streckten sich/ noch Puls noch Ader wacht.", "tokens": ["Die", "Bei\u00b7ne", "streck\u00b7ten", "sich", "/", "noch", "Puls", "noch", "A\u00b7der", "wacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ADV", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Man h\u00f6rte weiter nichts als ein bedr\u00e4ngtes r\u00f6cheln/", "tokens": ["Man", "h\u00f6r\u00b7te", "wei\u00b7ter", "nichts", "als", "ein", "be\u00b7dr\u00e4ng\u00b7tes", "r\u00f6\u00b7cheln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "PIS", "KOKOM", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und wie du mit dem Tod un\u00fcberwindlich schl\u00e4gst/", "tokens": ["Und", "wie", "du", "mit", "dem", "Tod", "un\u00b7\u00fc\u00b7berw\u00b7ind\u00b7lich", "schl\u00e4gst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wie du bi\u00dfweilen noch das matte Hertz bewegst", "tokens": ["Wie", "du", "bi\u00df\u00b7wei\u00b7len", "noch", "das", "mat\u00b7te", "Hertz", "be\u00b7wegst"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und wilst der m\u00fcden Lung\u2019 ein wenig Luft zuf\u00e4cheln/", "tokens": ["Und", "wilst", "der", "m\u00fc\u00b7den", "Lung'", "ein", "we\u00b7nig", "Luft", "zu\u00b7f\u00e4\u00b7cheln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "ADJA", "NN", "ART", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Bi\u00df dich des H\u00f6chsten Hand aus solchem Kercker f\u00fchrt/", "tokens": ["Bi\u00df", "dich", "des", "H\u00f6chs\u00b7ten", "Hand", "aus", "sol\u00b7chem", "Ker\u00b7cker", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und deine Tapfferkeit mit Sieges-Palmen ziert.", "tokens": ["Und", "dei\u00b7ne", "Tapf\u00b7fer\u00b7keit", "mit", "Sie\u00b7ges\u00b7Pal\u00b7men", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Jtzt wirstu erst belehr\u2019t was wir im Busen tragen/", "tokens": ["Jtzt", "wirs\u00b7tu", "erst", "be\u00b7lehr't", "was", "wir", "im", "Bu\u00b7sen", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "VVFIN", "PWS", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ob gleich der Jugend M\u00e4y aus allen Gliedern lacht/", "tokens": ["Ob", "gleich", "der", "Ju\u00b7gend", "M\u00e4y", "aus", "al\u00b7len", "Glie\u00b7dern", "lacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "NE", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ob sich das Angesicht zu Ros\u2019 und Lilgen macht/", "tokens": ["Ob", "sich", "das", "An\u00b7ge\u00b7sicht", "zu", "Ros'", "und", "Lil\u00b7gen", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "APPR", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So werden W\u00fcrmer doch den sch\u00f6nsten Schmuck benagen/", "tokens": ["So", "wer\u00b7den", "W\u00fcr\u00b7mer", "doch", "den", "sch\u00f6ns\u00b7ten", "Schmuck", "be\u00b7na\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die Kr\u00e4ffte schw\u00e4chen sich/ die Wei\u00dfheit lig\u2019t entsin\u2019t", "tokens": ["Die", "Kr\u00e4ff\u00b7te", "schw\u00e4\u00b7chen", "sich", "/", "die", "Wei\u00df\u00b7heit", "lig't", "ent\u00b7sin't"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "ART", "NN", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wo nicht der Mensch sein Ziel auf etwas bessers gr\u00fcndt.", "tokens": ["Wo", "nicht", "der", "Mensch", "sein", "Ziel", "auf", "et\u00b7was", "bes\u00b7sers", "gr\u00fcndt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "APPR", "PIAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Ein Sammelplatz der Noth ist ein verl\u00e4ngtes Leben/", "tokens": ["Ein", "Sam\u00b7mel\u00b7platz", "der", "Noth", "ist", "ein", "ver\u00b7l\u00e4ng\u00b7tes", "Le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die besten Sch\u00e4tze sind/ Pest/ Seuche/ Kranckheit/ Gifft/", "tokens": ["Die", "bes\u00b7ten", "Sch\u00e4t\u00b7ze", "sind", "/", "Pest", "/", "Seu\u00b7che", "/", "Kran\u00b7ck\u00b7heit", "/", "Gifft", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$(", "NE", "$(", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+--++-+--+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Und wer gleich bey Charybd gl\u00fcckseelig weggeschifft/", "tokens": ["Und", "wer", "gleich", "bey", "Cha\u00b7rybd", "gl\u00fcck\u00b7see\u00b7lig", "weg\u00b7ge\u00b7schifft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "APPR", "NE", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Mu\u00df doch bey Scyllens Schlund den Zoll dem Tode geben/", "tokens": ["Mu\u00df", "doch", "bey", "Scyl\u00b7lens", "Schlund", "den", "Zoll", "dem", "To\u00b7de", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wo