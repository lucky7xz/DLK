{"dta.poem.18846": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "H\u00f6heres und H\u00f6chstes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Dass wir solche Dinge lehren", "tokens": ["Dass", "wir", "sol\u00b7che", "Din\u00b7ge", "leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00f6ge man uns nicht bestrafen:", "tokens": ["M\u00f6\u00b7ge", "man", "uns", "nicht", "be\u00b7stra\u00b7fen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das alles zu erkl\u00e4ren", "tokens": ["Wie", "das", "al\u00b7les", "zu", "er\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "PIS", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "D\u00fcrft ihr euer Tiefstes fragen.", "tokens": ["D\u00fcrft", "ihr", "eu\u00b7er", "Tiefs\u00b7tes", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und so werdet ihr vernehmen", "tokens": ["Und", "so", "wer\u00b7det", "ihr", "ver\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dass der Mensch, mit sich zufrieden,", "tokens": ["Dass", "der", "Mensch", ",", "mit", "sich", "zu\u00b7frie\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "APPR", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gern sein Ich gerettet s\u00e4he,", "tokens": ["Gern", "sein", "Ich", "ge\u00b7ret\u00b7tet", "s\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "PPER", "VVPP", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "So dadroben wie hienieden.", "tokens": ["So", "da\u00b7dro\u00b7ben", "wie", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und mein liebes Ich bed\u00fcrfte", "tokens": ["Und", "mein", "lie\u00b7bes", "Ich", "be\u00b7d\u00fcrf\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mancherley Bequemlichkeiten,", "tokens": ["Man\u00b7cher\u00b7ley", "Be\u00b7quem\u00b7lich\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Freuden wie ich hier sie schl\u00fcrfte", "tokens": ["Freu\u00b7den", "wie", "ich", "hier", "sie", "schl\u00fcrf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "PPER", "ADV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcnscht\u2019 ich auch f\u00fcr ew\u2019ge Zeiten.", "tokens": ["W\u00fcnscht'", "ich", "auch", "f\u00fcr", "ew'\u00b7ge", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "So gefallen sch\u00f6ne G\u00e4rten", "tokens": ["So", "ge\u00b7fal\u00b7len", "sch\u00f6\u00b7ne", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVPP", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blum und Frucht und h\u00fcbsche Kinder,", "tokens": ["Blum", "und", "Frucht", "und", "h\u00fcb\u00b7sche", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die uns allen hier gefielen,", "tokens": ["Die", "uns", "al\u00b7len", "hier", "ge\u00b7fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch verj\u00fcngtem Geist nicht minder.", "tokens": ["Auch", "ver\u00b7j\u00fcng\u00b7tem", "Geist", "nicht", "min\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und so m\u00f6cht\u2019 ich alle Freunde", "tokens": ["Und", "so", "m\u00f6cht'", "ich", "al\u00b7le", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jung und alt in Eins versammlen,", "tokens": ["Jung", "und", "alt", "in", "Eins", "ver\u00b7samm\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gar zu gern in deutscher Sprache", "tokens": ["Gar", "zu", "gern", "in", "deut\u00b7scher", "Spra\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKA", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Paradieses-Worte stammlen.", "tokens": ["Pa\u00b7ra\u00b7die\u00b7ses\u00b7Wor\u00b7te", "stamm\u00b7len", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch man horcht nun Dialekten", "tokens": ["Doch", "man", "horcht", "nun", "Di\u00b7a\u00b7lek\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie sich Mensch und Engel kosen,", "tokens": ["Wie", "sich", "Mensch", "und", "En\u00b7gel", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Der Grammatik, der versteckten,", "tokens": ["Der", "Gram\u00b7ma\u00b7tik", ",", "der", "ver\u00b7steck\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Declinirend Mohn und Rosen.", "tokens": ["De\u00b7cli\u00b7ni\u00b7rend", "Mohn", "und", "Ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Mag man ferner auch in Blicken", "tokens": ["Mag", "man", "fer\u00b7ner", "auch", "in", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich rhetorisch gern ergehen,", "tokens": ["Sich", "rhe\u00b7to\u00b7risch", "gern", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und zu himmlischem Entz\u00fccken", "tokens": ["Und", "zu", "himm\u00b7li\u00b7schem", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Klang und Ton erh\u00f6hen.", "tokens": ["Oh\u00b7ne", "Klang", "und", "Ton", "er\u00b7h\u00f6\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ton und Klang jedoch entwindet", "tokens": ["Ton", "und", "Klang", "je\u00b7doch", "ent\u00b7win\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich dem Worte selbstverst\u00e4ndlich,", "tokens": ["Sich", "dem", "Wor\u00b7te", "selbst\u00b7ver\u00b7st\u00e4nd\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und entschiedener empfindet", "tokens": ["Und", "ent\u00b7schie\u00b7de\u00b7ner", "emp\u00b7fin\u00b7det"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Verkl\u00e4rte sich unendlich.", "tokens": ["Der", "Ver\u00b7kl\u00e4r\u00b7te", "sich", "un\u00b7end\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ist somit dem F\u00fcnf der Sinne", "tokens": ["Ist", "so\u00b7mit", "dem", "F\u00fcnf", "der", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vorgesehn im Paradiese,", "tokens": ["Vor\u00b7ge\u00b7sehn", "im", "Pa\u00b7ra\u00b7die\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sicher ist es ich gewinne", "tokens": ["Si\u00b7cher", "ist", "es", "ich", "ge\u00b7win\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Einen Sinn f\u00fcr alle diese.", "tokens": ["Ei\u00b7nen", "Sinn", "f\u00fcr", "al\u00b7le", "die\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIS", "PDAT", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und nun dring ich aller Orten", "tokens": ["Und", "nun", "dring", "ich", "al\u00b7ler", "Or\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leichter durch die ewigen Kreise,", "tokens": ["Leich\u00b7ter", "durch", "die", "e\u00b7wi\u00b7gen", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die durchdrungen sind vom Worte", "tokens": ["Die", "durch\u00b7drun\u00b7gen", "sind", "vom", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "VVPP", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gottes rein-lebendigerweise.", "tokens": ["Got\u00b7tes", "rein\u00b7le\u00b7ben\u00b7di\u00b7ger\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADJD", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Ungehemmt mit heissem Triebe", "tokens": ["Un\u00b7ge\u00b7hemmt", "mit", "heis\u00b7sem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "L\u00e4sst sich da kein Ende finden,", "tokens": ["L\u00e4sst", "sich", "da", "kein", "En\u00b7de", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis im Anschaun ewiger Liebe", "tokens": ["Bis", "im", "An\u00b7schaun", "e\u00b7wi\u00b7ger", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPRART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wir verschweben, wir verschwinden.", "tokens": ["Wir", "ver\u00b7schwe\u00b7ben", ",", "wir", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}