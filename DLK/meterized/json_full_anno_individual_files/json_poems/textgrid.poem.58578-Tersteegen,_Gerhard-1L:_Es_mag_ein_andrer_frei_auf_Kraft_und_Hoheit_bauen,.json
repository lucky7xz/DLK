{"textgrid.poem.58578": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es mag ein andrer frei auf Kraft und Hoheit bauen,", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es mag ein andrer frei auf Kraft und Hoheit bauen,", "tokens": ["Es", "mag", "ein", "an\u00b7drer", "frei", "auf", "Kraft", "und", "Ho\u00b7heit", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Ruhm ist, da\u00df ich arm und ganz geringe bin;", "tokens": ["Mein", "Ruhm", "ist", ",", "da\u00df", "ich", "arm", "und", "ganz", "ge\u00b7rin\u00b7ge", "bin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "ADV", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Herr ist meine Kraft, auf den Entbl\u00f6\u00dfte trauen,", "tokens": ["Der", "Herr", "ist", "mei\u00b7ne", "Kraft", ",", "auf", "den", "Ent\u00b7bl\u00f6\u00df\u00b7te", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zwar \u00e4ngstet mich mein Feind, mit Gott ich doch gewinn'.", "tokens": ["Zwar", "\u00e4ngs\u00b7tet", "mich", "mein", "Feind", ",", "mit", "Gott", "ich", "doch", "ge\u00b7winn'", "."], "token_info": ["word", "word", "word", "word", "word", "punct