mbic.tri"}, "line.2": {"text": "Von s\u00fc\u00dfer Lieb' und Treu',", "tokens": ["Von", "s\u00fc\u00b7\u00dfer", "Lieb'", "und", "Treu'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und flehen doch so innig,", "tokens": ["Und", "fle\u00b7hen", "doch", "so", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll zarter M\u00e4dchenscheu.", "tokens": ["Voll", "zar\u00b7ter", "M\u00e4d\u00b7chen\u00b7scheu", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbo schweige noch, du Trauter,", "tokens": ["\u00bb", "o", "schwei\u00b7ge", "noch", ",", "du", "Trau\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "FM", "ADJA", "ADV", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df noch kein Hauch gesteht", "tokens": ["Da\u00df", "noch", "kein", "Hauch", "ge\u00b7steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Der Seele heil'gen Schauder,", "tokens": ["Der", "See\u00b7le", "heil'\u00b7gen", "Schau\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der wonnig uns durchweht.\u00ab", "tokens": ["Der", "won\u00b7nig", "uns", "durch\u00b7weht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "Er mu\u00df sich vor ihr neigen", "tokens": ["Er", "mu\u00df", "sich", "vor", "ihr", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In sel'ger Trunkenheit,", "tokens": ["In", "sel'\u00b7ger", "Trun\u00b7ken\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann bringt ihr banges Schweigen", "tokens": ["Dann", "bringt", "ihr", "ban\u00b7ges", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihm neue Traurigkeit;", "tokens": ["Ihm", "neu\u00b7e", "Trau\u00b7rig\u00b7keit", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Aufs neue weilt voll Sorgen", "tokens": ["Aufs", "neu\u00b7e", "weilt", "voll", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf ihr sein Sehnsuchtsblick,", "tokens": ["Auf", "ihr", "sein", "Sehn\u00b7suchts\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbgeduld, noch gibt's ein Morgen!\u00ab", "tokens": ["\u00bb", "ge\u00b7duld", ",", "noch", "gibt's", "ein", "Mor\u00b7gen", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Schaut l\u00e4chelnd sie zur\u00fcck.", "tokens": ["Schaut", "l\u00e4\u00b7chelnd", "sie", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "O Knospe junger Liebe,", "tokens": ["O", "Knos\u00b7pe", "jun\u00b7ger", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die sich entfaltet still,", "tokens": ["Die", "sich", "ent\u00b7fal\u00b7tet", "still", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und noch in spr\u00f6dem Triebe", "tokens": ["Und", "noch", "in", "spr\u00f6\u00b7dem", "Trie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Kelch nicht \u00f6ffnen will,", "tokens": ["Den", "Kelch", "nicht", "\u00f6ff\u00b7nen", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Du gleichst der Alpen-Frauen", "tokens": ["Du", "gleichst", "der", "Al\u00b7pen\u00b7Frau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erhabner Majest\u00e4t,", "tokens": ["Er\u00b7hab\u00b7ner", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So hat bei deren Schauen", "tokens": ["So", "hat", "bei", "de\u00b7ren", "Schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PRELAT", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Ihn Ehrfurcht tief umweht!", "tokens": ["Ihn", "Ehr\u00b7furcht", "tief", "um\u00b7weht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}