{"textgrid.poem.33123": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Frisch wie des Gletschers Quelle", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frisch wie des Gletschers Quelle", "tokens": ["Frisch", "wie", "des", "Glet\u00b7schers", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hervorspringt in das Thal,", "tokens": ["Her\u00b7vor\u00b7springt", "in", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entz\u00fcckt, da\u00df ihre Welle", "tokens": ["Ent\u00b7z\u00fcckt", ",", "da\u00df", "ih\u00b7re", "Wel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Begr\u00fc\u00dft des Tages Strahl,", "tokens": ["Be\u00b7gr\u00fc\u00dft", "des", "Ta\u00b7ges", "Strahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und brausend nun zerschl\u00e4get", "tokens": ["Und", "brau\u00b7send", "nun", "zer\u00b7schl\u00e4\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Bett von Felsgestein,", "tokens": ["Ihr", "Bett", "von", "Fels\u00b7ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nachdem sie sich beweget", "tokens": ["Nach\u00b7dem", "sie", "sich", "be\u00b7we\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So lang in Nacht allein; \u2013", "tokens": ["So", "lang", "in", "Nacht", "al\u00b7lein", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "So frisch und so urkr\u00e4ftig", "tokens": ["So", "frisch", "und", "so", "ur\u00b7kr\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+++-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Herrn Corsant es durchwallt,", "tokens": ["Herrn", "Cor\u00b7sant", "es", "durch\u00b7wallt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PPER", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "So war in ihm gesch\u00e4ftig", "tokens": ["So", "war", "in", "ihm", "ge\u00b7sch\u00e4f\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Liebe Allgewalt;", "tokens": ["Der", "Lie\u00b7be", "All\u00b7ge\u00b7walt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Es sprenget ihre Quelle", "tokens": ["Es", "spren\u00b7get", "ih\u00b7re", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mit um so h\u00f6h'rer Lust", "tokens": ["Mit", "um", "so", "h\u00f6h'\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die eigensinn'ge Schwelle", "tokens": ["Die", "ei\u00b7gen\u00b7sinn'\u00b7ge", "Schwel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Der kalten Felsenbrust.", "tokens": ["Der", "kal\u00b7ten", "Fel\u00b7sen\u00b7brust", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Rasch wollt' er sich erretten", "tokens": ["Rasch", "wollt'", "er", "sich", "er\u00b7ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus seiner Bu\u00dfe Nacht,", "tokens": ["Aus", "sei\u00b7ner", "Bu\u00b7\u00dfe", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun seufzet er nach Ketten,", "tokens": ["Nun", "seuf\u00b7zet", "er", "nach", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die er so oft verlacht.", "tokens": ["Die", "er", "so", "oft", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wo sind der Heimath Sorgen?", "tokens": ["Wo", "sind", "der", "Hei\u00b7math", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was k\u00fcmmert ihn Turin?", "tokens": ["Was", "k\u00fcm\u00b7mert", "ihn", "Tu\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Mit jedem neuen Morgen", "tokens": ["Mit", "je\u00b7dem", "neu\u00b7en", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sieht man zum Schlo\u00df ihn ziehn.", "tokens": ["Sieht", "man", "zum", "Schlo\u00df", "ihn", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort unter jenen B\u00e4umen,", "tokens": ["Dort", "un\u00b7ter", "je\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo er zuerst sie sah,", "tokens": ["Wo", "er", "zu\u00b7erst", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geht er in wachen Tr\u00e4umen,", "tokens": ["Geht", "er", "in", "wa\u00b7chen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis sie ihm wieder nah;", "tokens": ["Bis", "sie", "ihm", "wie\u00b7der", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis sie lustwandelnd kommen,", "tokens": ["Bis", "sie", "lust\u00b7wan\u00b7delnd", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Frau Bertha mit dem Kind,", "tokens": ["Frau", "Bert\u00b7ha", "mit", "dem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jolanthe zwar beklommen,", "tokens": ["Jo\u00b7lan\u00b7the", "zwar", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Herz doch frohgesinnt.", "tokens": ["Das", "Herz", "doch", "froh\u00b7ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zum Seufzen doch und Schmachten", "tokens": ["Zum", "Seuf\u00b7zen", "doch", "und", "Schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4\u00dft Bertha ihm nicht Zeit,", "tokens": ["L\u00e4\u00dft", "Bert\u00b7ha", "ihm", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollt' er die Lieb' verachten,", "tokens": ["Wollt'", "er", "die", "Lieb'", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchl' er auch jetzt ihr Leid.", "tokens": ["F\u00fchl'", "er", "auch", "jetzt", "ihr", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er m\u00f6cht' so gern ihr sagen,", "tokens": ["Er", "m\u00f6cht'", "so", "gern", "ihr", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was tief sein Herz bewegt \u2013", "tokens": ["Was", "tief", "sein", "Herz", "be\u00b7wegt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mit tausend lust'gen Fragen", "tokens": ["Mit", "tau\u00b7send", "lust'\u00b7gen", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "CARD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie stets zur\u00fcck ihn schl\u00e4gt.", "tokens": ["Sie", "stets", "zu\u00b7r\u00fcck", "ihn", "schl\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Jolanthe zittert freilich,", "tokens": ["Jo\u00b7lan\u00b7the", "zit\u00b7tert", "frei\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sie ihn neckt und plagt.", "tokens": ["Wie", "sie", "ihn", "neckt", "und", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbihr hattet's doch so eilig,", "tokens": ["\u00bb", "ihr", "hat\u00b7tet's", "doch", "so", "ei\u00b7lig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann zieht Ihr heim denn, sagt?", "tokens": ["Wann", "zieht", "Ihr", "heim", "denn", ",", "sagt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PTKVZ", "KON", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Warum geht Ihr nicht holen", "tokens": ["Wa\u00b7rum", "geht", "Ihr", "nicht", "ho\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Am Hof des Herzogs dort,", "tokens": ["Am", "Hof", "des", "Her\u00b7zogs", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie ich es doch befohlen,", "tokens": ["Wie", "ich", "es", "doch", "be\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr Euch ein Weib sofort?\u00ab", "tokens": ["F\u00fcr", "Euch", "ein", "Weib", "so\u00b7fort", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbweil ich nur hier will minnen\u00ab,", "tokens": ["\u00bb", "weil", "ich", "nur", "hier", "will", "min\u00b7nen", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Herr Corsant drauf entbeut,", "tokens": ["Herr", "Cor\u00b7sant", "drauf", "ent\u00b7beut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PAV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbich streb' mit allen Sinnen", "tokens": ["\u00bb", "ich", "streb'", "mit", "al\u00b7len", "Sin\u00b7nen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nach einer Schweizermaid.\u00ab", "tokens": ["Nach", "ei\u00b7ner", "Schwei\u00b7zer\u00b7maid", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdann bleibet hier nicht stehen,", "tokens": ["\u00bb", "dann", "blei\u00b7bet", "hier", "nicht", "ste\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sonst seid Ihr \u00fcbel dran,", "tokens": ["Sonst", "seid", "Ihr", "\u00fc\u00b7bel", "dran", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbund doch will hier ich freien,", "tokens": ["\u00bb", "und", "doch", "will", "hier", "ich", "frei\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "ADV", "VMFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frau Bertha von Blonay,", "tokens": ["Frau", "Bert\u00b7ha", "von", "Blo\u00b7nay", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "NE", "$,"], "meter": "++--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Trotz Euren Schelmereien", "tokens": ["Trotz", "Eu\u00b7ren", "Schel\u00b7me\u00b7rei\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleib' ich in Eurer N\u00e4h'!\u00ab", "tokens": ["Bleib'", "ich", "in", "Eu\u00b7rer", "N\u00e4h'", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er mochte schon vertrauen", "tokens": ["Er", "moch\u00b7te", "schon", "ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Auf seinen Uebermuth,", "tokens": ["Auf", "sei\u00b7nen", "Ue\u00b7ber\u00b7muth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Augen dort, die blauen,", "tokens": ["Die", "Au\u00b7gen", "dort", ",", "die", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sie machen alles gut.", "tokens": ["Sie", "ma\u00b7chen", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Sie sprechen so herzinnig", "tokens": ["Sie", "spre\u00b7chen", "so", "her\u00b7zin\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von s\u00fc\u00dfer 