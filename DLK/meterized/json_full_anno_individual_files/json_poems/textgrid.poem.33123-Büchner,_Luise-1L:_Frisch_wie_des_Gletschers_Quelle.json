: {"text": "Was k\u00fcmmert ihn Turin?", "tokens": ["Was", "k\u00fcm\u00b7mert", "ihn", "Tu\u00b7rin", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Mit jedem neuen Morgen", "tokens": ["Mit", "je\u00b7dem", "neu\u00b7en", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Sieht man zum Schlo\u00df ihn ziehn.", "tokens": ["Sieht", "man", "zum", "Schlo\u00df", "ihn", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Dort unter jenen B\u00e4umen,", "tokens": ["Dort", "un\u00b7ter", "je\u00b7nen", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo er zuerst sie sah,", "tokens": ["Wo", "er", "zu\u00b7erst", "sie", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geht er in wachen Tr\u00e4umen,", "tokens": ["Geht", "er", "in", "wa\u00b7chen", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis sie ihm wieder nah;", "tokens": ["Bis", "sie", "ihm", "wie\u00b7der", "nah", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bis sie lustwandelnd kommen,", "tokens": ["Bis", "sie", "lust\u00b7wan\u00b7delnd", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Frau Bertha mit dem Kind,", "tokens": ["Frau", "Bert\u00b7ha", "mit", "dem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Jolanthe zwar beklommen,", "tokens": ["Jo\u00b7lan\u00b7the", "zwar", "be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Herz doch frohgesinnt.", "tokens": ["Das", "Herz", "doch", "froh\u00b7ge\u00b7sinnt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Zum Seufzen doch und Schmachten", "tokens": ["Zum", "Seuf\u00b7zen", "doch", "und", "Schmach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "L\u00e4\u00dft Bertha ihm nicht Zeit,", "tokens": ["L\u00e4\u00dft", "Bert\u00b7ha", "ihm", "nicht", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "PPER", "PTKNEG", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wollt' er die Lieb' verachten,", "tokens": ["Wollt'", "er", "die", "Lieb'", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchl' er auch jetzt ihr Leid.", "tokens": ["F\u00fchl'", "er", "auch", "jetzt", "ihr", "Leid