{"textgrid.poem.65323": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Motte", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie eine Motte flattert um die Flamme,", "tokens": ["Wie", "ei\u00b7ne", "Mot\u00b7te", "flat\u00b7tert", "um", "die", "Flam\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schwebt auf leisen Schwingen meine Stimme", "tokens": ["So", "schwebt", "auf", "lei\u00b7sen", "Schwin\u00b7gen", "mei\u00b7ne", "Stim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um das erhellte Fenster deiner Kammer.", "tokens": ["Um", "das", "er\u00b7hell\u00b7te", "Fens\u00b7ter", "dei\u00b7ner", "Kam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wird sie die Schwingen sich am Licht verbrennen?", "tokens": ["Wird", "sie", "die", "Schwin\u00b7gen", "sich", "am", "Licht", "ver\u00b7bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wag' es drauf! Das Wagen hilft gewinnen.", "tokens": ["Ich", "wag'", "es", "drauf", "!", "Das", "Wa\u00b7gen", "hilft", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich wag' ein St\u00e4ndchen unter deinem Fenster!", "tokens": ["Ich", "wag'", "ein", "St\u00e4nd\u00b7chen", "un\u00b7ter", "dei\u00b7nem", "Fens\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn du schm\u00e4hlst und rufst wohl gar die Mutter,", "tokens": ["Und", "wenn", "du", "schm\u00e4hlst", "und", "rufst", "wohl", "gar", "die", "Mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So rei\u00df' ich alle Saiten von der Zither,", "tokens": ["So", "rei\u00df'", "ich", "al\u00b7le", "Sai\u00b7ten", "von", "der", "Zi\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und bei\u00df' ein St\u00fcckchen ab von meiner Zunge.", "tokens": ["Und", "bei\u00df'", "ein", "St\u00fcck\u00b7chen", "ab", "von", "mei\u00b7ner", "Zun\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie eine Motte flattert um die Flamme,", "tokens": ["Wie", "ei\u00b7ne", "Mot\u00b7te", "flat\u00b7tert", "um", "die", "Flam\u00b7me", ","], "token_info": ["wo