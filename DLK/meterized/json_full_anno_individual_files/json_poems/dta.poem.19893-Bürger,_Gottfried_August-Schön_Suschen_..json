{"dta.poem.19893": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6n Suschen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n Suschen kant' ich lange Zeit:               ", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "kant'", "ich", "lan\u00b7ge", "Zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VMFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00f6n Suschen war wol fein;", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "war", "wol", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vol Tugend war\u2019s und Sitsamkeit:", "tokens": ["Vol", "Tu\u00b7gend", "wa\u00b7r's", "und", "Sit\u00b7sam\u00b7keit", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "KON", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das sah ich kl\u00e4rlich ein.", "tokens": ["Das", "sah", "ich", "kl\u00e4r\u00b7lich", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kam und ging, ich ging und kam,", "tokens": ["Ich", "kam", "und", "ging", ",", "ich", "ging", "und", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Ebb\u2019 und Flut zur See.", "tokens": ["Wie", "Ebb'", "und", "Flut", "zur", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ganz wol mir that es, wann ich kam,", "tokens": ["Ganz", "wol", "mir", "that", "es", ",", "wann", "ich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch, wann ich ging, nicht weh.", "tokens": ["Doch", ",", "wann", "ich", "ging", ",", "nicht", "weh", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Und es geschah, da\u00df nach der Zeit,", "tokens": ["Und", "es", "ge\u00b7schah", ",", "da\u00df", "nach", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar andres ich vernam:", "tokens": ["Gar", "and\u00b7res", "ich", "ver\u00b7nam", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da thats mir, wann ich schied, so leid,", "tokens": ["Da", "thats", "mir", ",", "wann", "ich", "schied", ",", "so", "leid", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wol mir, wann ich kam:", "tokens": ["So", "wol", "mir", ",", "wann", "ich", "kam", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da hatt\u2019 ich keinen Zeitvertreib,", "tokens": ["Da", "hatt'", "ich", "kei\u00b7nen", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kein Gesch\u00e4ft, als sie:", "tokens": ["Und", "kein", "Ge\u00b7sch\u00e4ft", ",", "als", "sie", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da f\u00fchlt\u2019 ich ganz an Seel\u2019 und Leib,", "tokens": ["Da", "f\u00fchlt'", "ich", "ganz", "an", "Seel'", "und", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00fchlte nichts, als sie.", "tokens": ["Und", "f\u00fchl\u00b7te", "nichts", ",", "als", "sie", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "$,", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich war wol dum, und stum, und taub;", "tokens": ["Ich", "war", "wol", "dum", ",", "und", "stum", ",", "und", "taub", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NE", "$,", "KON", "ADJD", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vernam nichts, ausser ihr;", "tokens": ["Ver\u00b7nam", "nichts", ",", "aus\u00b7ser", "ihr", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "APPR", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sah nirgends bl\u00fchen Blum\u2019 und Laub;", "tokens": ["Sah", "nir\u00b7gends", "bl\u00fc\u00b7hen", "Blum'", "und", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Suschen bl\u00fchte mir.", "tokens": ["Nur", "Su\u00b7schen", "bl\u00fch\u00b7te", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Nicht Sonne, Mond, und Sternenschein,", "tokens": ["Nicht", "Son\u00b7ne", ",", "Mond", ",", "und", "Ster\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "NN", "$,", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir gl\u00e4nzte nur mein Kind;", "tokens": ["Mir", "gl\u00e4nz\u00b7te", "nur", "mein", "Kind", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich sah, wie in die Sonn\u2019, hinein,", "tokens": ["Ich", "sah", ",", "wie", "in", "die", "Sonn'", ",", "hin\u00b7ein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "APPR", "ART", "NN", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und sah mein Auge blind.", "tokens": ["Und", "sah", "mein", "Au\u00b7ge", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und wieder kam gar andre Zeit,", "tokens": ["Und", "wie\u00b7der", "kam", "gar", "and\u00b7re", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gar anders ward es mir:", "tokens": ["Gar", "an\u00b7ders", "ward", "es", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch alle Tugend, Sitsamkeit,", "tokens": ["Doch", "al\u00b7le", "Tu\u00b7gend", ",", "Sit\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Sch\u00f6nheit blieb an ihr.", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "blieb", "an", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich kam und ging, ich ging und kam,", "tokens": ["Ich", "kam", "und", "ging", ",", "ich", "ging", "und", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Ebb\u2019 und Flut zur See.", "tokens": ["Wie", "Ebb'", "und", "Flut", "zur", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ganz wol mir that es, wann ich kam,", "tokens": ["Ganz", "wol", "mir", "that", "es", ",", "wann", "ich", "kam", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch, wann ich ging, nicht weh. \u2014", "tokens": ["Doch", ",", "wann", "ich", "ging", ",", "nicht", "weh", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "VVFIN", "$,", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr Weisen, hoch und tief gelahrt,", "tokens": ["Ihr", "Wei\u00b7sen", ",", "hoch", "und", "tief", "ge\u00b7lahrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr\u2019s ersint, und wist,", "tokens": ["Die", "ih\u00b7r's", "er\u00b7sint", ",", "und", "wist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie, wo und wann sich alles paart?", "tokens": ["Wie", ",", "wo", "und", "wann", "sich", "al\u00b7les", "paart", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "KON", "PWAV", "PRF", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Warum sich\u2019s liebt und k\u00fcst?", "tokens": ["Wa\u00b7rum", "sich's", "liebt", "und", "k\u00fcst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr hohen Weisen, sagt mir\u2019s an!", "tokens": ["Ihr", "ho\u00b7hen", "Wei\u00b7sen", ",", "sagt", "mir's", "an", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "NE", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ergr\u00fcbelt, was mir da,", "tokens": ["Er\u00b7gr\u00fc\u00b7belt", ",", "was", "mir", "da", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ergr\u00fcbelt mir, wo, wie und wann?", "tokens": ["Er\u00b7gr\u00fc\u00b7belt", "mir", ",", "wo", ",", "wie", "und", "wann", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "$,", "PWAV", "KON", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Warum mir so geschah? \u2014", "tokens": ["Wa\u00b7rum", "mir", "so", "ge\u00b7schah", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich selber san wol Nacht und Tag,", "tokens": ["Ich", "sel\u00b7ber", "san", "wol", "Nacht", "und", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wieder Tag und Nacht,", "tokens": ["Und", "wie\u00b7der", "Tag", "und", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wundersamen Dingen nach;", "tokens": ["So", "wun\u00b7der\u00b7sa\u00b7men", "Din\u00b7gen", "nach", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch hab\u2019 ich nichts erdacht. \u2014", "tokens": ["Doch", "hab'", "ich", "nichts", "er\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIS", "VVPP", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Drum, Lieb\u2019 ist wol, wie Wind im Meer:", "tokens": ["Drum", ",", "Lieb'", "ist", "wol", ",", "wie", "Wind", "im", "Meer", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "VAFIN", "ADV", "$,", "PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein Sausen ihr wol h\u00f6rt,", "tokens": ["Sein", "Sau\u00b7sen", "ihr", "wol", "h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allein ihr wisset nicht, woher?", "tokens": ["Al\u00b7lein", "ihr", "wis\u00b7set", "nicht", ",", "wo\u00b7her", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wist nicht, wohin er f\u00e4hrt?", "tokens": ["Wist", "nicht", ",", "wo\u00b7hin", "er", "f\u00e4hrt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}