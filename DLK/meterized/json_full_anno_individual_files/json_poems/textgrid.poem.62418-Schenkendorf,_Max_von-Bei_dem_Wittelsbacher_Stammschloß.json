{"textgrid.poem.62418": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Bei dem Wittelsbacher Stammschlo\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wittelsbacher, Wittelsbacher!", "tokens": ["Wit\u00b7tels\u00b7ba\u00b7cher", ",", "Wit\u00b7tels\u00b7ba\u00b7cher", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaft ihr denn so eisern fest?", "tokens": ["Schlaft", "ihr", "denn", "so", "ei\u00b7sern", "fest", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00e4lt euch, welche keinen l\u00e4\u00dft,", "tokens": ["H\u00e4lt", "euch", ",", "wel\u00b7che", "kei\u00b7nen", "l\u00e4\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PRELS", "PIAT", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Bindet euch die Hand der Rache?", "tokens": ["Bin\u00b7det", "euch", "die", "Hand", "der", "Ra\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Horch' \u2013 es wandeln in den L\u00fcften", "tokens": ["Horch'", "\u2013", "es", "wan\u00b7deln", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$(", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hohes Kriegs- und Siegsgeschrei,", "tokens": ["Ho\u00b7hes", "Kriegs", "und", "Siegs\u00b7ge\u00b7schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "TRUNC", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ritter eure Zeit wird neu,", "tokens": ["Rit\u00b7ter", "eu\u00b7re", "Zeit", "wird", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Regt sich nichts in euren Gr\u00fcften?", "tokens": ["Regt", "sich", "nichts", "in", "eu\u00b7ren", "Gr\u00fcf\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wappnet euch mit allen Schrecken", "tokens": ["Wapp\u00b7net", "euch", "mit", "al\u00b7len", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der geheimen langen Nacht,", "tokens": ["Der", "ge\u00b7hei\u00b7men", "lan\u00b7gen", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kommt in alter schwerer Pracht,", "tokens": ["Kommt", "in", "al\u00b7ter", "schwe\u00b7rer", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Enkel aufzuwecken.", "tokens": ["Eu\u00b7re", "En\u00b7kel", "auf\u00b7zu\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Komm' herauf du bleicher Schatten,", "tokens": ["Komm'", "her\u00b7auf", "du", "blei\u00b7cher", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die langen Qualen trug,", "tokens": ["Der", "die", "lan\u00b7gen", "Qua\u00b7len", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil er seinen Kaiser schlug,", "tokens": ["Weil", "er", "sei\u00b7nen", "Kai\u00b7ser", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Otto, strafe du die Matten.", "tokens": ["Ot\u00b7to", ",", "stra\u00b7fe", "du", "die", "Mat\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Deine Schuld ist abgetragen,", "tokens": ["Dei\u00b7ne", "Schuld", "ist", "ab\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaisermord erscheint ein Tand,", "tokens": ["Kai\u00b7ser\u00b7mord", "er\u00b7scheint", "ein", "Tand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn dem ganzen Vaterland", "tokens": ["Wenn", "dem", "gan\u00b7zen", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche Wunden sind geschlagen.", "tokens": ["Sol\u00b7che", "Wun\u00b7den", "sind", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Bayern-Ludwig, gro\u00dfer Kaiser,", "tokens": ["Bay\u00b7ern\u00b7Lud\u00b7wig", ",", "gro\u00b7\u00dfer", "Kai\u00b7ser", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der so k\u00fchn mit Oestreich rang", "tokens": ["Der", "so", "k\u00fchn", "mit", "O\u00b7e\u00b7streich", "rang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "APPR", "NE", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und den Feind zur Freundschaft zwang,", "tokens": ["Und", "den", "Feind", "zur", "Freund\u00b7schaft", "zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Welk sind deine Lorbeerreiser.", "tokens": ["Welk", "sind", "dei\u00b7ne", "Lor\u00b7beer\u00b7rei\u00b7ser", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Denn dein Bayern hat vergessen,", "tokens": ["Denn", "dein", "Bay\u00b7ern", "hat", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df es mit im F\u00fcrstensaal,", "tokens": ["Da\u00df", "es", "mit", "im", "F\u00fcrs\u00b7ten\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In der W\u00e4hler heil'ger Zahl", "tokens": ["In", "der", "W\u00e4h\u00b7ler", "heil'\u00b7ger", "Zahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst voll hohem Ruhm gesessen.", "tokens": ["Einst", "voll", "ho\u00b7hem", "Ruhm", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Fester treuer Max von Bayern", "tokens": ["Fes\u00b7ter", "treu\u00b7er", "Max", "von", "Bay\u00b7ern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJA", "NE", "APPR", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wieder komm' uns deine Zeit,", "tokens": ["Wie\u00b7der", "komm'", "uns", "dei\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alter Neid und alter Streit,", "tokens": ["Al\u00b7ter", "Neid", "und", "al\u00b7ter", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Willst du nicht dem Frevel steuern?", "tokens": ["Willst", "du", "nicht", "dem", "Fre\u00b7vel", "steu\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Hast zum Kaiser treu gehalten,", "tokens": ["Hast", "zum", "Kai\u00b7ser", "treu", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Starker Arm und weiser Rath,", "tokens": ["Star\u00b7ker", "Arm", "und", "wei\u00b7ser", "Rath", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wieder spro\u00df die Drachensaat,", "tokens": ["Wie\u00b7der", "spro\u00df", "die", "Dra\u00b7chen\u00b7saat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm' dein altes Amt verwalten.", "tokens": ["Komm'", "dein", "al\u00b7tes", "Amt", "ver\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Ich beschw\u00f6r' euch Heldengeister,", "tokens": ["Ich", "be\u00b7schw\u00f6r'", "euch", "Hel\u00b7den\u00b7geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lad' euch in die V\u00f6lkerschlacht, \u2013", "tokens": ["Lad'", "euch", "in", "die", "V\u00f6l\u00b7ker\u00b7schlacht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "NN", "$,", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die deutsche Treu' erwacht,", "tokens": ["Wenn", "die", "deut\u00b7sche", "Treu'", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchlt der ", "tokens": ["F\u00fchlt", "der"], "token_info": ["word", "word"], "pos": ["VVFIN", "ART"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.11": {"line.1": {"text": "Bayerland, o Land der St\u00e4rke,", "tokens": ["Bay\u00b7er\u00b7land", ",", "o", "Land", "der", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles Sch\u00f6nen heil'ger Herd,", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7nen", "heil'\u00b7ger", "Herd", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist wol be\u00dfrer Ehren werth,", "tokens": ["Bist", "wol", "be\u00df\u00b7rer", "Eh\u00b7ren", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Darfst nicht fehlen bei dem Werke.", "tokens": ["Darfst", "nicht", "feh\u00b7len", "bei", "dem", "Wer\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Was wir wollen, was wir schw\u00f6ren,", "tokens": ["Was", "wir", "wol\u00b7len", ",", "was", "wir", "schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Menschenfreude, Gotteslust,", "tokens": ["Men\u00b7schen\u00b7freu\u00b7de