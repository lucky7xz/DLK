{"textgrid.poem.65424": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Rosen und Rosen\u00f6l", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Alle Morgen weht der Wind Rosenbl\u00e4tter von den Zweigen,", "tokens": ["Al\u00b7le", "Mor\u00b7gen", "weht", "der", "Wind", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7ter", "von", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-++-+-+-+-", "measure": "unknown.measure.octa.plus"}, "line.2": {"text": "Und sie schwimmen auf dem See, sich als Lettern dir zu zeigen.", "tokens": ["Und", "sie", "schwim\u00b7men", "auf", "dem", "See", ",", "sich", "als", "Let\u00b7tern", "dir", "zu", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRF", "KOUS", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "--+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Sag', verstehst du ihre Schrift? \u2013 La\u00dft uns unsre Rosen pfl\u00fccken,", "tokens": ["Sag'", ",", "ver\u00b7stehst", "du", "ih\u00b7re", "Schrift", "?", "\u2013", "La\u00dft", "uns", "uns\u00b7re", "Ro\u00b7sen", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$(", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und daraus das Balsam\u00f6l reiner Liebeswonne dr\u00fccken.", "tokens": ["Und", "da\u00b7raus", "