{"dta.poem.14490": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "In der Nacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das Leben drau\u00dfen ist verrauschet,", "tokens": ["Das", "Le\u00b7ben", "drau\u00b7\u00dfen", "ist", "ver\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Lichter l\u00f6schen aus,", "tokens": ["Die", "Lich\u00b7ter", "l\u00f6\u00b7schen", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schauernd mein Herz am Fenster lauschet", "tokens": ["Schau\u00b7ernd", "mein", "Herz", "am", "Fens\u00b7ter", "lau\u00b7schet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PPOSAT", "NN", "APPRART", "NN", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Still in die Nacht hinaus.", "tokens": ["Still", "in", "die", "Nacht", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}