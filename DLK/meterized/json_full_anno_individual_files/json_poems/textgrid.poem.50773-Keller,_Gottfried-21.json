{"textgrid.poem.50773": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "21", "genre": "verse", "period": "N.A.", "pub_year": 1845, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe sie gesehen", "tokens": ["Ich", "ha\u00b7be", "sie", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf Blumen in einem Sarg;", "tokens": ["Auf", "Blu\u00b7men", "in", "ei\u00b7nem", "Sarg", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das bleiche, traute Antlitz", "tokens": ["Das", "blei\u00b7che", ",", "trau\u00b7te", "Ant\u00b7litz"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein wei\u00dfes T\u00fcchlein barg.", "tokens": ["Ein", "wei\u00b7\u00dfes", "T\u00fcch\u00b7lein", "barg", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich hob es in die H\u00f6he", "tokens": ["Ich", "hob", "es", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und legte meine Hand", "tokens": ["Und", "leg\u00b7te", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf ihre dunklen Augen,", "tokens": ["Auf", "ih\u00b7re", "dunk\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ihre kalte Hand!", "tokens": ["Auf", "ih\u00b7re", "kal\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf ihre verschlo\u00dfnen Lippen \u2013", "tokens": ["Auf", "ih\u00b7re", "ver\u00b7schlo\u00df\u00b7nen", "Lip\u00b7pen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Fahr wohl, du bl\u00fchendes Rot! \u2013", "tokens": ["Fahr", "wohl", ",", "du", "bl\u00fc\u00b7hen\u00b7des", "Rot", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "O weh mir, ich mu\u00dfte sagen:", "tokens": ["O", "weh", "mir", ",", "ich", "mu\u00df\u00b7te", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun wahrlich ist sie tot!", "tokens": ["Nun", "wahr\u00b7lich", "ist", "sie", "tot", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da liegt die edle Rose,", "tokens": ["Da", "liegt", "die", "ed\u00b7le", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die einst so purpurn gelacht!", "tokens": ["Die", "einst", "so", "pur\u00b7purn", "ge\u00b7lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es hat ein fremder K\u00fcnstler", "tokens": ["Es", "hat", "ein", "frem\u00b7der", "K\u00fcnst\u00b7ler"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Eine wei\u00dfe aus ihr gemacht.", "tokens": ["Ei\u00b7ne", "wei\u00b7\u00dfe", "aus", "ihr", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.5": {"line.1": {"text": "Da liegt sie so starr und traurig,", "tokens": ["Da", "liegt", "sie", "so", "starr", "und", "trau\u00b7rig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als h\u00e4tte sie nie gelebt;", "tokens": ["Als", "h\u00e4t\u00b7te", "sie", "nie", "ge\u00b7lebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach Gott, es nimmt mich wunder,", "tokens": ["Ach", "Gott", ",", "es", "nimmt", "mich", "wun\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo ihre Seele schwebt!", "tokens": ["Wo", "ih\u00b7re", "See\u00b7le", "schwebt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Kein Laut, kein Hauch, kein Ahnen,", "tokens": ["Kein", "Laut", ",", "kein", "Hauch", ",", "kein", "Ah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kein Fl\u00fcstern um mich her!", "tokens": ["Kein", "Fl\u00fcs\u00b7tern", "um", "mich", "her", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Leib und ich in der Kammer \u2013", "tokens": ["Der", "Leib", "und", "ich", "in", "der", "Kam\u00b7mer", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPER", "APPR", "ART", "NN", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sonst alles still und leer!", "tokens": ["Sonst", "al\u00b7les", "still", "und", "leer", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich habe gespielt mit dem Leben", "tokens": ["Ich", "ha\u00b7be", "ge\u00b7spielt", "mit", "dem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und habe den Tod verlacht,", "tokens": ["Und", "ha\u00b7be", "den", "Tod", "ver\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "