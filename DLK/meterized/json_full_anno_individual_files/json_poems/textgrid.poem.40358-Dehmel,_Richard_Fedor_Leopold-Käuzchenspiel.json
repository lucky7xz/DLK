{"textgrid.poem.40358": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "K\u00e4uzchenspiel", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kinder, kommt, verz\u00e4hlt euch nicht,", "tokens": ["Kin\u00b7der", ",", "kommt", ",", "ver\u00b7z\u00e4hlt", "euch", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jeder hat zehn Zehen;", "tokens": ["Je\u00b7der", "hat", "zehn", "Ze\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "CARD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "wer die letzte Silbe krigt,", "tokens": ["wer", "die", "letz\u00b7te", "Sil\u00b7be", "krigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "der mu\u00df suchen gehen.", "tokens": ["der", "mu\u00df", "su\u00b7chen", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Suche, suche, warte noch,", "tokens": ["Su\u00b7che", ",", "su\u00b7che", ",", "war\u00b7te", "noch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4uzchen schreit im Turmloch,", "tokens": ["K\u00e4uz\u00b7chen", "schreit", "im", "Turm\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", 