{"textgrid.poem.63879": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Warum w\u00fcnscht ein Dilettant, da\u00df die Kinder seiner Laune,", "tokens": ["Wa\u00b7rum", "w\u00fcnscht", "ein", "Di\u00b7let\u00b7tant", ",", "da\u00df", "die", "Kin\u00b7der", "sei\u00b7ner", "Lau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "KOUS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Dran er sein ", "tokens": ["Dran", "er", "sein"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PPER", "PPOSAT"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "H\u00fct' er seine Liebe doch, die ihn selig macht im Stillen,", "tokens": ["H\u00fct'", "er", "sei\u00b7ne", "Lie\u00b7be", "doch", ",", "die", "ihn", "se\u00b7lig", "macht", "im", "Stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADV", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Vor gestrengem Kunstverstand oder sp\u00f6ttischem Geraune.", "tokens": ["Vor", "ge\u00b7stren\u00b7gem", "Kunst\u00b7ver\u00b7stand", "o\u00b7der", "sp\u00f6t\u00b7ti\u00b7schem", "Ge\u00b7rau\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Leider nicht geschaffen ist jede Muse, zu begl\u00fccken", "token