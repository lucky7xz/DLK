{"textgrid.poem.41599": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich wohnte lange Zeit in weiten S\u00e4uleng\u00e4ngen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wohnte lange Zeit in weiten S\u00e4uleng\u00e4ngen,", "tokens": ["Ich", "wohn\u00b7te", "lan\u00b7ge", "Zeit", "in", "wei\u00b7ten", "S\u00e4u\u00b7len\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um die vielf\u00e4ltger Glanz von Meeressonnen weht.", "tokens": ["Um", "die", "viel\u00b7f\u00e4lt\u00b7ger", "Glanz", "von", "Mee\u00b7res\u00b7son\u00b7nen", "weht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit hohen Pfeilern, stolz und voll von Majest\u00e4t,", "tokens": ["Mit", "ho\u00b7hen", "Pfei\u00b7lern", ",", "stolz", "und", "voll", "von", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sahn sie am Abend gleich basaltnen Grottenh\u00e4ngen.", "tokens": ["Sahn", "sie", "am", "A\u00b7bend", "gleich", "ba\u00b7salt\u00b7nen", "Grot\u00b7ten\u00b7h\u00e4n\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Woge, drin das Bild der Himmel kommt und geht,", "tokens": ["Die", "Wo\u00b7ge", ",", "drin", "das", "Bild", "der", "Him\u00b7mel", "kommt", "und", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwob geheimnisreich in feierlichen S\u00e4ngen", "tokens": ["Ver\u00b7wob", "ge\u00b7heim\u00b7nis\u00b7reich", "in", "fei\u00b7er\u00b7li\u00b7chen", "S\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den m\u00e4chtigen Akkord von ihren reinen Kl\u00e4ngen", "tokens": ["Den", "m\u00e4ch\u00b7ti\u00b7gen", "Ak\u00b7kord", "von", "ih\u00b7ren", "rei\u00b7nen", "Kl\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In Abendgluten, die mein spiegelnd Aug ersp\u00e4ht.", "tokens": ["In", "A\u00b7bend\u00b7glu\u00b7ten", ",", "die", "mein", "spie\u00b7gelnd", "Aug", "er\u00b7sp\u00e4ht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dort habe ich gelebt in stiller Wollust L\u00e4cheln,", "tokens": ["Dort", "ha\u00b7be", "ich", "ge\u00b7lebt", "in", "stil\u00b7ler", "Wol\u00b7lust", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": 