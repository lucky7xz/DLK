{"textgrid.poem.40089": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Fehdebrief", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ", "tokens": ["Ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "ich ", "tokens": ["ich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}}, "stanza.2": {"line.1": {"text": "Ich habe eine Welt in meinen Sinnen,", "tokens": ["Ich", "ha\u00b7be", "ei\u00b7ne", "Welt", "in", "mei\u00b7nen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Ihr nicht ahnt mit euern Biedergeistern!", "tokens": ["die", "Ihr", "nicht", "ahnt", "mit", "eu\u00b7ern", "Bie\u00b7der\u00b7geis\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Drum ", "tokens": ["Drum"], "token_info": ["word"], "pos": ["PAV"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "ich f\u00fchl's: ich ", "tokens": ["ich", "f\u00fchl's", ":", "ich"], "token_info": ["word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER"], "meter": "++-", "measure": "unknown.measure.di"}}, "stanza.3": {"line.1": {"text": "Und habt ihr dennoch Recht mit euern Lehren", "tokens": ["Und", "habt", "ihr", "den\u00b7noch", "Recht", "mit", "eu\u00b7ern", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und sollt' ich zu entketten ", "tokens": ["und", "sollt'", "ich", "zu", "ent\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "was in mir st\u00f6hnt und schreit dem Licht entgegen:", "tokens": ["was", "in", "mir", "st\u00f6hnt", "und", "schreit", "dem", "Licht", "ent\u00b7ge\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "so werd' ich ", "tokens": ["so", "werd'", "ich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER"], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Ich lege eher nicht das Schwert von H\u00e4nden,", "tokens": ["Ich", "le\u00b7ge", "e\u00b7her", "nicht", "das", "Schwert", "von", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bis Wunden \u2013 oder Kronen mich ermatten;", "tokens": ["bis", "Wun\u00b7den", "\u2013", "o\u00b7der", "Kro\u00b7nen", "mich", "er\u00b7mat\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und eher nich