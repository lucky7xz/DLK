{"textgrid.poem.37100": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An einen sch\u00f6nen Busen", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dfe, blendende Brust, welche den Einsiedler,", "tokens": ["Wei\u00b7\u00dfe", ",", "blen\u00b7den\u00b7de", "Brust", ",", "wel\u00b7che", "den", "Ein\u00b7sied\u00b7ler", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Den die Klause verschleu\u00dft, mit der verha\u00dften Welt", "tokens": ["Den", "die", "Klau\u00b7se", "ver\u00b7schleu\u00dft", ",", "mit", "der", "ver\u00b7ha\u00df\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Auszus\u00f6hnen verm\u00f6chte,", "tokens": ["Aus\u00b7zu\u00b7s\u00f6h\u00b7nen", "ver\u00b7m\u00f6ch\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Stets berauschest du meinen Blick.", "tokens": ["Stets", "be\u00b7rau\u00b7schest", "du", "mei\u00b7nen", "Blick", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Stets, o Himmel von Reiz, wenn du das Busentuch", "tokens": ["Stets", ",", "o", "Him\u00b7mel", "von", "Reiz", ",", "wenn", "du", "das", "Bu\u00b7sen\u00b7tuch"], "token_info": ["word", "punct", "word", "word", "word"