{"dta.poem.23406": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Tragische Geschichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst ist ein General erwacht,", "tokens": ["J\u00fcngst", "ist", "ein", "Ge\u00b7ne\u00b7ral", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein tapfrer General,", "tokens": ["Ein", "tapf\u00b7rer", "Ge\u00b7ne\u00b7ral", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dem hat ein Traum um Mitternacht", "tokens": ["Dem", "hat", "ein", "Traum", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gemacht viel Angst und Qual.", "tokens": ["Ge\u00b7macht", "viel", "Angst", "und", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er war im Leben noch erschreckt", "tokens": ["Er", "war", "im", "Le\u00b7ben", "noch", "er\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch keinerlei Gefahr,", "tokens": ["Durch", "kei\u00b7ner\u00b7lei", "Ge\u00b7fahr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch hat ein Traum ihn aufgeweckt,", "tokens": ["Doch", "hat", "ein", "Traum", "ihn", "auf\u00b7ge\u00b7weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Traum gar wunderbar.", "tokens": ["Ein", "Traum", "gar", "wun\u00b7der\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was tr\u00e4umte denn dem General", "tokens": ["Was", "tr\u00e4um\u00b7te", "denn", "dem", "Ge\u00b7ne\u00b7ral"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In sp\u00e4ter Mitternacht?", "tokens": ["In", "sp\u00e4\u00b7ter", "Mit\u00b7ter\u00b7nacht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was hat ihm denn so gro\u00dfe Qual", "tokens": ["Was", "hat", "ihm", "denn", "so", "gro\u00b7\u00dfe", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und soviel Angst gemacht?", "tokens": ["Und", "so\u00b7viel", "Angst", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihn der gebebt in keiner Schlacht,", "tokens": ["Ihn", "der", "ge\u00b7bebt", "in", "kei\u00b7ner", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den nichts noch hatt' erschreckt,", "tokens": ["Den", "nichts", "noch", "hatt'", "er\u00b7schreckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was hat ihn denn um Mitternacht", "tokens": ["Was", "hat", "ihn", "denn", "um", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Aus seinem Schlaf geweckt?", "tokens": ["Aus", "sei\u00b7nem", "Schlaf", "ge\u00b7weckt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "War's Krieg und Pest, war's Hungersnoth?", "tokens": ["Wa\u00b7r's", "Krieg", "und", "Pest", ",", "wa\u00b7r's", "Hun\u00b7gers\u00b7noth", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "VAFIN", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "War's H\u00fclf- und Feuerschrei?", "tokens": ["Wa\u00b7r's", "H\u00fcl\u00b7f", "und", "Feu\u00b7er\u00b7schrei", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "TRUNC", "KON", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "War's Hochverrath, und Mord und Tod?", "tokens": ["Wa\u00b7r's", "Hoch\u00b7ver\u00b7rath", ",", "und", "Mord", "und", "Tod", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "War's blut'ge Meuterei?", "tokens": ["Wa\u00b7r's", "blut'\u00b7ge", "Meu\u00b7te\u00b7rei", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Ihm tr\u00e4umte \u2014 nun, es war enorm! \u2014", "tokens": ["Ihm", "tr\u00e4um\u00b7te", "nun", ",", "es", "war", "en\u00b7orm", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "$,", "PPER", "VAFIN", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Da\u00df durch das ganze Heer", "tokens": ["Da\u00df", "durch", "das", "gan\u00b7ze", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erhielte jede Uniform", "tokens": ["Er\u00b7hiel\u00b7te", "je\u00b7de", "U\u00b7ni\u00b7form"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hinfort zwei Kn\u00f6pfe mehr.", "tokens": ["Hin\u00b7fort", "zwei", "Kn\u00f6p\u00b7fe", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}