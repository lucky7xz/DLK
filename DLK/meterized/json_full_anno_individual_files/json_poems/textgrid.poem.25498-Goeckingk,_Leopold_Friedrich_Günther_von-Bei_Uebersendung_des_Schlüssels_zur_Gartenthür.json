{"textgrid.poem.25498": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "Bei Uebersendung des Schl\u00fcssels zur Gartenth\u00fcr", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Soll ich dich in den Brunnen werfen?", "tokens": ["Soll", "ich", "dich", "in", "den", "Brun\u00b7nen", "wer\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Schick' ich dich hin zu Amarant?", "tokens": ["Schick'", "ich", "dich", "hin", "zu", "A\u00b7ma\u00b7rant", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sollt' ich vielleicht das Schwert zu meinem Tode sch\u00e4rfen?", "tokens": ["Sollt'", "ich", "viel\u00b7leicht", "das", "Schwert", "zu", "mei\u00b7nem", "To\u00b7de", "sch\u00e4r\u00b7fen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Selbst geben in des M\u00f6rders Hand?", "tokens": ["Selbst", "ge\u00b7ben", "in", "des", "M\u00f6r\u00b7ders", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was soll ich thun? Vernunft, du prahlest immer", "tokens": ["Was", "soll", "ich", "thun", "?", "Ver\u00b7nunft", ",", "du", "prah\u00b7lest", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "NN", "$,", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mir deine weisen Lehren vor,", "tokens": ["Mir", "dei\u00b7ne", "wei\u00b7sen", "Leh\u00b7ren", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch lauter steiget noch der Liebe sanft Gewimmer", "tokens": ["Doch", "lau\u00b7ter", "stei\u00b7get", "noch", "der", "Lie\u00b7be", "sanft", "Ge\u00b7wim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "ART", "NN", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Aus der beklemmten Brust empor.", "tokens": ["Aus", "der", "be\u00b7klemm\u00b7ten", "Brust", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wohlan es sey! Zwar k\u00f6nnt' ich widerstehen,", "tokens": ["Wo\u00b7hlan", "es", "sey", "!", "Zwar", "k\u00f6nnt'", "ich", "wi\u00b7der\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "$.", "ADV", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Weil dieses Herz mir das verspricht:", "tokens": ["Weil", "die\u00b7ses", "Herz", "mir", "das", "ver\u00b7spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "PPER", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Doch Amarant, in dir, in dir den M\u00f6rder sehen,", "tokens": ["Doch", "A\u00b7ma\u00b7rant", ",", "in", "dir", ",", "in", "dir", "den", "M\u00f6r\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "APPR", "PPER", "$,", "APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das will ich und das kann ich nicht.", "tokens": ["Das", "will", "ich", "und", "das", "kann", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "KON", "PDS", "VMFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da nimm ihn hin! Komm, wenn die kleine Glocke", "tokens": ["Da", "nimm", "ihn", "hin", "!", "Komm", ",", "wenn", "die", "klei\u00b7ne", "Glo\u00b7cke"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$.", "VVFIN", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Die Nonnen zu der Hora weckt,", "tokens": ["Die", "Non\u00b7nen", "zu", "der", "Ho\u00b7ra", "weckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Verh\u00fclle dich besorgt in deinem Ueberrocke,", "tokens": ["Ver\u00b7h\u00fcl\u00b7le", "dich", "be\u00b7sorgt", "in", "dei\u00b7nem", "Ue\u00b7berr\u00b7o\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und geh, von deinem Muth' bedeckt.", "tokens": ["Und", "geh", ",", "von", "dei\u00b7nem", "Muth'", "be\u00b7deckt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Schon an der Th\u00fcr' sollst du den Busen h\u00f6ren,", "tokens": ["Schon", "an", "der", "Th\u00fcr'", "sollst", "du", "den", "Bu\u00b7sen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Der wie ein Eisenhammer pocht;", "tokens": ["Der", "wie", "ein", "Ei\u00b7sen\u00b7ham\u00b7mer", "pocht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Sollst f\u00fchlen, wie das Blut in allen Herzensr\u00f6hren", "tokens": ["Sollst", "f\u00fch\u00b7len", ",", "wie", "das", "Blut", "in", "al\u00b7len", "Her\u00b7zens\u00b7r\u00f6h\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "PWAV", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Beim Feuer deiner K\u00fcsse kocht.", "tokens": ["Beim", "Feu\u00b7er", "dei\u00b7ner", "K\u00fcs\u00b7se", "kocht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Was willst du mehr? Schon das sollt' ich nicht geben;", "tokens": ["Was", "willst", "du", "mehr", "?", "Schon", "das", "sollt'", "ich", "nicht", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct", "wo