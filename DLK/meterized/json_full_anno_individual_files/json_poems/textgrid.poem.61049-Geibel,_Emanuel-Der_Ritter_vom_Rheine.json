", "mild", ",", "wie", "ist", "er", "so", "mild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er naht, und die Augen der G\u00e4ste ergl\u00fchn,", "tokens": ["Er", "naht", ",", "und", "die", "Au\u00b7gen", "der", "G\u00e4s\u00b7te", "er\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und der S\u00e4nger greift in die Harfe k\u00fchn,", "tokens": ["Und", "der", "S\u00e4n\u00b7ger", "greift", "in", "die", "Har\u00b7fe", "k\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und selbst die M\u00e4dchen im Kreise,", "tokens": ["Und", "selbst", "die", "M\u00e4d\u00b7chen", "im", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Sie k\u00fcssen ihn heimlicherweise.", "tokens": ["Sie", "k\u00fcs\u00b7sen", "ihn", "heim\u00b7li\u00b7cher\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "O komm, du Blume der Ritterschaft,", "tokens": ["O", "komm", ",", "du", "Blu\u00b7me", "der", "Rit\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "punct", "wor