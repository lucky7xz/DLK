{"textgrid.poem.61049": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Der Ritter vom Rheine", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df einen Helden von seltener Art,", "tokens": ["Ich", "wei\u00df", "ei\u00b7nen", "Hel\u00b7den", "von", "sel\u00b7te\u00b7ner", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "So stark und so zart, so stark und so zart;", "tokens": ["So", "stark", "und", "so", "zart", ",", "so", "stark", "und", "so", "zart", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das ist die Blume der Ritterschaft,", "tokens": ["Das", "ist", "die", "Blu\u00b7me", "der", "Rit\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das ist der erste an Milde und Kraft,", "tokens": ["Das", "ist", "der", "ers\u00b7te", "an", "Mil\u00b7de", "und", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So weit auf des Vaterlands Gauen", "tokens": ["So", "weit", "auf", "des", "Va\u00b7ter\u00b7lands", "Gau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Sterne vom Himmel schauen.", "tokens": ["Die", "Ster\u00b7ne", "vom", "Him\u00b7mel", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Er kam zur Welt auf sonnigem Stein", "tokens": ["Er", "kam", "zur", "Welt", "auf", "son\u00b7ni\u00b7gem", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Hoch \u00fcber dem Rhein, hoch \u00fcber dem Rhein;", "tokens": ["Hoch", "\u00fc\u00b7ber", "dem", "Rhein", ",", "hoch", "\u00fc\u00b7ber", "dem", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NE", "$,", "ADJD", "APPR", "ART", "NE", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und wie er geboren, da jauchzt' \u00fcberall", "tokens": ["Und", "wie", "er", "ge\u00b7bo\u00b7ren", ",", "da", "jauchzt'", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "VVPP", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Lande Trompeten- und Paukenschall,", "tokens": ["Im", "Lan\u00b7de", "Trom\u00b7pe\u00b7ten", "und", "Pau\u00b7ken\u00b7schall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "TRUNC", "KON", "NN", "$,"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da wehten von Burgen und H\u00fcgeln", "tokens": ["Da", "weh\u00b7ten", "von", "Bur\u00b7gen", "und", "H\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die Fahnen mit lustigen Fl\u00fcgeln.", "tokens": ["Die", "Fah\u00b7nen", "mit", "lus\u00b7ti\u00b7gen", "Fl\u00fc\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "In goldener R\u00fcstung geht der Gesell,", "tokens": ["In", "gol\u00b7de\u00b7ner", "R\u00fcs\u00b7tung", "geht", "der", "Ge\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das funkelt so hell, das funkelt so hell!", "tokens": ["Das", "fun\u00b7kelt", "so", "hell", ",", "das", "fun\u00b7kelt", "so", "hell", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$,", "PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und ob ihm auch mancher zum Kampf sich gestellt,", "tokens": ["Und", "ob", "ihm", "auch", "man\u00b7cher", "zum", "Kampf", "sich", "ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIS", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Wei\u00df keinen, den er nicht endlich gef\u00e4llt;", "tokens": ["Wei\u00df", "kei\u00b7nen", ",", "den", "er", "nicht", "end\u00b7lich", "ge\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "$,", "PRELS", "PPER", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Es sanken F\u00fcrsten und Pfaffen", "tokens": ["Es", "san\u00b7ken", "F\u00fcrs\u00b7ten", "und", "Pfaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Vor seinen feurigen Waffen.", "tokens": ["Vor", "sei\u00b7nen", "feu\u00b7ri\u00b7gen", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch wo es ein Fest zu verherrlichen gilt,", "tokens": ["Doch", "wo", "es", "ein", "Fest", "zu", "ver\u00b7herr\u00b7li\u00b7chen", "gilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wie ist er so mild, wie ist er so mild!", "tokens": ["Wie", "ist", "er", "so", "mild", ",", "wie", "ist", "er", "so", "mild", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Er naht, und die Augen der G\u00e4ste ergl\u00fchn,", "tokens": ["Er", "naht", ",", "und", "die", "Au\u00b7gen", "der", "G\u00e4s\u00b7te", "er\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und der S\u00e4nger