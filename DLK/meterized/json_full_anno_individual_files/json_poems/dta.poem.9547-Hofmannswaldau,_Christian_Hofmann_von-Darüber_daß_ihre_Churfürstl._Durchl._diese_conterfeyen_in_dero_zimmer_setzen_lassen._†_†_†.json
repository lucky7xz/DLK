{"dta.poem.9547": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Dar\u00fcber/ da\u00df ihre Churf\u00fcrstl. Durchl. diese  \n conterfeyen in dero zimmer setzen lassen.  \n \u2020 \u2020 \u2020", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr sch\u00f6nen aus dem Engellande", "tokens": ["Ihr", "sch\u00f6\u00b7nen", "aus", "dem", "En\u00b7gel\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Seyd wohl die gl\u00fccklichsten der welt/", "tokens": ["Seyd", "wohl", "die", "gl\u00fcck\u00b7lichs\u00b7ten", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADV", "ART", "ADJA", "ART", "NN", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Da\u00df man euch/ \u00fcber eurem stande/", "tokens": ["Da\u00df", "man", "euch", "/", "\u00fc\u00b7ber", "eu\u00b7rem", "stan\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "$(", "APPR", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In unser F\u00fcrstin zimmer stellt!", "tokens": ["In", "un\u00b7ser", "F\u00fcrs\u00b7tin", "zim\u00b7mer", "stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch ist es auch bey eurem gl\u00fccke", "tokens": ["Doch", "ist", "es", "auch", "bey", "eu\u00b7rem", "gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Um euren gantzen ruhm geschehn/", "tokens": ["Um", "eu\u00b7ren", "gant\u00b7zen", "ruhm", "ge\u00b7schehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo bleiben eure sch\u00f6nen blicke/", "tokens": ["Wo", "blei\u00b7ben", "eu\u00b7re", "sch\u00f6\u00b7nen", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sehn wir Charlotten bey euch stehn?", "tokens": ["Sehn", "wir", "Char\u00b7lot\u00b7ten", "bey", "euch", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}