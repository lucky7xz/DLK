{"dta.poem.2152": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das vern\u00fcnftige Werkzeug.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich den Menschen, nach des K\u00f6rpers und seines", "tokens": ["Wenn", "ich", "den", "Men\u00b7schen", ",", "nach", "des", "K\u00f6r\u00b7pers", "und", "sei\u00b7nes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "APPR", "ART", "NN", "KON", "PPOSAT"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Geistes Kraft, betrachte,", "tokens": ["Geis\u00b7tes", "Kraft", ",", "be\u00b7trach\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und, bey demselbigen, die Absicht, de\u00df, welcher ihn ge-", "tokens": ["Und", ",", "bey", "dem\u00b7sel\u00b7bi\u00b7gen", ",", "die", "Ab\u00b7sicht", ",", "de\u00df", ",", "wel\u00b7cher", "ihn", "ge"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "VVINF", "$,", "ART", "NN", "$,", "ART", "$,", "PRELS", "PPER", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "macht, beachte;", "tokens": ["macht", ",", "be\u00b7ach\u00b7te", ";"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "So f\u00e4llt, bey den Betrachtungen, mir diese Frage billig", "tokens": ["So", "f\u00e4llt", ",", "bey", "den", "Be\u00b7trach\u00b7tun\u00b7gen", ",", "mir", "die\u00b7se", "Fra\u00b7ge", "bil\u00b7lig"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "ART", "NN", "$,", "PPER", "PDAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "ein:", "tokens": ["ein", ":"], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Sollt etwan nicht der Mensch von Gott auch dazu mit", "tokens": ["Sollt", "et\u00b7wan", "nicht", "der", "Mensch", "von", "Gott", "auch", "da\u00b7zu", "mit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PTKNEG", "ART", "NN", "APPR", "NN", "ADV", "PAV", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "erschaffen seyn,", "tokens": ["er\u00b7schaf\u00b7fen", "seyn", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Da\u00df er der Erden Fl\u00e4che ziere,", "tokens": ["Da\u00df", "er", "der", "Er\u00b7den", "Fl\u00e4\u00b7che", "zie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ver\u00e4nderung und Ordnung mache, die Kreatur dazu re-", "tokens": ["Ver\u00b7\u00e4n\u00b7de\u00b7rung", "und", "Ord\u00b7nung", "ma\u00b7che", ",", "die", "Kre\u00b7a\u00b7tur", "da\u00b7zu", "re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN", "$,", "ART", "NN", "PAV", "TRUNC"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.11": {"text": "giere,", "tokens": ["gie\u00b7re", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Da\u00df auf dem \u00e4u\u00dfern Kreis der Erde", "tokens": ["Da\u00df", "auf", "dem", "\u00e4u\u00b7\u00dfern", "Kreis", "der", "Er\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Das Sch\u00f6ne nicht allein erhalten, nein, alles noch versch\u00f6-", "tokens": ["Das", "Sch\u00f6\u00b7ne", "nicht", "al\u00b7lein", "er\u00b7hal\u00b7ten", ",", "nein", ",", "al\u00b7les", "noch", "ver\u00b7sch\u00f6"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "ADV", "VVPP", "$,", "PTKANT", "$,", "PIS", "ADV", "TRUNC"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "nert werde,", "tokens": ["nert", "wer\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Und da\u00df er gleichsam der Natur zu einem Werkzeug die-", "tokens": ["Und", "da\u00df", "er", "gleich\u00b7sam", "der", "Na\u00b7tur", "zu", "ei\u00b7nem", "Werk\u00b7zeug", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADJD", "ART", "NN", "APPR", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "nen solle,", "tokens": ["nen", "sol\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VMFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Das ihr in ihren Wirkungen und ihre Ordnung auszu-", "tokens": ["Das", "ihr", "in", "ih\u00b7ren", "Wir\u00b7kun\u00b7gen", "und", "ih\u00b7re", "Ord\u00b7nung", "aus\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "richten", "tokens": ["rich\u00b7ten"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Beh\u00fclflich sey, und zugerichtet, da\u00df er ihr helfen k\u00f6nn\u2019", "tokens": ["Be\u00b7h\u00fcl\u00b7flich", "sey", ",", "und", "zu\u00b7ge\u00b7rich\u00b7tet", ",", "da\u00df", "er", "ihr", "hel\u00b7fen", "k\u00f6nn'"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$,", "KON", "VVPP", "$,", "KOUS", "PPER", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-++-+-+", "measure": "unknown.measure.octa.plus"}, "line.20": {"text": "und wolle?", "tokens": ["und", "wol\u00b7le", "?"], "token_info": ["word", "word", "punct"], "pos": ["KON", "VMFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.21": {"text": "Doch scheint der Mensch ein solches Werkzeug, das, in", "tokens": ["Doch", "scheint", "der", "Mensch", "ein", "sol\u00b7ches", "Werk\u00b7zeug", ",", "das", ",", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "PIAT", "NN", "$,", "PDS", "$,", "APPR"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "Vollbringung seiner Pflichten,", "tokens": ["Voll\u00b7brin\u00b7gung", "sei\u00b7ner", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ein eigenes Vergn\u00fcgen f\u00fchlen, den Sch\u00f6pfer kennen,", "tokens": ["Ein", "ei\u00b7ge\u00b7nes", "Ver\u00b7gn\u00fc\u00b7gen", "f\u00fch\u00b7len", ",", "den", "Sch\u00f6p\u00b7fer", "ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+---+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.24": {"text": "ihn verehren,", "tokens": ["ihn", "ver\u00b7eh\u00b7ren", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.25": {"text": "Jhn lieben und ihn loben kann.", "tokens": ["Jhn", "lie\u00b7ben", "und", "ihn", "lo\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Ach m\u00f6chte denn doch jedermann", "tokens": ["Ach", "m\u00f6ch\u00b7te", "denn", "doch", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADV", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die Pflicht verrichten, sich vergn\u00fcgen, und Gottes Herr-", "tokens": ["Die", "Pflicht", "ver\u00b7rich\u00b7ten", ",", "sich", "ver\u00b7gn\u00fc\u00b7gen", ",", "und", "Got\u00b7tes", "Herr"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "PRF", "VVPP", "$,", "KON", "NN", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "lichkeit vermehren!", "tokens": ["lich\u00b7keit", "ver\u00b7meh\u00b7ren", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}