{"dta.poem.14285": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Nachkl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Schon kehren die V\u00f6gel wieder ein,", "tokens": ["Schon", "keh\u00b7ren", "die", "V\u00f6\u00b7gel", "wie\u00b7der", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es schallen die alten Lieder,", "tokens": ["Es", "schal\u00b7len", "die", "al\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach, die fr\u00f6hliche Jugend mein", "tokens": ["Ach", ",", "die", "fr\u00f6h\u00b7li\u00b7che", "Ju\u00b7gend", "mein"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "PPOSAT"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Kommt sie wohl auch noch wieder? \u2014", "tokens": ["Kommt", "sie", "wohl", "auch", "noch", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}