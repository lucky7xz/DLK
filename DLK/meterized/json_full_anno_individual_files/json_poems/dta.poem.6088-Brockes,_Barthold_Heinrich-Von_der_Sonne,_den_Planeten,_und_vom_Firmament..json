{"dta.poem.6088": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In ihnen ist der Sonnen Weg zu sehen.", "tokens": ["In", "ih\u00b7nen", "ist", "der", "Son\u00b7nen", "Weg", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So gar ein Phaeton, gest\u00fcrtzt", "tokens": ["So", "gar", "ein", "Phae\u00b7ton", ",", "ge\u00b7st\u00fcrtzt"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ART", "NN", "$,", "VVPP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Durch th\u00f6riehte Vermessenheit,", "tokens": ["Durch", "th\u00f6\u00b7rieh\u00b7te", "Ver\u00b7mes\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "H\u00e4tt\u2019 in dem hellen Gang gantz sicher k\u00f6nnen gehen,", "tokens": ["H\u00e4tt'", "in", "dem", "hel\u00b7len", "Gang", "gantz", "si\u00b7cher", "k\u00f6n\u00b7nen", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bey der von Phoebus ihm gezeigten Deutlichkeit.", "tokens": ["Bey", "der", "von", "Phoe\u00b7bus", "ihm", "ge\u00b7zeig\u00b7ten", "Deut\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "APPR", "NE", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}