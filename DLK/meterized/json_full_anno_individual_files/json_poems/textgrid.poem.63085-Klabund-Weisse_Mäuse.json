{"textgrid.poem.63085": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Weisse M\u00e4use", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er kaufte auf dem Jahrmarkt sich zwei wei\u00dfe M\u00e4use", "tokens": ["Er", "kauf\u00b7te", "auf", "dem", "Jahr\u00b7markt", "sich", "zwei", "wei\u00b7\u00dfe", "M\u00e4u\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PRF", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und tat sie in ein gl\u00e4sernes Geh\u00e4use.", "tokens": ["Und", "tat", "sie", "in", "ein", "gl\u00e4\u00b7ser\u00b7nes", "Ge\u00b7h\u00e4u\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nun machen sie M\u00e4nnchen, lecken ihre Pfoten", "tokens": ["Nun", "ma\u00b7chen", "sie", "M\u00e4nn\u00b7chen", ",", "le\u00b7cken", "ih\u00b7re", "Pfo\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und sehen dich mit ihren roten", "tokens": ["Und", "se\u00b7hen", "dich", "mit", "ih\u00b7ren", "ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00c4uglein ein wenig melancholisch an", "tokens": ["\u00c4ug\u00b7lein", "ein", "we\u00b7nig", "me\u00b7lan\u00b7cho\u00b7lisch", "an"], "token_info": ["word", "word", "word", 