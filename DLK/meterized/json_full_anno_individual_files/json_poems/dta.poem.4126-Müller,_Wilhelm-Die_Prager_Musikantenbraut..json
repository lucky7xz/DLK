{"dta.poem.4126": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Prager Musikantenbraut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1821", "urn": "urn:nbn:de:kobv:b4-200905194209", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Prager Musikant,", "tokens": ["Ein", "Pra\u00b7ger", "Mu\u00b7si\u00b7kant", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Musikant von feiner Kunst", "tokens": ["Ein", "Mu\u00b7si\u00b7kant", "von", "fei\u00b7ner", "Kunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Ba\u00df und in Diskant.", "tokens": ["In", "Ba\u00df", "und", "in", "Dis\u00b7kant", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Und wi\u00dft ihr, wo mein Sch\u00e4tzel ist,", "tokens": ["Und", "wi\u00dft", "ihr", ",", "wo", "mein", "Sch\u00e4t\u00b7zel", "ist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So wi\u00dft ihr mehr als ich,", "tokens": ["So", "wi\u00dft", "ihr", "mehr", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn weil er halt nicht schreiben kann,", "tokens": ["Denn", "weil", "er", "halt", "nicht", "schrei\u00b7ben", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So denkt er nur an mich.", "tokens": ["So", "denkt", "er", "nur", "an", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und's Denken ist ein lustig Ding,", "tokens": ["Un\u00b7d's", "Den\u00b7ken", "ist", "ein", "lus\u00b7tig", "Ding", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Summt leis' in's Herz hinein;", "tokens": ["Summt", "leis'", "in's", "Herz", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Woher es kommt, wohin es geht,", "tokens": ["Wo\u00b7her", "es", "kommt", ",", "wo\u00b7hin", "es", "geht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das mu\u00df errathen sein.", "tokens": ["Das", "mu\u00df", "er\u00b7ra\u00b7then", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "VVINF", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ei, kommst denn nimmermehr zu Ruh',", "tokens": ["Ei", ",", "kommst", "denn", "nim\u00b7mer\u00b7mehr", "zu", "Ruh'", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du Musikantenblut?", "tokens": ["Du", "Mu\u00b7si\u00b7kan\u00b7ten\u00b7blut", "?"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ei, lernst denn nimmermehr verstehn,", "tokens": ["Ei", ",", "lernst", "denn", "nim\u00b7mer\u00b7mehr", "ver\u00b7stehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie lieb's in B\u00f6hmen thut?", "tokens": ["Wie", "lie\u00b7b's", "in", "B\u00f6h\u00b7men", "thut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NE", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "So zieh nur hin durch Stadt und Land,", "tokens": ["So", "zieh", "nur", "hin", "durch", "Stadt", "und", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dir Sankt Nepomuk,", "tokens": ["Mit", "dir", "Sankt", "Ne\u00b7po\u00b7muk", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der segne Fiedel dir und Ba\u00df", "tokens": ["Der", "seg\u00b7ne", "Fie\u00b7del", "dir", "und", "Ba\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit gutem Strich und Druck!", "tokens": ["Mit", "gu\u00b7tem", "Strich", "und", "Druck", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und wo in Gottes weiter Welt", "tokens": ["Und", "wo", "in", "Got\u00b7tes", "wei\u00b7ter", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "APPR", "NN", "ADV", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du klopfst an Th\u00fcr und Thor,", "tokens": ["Du", "klopfst", "an", "Th\u00fcr", "und", "Thor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Find' offne Beutel \u00fcberall", "tokens": ["Find'", "off\u00b7ne", "Beu\u00b7tel", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und ein geneigtes Ohr.", "tokens": ["Und", "ein", "ge\u00b7neig\u00b7tes", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die M\u00e4del schaun dir in's Gesicht,", "tokens": ["Die", "M\u00e4\u00b7del", "schaun", "dir", "in's", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4nner nach der Hand,", "tokens": ["Die", "M\u00e4n\u00b7ner", "nach", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Einer und die Andre spricht:", "tokens": ["Und", "Ei\u00b7ner", "und", "die", "And\u00b7re", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KON", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein braver Musikant!", "tokens": ["Ein", "bra\u00b7ver", "Mu\u00b7si\u00b7kant", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dann sing' ein Lied von deiner Braut,", "tokens": ["Dann", "sing'", "ein", "Lied", "von", "dei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die an der Moldau ist:", "tokens": ["Die", "an", "der", "Mol\u00b7dau", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das klingt mir hell durch Mark und Bein,", "tokens": ["Das", "klingt", "mir", "hell", "durch", "Mark", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sagt mir, wo du bist.", "tokens": ["Und", "sagt", "mir", ",", "wo", "du", "bist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und sagt mir noch so mancherlei,", "tokens": ["Und", "sagt", "mir", "noch", "so", "man\u00b7cher\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was schwer sich sagt im Reim,", "tokens": ["Was", "schwer", "sich", "sagt", "im", "Reim", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sagt mir: Wann die Lerche kommt,", "tokens": ["Und", "sagt", "mir", ":", "Wann", "die", "Ler\u00b7che", "kommt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kehr' ich nach B\u00f6hmen heim.", "tokens": ["Kehr'", "ich", "nach", "B\u00f6h\u00b7men", "heim", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NE", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}