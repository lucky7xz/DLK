{"dta.poem.23404": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "H\u00e4utiges.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr habt gehoffet und vertraut:", "tokens": ["Ihr", "habt", "ge\u00b7hof\u00b7fet", "und", "ver\u00b7traut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Wechsel sprie\u00dft ein Heil empor!", "tokens": ["Im", "Wech\u00b7sel", "sprie\u00dft", "ein", "Heil", "em\u00b7por", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr habt den Wechsel nun geschaut,", "tokens": ["Ihr", "habt", "den", "Wech\u00b7sel", "nun", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sagt an, was spro\u00df daraus hervor?", "tokens": ["Sagt", "an", ",", "was", "spro\u00df", "da\u00b7raus", "her\u00b7vor", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PWS", "VVFIN", "PAV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Personen wechseln Jahr f\u00fcr Jahr,", "tokens": ["Per\u00b7so\u00b7nen", "wech\u00b7seln", "Jahr", "f\u00fcr", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Wie ihr's in jedem Staate seht;", "tokens": ["Wie", "ih\u00b7r's", "in", "je\u00b7dem", "Staa\u00b7te", "seht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Er selber bleibt unwandelbar,", "tokens": ["Er", "sel\u00b7ber", "bleibt", "un\u00b7wan\u00b7del\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So lange sein Prinzip besteht.", "tokens": ["So", "lan\u00b7ge", "sein", "Prin\u00b7zip", "be\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wer auf das Drum und Dran nur baut,", "tokens": ["Wer", "auf", "das", "Drum", "und", "Dran", "nur", "baut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "KON", "PAV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist f\u00fcrwahr ein rechter Thor:", "tokens": ["Der", "ist", "f\u00fcr\u00b7wahr", "ein", "rech\u00b7ter", "Thor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schlange wechselt ihre Haut", "tokens": ["Die", "Schlan\u00b7ge", "wech\u00b7selt", "ih\u00b7re", "Haut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bleibet Schlange nach wie vor.", "tokens": ["Und", "blei\u00b7bet", "Schlan\u00b7ge", "nach", "wie", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "KOKOM", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}