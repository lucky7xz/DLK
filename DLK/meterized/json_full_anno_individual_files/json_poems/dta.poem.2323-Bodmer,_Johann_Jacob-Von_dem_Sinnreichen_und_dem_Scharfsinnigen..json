{"dta.poem.2323": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von dem  \n  Sinnreichen  \n und dem  \n  Scharfsinnigen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198534", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Geist der Ehre sucht, mu\u00df etwas weiter ziehn,", "tokens": ["Ein", "Geist", "der", "Eh\u00b7re", "sucht", ",", "mu\u00df", "et\u00b7was", "wei\u00b7ter", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn wo der Gr\u00e4ntzstein ligt. Drum bist du ausgerissen", "tokens": ["Denn", "wo", "der", "Gr\u00e4ntz\u00b7stein", "ligt", ".", "Drum", "bist", "du", "aus\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$.", "PAV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als wie ein junger Leu, im Fall er an den F\u00fcssen", "tokens": ["Als", "wie", "ein", "jun\u00b7ger", "Leu", ",", "im", "Fall", "er", "an", "den", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "ADJA", "NN", "$,", "APPRART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Klauen wachsen sieht, und um den Hal\u00df die M\u00e4hn,", "tokens": ["Die", "Klau\u00b7en", "wach\u00b7sen", "sieht", ",", "und", "um", "den", "Hal\u00df", "die", "M\u00e4hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Z\u00e4hn im Maule merckt; er will nun ferner gehn", "tokens": ["Die", "Z\u00e4hn", "im", "Mau\u00b7le", "merckt", ";", "er", "will", "nun", "fer\u00b7ner", "gehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$.", "PPER", "VMFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus seiner H\u00f6len Loch, in der er ist erzogen:", "tokens": ["Aus", "sei\u00b7ner", "H\u00f6\u00b7len", "Loch", ",", "in", "der", "er", "ist", "er\u00b7zo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,", "APPR", "PRELS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und wie ein Adler thut, der nicht l\u00e4\u00dft ungeflogen,", "tokens": ["Und", "wie", "ein", "Ad\u00b7ler", "thut", ",", "der", "nicht", "l\u00e4\u00dft", "un\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,", "PRELS", "PTKNEG", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wiewol er k\u00fcmmerlich erst jetzt hat ausgekielt,", "tokens": ["Wie\u00b7wol", "er", "k\u00fcm\u00b7mer\u00b7lich", "erst", "jetzt", "hat", "aus\u00b7ge\u00b7kielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und noch der Nordwind nicht mit seinen Federn spielt.", "tokens": ["Und", "noch", "der", "Nord\u00b7wind", "nicht", "mit", "sei\u00b7nen", "Fe\u00b7dern", "spielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Er macht sich in die Luft, und schwingt mit freyem Z\u00fcgel", "tokens": ["Er", "macht", "sich", "in", "die", "Luft", ",", "und", "schwingt", "mit", "frey\u00b7em", "Z\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Bis zum Gew\u00f6lcke hin die wenig starcken Fl\u00fcgel;", "tokens": ["Bis", "zum", "Ge\u00b7w\u00f6l\u00b7cke", "hin", "die", "we\u00b7nig", "star\u00b7cken", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "ADV", "ART", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Alsbald er etwas dann erblickt in einer Bach,", "tokens": ["Als\u00b7bald", "er", "et\u00b7was", "dann", "er\u00b7blickt", "in", "ei\u00b7ner", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So st\u00fcrtzet er herab, und setzt den Enten nach,", "tokens": ["So", "st\u00fcrt\u00b7zet", "er", "her\u00b7ab", ",", "und", "setzt", "den", "En\u00b7ten", "nach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die grossen Schreckens voll sich f\u00fcr ihm untertauchen.", "tokens": ["Die", "gros\u00b7sen", "Schre\u00b7ckens", "voll", "sich", "f\u00fcr", "ihm", "un\u00b7ter\u00b7tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PRF", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}