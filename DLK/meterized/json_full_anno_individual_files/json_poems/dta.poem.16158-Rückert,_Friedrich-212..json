{"dta.poem.16158": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "212.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kranke, wenn er klagt um bittern Schmack im Munde,", "tokens": ["Der", "Kran\u00b7ke", ",", "wenn", "er", "klagt", "um", "bit\u00b7tern", "Schmack", "im", "Mun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht s\u00fc\u00dfe Arzenei gibt ihm der Arzt zur Stunde;", "tokens": ["Nicht", "s\u00fc\u00b7\u00dfe", "Ar\u00b7ze\u00b7nei", "gibt", "ihm", "der", "Arzt", "zur", "Stun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er gibt ihm bittre, nicht damit ihm bitter bleibe", "tokens": ["Er", "gibt", "ihm", "bitt\u00b7re", ",", "nicht", "da\u00b7mit", "ihm", "bit\u00b7ter", "blei\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "$,", "PTKNEG", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mund, nein, Bitterkeit die Bitterkeit vertreibe.", "tokens": ["Der", "Mund", ",", "nein", ",", "Bit\u00b7ter\u00b7keit", "die", "Bit\u00b7ter\u00b7keit", "ver\u00b7trei\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKANT", "$,", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Kranke, wenn er ihm vertraut, genest vom Grunde,", "tokens": ["Der", "Kran\u00b7ke", ",", "wenn", "er", "ihm", "ver\u00b7traut", ",", "ge\u00b7nest", "vom", "Grun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PPER", "ADJD", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schmeckt die S\u00fc\u00dfe der Gesundheit neu im Munde.", "tokens": ["Und", "schmeckt", "die", "S\u00fc\u00b7\u00dfe", "der", "Ge\u00b7sund\u00b7heit", "neu", "im", "Mun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}