{"textgrid.poem.65938": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Pegasus", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf gro\u00dfen Wogen, Sohn der Gorgo, fliege,", "tokens": ["Auf", "gro\u00b7\u00dfen", "Wo\u00b7gen", ",", "Sohn", "der", "Gor\u00b7go", ",", "flie\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "ART", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beschwingtes Ro\u00df, ins Tagesgold empor!", "tokens": ["Be\u00b7schwing\u00b7tes", "Ro\u00df", ",", "ins", "Ta\u00b7ges\u00b7gold", "em\u00b7por", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich blicke hin, aus buntem Bogentor", "tokens": ["Ich", "bli\u00b7cke", "hin", ",", "aus", "bun\u00b7tem", "Bo\u00b7gen\u00b7tor"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Sonne, nach bedrohtem Wolkensiege:", "tokens": ["Der", "Son\u00b7ne", ",", "nach", "be\u00b7droh\u00b7tem", "Wol\u00b7ken\u00b7sie\u00b7ge", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-++", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Die Nebel perlen ihr, als Himmelsstiege,", "tokens": ["Die", "Ne\u00b7bel", "per\u00b7len", "ihr", ",", "als", "Him\u00b7mels\u00b7stie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erschreckt und rasch zu Tal: sie fruchtet vor.", "tokens": ["Er\u00b7schreckt", "und", "rasch", "zu", "Tal", ":", "sie", "fruch\u00b7tet", "vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "NN", "$.", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch Pegasus, nun strahl als Meteor,", "tokens": ["Doch", "Pe\u00b7ga\u00b7sus", ",", "nun", "strahl", "als", "Me\u00b7te\u00b7or", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ADV", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entz\u00fcck mein Blut zum Sang im Sonnenkriege!", "tokens": ["Ent\u00b7z\u00fcck", "mein", "Blut", "zum", "Sang", "im", "Son\u00b7nen\u00b7krie\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir folgen dir durchs Fl\u00fcgeln unsrer Ruder:", "tokens": ["Wir", "fol\u00b7gen", "dir", "durchs", "Fl\u00fc\u00b7geln", "uns\u00b7rer", "Ru\u00b7der", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Gl\u00fcckender, Berufer, st\u00fcrz dir nach!", "tokens": ["Ein", "Gl\u00fc\u00b7cken\u00b7der", ",", "Be\u00b7ru\u00b7fer", ",", "st\u00fcrz", "dir", "nach", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", 