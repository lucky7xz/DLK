{"dta.poem.23546": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der deutsche Zollverein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schwefelh\u00f6lzer, Fenchel, Bricken,", "tokens": ["Schwe\u00b7fel\u00b7h\u00f6l\u00b7zer", ",", "Fen\u00b7chel", ",", "Bri\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00fche, K\u00e4se, Krapp, Papier,", "tokens": ["K\u00fc\u00b7he", ",", "K\u00e4\u00b7se", ",", "Krapp", ",", "Pa\u00b7pier", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schinken, Scheeren, Stiefel, Wicken,", "tokens": ["Schin\u00b7ken", ",", "Schee\u00b7ren", ",", "Stie\u00b7fel", ",", "Wi\u00b7cken", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wolle, Seife, Garn und Bier;", "tokens": ["Wol\u00b7le", ",", "Sei\u00b7fe", ",", "Garn", "und", "Bier", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Pfefferkuchen, Lumpen, Trichter,", "tokens": ["Pfef\u00b7fer\u00b7ku\u00b7chen", ",", "Lum\u00b7pen", ",", "Trich\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "N\u00fcsse, Tabak, Gl\u00e4ser, Flachs,", "tokens": ["N\u00fcs\u00b7se", ",", "Ta\u00b7bak", ",", "Gl\u00e4\u00b7ser", ",", "Flachs", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Leder, Salz, Schmalz, Puppen, Lichter,", "tokens": ["Le\u00b7der", ",", "Salz", ",", "Schmalz", ",", "Pup\u00b7pen", ",", "Lich\u00b7ter", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Rettig, Rips, Raps, Schnaps, Lachs, Wachs!", "tokens": ["Ret\u00b7tig", ",", "Rips", ",", "Raps", ",", "Schnaps", ",", "Lachs", ",", "Wachs", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "NN", "$,", "NE", "$,", "NN", "$,", "NE", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ihr andern deutschen Sachen,", "tokens": ["Und", "ihr", "an\u00b7dern", "deut\u00b7schen", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tausend Dank sei euch gebracht!", "tokens": ["Tau\u00b7send", "Dank", "sei", "euch", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was kein Geist je konnte machen,", "tokens": ["Was", "kein", "Geist", "je", "konn\u00b7te", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "ADV", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ei, das habet ", "tokens": ["Ei", ",", "das", "ha\u00b7bet"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "PDS", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Denn ihr habt ein Band gewunden", "tokens": ["Denn", "ihr", "habt", "ein", "Band", "ge\u00b7wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VAPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um das deutsche Vaterland.", "tokens": ["Um", "das", "deut\u00b7sche", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Herzen hat verbunden", "tokens": ["Und", "die", "Her\u00b7zen", "hat", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr als unser Bund dies Band.", "tokens": ["Mehr", "als", "un\u00b7ser", "Bund", "dies", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "PPOSAT", "NN", "PDS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}