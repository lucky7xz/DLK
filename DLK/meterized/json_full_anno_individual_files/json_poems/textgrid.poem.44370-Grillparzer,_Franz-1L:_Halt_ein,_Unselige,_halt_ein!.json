{"textgrid.poem.44370": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "1L: Halt ein, Unselige, halt ein!", "genre": "verse", "period": "N.A.", "pub_year": 1819, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Halt ein, Unselige, halt ein!", "tokens": ["Halt", "ein", ",", "Un\u00b7se\u00b7li\u00b7ge", ",", "halt", "ein", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "ADJA", "$,", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wohin verlockst du mich?", "tokens": ["Wo\u00b7hin", "ver\u00b7lockst", "du", "mich", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00dcber Berge bin ich gekommen,", "tokens": ["\u00dc\u00b7ber", "Ber\u00b7ge", "bin", "ich", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Durch Schl\u00fcnde dir gefolgt,", "tokens": ["Durch", "Schl\u00fcn\u00b7de", "dir", "ge\u00b7folgt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Kein Pfad ist, wo ich trete, keine Spur \u2013", "tokens": ["Kein", "Pfad", "ist", ",", "wo", "ich", "tre\u00b7te", ",", "kei\u00b7ne", "Spur", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "$,", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Fern herauf t\u00f6nt der Menschen Stimme,", "tokens": ["Fern", "her\u00b7auf", "t\u00f6nt", "der", "Men\u00b7schen", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "T\u00f6nt der Herden fr\u00f6hliches Gel\u00e4ut", "tokens": ["T\u00f6nt", "der", "Her\u00b7den", "fr\u00f6h\u00b7li\u00b7ches", "Ge\u00b7l\u00e4ut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Und des Waldbachs Rauschen;", "tokens": ["Und", "des", "Wald\u00b7bachs", "Rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ringsum Klippen, wolkennahe Klippen,", "tokens": ["Ring\u00b7sum", "Klip\u00b7pen", ",", "wol\u00b7ken\u00b7na\u00b7he", "Klip\u00b7pen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "\u00dcber mir Duft und Nebel,", "tokens": ["\u00dc\u00b7ber", "mir", "Duft", "und", "Ne\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.11": {"text": "L\u00fcgend Gestalten!", "tokens": ["L\u00fc\u00b7gend", "Ge\u00b7stal\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Was willst du? \u2013 Steh und rede!", "tokens": ["Was", "willst", "du", "?", "\u2013", "Steh", "und", "re\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deiner Seite ein Weib", "tokens": ["An", "dei\u00b7ner", "Sei\u00b7te", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gr\u00e4ulichen Anblicks.", "tokens": ["Gr\u00e4u\u00b7li\u00b7chen", "An\u00b7blicks", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schwarz flattern die Haare,", "tokens": ["Schwarz", "flat\u00b7tern", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schwarz funkeln die Augen,", "tokens": ["Schwarz", "fun\u00b7keln", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schwarz das Gewand \u2013 Blut!", "tokens": ["Schwarz", "das", "Ge\u00b7wand", "\u2013", "Blut", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$(", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Blut an ihrem Gewande,", "tokens": ["Blut", "an", "ih\u00b7rem", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "An dem Dolch, den sie z\u00fcckt!", "tokens": ["An", "dem", "Dolch", ",", "den", "sie", "z\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Zwei Kinder tot zu ihren F\u00fc\u00dfen", "tokens": ["Zwei", "Kin\u00b7der", "tot", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und ein Greis und ein J\u00fcngling,", "tokens": ["Und", "ein", "Greis", "und", "ein", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.11": {"text": "Im Todeskampf verzerrend,", "tokens": ["Im", "To\u00b7des\u00b7kampf", "ver\u00b7zer\u00b7rend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Verwandte, \u00e4hnliche Z\u00fcge;", "tokens": ["Ver\u00b7wand\u00b7te", ",", "\u00e4hn\u00b7li\u00b7che", "Z\u00fc\u00b7ge", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Um die Schultern aber gl\u00e4nzt es \u2013", "tokens": ["Um", "die", "Schul\u00b7tern", "a\u00b7ber", "gl\u00e4nzt", "es", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ADV", "VVFIN", "PPER", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Ein Vlie\u00df \u2013 ein goldstrahlendes Vlie\u00df \u2013", "tokens": ["Ein", "Vlie\u00df", "\u2013", "ein", "gold\u00b7strah\u00b7len\u00b7des", "Vlie\u00df", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.15": {"text": "Medea! \u2013", "tokens": ["Me\u00b7dea", "!", "\u2013"], "token_info": ["word", "punct", "punct"], "pos": ["NE", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Hebe dich weg, Entsetzliche!", "tokens": ["He\u00b7be", "dich", "weg", ",", "Ent\u00b7setz\u00b7li\u00b7che", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "$,", "NN", "$."], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "Kinder-, Bruder-, Vaterm\u00f6rderin!", "tokens": ["Kin\u00b7der", ",", "Bru\u00b7der", ",", "Va\u00b7ter\u00b7m\u00f6r\u00b7de\u00b7rin", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["TRUNC", "$,", "TRUNC", "$,", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Was ist mir gemein mit dir?", "tokens": ["Was", "ist", "mir", "ge\u00b7mein", "mit", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Den Vater hab ich kindlich geehrt,", "tokens": ["Den", "Va\u00b7ter", "hab", "ich", "kind\u00b7lich", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Und als die Mutter starb,", "tokens": ["Und", "als", "die", "Mut\u00b7ter", "starb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Flossen fromme Tr\u00e4nen", "tokens": ["Flos\u00b7sen", "from\u00b7me", "Tr\u00e4\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Ihr nach ins unerw\u00fcnschte Grab.", "tokens": ["Ihr", "nach", "ins", "un\u00b7er\u00b7w\u00fcnschte", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Was hab ich gemein mit dir?", "tokens": ["Was", "hab", "ich", "ge\u00b7mein", "mit", "dir", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Mir schaudert. Geh! \u2013", "tokens": ["Mir", "schau\u00b7dert", ".", "Geh", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Und auch du, die mich hergelockt,", "tokens": ["Und", "auch", "du", ",", "die", "mich", "her\u00b7ge\u00b7lockt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Durch die Leier in deinem Arm", "tokens": ["Durch", "die", "Lei\u00b7er", "in", "dei\u00b7nem", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.12": {"text": "Und den Kranz, den du tr\u00e4gst,", "tokens": ["Und", "den", "Kranz", ",", "den", "du", "tr\u00e4gst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Vom immergr\u00fcnen Laub, das mich lockt,", "tokens": ["Vom", "im\u00b7mer\u00b7gr\u00fc\u00b7nen", "Laub", ",", "das", "mich", "lockt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.14": {"text": "Hebe dich weg und la\u00df mich!", "tokens": ["He\u00b7be", "dich", "weg", "und", "la\u00df", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKVZ", "KON", "VVIMP", "PPER", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.15": {"text": "Da\u00df ich, den R\u00fcckweg suchend,", "tokens": ["Da\u00df", "ich", ",", "den", "R\u00fcck\u00b7weg", "su\u00b7chend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Heimkehre zu den Meinen.", "tokens": ["Heim\u00b7keh\u00b7re", "zu", "den", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aber du schaust mich an?", "tokens": ["A\u00b7ber", "du", "schaust", "mich", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Mit dem Auge, streng zugleich und innig,", "tokens": ["Mit", "dem", "Au\u00b7ge", ",", "streng", "zu\u00b7gleich", "und", "in\u00b7nig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "VVFIN", "ADV", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit dem seelenbindenden Blick,", "tokens": ["Mit", "dem", "see\u00b7len\u00b7bin\u00b7den\u00b7den", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Der schon dem keimenden Knaben", "tokens": ["Der", "schon", "dem", "kei\u00b7men\u00b7den", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das Spielzeug wand aus den H\u00e4nden", "tokens": ["Das", "Spiel\u00b7zeug", "wand", "aus", "den", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und, ablockend vom Kreis der Gef\u00e4hrten,", "tokens": ["Und", ",", "ab\u00b7lo\u00b7ckend", "vom", "Kreis", "der", "Ge\u00b7f\u00e4hr\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "In einsiedlerische Still ihn bannend,", "tokens": ["In", "ein\u00b7sied\u00b7le\u00b7ri\u00b7sche", "Still", "ihn", "ban\u00b7nend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Das Geschick der K\u00f6nige", "tokens": ["Das", "Ge\u00b7schick", "der", "K\u00f6\u00b7ni\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und der Welt ungel\u00f6ste, ewige R\u00e4tsel", "tokens": ["Und", "der", "Welt", "un\u00b7ge\u00b7l\u00f6s\u00b7te", ",", "e\u00b7wi\u00b7ge", "R\u00e4t\u00b7sel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "--++-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ihm gab zum ahnungsvollen, ernsten Spiel.", "tokens": ["Ihm", "gab", "zum", "ah\u00b7nungs\u00b7vol\u00b7len", ",", "erns\u00b7ten", "Spiel", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Du schaust mich an und willst nicht gehn?", "tokens": ["Du", "schaust", "mich", "an", "und", "willst", "nicht", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "KON", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Winkst mir, zu folgen dir und der Gef\u00e4hrtin,", "tokens": ["Winkst", "mir", ",", "zu", "fol\u00b7gen", "dir", "und", "der", "Ge\u00b7f\u00e4hr\u00b7tin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PTKZU", "VVINF", "PPER", "KON", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "Medeen, mit dem gr\u00e4\u00dflichen Blick?", "tokens": ["Me\u00b7de\u00b7en", ",", "mit", "dem", "gr\u00e4\u00df\u00b7li\u00b7chen", "Blick", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.14": {"text": "Du nimmst den Kranz vom duftenden Haar", "tokens": ["Du", "nimmst", "den", "Kranz", "vom", "duf\u00b7ten\u00b7den", "Haar"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Und setzest ihn aufs Haupt der Entsetzlichen?", "tokens": ["Und", "set\u00b7zest", "ihn", "aufs", "Haupt", "der", "Ent\u00b7setz\u00