 "pos": ["ADJD", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Was willst du? \u2013 Steh und rede!", "tokens": ["Was", "willst", "du", "?", "\u2013", "Steh", "und", "re\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$.", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An deiner Seite ein Weib", "tokens": ["An", "dei\u00b7ner", "Sei\u00b7te", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Gr\u00e4ulichen Anblicks.", "tokens": ["Gr\u00e4u\u00b7li\u00b7chen", "An\u00b7blicks", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Schwarz flattern die Haare,", "tokens": ["Schwarz", "flat\u00b7tern", "die", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Schwarz funkeln die Augen,", "tokens": ["Schwarz", "fun\u00b7keln", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Schwarz das Gewand \u2013 Blut!", "tokens": ["Schwarz", "das", "Ge\u00b7wand", "\u2013", "Blut", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "$(", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Blut an ihrem Gewande,", "tokens": ["Blut", "an", "ih\u00b7rem", "Ge\u00b7wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$,"],