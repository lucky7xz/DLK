{"textgrid.poem.33525": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Die Plagiatoren", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein jeder ist ein Teil vom Schicksal andrer,", "tokens": ["Ein", "je\u00b7der", "ist", "ein", "Teil", "vom", "Schick\u00b7sal", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die vor ihm waren und die um ihn gehen,", "tokens": ["Die", "vor", "ihm", "wa\u00b7ren", "und", "die", "um", "ihn", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VAFIN", "KON", "ART", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die auch nur einmal, eilge Weiterwandrer,", "tokens": ["Die", "auch", "nur", "ein\u00b7mal", ",", "eil\u00b7ge", "Wei\u00b7ter\u00b7wand\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Weg ihm kreuzend, fl\u00fcchtig bei ihm stehen.", "tokens": ["Den", "Weg", "ihm", "kreu\u00b7zend", ",", "fl\u00fcch\u00b7tig", "bei", "ihm", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADJD", "$,", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie kommen, kommen ohne Zweck und Sinn,", "tokens": ["Sie", "kom\u00b7men", ",", "kom\u00b7men", "oh\u00b7ne", "Zweck", "und", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entfernen sich mit leichtem Wanderschritt.", "tokens": ["Ent\u00b7fer\u00b7nen", "sich", "mit", "leich\u00b7tem", "Wan\u00b7der\u00b7schritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie bringen alle etwas zu ihm hin.", "tokens": ["Sie", "brin\u00b7gen", "al\u00b7le", "et\u00b7was", "zu", "ihm", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie nehmen alle etwas von ihm mit.", "tokens": ["Sie", "neh\u00b7men", "al\u00b7le", "et\u00b7was", "von", "ihm", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "PIS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein jeder ist ein Teil vom Schicksal andrer,", "tokens": ["Ein", "je\u00b7der", "ist", "ein", "Teil", "vom", "Schick\u00b7sal", "an\u00b7drer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "APPRART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Die vor ihm waren und die um ihn gehen,", "tokens": ["Die", "vor", "ihm", "wa\u00b7ren", "und", "die", "um", "ihn", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER",