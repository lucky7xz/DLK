{"textgrid.poem.41531": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am Palmsonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgentau will steigen,", "tokens": ["Der", "Mor\u00b7gen\u00b7tau", "will", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind denn die Palmen gr\u00fcn?", "tokens": ["Sind", "denn", "die", "Pal\u00b7men", "gr\u00fcn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auf, la\u00dft mit hellen Zweigen", "tokens": ["Auf", ",", "la\u00dft", "mit", "hel\u00b7len", "Zwei\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns ihm entgegenziehn!", "tokens": ["Uns", "ihm", "ent\u00b7ge\u00b7gen\u00b7ziehn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er will in unser Haus,", "tokens": ["Er", "will", "in", "un\u00b7ser", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In unsre Kammern kommen;", "tokens": ["In", "uns\u00b7re", "Kam\u00b7mern", "kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Schon ziehen rings die Frommen", "tokens": ["Schon", "zie\u00b7hen", "rings", "die", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit Lobgesang heraus.", "tokens": ["Mit", "Lob\u00b7ge\u00b7sang", "he\u00b7raus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kann nicht mit euch gehen,", "tokens": ["Ich", "kann", "nicht", "mit", "euch", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir ist der Odem schwer;", "tokens": ["Mir", "ist", "der", "O\u00b7dem", "schwer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kreuzesfahnen wehen,", "tokens": ["Die", "Kreu\u00b7zes\u00b7fah\u00b7nen", "we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich folge nimmermehr.", "tokens": ["Ich", "fol\u00b7ge", "nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie wird so klar die Luft!", "tokens": ["Wie", "wird", "so", "klar", "die", "Luft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "O Jesu, s\u00fc\u00dfe Helle,", "tokens": ["O", "Je\u00b7su", ",", "s\u00fc\u00b7\u00dfe", "Hel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du k\u00f6mmst in meine Zelle,", "tokens": ["Du", "k\u00f6mmst", "in", "mei\u00b7ne", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In meine Modergruft!", "tokens": ["In", "mei\u00b7ne", "Mo\u00b7der\u00b7gruft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was soll ich dir bereiten,", "tokens": ["Was", "soll", "ich", "dir", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du wunderlicher Gast?", "tokens": ["Du", "wun\u00b7der\u00b7li\u00b7cher", "Gast", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich m\u00f6chte dich verleiten", "tokens": ["Ich", "m\u00f6ch\u00b7te", "dich", "ver\u00b7lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu langer Liebesrast.", "tokens": ["Zu", "lan\u00b7ger", "Lie\u00b7bes\u00b7rast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wohlan, ich schm\u00fccke dich,", "tokens": ["Wo\u00b7hlan", ",", "ich", "schm\u00fc\u00b7cke", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Will dich mit Blumen binden;", "tokens": ["Will", "dich", "mit", "Blu\u00b7men", "bin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Du sollst dich nicht entwinden,", "tokens": ["Du", "sollst", "dich", "nicht", "ent\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das wei\u00df ich sicherlich.", "tokens": ["Das", "wei\u00df", "ich", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Aus deiner Mutter Rechten", "tokens": ["Aus", "dei\u00b7ner", "Mut\u00b7ter", "Rech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Will ich um deinen Fu\u00df", "tokens": ["Will", "ich", "um", "dei\u00b7nen", "Fu\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die reine Lilie flechten", "tokens": ["Die", "rei\u00b7ne", "Li\u00b7lie", "flech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit demutsvollem Gru\u00df.", "tokens": ["Mit", "de\u00b7muts\u00b7vol\u00b7lem", "Gru\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Da\u00df ich dich fe\u00dfle ganz", "tokens": ["Da\u00df", "ich", "dich", "fe\u00df\u00b7le", "ganz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Liebesblumenringen,", "tokens": ["Mit", "Lie\u00b7bes\u00b7blu\u00b7men\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Will um dein Haupt ich schlingen", "tokens": ["Will", "um", "dein", "Haupt", "ich", "schlin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den heil'gen Rosenkranz.", "tokens": ["Den", "heil'\u00b7gen", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Den Boden will ich streuen", "tokens": ["Den", "Bo\u00b7den", "will", "ich", "streu\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Palmen ganz und gar,", "tokens": ["Mit", "Pal\u00b7men", "ganz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Leiden dir zu weihen,", "tokens": ["Mein", "Lei\u00b7den", "dir", "zu", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ich in diesem Jahr", "tokens": ["Was", "ich", "in", "die\u00b7sem", "Jahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Oft still, oft schwerer trug.", "tokens": ["Oft", "still", ",", "oft", "schwe\u00b7rer", "trug", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es liegt zu deinen F\u00fc\u00dfen,", "tokens": ["Es", "liegt", "zu", "dei\u00b7nen", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Es soll mich nicht verdrie\u00dfen,", "tokens": ["Es", "soll", "mich", "nicht", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Dein Will' ist mir genug.", "tokens": ["Dein", "Will'", "ist", "mir", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wie soll ich mich doch finden", "tokens": ["Wie", "soll", "ich", "mich", "doch", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In deine Liebesmacht,", "tokens": ["In", "dei\u00b7ne", "Lie\u00b7bes\u00b7macht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df du an meine S\u00fcnden", "tokens": ["Da\u00df", "du", "an", "mei\u00b7ne", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So gar nicht hast gedacht!", "tokens": ["So", "gar", "nicht", "hast", "ge\u00b7dacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich lasse nicht von dir,", "tokens": ["Ich", "las\u00b7se", "nicht", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mu\u00dft du gleich wieder scheiden;", "tokens": ["Mu\u00dft", "du", "gleich", "wie\u00b7der", "schei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ich f\u00fchl' es wohl in Freuden,", "tokens": ["Ich", "f\u00fchl'", "es", "wohl", "in", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Du k\u00f6mmst noch oft zu mir.", "tokens": ["Du", "k\u00f6mmst", "noch", "oft", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Morgentau will steigen,", "tokens": ["Der", "Mor\u00b7gen\u00b7tau", "will", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind denn die Palmen gr\u00fcn?", "tokens": ["Sind", "denn", "die", "Pal\u00b7men", "gr\u00fcn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "AR