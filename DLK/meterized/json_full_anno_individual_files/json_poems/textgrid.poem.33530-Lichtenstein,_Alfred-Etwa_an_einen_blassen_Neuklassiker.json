{"textgrid.poem.33530": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Etwa an einen blassen Neuklassiker", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, fr\u00fcher August, f\u00fchlst dich jetzt Hellene.", "tokens": ["Du", ",", "fr\u00fc\u00b7her", "Au\u00b7gust", ",", "f\u00fchlst", "dich", "jetzt", "Hel\u00b7le\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJD", "NN", "$,", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dahin sind Hurenhuld und Schiebet\u00e4nze,", "tokens": ["Da\u00b7hin", "sind", "Hu\u00b7ren\u00b7huld", "und", "Schie\u00b7be\u00b7t\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Poesie Berliner \u00c4ppelk\u00e4hne", "tokens": ["Die", "Poe\u00b7sie", "Ber\u00b7li\u00b7ner", "\u00c4p\u00b7pel\u00b7k\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Entschwand dir in dem Blau der Griechenlenze.", "tokens": ["Ent\u00b7schwand", "dir", "in", "dem", "Blau", "der", "Grie\u00b7chen\u00b7len\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Zeiten \u00e4ndern sich. Der Mann wird reifer,", "tokens": ["Die", "Zei\u00b7ten", "\u00e4n\u00b7dern", "sich", ".", "Der", "Mann", "wird", "rei\u00b7fer", ","], "token_info": ["word", "wor