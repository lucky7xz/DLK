{"dta.poem.14897": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Meeresstille .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Seek\u00f6nig auf seiner Warte", "tokens": ["See\u00b7k\u00f6\u00b7nig", "auf", "sei\u00b7ner", "War\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sitzt in der D\u00e4mm'rung tief,", "tokens": ["Sitzt", "in", "der", "D\u00e4m\u00b7m'\u00b7rung", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Als ob er mit langem Barte", "tokens": ["Als", "ob", "er", "mit", "lan\u00b7gem", "Bar\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ueber seiner Harfe schlief';", "tokens": ["Ue\u00b7ber", "sei\u00b7ner", "Har\u00b7fe", "schlie\u00b7f'", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da kommen und gehen die Schiffe", "tokens": ["Da", "kom\u00b7men", "und", "ge\u00b7hen", "die", "Schif\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "KON", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Dar\u00fcber, er merkt es kaum,", "tokens": ["Da\u00b7r\u00fc\u00b7ber", ",", "er", "merkt", "es", "kaum", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Von seinem Korallenriffe", "tokens": ["Von", "sei\u00b7nem", "Ko\u00b7ral\u00b7len\u00b7rif\u00b7fe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Gr\u00fc\u00dft er sie wie im Traum.", "tokens": ["Gr\u00fc\u00dft", "er", "sie", "wie", "im", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "KOKOM", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}