{"dta.poem.7102": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie wird nicht offt das Hertz ger\u00fchret,", "tokens": ["Wie", "wird", "nicht", "offt", "das", "Hertz", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PTKNEG", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn unser irdisch Ohr ein irdisch Singen sp\u00fcret:", "tokens": ["Wenn", "un\u00b7ser", "ir\u00b7disch", "Ohr", "ein", "ir\u00b7disch", "Sin\u00b7gen", "sp\u00fc\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie wird denn nicht ein himmlisch Schallen", "tokens": ["Wie", "wird", "denn", "nicht", "ein", "himm\u00b7lisch", "Schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PTKNEG", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Verkl\u00e4rten Seelen dort gefallen?", "tokens": ["Ver\u00b7kl\u00e4r\u00b7ten", "See\u00b7len", "dort", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn sie von tausend Engel-Ch\u00f6ren", "tokens": ["Wenn", "sie", "von", "tau\u00b7send", "En\u00b7gel\u00b7Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein ewigs Lob- und Danck-Lied h\u00f6ren?", "tokens": ["Ein", "e\u00b7wigs", "Lob", "und", "Dan\u00b7ck\u00b7Lied", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ach! wie wird nicht der Seraphinen Singen", "tokens": ["Ach", "!", "wie", "wird", "nicht", "der", "Se\u00b7ra\u00b7phi\u00b7nen", "Sin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PWAV", "VAFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jhr Innerstes, mit s\u00fcsser Lust, durchdringen!", "tokens": ["Ihr", "In\u00b7ners\u00b7tes", ",", "mit", "s\u00fcs\u00b7ser", "Lust", ",", "durch\u00b7drin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Welch\u2019 eine Freude, welch Entz\u00fccken", "tokens": ["Welch'", "ei\u00b7ne", "Freu\u00b7de", ",", "welch", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ART", "NN", "$,", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wird nicht, bey jedem Thon, sich in die Seele dr\u00fccken!", "tokens": ["Wird", "nicht", ",", "bey", "je\u00b7dem", "Thon", ",", "sich", "in", "die", "See\u00b7le", "dr\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "$,", "APPR", "PIAT", "NN", "$,", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Jhr Gantzes wird vergn\u00fcgt, ihr Gantzes wird ger\u00fchrt,", "tokens": ["Ihr", "Gant\u00b7zes", "wird", "ver\u00b7gn\u00fcgt", ",", "ihr", "Gant\u00b7zes", "wird", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Durch einen jeden Schall, den es so seelig sp\u00fcrt.", "tokens": ["Durch", "ei\u00b7nen", "je\u00b7den", "Schall", ",", "den", "es", "so", "see\u00b7lig", "sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ja man wird nicht, wie hier, durch einen Sinn allein,", "tokens": ["Ja", "man", "wird", "nicht", ",", "wie", "hier", ",", "durch", "ei\u00b7nen", "Sinn", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PIS", "VAFIN", "PTKNEG", "$,", "PWAV", "ADV", "$,", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch alle, ja durch mehr, zugleich vergn\u00fcget seyn.", "tokens": ["Durch", "al\u00b7le", ",", "ja", "durch", "mehr", ",", "zu\u00b7gleich", "ver\u00b7gn\u00fc\u00b7get", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "ADV", "APPR", "ADV", "$,", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}