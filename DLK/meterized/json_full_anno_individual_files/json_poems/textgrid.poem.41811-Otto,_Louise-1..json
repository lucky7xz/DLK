{"textgrid.poem.41811": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An die Vigilien, die wir gehalten", "tokens": ["An", "die", "Vi\u00b7gi\u00b7li\u00b7en", ",", "die", "wir", "ge\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Vor inhaltsvollen, heil'gen Festestagen", "tokens": ["Vor", "in\u00b7halts\u00b7vol\u00b7len", ",", "heil'\u00b7gen", "Fes\u00b7tes\u00b7ta\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denk' ich entz\u00fcckt wie an ein Fl\u00fcgelschlagen", "tokens": ["Denk'", "ich", "ent\u00b7z\u00fcckt", "wie", "an", "ein", "Fl\u00fc\u00b7gel\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Genius, trotz irdischer Gewalten.", "tokens": ["Des", "Ge\u00b7nius", ",", "trotz", "ir\u00b7di\u00b7scher", "Ge\u00b7wal\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Denk' ich mit andachtsvollem H\u00e4ndefalten!", "tokens": ["Denk'", "ich", "mit", "an\u00b7dachts\u00b7vol\u00b7lem", "H\u00e4n\u00b7de\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn ob dem Niederen emporgetragen", "tokens": ["Denn", "ob", "dem", "Nie\u00b7de\u00b7ren", "em\u00b7por\u00b7ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von der Begeistrung goldnem Sonnenwagen,", "tokens": ["Von", "der", "Be\u00b7geis\u00b7trung", "gold\u00b7nem", "Son\u00b7nen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umringten uns nur himmlische Gestalten.", "tokens": ["Um\u00b7ring\u00b7ten", "uns", "nur", "himm\u00b7li\u00b7sche", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie einstens jene frommen Br\u00fcderschaften,", "tokens": ["Wie", "eins\u00b7tens", "je\u00b7ne", "from\u00b7men", "Br\u00fc\u00b7der\u00b7schaf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die sich dem Dienst der heil'gen Baukunst weihten,", "tokens": ["Die", "sich", "dem", "Dienst", "der", "heil'\u00b7gen", "Bau\u00b7kunst", "weih\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Profanem Wesen siegreich sich entrafften:", "tokens": ["Pro\u00b7fa\u00b7nem", "We\u00b7sen", "sieg\u00b7reich", "sich", "en\u00b7traff\u00b7ten", ":"], "token_info": ["word", "word", "word", 