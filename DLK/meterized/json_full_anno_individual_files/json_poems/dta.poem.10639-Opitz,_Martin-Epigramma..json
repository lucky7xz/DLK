{"dta.poem.10639": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Epigramma.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Die V\u00f6gel von dem Leim des Stellers offt entschleichen/", "tokens": ["Die", "V\u00f6\u00b7gel", "von", "dem", "Leim", "des", "Stel\u00b7lers", "offt", "ent\u00b7schlei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Fisch des Fischers Netz nicht gern zu nahe geht/", "tokens": ["Der", "Fisch", "des", "Fi\u00b7schers", "Netz", "nicht", "gern", "zu", "na\u00b7he", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "PTKNEG", "ADV", "PTKA", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von wegen seiner Vers ist sicher der Poet/", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "Vers", "ist", "si\u00b7cher", "der", "Po\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VAFIN", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Kriegesmanne viel der Waffen halben weichen/", "tokens": ["Dem", "Krie\u00b7ges\u00b7man\u00b7ne", "viel", "der", "Waf\u00b7fen", "hal\u00b7ben", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man will den Scorpion des Gifftes wegen nicht/", "tokens": ["Man", "will", "den", "Scor\u00b7pi\u00b7on", "des", "Giff\u00b7tes", "we\u00b7gen", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ART", "NN", "APPR", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vmb grosse Listigkeit den Fuchs die Thiere schewen/", "tokens": ["Vmb", "gros\u00b7se", "Lis\u00b7tig\u00b7keit", "den", "Fuchs", "die", "Thie\u00b7re", "sche\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NE", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Von wegen seiner Macht verneidet man den L\u00f6wen/", "tokens": ["Von", "we\u00b7gen", "sei\u00b7ner", "Macht", "ver\u00b7nei\u00b7det", "man", "den", "L\u00f6\u00b7wen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "PPOSAT", "NN", "VVFIN", "PIS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An einem Weibesbild\u2019 ist m\u00e4chtig das Gesicht.", "tokens": ["An", "ei\u00b7nem", "Wei\u00b7bes\u00b7bild'", "ist", "m\u00e4ch\u00b7tig", "das", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}