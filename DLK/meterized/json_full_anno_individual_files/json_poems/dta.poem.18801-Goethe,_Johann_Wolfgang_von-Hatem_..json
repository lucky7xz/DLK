{"dta.poem.18801": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Hatem .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Kaum dass ich dich wieder habe", "tokens": ["Kaum", "dass", "ich", "dich", "wie\u00b7der", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADV", "VAFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich mit Kuss und Liedern labe,", "tokens": ["Dich", "mit", "Kuss", "und", "Lie\u00b7dern", "la\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist du still in dich gekehret;", "tokens": ["Bist", "du", "still", "in", "dich", "ge\u00b7keh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was beengt? und dr\u00fcckt und st\u00f6ret?", "tokens": ["Was", "be\u00b7engt", "?", "und", "dr\u00fcckt", "und", "st\u00f6\u00b7ret", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, Suleika, soll ich\u2019s sagen?", "tokens": ["Ach", ",", "Sul\u00b7ei\u00b7ka", ",", "soll", "ich's", "sa\u00b7gen", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VMFIN", "PIS", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Statt zu loben m\u00f6cht\u2019 ich klagen!", "tokens": ["Statt", "zu", "lo\u00b7ben", "m\u00f6cht'", "ich", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sangest sonst nur meine Lieder,", "tokens": ["San\u00b7gest", "sonst", "nur", "mei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Immer neu und immer wieder.", "tokens": ["Im\u00b7mer", "neu", "und", "im\u00b7mer", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sollte wohl auch diese loben,", "tokens": ["Soll\u00b7te", "wohl", "auch", "die\u00b7se", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sie sind nur eingeschoben;", "tokens": ["Doch", "sie", "sind", "nur", "ein\u00b7ge\u00b7scho\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht von Hafis, nicht Nisami,", "tokens": ["Nicht", "von", "Ha\u00b7fis", ",", "nicht", "Ni\u00b7sa\u00b7mi", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NE", "$,", "PTKNEG", "NE", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Nicht Saadi, nicht von Dschami.", "tokens": ["Nicht", "Saa\u00b7di", ",", "nicht", "von", "Dscha\u00b7mi", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "$,", "PTKNEG", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Kenn\u2019 ich doch der V\u00e4ter Menge,", "tokens": ["Kenn'", "ich", "doch", "der", "V\u00e4\u00b7ter", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sylb\u2019 um Sylbe, Klang um Kl\u00e4nge,", "tokens": ["Sylb'", "um", "Syl\u00b7be", ",", "Klang", "um", "Kl\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$,", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Im Ged\u00e4chtniss unverloren;", "tokens": ["Im", "Ge\u00b7d\u00e4cht\u00b7niss", "un\u00b7ver\u00b7lo\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diese da sind neu geboren.", "tokens": ["Die\u00b7se", "da", "sind", "neu", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gestern wurden sie gedichtet.", "tokens": ["Ge\u00b7stern", "wur\u00b7den", "sie", "ge\u00b7dich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Sag hast du dich neu verpflichtet?", "tokens": ["Sag", "hast", "du", "dich", "neu", "ver\u00b7pflich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hauchest du so froh-verwegen", "tokens": ["Hau\u00b7chest", "du", "so", "froh\u00b7ver\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fremden Athem mir entgegen!", "tokens": ["Frem\u00b7den", "A\u00b7them", "mir", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der dich eben so belebet,", "tokens": ["Der", "dich", "e\u00b7ben", "so", "be\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eben so in Liebe schwebet,", "tokens": ["E\u00b7ben", "so", "in", "Lie\u00b7be", "schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Lockend, ladend zum Vereine", "tokens": ["Lo\u00b7ckend", ",", "la\u00b7dend", "zum", "Ver\u00b7ei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So harmonisch als der meine.", "tokens": ["So", "har\u00b7mo\u00b7nisch", "als", "der", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "PPOSAT", "$."], "meter": "-++-+-+-", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "War Hatem lange doch entfernt,", "tokens": ["War", "Ha\u00b7tem", "lan\u00b7ge", "doch", "ent\u00b7fernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das M\u00e4dchen hatte was gelernt,", "tokens": ["Das", "M\u00e4d\u00b7chen", "hat\u00b7te", "was", "ge\u00b7lernt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihm war sie so sch\u00f6n gelobt,", "tokens": ["Von", "ihm", "war", "sie", "so", "sch\u00f6n", "ge\u00b7lobt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Da hat die Trennung sich erprobt.", "tokens": ["Da", "hat", "die", "Tren\u00b7nung", "sich", "er\u00b7probt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl dass sie dir nicht fremde scheinen;", "tokens": ["Wohl", "dass", "sie", "dir", "nicht", "frem\u00b7de", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "PTKNEG", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie sind Suleika\u2019s, sind die deinen!", "tokens": ["Sie", "sind", "Su\u00b7lei\u00b7ka's", ",", "sind", "die", "dei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}