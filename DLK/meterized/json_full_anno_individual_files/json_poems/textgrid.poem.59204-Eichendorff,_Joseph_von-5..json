{"textgrid.poem.59204": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht Tr\u00e4ume sind's und leere Wahngesichte,", "tokens": ["Nicht", "Tr\u00e4u\u00b7me", "sin\u00b7d's", "und", "lee\u00b7re", "Wahn\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was von dem Volk den Dichter unterscheidet.", "tokens": ["Was", "von", "dem", "Volk", "den", "Dich\u00b7ter", "un\u00b7ter\u00b7schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was er inbr\u00fcnstig bildet, liebt und leidet,", "tokens": ["Was", "er", "in\u00b7br\u00fcns\u00b7tig", "bil\u00b7det", ",", "liebt", "und", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ist des Lebens wahrhafte Geschichte.", "tokens": ["Es", "ist", "des", "Le\u00b7bens", "wahr\u00b7haf\u00b7te", "Ge\u00b7schich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Er fragt nicht viel, wie ihn die Menge richte,", "tokens": ["Er", "fragt", "nicht", "viel", ",", "wie", "ihn", "die", "Men\u00b7ge", "rich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der eignen Ehr nur in der Brust vereidet;", "tokens": ["Der", "eig\u00b7nen", "Ehr", "nur", "in", "der", "Brust", "ver\u00b7ei\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn wo begeistert er die Blicke weidet,", "tokens": ["Denn", "wo", "be\u00b7geis\u00b7tert", "er", "die", "Bli\u00b7cke", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gr\u00fc\u00dft ihn der Weltkreis mit verwandtem Lichte.", "tokens": ["Gr\u00fc\u00dft", "ihn", "der", "Welt\u00b7kreis", "mit", "ver\u00b7wand\u00b7tem", "Lich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die sch\u00f6ne Mutter, die ihn hat geboren,", "tokens": ["Die", "sch\u00f6\u00b7ne", "Mut\u00b7ter", ",", "die", "ihn", "hat", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Himmel liebt er, der ihn auserkoren,", "tokens": ["Den", "Him\u00b7mel", "liebt", "er", ",", "der", "ihn", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft beide Haupt und Brust sich heiter schm\u00fccken.", "tokens": ["L\u00e4\u00dft", "bei\u00b7de", "Haupt", "und", "Brust", "sich", "hei\u00b7ter", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "KON", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die Menge selbst, die herbraust, ihn zu fragen", "tokens": ["Die", "Men\u00b7ge", "selbst", ",", "die", "her\u00b7braust", ",", "ihn", "zu", "fra\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Nach seinem Recht, mu\u00df den Begl\u00fcckten tragen,", "tokens": ["Nach", "sei\u00b7nem", "Recht", ",", "mu\u00df", "den", "Be\u00b7gl\u00fcck\u00b7ten", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Element ihm bietend ihren R\u00fccken.", "tokens": ["Als", "E\u00b7le\u00b7ment", "ihm", "bie\u00b7tend", "ih\u00b7ren", "R\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nicht Tr\u00e4ume sind's und leere Wahngesichte,", "tokens": ["Nicht", "Tr\u00e4u\u00b7me", "sin\u00b7d's", "und", "lee\u00b7re", "Wahn\u00b7ge\u00b7sich