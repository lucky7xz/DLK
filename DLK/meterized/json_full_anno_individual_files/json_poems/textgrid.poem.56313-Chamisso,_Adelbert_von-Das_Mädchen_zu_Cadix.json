{"textgrid.poem.56313": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen zu Cadix", "genre": "verse", "period": "N.A.", "pub_year": 1828, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwillst, ein Schlechter unter Schlechten,", "tokens": ["\u00bb", "willst", ",", "ein", "Schlech\u00b7ter", "un\u00b7ter", "Schlech\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Spanierin du buhlen?", "tokens": ["Um", "die", "Spa\u00b7ni\u00b7e\u00b7rin", "du", "buh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Girrend zu der Laute singst du,", "tokens": ["Gir\u00b7rend", "zu", "der", "Lau\u00b7te", "singst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und der Franke h\u00e4lt die Runde.", "tokens": ["Und", "der", "Fran\u00b7ke", "h\u00e4lt", "die", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Geht, ich kenn euch, Taubenherzen!", "tokens": ["Geht", ",", "ich", "kenn", "euch", ",", "Tau\u00b7ben\u00b7her\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht, ich kenn euch, Andalusier!", "tokens": ["Geht", ",", "ich", "kenn", "euch", ",", "An\u00b7da\u00b7lu\u00b7sier", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Euch die Spindel, uns die Waffen,", "tokens": ["Euch", "die", "Spin\u00b7del", ",", "uns", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Besser st\u00e4nd's mit Spaniens Ruhme!", "tokens": ["Bes\u00b7ser", "st\u00e4n\u00b7d's", "mit", "Spa\u00b7ni\u00b7ens", "Ruh\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Regen sich in ihrer Scheide", "tokens": ["Re\u00b7gen", "sich", "in", "ih\u00b7rer", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Eure Messer ungeduldig", "tokens": ["Eu\u00b7re", "Mes\u00b7ser", "un\u00b7ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durstend nach dem Blut der Fremden,", "tokens": ["Durs\u00b7tend", "nach", "dem", "Blut", "der", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprecht ihr zu dem Eisen: ruhig!", "tokens": ["Sprecht", "ihr", "zu", "dem", "Ei\u00b7sen", ":", "ru\u00b7hig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O der \u00fcberm\u00fct'gen Fremden!", "tokens": ["O", "der", "\u00fc\u00b7ber\u00b7m\u00fct'\u00b7gen", "Frem\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber euch sei ihre Rute,", "tokens": ["\u00dc\u00b7ber", "euch", "sei", "ih\u00b7re", "Ru\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber euch, ihr feigen Knechte,", "tokens": ["\u00dc\u00b7ber", "euch", ",", "ihr", "fei\u00b7gen", "Knech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrdig solcher Nebenbuhler!\u00ab \u2013", "tokens": ["W\u00fcr\u00b7dig", "sol\u00b7cher", "Ne\u00b7ben\u00b7buh\u00b7ler", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "PIAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "\u00bbherrin, Worte schweren Inhalts", "tokens": ["\u00bb", "her\u00b7rin", ",", "Wor\u00b7te", "schwe\u00b7ren", "In\u00b7halts"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sprichst du aus mit leichter Zunge,", "tokens": ["Sprichst", "du", "aus", "mit", "leich\u00b7ter", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehst du mit den fremden Henkern", "tokens": ["Stehst", "du", "mit", "den", "frem\u00b7den", "Hen\u00b7kern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scherzend gegen mich im Bunde?\u00ab \u2013", "tokens": ["Scher\u00b7zend", "ge\u00b7gen", "mich", "im", "Bun\u00b7de", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbd\u00fcnken dich, mein zarter Knabe,", "tokens": ["\u00bb", "d\u00fcn\u00b7ken", "dich", ",", "mein", "zar\u00b7ter", "Kna\u00b7be", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon des M\u00e4dchens Worte furchtbar? \u2013", "tokens": ["Schon", "des", "M\u00e4d\u00b7chens", "Wor\u00b7te", "furcht\u00b7bar", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh den Franken! \u2013 willst du Schutz nicht", "tokens": ["Sieh", "den", "Fran\u00b7ken", "!", "\u2013", "willst", "du", "Schutz", "nicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "$(", "VMFIN", "PPER", "NN", "PTKNEG"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter meinem Mantel suchen?\u00ab \u2013", "tokens": ["Un\u00b7ter", "mei\u00b7nem", "Man\u00b7tel", "su\u00b7chen", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "\u00bbunverhohlen, was begehrst du?", "tokens": ["\u00bb", "un\u00b7ver\u00b7hoh\u00b7len", ",", "was", "be\u00b7gehrst", "du", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh ich solche Schmach erdulde,", "tokens": ["Eh", "ich", "sol\u00b7che", "Schmach", "er\u00b7dul\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich jede Tat begehen,", "tokens": ["Will", "ich", "je\u00b7de", "Tat", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gehen selber dann zu Grunde!\u00ab \u2013", "tokens": ["Ge\u00b7hen", "sel\u00b7ber", "dann", "zu", "Grun\u00b7de", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbdieser kommt im Glanz der Waffen", "tokens": ["\u00bb", "die\u00b7ser", "kommt", "im", "Glanz", "der", "Waf\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vertrauet seiner Jugend;", "tokens": ["Und", "ver\u00b7trau\u00b7et", "sei\u00b7ner", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist ein Spanier du, beweis es, \u2013", "tokens": ["Bist", "ein", "Spa\u00b7nier", "du", ",", "be\u00b7weis", "es", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder mit dem stolzen Buben!\u00ab \u2013", "tokens": ["Nie\u00b7der", "mit", "dem", "stol\u00b7zen", "Bu\u00b7ben", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Aber r\u00f6chelnd lag der fremde", "tokens": ["A\u00b7ber", "r\u00f6\u00b7chelnd", "lag", "der", "frem\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krieger schon in seinem Blute;", "tokens": ["Krie\u00b7ger", "schon", "in", "sei\u00b7nem", "Blu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schergen holten ein den T\u00e4ter,", "tokens": ["Scher\u00b7gen", "hol\u00b7ten", "ein", "den", "T\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brachten ihn daher gebunden.", "tokens": ["Brach\u00b7ten", "ihn", "da\u00b7her", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und das M\u00e4dchen sang frohlockend:", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "sang", "froh\u00b7lo\u00b7ckend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbdiesmal ist es mir gelungen!", "tokens": ["\u00bb", "dies\u00b7mal", "ist", "es", "mir", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines Toren werd ich ledig,", "tokens": ["Ei\u00b7nes", "To\u00b7ren", "werd", "ich", "le\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Franke zahlt die Bu\u00dfe.\u00ab", "tokens": ["Und", "der", "Fran\u00b7ke", "zahlt", "die", "Bu\u00b7\u00dfe", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Diese Worte h\u00f6rt der Spanier,", "tokens": ["Die\u00b7se", "Wor\u00b7te", "h\u00f6rt", "der", "Spa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Winket schweigsam seiner Buhlen,", "tokens": ["Win\u00b7ket", "schweig\u00b7sam", "sei\u00b7ner", "Buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziehet schweigsam dann vor\u00fcber,", "tokens": ["Zie\u00b7het", "schweig\u00b7sam", "dann", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Finstern Sinnes, kecken Mutes. \u2013", "tokens": ["Fins\u00b7tern", "Sin\u00b7nes", ",", "ke\u00b7cken", "Mu\u00b7tes", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "\u00bbnicht ihr, Franken, gebt den Tod mir,", "tokens": ["\u00bb", "nicht", "ihr", ",", "Fran\u00b7ken", ",", "gebt", "den", "Tod", "mir", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht um S\u00fchne mu\u00df ich bluten,", "tokens": ["Nicht", "um", "S\u00fch\u00b7ne", "mu\u00df", "ich", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich Spaniens Boden schm\u00fcckte", "tokens": ["Weil", "ich", "Spa\u00b7ni\u00b7ens", "Bo\u00b7den", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem ihm verfallnen Purpur.", "tokens": ["Mit", "dem", "ihm", "ver\u00b7fall\u00b7nen", "Pur\u00b7pur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.13": {"line.1": {"text": "Nein, ich trag in meinem Herzen", "tokens": ["Nein", ",", "ich", "trag", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigsam schon die Todeswunde;", "tokens": ["Schweig\u00b7sam", "schon", "die", "To\u00b7des\u00b7wun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Herrin hat gerichtet,", "tokens": ["Mei\u00b7ne", "Her\u00b7rin", "hat", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Stunde hat gerufen!\u00ab \u2013", "tokens": ["Mei\u00b7ne", "Stun\u00b7de", "hat", "ge\u00b7ru\u00b7fen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Also sang er vor der Fronte,", "tokens": ["Al\u00b7so", "sang", "er", "vor", "der", "Fron\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die Augen ihm verbunden;", "tokens": ["Als", "die", "Au\u00b7gen", "ihm", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Wink des F\u00fchrers sank er,", "tokens": ["Auf", "den", "Wink", "des", "F\u00fch\u00b7rers", "sank", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Herzen sieben Kugeln.", "tokens": ["In", "dem", "Her\u00b7zen", "sie\u00b7ben", "Ku\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "\u00bbwillst, ein Schlechter unter Schlechten,", "tokens": ["\u00bb", "willst", ",", "ein", "Schlech\u00b7ter", "un\u00b7ter", "Schlech\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Um die Spanierin du buhlen?", "tokens": ["Um", "die", "Spa\u00b7ni\u00b7e\u00b7rin", "du", "buh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Girrend zu der Laute singst du,", "tokens": ["Gir\u00b7rend", "zu", "der", "Lau\u00b7te", "singst", "du", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Und der Franke h\u00e4lt die Runde.", "tokens": ["Und", "der", "Fran\u00b7ke", "h\u00e4lt", "die", "Run\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Geht, ich kenn euch, Taubenherzen!", "tokens": ["Geht", ",", "ich", "kenn", "euch", ",", "Tau\u00b7ben\u00b7her\u00b7zen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht, ich kenn euch, Andalusier!", "tokens": ["Geht", ",", "ich", "kenn", "euch", ",", "An\u00b7da\u00b7lu\u00b7sier", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Euch die Spindel, uns die Waffen,", "tokens": ["Euch", "die", "Spin\u00b7del", ",", "uns", "die", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Besser st\u00e4nd's mit Spaniens Ruhme!", "tokens": ["Bes\u00b7ser", "st\u00e4n\u00b7d's", "mit", "Spa\u00b7ni\u00b7ens", "Ruh\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.17": {"line.1": {"text": "Regen sich in ihrer Scheide", "tokens": ["Re\u00b7gen", "sich", "in", "ih\u00b7rer", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.2": {"text": "Eure Messer ungeduldig", "tokens": ["Eu\u00b7re", "Mes\u00b7ser", "un\u00b7ge\u00b7dul\u00b7dig"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durstend nach dem Blut der Fremden,", "tokens": ["Durs\u00b7tend", "nach", "dem", "Blut", "der", "Frem\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprecht ihr zu dem Eisen: ruhig!", "tokens": ["Sprecht", "ihr", "zu", "dem", "Ei\u00b7sen", ":", "ru\u00b7hig", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "O der \u00fcberm\u00fct'gen Fremden!", "tokens": ["O", "der", "\u00fc\u00b7ber\u00b7m\u00fct'\u00b7gen", "Frem\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber euch sei ihre Rute,", "tokens": ["\u00dc\u00b7ber", "euch", "sei", "ih\u00b7re", "Ru\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber euch, ihr feigen Knechte,", "tokens": ["\u00dc\u00b7ber", "euch", ",", "ihr", "fei\u00b7gen", "Knech\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrdig solcher Nebenbuhler!\u00ab \u2013", "tokens": ["W\u00fcr\u00b7dig", "sol\u00b7cher", "Ne\u00b7ben\u00b7buh\u00b7ler", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "PIAT", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "\u00bbherrin, Worte schweren Inhalts", "tokens": ["\u00bb", "her\u00b7rin", ",", "Wor\u00b7te", "schwe\u00b7ren", "In\u00b7halts"], "token_info": ["punct", "word", "punct", "word", "word", "word"], "pos": ["$(", "PTKVZ", "$,", "NN", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sprichst du aus mit leichter Zunge,", "tokens": ["Sprichst", "du", "aus", "mit", "leich\u00b7ter", "Zun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehst du mit den fremden Henkern", "tokens": ["Stehst", "du", "mit", "den", "frem\u00b7den", "Hen\u00b7kern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scherzend gegen mich im Bunde?\u00ab \u2013", "tokens": ["Scher\u00b7zend", "ge\u00b7gen", "mich", "im", "Bun\u00b7de", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADJD", "APPR", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "\u00bbd\u00fcnken dich, mein zarter Knabe,", "tokens": ["\u00bb", "d\u00fcn\u00b7ken", "dich", ",", "mein", "zar\u00b7ter", "Kna\u00b7be", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon des M\u00e4dchens Worte furchtbar? \u2013", "tokens": ["Schon", "des", "M\u00e4d\u00b7chens", "Wor\u00b7te", "furcht\u00b7bar", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ART", "NN", "NN", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh den Franken! \u2013 willst du Schutz nicht", "tokens": ["Sieh", "den", "Fran\u00b7ken", "!", "\u2013", "willst", "du", "Schutz", "nicht"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "$.", "$(", "VMFIN", "PPER", "NN", "PTKNEG"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter meinem Mantel suchen?\u00ab \u2013", "tokens": ["Un\u00b7ter", "mei\u00b7nem", "Man\u00b7tel", "su\u00b7chen", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "\u00bbunverhohlen, was begehrst du?", "tokens": ["\u00bb", "un\u00b7ver\u00b7hoh\u00b7len", ",", "was", "be\u00b7gehrst", "du", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PWS", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh ich solche Schmach erdulde,", "tokens": ["Eh", "ich", "sol\u00b7che", "Schmach", "er\u00b7dul\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Will ich jede Tat begehen,", "tokens": ["Will", "ich", "je\u00b7de", "Tat", "be\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gehen selber dann zu Grunde!\u00ab \u2013", "tokens": ["Ge\u00b7hen", "sel\u00b7ber", "dann", "zu", "Grun\u00b7de", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ADV", "ADV", "APPR", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "\u00bbdieser kommt im Glanz der Waffen", "tokens": ["\u00bb", "die\u00b7ser", "kommt", "im", "Glanz", "der", "Waf\u00b7fen"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PDS", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und vertrauet seiner Jugend;", "tokens": ["Und", "ver\u00b7trau\u00b7et", "sei\u00b7ner", "Ju\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bist ein Spanier du, beweis es, \u2013", "tokens": ["Bist", "ein", "Spa\u00b7nier", "du", ",", "be\u00b7weis", "es", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER", "$,", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nieder mit dem stolzen Buben!\u00ab \u2013", "tokens": ["Nie\u00b7der", "mit", "dem", "stol\u00b7zen", "Bu\u00b7ben", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.23": {"line.1": {"text": "Aber r\u00f6chelnd lag der fremde", "tokens": ["A\u00b7ber", "r\u00f6\u00b7chelnd", "lag", "der", "frem\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krieger schon in seinem Blute;", "tokens": ["Krie\u00b7ger", "schon", "in", "sei\u00b7nem", "Blu\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schergen holten ein den T\u00e4ter,", "tokens": ["Scher\u00b7gen", "hol\u00b7ten", "ein", "den", "T\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Brachten ihn daher gebunden.", "tokens": ["Brach\u00b7ten", "ihn", "da\u00b7her", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "Und das M\u00e4dchen sang frohlockend:", "tokens": ["Und", "das", "M\u00e4d\u00b7chen", "sang", "froh\u00b7lo\u00b7ckend", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbdiesmal ist es mir gelungen!", "tokens": ["\u00bb", "dies\u00b7mal", "ist", "es", "mir", "ge\u00b7lun\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eines Toren werd ich ledig,", "tokens": ["Ei\u00b7nes", "To\u00b7ren", "werd", "ich", "le\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Franke zahlt die Bu\u00dfe.\u00ab", "tokens": ["Und", "der", "Fran\u00b7ke", "zahlt", "die", "Bu\u00b7\u00dfe", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Diese Worte h\u00f6rt der Spanier,", "tokens": ["Die\u00b7se", "Wor\u00b7te", "h\u00f6rt", "der", "Spa\u00b7nier", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Winket schweigsam seiner Buhlen,", "tokens": ["Win\u00b7ket", "schweig\u00b7sam", "sei\u00b7ner", "Buh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ziehet schweigsam dann vor\u00fcber,", "tokens": ["Zie\u00b7het", "schweig\u00b7sam", "dann", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Finstern Sinnes, kecken Mutes. \u2013", "tokens": ["Fins\u00b7tern", "Sin\u00b7nes", ",", "ke\u00b7cken", "Mu\u00b7tes", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["NN", "NE", "$,", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.26": {"line.1": {"text": "\u00bbnicht ihr, Franken, gebt den Tod mir,", "tokens": ["\u00bb", "nicht", "ihr", ",", "Fran\u00b7ken", ",", "gebt", "den", "Tod", "mir", ","], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PPER", "$,", "NN", "$,", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht um S\u00fchne mu\u00df ich bluten,", "tokens": ["Nicht", "um", "S\u00fch\u00b7ne", "mu\u00df", "ich", "blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Weil ich Spaniens Boden schm\u00fcckte", "tokens": ["Weil", "ich", "Spa\u00b7ni\u00b7ens", "Bo\u00b7den", "schm\u00fcck\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NE", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit dem ihm verfallnen Purpur.", "tokens": ["Mit", "dem", "ihm", "ver\u00b7fall\u00b7nen", "Pur\u00b7pur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.27": {"line.1": {"text": "Nein, ich trag in meinem Herzen", "tokens": ["Nein", ",", "ich", "trag", "in", "mei\u00b7nem", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweigsam schon die Todeswunde;", "tokens": ["Schweig\u00b7sam", "schon", "die", "To\u00b7des\u00b7wun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meine Herrin hat gerichtet,", "tokens": ["Mei\u00b7ne", "Her\u00b7rin", "hat", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine Stunde hat gerufen!\u00ab \u2013", "tokens": ["Mei\u00b7ne", "Stun\u00b7de", "hat", "ge\u00b7ru\u00b7fen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.28": {"line.1": {"text": "Also sang er vor der Fronte,", "tokens": ["Al\u00b7so", "sang", "er", "vor", "der", "Fron\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als die Augen ihm verbunden;", "tokens": ["Als", "die", "Au\u00b7gen", "ihm", "ver\u00b7bun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf den Wink des F\u00fchrers sank er,", "tokens": ["Auf", "den", "Wink", "des", "F\u00fch\u00b7rers", "sank", "er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem Herzen sieben Kugeln.", "tokens": ["In", "dem", "Her\u00b7zen", "sie\u00b7ben", "Ku\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}