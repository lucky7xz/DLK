{"dta.poem.18960": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "38.  \n  All Glick gut.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-200905198111", "language": ["de:0.99"], "booktitle": "Weckherlin, Georg Rodolf: Gaistliche und Weltliche Gedichte. Amsterdam, 1641."}, "poem": {"stanza.1": {"line.1": {"text": "Das Glick ist allen gleich vnd gut/", "tokens": ["Das", "Glick", "ist", "al\u00b7len", "gleich", "vnd", "gut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "ADV", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ja auch best\u00e4ndig heut vnd morgen.", "tokens": ["Ja", "auch", "be\u00b7st\u00e4n\u00b7dig", "heut", "vnd", "mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJD", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Reichen gibt es forcht/ m\u00fch/ sorgen;", "tokens": ["Den", "Rei\u00b7chen", "gibt", "es", "forcht", "/", "m\u00fch", "/", "sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$(", "ADJD", "$(", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Armen hofnung/ sinn vnd muht.", "tokens": ["Den", "Ar\u00b7men", "hof\u00b7nung", "/", "sinn", "vnd", "muht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}