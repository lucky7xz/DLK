{"textgrid.poem.64872": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn ich, inde\u00df in D\u00e4mmerungen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich, inde\u00df in D\u00e4mmerungen", "tokens": ["Wenn", "ich", ",", "in\u00b7de\u00df", "in", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschwimmt des Abendhimmels Glut,", "tokens": ["Ver\u00b7schwimmt", "des", "A\u00b7bend\u00b7him\u00b7mels", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deinem Arme liebumschlungen", "tokens": ["Von", "dei\u00b7nem", "Ar\u00b7me", "lie\u00b7bum\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hinschiffe auf der dunkeln Fluth;", "tokens": ["Hin\u00b7schif\u00b7fe", "auf", "der", "dun\u00b7keln", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn von der hohen Geistersonne,", "tokens": ["Wenn", "von", "der", "ho\u00b7hen", "Geis\u00b7ter\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die mystisch dein Gem\u00fcth erhellt,", "tokens": ["Die", "mys\u00b7tisch", "dein", "Ge\u00b7m\u00fcth", "er\u00b7hellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Strahl als \u00fcberird'sche Wonne", "tokens": ["Ein", "Strahl", "als", "\u00fc\u00b7ber\u00b7ird'\u00b7sche", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KOUS", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meine d\u00fcstre Seele f\u00e4llt;", "tokens": ["In", "mei\u00b7ne", "d\u00fcst\u00b7re", "See\u00b7le", "f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn mitten in den Fr\u00fchlingsscherzen,", "tokens": ["Wenn", "mit\u00b7ten", "in", "den", "Fr\u00fch\u00b7lings\u00b7scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Womit das Jetzt uns hold umwebt,", "tokens": ["Wo\u00b7mit", "das", "Jetzt", "uns", "hold", "um\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Schatten von vergangnen Schmerzen", "tokens": ["Ein", "Schat\u00b7ten", "von", "ver\u00b7gang\u00b7nen", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um deine sch\u00f6ne Stirne schwebt;", "tokens": ["Um", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Stir\u00b7ne", "schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn ich in deinem tiefsten Wesen,", "tokens": ["Wenn", "ich", "in", "dei\u00b7nem", "tiefs\u00b7ten", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von mir allein erforscht, erkannt,", "tokens": ["Von", "mir", "al\u00b7lein", "er\u00b7forscht", ",", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Desselben Schicksals Spur kann lesen,", "tokens": ["Des\u00b7sel\u00b7ben", "Schick\u00b7sals", "Spur", "kann", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das meinen Busen wund gebrannt;", "tokens": ["Das", "mei\u00b7nen", "Bu\u00b7sen", "wund", "ge\u00b7brannt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wenn du, vergessend deiner Loose,", "tokens": ["Wenn", "du", ",", "ver\u00b7ges\u00b7send", "dei\u00b7ner", "Loo\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit meinen dich betr\u00fcbst und freust,", "tokens": ["Mit", "mei\u00b7nen", "dich", "be\u00b7tr\u00fcbst", "und", "freust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und deines Hoffens sch\u00f6nste Rose", "tokens": ["Und", "dei\u00b7nes", "Hof\u00b7fens", "sch\u00f6ns\u00b7te", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In meines Lebens Abgrund streust:", "tokens": ["In", "mei\u00b7nes", "Le\u00b7bens", "Ab\u00b7grund", "streust", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da sinkt die letzte, dunkle Schranke;", "tokens": ["Da", "sinkt", "die", "letz\u00b7te", ",", "dunk\u00b7le", "Schran\u00b7ke", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein inn'rer Zwiespalt ist vers\u00f6hnt,", "tokens": ["Mein", "inn'\u00b7rer", "Zwies\u00b7palt", "ist", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und aufw\u00e4rts ringt sich mein Gedanke,", "tokens": ["Und", "auf\u00b7w\u00e4rts", "ringt", "sich", "mein", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Sang durch Morgenluft get\u00f6nt!", "tokens": ["Wie", "Sang", "durch", "Mor\u00b7gen\u00b7luft", "ge\u00b7t\u00f6nt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da wird von tausend Seligkeiten", "tokens": ["Da", "wird", "von", "tau\u00b7send", "Se\u00b7lig\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein staunend Herz erfa\u00dft, entf\u00fchrt,", "tokens": ["Mein", "stau\u00b7nend", "Herz", "er\u00b7fa\u00dft", ",", "ent\u00b7f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und klingt wie einer Lyra Saiten,", "tokens": ["Und", "klingt", "wie", "ei\u00b7ner", "Ly\u00b7ra", "Sai\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn sie geweihte Hand ber\u00fchrt!", "tokens": ["Wenn", "sie", "ge\u00b7weih\u00b7te", "Hand", "be\u00b7r\u00fchrt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da f\u00fchl' ich, was der Meereswelle", "tokens": ["Da", "f\u00fchl'", "ich", ",", "was", "der", "Mee\u00b7res\u00b7wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Mondenstrahls geheimer Ku\u00df,", "tokens": ["Des", "Mon\u00b7dens\u00b7trahls", "ge\u00b7hei\u00b7mer", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und was den Bienen ihre Zelle,", "tokens": ["Und", "was", "den", "Bie\u00b7nen", "ih\u00b7re", "Zel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Bergen ist der Sonne Gru\u00df;", "tokens": ["Den", "Ber\u00b7gen", "ist", "der", "Son\u00b7ne", "Gru\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und was dem Wandrer durch die W\u00fcste", "tokens": ["Und", "was", "dem", "Wand\u00b7rer", "durch", "die", "W\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Quell, auf den er d\u00fcrstend trifft,", "tokens": ["Ein", "Quell", ",", "auf", "den", "er", "d\u00fcrs\u00b7tend", "trifft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was m\u00fcdem Segler ist die K\u00fcste,", "tokens": ["Was", "m\u00fc\u00b7dem", "Seg\u00b7ler", "ist", "die", "K\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem das Weltmeer er durchschifft;", "tokens": ["Nach\u00b7dem", "das", "Welt\u00b7meer", "er", "durch\u00b7schifft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Was einem Weib im Wittwenschleier", "tokens": ["Was", "ei\u00b7nem", "Weib", "im", "Witt\u00b7wensc\u00b7hlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kind, das ihres Lebens Zier,", "tokens": ["Das", "Kind", ",", "das", "ih\u00b7res", "Le\u00b7bens", "Zier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was einem Sclaven sein Befreier,", "tokens": ["Was", "ei\u00b7nem", "Scla\u00b7ven", "sein", "Be\u00b7frei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mehr noch ist dein Lieben mir!", "tokens": ["Und", "mehr", "noch", "ist", "dein", "Lie\u00b7ben", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "PPOSAT", "ADJA", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wenn ich, inde\u00df in D\u00e4mmerungen", "tokens": ["Wenn", "ich", ",", "in\u00b7de\u00df", "in", "D\u00e4m\u00b7me\u00b7run\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verschwimmt des Abendhimmels Glut,", "tokens": ["Ver\u00b7schwimmt", "des", "A\u00b7bend\u00b7him\u00b7mels", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von deinem Arme liebumschlungen", "tokens": ["Von", "dei\u00b7nem", "Ar\u00b7me", "lie\u00b7bum\u00b7schlun\u00b7gen"]