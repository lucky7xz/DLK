{"dta.poem.7274": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Steppe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Standest du je am Strande,", "tokens": ["Stan\u00b7dest", "du", "je", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wenn Tag und Nacht sich gleichen,", "tokens": ["Wenn", "Tag", "und", "Nacht", "sich", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sah'st aus Lehm und Sande", "tokens": ["Und", "sah'st", "aus", "Lehm", "und", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Regenrinnen schleichen \u2014", "tokens": ["Die", "Re\u00b7gen\u00b7rin\u00b7nen", "schlei\u00b7chen"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zahllose Schmugglerquellen,", "tokens": ["Zahl\u00b7lo\u00b7se", "Schmugg\u00b7ler\u00b7quel\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Und dann, so weit das Auge", "tokens": ["Und", "dann", ",", "so", "weit", "das", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Nur reicht, des Meeres Wellen", "tokens": ["Nur", "reicht", ",", "des", "Mee\u00b7res", "Wel\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gef\u00e4rbt mit gelber Lauge? \u2014", "tokens": ["Ge\u00b7f\u00e4rbt", "mit", "gel\u00b7ber", "Lau\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Hier ist die D\u00fcn' und drunten", "tokens": ["Hier", "ist", "die", "D\u00fcn'", "und", "drun\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Meer; Kanonen gleichend", "tokens": ["Das", "Meer", ";", "Ka\u00b7no\u00b7nen", "glei\u00b7chend"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$.", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stehn Sch\u00e4ferkarrn, die Lunten", "tokens": ["Stehn", "Sch\u00e4\u00b7fer\u00b7karrn", ",", "die", "Lun\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verl\u00f6scht am Boden streichend.", "tokens": ["Ver\u00b7l\u00f6scht", "am", "Bo\u00b7den", "strei\u00b7chend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gilt's etwa dem Korsaren", "tokens": ["Gilt's", "et\u00b7wa", "dem", "Kor\u00b7sa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Im flatternden Kaftane,", "tokens": ["Im", "flat\u00b7tern\u00b7den", "Kaf\u00b7ta\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den dort ich kann gewahren", "tokens": ["Den", "dort", "ich", "kann", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Im gelben Oceane?", "tokens": ["Im", "gel\u00b7ben", "O\u00b7cea\u00b7ne", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Er scheint das Tau zu schlagen,", "tokens": ["Er", "scheint", "das", "Tau", "zu", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Schiff verdeckt die D\u00fcne,", "tokens": ["Sein", "Schiff", "ver\u00b7deckt", "die", "D\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch sieht den Mast man ragen, \u2014", "tokens": ["Doch", "sieht", "den", "Mast", "man", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PIS", "VVINF", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein d\u00fcrrer Fichtenh\u00fcne;", "tokens": ["Ein", "d\u00fcr\u00b7rer", "Fich\u00b7ten\u00b7h\u00fc\u00b7ne", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von seines Toppes Kunkel", "tokens": ["Von", "sei\u00b7nes", "Top\u00b7pes", "Kun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die Seile stramm wie Aeste,", "tokens": ["Die", "Sei\u00b7le", "stramm", "wie", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Mastkorb, rauh und dunkel,", "tokens": ["Der", "Mast\u00b7korb", ",", "rauh", "und", "dun\u00b7kel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gleicht einem Weihenneste! \u2014", "tokens": ["Gleicht", "ei\u00b7nem", "Wei\u00b7hen\u00b7nes\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}