{"dta.poem.2743": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Leyd-tragenden Cypressen/  \n Bey Beerdigung Hn. C. S.   LL. S.   den 5.  \n Decembr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Leichen-Schm\u00fcck/ ihr dunckelen Cypressen/", "tokens": ["Ihr", "Lei\u00b7chen\u00b7Schm\u00fcck", "/", "ihr", "dun\u00b7cke\u00b7len", "Cyp\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jhr Kronen ihr/ die nasse Wehmuth flicht/", "tokens": ["Ihr", "Kro\u00b7nen", "ihr", "/", "die", "nas\u00b7se", "Weh\u00b7muth", "flicht", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Jhr Zeugen ihr/ da\u00df nicht das Leyd vergessen/", "tokens": ["Ihr", "Zeu\u00b7gen", "ihr", "/", "da\u00df", "nicht", "das", "Leyd", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "$(", "KOUS", "PTKNEG", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Anwalde/ wenn der Tod sein Urtheil spricht;", "tokens": ["An\u00b7wal\u00b7de", "/", "wenn", "der", "Tod", "sein", "Ur\u00b7theil", "spricht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Euch forder ich di\u00df Grab noch zu begleiten/", "tokens": ["Euch", "for\u00b7der", "ich", "di\u00df", "Grab", "noch", "zu", "be\u00b7glei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDS", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie ihr selbst von Trauren seyd erblast/", "tokens": ["Und", "wie", "ihr", "selbst", "von", "Trau\u00b7ren", "seyd", "er\u00b7blast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So schont auch nicht die Zweig\u2019 jetzt auszubreiten", "tokens": ["So", "schont", "auch", "nicht", "die", "Zweig'", "jetzt", "aus\u00b7zu\u00b7brei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PTKNEG", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und pflantzt hieher den ausgedorrten Ast.", "tokens": ["Und", "pflantzt", "hie\u00b7her", "den", "aus\u00b7ge\u00b7dorr\u00b7ten", "Ast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie taumelt ihr/ ihr schwancken Cyparissen?", "tokens": ["Wie", "tau\u00b7melt", "ihr", "/", "ihr", "schwan\u00b7cken", "Cy\u00b7pa\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedenckt ihr noch wer ihr gewesen seyd?", "tokens": ["Ge\u00b7denckt", "ihr", "noch", "wer", "ihr", "ge\u00b7we\u00b7sen", "seyd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "PPER", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Eh als euch pflag die Rinde zu beschliessen/", "tokens": ["Eh", "als", "euch", "pflag", "die", "Rin\u00b7de", "zu", "be\u00b7schlies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hie\u00df Ph\u00f6bus euch nicht seine Lust und Freud?", "tokens": ["Hie\u00df", "Ph\u00f6\u00b7bus", "euch", "nicht", "sei\u00b7ne", "Lust", "und", "Freud", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPER", "PTKNEG", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Hirsch den du O Cypari\u00df geliebet/", "tokens": ["Der", "Hirsch", "den", "du", "O", "Cy\u00b7pa\u00b7ri\u00df", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "NE", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und auch zugleich durch deinen Pfeil verwundt/", "tokens": ["Und", "auch", "zu\u00b7gleich", "durch", "dei\u00b7nen", "Pfeil", "ver\u00b7wundt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der hat dich so bi\u00df auff den Tod betr\u00fcbet/", "tokens": ["Der", "hat", "dich", "so", "bi\u00df", "auff", "den", "Tod", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df drauff dein Leib in Baum verkehret stund.", "tokens": ["Da\u00df", "drauff", "dein", "Leib", "in", "Baum", "ver\u00b7keh\u00b7ret", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PAV", "PPOSAT", "NN", "APPR", "NE", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Di\u00df Sinnen-Bild Leyd-tragende Cypressen/", "tokens": ["Di\u00df", "Sin\u00b7nen\u00b7Bild", "Ley\u00b7dtra\u00b7gen\u00b7de", "Cyp\u00b7res\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Entwirfft uns auch des Lebens Eitelkeit.", "tokens": ["Ent\u00b7wirfft", "uns", "auch", "des", "Le\u00b7bens", "Ei\u00b7tel\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was uns ergetzt/ wobey wir offt gesessen/", "tokens": ["Was", "uns", "er\u00b7getzt", "/", "wo\u00b7bey", "wir", "offt", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$(", "KOUS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was uns gebracht die angenehmste Freud/", "tokens": ["Was", "uns", "ge\u00b7bracht", "die", "an\u00b7ge\u00b7nehms\u00b7te", "Freud", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das wird hernach viel hefftiger verletzen.", "tokens": ["Das", "wird", "her\u00b7nach", "viel", "heff\u00b7ti\u00b7ger", "ver\u00b7let\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die gr\u00f6ste Lust beschleust ein herbes Weh;", "tokens": ["Die", "gr\u00f6s\u00b7te", "Lust", "be\u00b7schleust", "ein", "her\u00b7bes", "Weh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und was die Welt nur zeigt von ihren Sch\u00e4tzen", "tokens": ["Und", "was", "die", "Welt", "nur", "zeigt", "von", "ih\u00b7ren", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "ART", "NN", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das ist nichts mehr als ein verg\u00e4nglich Schnee.", "tokens": ["Das", "ist", "nichts", "mehr", "als", "ein", "ver\u00b7g\u00e4ng\u00b7lich", "Schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "PIS", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie/ oder seyd ihr Gratien gewesen/", "tokens": ["Wie", "/", "o\u00b7der", "seyd", "ihr", "Gra\u00b7ti\u00b7en", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KON", "VAFIN", "PPOSAT", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Voll Lieblichkeit/ voll Anmuth/ voller Wonn\u2019/", "tokens": ["Voll", "Lieb\u00b7lich\u00b7keit", "/", "voll", "An\u00b7muth", "/", "vol\u00b7ler", "Wonn'", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$(", "ADJD", "NN", "$(", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie das Buch der Zeiten gibt zu lesen/", "tokens": ["Und", "wie", "das", "Buch", "der", "Zei\u00b7ten", "gibt", "zu", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In einem Tantz verfallen in den Bronn", "tokens": ["In", "ei\u00b7nem", "Tantz", "ver\u00b7fal\u00b7len", "in", "den", "Bronn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und den versetzt in blasse Str\u00e4ucher worden?", "tokens": ["Und", "den", "ver\u00b7setzt", "in", "blas\u00b7se", "Str\u00e4u\u00b7cher", "wor\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "APPR", "ADJA", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So stellt ihr doch des Todes Rasen f\u00fcr.", "tokens": ["So", "stellt", "ihr", "doch", "des", "To\u00b7des", "Ra\u00b7sen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "APPR", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein Grimm der f\u00e4hrt durch aller Menschen Orden", "tokens": ["Sein", "Grimm", "der", "f\u00e4hrt", "durch", "al\u00b7ler", "Men\u00b7schen", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und raubt zu erst der Jugend beste Zier.", "tokens": ["Und", "raubt", "zu", "erst", "der", "Ju\u00b7gend", "bes\u00b7te", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Herr Seelmann den wir mit Cypressen decken/", "tokens": ["Herr", "Seel\u00b7mann", "den", "wir", "mit", "Cyp\u00b7res\u00b7sen", "de\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mu\u00df meiner Wort ein kl\u00e4glich Beyspiel seyn.", "tokens": ["Mu\u00df", "mei\u00b7ner", "Wort", "ein", "kl\u00e4g\u00b7lich", "Bey\u00b7spiel", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wir sehen ihn die Hand des Todes strecken/", "tokens": ["Wir", "se\u00b7hen", "ihn", "die", "Hand", "des", "To\u00b7des", "stre\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sein letztes Hau\u00df ist dieser schwartze Schrein.", "tokens": ["Sein", "letz\u00b7tes", "Hau\u00df", "ist", "die\u00b7ser", "schwart\u00b7ze", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Jugend Lentz/ die Geister-reichen Jahre/", "tokens": ["Der", "Ju\u00b7gend", "Lentz", "/", "die", "Geis\u00b7ter\u00b7rei\u00b7chen", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind vor dem Sto\u00df der Parten nicht befreyt.", "tokens": ["Sind", "vor", "dem", "Sto\u00df", "der", "Par\u00b7ten", "nicht", "be\u00b7freyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Man legt so bald die Jungen auff die Baare/", "tokens": ["Man", "legt", "so", "bald", "die", "Jun\u00b7gen", "auff", "die", "Baa\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als die gar tieff im Leben gehn beschneyt.", "tokens": ["Als", "die", "gar", "tieff", "im", "Le\u00b7ben", "gehn", "be\u00b7schneyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "APPRART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Cypressen seyd ihr nichts als Todes Zeichen/", "tokens": ["Cyp\u00b7res\u00b7sen", "seyd", "ihr", "nichts", "als", "To\u00b7des", "Zei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PIS", "KOKOM", "NN", "NN", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Heyden auch dem Pluto nur geweyht/", "tokens": ["Die", "Hey\u00b7den", "auch", "dem", "Plu\u00b7to", "nur", "ge\u00b7weyht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So mu\u00df ich bald aus Abscheu von euch weichen/", "tokens": ["So", "mu\u00df", "ich", "bald", "aus", "Ab\u00b7scheu", "von", "euch", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie/ oder lehrt ihr auch die Ewigkeit?", "tokens": ["Wie", "/", "o\u00b7der", "lehrt", "ihr", "auch", "die", "E\u00b7wig\u00b7keit", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KON", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ach freylich! ja den Stab den G\u00f6tter tragen/", "tokens": ["Ach", "frey\u00b7lich", "!", "ja", "den", "Stab", "den", "G\u00f6t\u00b7ter", "tra\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$.", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der war zu erst von eurem Holtz gemacht/", "tokens": ["Der", "war", "zu", "erst", "von", "eu\u00b7rem", "Holtz", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und weil ihr k\u00f6nt die Schlangen gantz verjagen/", "tokens": ["Und", "weil", "ihr", "k\u00f6nt", "die", "Schlan\u00b7gen", "gantz", "ver\u00b7ja\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "So hat euch hoch das Alterthum geacht.", "tokens": ["So", "hat", "euch", "hoch", "das", "Al\u00b7ter\u00b7thum", "ge\u00b7acht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}