{"text": "Aus hellgeschliffenem Kristall,", "tokens": ["Aus", "hell\u00b7ge\u00b7schlif\u00b7fe\u00b7nem", "Kris\u00b7tall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wenn der Abend naht, den leichten Rausch zu enden,", "tokens": ["Und", "wenn", "der", "A\u00b7bend", "naht", ",", "den", "leich\u00b7ten", "Rausch", "zu", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sei sie dir gew\u00e4hrt, die Wollust, zu verschwenden", "tokens": ["So", "sei", "sie", "dir", "ge\u00b7w\u00e4hrt", ",", "die", "Wol\u00b7lust", ",", "zu", "ver\u00b7schwen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,", "ART", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bei Kartenspiel und W\u00fcrfelfall.", "tokens": ["Bei", "Kar\u00b7ten\u00b7spiel", "und", "W\u00fcr\u00b7fel\u00b7fall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du sollst auf pr\u00e4cht'gem Ball, wenn tausend Kerzen funkeln,", "tokens": ["Du", "sollst", "auf", "pr\u00e4cht'\u00b7gem", "Ball", ",", "wenn", "tau\u00b7send", "Ker\u00b7zen", "fun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "$,", "KOUS", "CARD", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit deiner reichen Tracht, mit deinem Wuchs verdunkeln", "tokens": ["Mit", "dei\u00b7ner", "rei\u00b7chen", "Tracht", ",", "mit", "dei\u00b7nem", "Wuchs", "ver\u00b7dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Kronbeamten stolzen Schwarm;", "tokens": ["Der", "Kron\u00b7be\u00b7am\u00b7ten", "stol\u00b7zen", "Schwarm", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Wellen der Musik sollst du dich jauchzend wiegen", "tokens": ["Auf", "Wel\u00b7len", "der", "Mu\u00b7sik", "sollst", "du", "dich", "jauch\u00b7zend", "wie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ART", "NN", "VMFIN", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und sporenklirrend durch den Saal im Tanze fliegen", "tokens": ["Und", "spo\u00b7renk\u00b7lir\u00b7rend", "durch", "den", "Saal", "im", "Tan\u00b7ze", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "An einer Kaisertochter Arm.", "tokens": ["An", "ei\u00b7ner", "Kai\u00b7ser\u00b7toch\u00b7ter", "Arm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Beim Lager sollst du schaun, wie sich im Flintenfeuer", "tokens": ["Beim", "La\u00b7ger", "sollst", "du", "schaun", ",", "wie", "sich", "im", "Flin\u00b7ten\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Regimenter drehn, vielf\u00fc\u00df'ge Ungeheuer,", "tokens": ["Die", "Re\u00b7gi\u00b7men\u00b7ter", "drehn", ",", "viel\u00b7f\u00fc\u00df'\u00b7ge", "Un\u00b7ge\u00b7heu\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf denen hoch die Fahne schwankt;", "tokens": ["Auf", "de\u00b7nen", "hoch", "die", "Fah\u00b7ne", "schwankt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Trommel wirbelt dumpf, das Feldhorn l\u00e4\u00dft sich h\u00f6ren,", "tokens": ["Die", "Trom\u00b7mel", "wir\u00b7belt", "dumpf", ",", "das", "Feld\u00b7horn", "l\u00e4\u00dft", "sich", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Batterie f\u00e4llt ein mit ihren Donnerch\u00f6ren,", "tokens": ["Die", "Bat\u00b7te\u00b7rie", "f\u00e4llt", "ein", "mit", "ih\u00b7ren", "Don\u00b7ner\u00b7ch\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df unter ihr der Boden wankt.", "tokens": ["Da\u00df", "un\u00b7ter", "ihr", "der", "Bo\u00b7den", "wankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ja, mehr der Wunder noch! Gro\u00df ist die Macht des Zaren;", "tokens": ["Ja", ",", "mehr", "der", "Wun\u00b7der", "noch", "!", "Gro\u00df", "ist", "die", "Macht", "des", "Za\u00b7ren", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "ADV", "$.", "NE", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Du sollst auf einem Schiff mit Doppelr\u00e4dern fahren,", "tokens": ["Du", "sollst", "auf", "ei\u00b7nem", "Schiff", "mit", "Dop\u00b7pel\u00b7r\u00e4\u00b7dern", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Von keines Tauwerks Last beschwert;", "tokens": ["Von", "kei\u00b7nes", "Tau\u00b7werks", "Last", "be\u00b7schwert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Strome beut es Trotz und Trotz dem Sturmgeheule,", "tokens": ["Dem", "Stro\u00b7me", "beut", "es", "Trotz", "und", "Trotz", "dem", "Sturm\u00b7ge\u00b7heu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn drin die Esse gl\u00fcht, und wenn aus schwarzer S\u00e4ule", "tokens": ["Wenn", "drin", "die", "Es\u00b7se", "gl\u00fcht", ",", "und", "wenn", "aus", "schwar\u00b7zer", "S\u00e4u\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,", "KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Gischt des Dampfes brausend f\u00e4hrt.", "tokens": ["Der", "Gischt", "des", "Damp\u00b7fes", "brau\u00b7send", "f\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Das alles bieten wir. Nur la\u00df die blut'gen Horden,", "tokens": ["Das", "al\u00b7les", "bie\u00b7ten", "wir", ".", "Nur", "la\u00df", "die", "blut'\u00b7gen", "Hor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "PPER", "$.", "ADV", "VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df Steppe, Krieg und Zelt; komm reuig her zum Norden", "tokens": ["La\u00df", "Step\u00b7pe", ",", "Krieg", "und", "Zelt", ";", "komm", "reu\u00b7ig", "her", "zum", "Nor\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "NN", "$,", "NN", "KON", "NN", "$.", "VVFIN", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und vor dem Herrscher beuge dich.\u00ab -", "tokens": ["Und", "vor", "dem", "Herr\u00b7scher", "beu\u00b7ge", "dich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber wandte mich bei ihrer Worte Hadern,", "tokens": ["Ich", "a\u00b7ber", "wand\u00b7te", "mich", "bei", "ih\u00b7rer", "Wor\u00b7te", "Ha\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es schwoll der rote Zorn empor in meinen Adern -", "tokens": ["Es", "schwoll", "der", "ro\u00b7te", "Zorn", "em\u00b7por", "in", "mei\u00b7nen", "A\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Zar ist nur ein F\u00fcrst wie ich.", "tokens": ["Der", "Zar", "ist", "nur", "ein", "F\u00fcrst", "wie", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Kasan hat seine Fraun, schneewei\u00df mit schwarzen Locken,", "tokens": ["Ka\u00b7san", "hat", "sei\u00b7ne", "Fraun", ",", "schnee\u00b7wei\u00df", "mit", "schwar\u00b7zen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Moskau hat seinen Kreml, und Kiew seine Glocken,", "tokens": ["Mos\u00b7kau", "hat", "sei\u00b7nen", "Kreml", ",", "und", "Kiew", "sei\u00b7ne", "Glo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "KON", "NE", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Petersburg hat mehr als das;", "tokens": ["Und", "Pe\u00b7ters\u00b7burg", "hat", "mehr", "als", "das", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "PIAT", "KOKOM", "PDS", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Doch b\u00f6ten sie mir auch die Wunder aller Fremde:", "tokens": ["Doch", "b\u00f6\u00b7ten", "sie", "mir", "auch", "die", "Wun\u00b7der", "al\u00b7ler", "Frem\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nicht k\u00e4uflich sind mir drum mein schuppig Panzerhemde", "tokens": ["Nicht", "k\u00e4uf\u00b7lich", "sind", "mir", "drum", "mein", "schup\u00b7pig", "Pan\u00b7zer\u00b7hem\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADJD", "VAFIN", "PPER", "PAV", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und meine Freiheit und mein Ha\u00df.", "tokens": ["Und", "mei\u00b7ne", "Frei\u00b7heit", "und", "mein", "Ha\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}