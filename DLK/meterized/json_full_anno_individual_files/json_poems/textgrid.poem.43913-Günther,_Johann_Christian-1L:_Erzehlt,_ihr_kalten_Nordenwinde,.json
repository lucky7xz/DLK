{"textgrid.poem.43913": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Erzehlt, ihr kalten Nordenwinde,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erzehlt, ihr kalten Nordenwinde,", "tokens": ["Er\u00b7zehlt", ",", "ihr", "kal\u00b7ten", "Nor\u00b7den\u00b7win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seufzer meiner Sch\u00e4ferin,", "tokens": ["Die", "Seuf\u00b7zer", "mei\u00b7ner", "Sch\u00e4\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verk\u00fcndigt dem verla\u00dfnen Kinde,", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "dem", "ver\u00b7la\u00df\u00b7nen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich der alte Redlich bin,", "tokens": ["Da\u00df", "ich", "der", "al\u00b7te", "Red\u00b7lich", "bin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und macht ihr diese Botschaft kund:", "tokens": ["Und", "macht", "ihr", "die\u00b7se", "Bot\u00b7schaft", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Herze liebet wie der Mund.", "tokens": ["Das", "Her\u00b7ze", "lie\u00b7bet", "wie", "der", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Des Monden Antliz sieht die Fluthen", "tokens": ["Des", "Mon\u00b7den", "Ant\u00b7liz", "sieht", "die", "Flut\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der stummen Wehmuth kl\u00e4glich an,", "tokens": ["Der", "stum\u00b7men", "Weh\u00b7muth", "kl\u00e4g\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Geister wollen mir verbluten,", "tokens": ["Die", "Geis\u00b7ter", "wol\u00b7len", "mir", "ver\u00b7blu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil ich mein Kind nicht sprechen kan;", "tokens": ["Weil", "ich", "mein", "Kind", "nicht", "spre\u00b7chen", "kan", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich dencke der vergangnen Zeit,", "tokens": ["Ich", "den\u00b7cke", "der", "ver\u00b7gang\u00b7nen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von der mich die Verschwendung reut.", "tokens": ["Von", "der", "mich", "die", "Ver\u00b7schwen\u00b7dung", "reut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Elbstrom f\u00e4llt mir in die Augen,", "tokens": ["Der", "E\u00b7lbstrom", "f\u00e4llt", "mir", "in", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Ei\u00df erhizet meinen Geist.", "tokens": ["Sein", "Ei\u00df", "er\u00b7hi\u00b7zet", "mei\u00b7nen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, k\u00f6nt ich jezt die K\u00fc\u00dfe saugen,", "tokens": ["Ach", ",", "k\u00f6nt", "ich", "jezt", "die", "K\u00fc\u00b7\u00dfe", "sau\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich zuvor vollauf gespeist,", "tokens": ["Die", "mich", "zu\u00b7vor", "vol\u00b7lauf", "ge\u00b7speist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wollte meinen Fu\u00df bem\u00fchn", "tokens": ["Ich", "woll\u00b7te", "mei\u00b7nen", "Fu\u00df", "be\u00b7m\u00fchn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und gar von hier nach Bre\u00dflau ziehn.", "tokens": ["Und", "gar", "von", "hier", "nach", "Bre\u00df\u00b7lau", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "APPR", "NE", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Geliebtes Schweidniz, das Vergn\u00fcgen,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Schweid\u00b7niz", ",", "das", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So mich bey dir im Schoo\u00dfe trug,", "tokens": ["So", "mich", "bey", "dir", "im", "Schoo\u00b7\u00dfe", "trug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wird nicht so bald mein Herz besiegen,", "tokens": ["Wird", "nicht", "so", "bald", "mein", "Herz", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das von der Wollust heftig schlug,", "tokens": ["Das", "von", "der", "Wol\u00b7lust", "hef\u00b7tig", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn die getreue Magdalis", "tokens": ["Wenn", "die", "ge\u00b7treu\u00b7e", "Mag\u00b7da\u00b7lis"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mich br\u00fcnstig in die Armen ri\u00df.", "tokens": ["Mich", "br\u00fcns\u00b7tig", "in", "die", "Ar\u00b7men", "ri\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Zeiten sind bereits gestorben;", "tokens": ["Die", "Zei\u00b7ten", "sind", "be\u00b7reits", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Drum fluch ich der Verg\u00e4ngligkeit,", "tokens": ["Drum", "fluch", "ich", "der", "Ver\u00b7g\u00e4n\u00b7glig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die mir und ihr den Schmerz erworben,", "tokens": ["Die", "mir", "und", "ihr", "den", "Schmerz", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KON", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den unser Abschied prophezeit,", "tokens": ["Den", "un\u00b7ser", "Ab\u00b7schied", "pro\u00b7phe\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da mich das werthe Sachsenland", "tokens": ["Da", "mich", "das", "wert\u00b7he", "Sach\u00b7sen\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von meiner Sch\u00e4ferin getrand.", "tokens": ["Von", "mei\u00b7ner", "Sch\u00e4\u00b7fe\u00b7rin", "ge\u00b7trand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": 