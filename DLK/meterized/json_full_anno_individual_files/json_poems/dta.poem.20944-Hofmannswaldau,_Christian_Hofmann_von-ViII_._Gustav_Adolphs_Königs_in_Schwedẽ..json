{"dta.poem.20944": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "ViII .  \n Gustav Adolphs  K\u00f6nigs in Schwede\u0303.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "So weit als ", "tokens": ["So", "weit", "als"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Hat meiner Thaten Glantz auch das Ger\u00fcchte bracht.", "tokens": ["Hat", "mei\u00b7ner", "Tha\u00b7ten", "Glantz", "auch", "das", "Ge\u00b7r\u00fcch\u00b7te", "bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vor L\u00fctzen hat mein Muth mich in das Gras gebeget/", "tokens": ["Vor", "L\u00fct\u00b7zen", "hat", "mein", "Muth", "mich", "in", "das", "Gras", "ge\u00b7be\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch fiel ich als ein Baum der tausend \u00e4ste schl\u00e4get.", "tokens": ["Doch", "fiel", "ich", "als", "ein", "Baum", "der", "tau\u00b7send", "\u00e4s\u00b7te", "schl\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "ART", "NN", "ART", "CARD", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}