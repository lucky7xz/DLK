{"textgrid.poem.43354": {"metadata": {"author": {"name": "Leopardi, Giacomo", "birth": "N.A.", "death": "N.A."}, "title": "XxI. An Silvia.", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Silvia, gedenkst du noch", "tokens": ["Sil\u00b7via", ",", "ge\u00b7denkst", "du", "noch"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "ADV"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "An jene Zeit in deinem Erdenleben,", "tokens": ["An", "je\u00b7ne", "Zeit", "in", "dei\u00b7nem", "Er\u00b7den\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als dir von Sch\u00f6nheit gl\u00e4nzte", "tokens": ["Als", "dir", "von", "Sch\u00f6n\u00b7heit", "gl\u00e4nz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein lachend Augenpaar in muntrer Helle", "tokens": ["Dein", "la\u00b7chend", "Au\u00b7gen\u00b7paar", "in", "mun\u00b7trer", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und du betratst, froh und gedankenvoll,", "tokens": ["Und", "du", "be\u00b7tratst", ",", "froh", "und", "ge\u00b7dan\u00b7ken\u00b7voll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Des Jungfraunalters Schwelle?", "tokens": ["Des", "Jung\u00b7frau\u00b7nal\u00b7ters", "Schwel\u00b7le", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Von fr\u00fch bis sp\u00e4t erklangen", "tokens": ["Von", "fr\u00fch", "bis", "sp\u00e4t", "er\u00b7klan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "APPR", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die stillen Zimmer und ringsum die Gassen", "tokens": ["Die", "stil\u00b7len", "Zim\u00b7mer", "und", "ring\u00b7sum", "die", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADV", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Von deinem hellen Singen,", "tokens": ["Von", "dei\u00b7nem", "hel\u00b7len", "Sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn bei der Arbeit eifrig ohne S\u00e4umen", "tokens": ["Wenn", "bei", "der", "Ar\u00b7beit", "eif\u00b7rig", "oh\u00b7ne", "S\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du sa\u00dfest und in Tr\u00e4umen", "tokens": ["Du", "sa\u00b7\u00dfest", "und", "in", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von sch\u00f6ner Zukunft fr\u00f6hlich war dein Sinn.", "tokens": ["Von", "sch\u00f6\u00b7ner", "Zu\u00b7kunft", "fr\u00f6h\u00b7lich", "war", "dein", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "S\u00fc\u00df duftete der Mai. So pflegtest du", "tokens": ["S\u00fc\u00df", "duf\u00b7te\u00b7te", "der", "Mai", ".", "So", "pfleg\u00b7test", "du"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Tage zu verbringen.", "tokens": ["Die", "Ta\u00b7ge", "zu", "ver\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dann meinen theuren B\u00fcchern", "tokens": ["Dann", "mei\u00b7nen", "theu\u00b7ren", "B\u00fc\u00b7chern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Abtr\u00fcnnig und den m\u00fchevollen Heften,", "tokens": ["Ab\u00b7tr\u00fcn\u00b7nig", "und", "den", "m\u00fc\u00b7he\u00b7vol\u00b7len", "Hef\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "An die ich fr\u00fch gewendet", "tokens": ["An", "die", "ich", "fr\u00fch", "ge\u00b7wen\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den besten Theil von meinen Jugendkr\u00e4ften,", "tokens": ["Den", "bes\u00b7ten", "Theil", "von", "mei\u00b7nen", "Ju\u00b7gend\u00b7kr\u00e4f\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie manchmal von des Vaterhauses S\u00f6ller", "tokens": ["Wie", "manch\u00b7mal", "von", "des", "Va\u00b7ter\u00b7hau\u00b7ses", "S\u00f6l\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Lauscht' ich auf deine Stimme unverwandt", "tokens": ["Lauscht'", "ich", "auf", "dei\u00b7ne", "Stim\u00b7me", "un\u00b7ver\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sp\u00e4hte nach der Hand,", "tokens": ["Und", "sp\u00e4h\u00b7te", "nach", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die flink das Linnen hin und her durchlief.", "tokens": ["Die", "flink", "das", "Lin\u00b7nen", "hin", "und", "her", "durch\u00b7lief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PTKVZ", "KON", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wie still die Luft sich k\u00fchlte!", "tokens": ["Wie", "still", "die", "Luft", "sich", "k\u00fchl\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Wie golden Weg' und G\u00e4rten,", "tokens": ["Wie", "gol\u00b7den", "Weg'", "und", "G\u00e4r\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Und hier das ferne Meer und dort die Berge!", "tokens": ["Und", "hier", "das", "fer\u00b7ne", "Meer", "und", "dort", "die", "Ber\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "KON", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Kein Menschenmund spricht aus,", "tokens": ["Kein", "Men\u00b7schen\u00b7mund", "spricht", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Was ich im Busen f\u00fchlte!", "tokens": ["Was", "ich", "im", "Bu\u00b7sen", "f\u00fchl\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wie liebliche Gedanken,", "tokens": ["Wie", "lieb\u00b7li\u00b7che", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O meine Silvia, welch ein hoffend Streben!", "tokens": ["O", "mei\u00b7ne", "Sil\u00b7via", ",", "welch", "ein", "hof\u00b7fend", "Stre\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "$,", "PWAT", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie schien das Menschenleben", "tokens": ["Wie", "schien", "das", "Men\u00b7schen\u00b7le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns damals wundersam!", "tokens": ["Uns", "da\u00b7mals", "wun\u00b7der\u00b7sam", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Bedenk' ich, wie viel T\u00e4uschungen verglommen,", "tokens": ["Be\u00b7denk'", "ich", ",", "wie", "viel", "T\u00e4u\u00b7schun\u00b7gen", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "PIAT", "NN", "VVPP", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "F\u00fchl' ich mein Herz beklommen", "tokens": ["F\u00fchl'", "ich", "mein", "Herz", "be\u00b7klom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Von trostlos bittrem Gram,", "tokens": ["Von", "trost\u00b7los", "bitt\u00b7rem", "Gram", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und all mein Elend d\u00e4ucht mir schwerer nur.", "tokens": ["Und", "all", "mein", "E\u00b7lend", "d\u00e4ucht", "mir", "schwe\u00b7rer", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Warum, warum, Natur,", "tokens": ["Wa\u00b7rum", ",", "wa\u00b7rum", ",", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "H\u00e4ltst du nicht Wort, erf\u00fcllest,", "tokens": ["H\u00e4ltst", "du", "nicht", "Wort", ",", "er\u00b7f\u00fcl\u00b7lest", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Was du versprachst, und tr\u00fcgst die eignen Kinder,", "tokens": ["Was", "du", "ver\u00b7sprachst", ",", "und", "tr\u00fcgst", "die", "eig\u00b7nen", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Die du mit Wahn umh\u00fcllest?", "tokens": ["Die", "du", "mit", "Wahn", "um\u00b7h\u00fcl\u00b7lest", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du, eh' im Winter noch die Flur erstarrt,", "tokens": ["Du", ",", "eh'", "im", "Win\u00b7ter", "noch", "die", "Flur", "er\u00b7starrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "APPRART", "NN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von t\u00fcckisch leisem Siechthum hingerafft", "tokens": ["Von", "t\u00fc\u00b7ckisch", "lei\u00b7sem", "Siech\u00b7thum", "hin\u00b7ge\u00b7rafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vergingst, du Z\u00e4rtliche, und schautest nicht", "tokens": ["Ver\u00b7gingst", ",", "du", "Z\u00e4rt\u00b7li\u00b7che", ",", "und", "schau\u00b7test", "nicht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "NN", "$,", "KON", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Bl\u00fcte deiner Jahre", "tokens": ["Die", "Bl\u00fc\u00b7te", "dei\u00b7ner", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und durftest nicht erst f\u00fchlen,", "tokens": ["Und", "durf\u00b7test", "nicht", "erst", "f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie s\u00fc\u00df das Lob auf deine schwarzen Locken,", "tokens": ["Wie", "s\u00fc\u00df", "das", "Lob", "auf", "dei\u00b7ne", "schwar\u00b7zen", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Auf deine feurigscheuen Liebesblicke;", "tokens": ["Auf", "dei\u00b7ne", "feu\u00b7rig\u00b7scheu\u00b7en", "Lie\u00b7bes\u00b7bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nicht plauderten mit dir von holdem Gl\u00fccke", "tokens": ["Nicht", "plau\u00b7der\u00b7ten", "mit", "dir", "von", "hol\u00b7dem", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Am Festtag die Gespielen.", "tokens": ["Am", "Fest\u00b7tag", "die", "Ge\u00b7spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auch mir verging \u2013 wie bald! \u2013", "tokens": ["Auch", "mir", "ver\u00b7ging", "\u2013", "wie", "bald", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "VVFIN", "$(", "KOKOM", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mein liebstes Hoffen, meinen Jahren auch", "tokens": ["Mein", "liebs\u00b7tes", "Hof\u00b7fen", ",", "mei\u00b7nen", "Jah\u00b7ren", "auch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Versagten die Geschicke", "tokens": ["Ver\u00b7sag\u00b7ten", "die", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Jugendglanz. Wie bist du", "tokens": ["Den", "Ju\u00b7gend\u00b7glanz", ".", "Wie", "bist", "du"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "PWAV", "VAFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Entschwebt, gleich einem Hauch,", "tokens": ["Ent\u00b7schwebt", ",", "gleich", "ei\u00b7nem", "Hauch", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Holde Gef\u00e4hrtin meiner Knabenzeit,", "tokens": ["Hol\u00b7de", "Ge\u00b7f\u00e4hr\u00b7tin", "mei\u00b7ner", "Kna\u00b7ben\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Hoffnung, du vielbeweinte!", "tokens": ["Hoff\u00b7nung", ",", "du", "viel\u00b7be\u00b7wein\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das also ist die Welt,", "tokens": ["Das", "al\u00b7so", "ist", "die", "Welt", ","], "token_info": [