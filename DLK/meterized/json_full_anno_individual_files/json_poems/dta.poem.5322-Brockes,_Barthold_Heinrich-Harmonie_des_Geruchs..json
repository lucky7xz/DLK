{"dta.poem.5322": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Harmonie des Geruchs.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer zu unsers Sch\u00f6pfers Ehren,", "tokens": ["Wer", "zu", "un\u00b7sers", "Sch\u00f6p\u00b7fers", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit befriedigtem Gem\u00fcthe,", "tokens": ["Mit", "be\u00b7frie\u00b7dig\u00b7tem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein Vergn\u00fcgen will vermehren,", "tokens": ["Sein", "Ver\u00b7gn\u00fc\u00b7gen", "will", "ver\u00b7meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Riech\u2019 im bunten Blumen-Reich,", "tokens": ["Riech'", "im", "bun\u00b7ten", "Blu\u00b7men\u00b7Reich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mit Bedachtsamkeit, zugleich", "tokens": ["Mit", "Be\u00b7dacht\u00b7sam\u00b7keit", ",", "zu\u00b7gleich"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Rosen und Orangen-Bl\u00fchte.", "tokens": ["Ro\u00b7sen", "und", "O\u00b7ran\u00b7gen\u00b7B\u00b7l\u00fch\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Dem Geruch recht lieb zu kosen", "tokens": ["Dem", "Ge\u00b7ruch", "recht", "lieb", "zu", "ko\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sind ja wol die holden Rosen", "tokens": ["Sind", "ja", "wol", "die", "hol\u00b7den", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wunderw\u00fcrdig zugericht.", "tokens": ["Wun\u00b7der\u00b7w\u00fcr\u00b7dig", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Recht mit Balsam eingemischet", "tokens": ["Recht", "mit", "Bal\u00b7sam", "ein\u00b7ge\u00b7mi\u00b7schet"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Ist was aus der Rose bricht,", "tokens": ["Ist", "was", "aus", "der", "Ro\u00b7se", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Und so Hirn als Hertz erfrischet.", "tokens": ["Und", "so", "Hirn", "als", "Hertz", "er\u00b7fri\u00b7schet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "KOUS", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wird von Blumen f\u00fcr die Nasen", "tokens": ["Wird", "von", "Blu\u00b7men", "f\u00fcr", "die", "Na\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Etwas lieblichs ausgeblasen,", "tokens": ["Et\u00b7was", "lieb\u00b7lichs", "aus\u00b7ge\u00b7bla\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ist es ebenfals die Bl\u00fcht,", "tokens": ["Ist", "es", "e\u00b7ben\u00b7fals", "die", "Bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Welche man im gr\u00fcnem Glantze,", "tokens": ["Wel\u00b7che", "man", "im", "gr\u00fc\u00b7nem", "Glant\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Recht als im smaragdnen Krantze,", "tokens": ["Recht", "als", "im", "sma\u00b7ragd\u00b7nen", "Krant\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.18": {"text": "Und bey g\u00fcldnen Aepfeln sieht;", "tokens": ["Und", "bey", "g\u00fcld\u00b7nen", "A\u00b7e\u00b7pfeln", "sieht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Die ein Auszug in der K\u00fcrtze", "tokens": ["Die", "ein", "Aus\u00b7zug", "in", "der", "K\u00fcrt\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Aller lieblichen Gew\u00fcrtze.", "tokens": ["Al\u00b7ler", "lieb\u00b7li\u00b7chen", "Ge\u00b7w\u00fcrt\u00b7ze", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Aber mischen beider Flammen,", "tokens": ["A\u00b7ber", "mi\u00b7schen", "bei\u00b7der", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Die nicht sichtbar, sich zusammen;", "tokens": ["Die", "nicht", "sicht\u00b7bar", ",", "sich", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJD", "$,", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Sp\u00fcrt man eine holde Glut,", "tokens": ["Sp\u00fcrt", "man", "ei\u00b7ne", "hol\u00b7de", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "Die der Seelen sanfte thut,", "tokens": ["Die", "der", "See\u00b7len", "sanf\u00b7te", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Die, wenn wir sie wol bemercken,", "tokens": ["Die", ",", "wenn", "wir", "sie", "wol", "be\u00b7mer\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Nicht allein den Geist zu st\u00e4rcken,", "tokens": ["Nicht", "al\u00b7lein", "den", "Geist", "zu", "st\u00e4r\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Zu vergn\u00fcgen, zu erqvicken,", "tokens": ["Zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "zu", "er\u00b7qvi\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Ja fast gleichsam zu entz\u00fccken,", "tokens": ["Ja", "fast", "gleich\u00b7sam", "zu", "ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Von recht sonderlicher Kraft;", "tokens": ["Von", "recht", "son\u00b7der\u00b7li\u00b7cher", "Kraft", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}