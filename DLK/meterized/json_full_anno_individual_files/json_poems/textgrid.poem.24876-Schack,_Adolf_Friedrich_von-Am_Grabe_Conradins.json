{"textgrid.poem.24876": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Am Grabe Conradins", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Staufe, dem zum Throne", "tokens": ["Du", "Stau\u00b7fe", ",", "dem", "zum", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Blutger\u00fcst verliehn,", "tokens": ["Ein", "Blut\u00b7ge\u00b7r\u00fcst", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der statt der Kaiserkrone", "tokens": ["Der", "statt", "der", "Kai\u00b7ser\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Kranz von Rosmarin,", "tokens": ["Den", "Kranz", "von", "Ros\u00b7ma\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Statt Hermelin und Seide", "tokens": ["Statt", "Her\u00b7me\u00b7lin", "und", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Leichentuch geerbt", "tokens": ["Ein", "Lei\u00b7chen\u00b7tuch", "ge\u00b7erbt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und es zum Purpurkleide", "tokens": ["Und", "es", "zum", "Pur\u00b7pur\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit eignem Blut gef\u00e4rbt;", "tokens": ["Mit", "eig\u00b7nem", "Blut", "ge\u00b7f\u00e4rbt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der nun am w\u00e4lschen Strande,", "tokens": ["Der", "nun", "am", "w\u00e4l\u00b7schen", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wo fremd die Woge sch\u00e4umt,", "tokens": ["Wo", "fremd", "die", "Wo\u00b7ge", "sch\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In fremder M\u00e4nner Lande", "tokens": ["In", "frem\u00b7der", "M\u00e4n\u00b7ner", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Lebensschlaf vertr\u00e4umt:", "tokens": ["Den", "Le\u00b7bens\u00b7schlaf", "ver\u00b7tr\u00e4umt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mich gr\u00fc\u00dft von deinem Steine", "tokens": ["Mich", "gr\u00fc\u00dft", "von", "dei\u00b7nem", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Heimatklang so traut,", "tokens": ["Der", "Hei\u00b7mat\u00b7klang", "so", "traut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie dich in deinem Schreine", "tokens": ["Wie", "dich", "in", "dei\u00b7nem", "Schrei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vielleicht mein deutscher Laut.", "tokens": ["Viel\u00b7leicht", "mein", "deut\u00b7scher", "Laut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nimm freundlich hin die Gaben,", "tokens": ["Nimm", "freund\u00b7lich", "hin", "die", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dir die Liebe streut,", "tokens": ["Die", "dir", "die", "Lie\u00b7be", "streut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Gr\u00fc\u00dfe, die dein Schwaben", "tokens": ["Die", "Gr\u00fc\u00b7\u00dfe", ",", "die", "dein", "Schwa\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch meine Hand dir beut;", "tokens": ["Durch", "mei\u00b7ne", "Hand", "dir", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zwei gr\u00fcne Eichenreiser,", "tokens": ["Zwei", "gr\u00fc\u00b7ne", "Ei\u00b7chen\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am Staufenschlo\u00df gepfl\u00fcckt,", "tokens": ["Am", "Stau\u00b7fen\u00b7schlo\u00df", "ge\u00b7pfl\u00fcckt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie sie, du junger Kaiser,", "tokens": ["Wie", "sie", ",", "du", "jun\u00b7ger", "Kai\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir oft das Haupt geschm\u00fcckt,", "tokens": ["Dir", "oft", "das", "Haupt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Wenn \u00fcber Alp' und Kuppe,", "tokens": ["Wenn", "\u00fc\u00b7ber", "Al\u00b7p'", "und", "Kup\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vom Waldesgr\u00fcn umwogt,", "tokens": ["Vom", "Wal\u00b7des\u00b7gr\u00fcn", "um\u00b7wogt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In froher J\u00e4gertruppe", "tokens": ["In", "fro\u00b7her", "J\u00e4\u00b7ger\u00b7trup\u00b7pe"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr aus zum Birschen zogt.", "tokens": ["Ihr", "aus", "zum", "Bir\u00b7schen", "zogt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O schl\u00fcgen tief und tiefer", "tokens": ["O", "schl\u00fc\u00b7gen", "tief", "und", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sie Wurzeln in dem Stein,", "tokens": ["Sie", "Wur\u00b7zeln", "in", "dem", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So wie auf kahlem Schiefer", "tokens": ["So", "wie", "auf", "kah\u00b7lem", "Schie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Tannen stolz gedeihn,", "tokens": ["Die", "Tan\u00b7nen", "stolz", "ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und streuten sie als B\u00e4ume,", "tokens": ["Und", "streu\u00b7ten", "sie", "als", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOUS", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von frischem Gr\u00fcn umlaubt,", "tokens": ["Von", "fri\u00b7schem", "Gr\u00fcn", "um\u00b7laubt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dir liebe alte Tr\u00e4ume", "tokens": ["Dir", "lie\u00b7be", "al\u00b7te", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ums fr\u00fch gesunkne Haupt!", "tokens": ["Ums", "fr\u00fch", "ge\u00b7sunk\u00b7ne", "Haupt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dann statt des dumpfen Ave,", "tokens": ["Dann", "statt", "des", "dum\u00b7pfen", "A\u00b7ve", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das durch die W\u00f6lbung hallt,", "tokens": ["Das", "durch", "die", "W\u00f6l\u00b7bung", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Umspielte dich im Schlafe", "tokens": ["Um\u00b7spiel\u00b7te", "dich", "im", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Ton, der s\u00fc\u00dfer schallt;", "tokens": ["Ein", "Ton", ",", "der", "s\u00fc\u00b7\u00dfer", "schallt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ein Ton aus besserm Dome,", "tokens": ["Ein", "Ton", "aus", "bes\u00b7serm", "Do\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus deutschem Eichenhain,", "tokens": ["Aus", "deut\u00b7schem", "Ei\u00b7chen\u00b7hain", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gru\u00df vom Donaustrome", "tokens": ["Ein", "Gru\u00df", "vom", "Do\u00b7naus\u00b7tro\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und vom geliebten Rhein,", "tokens": ["Und", "vom", "ge\u00b7lieb\u00b7ten", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und s\u00e4uselnd stiege nieder", "tokens": ["Und", "s\u00e4u\u00b7selnd", "stie\u00b7ge", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus gr\u00fcnem Laub der Klang,", "tokens": ["Aus", "gr\u00fc\u00b7nem", "Laub", "der", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So s\u00fc\u00df wie Uhlands Lieder", "tokens": ["So", "s\u00fc\u00df", "wie", "Uh\u00b7lands", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Walthers Minnesang.", "tokens": ["Und", "Walt\u00b7hers", "Min\u00b7ne\u00b7sang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Du Staufe, dem zum Throne", "tokens": ["Du", "Stau\u00b7fe", ",", "dem", "zum", "Thro\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Blutger\u00fcst verliehn,", "tokens": ["Ein", "Blut\u00b7ge\u00b7r\u00fcst", "ver\u00b7liehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line