{"dta.poem.12827": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen alten g\u00e4rtner, so ein jun-  \n ges m\u00e4gdgen geheyrathet hatte.  \n G. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alter bart! was nimmst du f\u00fcr?", "tokens": ["Al\u00b7ter", "bart", "!", "was", "nimmst", "du", "f\u00fcr", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses sind verkehrte possen.", "tokens": ["Die\u00b7ses", "sind", "ver\u00b7kehr\u00b7te", "pos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Warum hast du doch die th\u00fcr", "tokens": ["Wa\u00b7rum", "hast", "du", "doch", "die", "th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu dem garten aufgeschlossen?", "tokens": ["Zu", "dem", "gar\u00b7ten", "auf\u00b7ge\u00b7schlos\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VVFIN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nunmehr darffst du bey dem graben", "tokens": ["Nun\u00b7mehr", "darffst", "du", "bey", "dem", "gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Kein ger\u00e4th mit scharten haben.", "tokens": ["Kein", "ge\u00b7r\u00e4\u00b7th", "mit", "schar\u00b7ten", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "ADJA", "VAINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Du must frischen saamen kauffen,", "tokens": ["Du", "must", "fri\u00b7schen", "saa\u00b7men", "kauf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Oder bey der schlimmen zeit,", "tokens": ["O\u00b7der", "bey", "der", "schlim\u00b7men", "zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wenn sich alle welt erfreut,", "tokens": ["Wenn", "sich", "al\u00b7le", "welt", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Sch\u00e4ndlich aus dem pacht entlauffen.", "tokens": ["Sch\u00e4nd\u00b7lich", "aus", "dem", "pacht", "ent\u00b7lauf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}