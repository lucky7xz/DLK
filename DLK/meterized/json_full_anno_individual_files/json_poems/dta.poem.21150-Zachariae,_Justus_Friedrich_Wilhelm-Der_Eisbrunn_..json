{"dta.poem.21150": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Eisbrunn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der du vom nackenden Fels im Krais der finstern Ge-\nb\u00fcsche", "tokens": ["Der", "du", "vom", "na\u00b7cken\u00b7den", "Fels", "im", "Krais", "der", "fins\u00b7tern", "Ge", "b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "APPRART", "NN", "ART", "ADJA", "TRUNC", "ADJA"], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dich sammelst, und in die Wiese dich schlingst;", "tokens": ["Dich", "sam\u00b7melst", ",", "und", "in", "die", "Wie\u00b7se", "dich", "schlingst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "O Quell, der Lieder verdient, so wie Blandusiens Quel-", "tokens": ["O", "Quell", ",", "der", "Lie\u00b7der", "ver\u00b7dient", ",", "so", "wie", "Blan\u00b7du\u00b7si\u00b7ens", "Quel"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ART", "NN", "VVPP", "$,", "ADV", "KOKOM", "NE", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dich singt mein Lied in die kommende Welt.", "tokens": ["Dich", "singt", "mein", "Lied", "in", "die", "kom\u00b7men\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon sieht mein heiterer Blick von fern den moosich-", "tokens": ["Schon", "sieht", "mein", "hei\u00b7te\u00b7rer", "Blick", "von", "fern", "den", "moo\u00b7sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJD", "ART", "TRUNC"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der \u00fcber den kahlen Felsen sich neigt;", "tokens": ["Der", "\u00fc\u00b7ber", "den", "kah\u00b7len", "Fel\u00b7sen", "sich", "neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und der durch d\u00fcrres Gestein, mit halbverdorreten Wur-", "tokens": ["Und", "der", "durch", "d\u00fcr\u00b7res", "Ge\u00b7stein", ",", "mit", "halb\u00b7ver\u00b7dor\u00b7re\u00b7ten", "Wur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Zu deinen wohlth\u00e4tigen Wellen sich dringt.", "tokens": ["Zu", "dei\u00b7nen", "wohlt\u00b7h\u00e4\u00b7ti\u00b7gen", "Wel\u00b7len", "sich", "dringt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "O du krystallener Quell, zu dir komm ich mit Selinen,", "tokens": ["O", "du", "krys\u00b7tal\u00b7le\u00b7ner", "Quell", ",", "zu", "dir", "komm", "ich", "mit", "Se\u00b7li\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJA", "NN", "$,", "APPR", "PPER", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Dein angenehmes Gestade zu weihn.", "tokens": ["Dein", "an\u00b7ge\u00b7neh\u00b7mes", "Ge\u00b7sta\u00b7de", "zu", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit einem lachenden Strau\u00df will ich den Sonnenhut", "tokens": ["Mit", "ei\u00b7nem", "la\u00b7chen\u00b7den", "Strau\u00df", "will", "ich", "den", "Son\u00b7nen\u00b7hut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NE", "VMFIN", "PPER", "ART", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Von dem die schimmernde Schleife sich kr\u00fcmmt.", "tokens": ["Von", "dem", "die", "schim\u00b7mern\u00b7de", "Schlei\u00b7fe", "sich", "kr\u00fcmmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und aus der silbernen Fluth will ich die Wangen bene-", "tokens": ["Und", "aus", "der", "sil\u00b7ber\u00b7nen", "Fluth", "will", "ich", "die", "Wan\u00b7gen", "be\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VMFIN", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Die ihr mein Blick oft mit Unschuld gef\u00e4rbt.", "tokens": ["Die", "ihr", "mein", "Blick", "oft", "mit", "Un\u00b7schuld", "ge\u00b7f\u00e4rbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Zu gleichem Scherze bereit, wird sie mich l\u00e4chelnd be-", "tokens": ["Zu", "glei\u00b7chem", "Scher\u00b7ze", "be\u00b7reit", ",", "wird", "sie", "mich", "l\u00e4\u00b7chelnd", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJD", "$,", "VAFIN", "PPER", "PRF", "ADJD", "TRUNC"], "meter": "-+-+---+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und dankbar k\u00fc\u00df ich die r\u00e4chende Hand.", "tokens": ["Und", "dank\u00b7bar", "k\u00fc\u00df", "ich", "die", "r\u00e4\u00b7chen\u00b7de", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "So z\u00e4hlt der Enkel dich einst zu jenen unsterblichen", "tokens": ["So", "z\u00e4hlt", "der", "En\u00b7kel", "dich", "einst", "zu", "je\u00b7nen", "uns\u00b7terb\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Weil ich die rauschenden Eichen ger\u00fchmt,", "tokens": ["Weil", "ich", "die", "rau\u00b7schen\u00b7den", "Ei\u00b7chen", "ge\u00b7r\u00fchmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In deren Schatten zuerst ich sanfterr\u00f6thend Selinen,", "tokens": ["In", "de\u00b7ren", "Schat\u00b7ten", "zu\u00b7erst", "ich", "sanf\u00b7ter\u00b7r\u00f6\u00b7thend", "Se\u00b7li\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+--+-+-+----", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die sch\u00f6nste Hand, mit Empfindung gek\u00fc\u00dft.", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Hand", ",", "mit", "Emp\u00b7fin\u00b7dung", "ge\u00b7k\u00fc\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}