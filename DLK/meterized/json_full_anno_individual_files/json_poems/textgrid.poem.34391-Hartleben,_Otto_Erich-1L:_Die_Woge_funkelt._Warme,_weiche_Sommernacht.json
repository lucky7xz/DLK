{"textgrid.poem.34391": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Woge funkelt. Warme, weiche Sommernacht", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Woge funkelt. Warme, weiche Sommernacht", "tokens": ["Die", "Wo\u00b7ge", "fun\u00b7kelt", ".", "War\u00b7me", ",", "wei\u00b7che", "Som\u00b7mer\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sank nieder, doch am Strande lebt es noch und lacht.", "tokens": ["sank", "nie\u00b7der", ",", "doch", "am", "Stran\u00b7de", "lebt", "es", "noch", "und", "lacht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "ADV", "APPRART", "NN", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da freuen frohe Menschen sich am seltnen Spiel,", "tokens": ["Da", "freu\u00b7en", "fro\u00b7he", "Men\u00b7schen", "sich", "am", "selt\u00b7nen", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "das die Natur spielt, jauchzen, wenn die Welle fiel", "tokens": ["das", "die", "Na\u00b7tur", "spielt", ",", "jauch\u00b7zen", ",", "wenn", "die", "Wel\u00b7le", "fiel"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "VVFIN", "$,", "VVINF", "$,", "KOUS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und blinkte ...", "tokens": ["und", "blink\u00b7te", "..."], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Aber freudefeindlich, fern dem Schwarm", "tokens": ["A\u00b7ber", "freu\u00b7de\u00b7feind\u00b7lich", ",", "fern", "dem", "Schwarm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "wandl ich allein und n\u00e4hre finster meinen Harm,", "tokens": ["wandl", "ich", "al\u00b7lein", "und", "n\u00e4h\u00b7re", "fins\u00b7ter", "mei\u00b7nen", "Harm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Zu meinem Herzen redet die Natur nicht mehr,", "tokens": ["Zu", "mei\u00b7nem", "Her\u00b7zen", "re\u00b7det", "die", "Na\u00b7tur", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mir schweigt des Lenzes Saatengr\u00fcn, mir schweigt das Meer.", "tokens": ["mir", "schweigt", "des", "Len\u00b7zes", "Saa\u00b7ten\u00b7gr\u00fcn", ",", "mir", "schweigt", "das", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ehmals wohl fasste Wonne meine Seele ganz:", "tokens": ["Eh\u00b7mals", "wohl", "fass\u00b7te", "Won\u00b7ne", "mei\u00b7ne", "See\u00b7le", "ganz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJA", "NN", "PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "ich kniete, wenn am Meeressaum der Sonnenglanz", "tokens": ["ich", "knie\u00b7te", ",", "wenn", "am", "Mee\u00b7res\u00b7saum", "der", "Son\u00b7nen\u00b7glanz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "aufglomm, und hehre Schauer f\u00fcllten mir die Brust.", "tokens": ["auf\u00b7glomm", ",", "und", "heh\u00b7re", "Schau\u00b7er", "f\u00fcll\u00b7ten", "mir", "die", "Brust", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Kam eine Nacht, voll Scheideleid und Liebeslust,", "tokens": ["Kam", "ei\u00b7ne", "Nacht", ",", "voll", "Schei\u00b7de\u00b7leid", "und", "Lie\u00b7bes\u00b7lust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "kam eine Nacht, sternblinkend wie die heutige zwar,", "tokens": ["kam", "ei\u00b7ne", "Nacht", ",", "stern\u00b7blin\u00b7kend", "wie", "die", "heu\u00b7ti\u00b7ge", "zwar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.6": {"text": "die doch unendlich sch\u00f6ner wie die heutige war!", "tokens": ["die", "doch", "un\u00b7end\u00b7lich", "sch\u00f6\u00b7ner", "wie", "die", "heu\u00b7ti\u00b7ge", "war", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADJD", "KOKOM", "ART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.7": {"text": "Ich lag zu F\u00fcssen der Geliebten, sah empor", "tokens": ["Ich", "lag", "zu", "F\u00fcs\u00b7sen", "der", "Ge\u00b7lieb\u00b7ten", ",", "sah", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "zu des geflammten Himmels ewiger Pracht und schwor:", "tokens": ["zu", "des", "ge\u00b7flamm\u00b7ten", "Him\u00b7mels", "e\u00b7wi\u00b7ger", "Pracht", "und", "schwor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "So wahr erhabene Andacht meine Seele f\u00fcllt,", "tokens": ["So", "wahr", "er\u00b7ha\u00b7be\u00b7ne", "An\u00b7dacht", "mei\u00b7ne", "See\u00b7le", "f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "da heilige Ruh in Schlummer alles Leben h\u00fcllt,", "tokens": ["da", "hei\u00b7li\u00b7ge", "Ruh", "in", "Schlum\u00b7mer", "al\u00b7les", "Le\u00b7ben", "h\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "NN", "