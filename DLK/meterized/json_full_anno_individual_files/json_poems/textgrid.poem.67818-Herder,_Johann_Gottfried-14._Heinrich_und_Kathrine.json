{"textgrid.poem.67818": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14. Heinrich und Kathrine", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vor Zeiten war in Engelland", "tokens": ["Vor", "Zei\u00b7ten", "war", "in", "En\u00b7gel\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "APPR", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lord Heinrich Weltgepriesen;", "tokens": ["Lord", "Hein\u00b7rich", "Welt\u00b7ge\u00b7prie\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kein Ritter, der mehr Heldenthum", "tokens": ["Kein", "Rit\u00b7ter", ",", "der", "mehr", "Hel\u00b7den\u00b7thum"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Freudigkeit bewiesen.", "tokens": ["Und", "Freu\u00b7dig\u00b7keit", "be\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach Ruhm hinan ging stets sein Sinn,", "tokens": ["Nach", "Ruhm", "hi\u00b7nan", "ging", "stets", "sein", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Liebe nicht verf\u00fchret;", "tokens": ["Von", "Lie\u00b7be", "nicht", "ver\u00b7f\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Das sch\u00f6nste Fr\u00e4ulein hatte nie", "tokens": ["Das", "sch\u00f6ns\u00b7te", "Fr\u00e4u\u00b7lein", "hat\u00b7te", "nie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sein m\u00e4nnlich Herz ger\u00fchret.", "tokens": ["Sein", "m\u00e4nn\u00b7lich", "Herz", "ge\u00b7r\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohin in aller Sch\u00f6nen Kreis", "tokens": ["Wo\u00b7hin", "in", "al\u00b7ler", "Sch\u00f6\u00b7nen", "Kreis"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kathrine trat, trat Wonne,", "tokens": ["Ka\u00b7thri\u00b7ne", "trat", ",", "trat", "Won\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bl\u00fcht' auf, als wie die Rose s\u00fc\u00df,", "tokens": ["Bl\u00fcht'", "auf", ",", "als", "wie", "die", "Ro\u00b7se", "s\u00fc\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "KOKOM", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ging auf, als wie die Sonne.", "tokens": ["Ging", "auf", ",", "als", "wie", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ob immer war ihr Stand gering,", "tokens": ["Ob", "im\u00b7mer", "war", "ihr", "Stand", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "VAFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gewann doch sie nur Herzen;", "tokens": ["Ge\u00b7wann", "doch", "sie", "nur", "Her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kein J\u00fcngling sahe sie und sank", "tokens": ["Kein", "J\u00fcng\u00b7ling", "sa\u00b7he", "sie", "und", "sank"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nicht schon in Liebesschmerzen.", "tokens": ["Nicht", "schon", "in", "Lie\u00b7bes\u00b7schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch bald verlor ihr Auge Schein", "tokens": ["Doch", "bald", "ver\u00b7lor", "ihr", "Au\u00b7ge", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Klarheit. Ihre Wangen", "tokens": ["Und", "Klar\u00b7heit", ".", "Ih\u00b7re", "Wan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erbla\u00dften. Ihrem Angesicht", "tokens": ["Er\u00b7bla\u00df\u00b7ten", ".", "Ih\u00b7rem", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War aller Reiz entgangen.", "tokens": ["War", "al\u00b7ler", "Reiz", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie siechte lang und nie vertraut", "tokens": ["Sie", "siech\u00b7te", "lang", "und", "nie", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie Jemand ihren Kummer;", "tokens": ["Sie", "Je\u00b7mand", "ih\u00b7ren", "Kum\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "In Thr\u00e4nen flo\u00df ihr Tag dahin,", "tokens": ["In", "Thr\u00e4\u00b7nen", "flo\u00df", "ihr", "Tag", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Nacht in kurzem Schlummer.", "tokens": ["Die", "Nacht", "in", "kur\u00b7zem", "Schlum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Einmal im Traume rief sie laut:", "tokens": ["Ein\u00b7mal", "im", "Trau\u00b7me", "rief", "sie", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbach Heinrich, sieh mich leiden!", "tokens": ["\u00bb", "ach", "Hein\u00b7rich", ",", "sieh", "mich", "lei\u00b7den", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "XY", "NE", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O hart Geschick! ich armes Kind", "tokens": ["O", "hart", "Ge\u00b7schick", "!", "ich", "ar\u00b7mes", "Kind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "$.", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df liebeschmachtend scheiden.", "tokens": ["Mu\u00df", "lie\u00b7be\u00b7schmach\u00b7tend", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch ach \u2013 ein armes M\u00e4dchen mu\u00df", "tokens": ["Doch", "ach", "\u2013", "ein", "ar\u00b7mes", "M\u00e4d\u00b7chen", "mu\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$(", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mu\u00df Wahrheit schon verstecken.", "tokens": ["Mu\u00df", "Wahr\u00b7heit", "schon", "ver\u00b7ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Viel lieber todt zehntausendmal,", "tokens": ["Viel", "lie\u00b7ber", "todt", "zehn\u00b7tau\u00b7send\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als meine Lieb' entdecken!", "tokens": ["Als", "mei\u00b7ne", "Lieb'", "ent\u00b7de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das h\u00f6rt die treue W\u00e4chterin;", "tokens": ["Das", "h\u00f6rt", "die", "treu\u00b7e", "W\u00e4ch\u00b7te\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie eilt zum jungen Helden,", "tokens": ["Sie", "eilt", "zum", "jun\u00b7gen", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach, Herr! nun kann ich dir die Noth", "tokens": ["\u00bb", "ach", ",", "Herr", "!", "nun", "kann", "ich", "dir", "die", "Noth"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "ADV", "VMFIN", "PPER", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der kranken Freundin melden.", "tokens": ["Der", "kran\u00b7ken", "Freun\u00b7din", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Traum, ein Traum hat's offenbart,", "tokens": ["Ein", "Traum", ",", "ein", "Traum", "hat's", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was sie so tief betr\u00fcbet.", "tokens": ["Was", "sie", "so", "tief", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach! Katharine liegt und stirbt,", "tokens": ["Ach", "!", "Ka\u00b7tha\u00b7ri\u00b7ne", "liegt", "und", "stirbt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "NE", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Stirbt nun \u2013 weil sie \u2013 dich liebet.\u00ab", "tokens": ["Stirbt", "nun", "\u2013", "weil", "sie", "\u2013", "dich", "lie\u00b7bet", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "$(", "KOUS", "PPER", "$(", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das traf des edlen Heinrichs Herz;", "tokens": ["Das", "traf", "des", "ed\u00b7len", "Hein\u00b7richs", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schnell schlug es auf in Flammen!", "tokens": ["Schnell", "schlug", "es", "auf", "in", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbach armes ungl\u00fcckseligs Kind! \u2013", "tokens": ["\u00bb", "ach", "ar\u00b7mes", "un\u00b7gl\u00fcck\u00b7se\u00b7ligs", "Kind", "!", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "XY", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Doch wer kann mich verdammen?", "tokens": ["Doch", "wer", "kann", "mich", "ver\u00b7dam\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wust' ich, zu zu Bescheidene,", "tokens": ["Wust'", "ich", ",", "zu", "zu", "Be\u00b7schei\u00b7de\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.6": {"text": "Was dir den Tod bereite?", "tokens": ["Was", "dir", "den", "Tod", "be\u00b7rei\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wohlan ich komm'!\u00ab Und wie der Wind", "tokens": ["Wo\u00b7hlan", "ich", "komm'", "!", "\u00ab", "Und", "wie", "der", "Wind"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$.", "$(", "KON", "PWAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Flog er an ihre Seite.", "tokens": ["Flog", "er", "an", "ih\u00b7re", "Sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "\u00bberwach, erwach Holdselige!", "tokens": ["\u00bb", "er\u00b7wach", ",", "er\u00b7wach", "Hold\u00b7se\u00b7li\u00b7ge", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erwache, meine Sch\u00f6ne!", "tokens": ["Er\u00b7wa\u00b7che", ",", "mei\u00b7ne", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ach h\u00e4tte mirs geahndet je \u2013", "tokens": ["Ach", "h\u00e4t\u00b7te", "mirs", "ge\u00b7ahn\u00b7det", "je", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VAFIN", "NE", "VVPP", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Eine, Eine Thr\u00e4ne", "tokens": ["Nicht", "Ei\u00b7ne", ",", "Ei\u00b7ne", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "$,", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "H\u00e4ttst du verweinet \u2013 Heinrich ruft!", "tokens": ["H\u00e4ttst", "du", "ver\u00b7wei\u00b7net", "\u2013", "Hein\u00b7rich", "ruft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$(", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mistraue nicht, erwarme!", "tokens": ["Mis\u00b7trau\u00b7e", "nicht", ",", "er\u00b7war\u00b7me", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Bl\u00fch auf, wach auf, vom Tode. Komm", "tokens": ["Bl\u00fch", "auf", ",", "wach", "auf", ",", "vom", "To\u00b7de", ".", "Komm"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADJD", "PTKVZ", "$,", "ADJD", "PTKVZ", "$,", "APPRART", "NN", "$.", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zur\u00fcck in meine Arme!\u00ab", "tokens": ["Zu\u00b7r\u00fcck", "in", "mei\u00b7ne", "Ar\u00b7me", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Da kam die Holdentschlafne noch", "tokens": ["Da", "kam", "die", "Hol\u00b7dent\u00b7schlaf\u00b7ne", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Einmal zur\u00fcck ins Leben.", "tokens": ["Ein\u00b7mal", "zu\u00b7r\u00fcck", "ins", "Le\u00b7ben", "."], "