{"dta.poem.15735": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "68.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Alswie ein Vater gibt die Freiheit seinem Sohne,", "tokens": ["Als\u00b7wie", "ein", "Va\u00b7ter", "gibt", "die", "Frei\u00b7heit", "sei\u00b7nem", "Soh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht zur Versuchung ihm, nein, zur Vollendungskrone;", "tokens": ["Nicht", "zur", "Ver\u00b7su\u00b7chung", "ihm", ",", "nein", ",", "zur", "Vol\u00b7len\u00b7dungs\u00b7kro\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PPER", "$,", "PTKANT", "$,", "APPRART", "NN", "$."], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Nicht um zu gleiten, um zu st\u00e4rken seinen Tritt,", "tokens": ["Nicht", "um", "zu", "glei\u00b7ten", ",", "um", "zu", "st\u00e4r\u00b7ken", "sei\u00b7nen", "Tritt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUI", "PTKZU", "VVINF", "$,", "KOUI", "PTKZU", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Selbst fest zu stehn, und aufzustehn auch wann er glitt:", "tokens": ["Selbst", "fest", "zu", "stehn", ",", "und", "auf\u00b7zu\u00b7stehn", "auch", "wann", "er", "glitt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VVINF", "$,", "KON", "VVFIN", "ADV", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So gab dir Gott, o Mensch, den freien Willen auch;", "tokens": ["So", "gab", "dir", "Gott", ",", "o", "Mensch", ",", "den", "frei\u00b7en", "Wil\u00b7len", "auch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "FM", "NN", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Misbrauchs M\u00f6glichkeit macht m\u00f6glich den Gebrauch.", "tokens": ["Des", "Mis\u00b7brauchs", "M\u00f6g\u00b7lich\u00b7keit", "macht", "m\u00f6g\u00b7lich", "den", "Ge\u00b7brauch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}