{"textgrid.poem.35175": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Zeit", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es klingt ein Ruf aus alter, alter Zeit", "tokens": ["Es", "klingt", "ein", "Ruf", "aus", "al\u00b7ter", ",", "al\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "An unser Ohr wie aus Prophetenmund:", "tokens": ["An", "un\u00b7ser", "Ohr", "wie", "aus", "Pro\u00b7phe\u00b7ten\u00b7mund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KOKOM", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist dir verborgen die Vergangenheit,", "tokens": ["Ist", "dir", "ver\u00b7bor\u00b7gen", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So thut sich dir das Werdende nicht kund.", "tokens": ["So", "thut", "sich", "dir", "das", "Wer\u00b7den\u00b7de", "nicht", "kund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPER", "ART", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du willst so gern in deine Zukunft schaun;", "tokens": ["Du", "willst", "so", "gern", "in", "dei\u00b7ne", "Zu\u00b7kunft", "schaun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da mu\u00dft du erst die Gegenwart begreifen,", "tokens": ["Da", "mu\u00dft", "du", "erst", "die", "Ge\u00b7gen\u00b7wart", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und diese hat sich stetig neu zu baun", "tokens": ["Und", "die\u00b7se", "hat", "sich", "ste\u00b7tig", "neu", "zu", "baun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VAFIN", "PRF", "ADJD", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Nach Normen, die aus dem Vergangnen reifen.", "tokens": ["Nach", "Nor\u00b7men", ",", "die", "aus", "dem", "Ver\u00b7gang\u00b7nen", "rei\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Sind diese Normen dir vielleicht bekannt?", "tokens": ["Sind", "die\u00b7se", "Nor\u00b7men", "dir", "viel\u00b7leicht", "be\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ward eine dir von ihnen offenbar?", "tokens": ["Ward", "ei\u00b7ne", "dir", "von", "ih\u00b7nen", "of\u00b7fen\u00b7bar", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "PPER", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du kennst ja nicht das ferne, ferne Land,", "tokens": ["Du", "kennst", "ja", "nicht", "das", "fer\u00b7ne", ",", "fer\u00b7ne", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In dem die Gegenwart einst Zukunft war.", "tokens": ["In", "dem", "die", "Ge\u00b7gen\u00b7wart", "einst", "Zu\u00b7kunft", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was k\u00fcmmert dich das, was verschwunden ist!", "tokens": ["Was", "k\u00fcm\u00b7mert", "dich", "das", ",", "was", "ver\u00b7schwun\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PDS", "$,", "PRELS", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Soll sich die Nachwelt nicht um uns bek\u00fcmmern?", "tokens": ["Soll", "sich", "die", "Nach\u00b7welt", "nicht", "um", "uns", "be\u00b7k\u00fcm\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "ART", "NN", "PTKNEG", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es hat die Menschheit das, was sie vergi\u00dft,", "tokens": ["Es", "hat", "die", "Menschheit", "das", ",", "was", "sie", "ver\u00b7gi\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PDS", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Zur S\u00fchne auszugraben unter Tr\u00fcmmern.", "tokens": ["Zur", "S\u00fch\u00b7ne", "aus\u00b7zu\u00b7gra\u00b7ben", "un\u00b7ter", "Tr\u00fcm\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und solche S\u00fchne ist auch unsre Pflicht,", "tokens": ["Und", "sol\u00b7che", "S\u00fch\u00b7ne", "ist", "auch", "uns\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die wir verga\u00dfen, was die Vorwelt gab.", "tokens": ["Die", "wir", "ver\u00b7ga\u00b7\u00dfen", ",", "was", "die", "Vor\u00b7welt", "gab", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erkennen wir der Menschheit Seele nicht,", "tokens": ["Er\u00b7ken\u00b7nen", "wir", "der", "Menschheit", "See\u00b7le", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "PTKNEG", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "So sind wir nichts, als dieser Seele Grab.", "tokens": ["So", "sind", "wir", "nichts", ",", "als", "die\u00b7ser", "See\u00b7le", "Grab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "$,", "KOUS", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Drum w\u00fcnschest du nach dieser, deiner Zeit", "tokens": ["Drum", "w\u00fcn\u00b7schest", "du", "nach", "die\u00b7ser", ",", "dei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "PDAT", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Den Kommenden als lebend dich zu zeigen,", "tokens": ["Den", "Kom\u00b7men\u00b7den", "als", "le\u00b7bend", "dich", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "So geh zum Ursprung, zur Vergangenheit,", "tokens": ["So", "geh", "zum", "Ur\u00b7sprung", ",", "zur", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Um dann belehrt aus ihr emporzusteigen.", "tokens": ["Um", "dann", "be\u00b7lehrt", "aus", "ihr", "em\u00b7por\u00b7zu\u00b7stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "VVFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dort liegt der Quell, der unaufhaltsam flie\u00dft,", "tokens": ["Dort", "liegt", "der", "Quell", ",", "der", "un\u00b7auf\u00b7halt\u00b7sam", "flie\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weil jede, jede Stunde vorw\u00e4rts geht,", "tokens": ["Weil", "je\u00b7de", ",", "je\u00b7de", "Stun\u00b7de", "vor\u00b7w\u00e4rts", "geht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und sich als Tugend \u00fcber den ergie\u00dft,", "tokens": ["Und", "sich", "als", "Tu\u00b7gend", "\u00fc\u00b7ber", "den", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "KOUS", "NN", "APPR", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der dieser Stunden ernsten Wink versteht.", "tokens": ["Der", "die\u00b7ser", "Stun\u00b7den", "erns\u00b7ten", "Wink", "ver\u00b7steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von dort erklang zuerst das gro\u00dfe Wort", "tokens": ["Von", "dort", "er\u00b7klang", "zu\u00b7erst", "das", "gro\u00b7\u00dfe", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Vom Leben, von gesprengten Todesbanden;", "tokens": ["Vom", "Le\u00b7ben", ",", "von", "ge\u00b7spreng\u00b7ten", "To\u00b7des\u00b7ban\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von dort erklingt es heut noch fort und fort,", "tokens": ["Von", "dort", "er\u00b7klingt", "es", "heut", "noch", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und wer es achtet, der ist auferstanden.", "tokens": ["Und", "wer", "es", "ach\u00b7tet", ",", "der", "ist", "auf\u00b7er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PRELS", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Dort liegt vergraben, was der Tag einst sagt,", "tokens": ["Dort", "liegt", "ver\u00b7gra\u00b7ben", ",", "was", "der", "Tag", "einst", "sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "$,", "PRELS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der uns das Leben aus dem Leben giebt;", "tokens": ["Der", "uns", "das", "Le\u00b7ben", "aus", "dem", "Le\u00b7ben", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dort liegt vergraben, was dies Leben fragt,", "tokens": ["Dort", "liegt", "ver\u00b7gra\u00b7ben", ",", "was", "dies", "Le\u00b7ben", "fragt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "$,", "PWS", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn man nichts als nur dieses Leben liebt.", "tokens": ["Wenn", "man", "nichts", "als", "nur", "die\u00b7ses", "Le\u00b7ben", "liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "KOKOM", "ADV", "PDAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So gehe hin, und forsche, forsche gern;", "tokens": ["So", "ge\u00b7he", "hin", ",", "und", "for\u00b7sche", ",", "for\u00b7sche", "gern", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "KON", "ADJA", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Such nicht das Wort; such den, der es gesprochen;", "tokens": ["Such", "nicht", "das", "Wort", ";", "such", "den", ",", "der", "es", "ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "$.", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Dann leuchtet dir die Herrlichkeit des Herrn,", "tokens": ["Dann", "leuch\u00b7tet", "dir", "die", "Herr\u00b7lich\u00b7keit", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und alle Erdenketten sind zerbrochen.", "tokens": ["Und", "al\u00b7le", "Er\u00b7den\u00b7ket\u00b7ten", "sind", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es wird f\u00fcr dich dann diese Spanne Zeit,", "tokens": ["Es", "wird", "f\u00fcr", "dich", "dann", "die\u00b7se", "Span\u00b7ne", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADV", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die du so f\u00e4lschlich in Minuten trennst,", "tokens": ["Die", "du", "so", "f\u00e4lschlich", "in", "Mi\u00b7nu\u00b7ten", "trennst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Zum untrennbaren Theil der Ewigkeit,", "tokens": ["Zum", "un\u00b7trenn\u00b7ba\u00b7ren", "Theil", "der", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die du als dir geh\u00f6rig kennen lernst.", "tokens": ["Die", "du", "als", "dir", "ge\u00b7h\u00f6\u00b7rig", "ken\u00b7nen", "lernst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "PPER", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du steigst empor, nicht wie man Stufen steigt;", "tokens": ["Du", "steigst", "em\u00b7por", ",", "nicht", "wie", "man", "Stu\u00b7fen", "steigt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PTKNEG", "PWAV", "PIS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es giebt f\u00fcr dich nicht Jahre und nicht Stunden,", "tokens": ["Es", "giebt", "f\u00fcr", "dich", "nicht", "Jah\u00b7re", "und", "nicht", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und wenn sich dann dir keine Zeit mehr zeigt,", "tokens": ["Und", "wenn", "sich", "dann", "dir", "kei\u00b7ne", "Zeit", "mehr", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ADV", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "So bist du Sieger und hast \u