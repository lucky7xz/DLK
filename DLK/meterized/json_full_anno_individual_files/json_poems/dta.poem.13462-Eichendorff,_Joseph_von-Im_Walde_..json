{"dta.poem.13462": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Im Walde .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Es zog eine Hochzeit den Berg entlang,", "tokens": ["Es", "zog", "ei\u00b7ne", "Hoch\u00b7zeit", "den", "Berg", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ich h\u00f6rte die V\u00f6gel schlagen,", "tokens": ["Ich", "h\u00f6r\u00b7te", "die", "V\u00f6\u00b7gel", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da blitzten viel' Reiter, das Waldhorn klang,", "tokens": ["Da", "blitz\u00b7ten", "viel'", "Rei\u00b7ter", ",", "das", "Wald\u00b7horn", "klang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das war ein lustiges Jagen!", "tokens": ["Das", "war", "ein", "lus\u00b7ti\u00b7ges", "Ja\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Und eh' ich's gedacht, war Alles verhallt,", "tokens": ["Und", "eh'", "ich's", "ge\u00b7dacht", ",", "war", "Al\u00b7les", "ver\u00b7hallt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "VVPP", "$,", "VAFIN", "PIS", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Nacht bedecket die Runde,", "tokens": ["Die", "Nacht", "be\u00b7de\u00b7cket", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Nur von den Bergen noch rauschet der Wald", "tokens": ["Nur", "von", "den", "Ber\u00b7gen", "noch", "rau\u00b7schet", "der", "Wald"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "VVFIN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und mich schauert im Herzensgrunde.", "tokens": ["Und", "mich", "schau\u00b7ert", "im", "Her\u00b7zens\u00b7grun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}