{"dta.poem.9523": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Augen/ br\u00fcste.  \n C. H. v. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich von deinen augen/ un\u0303 den weissen br\u00fcsten sage\u0303?", "tokens": ["Was", "soll", "ich", "von", "dei\u00b7nen", "au\u00b7gen", "/", "u\u00f1", "den", "weis\u00b7sen", "br\u00fcs\u00b7ten", "sag\u1ebd", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "PPOSAT", "NN", "$(", "KON", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jene sind der Venus f\u00fchrer/ diese sind ihr sieges-wagen.", "tokens": ["Je\u00b7ne", "sind", "der", "Ve\u00b7nus", "f\u00fch\u00b7rer", "/", "die\u00b7se", "sind", "ihr", "sie\u00b7ges\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "$(", "PDS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}