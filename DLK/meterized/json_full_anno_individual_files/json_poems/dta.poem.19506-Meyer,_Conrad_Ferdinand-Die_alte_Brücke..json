{"dta.poem.19506": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Die alte Br\u00fccke.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Bogen, grauer Zeit entstammt,", "tokens": ["Dein", "Bo\u00b7gen", ",", "grau\u00b7er", "Zeit", "ent\u00b7stammt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht manch Jahrhundert au\u00dfer Amt;", "tokens": ["Steht", "manch", "Jahr\u00b7hun\u00b7dert", "au\u00b7\u00dfer", "Amt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein neuer Bau ragt \u00fcber dir:", "tokens": ["Ein", "neu\u00b7er", "Bau", "ragt", "\u00fc\u00b7ber", "dir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dort fahren sie! Du feierst hier.", "tokens": ["Dort", "fah\u00b7ren", "sie", "!", "Du", "fei\u00b7erst", "hier", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Stra\u00dfe, die getragen du,", "tokens": ["Die", "Stra\u00b7\u00dfe", ",", "die", "ge\u00b7tra\u00b7gen", "du", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVPP", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Deckt Wuchs und rothe Bl\u00fcthe zu!", "tokens": ["Deckt", "Wuchs", "und", "ro\u00b7the", "Bl\u00fc\u00b7the", "zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Nebel netzt und tr\u00e4nkt dein Moos,", "tokens": ["Ein", "Ne\u00b7bel", "netzt", "und", "tr\u00e4nkt", "dein", "Moos", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er steigt aus dumpfem Reu\u00dfgetos:", "tokens": ["Er", "steigt", "aus", "dum\u00b7pfem", "Reu\u00df\u00b7ge\u00b7tos", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mit einem luftgewobnen Kleid", "tokens": ["Mit", "ei\u00b7nem", "luft\u00b7ge\u00b7wob\u00b7nen", "Kleid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Umschleiert dich Vergangenheit", "tokens": ["Um\u00b7schlei\u00b7ert", "dich", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und statt des Lebens geht der Traum", "tokens": ["Und", "statt", "des", "Le\u00b7bens", "geht", "der", "Traum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf deines Pfades engem Raum.", "tokens": ["Auf", "dei\u00b7nes", "Pfa\u00b7des", "en\u00b7gem", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Das Carmen, das der Sch\u00fcler sang,", "tokens": ["Das", "Car\u00b7men", ",", "das", "der", "Sch\u00fc\u00b7ler", "sang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tr\u00e4umt noch im Felsenwiederklang,", "tokens": ["Tr\u00e4umt", "noch", "im", "Fel\u00b7sen\u00b7wie\u00b7der\u00b7klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gewieher und Drommetenhall", "tokens": ["Ge\u00b7wie\u00b7her", "und", "Drom\u00b7me\u00b7ten\u00b7hall"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Tr\u00e4umt und verdr\u00f6hnt im Wogenschwall.", "tokens": ["Tr\u00e4umt", "und", "ver\u00b7dr\u00f6hnt", "im", "Wo\u00b7gen\u00b7schwall", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Kaiser ritt auf deinem Steg,", "tokens": ["Der", "Kai\u00b7ser", "ritt", "auf", "dei\u00b7nem", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du warst nach Rom der arge Weg,", "tokens": ["Du", "warst", "nach", "Rom", "der", "ar\u00b7ge", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Parricida, frevelbla\u00df,", "tokens": ["Und", "Par\u00b7ri\u00b7ci\u00b7da", ",", "fre\u00b7vel\u00b7bla\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NE", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ward hier vom Staub der Welle na\u00df!", "tokens": ["Ward", "hier", "vom", "Staub", "der", "Wel\u00b7le", "na\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du brachtest nordw\u00e4rts manchen Brief,", "tokens": ["Du", "brach\u00b7test", "nord\u00b7w\u00e4rts", "man\u00b7chen", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drin r\u00f6mische Verleumdung schlief \u2014", "tokens": ["Drin", "r\u00f6\u00b7mi\u00b7sche", "Ver\u00b7leum\u00b7dung", "schlief"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gemengt mit S\u00f6ldnern beuteschwer", "tokens": ["Ge\u00b7mengt", "mit", "S\u00f6ld\u00b7nern", "beu\u00b7te\u00b7schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schlich Pest und schwarzer Tod daher!", "tokens": ["Schlich", "Pest", "und", "schwar\u00b7zer", "Tod", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJA", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vorbei! Vor\u00fcber ohne Spur!", "tokens": ["Vor\u00b7bei", "!", "Vor\u00b7\u00fc\u00b7ber", "oh\u00b7ne", "Spur", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du fielest heim an die Natur,", "tokens": ["Du", "fie\u00b7lest", "heim", "an", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die dich umwildert, dich umgr\u00fcnt,", "tokens": ["Die", "dich", "um\u00b7wil\u00b7dert", ",", "dich", "um\u00b7gr\u00fcnt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "$,", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Tritt des Menschen dich ents\u00fchnt!", "tokens": ["Vom", "Tritt", "des", "Men\u00b7schen", "dich", "ent\u00b7s\u00fchnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}