{"dta.poem.46": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Stimme GOttes im Donner.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der rege Kreis der schw\u00fclen L\u00fcfte,", "tokens": ["Der", "re\u00b7ge", "Kreis", "der", "schw\u00fc\u00b7len", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wodurch die Sonne feurig gl\u00fcht,", "tokens": ["Wo\u00b7durch", "die", "Son\u00b7ne", "feu\u00b7rig", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verschlingt in seinem Raum die", "tokens": ["Ver\u00b7schlingt", "in", "sei\u00b7nem", "Raum", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Licht und Strahl zur H\u00f6he", "tokens": ["Die", "Licht", "und", "Strahl", "zur", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des grossen Lichtes blitzend Flam-", "tokens": ["Des", "gros\u00b7sen", "Lich\u00b7tes", "blit\u00b7zend", "Flam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "TRUNC"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lokt aus der Erden Schoos zusammen,", "tokens": ["Lokt", "aus", "der", "Er\u00b7den", "Schoos", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Den Schwefel-Dunst der darin liegt:", "tokens": ["Den", "Schwe\u00b7fel\u00b7Dunst", "der", "da\u00b7rin", "liegt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der durch die W\u00e4rme los gezogen,", "tokens": ["Der", "durch", "die", "W\u00e4r\u00b7me", "los", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PTKVZ", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Als leichter Stof zum blauen Bogen,", "tokens": ["Als", "leich\u00b7ter", "Stof", "zum", "blau\u00b7en", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Zur d\u00fcnn gewebten H\u00f6he fliegt.", "tokens": ["Zur", "d\u00fcnn", "ge\u00b7web\u00b7ten", "H\u00f6\u00b7he", "fliegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Wolken die zusammen rennen,", "tokens": ["Die", "Wol\u00b7ken", "die", "zu\u00b7sam\u00b7men", "ren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Beschw\u00e4rzen ienen Oberkreis;", "tokens": ["Be\u00b7schw\u00e4r\u00b7zen", "ie\u00b7nen", "O\u00b7ber\u00b7kreis", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Schweffel f\u00e4nget an zu brennen", "tokens": ["Der", "Schwef\u00b7fel", "f\u00e4n\u00b7get", "an", "zu", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf seines Sch\u00f6pfers Machtgeheis.", "tokens": ["Auf", "sei\u00b7nes", "Sch\u00f6p\u00b7fers", "Macht\u00b7ge\u00b7heis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die f\u00fcrchterliche Welt die zittert,", "tokens": ["Die", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Welt", "die", "zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dieweil die Luft so drohend wittert,", "tokens": ["Die\u00b7weil", "die", "Luft", "so", "dro\u00b7hend", "wit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie siehet auf der d\u00fcstren Bahn,", "tokens": ["Sie", "sie\u00b7het", "auf", "der", "d\u00fcst\u00b7ren", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Des H\u00f6chsten schnellen Wolken Wagen,", "tokens": ["Des", "H\u00f6chs\u00b7ten", "schnel\u00b7len", "Wol\u00b7ken", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Noch eh er rasselt, schon mit Zagen,", "tokens": ["Noch", "eh", "er", "ras\u00b7selt", ",", "schon", "mit", "Za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als eines Wetters Vortrab an.", "tokens": ["Als", "ei\u00b7nes", "Wet\u00b7ters", "Vor\u00b7trab", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der HErr erscheint in seinem Grimme,", "tokens": ["Der", "Herr", "er\u00b7scheint", "in", "sei\u00b7nem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des schnellen Blizzes Feuerstrahl", "tokens": ["Des", "schnel\u00b7len", "Bliz\u00b7zes", "Feu\u00b7er\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Entz\u00fcndet sich, und ", "tokens": ["Ent\u00b7z\u00fcn\u00b7det", "sich", ",", "und"], "token_info": ["word", "word", "punct", "word"], "pos": ["VVFIN", "PRF", "$,", "KON"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Ersch\u00fcttert Berge, Wald und Thal.", "tokens": ["Er\u00b7sch\u00fct\u00b7tert", "Ber\u00b7ge", ",", "Wald", "und", "Thal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auf eine f\u00fcrchterliche Stille,", "tokens": ["Auf", "ei\u00b7ne", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Enth\u00f6nnt des Donners stark Gebr\u00fclle", "tokens": ["En\u00b7th\u00f6nnt", "des", "Don\u00b7ners", "stark", "Ge\u00b7br\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Und rollet durch den schwarzen Dufft:", "tokens": ["Und", "rol\u00b7let", "durch", "den", "schwar\u00b7zen", "Dufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Bliz verk\u00fcndigt wieder Schl\u00e4ge,", "tokens": ["Der", "Bliz", "ver\u00b7k\u00fcn\u00b7digt", "wie\u00b7der", "Schl\u00e4\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So bald ist alles wieder rege,", "tokens": ["So", "bald", "ist", "al\u00b7les", "wie\u00b7der", "re\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PIS", "ADV", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "In dem zerborstnen Schlauch der Lufft.", "tokens": ["In", "dem", "zer\u00b7borst\u00b7nen", "Schlauch", "der", "Lufft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da Bliz auf blizzen Knall auf knallen,", "tokens": ["Da", "Bliz", "auf", "bliz\u00b7zen", "Knall", "auf", "knal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ADJA", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erbebt des Himmels Firmament,", "tokens": ["Er\u00b7bebt", "des", "Him\u00b7mels", "Fir\u00b7ma\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das durch des Feuers fliessend Wallen,", "tokens": ["Das", "durch", "des", "Feu\u00b7ers", "flies\u00b7send", "Wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gleichsam in lichten Flammen brennt:", "tokens": ["Gleich\u00b7sam", "in", "lich\u00b7ten", "Flam\u00b7men", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.5": {"text": "Es sprudeln die entglomnen G\u00fcsse,", "tokens": ["Es", "spru\u00b7deln", "die", "ent\u00b7glom\u00b7nen", "G\u00fcs\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Schweffel schwangren Wolken-Fl\u00fcsse", "tokens": ["Der", "Schwef\u00b7fel", "schwang\u00b7ren", "Wol\u00b7ken\u00b7F\u00b7l\u00fcs\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und gehn entsezlich hin und her,", "tokens": ["Und", "gehn", "ent\u00b7sez\u00b7lich", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Es zischt und pfeift mit dem Geheule,", "tokens": ["Es", "zischt", "und", "pfeift", "mit", "dem", "Ge\u00b7heu\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es flattern die gestrahlten Pfeile,", "tokens": ["Es", "flat\u00b7tern", "die", "ge\u00b7strahl\u00b7ten", "Pfei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Noch aus dem feurgen Schweffel-Meer.", "tokens": ["Noch", "aus", "dem", "feur\u00b7gen", "Schwef\u00b7fel\u00b7Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Lufft wird dadurch ausgedehnet,", "tokens": ["Die", "Lufft", "wird", "da\u00b7durch", "aus\u00b7ge\u00b7deh\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und schnell bewegt und fort gedr\u00fckt:", "tokens": ["Und", "schnell", "be\u00b7wegt", "und", "fort", "ge\u00b7dr\u00fckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "KON", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Donner rasselt und erth\u00f6net,", "tokens": ["Der", "Don\u00b7ner", "ras\u00b7selt", "und", "er\u00b7th\u00f6\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Davor die ganze Welt erschrikt.", "tokens": ["Da\u00b7vor", "die", "gan\u00b7ze", "Welt", "er\u00b7schrikt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Berge starre F\u00fcsse beben,", "tokens": ["Der", "Ber\u00b7ge", "star\u00b7re", "F\u00fcs\u00b7se", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wolten sie sich weiter heben;", "tokens": ["Als", "wol\u00b7ten", "sie", "sich", "wei\u00b7ter", "he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Meereswellen Wirbel-Fluht", "tokens": ["Der", "Mee\u00b7res\u00b7wel\u00b7len", "Wir\u00b7bel\u00b7Fluht"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00e4ngt bei des Himmels grausen St\u00fcrmen,", "tokens": ["F\u00e4ngt", "bei", "des", "Him\u00b7mels", "grau\u00b7sen", "St\u00fcr\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Erschreklich an, sich auf zu th\u00fcrmen,", "tokens": ["Er\u00b7schrek\u00b7lich", "an", ",", "sich", "auf", "zu", "th\u00fcr\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,", "PRF", "APPR", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und sch\u00e4umt bei des Gewitters Wuth.", "tokens": ["Und", "sch\u00e4umt", "bei", "des", "Ge\u00b7wit\u00b7ters", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Es wanken die besaamten Felder,", "tokens": ["Es", "wan\u00b7ken", "die", "be\u00b7saam\u00b7ten", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dadurch der Blizzen Schrekstrahl f\u00e4hrt,", "tokens": ["Da\u00b7durch", "der", "Bliz\u00b7zen", "Schreks\u00b7trahl", "f\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es krachen die belaubten W\u00e4lder;", "tokens": ["Es", "kra\u00b7chen", "die", "be\u00b7laub\u00b7ten", "W\u00e4l\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als w\u00fcrden sie ganz umgekehrt,", "tokens": ["Als", "w\u00fcr\u00b7den", "sie", "ganz", "um\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Strahl f\u00e4hrt aus, durchstreicht die Eichen,", "tokens": ["Ein", "Strahl", "f\u00e4hrt", "aus", ",", "durch\u00b7streicht", "die", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da liegt der Gipfel mit Gestr\u00e4uchen,", "tokens": ["Da", "liegt", "der", "Gip\u00b7fe