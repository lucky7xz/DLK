{"textgrid.poem.61811": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.71", "da:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun kommt der Sturm geflogen,", "tokens": ["Nun", "kommt", "der", "Sturm", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der heulende Nordost,", "tokens": ["Der", "heu\u00b7len\u00b7de", "Nord\u00b7ost", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Da\u00df hoch in Riesenwogen", "tokens": ["Da\u00df", "hoch", "in", "Rie\u00b7sen\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die See ans Ufer tost.", "tokens": ["Die", "See", "ans", "U\u00b7fer", "tost", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das ist ein rasend Gischen,", "tokens": ["Das", "ist", "ein", "ra\u00b7send", "Gi\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Donnern und ein Schwall,", "tokens": ["Ein", "Don\u00b7nern", "und", "ein", "Schwall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gew\u00f6lk und Abgrund mischen", "tokens": ["Ge\u00b7w