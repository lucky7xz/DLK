{"dta.poem.21688": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Hymne an das Eisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Heil dir, Mark der Natur, der gabenspendenden\nErde", "tokens": ["Heil", "dir", ",", "Mark", "der", "Na\u00b7tur", ",", "der", "ga\u00b7ben\u00b7spen\u00b7den\u00b7den", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Stilles Erzeugniss, doch gross von Kraft und herrlich", "tokens": ["Stil\u00b7les", "Er\u00b7zeug\u00b7niss", ",", "doch", "gross", "von", "Kraft", "und", "herr\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "ADV", "ADJD", "APPR", "NN", "KON", "ADJD"], "meter": "+-----+-+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Nimmer r\u00fchmt' ich das Gold, und dein, jungfr\u00e4uli-", "tokens": ["Nim\u00b7mer", "r\u00fchmt'", "ich", "das", "Gold", ",", "und", "dein", ",", "jung\u00b7fr\u00e4u\u00b7li"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "PPOSAT", "$,", "TRUNC"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Dacht' ich nimmer im Liede. Dir aber, Preis der", "tokens": ["Dacht'", "ich", "nim\u00b7mer", "im", "Lie\u00b7de", ".", "Dir", "a\u00b7ber", ",", "Preis", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "PPER", "ADV", "$,", "NN", "ART"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Will ich Ehre verleihn, und deine Tugenden singen.", "tokens": ["Will", "ich", "Eh\u00b7re", "ver\u00b7leihn", ",", "und", "dei\u00b7ne", "Tu\u00b7gen\u00b7den", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$,", "KON", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}}, "stanza.2": {"line.1": {"text": "Heil dir, \u00e4ltestes Kind der Geb\u00fcrg'! und ihr edel-", "tokens": ["Heil", "dir", ",", "\u00e4l\u00b7tes\u00b7tes", "Kind", "der", "Ge\u00b7b\u00fcr\u00b7g'", "!", "und", "ihr", "e\u00b7del"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "ADJA", "NN", "ART", "NN", "$.", "KON", "PPOSAT", "TRUNC"], "meter": "+-+--+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Erstgeborner im Reiche der vielgestalteten Erze.", "tokens": ["Erst\u00b7ge\u00b7bor\u00b7ner", "im", "Rei\u00b7che", "der", "viel\u00b7ge\u00b7stal\u00b7te\u00b7ten", "Er\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NE", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Schon in der Dinge Beginn, als die uranf\u00e4nglichen", "tokens": ["Schon", "in", "der", "Din\u00b7ge", "Be\u00b7ginn", ",", "als", "die", "u\u00b7ran\u00b7f\u00e4ng\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,", "KOUS", "ART", "ADJA"], "meter": "-+-+--+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Jegliches Stoffes tr\u00e4chtig die kraysende Erde noch", "tokens": ["Jeg\u00b7li\u00b7ches", "Stof\u00b7fes", "tr\u00e4ch\u00b7tig", "die", "kray\u00b7sen\u00b7de", "Er\u00b7de", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJD", "ART", "ADJA", "NN", "ADV"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Schwebtest du in dem unendlichen Meere, geselltest", "tokens": ["Schweb\u00b7test", "du", "in", "dem", "un\u00b7end\u00b7li\u00b7chen", "Mee\u00b7re", ",", "ge\u00b7sell\u00b7test"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,", "VVPP"], "meter": "------+--+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Zu dem Ges\u00e4me des Quarz, zu des Feldspath binden-", "tokens": ["Zu", "dem", "Ge\u00b7s\u00e4\u00b7me", "des", "Quarz", ",", "zu", "des", "Feld\u00b7spath", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "TRUNC"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.7": {"text": "Schw\u00e4rztest den ernsten Sch\u00f6rl, durchblinktest die", "tokens": ["Schw\u00e4rz\u00b7test", "den", "erns\u00b7ten", "Sch\u00f6rl", ",", "durch\u00b7blink\u00b7test", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "H\u00e4rtetest, heilige Kraft, die Granitgerippe des", "tokens": ["H\u00e4r\u00b7te\u00b7test", ",", "hei\u00b7li\u00b7ge", "Kraft", ",", "die", "Gra\u00b7nit\u00b7ge\u00b7rip\u00b7pe", "des"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ART", "NN", "ART"], "meter": "+--+--+-+--+--", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Aber zu rasten vermochtest du nicht mit dem", "tokens": ["A\u00b7ber", "zu", "ras\u00b7ten", "ver\u00b7moch\u00b7test", "du", "nicht", "mit", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKZU", "VVINF", "VVFIN", "PPER", "PTKNEG", "APPR", "ART"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nicht zu b\u00e4ndigen taugte den Trotz des Titanen der", "tokens": ["Nicht", "zu", "b\u00e4n\u00b7di\u00b7gen", "taug\u00b7te", "den", "Trotz", "des", "Ti\u00b7ta\u00b7nen", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKZU", "VVINF", "VVFIN", "ART", "NN", "ART", "NN", "ART"], "meter": "+-+--+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Noch der Sudeten Gewicht, das schwer auf die", "tokens": ["Noch", "der", "Su\u00b7de\u00b7ten", "Ge\u00b7wicht", ",", "das", "schwer", "auf", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "APPR", "ART"], "meter": "+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Tief aufst\u00f6hnend enthobst du der Last dich; aus", "tokens": ["Tief", "auf\u00b7st\u00f6h\u00b7nend", "ent\u00b7hobst", "du", "der", "Last", "dich", ";", "aus"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "VVPP", "VVFIN", "PPER", "ART", "NN", "PPER", "$.", "APPR"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Quollst du hervor, ein Feuerstrom, gerannst zum", "tokens": ["Quollst", "du", "her\u00b7vor", ",", "ein", "Feu\u00b7er\u00b7strom", ",", "ge\u00b7rannst", "zum"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ART", "NN", "$,", "ADV", "APPRART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Pflastertest Riesenweg', und w\u00f6lbetest Grotten des", "tokens": ["Pflas\u00b7ter\u00b7test", "Rie\u00b7sen\u00b7weg'", ",", "und", "w\u00f6l\u00b7be\u00b7test", "Grot\u00b7ten", "des"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "KON", "VVFIN", "NN", "ART"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Rastlos g\u00e4hrt' es indess in des Meers arbeiten-", "tokens": ["Rast\u00b7los", "g\u00e4hrt'", "es", "in\u00b7dess", "in", "des", "Meers", "ar\u00b7bei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Niederschlugen die Lager der Erden, des Thons und", "tokens": ["Nie\u00b7der\u00b7schlu\u00b7gen", "die", "La\u00b7ger", "der", "Er\u00b7den", ",", "des", "Thons", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "ART", "NN", "KON"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wechselnde Schichten. Wer sprengte die M\u00e4chtigen?", "tokens": ["Wech\u00b7seln\u00b7de", "Schich\u00b7ten", ".", "Wer", "spreng\u00b7te", "die", "M\u00e4ch\u00b7ti\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "H\u00f6hlt' im gediegenen Fl\u00f6tz der G\u00e4ng' und Minen", "tokens": ["H\u00f6hlt'", "im", "ge\u00b7die\u00b7ge\u00b7nen", "Fl\u00f6tz", "der", "G\u00e4ng'", "und", "Mi\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "ADJA", "NN", "ART", "NN", "KON", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Labyrinthisch Gekl\u00fcft? Du thatest es, Heros! Und", "tokens": ["La\u00b7by\u00b7rin\u00b7thisch", "Ge\u00b7kl\u00fcft", "?", "Du", "tha\u00b7test", "es", ",", "He\u00b7ros", "!", "Und"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "PPER", "$,", "NE", "$.", "KON"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Dich zu entwinden dem sp\u00e4henden Blick hell\u00e4ugiger", "tokens": ["Dich", "zu", "ent\u00b7win\u00b7den", "dem", "sp\u00e4\u00b7hen\u00b7den", "Blick", "hel\u00b7l\u00e4u\u00b7gi\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "ART", "ADJA", "NN", "ADJA"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.7": {"text": "W\u00e4hltest du dir zum geheimeren Sitz das verborgne", "tokens": ["W\u00e4hl\u00b7test", "du", "dir", "zum", "ge\u00b7hei\u00b7me\u00b7ren", "Sitz", "das", "ver\u00b7borg\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Lauschest dort in des Dunkels Schirm in mancher", "tokens": ["Lau\u00b7schest", "dort", "in", "des", "Dun\u00b7kels", "Schirm", "in", "man\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "APPR", "PIAT"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.9": {"text": "Bald gel\u00fcstet es dich, als Druse zu blinken. Be-", "tokens": ["Bald", "ge\u00b7l\u00fcs\u00b7tet", "es", "dich", ",", "als", "Dru\u00b7se", "zu", "blin\u00b7ken", ".", "Be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$.", "TRUNC"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Birgst du ein anderes Mal dich in unscheinbarer", "tokens": ["Birgst", "du", "ein", "an\u00b7de\u00b7res", "Mal", "dich", "in", "un\u00b7schein\u00b7ba\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "PRF", "APPR", "PPOSAT"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Tropfest itzt 'gar die Teufe herab ein nichtiges", "tokens": ["Trop\u00b7fest", "itzt", "'gar", "die", "Teu\u00b7fe", "her\u00b7ab", "ein", "nich\u00b7ti\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "ADV", "ART", "ADJA"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.12": {"text": "Bl\u00fchest als Blume dann, und schossest ein \u00e4stiges", "tokens": ["Bl\u00fc\u00b7hest", "als", "Blu\u00b7me", "dann", ",", "und", "schos\u00b7sest", "ein", "\u00e4s\u00b7ti\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KOUS", "NN", "ADV", "$,", "KON", "VVFIN", "ART", "ADJA"], "meter": "+--+-+-+--+-+", "measure": "iambic.hexa.invert"}}, "stanza.5": {"line.1": {"text": "Tausend sind deiner Launen und deiner Verlar-", "tokens": ["Tau\u00b7send", "sind", "dei\u00b7ner", "Lau\u00b7nen", "und", "dei\u00b7ner", "Ver\u00b7la\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "TRUNC"], "meter": "+-++-+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Dennoch fessl' ich dich, Proteus, mit m\u00e4chtigem Zau-", "tokens": ["Den\u00b7noch", "fessl'", "ich", "dich", ",", "Pro\u00b7teus", ",", "mit", "m\u00e4ch\u00b7ti\u00b7gem", "Zau"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$,", "NE", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Dich ertapp' ich im wilden Gestein, in der bl\u00e4ulich-", "tokens": ["Dich", "er\u00b7tapp'", "ich", "im", "wil\u00b7den", "Ge\u00b7stein", ",", "in", "der", "bl\u00e4u\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dich in des Schmirgels z\u00e4herem Korn, im fl\u00fcchtigen", "tokens": ["Dich", "in", "des", "Schmir\u00b7gels", "z\u00e4\u00b7he\u00b7rem", "Korn", ",", "im", "fl\u00fcch\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,", "APPRART", "ADJA"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Freundschaft pflegst du mit jedem Genossen der wu-", "tokens": ["Freund\u00b7schaft", "pflegst", "du", "mit", "je\u00b7dem", "Ge\u00b7nos\u00b7sen", "der", "wu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "ART", "TRUNC"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Willig gesellen sich dir die minder edleren", "tokens": ["Wil\u00b7lig", "ge\u00b7sel\u00b7len", "sich", "dir", "die", "min\u00b7der", "ed\u00b7le\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "PRF", "PPER", "ART", "ADJA", "ADJA"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber nur ungern gehorcht dir der Sol; die z\u00fcchtige", "tokens": ["A\u00b7ber", "nur", "un\u00b7gern", "ge\u00b7horcht", "dir", "der", "Sol", ";", "die", "z\u00fcch\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "ADJA"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Wegert sich lang; es str\u00e4ubt sich versch\u00e4mt die keu-", "tokens": ["We\u00b7gert", "sich", "lang", ";", "es", "str\u00e4ubt", "sich", "ver\u00b7sch\u00e4mt", "die", "keu"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADJD", "$.", "PPER", "VVFIN", "PRF", "ADJD", "ART", "TRUNC"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "K\u00fchner Ares, du steigst hinab in der sch\u00fcchter-", "tokens": ["K\u00fch\u00b7ner", "A\u00b7res", ",", "du", "steigst", "hin\u00b7ab", "in", "der", "sch\u00fcch\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "ADV", "APPR", "ART", "TRUNC"], "meter": "+-+-++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Heimliche Grotte. Verwegen umschlingst du die Bl\u00f6-", "tokens": ["Heim\u00b7li\u00b7che", "Grot\u00b7te", ".", "Ver\u00b7we\u00b7gen", "um\u00b7schlingst", "du", "die", "Bl\u00f6"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "NN", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "Sinket sie dir in den br\u00fcnstigen Arm. Der Umar-", "tokens": ["Sin\u00b7ket", "sie", "dir", "in", "den", "br\u00fcns\u00b7ti\u00b7gen", "Arm", ".", "Der", "Um\u00b7a\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$.", "ART", "TRUNC"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.4": {"text": "Heilende