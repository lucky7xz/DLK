{"dta.poem.18045": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man sagt, da\u00df eine Flieg in Ungest\u00fcme flog,", "tokens": ["Man", "sagt", ",", "da\u00df", "ei\u00b7ne", "Flieg", "in", "Un\u00b7ge\u00b7st\u00fc\u00b7me", "flog", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und einen kahlen Mann vielmahls betrog.", "tokens": ["Und", "ei\u00b7nen", "kah\u00b7len", "Mann", "viel\u00b7mahls", "be\u00b7trog", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Indem sie ihm oft an die Stirne fa\u00df;", "tokens": ["In\u00b7dem", "sie", "ihm", "oft", "an", "die", "Stir\u00b7ne", "fa\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn er dann seiner nicht verga\u00df,", "tokens": ["Wenn", "er", "dann", "sei\u00b7ner", "nicht", "ver\u00b7ga\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und mit der Hand oft nach ihr schlug,", "tokens": ["Und", "mit", "der", "Hand", "oft", "nach", "ihr", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da floh sie bald und schnell genug.", "tokens": ["Da", "floh", "sie", "bald", "und", "schnell", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "ADJD", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie trieb hernach noch ihr Gesp\u00f6tte,", "tokens": ["Sie", "trieb", "her\u00b7nach", "noch", "ihr", "Ge\u00b7sp\u00f6t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df er sich selbst geschlagen h\u00e4tte.", "tokens": ["Da\u00df", "er", "sich", "selbst", "ge\u00b7schla\u00b7gen", "h\u00e4t\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bald aber flog sie wieder dar.", "tokens": ["Bald", "a\u00b7ber", "flog", "sie", "wie\u00b7der", "dar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der kahle Mann nimmt ihrer wahr", "tokens": ["Der", "kah\u00b7le", "Mann", "nimmt", "ih\u00b7rer", "wahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und spricht: O Fliege h\u00f6r was ich dir sage.", "tokens": ["Und", "spricht", ":", "O", "Flie\u00b7ge", "h\u00f6r", "was", "ich", "dir", "sa\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NE", "NN", "VVFIN", "PWS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Ob ich dir deinen Spott vertrage,", "tokens": ["Ob", "ich", "dir", "dei\u00b7nen", "Spott", "ver\u00b7tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und ob ich mich gleich zehnmahl schlage:", "tokens": ["Und", "ob", "ich", "mich", "gleich", "zehn\u00b7mahl", "schla\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "So werd ich doch davon nicht wund,", "tokens": ["So", "werd", "ich", "doch", "da\u00b7von", "nicht", "wund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ich bleib hernach wie vor gesund.", "tokens": ["Ich", "bleib", "her\u00b7nach", "wie", "vor", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOKOM", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dein Stachel t\u00f6det auch nicht mich;", "tokens": ["Dein", "Sta\u00b7chel", "t\u00f6\u00b7det", "auch", "nicht", "mich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "PTKNEG", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Tr\u00e4ff\u2019 aber ich nur einmahl dich,", "tokens": ["Tr\u00e4ff'", "a\u00b7ber", "ich", "nur", "ein\u00b7mahl", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "PPER", "ADV", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So l\u00e4gest du im Staube todt.", "tokens": ["So", "l\u00e4\u00b7gest", "du", "im", "Stau\u00b7be", "todt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Begiebt sich jemand selbst in Noth", "tokens": ["Be\u00b7giebt", "sich", "je\u00b7mand", "selbst", "in", "Noth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PIS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Durch kleinen Schaden, den er thut,", "tokens": ["Durch", "klei\u00b7nen", "Scha\u00b7den", ",", "den", "er", "thut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Der hat wohl einen tummen Muth.", "tokens": ["Der", "hat", "wohl", "ei\u00b7nen", "tum\u00b7men", "Muth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}