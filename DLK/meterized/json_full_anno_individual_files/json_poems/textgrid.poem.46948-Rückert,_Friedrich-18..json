{"textgrid.poem.46948": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdas Schwert, das Schwert, das ich in meinen Tagen", "tokens": ["\u00bb", "das", "Schwert", ",", "das", "Schwert", ",", "das", "ich", "in", "mei\u00b7nen", "Ta\u00b7gen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschwungen, ich verga\u00df, in wieviel Schlachten,", "tokens": ["Ge\u00b7schwun\u00b7gen", ",", "ich", "ver\u00b7ga\u00df", ",", "in", "wie\u00b7viel", "Schlach\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "$,", "APPR", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Schwert, ob dessen Klang nicht Feinde lachten,", "tokens": ["Das", "Schwert", ",", "ob", "des\u00b7sen", "Klang", "nicht", "Fein\u00b7de", "lach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PRELAT", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als sie bei Ro\u00dfbach und bei Lissa lagen!", "tokens": ["Als", "sie", "bei", "Ro\u00df\u00b7bach", "und", "bei", "Lis\u00b7sa", "la\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NE", "KON", "APPR", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das Schwert! Wer nahm's von meinen Sarkophagen?", "tokens": ["Das", "Schwert", "!", "Wer", "nahm's", "von", "mei\u00b7nen", "Sar\u00b7ko\u00b7pha\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PWS", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wes sind die H\u00e4nde, die so keck sich machten,", "tokens": ["Wes", "sind", "die", "H\u00e4n\u00b7de", ",", "die", "so", "keck", "sich", "mach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "PRF", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.7": {"text": "Da\u00df sie von dort zu seiner Schmach es brachten", "tokens": ["Da\u00df", "sie", "von", "dort", "zu", "sei\u00b7ner", "Schmach", "es", "brach\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dahin, wo niemand ist, der es kann tragen?", "tokens": ["Da\u00b7hin", ",", "wo", "nie\u00b7mand", "ist", ",", "der", "es", "kann", "tra\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PWAV", "PIS", "VAFIN", "$,", "PRELS", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ihr S\u00f6hne Preu\u00dfens aus dem West und Oste!", "tokens": ["Ihr", "S\u00f6h\u00b7ne", "Preu\u00b7\u00dfens", "aus", "dem", "West", "und", "Os\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measur