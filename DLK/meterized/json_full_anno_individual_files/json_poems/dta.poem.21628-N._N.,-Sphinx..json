{"dta.poem.21628": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Sphinx.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Du sch\u00f6nes Kind schau\u2019 mir in\u2019s Angesicht", "tokens": ["Du", "sch\u00f6\u00b7nes", "Kind", "schau'", "mir", "in's", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sprich von Liebe mir ein kleines Wort \u2014", "tokens": ["Und", "sprich", "von", "Lie\u00b7be", "mir", "ein", "klei\u00b7nes", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201eich liebe dich \u2014 (ich hasse dich ja nicht \u2014", "tokens": ["\u201e", "ich", "lie\u00b7be", "dich", "(", "ich", "has\u00b7se", "dich", "ja", "nicht"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$(", "$(", "PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Narr, geht er denn immer noch nicht fort?!)\u201c", "tokens": ["Der", "Narr", ",", "geht", "er", "denn", "im\u00b7mer", "noch", "nicht", "fort", "?!", ")", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PTKNEG", "PTKVZ", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Komm, k\u00fcsse mich, o bleiche Lilie du,", "tokens": ["Komm", ",", "k\u00fcs\u00b7se", "mich", ",", "o", "blei\u00b7che", "Li\u00b7lie", "du", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "FM", "ADJA", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich k\u00fcss\u2019 daf\u00fcr dich wie die Rose roth \u2014", "tokens": ["Ich", "k\u00fcss'", "da\u00b7f\u00fcr", "dich", "wie", "die", "Ro\u00b7se", "roth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PPER", "KOKOM", "ART", "NN", "ADJD", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201eich bin recht m\u00fcd\u2019, mir f\u00e4llt die Wimper zu", "tokens": ["\u201e", "ich", "bin", "recht", "m\u00fcd'", ",", "mir", "f\u00e4llt", "die", "Wim\u00b7per", "zu"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bei dem Geschw\u00e4tz langweil\u2019 ich mich zu Tod.\u201c", "tokens": ["Bei", "dem", "Ge\u00b7schw\u00e4tz", "lang\u00b7weil'", "ich", "mich", "zu", "Tod", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Komm\u2019, plaudre mir geheimni\u00dfvoll und sacht,", "tokens": ["Komm'", ",", "plaud\u00b7re", "mir", "ge\u00b7heim\u00b7ni\u00df\u00b7voll", "und", "sacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So wie die Quelle unter\u2019m Mondenstrahl \u2014", "tokens": ["So", "wie", "die", "Quel\u00b7le", "un\u00b7ter'm", "Mon\u00b7dens\u00b7trahl"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "\u201eich bin gewi\u00df recht h\u00e4\u00dflich \u00fcberwacht,", "tokens": ["\u201e", "ich", "bin", "ge\u00b7wi\u00df", "recht", "h\u00e4\u00df\u00b7lich", "\u00fc\u00b7ber\u00b7wacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das Haar wie wirr, die Wange wie so fahl.\u201c", "tokens": ["Das", "Haar", "wie", "wirr", ",", "die", "Wan\u00b7ge", "wie", "so", "fahl", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "$,", "ART", "NN", "KOKOM", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "\u201eich mu\u00df jetzt fort! Nein, bleibe noch bei mir!", "tokens": ["\u201e", "ich", "mu\u00df", "jetzt", "fort", "!", "Nein", ",", "blei\u00b7be", "noch", "bei", "mir", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "ADV", "PTKVZ", "$.", "PTKANT", "$,", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "(gott Lob, er geht! wie ist zerdr\u00fcckt mein Kleid)\u201c", "tokens": ["(", "gott", "Lob", ",", "er", "geht", "!", "wie", "ist", "zer\u00b7dr\u00fcckt", "mein", "Kleid", ")", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "NN", "$,", "PPER", "VVFIN", "$.", "PWAV", "VAFIN", "VVPP", "PPOSAT", "NN", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch morgen, morgen fliege ich zu dir!", "tokens": ["Doch", "mor\u00b7gen", ",", "mor\u00b7gen", "flie\u00b7ge", "ich", "zu", "dir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "PPER", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "\u201e(Schon morgen? \u2014 l\u00e4g\u2019 das Morgen doch recht weit!)\u201c", "tokens": ["\u201e", "(", "Schon", "mor\u00b7gen", "?", "l\u00e4g'", "das", "Mor\u00b7gen", "doch", "recht", "weit", "!", ")", "\u201c"], "token_info": ["punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "ADV", "ADV", "$.", "$(", "VVFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$.", "$(", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Gieb einen Ku\u00df \u2014 \u201e(Er geht noch immer nicht!)\u201c", "tokens": ["Gieb", "ei\u00b7nen", "Ku\u00df", "\u201e", "(", "Er", "geht", "noch", "im\u00b7mer", "nicht", "!", ")", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVIMP", "ART", "NN", "$(", "$(", "$(", "PPER", "VVFIN", "ADV", "ADV", "PTKNEG", "$.", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch einen Blick \u2014 \u201e(Noch immer folgt ein \u201edann\u201c!)\u201c", "tokens": ["Noch", "ei\u00b7nen", "Blick", "\u201e", "(", "Noch", "im\u00b7mer", "folgt", "ein", "\u201e", "dann", "\u201c", "!", ")", "\u201c"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "word", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["ADV", "ART", "NN", "$(", "$(", "$(", "ADV", "ADV", "VVFIN", "ART", "$(", "ADV", "$(", "$.", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sag\u2019 mir, warum so sch\u00f6n dein Angesicht?", "tokens": ["Sag'", "mir", ",", "wa\u00b7rum", "so", "sch\u00f6n", "dein", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "PWAV", "ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "(\u201eDamit ich dumme Gimpel leimen kann.)\u201c", "tokens": ["(", "\u201e", "Da\u00b7mit", "ich", "dum\u00b7me", "Gim\u00b7pel", "lei\u00b7men", "kann", ".", ")", "\u201c"], "token_info": ["punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "$(", "KOUS", "PPER", "ADJA", "NN", "VVINF", "VMFIN", "$.", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Jetzt lebe wohl! \u201eWillst du denn wirklich gehn?", "tokens": ["Jetzt", "le\u00b7be", "wohl", "!", "\u201e", "Willst", "du", "denn", "wirk\u00b7lich", "gehn", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$.", "$(", "VMFIN", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So schlafe s\u00fc\u00df und tr\u00e4ume hell und sacht \u2014", "tokens": ["So", "schla\u00b7fe", "s\u00fc\u00df", "und", "tr\u00e4u\u00b7me", "hell", "und", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "KON", "VVFIN", "ADJD", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und gute Nacht \u2014 auf Wieder-, Wiedersehn!\u201c \u2014", "tokens": ["Und", "gu\u00b7te", "Nacht", "auf", "Wie\u00b7der", ",", "Wie\u00b7der\u00b7sehn", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["KON", "ADJA", "NN", "$(", "APPR", "TRUNC", "$,", "PWAV", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Wiedersehn, mein Kind, und gute Nacht!", "tokens": ["Auf", "Wie\u00b7der\u00b7sehn", ",", "mein", "Kind", ",", "und", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PPOSAT", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "\u201e(Er geht! Es war recht h\u00e4\u00dflich mein Gesicht \u2014", "tokens": ["\u201e", "(", "Er", "geht", "!", "Es", "war", "recht", "h\u00e4\u00df\u00b7lich", "mein", "Ge\u00b7sicht"], "token_info": ["punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Er fand es sch\u00f6n \u2014 \u2014 er schmeichelte vielleicht?", "tokens": ["Er", "fand", "es", "sch\u00f6n", "er", "schmei\u00b7chel\u00b7te", "viel\u00b7leicht", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "$(", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie leicht man doch das W\u00f6rtchen \u201eLiebe\u201c spricht", "tokens": ["Wie", "leicht", "man", "doch", "das", "W\u00f6rt\u00b7chen", "\u201e", "Lie\u00b7be", "\u201c", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "PIS", "ADV", "ART", "NN", "$(", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wiederum wie glaubt man es uns leicht!", "tokens": ["Und", "wie\u00b7de\u00b7rum", "wie", "glaubt", "man", "es", "uns", "leicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "VVFIN", "PIS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Das Armband hat er endlich mir gebracht \u2014", "tokens": ["Das", "Arm\u00b7band", "hat", "er", "end\u00b7lich", "mir", "ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob echt die Steine? morgen leg ich\u2019s an \u2014 \u2014", "tokens": ["Ob", "echt", "die", "Stei\u00b7ne", "?", "mor\u00b7gen", "leg", "ich's", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "$.", "ADV", "NE", "PIS", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch jetzt zu Bett! es flieht so schnell die Nacht!", "tokens": ["Doch", "jetzt", "zu", "Bett", "!", "es", "flieht", "so", "schnell", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "$.", "PPER", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Seidenkleid w\u00fcnsch ich mir, nun und dann \u2026)\u201c", "tokens": ["Ein", "Sei\u00b7den\u00b7kleid", "w\u00fcnsch", "ich", "mir", ",", "nun", "und", "dann", "\u2026", ")", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "KON", "ADV", "$(", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Sie schlummert schon. Er aber wacht in Qual,", "tokens": ["Sie", "schlum\u00b7mert", "schon", ".", "Er", "a\u00b7ber", "wacht", "in", "Qual", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und lauscht wie tr\u00e4ge jede Stunde rinnt \u2014", "tokens": ["Und", "lauscht", "wie", "tr\u00e4\u00b7ge", "je\u00b7de", "Stun\u00b7de", "rinnt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "VVFIN", "PIAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und als der Morgen naht so bla\u00df, so fahl,", "tokens": ["Und", "als", "der", "Mor\u00b7gen", "naht", "so", "bla\u00df", ",", "so", "fahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da betet hei\u00df er f\u00fcr sein bleiches Kind. \u2014", "tokens": ["Da", "be\u00b7tet", "hei\u00df", "er", "f\u00fcr", "sein", "blei\u00b7ches", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}