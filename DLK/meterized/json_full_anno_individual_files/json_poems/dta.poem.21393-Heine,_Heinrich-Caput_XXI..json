{"dta.poem.21393": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XXI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Stadt, zur H\u00e4lfte abgebrannt,", "tokens": ["Die", "Stadt", ",", "zur", "H\u00e4lf\u00b7te", "ab\u00b7ge\u00b7brannt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird aufgebaut allm\u00e4hlig;", "tokens": ["Wird", "auf\u00b7ge\u00b7baut", "all\u00b7m\u00e4h\u00b7lig", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie\u2019n Pudel, der halb geschoren ist,", "tokens": ["Wie'n", "Pu\u00b7del", ",", "der", "halb", "ge\u00b7scho\u00b7ren", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sieht Hamburg aus, tr\u00fcbselig.", "tokens": ["Sieht", "Ham\u00b7burg", "aus", ",", "tr\u00fcb\u00b7se\u00b7lig", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "NE", "PTKVZ", "$,", "ADJD", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}}}}}