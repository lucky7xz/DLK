{"dta.poem.8463": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Besch\u00e4ftigungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Und franz\u00f6sischen Modewiz,", "tokens": ["Und", "fran\u00b7z\u00f6\u00b7si\u00b7schen", "Mo\u00b7de\u00b7wiz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "K\u00fcsst den Damen die Hand, mischet den Pot pourri,", "tokens": ["K\u00fcsst", "den", "Da\u00b7men", "die", "Hand", ",", "mi\u00b7schet", "den", "Pot", "pou\u00b7rri", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "NE", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Kocht Pommaden und strickt Filet;", "tokens": ["Kocht", "Pom\u00b7ma\u00b7den", "und", "strickt", "Fi\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "VVFIN", "NE", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Zieht die S\u00e4le voll Tanz Wiesen des Fr\u00fchlings vor,", "tokens": ["Zieht", "die", "S\u00e4\u00b7le", "voll", "Tanz", "Wie\u00b7sen", "des", "Fr\u00fch\u00b7lings", "vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "--+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Roms Kastraten der Nachtigall;", "tokens": ["Roms", "Kas\u00b7tra\u00b7ten", "der", "Nach\u00b7ti\u00b7gall", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Lebt vom L\u00e4cheln des Herrn, dreht, wie ein Wetterhahn,", "tokens": ["Lebt", "vom", "L\u00e4\u00b7cheln", "des", "Herrn", ",", "dreht", ",", "wie", "ein", "Wet\u00b7ter\u00b7hahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,", "VVFIN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Nach dem Winde des Hofes sich.", "tokens": ["Nach", "dem", "Win\u00b7de", "des", "Ho\u00b7fes", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "PRF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.3": {"line.1": {"text": "Dieser liebet den Prunk gleissender Wissenschaft,", "tokens": ["Die\u00b7ser", "lie\u00b7bet", "den", "Prunk", "gleis\u00b7sen\u00b7der", "Wis\u00b7sen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Stapelt B\u00fccher auf B\u00fccher auf,", "tokens": ["Sta\u00b7pelt", "B\u00fc\u00b7cher", "auf", "B\u00fc\u00b7cher", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und begaffet den Band, und den bemalten Schnitt,", "tokens": ["Und", "be\u00b7gaf\u00b7fet", "den", "Band", ",", "und", "den", "be\u00b7mal\u00b7ten", "Schnitt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und den gl\u00e4sernen B\u00fccherschrank.", "tokens": ["Und", "den", "gl\u00e4\u00b7ser\u00b7nen", "B\u00fc\u00b7cher\u00b7schrank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Jener schachert umher, wie ein Beschnittener,", "tokens": ["Je\u00b7ner", "scha\u00b7chert", "um\u00b7her", ",", "wie", "ein", "Be\u00b7schnit\u00b7te\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDAT", "VVFIN", "PTKVZ", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+--+-+-+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Stopfet Beutel auf Beutel voll;", "tokens": ["Stop\u00b7fet", "Beu\u00b7tel", "auf", "Beu\u00b7tel", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Schliesst sein K\u00e4mmerlein zu, sch\u00fcttelt die Beutel aus,", "tokens": ["Schliesst", "sein", "K\u00e4m\u00b7mer\u00b7lein", "zu", ",", "sch\u00fct\u00b7telt", "die", "Beu\u00b7tel", "aus", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und be\u00e4ugelt den Seelenschaz.", "tokens": ["Und", "be\u00b7\u00e4u\u00b7gelt", "den", "See\u00b7len\u00b7schaz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Mich entz\u00fccket der Wald, mich der entbl\u00fchte Baum,", "tokens": ["Mich", "ent\u00b7z\u00fc\u00b7cket", "der", "Wald", ",", "mich", "der", "ent\u00b7bl\u00fch\u00b7te", "Baum", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Mich der tanzende Wiesenquell,", "tokens": ["Mich", "der", "tan\u00b7zen\u00b7de", "Wie\u00b7sen\u00b7quell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Mich der Morgengesang oder das Abendlied", "tokens": ["Mich", "der", "Mor\u00b7gen\u00b7ge\u00b7sang", "o\u00b7der", "das", "A\u00b7ben\u00b7dlied"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Meiner Freundin der Nachtigall.", "tokens": ["Mei\u00b7ner", "Freun\u00b7din", "der", "Nach\u00b7ti\u00b7gall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "D\u00e4mmert endlich mein Traum heiter zum Leben auf,", "tokens": ["D\u00e4m\u00b7mert", "end\u00b7lich", "mein", "Traum", "hei\u00b7ter", "zum", "Le\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ADJD", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Giebt der Himmel das M\u00e4dchen mir,", "tokens": ["Giebt", "der", "Him\u00b7mel", "das", "M\u00e4d\u00b7chen", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Dessen l\u00e4chelndes Bild mir um die Seele schwebt;", "tokens": ["Des\u00b7sen", "l\u00e4\u00b7cheln\u00b7des", "Bild", "mir", "um", "die", "See\u00b7le", "schwebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Dann, dann bin ich ein Erdengott!", "tokens": ["Dann", ",", "dann", "bin", "ich", "ein", "Er\u00b7den\u00b7gott", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie ein m\u00e4chtiger Gott, flieg' ich den Himmel durch,", "tokens": ["Wie", "ein", "m\u00e4ch\u00b7ti\u00b7ger", "Gott", ",", "flieg'", "ich", "den", "Him\u00b7mel", "durch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Reisse Sterne, wie Blumen, ab,", "tokens": ["Reis\u00b7se", "Ster\u00b7ne", ",", "wie", "Blu\u00b7men", ",", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "NN", "$,", "PWAV", "NN", "$,", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Und bekr\u00e4nze mein Haupt, trinke die Quelle leer,", "tokens": ["Und", "be\u00b7kr\u00e4n\u00b7ze", "mein", "Haupt", ",", "trin\u00b7ke", "die", "Quel\u00b7le", "leer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die durch Rosen der Engel fleusst!", "tokens": ["Die", "durch", "Ro\u00b7sen", "der", "En\u00b7gel", "fleusst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ART", "NN", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}