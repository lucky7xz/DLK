{"dta.poem.116": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er ist Eyfers\u00fcchtig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Verdammte Eyfersucht/ du Hencker meiner Seelen", "tokens": ["Ver\u00b7damm\u00b7te", "Ey\u00b7fer\u00b7sucht", "/", "du", "Hen\u00b7cker", "mei\u00b7ner", "See\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "PPER", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was plagst du mich so sehr? halt ein mit deinen Qu\u00e4len/", "tokens": ["Was", "plagst", "du", "mich", "so", "sehr", "?", "halt", "ein", "mit", "dei\u00b7nen", "Qu\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADV", "$.", "VVFIN", "ART", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Bild des herben Tods/ du Gallen-schwanger Frucht/", "tokens": ["Du", "Bild", "des", "her\u00b7ben", "Tods", "/", "du", "Gal\u00b7len\u00b7schwan\u00b7ger", "Frucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was plagst du mich so sehr verdammte Eyfersucht?", "tokens": ["Was", "plagst", "du", "mich", "so", "sehr", "ver\u00b7damm\u00b7te", "Ey\u00b7fer\u00b7sucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Die Frucht der s\u00fc\u00dften Bl\u00fcth\u2019 ist H\u00f6llisches Betr\u00fcben/", "tokens": ["Die", "Frucht", "der", "s\u00fc\u00df\u00b7ten", "Bl\u00fcth'", "ist", "H\u00f6l\u00b7li\u00b7sches", "Be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie! da\u00df dein Schlangen-Maul mir Geist und Hertze trifft?", "tokens": ["Wie", "!", "da\u00df", "dein", "Schlan\u00b7gen\u00b7Maul", "mir", "Geist", "und", "Hert\u00b7ze", "trifft", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "KOUS", "PPOSAT", "NN", "PPER", "NN", "KON", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Verdammte Eyfersucht aus ", "tokens": ["Ver\u00b7damm\u00b7te", "Ey\u00b7fer\u00b7sucht", "aus"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "APPR"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Du Kind der d\u00fcstern Nacht/ du Vorschmack von der H\u00f6llen/", "tokens": ["Du", "Kind", "der", "d\u00fcs\u00b7tern", "Nacht", "/", "du", "Vor\u00b7schmack", "von", "der", "H\u00f6l\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$(", "PPER", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.10": {"text": "Dein ", "tokens": ["Dein"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Du Mord Geist dessen Quaal mich rasend hat gemacht/", "tokens": ["Du", "Mord", "Geist", "des\u00b7sen", "Qua\u00b7al", "mich", "ra\u00b7send", "hat", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "PRELAT", "NN", "PPER", "VVPP", "VAFIN", "VVPP", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Aus ", "tokens": ["Aus"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Du Hencker s\u00fcsser Huld/ du Blend-Werck meiner Augen/", "tokens": ["Du", "Hen\u00b7cker", "s\u00fcs\u00b7ser", "Huld", "/", "du", "Blen\u00b7d\u00b7Werck", "mei\u00b7ner", "Au\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$(", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Das mir aus Rosen l\u00e4\u00dft ", "tokens": ["Das", "mir", "aus", "Ro\u00b7sen", "l\u00e4\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Und meinem Hertzen raubt so Hoffnung als Gedult/", "tokens": ["Und", "mei\u00b7nem", "Hert\u00b7zen", "raubt", "so", "Hoff\u00b7nung", "als", "Ge\u00b7dult", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "NN", "KOUS", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du Kind der d\u00fcstern Nacht/ du Hencker s\u00fcsser Huld.", "tokens": ["Du", "Kind", "der", "d\u00fcs\u00b7tern", "Nacht", "/", "du", "Hen\u00b7cker", "s\u00fcs\u00b7ser", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$(", "PPER", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Vermaledeyte Wuht/ du Folter-Banck der Sinnen", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7te", "Wuht", "/", "du", "Fol\u00b7ter\u00b7Banck", "der", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "PPER", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Soll ich einst \u00fcber dich die Ober-Hand gewinnen/", "tokens": ["Soll", "ich", "einst", "\u00fc\u00b7ber", "dich", "die", "O\u00b7ber\u00b7Hand", "ge\u00b7win\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PRF", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und werd\u2019 ich ruhig seyn wenn fliest des Feindes Blut?", "tokens": ["Und", "werd'", "ich", "ru\u00b7hig", "seyn", "wenn", "fliest", "des", "Fein\u00b7des", "Blut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "VAINF", "KOUS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Du Hencker s\u00fcsser Huld/ vermaledeyte Wuth!", "tokens": ["Du", "Hen\u00b7cker", "s\u00fcs\u00b7ser", "Huld", "/", "ver\u00b7ma\u00b7le\u00b7dey\u00b7te", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$(", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Die meiste Quaal ist hin wenn sich mein Muth gerochen/", "tokens": ["Die", "meis\u00b7te", "Qua\u00b7al", "ist", "hin", "wenn", "sich", "mein", "Muth", "ge\u00b7ro\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "KOUS", "PRF", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Denn folgt das Oster-Fest auf meine Marter-Wochen/", "tokens": ["Denn", "folgt", "das", "Os\u00b7ter\u00b7Fest", "auf", "mei\u00b7ne", "Mar\u00b7ter\u00b7Wo\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und ich bin schon vergn\u00fcgt wenn ich gerochen bin/", "tokens": ["Und", "ich", "bin", "schon", "ver\u00b7gn\u00fcgt", "wenn", "ich", "ge\u00b7ro\u00b7chen", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "VVPP", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vermaledeyte Wuth! so ist die Quaal meist hin.", "tokens": ["Ver\u00b7ma\u00b7le\u00b7dey\u00b7te", "Wuth", "!", "so", "ist", "die", "Qua\u00b7al", "meist", "hin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}}}}