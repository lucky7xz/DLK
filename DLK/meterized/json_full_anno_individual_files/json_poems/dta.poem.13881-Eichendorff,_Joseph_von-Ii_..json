{"dta.poem.13881": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ii .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Viel Reden ist manierlich:", "tokens": ["Viel", "Re\u00b7den", "ist", "ma\u00b7nier\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ewohlauf?\u201c \u2014 Ein wenig flau. \u2014", "tokens": ["\u201e", "wohl\u00b7auf", "?", "\u201c", "Ein", "we\u00b7nig", "flau", "."], "token_info": ["punct", "word", "punct", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "$.", "$(", "$(", "ART", "PIS", "ADJD", "$.", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "\u201edas Wetter ist spazierlich.\u201c \u2014", "tokens": ["\u201e", "das", "Wet\u00b7ter", "ist", "spa\u00b7zier\u00b7lich", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "ADJD", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was macht die liebe Frau? \u2014", "tokens": ["Was", "macht", "die", "lie\u00b7be", "Frau", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "\u201eich danke\u201c \u2014 und so weiter,", "tokens": ["\u201e", "ich", "dan\u00b7ke", "\u201c", "und", "so", "wei\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$(", "$(", "KON", "ADV", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Und breiter als ein See \u2014", "tokens": ["Und", "brei\u00b7ter", "als", "ein", "See"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das Singen ist gescheidter,", "tokens": ["Das", "Sin\u00b7gen", "ist", "ge\u00b7scheid\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das geht gleich in die H\u00f6h'.", "tokens": ["Das", "geht", "gleich", "in", "die", "H\u00f6h'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}