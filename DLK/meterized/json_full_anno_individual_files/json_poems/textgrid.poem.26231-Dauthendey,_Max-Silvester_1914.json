{"textgrid.poem.26231": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Silvester 1914", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Neunzehnhundertvierzehn, hast ausgek\u00e4mpft,", "tokens": ["Neun\u00b7zehn\u00b7hun\u00b7dert\u00b7vier\u00b7zehn", ",", "hast", "aus\u00b7ge\u00b7k\u00e4mpft", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie nennen dich laut, mancher ged\u00e4mpft.", "tokens": ["Sie", "nen\u00b7nen", "dich", "laut", ",", "man\u00b7cher", "ge\u00b7d\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PIS", "VVPP", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Manchem dr\u00fcckst du die Kehle eng.", "tokens": ["Man\u00b7chem", "dr\u00fcckst", "du", "die", "Keh\u00b7le", "eng", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Blutiges Jahr, wie warst du so streng!", "tokens": ["Blu\u00b7ti\u00b7ges", "Jahr", ",", "wie", "warst", "du", "so", "streng", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Kinder, die einst zur Schule gehn,", "tokens": ["Kin\u00b7der", ",", "die", "einst", "zur", "Schu\u00b7le", "gehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "APPRART", "