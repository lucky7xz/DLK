{"dta.poem.446": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der n\u00e4chtliche Thier-Krey\u00df der Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn meine Lesbia mein Thier-Krey\u00df denckt zu seyn/", "tokens": ["Wenn", "mei\u00b7ne", "Les\u00b7bia", "mein", "Thier\u00b7Krey\u00df", "denckt", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "PTKZU", "VAINF", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So setz\u2019 ich mich zur Sonn mit dem Bedinge ein;", "tokens": ["So", "setz'", "ich", "mich", "zur", "Sonn", "mit", "dem", "Be\u00b7din\u00b7ge", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Sonne braucht ein Jahr den Thier-Krey\u00df durchzugehen/", "tokens": ["Die", "Son\u00b7ne", "braucht", "ein", "Jahr", "den", "Thier\u00b7Krey\u00df", "durch\u00b7zu\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich will sie aber Nacht f\u00fcr Nacht zum Thier-Krey\u00df sehen.", "tokens": ["Ich", "will", "sie", "a\u00b7ber", "Nacht", "f\u00fcr", "Nacht", "zum", "Thier\u00b7Krey\u00df", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "NN", "APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}