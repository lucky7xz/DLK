{"dta.poem.2709": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Himmlische Verm\u00e4hlung  \n  Jfr. M. M. W. v. W. den 27. Martii  \n 1678.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So zeuchstu/ Seeligste/ numehr von dieser Erden?", "tokens": ["So", "zeuchs\u00b7tu", "/", "See\u00b7ligs\u00b7te", "/", "nu\u00b7mehr", "von", "die\u00b7ser", "Er\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "NN", "$(", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verl\u00e4st des Vaters Haus/ dein Volck und diese Stadt?", "tokens": ["Ver\u00b7l\u00e4st", "des", "Va\u00b7ters", "Haus", "/", "dein", "Volck", "und", "die\u00b7se", "Stadt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$(", "PPOSAT", "NN", "KON", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach ja! der H\u00f6chste selbst wil jetzt dein Br\u00e4utgam werden.", "tokens": ["Ach", "ja", "!", "der", "H\u00f6chs\u00b7te", "selbst", "wil", "jetzt", "dein", "Br\u00e4ut\u00b7gam", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ART", "NN", "ADV", "VMFIN", "ADV", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was Wunder? da\u00df kein Mensch theil deiner Liebe hat?", "tokens": ["Was", "Wun\u00b7der", "?", "da\u00df", "kein", "Mensch", "theil", "dei\u00b7ner", "Lie\u00b7be", "hat", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$.", "KOUS", "PIAT", "NN", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du weist da\u00df doch die Welt mu\u00df wie ein Kleid veralten/", "tokens": ["Du", "weist", "da\u00df", "doch", "die", "Welt", "mu\u00df", "wie", "ein", "Kleid", "ver\u00b7al\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "ADV", "ART", "NN", "VMFIN", "KOKOM", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df nichts best\u00e4ndig hier als Unbest\u00e4ndigkeit/", "tokens": ["Da\u00df", "nichts", "be\u00b7st\u00e4n\u00b7dig", "hier", "als", "Un\u00b7be\u00b7st\u00e4n\u00b7dig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "KOUS", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Darumb so l\u00e4stu sie den schwartzen Sontag halten/", "tokens": ["Da\u00b7rumb", "so", "l\u00e4s\u00b7tu", "sie", "den", "schwart\u00b7zen", "Son\u00b7tag", "hal\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil Jubilate du feyrst in der Ewigkeit.", "tokens": ["Weil", "Ju\u00b7bi\u00b7la\u00b7te", "du", "feyrst", "in", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Es geht zwar bitter ein so zeitlich dich zu missen/", "tokens": ["Es", "geht", "zwar", "bit\u00b7ter", "ein", "so", "zeit\u00b7lich", "dich", "zu", "mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "ART", "ADV", "ADJD", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die Welt kennt dich kaum recht/ so sagstu schon Ade!", "tokens": ["Die", "Welt", "kennt", "dich", "kaum", "recht", "/", "so", "sags\u00b7tu", "schon", "A\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$(", "ADV", "VVFIN", "ADV", "NN", "$."], "meter": "--+-++-+--+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Doch siht man da\u00df dein Geist den rechten Weg kan wissen/", "tokens": ["Doch", "siht", "man", "da\u00df", "dein", "Geist", "den", "rech\u00b7ten", "Weg", "kan", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du suchst den Himmels; May vor diesen Mertzen-Schnee.", "tokens": ["Du", "suchst", "den", "Him\u00b7mels", ";", "May", "vor", "die\u00b7sen", "Mert\u00b7zen\u00b7Schnee", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Es m\u00f6gen andre sich den Kindern gleich vergaffen", "tokens": ["Es", "m\u00f6\u00b7gen", "and\u00b7re", "sich", "den", "Kin\u00b7dern", "gleich", "ver\u00b7gaf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PIS", "PRF", "ART", "NN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "An dem/ was sonst der Mensch zum h\u00f6chsten Gute zehlt.", "tokens": ["An", "dem", "/", "was", "sonst", "der", "Mensch", "zum", "h\u00f6chs\u00b7ten", "Gu\u00b7te", "zehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$(", "PWS", "ADV", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die Welt gab ", "tokens": ["Die", "Welt", "gab"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Drumb hast Maria du das beste Theil erwehlt.", "tokens": ["Drumb", "hast", "Ma\u00b7ria", "du", "das", "bes\u00b7te", "Theil", "er\u00b7wehlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "NE", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.17": {"text": "Ja wol! das beste Theil du hast den Schatz gefunden/", "tokens": ["Ja", "wol", "!", "das", "bes\u00b7te", "Theil", "du", "hast", "den", "Schatz", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "$.", "ART", "ADJA", "NN", "PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den uns kein Potosi kein Peru geben kan.", "tokens": ["Den", "uns", "kein", "Po\u00b7to\u00b7si", "kein", "Pe\u00b7ru", "ge\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIAT", "NN", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Die zehlst nicht mehr wie vor Schlafflose N\u00e4cht und Stunden:", "tokens": ["Die", "zehlst", "nicht", "mehr", "wie", "vor", "Schlaf\u00b7flo\u00b7se", "N\u00e4cht", "und", "Stun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ADV", "KOKOM", "APPR", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Hier geht dein Ruhe-Tag und Feyerabend an.", "tokens": ["Hier", "geht", "dein", "Ru\u00b7he\u00b7Tag", "und", "Fe\u00b7ye\u00b7ra\u00b7bend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Jtzt wechselstu mit GOtt/ Verlobte/ Hand und Ringe", "tokens": ["Jtzt", "wech\u00b7sel\u00b7stu", "mit", "Gott", "/", "Ver\u00b7lob\u00b7te", "/", "Hand", "und", "Rin\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du bringst des Glaubens-Schmuck mit dir zum Heyrath-", "tokens": ["Du", "bringst", "des", "Glau\u00b7bens\u00b7Schmuck", "mit", "dir", "zum", "Hey\u00b7ra\u00b7th"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Er gibt den Himmel dir zu einem Leibgedinge", "tokens": ["Er", "gibt", "den", "Him\u00b7mel", "dir", "zu", "ei\u00b7nem", "Leib\u00b7ge\u00b7din\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "(gut:", "tokens": ["(", "gut", ":"], "token_info": ["punct", "word", "punct"], "pos": ["$(", "ADJD", "$."], "meter": "-", "measure": "single.down"}, "line.25": {"text": "Eur Ehberedung ist besiegelt durch sein Blut.", "tokens": ["Eur", "Eh\u00b7be\u00b7re\u00b7dung", "ist", "be\u00b7sie\u00b7gelt", "durch", "sein", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Prangt Mogols Hochzeit-Fest mit lichten Diamanten", "tokens": ["Prangt", "Mo\u00b7gols", "Hoch\u00b7zeit\u00b7Fest", "mit", "lich\u00b7ten", "Di\u00b7a\u00b7man\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "NE", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Mit Lampen welche gar von Balsam sind gemacht?", "tokens": ["Mit", "Lam\u00b7pen", "wel\u00b7che", "gar", "von", "Bal\u00b7sam", "sind", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRELS", "ADV", "APPR", "NE", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Das Stern- und Engel-Heer sind Lichter und Trabanten", "tokens": ["Das", "Stern", "und", "En\u00b7gel\u00b7Heer", "sind", "Lich\u00b7ter", "und", "Tra\u00b7ban\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "TRUNC", "KON", "NN", "VAFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So dir zur Seite stehn in unersch\u00f6pffter Pracht.", "tokens": ["So", "dir", "zur", "Sei\u00b7te", "stehn", "in", "un\u00b7er\u00b7sch\u00f6pff\u00b7ter", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Es ist das Paradi\u00df dein himmlisch Hochzeit-Bette/", "tokens": ["Es", "ist", "das", "Pa\u00b7ra\u00b7di\u00df", "dein", "himm\u00b7lisch", "Hoch\u00b7zeit\u00b7Bet\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hier liegstu hochvergn\u00fcgt dem Br\u00e4utgam an der Seit\u2019", "tokens": ["Hier", "liegs\u00b7tu", "hoch\u00b7ver\u00b7gn\u00fcgt", "dem", "Br\u00e4ut\u00b7gam", "an", "der", "Seit'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "ART", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Er gibt sich selber dir statt der Verm\u00e4hlungs-Kette", "tokens": ["Er", "gibt", "sich", "sel\u00b7ber", "dir", "statt", "der", "Ver\u00b7m\u00e4h\u00b7lungs\u00b7Ket\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "Dein Schmuck ist sein Verdienst/ dein Krantz die Ewigkeit.", "tokens": ["Dein", "Schmuck", "ist", "sein", "Ver\u00b7dienst", "/", "dein", "Krantz", "die", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verg\u00f6nn\u2019 uns/ Sch\u00f6ne Braut/ die wir im Geiste schauen/", "tokens": ["Ver\u00b7g\u00f6nn'", "uns", "/", "Sch\u00f6\u00b7ne", "Braut", "/", "die", "wir", "im", "Geis\u00b7te", "schau\u00b7en", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "ADJA", "NN", "$(", "PRELS", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Wie du in Eden schon gleich Palm und Cedern gr\u00fcnst/", "tokens": ["Wie", "du", "in", "E\u00b7den", "schon", "gleich", "Palm", "und", "Ce\u00b7dern", "gr\u00fcnst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "ADV", "ADV", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Da\u00df deiner Tugend wir ein Ehren-Mahl noch bauen/", "tokens": ["Da\u00df", "dei\u00b7ner", "Tu\u00b7gend", "wir", "ein", "Eh\u00b7ren\u00b7Mahl", "noch", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Und melden durch di\u00df Blat was du mit Ruhm verdienst.", "tokens": ["Und", "mel\u00b7den", "durch", "di\u00df", "Blat", "was", "du", "mit", "Ruhm", "ver\u00b7dienst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PDS", "NN", "PWS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Dein sch\u00f6n Ged\u00e4chtn\u00fc\u00df brennt noch hell in unsren Seelen/", "tokens": ["Dein", "sch\u00f6n", "Ge\u00b7d\u00e4cht\u00b7n\u00fc\u00df", "brennt", "noch", "hell", "in", "un\u00b7sren", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es lebt im Seegen hier/ wie bey dem Heiland du/", "tokens": ["Es", "lebt", "im", "See\u00b7gen", "hier", "/", "wie", "bey", "dem", "Hei\u00b7land", "du", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "$(", "KOKOM", "APPR", "ART", "NN", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Tr\u00e4gt man was Erde war gleich in der Erden H\u00f6len/", "tokens": ["Tr\u00e4gt", "man", "was", "Er\u00b7de", "war", "gleich", "in", "der", "Er\u00b7den", "H\u00f6\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PWS", "NN", "VAFIN", "ADV", "APPR", "ART", "NN", "NN", "$("], "meter": "++-+-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.41": {"text": "Druckt deinem C\u00f6rper man gleich Mund und Augen zu.", "tokens": ["Druckt", "dei\u00b7nem", "C\u00f6r\u00b7per", "man", "gleich", "Mund", "und", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PIS", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der Tugend Ph\u00f6nix steigt nichts minder aus der Aschen/", "tokens": ["Der", "Tu\u00b7gend", "Ph\u00f6\u00b7nix", "steigt", "nichts", "min\u00b7der", "aus", "der", "