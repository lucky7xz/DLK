{"textgrid.poem.48279": {"metadata": {"author": {"name": "Fontane, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Lied des James Monmouth", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zieht sich eine blutige Spur", "tokens": ["Es", "zieht", "sich", "ei\u00b7ne", "blu\u00b7ti\u00b7ge", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Durch unser Haus von alters,", "tokens": ["Durch", "un\u00b7ser", "Haus", "von", "al\u00b7ters", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Meine Mutter war seine Buhle nur,", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "war", "sei\u00b7ne", "Buh\u00b7le", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Die sch\u00f6ne Lucy Walters.", "tokens": ["Die", "sch\u00f6\u00b7ne", "Lu\u00b7cy", "Wal\u00b7ters", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Am Abend war's, leis wogte das Korn,", "tokens": ["Am", "A\u00b7bend", "wa\u00b7r's", ",", "leis", "wog\u00b7te", "das", "Korn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Sie k\u00fc\u00dften sich unter der Linde,", "tokens": ["Sie", "k\u00fc\u00df\u00b7ten", "sich", "un\u00b7ter", "der", "Lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NE", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Eine Lerche klang und ein J\u00e4gerhorn \u2013", "tokens": ["Ei\u00b7ne", "Ler\u00b7che", "klang", "und", "ein", "J\u00e4\u00b7ger\u00b7horn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ART", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ich bin ein Kind der S\u00fcnde.", "tokens": ["Ich", "bin", "ein", "Kind", "der", "S\u00fcn\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Meine Mutter hat mir oft erz\u00e4hlt", "tokens": ["Mei\u00b7ne", "Mut\u00b7ter", "hat", "mir", "oft", "er\u00b7z\u00e4hlt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Von jenes Abends Sonne,", "tokens": ["Von", "je\u00b7nes", "A\u00b7bends", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihre Lippen sprachen: Ich habe gefehlt!", "tokens": ["Ih\u00b7re", "Lip\u00b7pen", "spra\u00b7chen", ":", "Ich", "ha\u00b7be", "ge\u00b7fehlt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ihre Augen lachten vor Wonne.", "tokens": ["Ih\u00b7re", "Au\u00b7gen", "lach\u