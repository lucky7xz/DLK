{"textgrid.poem.34174": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Freiheit der Presse", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4re der Presse Freiheit ein Gut nur der einzelnen V\u00f6lker,", "tokens": ["W\u00e4\u00b7re", "der", "Pres\u00b7se", "Frei\u00b7heit", "ein", "Gut", "nur", "der", "ein\u00b7zel\u00b7nen", "V\u00f6l\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "So verschmerzt' ich's wohl noch, w\u00fcrde sie einem verk\u00fcrzt.", "tokens": ["So", "ver\u00b7schmerzt'", "ich's", "wohl", "noch", ",", "w\u00fcr\u00b7de", "sie", "ei\u00b7nem", "ver\u00b7k\u00fcrzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "ADV", "$,", "VAFIN", "PPER", "ART", "VVPP", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aber sie eignet der Welt, Gedanken, noch sch\u00e4dlich im Norden,", "tokens": ["A\u00b7ber", "sie", "eig\u00b7net", "der", "Welt", ",", "Ge\u00b7dan\u00b7ken", ",", "noch", "sch\u00e4d\u00b7lich", "im", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,", "NN", "$,", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "N\u00fctzen dem S\u00fcden vielleicht, w\u00e4hrend sie jener erzeugt!", "tokens": ["N\u00fct\u00b7zen", "dem", "S\u00fc\u00b7den", "viel\u00b7leicht", ",", "w\u00e4h\u00