{"textgrid.poem.55998": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Esther", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Dienerinnen k\u00e4mmten sieben Tage", "tokens": ["Die", "Die\u00b7ne\u00b7rin\u00b7nen", "k\u00e4mm\u00b7ten", "sie\u00b7ben", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Asche ihres Grams und ihrer Plage", "tokens": ["die", "A\u00b7sche", "ih\u00b7res", "Grams", "und", "ih\u00b7rer", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Neige und Niederschlag aus ihrem Haar,", "tokens": ["Nei\u00b7ge", "und", "Nie\u00b7der\u00b7schlag", "aus", "ih\u00b7rem", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "und trugen es und sonnten es im Freien", "tokens": ["und", "tru\u00b7gen", "es", "und", "sonn\u00b7ten", "es", "im", "Frei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und speisten es mit reinen Spezereien", "tokens": ["und", "speis\u00b7ten", "es", "mit", "rei\u00b7nen", "Spe\u00b7ze\u00b7rei\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "noch diesen Tag und den: dann aber war", "tokens": ["noch", "die\u00b7sen", "Tag", "und", "den", ":", "dann", "a\u00b7ber", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "KON", "ART", "$.", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "die Zeit gekommen, da sie, ungeboten,", "tokens": ["die", "Zeit", "ge\u00b7kom\u00b7men", ",", "da", "sie", ",", "un\u00b7ge\u00b7bo\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "KOUS", "PPER", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "zu keiner Frist, wie eine von den Toten", "tokens": ["zu", "kei\u00b7ner", "Frist", ",", "wie", "ei\u00b7ne", "von", "den", "To\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "ART", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "den drohend offenen Palast betrat,", "tokens": ["den", "dro\u00b7hend", "of\u00b7fe\u00b7nen", "Pa\u00b7last", "be\u00b7trat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.4": {"text": "um gleich, gelegt auf ihre Kammerfrauen,", "tokens": ["um", "gleich", ",", "ge\u00b7legt", "auf", "ih\u00b7re", "Kam\u00b7mer\u00b7frau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "$,", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "am Ende ihres Weges ", "tokens": ["am", "En\u00b7de", "ih\u00b7res", "We\u00b7ges"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "an dem man stirbt, wenn man ihm naht.", "tokens": ["an", "dem", "man", "stirbt", ",", "wenn", "man", "ihm", "naht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIS", "VVFIN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er gl\u00e4nzte so, da\u00df sie die Kronrubine", "tokens": ["Er", "gl\u00e4nz\u00b7te", "so", ",", "da\u00df", "sie", "die", "Kron\u00b7ru\u00b7bi\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "aufflammen f\u00fchlte, die sie an sich trug;", "tokens": ["auf\u00b7flam\u00b7men", "f\u00fchl\u00b7te", ",", "die", "sie", "an", "sich", "trug", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "$,", "PRELS", "PPER", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "sie f\u00fcllte sich ganz rasch mit seiner Miene", "tokens": ["sie", "f\u00fcll\u00b7te", "sich", "ganz", "rasch", "mit", "sei\u00b7ner", "Mie\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "wie ein Gef\u00e4\u00df und war schon voll genug", "tokens": ["wie", "ein", "Ge\u00b7f\u00e4\u00df", "und", "war", "schon", "voll", "ge\u00b7nug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "VAFIN", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "und flo\u00df schon \u00fcber von des K\u00f6nigs Macht,", "tokens": ["und", "flo\u00df", "schon", "\u00fc\u00b7ber", "von", "des", "K\u00f6\u00b7nigs", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "bevor sie noch den dritten Saal durchschritt,", "tokens": ["be\u00b7vor", "sie", "noch", "den", "drit\u00b7ten", "Saal", "durch\u00b7schritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "der sie mit seiner W\u00e4nde Malachit", "tokens": ["der", "sie", "mit", "sei\u00b7ner", "W\u00e4n\u00b7de", "Ma\u00b7la\u00b7chit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "gr\u00fcn \u00fcberlief. Sie hatte nicht gedacht,", "tokens": ["gr\u00fcn", "\u00fc\u00b7berl\u00b7ief", ".", "Sie", "hat\u00b7te", "nicht", "ge\u00b7dacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$.", "PPER", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "so langen Gang zu tun mit allen Steinen,", "tokens": ["so", "lan\u00b7gen", "Gang", "zu", "tun", "mit", "al\u00b7len", "Stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die schwerer wurden von des K\u00f6nigs Scheinen", "tokens": ["die", "schwe\u00b7rer", "wur\u00b7den", "von", "des", "K\u