{"dta.poem.18803": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Deinem Blick mich zu bequemen,", "tokens": ["Dei\u00b7nem", "Blick", "mich", "zu", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinem Munde, deiner Brust,", "tokens": ["Dei\u00b7nem", "Mun\u00b7de", ",", "dei\u00b7ner", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Stimme zu vernehmen", "tokens": ["Dei\u00b7ne", "Stim\u00b7me", "zu", "ver\u00b7neh\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "War die letzt\u2019 und erste Lust.", "tokens": ["War", "die", "letzt'", "und", "ers\u00b7te", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gestern, Ach! war sie die letzte,", "tokens": ["Ge\u00b7stern", ",", "Ach", "!", "war", "sie", "die", "letz\u00b7te", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$.", "VAFIN", "PPER", "ART", "ADJA", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dann verlosch mir Leucht\u2019 und Feuer,", "tokens": ["Dann", "ver\u00b7losch", "mir", "Leucht'", "und", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jeder Scherz der mich ergetzte", "tokens": ["Je\u00b7der", "Scherz", "der", "mich", "er\u00b7getz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ART", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird nun schuldenschwer und theuer.", "tokens": ["Wird", "nun", "schul\u00b7den\u00b7schwer", "und", "theu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Eh es Allah nicht gef\u00e4llt", "tokens": ["Eh", "es", "Al\u00b7lah", "nicht", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "PTKNEG", "VVPP"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Uns aufs neue zu vereinen,", "tokens": ["Uns", "aufs", "neu\u00b7e", "zu", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Giebt mir Sonne, Mond und Welt", "tokens": ["Giebt", "mir", "Son\u00b7ne", ",", "Mond", "und", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nur Gelegenheit zum Weinen.", "tokens": ["Nur", "Ge\u00b7le\u00b7gen\u00b7heit", "zum", "Wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}