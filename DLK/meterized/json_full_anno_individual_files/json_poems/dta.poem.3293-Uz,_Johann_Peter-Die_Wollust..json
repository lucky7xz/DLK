{"dta.poem.3293": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die Wollust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Hier im Gestr\u00e4uch, an Florens weichem Busen,", "tokens": ["Hier", "im", "Ge\u00b7str\u00e4uch", ",", "an", "Flo\u00b7rens", "wei\u00b7chem", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Balsam haucht, geruhig hingestreckt,", "tokens": ["Die", "Bal\u00b7sam", "haucht", ",", "ge\u00b7ru\u00b7hig", "hin\u00b7ge\u00b7streckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erwart ich sie, die g\u00f6ttlichste der Musen,", "tokens": ["Er\u00b7wart", "ich", "sie", ",", "die", "g\u00f6tt\u00b7lichs\u00b7te", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die sich im Busch vor meinem Wunsch versteckt.", "tokens": ["Die", "sich", "im", "Busch", "vor", "mei\u00b7nem", "Wunsch", "ver\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie kommt, sie kommt! ich h\u00f6re schon vom weiten,", "tokens": ["Sie", "kommt", ",", "sie", "kommt", "!", "ich", "h\u00f6\u00b7re", "schon", "vom", "wei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In stiller Luft, die Stimme g\u00fcldner Saiten.", "tokens": ["In", "stil\u00b7ler", "Luft", ",", "die", "Stim\u00b7me", "g\u00fcld\u00b7ner", "Sai\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Jhr Sterblichen, die ihr dem Schicksal fluchet,", "tokens": ["Ihr", "Sterb\u00b7li\u00b7chen", ",", "die", "ihr", "dem", "Schick\u00b7sal", "flu\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn euern Arm gew\u00fcnschte Ruhe flieht;", "tokens": ["Wenn", "eu\u00b7ern", "Arm", "ge\u00b7w\u00fcnschte", "Ru\u00b7he", "flieht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die ihr umsonst sie unter Dornen suchet!", "tokens": ["Die", "ihr", "um\u00b7sonst", "sie", "un\u00b7ter", "Dor\u00b7nen", "su\u00b7chet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O h\u00f6ret mich! o h\u00f6rt mein lehrend Lied!", "tokens": ["O", "h\u00f6\u00b7ret", "mich", "!", "o", "h\u00f6rt", "mein", "leh\u00b7rend", "Lied", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$.", "FM", "VVFIN", "PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Was qu\u00e4lt ihr euch? die holde Wollust winket,", "tokens": ["Was", "qu\u00e4lt", "ihr", "euch", "?", "die", "hol\u00b7de", "Wol\u00b7lust", "win\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "$.", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und beut euch an, was euch so sch\u00e4tzbar d\u00fcnket:", "tokens": ["Und", "beut", "euch", "an", ",", "was", "euch", "so", "sch\u00e4tz\u00b7bar", "d\u00fcn\u00b7ket", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PRELS", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Wollust nicht, die auch der P\u00f6bel kennet;", "tokens": ["Die", "Wol\u00b7lust", "nicht", ",", "die", "auch", "der", "P\u00f6\u00b7bel", "ken\u00b7net", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die viehisch ras\u2019t, nicht sich vern\u00fcnftig freut;", "tokens": ["Die", "vie\u00b7hisch", "ras't", ",", "nicht", "sich", "ver\u00b7n\u00fcnf\u00b7tig", "freut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "PTKNEG", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von Lieb und Wein, umkr\u00e4nzt mit Epheu, brennet,", "tokens": ["Von", "Lieb", "und", "Wein", ",", "um\u00b7kr\u00e4nzt", "mit", "E\u00b7pheu", ",", "bren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Lieb und Wein durch Uebermaa\u00df entweiht!", "tokens": ["Und", "Lieb", "und", "Wein", "durch", "Ue\u00b7berm\u00b7aa\u00df", "ent\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nein! die zugleich Natur und Weisheit preisen;", "tokens": ["Nein", "!", "die", "zu\u00b7gleich", "Na\u00b7tur", "und", "Weis\u00b7heit", "prei\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Weisheit Kind, die K\u00f6niginn der Weisen!", "tokens": ["Der", "Weis\u00b7heit", "Kind", ",", "die", "K\u00f6\u00b7ni\u00b7ginn", "der", "Wei\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich sehe sie, und Morgen-Rosen schm\u00fccken", "tokens": ["Ich", "se\u00b7he", "sie", ",", "und", "Mor\u00b7gen\u00b7Ro\u00b7sen", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die heitre Stirn und gl\u00e4nzen um ihr Haupt.", "tokens": ["Die", "heit\u00b7re", "Stirn", "und", "gl\u00e4n\u00b7zen", "um", "ihr", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie ruhig strahlt aus ihren s\u00fcssen Blicken", "tokens": ["Wie", "ru\u00b7hig", "strahlt", "aus", "ih\u00b7ren", "s\u00fcs\u00b7sen", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die reine Lust, die kein Verh\u00e4ngni\u00df raubt!", "tokens": ["Die", "rei\u00b7ne", "Lust", ",", "die", "kein", "Ver\u00b7h\u00e4ng\u00b7ni\u00df", "raubt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Durch sie wird selbst Ly\u00e4us zahm gemachet,", "tokens": ["Durch", "sie", "wird", "selbst", "Ly\u00b7\u00e4us", "zahm", "ge\u00b7ma\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "NE", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Der hinter ihr mit einer Muse lachet.", "tokens": ["Der", "hin\u00b7ter", "ihr", "mit", "ei\u00b7ner", "Mu\u00b7se", "la\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Freude schwingt um sie die g\u00fcldnen Fl\u00fcgel", "tokens": ["Die", "Freu\u00b7de", "schwingt", "um", "sie", "die", "g\u00fcld\u00b7nen", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu aller Zeit, auch wenn das Gl\u00fcck entflieht.", "tokens": ["Zu", "al\u00b7ler", "Zeit", ",", "auch", "wenn", "das", "Gl\u00fcck", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So \u00f6de scheint kein d\u00fcrrverbrannter H\u00fcgel,", "tokens": ["So", "\u00f6\u00b7de", "scheint", "kein", "d\u00fcrr\u00b7ver\u00b7brann\u00b7ter", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wo nicht f\u00fcr sie noch manche Bluhme bl\u00fcht:", "tokens": ["Wo", "nicht", "f\u00fcr", "sie", "noch", "man\u00b7che", "Bluh\u00b7me", "bl\u00fcht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "APPR", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und rings umher schwatzt unter Laub und Zweigen", "tokens": ["Und", "rings", "um\u00b7her", "schwatzt", "un\u00b7ter", "Laub", "und", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKVZ", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein sanfter West, und rauhe St\u00fcrme schweigen.", "tokens": ["Ein", "sanf\u00b7ter", "West", ",", "und", "rau\u00b7he", "St\u00fcr\u00b7me", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Wie sollte dir nicht alles dienen m\u00fcssen,", "tokens": ["Wie", "soll\u00b7te", "dir", "nicht", "al\u00b7les", "die\u00b7nen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PTKNEG", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du, die allein die Sterblichen begl\u00fcckt!", "tokens": ["Du", ",", "die", "al\u00b7lein", "die", "Sterb\u00b7li\u00b7chen", "be\u00b7gl\u00fcckt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Gefesselt liegt, o G\u00f6ttinn! dir zu F\u00fcssen", "tokens": ["Ge\u00b7fes\u00b7selt", "liegt", ",", "o", "G\u00f6t\u00b7tinn", "!", "dir", "zu", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "FM", "NN", "$.", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der bleiche Gram, der schwache Seelen dr\u00fcckt.", "tokens": ["Der", "blei\u00b7che", "Gram", ",", "der", "schwa\u00b7che", "See\u00b7len", "dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du b\u00e4ndigest die hungrigen Begierden,", "tokens": ["Du", "b\u00e4n\u00b7di\u00b7gest", "die", "hung\u00b7ri\u00b7gen", "Be\u00b7gier\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die ohne dich verderblich herrschen w\u00fcrden.", "tokens": ["Die", "oh\u00b7ne", "dich", "ver\u00b7derb\u00b7lich", "herr\u00b7schen", "w\u00fcr\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "ADJD", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie, wann der Sud sein schwarz Gefieder sch\u00fcttert,", "tokens": ["Wie", ",", "wann", "der", "Sud", "sein", "schwarz", "Ge\u00b7fie\u00b7der", "sch\u00fct\u00b7tert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf der See sich als Tyrann erhebt;", "tokens": ["Und", "auf", "der", "See", "sich", "als", "Ty\u00b7rann", "er\u00b7hebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PRF", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Ocean bis an den Grund erzittert,", "tokens": ["Der", "O\u00b7cean", "bis", "an", "den", "Grund", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und wei\u00dfbesch\u00e4umt hoch in die L\u00fcfte strebt:", "tokens": ["Und", "wei\u00df\u00b7be\u00b7sch\u00e4umt", "hoch", "in", "die", "L\u00fcf\u00b7te", "strebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Indem kein Stern die bange Nacht erheitert,", "tokens": ["In\u00b7dem", "kein", "Stern", "die", "ban\u00b7ge", "Nacht", "er\u00b7hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Verirret sich das kranke Schiff und scheitert:", "tokens": ["Ver\u00b7ir\u00b7ret", "sich", "das", "kran\u00b7ke", "Schiff", "und", "schei\u00b7tert", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "So w\u00fcthen auch die z\u00fcgellosen Triebe,", "tokens": ["So", "w\u00fct\u00b7hen", "auch", "die", "z\u00fc\u00b7gel\u00b7lo\u00b7sen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die uns Natur mitleidig eingesenkt.", "tokens": ["Die", "uns", "Na\u00b7tur", "mit\u00b7lei\u00b7dig", "ein\u00b7ge\u00b7senkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie brechen los; und Recht und Menschenliebe,", "tokens": ["Sie", "bre\u00b7chen", "los", ";", "und", "Recht", "und", "Men\u00b7schen\u00b7lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was heilig ist, wird un