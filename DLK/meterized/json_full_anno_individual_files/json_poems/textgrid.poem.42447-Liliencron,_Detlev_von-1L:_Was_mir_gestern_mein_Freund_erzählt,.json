laxed"}}, "stanza.4": {"line.1": {"text": "Nie wieder hab' ich von ihr geh\u00f6rt,", "tokens": ["Nie", "wie\u00b7der", "hab'", "ich", "von", "ihr", "ge\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Ob sie gestorben, gerettet, beth\u00f6rt,", "tokens": ["Ob", "sie", "ge\u00b7stor\u00b7ben", ",", "ge\u00b7ret\u00b7tet", ",", "be\u00b7th\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ob ihr das Gl\u00fcck seinen Hellmorgen gezeigt,", "tokens": ["Ob", "ihr", "das", "Gl\u00fcck", "sei\u00b7nen", "Hell\u00b7mor\u00b7gen", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ob kr\u00e4chzend der Kummer die Fidel ihr geigt.", "tokens": ["Ob", "kr\u00e4ch\u00b7zend", "der", "Kum\u00b7mer", "die", "Fi\u00b7del", "ihr", "geigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Zuweilen, die grausam ich von mir stie\u00df,", "tokens": ["Zu\u00b7wei\u00b7len", ",", "die", "grau\u00b7sam", "ich", "von", "mir", "stie\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Die undankbar ich von mir lie\u00df,", "tokens": ["Die", "un\u00b7dank\u00b7bar", "ich", "von", "mir", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Steht nachts sie vor mir, l\u00e4chelnd, fahl \u2013", "tokens": ["Steht", "nachts", "sie", "vor", "mir", ",", "l\u00e4\u00b7chelnd", ",", "fahl", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "APPR", "PPER", "$,", "ADJD", "$,", "KOUS", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Leben, \u00e4h was, macht uns alle brutal.", "tokens": ["Das", "Le\u00b7ben", ",", "\u00e4h", "was", ",", "macht", "uns", "al\u00b7le", "bru\u00b7tal", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PIS", "$,", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.5": {"line.1": {"text": "Was mir gestern mein Freund erz\u00e4hlt,", "tokens": ["Was", "mir", "ge\u00b7stern", "mein", "Freund", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat mich bis in den Traum gequ\u00e4lt.", "tokens": ["Hat", "mich", "bis", "in", "den", "Traum", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt ist so roh, ich versteh' sie nicht \u2013", "tokens": ["Die", "Welt", "ist", "so", "roh", ",", "ich", "ver\u00b7steh'", "sie", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und also lautete sein Bericht:", "tokens": ["Und", "al\u00b7so", "lau\u00b7te\u00b7te", "sein", "Be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "In der gro\u00dfen s\u00fcddeutschen Stadt,", "tokens": ["In", "der", "gro\u00b7\u00dfen", "s\u00fcd\u00b7deut\u00b7schen", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.2": {"text": "Die ein drollig Kindl im Wappen hat,", "tokens": ["Die", "ein", "drol\u00b7lig", "Kindl", "im", "Wap\u00b7pen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hab' ich die Hochschule einst besucht,", "tokens": ["Hab'", "ich", "die", "Hoch\u00b7schu\u00b7le", "einst", "be\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit wackrem Flei\u00dfe vieles gebucht,", "tokens": ["Mit", "wack\u00b7rem", "Flei\u00b7\u00dfe", "vie\u00b7les", "ge\u00b7bucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Da\u00df es mir sp\u00e4ter im Leben n\u00fctze.", "tokens":