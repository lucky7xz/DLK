{"textgrid.poem.42447": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was mir gestern mein Freund erz\u00e4hlt,", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was mir gestern mein Freund erz\u00e4hlt,", "tokens": ["Was", "mir", "ge\u00b7stern", "mein", "Freund", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hat mich bis in den Traum gequ\u00e4lt.", "tokens": ["Hat", "mich", "bis", "in", "den", "Traum", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Welt ist so roh, ich versteh' sie nicht \u2013", "tokens": ["Die", "Welt", "ist", "so", "roh", ",", "ich", "ver\u00b7steh'", "sie", "nicht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und also lautete sein Bericht:", "tokens": ["Und", "al\u00b7so", "lau\u00b7te\u00b7te", "sein", "Be\u00b7richt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "In der gro\u00dfen s\u00fcddeutschen Stadt,", "tokens": ["In", "der", "gro\u00b7\u00dfen", "s\u00fcd\u00b7deut\u00b7schen", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+-++-+", "measure": "anapaest.init"}, "line.2": {"text": "Die ein drollig Kindl im Wappen hat,", "tokens": ["Die", "ein", "drol\u00b7lig", "Kindl", "im", "Wap\u00b7pen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "NN", "APPRART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Hab' ich die Hochschule einst besucht,", "tokens": ["Hab'", "ich", "die", "Hoch\u00b7schu\u00b7le", "einst", "be\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit wackrem Flei\u00dfe vieles gebucht,", "tokens": ["Mit", "wack\u00b7rem", "Flei\u00b7\u00dfe", "vie\u00b7les", "ge\u00b7bucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PIS", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Da\u00df es mir sp\u00e4ter im Leben n\u00fctze.", "tokens": ["Da\u00df", "es", "mir", "sp\u00e4\u00b7ter", "im", "Le\u00b7ben", "n\u00fct\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "Doch nebenbei, meine bunte M\u00fctze", "tokens": ["Doch", "ne\u00b7ben\u00b7bei", ",", "mei\u00b7ne", "bun\u00b7te", "M\u00fct\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "War der B\u00fcrge, da\u00df nicht alle Zeit", "tokens": ["War", "der", "B\u00fcr\u00b7ge", ",", "da\u00df", "nicht", "al\u00b7le", "Zeit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "KOUS", "PTKNEG", "PIAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ich hinbrachte nur in Gelehrsamkeit.", "tokens": ["Ich", "hin\u00b7brach\u00b7te", "nur", "in", "Ge\u00b7lehr\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Gesang und Trunk und mancher Schmi\u00df,", "tokens": ["Ge\u00b7sang", "und", "Trunk", "und", "man\u00b7cher", "Sch\u00b7mi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.10": {"text": "Der rechts und links mir die Backen zerri\u00df,", "tokens": ["Der", "rechts", "und", "links", "mir", "die", "Ba\u00b7cken", "zer\u00b7ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "KON", "ADV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Sind Zeugen, da\u00df ich kein Duckm\u00e4user war", "tokens": ["Sind", "Zeu\u00b7gen", ",", "da\u00df", "ich", "kein", "Duck\u00b7m\u00e4u\u00b7ser", "war"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KOUS", "PPER", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "In jenem lustigen, jubelnden Jahr.", "tokens": ["In", "je\u00b7nem", "lus\u00b7ti\u00b7gen", ",", "ju\u00b7beln\u00b7den", "Jahr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Ein M\u00e4del, wie's mit sich bringt der Brauch,", "tokens": ["Ein", "M\u00e4\u00b7del", ",", "wie's", "mit", "sich", "bringt", "der", "Brauch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "APPR", "PRF", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Hab' ich damals besessen auch,", "tokens": ["Hab'", "ich", "da\u00b7mals", "be\u00b7ses\u00b7sen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ein liebes, gutes, vergn\u00fcgtes Ding,", "tokens": ["Ein", "lie\u00b7bes", ",", "gu\u00b7tes", ",", "ver\u00b7gn\u00fcg\u00b7tes", "Ding", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Die voller Dargebung an mir hing.", "tokens": ["Die", "vol\u00b7ler", "Dar\u00b7ge\u00b7bung", "an", "mir", "hing", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Doch pl\u00f6tzlich, wer wagt unser Herz zu kennen,", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", ",", "wer", "wagt", "un\u00b7ser", "Herz", "zu", "ken\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWS", "VVFIN", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Ward sie mir l\u00e4stig, ich mu\u00dfte mich trennen.", "tokens": ["Ward", "sie", "mir", "l\u00e4s\u00b7tig", ",", "ich", "mu\u00df\u00b7te", "mich", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Das konnte das arme Gesch\u00f6pf nicht begreifen,", "tokens": ["Das", "konn\u00b7te", "das", "ar\u00b7me", "Ge\u00b7sch\u00f6pf", "nicht", "be\u00b7grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.20": {"text": "Da\u00df ich so schnell sie wollt' von mir streifen.", "tokens": ["Da\u00df", "ich", "so", "schnell", "sie", "wollt'", "von", "mir", "strei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "VMFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Sie wehrte sich, das half ihr nicht viel,", "tokens": ["Sie", "wehr\u00b7te", "sich", ",", "das", "half", "ihr", "nicht", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PDS", "VVFIN", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.22": {"text": "Ich hielt punktfest nur auf mein Ziel.", "tokens": ["Ich", "hielt", "punkt\u00b7fest", "nur", "auf", "mein", "Ziel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Und endlich, ich gab ihr manch rauhes Wort,", "tokens": ["Und", "end\u00b7lich", ",", "ich", "gab", "ihr", "manch", "rau\u00b7hes", "Wort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VVFIN", "PPER", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Sagte sie traurig: Weit zieh' ich fort,", "tokens": ["Sag\u00b7te", "sie", "trau\u00b7rig", ":", "Weit", "zieh'", "ich", "fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$.", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Ich kann da nimmer des Schmerzes genesen,", "tokens": ["Ich", "kann", "da", "nim\u00b7mer", "des", "Schmer\u00b7zes", "ge\u00b7ne\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "Wo ich so fr\u00f6hlich mit dir gewesen.", "tokens": ["Wo", "ich", "so", "fr\u00f6h\u00b7lich", "mit", "dir", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "APPR", "PPER", "VAPP", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich schenkt' ihr, was ich grad' hatt' an Geld,", "tokens": ["Ich", "schenkt'", "ihr", ",", "was", "ich", "grad'", "hatt'", "an", "Geld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und habe sie dann auf den Bahnhof bestellt.", "tokens": ["Und", "ha\u00b7be", "sie", "dann", "auf", "den", "Bahn\u00b7hof", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Durch die Glasth\u00fcr konnt' ich, von ihr nicht erkannt,", "tokens": ["Durch", "die", "Glast\u00b7h\u00fcr", "konnt'", "ich", ",", "von", "ihr", "nicht", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "$,", "APPR", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Sie beobachten in ihrem Witwenstand:", "tokens": ["Sie", "be\u00b7ob\u00b7ach\u00b7ten", "in", "ih\u00b7rem", "Wit\u00b7wen\u00b7stand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sie sa\u00df mit tief gesenktem Kinn", "tokens": ["Sie", "sa\u00df", "mit", "tief", "ge\u00b7senk\u00b7tem", "Kinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und starrte teilnamlos vor sich hin.", "tokens": ["Und", "starr\u00b7te", "teil\u00b7nam\u00b7los", "vor", "sich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PRF", "PTKVZ", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Um sie her Gel\u00e4chter, Geplapper,", "tokens": ["Um", "sie", "her", "Ge\u00b7l\u00e4ch\u00b7ter", ",", "Ge\u00b7plap\u00b7per", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUI", "PPER", "ADV", "NN", "$,", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Biergl\u00e4sergeklirr und Tellergeklapper,", "tokens": ["Bier\u00b7gl\u00e4\u00b7ser\u00b7ge\u00b7klirr", "und", "Tel\u00b7ler\u00b7ge\u00b7klap\u00b7per", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Hier vom Trost beruhigte Abschiedsthr\u00e4nen,", "tokens": ["Hier", "vom", "Trost", "be\u00b7ru\u00b7hig\u00b7te", "Ab\u00b7schiedst\u00b7hr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Dort munter den Goldtag der Zukunft w\u00e4hnen.", "tokens": ["Dort", "mun\u00b7ter", "den", "Gold\u00b7tag", "der", "Zu\u00b7kunft", "w\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und unter all' den Menschengrimassen", "tokens": ["Und", "un\u00b7ter", "all'", "den", "Men\u00b7schen\u00b7gri\u00b7mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "ART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Qu\u00e4lt sie allein sich, von allen verlassen.", "tokens": ["Qu\u00e4lt", "sie", "al\u00b7lein", "sich", ",", "von", "al\u00b7len", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PRF", "$,", "APPR", "PIS", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.13": {"text": "Nun trat ich ein, ihren Schein in H\u00e4nden,", "tokens": ["Nun", "trat", "ich", "ein", ",", "ih\u00b7ren", "Schein", "in", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "In Z\u00fcrich erst wollte die Fahrt sie beenden.", "tokens": ["In", "Z\u00fc\u00b7rich", "erst", "woll\u00b7te", "die", "Fahrt", "sie", "be\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "VMFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.15": {"text": "Als sie mich sah, einen Augenblick", "tokens": ["Als", "sie", "mich", "sah", ",", "ei\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Dachte sie wohl an ein wendend Geschick,", "tokens": ["Dach\u00b7te", "sie", "wohl", "an", "ein", "wen\u00b7dend", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.17": {"text": "Doch als halb verdrossen, halb unverhohlen", "tokens": ["Doch", "als", "halb", "ver\u00b7dros\u00b7sen", ",", "halb", "un\u00b7ver\u00b7hoh\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "KOUS", "ADJD", "VVPP", "$,", "ADJD", "ADJA"], "meter": "+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.18": {"text": "Meine Freude ich kundgab, schaut sie verstohlen", "tokens": ["Mei\u00b7ne", "Freu\u00b7de", "ich", "kund\u00b7gab", ",", "schaut", "sie", "ver\u00b7stoh\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "PTKVZ", "$,", "VVFIN", "PPER", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.19": {"text": "Noch einmal zu mir: Das war sein Lieben,", "tokens": ["Noch", "ein\u00b7mal", "zu", "mir", ":", "Das", "war", "sein", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$.", "PDS", "VAFIN", "PPOSAT", "ADJA", "$,"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.20": {"text": "Von ihm, ach, von ihm in's Elend getrieben.", "tokens": ["Von", "ihm", ",", "ach", ",", "von", "ihm", "in's", "E\u00b7lend", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ITJ", "$,", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.21": {"text": "\u00bbeinsteigen nach Lindau\u00ab, und ohne zu zagen,", "tokens": ["\u00bb", "ein\u00b7stei\u00b7gen", "nach", "Lin\u00b7dau", "\u00ab", ",", "und", "oh\u00b7ne", "zu", "za\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "APPR", "NE", "$(", "$,", "KON", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.22": {"text": "F\u00fchrt' ich am Arm sie zum Eisenbahnwagen.", "tokens": ["F\u00fchrt'", "ich", "am", "Arm", "sie", "zum", "Ei\u00b7sen\u00b7bahn\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "\u00bbdein liebes Katherl,\u00ab schluchzt sie zuletzt,", "tokens": ["\u00bb", "dein", "lie\u00b7bes", "Ka\u00b7therl", ",", "\u00ab", "schluchzt", "sie", "zu\u00b7letzt", ","], "token_info": ["punct", "word", "word", "word