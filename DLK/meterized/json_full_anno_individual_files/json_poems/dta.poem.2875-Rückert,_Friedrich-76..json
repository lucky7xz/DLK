{"dta.poem.2875": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Bettler liefen rechts und links am Reisewagen,", "tokens": ["Zwei", "Bett\u00b7ler", "lie\u00b7fen", "rechts", "und", "links", "am", "Rei\u00b7se\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "KON", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ein Almosen wollt' ich ihnen nicht versagen.", "tokens": ["Und", "ein", "Al\u00b7mo\u00b7sen", "wollt'", "ich", "ih\u00b7nen", "nicht", "ver\u00b7sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VMFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem einen warf ichs zu im schnellen Vorwertseilen,", "tokens": ["Dem", "ei\u00b7nen", "warf", "ichs", "zu", "im", "schnel\u00b7len", "Vor\u00b7wert\u00b7sei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PIS", "PTKZU", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und rief: Ihr beiden theilt! Es war genug zum Theilen.", "tokens": ["Und", "rief", ":", "Ihr", "bei\u00b7den", "theilt", "!", "Es", "war", "ge\u00b7nug", "zum", "Thei\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "PIAT", "VVFIN", "$.", "PPER", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der aber nahm es ganz, dem's zugefallen war,", "tokens": ["Der", "a\u00b7ber", "nahm", "es", "ganz", ",", "dem's", "zu\u00b7ge\u00b7fal\u00b7len", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "PPER", "ADV", "$,", "PRELS", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und leer vondannen gieng der andre ganz und gar.", "tokens": ["Und", "leer", "von\u00b7dan\u00b7nen", "gieng", "der", "and\u00b7re", "ganz", "und", "gar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "ART", "ADJA", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der hat mir wol geflucht, und jener mich gesegnet;", "tokens": ["Der", "hat", "mir", "wol", "ge\u00b7flucht", ",", "und", "je\u00b7ner", "mich", "ge\u00b7seg\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KON", "PDS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ist mir denn geschehn, was oft dem Gl\u00fcck begegnet,", "tokens": ["So", "ist", "mir", "denn", "ge\u00b7schehn", ",", "was", "oft", "dem", "Gl\u00fcck", "be\u00b7geg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Das seine Gaben auch uns Bettlern im Enteilen", "tokens": ["Das", "sei\u00b7ne", "Ga\u00b7ben", "auch", "uns", "Bett\u00b7lern", "im", "Ent\u00b7ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "ADV", "PPER", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zuwirft, und denkt da\u00df wir als Br\u00fcder sollen theilen.", "tokens": ["Zu\u00b7wirft", ",", "und", "denkt", "da\u00df", "wir", "als", "Br\u00fc\u00b7der", "sol\u00b7len", "thei\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KON", "VVFIN", "KOUS", "PPER", "KOUS", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Zureichen w\u00fcrden auch getheilt die Gaben allen;", "tokens": ["Zu\u00b7rei\u00b7chen", "w\u00fcr\u00b7den", "auch", "ge\u00b7theilt", "die", "Ga\u00b7ben", "al\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "VVPP", "ART", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ganz steckt jeder ein, was ihm ist zugefallen.", "tokens": ["Doch", "ganz", "steckt", "je\u00b7der", "ein", ",", "was", "ihm", "ist", "zu\u00b7ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "PTKVZ", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}}}}