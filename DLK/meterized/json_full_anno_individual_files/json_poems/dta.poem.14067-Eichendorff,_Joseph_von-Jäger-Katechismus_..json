{"dta.poem.14067": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "J\u00e4ger-Katechismus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schatz ist K\u00f6nigin im Walde,", "tokens": ["Mein", "Schatz", "ist", "K\u00f6\u00b7ni\u00b7gin", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sto\u00df' in's Horn, in's J\u00e4gerhorn!", "tokens": ["Ich", "sto\u00df'", "in's", "Horn", ",", "in's", "J\u00e4\u00b7ger\u00b7horn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie h\u00f6rt mich fern und naht wohl balde,", "tokens": ["Sie", "h\u00f6rt", "mich", "fern", "und", "naht", "wohl", "bal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was ich blas', ist nicht verlor'n! \u2014", "tokens": ["Und", "was", "ich", "blas'", ",", "ist", "nicht", "ver\u00b7lor'n", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWS", "PPER", "PTKVZ", "$,", "VAFIN", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}