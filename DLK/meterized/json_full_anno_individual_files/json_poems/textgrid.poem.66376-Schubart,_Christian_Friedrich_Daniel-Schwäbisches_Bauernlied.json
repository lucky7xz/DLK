{"textgrid.poem.66376": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Schw\u00e4bisches Bauernlied", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So herzig, wie mein' Lisel,", "tokens": ["So", "her\u00b7zig", ",", "wie", "mein'", "Li\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gibt's halt nichts auf der Welt!", "tokens": ["Gibt's", "halt", "nichts", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vom K\u00f6pflein bis zum F\u00fc\u00dfel", "tokens": ["Vom", "K\u00f6\u00b7pflein", "bis", "zum", "F\u00fc\u00b7\u00dfel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ist sie gar wohl bestellt:", "tokens": ["Ist", "sie", "gar", "wohl", "be\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die W\u00e4nglein wei\u00df und roth;", "tokens": ["Die", "W\u00e4ng\u00b7lein", "wei\u00df", "und", "roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Mund, wie Zuckerbrod.", "tokens": ["Ihr", "Mund", ",", "wie", "Zu\u00b7cker\u00b7brod", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So herzig, wie mein' Lisel,", "tokens": ["So", "her\u00b7zig", ",", "wie", "mein'", "Li\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gibt's halt nichts auf der Welt.", "tokens": ["Gibt's", "halt", "nichts", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Viel weicher als die Seide", "tokens": ["Viel", "wei\u00b7cher", "als", "die", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist ihr kohlschwarzes Haar,", "tokens": ["Ist", "ihr", "kohl\u00b7schwar\u00b7zes", "Haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ihre Aeuglein beide", "tokens": ["Und", "ih\u00b7re", "A\u00b7e\u00b7u\u00b7glein", "bei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind wie die Sternlein klar;", "tokens": ["Sind", "wie", "die", "Stern\u00b7lein", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie blinzeln hin und her,", "tokens": ["Sie", "blin\u00b7zeln", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Sind schwarz, wie Vogelbeer.", "tokens": ["Sind", "schwarz", ",", "wie", "Vo\u00b7gel\u00b7beer", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So herzig, wie mein' Lisel,", "tokens": ["So", "her\u00b7zig", ",", "wie", "mein'", "Li\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gibt's halt nichts auf der Welt.", "tokens": ["Gibt's", "halt", "nichts", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Im D\u00f6rflein ist kein M\u00e4dchen", "tokens": ["Im", "D\u00f6r\u00b7flein", "ist", "kein", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So flei\u00dfig, wie mein' Braut.", "tokens": ["So", "flei\u00b7\u00dfig", ",", "wie", "mein'", "Braut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Winter dreht sie 's R\u00e4dchen,", "tokens": ["Im", "Win\u00b7ter", "dreht", "sie", "'s", "R\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PPER", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im Fr\u00fchling pflanzt sie Kraut.", "tokens": ["Im", "Fr\u00fch\u00b7ling", "pflanzt", "sie", "Kraut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im Sommer macht sie Heu,", "tokens": ["Im", "Som\u00b7mer", "macht", "sie", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Tr\u00e4gt Obst im Herbst herbei.", "tokens": ["Tr\u00e4gt", "Obst", "im", "Herbst", "her\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So herzig, wie mein' Lisel,", "tokens": ["So", "her\u00b7zig", ",", "wie", "mein'", "Li\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gibt's halt nichts auf der Welt.", "tokens": ["Gibt's", "halt", "nichts", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch schreibt sie, 's ist ein Wunder;", "tokens": ["Auch", "schreibt", "sie", ",", "'s", "ist", "ein", "Wun\u00b7der", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "J\u00fcngst schickt sie mir 'nen Brief,", "tokens": ["J\u00fcngst", "schickt", "sie", "mir", "'nen", "Brief", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df mir die Backen 'runter", "tokens": ["Da\u00df", "mir", "die", "Ba\u00b7cken", "'r\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das helle Wasser lief.", "tokens": ["Das", "hel\u00b7le", "Was\u00b7ser", "lief", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Liest sie in der Postill,", "tokens": ["Liest", "sie", "in", "der", "Pos\u00b7till", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "So bin ich m\u00e4uschenstill.", "tokens": ["So", "bin", "ich", "m\u00e4u\u00b7schen\u00b7still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So herzig, wie mein' Lisel,", "tokens": ["So", "her\u00b7zig", ",", "wie", "mein'", "Li\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gibt's halt nichts auf der Welt.", "tokens": ["Gibt's", "halt", "nichts", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr sollt sie tanzen sehen,", "tokens": ["Ihr", "sollt", "sie", "tan\u00b7zen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das traute Liselein!", "tokens": ["Das", "trau\u00b7te", "Li\u00b7se\u00b7lein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie h\u00fcpft und kann sich drehen,", "tokens": ["Sie", "h\u00fcpft", "und", "kann", "sich", "dre\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wie ein Wieselein;", "tokens": ["Als", "wie", "ein", "Wie\u00b7sel\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch schleift und tanzt sie dir", "tokens": ["Doch", "schleift", "und", "tanzt", "sie", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Am liebsten nur mit mir.", "tokens": ["Am", "liebs\u00b7ten", "nur", "mit", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "So herzig, wie mein' Lisel,", "tokens": ["So", "her\u00b7zig", ",", "wie", "mein'", "Li\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Gibt's halt nichts auf der Welt.", "tokens": ["Gibt's", "halt", "nichts", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O, traute Lisel! l\u00e4nger", "tokens": ["O", ",", "trau\u00b7te", "Li\u00b7sel", "!", "l\u00e4n\u00b7ger"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "NE", "$.", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Renn' ich nicht hin und her,", "tokens": ["Renn'", "ich", "nicht", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Es wird mir immer b\u00e4nger;", "tokens": ["Es", "wird", "mir", "im\u00b7mer", "b\u00e4n\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn doch die Hochzeit w\u00e4r'!", "tokens": ["Wenn", "doch", "die", "Hoch\u00b7zeit", "w\u00e4r'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Im ganzen Schwabenland", "tokens": ["Im", "gan\u00b7zen", "Schwa\u00b7ben\u00b7land"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Kriegst keine treu're Hand.", "tokens": ["Kriegst", "kei\u00b7ne", "treu'\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O du, mein' traute Lisel,", "tokens": ["O", "du", ",", "mein'", "trau\u00b7te", "Li\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "l