], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erz\u00e4hlt uns das Gewissen;", "tokens": ["Er\u00b7z\u00e4hlt", "uns", "das", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was sich der Heuchler lang' verhehlt,", "tokens": ["Was", "sich", "der", "Heuch\u00b7ler", "lang'", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird er sich sagen m\u00fcssen,", "tokens": ["Wird", "er", "sich", "sa\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "VMINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wenn Gottes Zeit kommt und ihn schilt,", "tokens": ["Wenn", "Got\u00b7tes", "Zeit", "kommt", "und", "ihn", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Gottes Zeit kommt und vergilt", "tokens": ["Wenn", "Got\u00b7tes", "Zeit", "kommt", "und", "ver\u00b7gilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4\u00dft den Frevler b\u00fc\u00dfen.", "tokens": ["Und", "l\u00e4\u00dft", "den", "Frev\u00b7ler", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "mete