{"textgrid.poem.67799": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Gewissen", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann kommt der Herr der Herrlichkeit", "tokens": ["Wann", "kommt", "der", "Herr", "der", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seines Reiches Freuden?", "tokens": ["Mit", "sei\u00b7nes", "Rei\u00b7ches", "Freu\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann kommt der Richter, Freud' und Leid,", "tokens": ["Wann", "kommt", "der", "Rich\u00b7ter", ",", "Freud'", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und B\u00f6s und Gut zu scheiden?", "tokens": ["Und", "B\u00f6s", "und", "Gut", "zu", "schei\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er ist nicht fern, er ist uns nah;", "tokens": ["Er", "ist", "nicht", "fern", ",", "er", "ist", "uns", "nah", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,", "PPER", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In unserm Herzen ist er da!", "tokens": ["In", "un\u00b7serm", "Her\u00b7zen", "ist", "er", "da", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du kannst ihn nicht vermeiden.", "tokens": ["Du", "kannst", "ihn", "nicht", "ver\u00b7mei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In unserm Herzen spricht sein Spruch;", "tokens": ["In", "un\u00b7serm", "Her\u00b7zen", "spricht", "sein", "Spruch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer mag den Spruch bestehen?", "tokens": ["Wer", "mag", "den", "Spruch", "be\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Frei aufgeschlagen ist sein Buch", "tokens": ["Frei", "auf\u00b7ge\u00b7schla\u00b7gen", "ist", "sein", "Buch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit jeglichem Vergehen.", "tokens": ["Mit", "jeg\u00b7li\u00b7chem", "Ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Blick wie Feuerflamme f\u00e4hrt", "tokens": ["Sein", "Blick", "wie", "Feu\u00b7er\u00b7flam\u00b7me", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KOKOM", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und theilt wie ein zweischneidig Schwert,", "tokens": ["Und", "theilt", "wie", "ein", "zwei\u00b7schnei\u00b7dig", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was keine Augen sehen.", "tokens": ["Was", "kei\u00b7ne", "Au\u00b7gen", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was keines Feindes Mund erz\u00e4hlt,", "tokens": ["Was", "kei\u00b7nes", "Fein\u00b7des", "Mund", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erz\u00e4hlt uns das Gewissen;", "tokens": ["Er\u00b7z\u00e4hlt", "uns", "das", "Ge\u00b7wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was sich der Heuchler lang' verhehlt,", "tokens": ["Was", "sich", "der", "Heuch\u00b7ler", "lang'", "ver\u00b7hehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird er sich sagen m\u00fcssen,", "tokens": ["Wird", "er", "sich", "sa\u00b7gen", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "VVINF", "VMINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Wenn Gottes Zeit kommt und ihn schilt,", "tokens": ["Wenn", "Got\u00b7tes", "Zeit", "kommt", "und", "ihn", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn Gottes Zeit kommt und vergilt", "tokens": ["Wenn", "Got\u00b7tes", "Zeit", "kommt", "und", "ver\u00b7gilt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und l\u00e4\u00dft den Frevler b\u00fc\u00dfen.", "tokens": ["Und", "l\u00e4\u00dft", "den", "Frev\u00b7ler", "b\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wem kam nicht diese Gotteszeit", "tokens": ["Wem", "kam", "nicht", "die\u00b7se", "Got\u00b7tes\u00b7zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So oft und oft im Leben?", "tokens": ["So", "oft", "und", "oft", "im", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer mu\u00df nicht die Gerechtigkeit", "tokens": ["Wer", "mu\u00df", "nicht", "die", "Ge\u00b7rech\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Anflehn, ihm zu vergeben?", "tokens": ["An\u00b7flehn", ",", "ihm", "zu", "ver\u00b7ge\u00b7ben", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und f\u00fchlt in seinem Innern noch", "tokens": ["Und", "f\u00fchlt", "in", "sei\u00b7nem", "In\u00b7nern", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Viel stumme Schulden, denen doch", "tokens": ["Viel", "stum\u00b7me", "Schul\u00b7den", ",", "de\u00b7nen", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er einst wird m\u00fcssen beben!", "tokens": ["Er", "einst", "wird", "m\u00fcs\u00b7sen", "be\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du Herzensrichter, auf! erfahr", "tokens": ["Du", "Her\u00b7zens\u00b7rich\u00b7ter", ",", "auf", "!", "er\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "NN", "$,", "PTKVZ", "$.", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und pr\u00fcfe, wie wir's meinen!", "tokens": ["Und", "pr\u00fc\u00b7fe", ",", "wie", "wir's", "mei\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mach unsre Fehl uns offenbar;", "tokens": ["Mach", "uns\u00b7re", "Fehl", "uns", "of\u00b7fen\u00b7bar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was n\u00fctzt es, gut zu scheinen!", "tokens": ["Was", "n\u00fctzt", "es", ",", "gut", "zu", "schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Ausspruch des Gewissens treu", "tokens": ["Dem", "Aus\u00b7spruch", "des", "Ge\u00b7wis\u00b7sens", "treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und feind sein jeder Heuchelei,", "tokens": ["Und", "feind", "sein", "je\u00b7der", "Heu\u00b7che\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dies stellt uns zu den Deinen.", "tokens": ["Dies", "stellt", "uns", "zu", "den", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Denn wen sein eignes Herz besch\u00e4mt", "tokens": ["Denn", "wen", "sein", "eig\u00b7nes", "Herz", "be\u00b7sch\u00e4mt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit innerstem Besch\u00e4men,", "tokens": ["Mit", "in\u00b7ners\u00b7tem", "Be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Schuld, die uns im Innern gr\u00e4mt,", "tokens": ["Die", "Schuld", ",", "die", "uns", "im", "In\u00b7nern", "gr\u00e4mt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer k\u00f6nnt' uns die entnehmen?", "tokens": ["Wer", "k\u00f6nnt'", "uns", "die", "ent\u00b7neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr, gieb, da\u00df wir der S\u00fcnde Schritt", "tokens": ["Herr", ",", "gieb", ",", "da\u00df", "wir", "der", "S\u00fcn\u00b7de", "Schritt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Deiner Strafe leisen Tritt,", "tokens": ["Und", "Dei\u00b7ner", "Stra\u00b7fe", "lei\u00b7sen", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eh sie uns naht, vernehmen!", "tokens": ["Eh", "sie", "uns", "naht", ",", "ver\u00b7neh\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und wenn die letzte Stunde schl\u00e4gt,", "tokens": ["Und", "wenn", "die", "letz\u00b7te", "Stun\u00b7de", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Niemand kann entgehen,", "tokens": ["Der", "Nie\u00b7mand", "kann", "ent\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gieb, Herr, da\u00df wir unbewegt", "tokens": ["So", "gieb", ",", "Herr", ",", "da\u00df", "wir", "un\u00b7be\u00b7wegt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "$,", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf unser Innres sehen,", "tokens": ["Auf", "un\u00b7ser", "Inn\u00b7res", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df unser Leben uns dann klar", "tokens": ["Da\u00df", "un\u00b7ser", "Le\u00b7ben", "uns", "dann", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rein erschein' und offenbar", "tokens": ["Und", "rein", "er\u00b7schein'", "und", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das kleineste Vergehen!", "tokens": ["Das", "klei\u00b7nes\u00b7te", "Ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dann sprich in uns, o Richter: \u00bbKomm!", "tokens": ["Dann", "sprich", "in", "uns", ",", "o", "Rich\u00b7ter", ":", "\u00bb", "Komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$.", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Lohn ist Dir beschieden.", "tokens": ["Dein", "Lohn", "ist", "Dir", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Du gethan hast gut und fromm", "tokens": ["Was", "Du", "ge\u00b7than", "hast", "gut", "und", "fromm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem D\u00fcrftigsten hienieden,", "tokens": ["Dem", "D\u00fcrf\u00b7tigs\u00b7ten", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Das hast der Menschheit Du gethan,", "tokens": ["Das", "hast", "der", "Menschheit", "Du", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Dem Menschensohne. Komm hinan!", "tokens": ["Dem", "Men\u00b7schen\u00b7soh\u00b7ne", ".", "Komm", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Genie\u00dfe Himmelsfrieden!\u00ab", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "Him\u00b7mels\u00b7frie\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wann kommt der Herr der Herrlichkeit", "tokens": ["Wann", "kommt", "der", "Herr", "der", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seines Reiches Freuden?", "tokens": ["Mit", "sei\u00b7nes", "Rei\u00b7ches", "Freu\u00b7den", "?"], "token_info": ["word