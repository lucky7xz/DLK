": "Dies stellt uns zu den Deinen.", "tokens": ["Dies", "stellt", "uns", "zu", "den", "Dei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "PPOSAT", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Denn wen sein eignes Herz besch\u00e4mt", "tokens": ["Denn", "wen", "sein", "eig\u00b7nes", "Herz", "be\u00b7sch\u00e4mt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit innerstem Besch\u00e4men,", "tokens": ["Mit", "in\u00b7ners\u00b7tem", "Be\u00b7sch\u00e4\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Die Schuld, die uns im Innern gr\u00e4mt,", "tokens": ["Die", "Schuld", ",", "die", "uns", "im", "In\u00b7nern", "gr\u00e4mt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer k\u00f6nnt' uns die entnehmen?", "tokens": ["Wer", "k\u00f6nnt'", "uns", "die", "ent\u00b7neh\u00b7men", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ART", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Herr, gieb, da\u00df wir der S\u00fcnde Schritt", "tokens": ["Herr", ",", "gieb", ",", "da\u00df", "wir", "der", "S\u00fcn\u00b7de", "Schritt"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVIMP", "$,", "KOUS", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Deiner Strafe leisen Tritt,", "tokens": ["Und", "Dei\u00b7ner", "Stra\u00b7fe", "lei\u00b7sen", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eh sie uns naht, vernehmen!", "tokens": ["Eh", "sie", "uns", "naht", ",", "ver\u00b7neh\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Und wenn die letzte Stunde schl\u00e4gt,", "tokens": ["Und", "wenn", "die", "letz\u00b7te", "Stun\u00b7de", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Niemand kann entgehen,", "tokens": ["Der", "Nie\u00b7mand", "kann", "ent\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gieb, Herr, da\u00df wir unbewegt", "tokens": ["So", "gieb", ",", "Herr", ",", "da\u00df", "wir", "un\u00b7be\u00b7wegt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "$,", "NN", "$,", "KOUS", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf unser Innres sehen,", "tokens": ["Auf", "un\u00b7ser", "Inn\u00b7res", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df unser Leben uns dann klar", "tokens": ["Da\u00df", "un\u00b7ser", "Le\u00b7ben", "uns", "dann", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und rein erschein' und offenbar", "tokens": ["Und", "rein", "er\u00b7schein'", "und", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das kleineste Vergehen!", "tokens": ["Das", "klei\u00b7nes\u00b7te", "Ver\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Dann sprich in uns, o Richter: \u00bbKomm!", "tokens": ["Dann", "sprich", "in", "uns", ",", "o", "Rich\u00b7ter", ":", "\u00bb", "Komm", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "$,", "FM", "NN", "$.", "$(", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Lohn ist Dir beschieden.", "tokens": ["Dein", "Lohn", "ist", "Dir", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was Du gethan hast gut und fromm", "tokens": ["Was", "Du", "ge\u00b7than", "hast", "gut", "und", "fromm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "VVPP", "VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dem D\u00fcrftigsten hienieden,", "tokens": ["Dem", "D\u00fcrf\u00b7tigs\u00b7ten", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.5": {"text": "Das hast der Menschheit Du gethan,", "tokens": ["Das", "hast", "der", "Menschheit", "Du", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Dem Menschensohne. Komm hinan!", "tokens": ["Dem", "Men\u00b7schen\u00b7soh\u00b7ne", ".", "Komm", "hi\u00b7nan", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Genie\u00dfe Himmelsfrieden!\u00ab", "tokens": ["Ge\u00b7nie\u00b7\u00dfe", "Him\u00b7mels\u00b7frie\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}