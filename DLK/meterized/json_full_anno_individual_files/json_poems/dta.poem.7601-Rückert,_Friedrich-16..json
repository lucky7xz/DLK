{"dta.poem.7601": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Eisen, wenn sich ihm des Feuers Kraft vereint,", "tokens": ["Das", "Ei\u00b7sen", ",", "wenn", "sich", "ihm", "des", "Feu\u00b7ers", "Kraft", "ver\u00b7eint", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PRF", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Roth gl\u00fcht es, da\u00df es wie ein Edelstein erscheint.", "tokens": ["Roth", "gl\u00fcht", "es", ",", "da\u00df", "es", "wie", "ein", "E\u00b7del\u00b7stein", "er\u00b7scheint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "KOUS", "PPER", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der roth von selber ist, der feurige Rubin,", "tokens": ["Der", "roth", "von", "sel\u00b7ber", "ist", ",", "der", "feu\u00b7ri\u00b7ge", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ADV", "VAFIN", "$,", "ART", "ADJA", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheint dagegen bla\u00df, gl\u00fcht man im Feuer ihn.", "tokens": ["Er\u00b7scheint", "da\u00b7ge\u00b7gen", "bla\u00df", ",", "gl\u00fcht", "man", "im", "Feu\u00b7er", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ADJD", "$,", "VVFIN", "PIS", "APPRART", "NN", "PPER", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "So hat des Menschen Sinn, von Leidenschaft berauscht,", "tokens": ["So", "hat", "des", "Men\u00b7schen", "Sinn", ",", "von", "Lei\u00b7den\u00b7schaft", "be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein Eignes, auf den Schein, um Fremdes ausgetauscht.", "tokens": ["Sein", "Eig\u00b7nes", ",", "auf", "den", "Schein", ",", "um", "Frem\u00b7des", "aus\u00b7ge\u00b7tauscht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,", "KOUI", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch, wenn erkaltet, wird das Eisen wieder dunkel,", "tokens": ["Doch", ",", "wenn", "er\u00b7kal\u00b7tet", ",", "wird", "das", "Ei\u00b7sen", "wie\u00b7der", "dun\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "VVPP", "$,", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wieder hell, wie er gewesen, der Karfunkel.", "tokens": ["Und", "wie\u00b7der", "hell", ",", "wie", "er", "ge\u00b7we\u00b7sen", ",", "der", "Kar\u00b7fun\u00b7kel", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "PPER", "VAPP", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}