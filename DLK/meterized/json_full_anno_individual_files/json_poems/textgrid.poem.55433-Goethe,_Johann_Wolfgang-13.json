{"textgrid.poem.55433": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "13", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich dachte dein, und Farben bunt erschienen", "tokens": ["Ich", "dach\u00b7te", "dein", ",", "und", "Far\u00b7ben", "bunt", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "KON", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Sonnenglanz mir vorm Gesicht,", "tokens": ["Im", "Son\u00b7nen\u00b7glanz", "mir", "vorm", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Bl\u00e4ttern sah ich mancherlei ergr\u00fcnen,", "tokens": ["Von", "Bl\u00e4t\u00b7tern", "sah", "ich", "man\u00b7cher\u00b7lei", "er\u00b7gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da waren Rosen, auch Vergi\u00dfmeinnicht!", "tokens": ["Da", "wa\u00b7ren", "Ro\u00b7sen", ",", "auch", "Ver\u00b7gi\u00df\u00b7mein\u00b7nicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Pfeile dazwischen, golden anzuschauen,", "tokens": ["Pfei\u00b7le", "da\u00b7zwi\u00b7schen", ",", "gol\u00b7den", "an\u00b7zu\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PAV", "$,", "ADJD", "VVIZU", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Durchscheinend alles, rings ein goldner Kranz;", "tokens": ["Durch\u00b7schei\u00b7nend", "al\u00b7les", ",", "rings", "ein", "gold\u00b7ner", "Kranz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "$,", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und angestimmt das hohe Lob der Frauen \u2013", "tokens": ["Und", "an\u00b7ge\u00b7stimmt", "das", "ho\u00b7he", "Lob", "der", "Frau\u00b7en", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nun, Becher, zu der Freundin! Bleibe klar und ganz.", "tokens": ["Nun", ",", "Be\u00b7cher", ",", "zu", "der", "Freun\u00b7din", "!", "Blei\u00b7be", "klar", "und", "ganz", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "APPR", "ART", "NN", "$.", "NN", "ADJD", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich dachte dein, und Farben bunt erschienen", "tokens": ["Ich", "dach\u00b7te", "dein", ",", "und", "Far\u00b7ben", "bunt", "er\u00b7schie\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "$,", "KON", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Sonnenglanz mir vorm Gesicht,", "tokens": ["Im", "Son\u00b7nen\u00b7glanz", "mir", "vorm", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vo