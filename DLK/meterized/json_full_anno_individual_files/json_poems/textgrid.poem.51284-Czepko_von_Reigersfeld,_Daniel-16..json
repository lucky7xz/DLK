{"textgrid.poem.51284": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auff da\u00df du Menschen Kind dein Ungl\u00fcck kanst ertragen,", "tokens": ["Auff", "da\u00df", "du", "Men\u00b7schen", "Kind", "dein", "Un\u00b7gl\u00fcck", "kanst", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "NN", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nihm es an von Gott: Was wilt du dr\u00fcber klagen?", "tokens": ["So", "nihm", "es", "an", "von", "Gott", ":", "Was", "wilt", "du", "dr\u00fc\u00b7ber", "kla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN", "$.", "PWS", "VMFIN", "PPER", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wann deinen Willen du Gott g\u00e4ntzlich tr\u00e4gest f\u00fcr,", "tokens": ["Wann", "dei\u00b7nen", "Wil\u00b7len", "du", "Gott", "g\u00e4ntz\u00b7lich", "tr\u00e4\u00b7gest", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "NN", "ADJD", "VVFIN", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So mu\u00df vor iedes Haar er Antwort geben dir.", "tokens": ["So", "mu\u00df", "vor", "ie\u00b7des", "Haar", "er", "Ant\u00b7wort", "ge\u00b7ben", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN"