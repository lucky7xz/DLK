{"textgrid.poem.59493": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Donna Alda", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.57", "et:0.28", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Paris sa\u00df Donna Alda,", "tokens": ["In", "Pa\u00b7ris", "sa\u00df", "Don\u00b7na", "Al\u00b7da", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rolands Braut, im hohen Saal", "tokens": ["Ro\u00b7lands", "Braut", ",", "im", "ho\u00b7hen", "Saal"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit ihr dreihundert Damen,", "tokens": ["Und", "mit", "ihr", "drei\u00b7hun\u00b7dert", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "CARD", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Ihrer Gespielinnen Schar;", "tokens": ["Ih\u00b7rer", "Ge\u00b7spie\u00b7lin\u00b7nen", "Schar", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "Alle waren gleich beschuhet,", "tokens": ["Al\u00b7le", "wa\u00b7ren", "gleich", "be\u00b7schu\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle trugen gleich Gewand,", "tokens": ["Al\u00b7le", "tru\u00b7gen", "gleich", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "A\u00dfen rund um eine Tafel", "tokens": ["A\u00b7\u00dfen", "rund", "um", "ei\u00b7ne", "Ta\u00b7fel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Von demselben Brot zumal,", "tokens": ["Von", "dem\u00b7sel\u00b7ben", "Brot", "zu\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Donna Alda ausgenommen,", "tokens": ["Don\u00b7na", "Al\u00b7da", "aus\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Weil sie ihre Herrin war.", "tokens": ["Weil", "sie", "ih\u00b7re", "Her\u00b7rin", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Hundert spannen goldne F\u00e4den,", "tokens": ["Hun\u00b7dert", "span\u00b7nen", "gold\u00b7ne", "F\u00e4\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hundert woben Tepp'che zart,", "tokens": ["Hun\u00b7dert", "wo\u00b7ben", "Tepp'\u00b7che", "zart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Hundert aber musizierten,", "tokens": ["Hun\u00b7dert", "a\u00b7ber", "mu\u00b7si\u00b7zier\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["CARD", "ADV", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Sie zu tr\u00f6sten mit Gesang.", "tokens": ["Sie", "zu", "tr\u00f6s\u00b7ten", "mit", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Donna Alda war entschlummert", "tokens": ["Don\u00b7na", "Al\u00b7da", "war", "ent\u00b7schlum\u00b7mert"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NE", "VAFIN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei der Instrumente Klang,", "tokens": ["Bei", "der", "Inst\u00b7ru\u00b7men\u00b7te", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Pl\u00f6tzlich fuhr sie auf, laut schreiend,", "tokens": ["Pl\u00f6tz\u00b7lich", "fuhr", "sie", "auf", ",", "laut", "schrei\u00b7end", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df man's h\u00f6rt' bis in die Stadt.", "tokens": ["Da\u00df", "man's", "h\u00f6rt'", "bis", "in", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zu ihr sprachen da die Jungfraun:", "tokens": ["Zu", "ihr", "spra\u00b7chen", "da", "die", "Jung\u00b7fraun", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00bbwer tat Euch was Schlimmes an? \u2013\u00ab", "tokens": ["\u00bb", "wer", "tat", "Euch", "was", "Schlim\u00b7mes", "an", "?", "\u2013", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "PIS", "NN", "PTKVZ", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbeinen Traum hatt ich, ihr M\u00e4dchen,", "tokens": ["\u00bb", "ei\u00b7nen", "Traum", "hatt", "ich", ",", "ihr", "M\u00e4d\u00b7chen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der mir gro\u00dfen Schrecken gab:", "tokens": ["Der", "mir", "gro\u00b7\u00dfen", "Schre\u00b7cken", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Einsam im Gebirge stand ich,", "tokens": ["Ein\u00b7sam", "im", "Ge\u00b7bir\u00b7ge", "stand", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch die \u00d6de flog ein Falk,", "tokens": ["Durch", "die", "\u00d6\u00b7de", "flog", "ein", "Falk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hinterdrein ein junger Adler,", "tokens": ["Hin\u00b7ter\u00b7drein", "ein", "jun\u00b7ger", "Ad\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00e4ngend ihn in wilder Jagd,", "tokens": ["Dr\u00e4n\u00b7gend", "ihn", "in", "wil\u00b7der", "Jagd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "So ge\u00e4ngstigt st\u00fcrzt der Falke", "tokens": ["So", "ge\u00b7\u00e4ngs\u00b7tigt", "st\u00fcrzt", "der", "Fal\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Fl\u00fcchtend sich in mein Gewand,", "tokens": ["Fl\u00fcch\u00b7tend", "sich", "in", "mein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Doch der Aar mit seinen F\u00e4ngen", "tokens": ["Doch", "der", "Aar", "mit", "sei\u00b7nen", "F\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Hatt ihn zornig schon umkrallt,", "tokens": ["Hatt", "ihn", "zor\u00b7nig", "schon", "um\u00b7krallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ri\u00df den Falken mir in St\u00fccke,", "tokens": ["Ri\u00df", "den", "Fal\u00b7ken", "mir", "in", "St\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Streut' die Federn \u00fcbern Plan.\u00ab", "tokens": ["Streut'", "die", "Fe\u00b7dern", "\u00fc\u00b7bern", "Plan", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drauf zu der erschrocknen Herrin", "tokens": ["Drauf", "zu", "der", "er\u00b7schrock\u00b7nen", "Her\u00b7rin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "APPR", "ART", "ADJA", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Eins der Kammerfr\u00e4ulein sprach:", "tokens": ["Eins", "der", "Kam\u00b7mer\u00b7fr\u00e4u\u00b7lein", "sprach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbdiesen Traum will ich Euch deuten:", "tokens": ["\u00bb", "die\u00b7sen", "Traum", "will", "ich", "Euch", "deu\u00b7ten", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer Br\u00e4ut'gam ist der Falk,", "tokens": ["Eu\u00b7er", "Br\u00e4ut'\u00b7gam", "ist", "der", "Falk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der sich \u00fcbers Meer verflogen,", "tokens": ["Der", "sich", "\u00fc\u00b7bers", "Meer", "ver\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eure Sch\u00f6nheit ist der Aar,", "tokens": ["Eu\u00b7re", "Sch\u00f6n\u00b7heit", "ist", "der", "Aar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Der den wilden Edelfalken", "tokens": ["Der", "den", "wil\u00b7den", "E\u00b7del\u00b7fal\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich im Flug gefangen hat,", "tokens": ["Sich", "im", "Flug", "ge\u00b7fan\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und das Hochgebirg die Kirche,", "tokens": ["Und", "das", "Hoch\u00b7ge\u00b7birg", "die", "Kir\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wo man traut Euch am Altar. \u2013\u00ab", "tokens": ["Wo", "man", "traut", "Euch", "am", "Al\u00b7tar", ".", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "\u00bbreichlich wohl will ich dir's lohnen,", "tokens": ["\u00bb", "reich\u00b7lich", "wohl", "will", "ich", "dir's", "loh\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "VMFIN", "PPER", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Liebes M\u00e4dchen, sprichst du wahr.\u00ab", "tokens": ["Lie\u00b7bes", "M\u00e4d\u00b7chen", ",", "sprichst", "du", "wahr", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kam ein Brief am andern Morgen,", "tokens": ["Kam", "ein", "Brief", "am", "an\u00b7dern", "Mor\u00b7gen", ","], "token_info": ["word", "wor