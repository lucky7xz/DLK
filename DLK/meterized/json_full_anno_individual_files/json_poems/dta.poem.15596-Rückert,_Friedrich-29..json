{"dta.poem.15596": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "29.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sich selbst gen\u00fcgen und von andern nichts verlangen,", "tokens": ["Sich", "selbst", "ge\u00b7n\u00fc\u00b7gen", "und", "von", "an\u00b7dern", "nichts", "ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "KON", "APPR", "PIS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist Weisheit frostige, die zeitig mir zergangen.", "tokens": ["Ist", "Weis\u00b7heit", "fros\u00b7ti\u00b7ge", ",", "die", "zei\u00b7tig", "mir", "zer\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nie gn\u00fcgest du dir selbst, wenn du nicht andre liebst,", "tokens": ["Nie", "gn\u00fc\u00b7gest", "du", "dir", "selbst", ",", "wenn", "du", "nicht", "and\u00b7re", "liebst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "KOUS", "PPER", "PTKNEG", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von denen du empf\u00e4ngst, und ihnen wieder giebst.", "tokens": ["Von", "de\u00b7nen", "du", "emp\u00b7f\u00e4ngst", ",", "und", "ih\u00b7nen", "wie\u00b7der", "giebst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", 