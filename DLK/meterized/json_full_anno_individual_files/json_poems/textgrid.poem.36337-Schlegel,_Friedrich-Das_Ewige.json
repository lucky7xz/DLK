{"textgrid.poem.36337": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Ewige", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fcchte fallen, Rosen bleichen,", "tokens": ["Fr\u00fcch\u00b7te", "fal\u00b7len", ",", "Ro\u00b7sen", "blei\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bl\u00fcte mu\u00df der Bl\u00fcte weichen;", "tokens": ["Bl\u00fc\u00b7te", "mu\u00df", "der", "Bl\u00fc\u00b7te", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nimmer doch, vom Tode grau,", "tokens": ["Nim\u00b7mer", "doch", ",", "vom", "To\u00b7de", "grau", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lischt des Himmels Sternenblau;", "tokens": ["Lischt", "des", "Him\u00b7mels", "Ster\u00b7nen\u00b7blau", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewig auf und nieder schwellen", "tokens": ["E\u00b7wig", "auf", "und", "nie\u00b7der", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "KON", "PTKVZ", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses Meeres alte Wellen.", "tokens": ["Die\u00b7ses", "Mee\u00b7res", "al\u00b7te", "Wel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos":