{"dta.poem.19155": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.85", "id:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geh, wann du hast am Tag im Hause still zu thun,", "tokens": ["Geh", ",", "wann", "du", "hast", "am", "Tag", "im", "Hau\u00b7se", "still", "zu", "thun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "VAFIN", "APPRART", "NN", "APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Am Abend aus, das ist der Weg um auszuruhn.", "tokens": ["Am", "A\u00b7bend", "aus", ",", "das", "ist", "der", "Weg", "um", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "PDS", "VAFIN", "ART", "NN", "APPR", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Ruh erm\u00fcdete, Bewegung ruhet aus,", "tokens": ["Die", "Ruh", "er\u00b7m\u00fc\u00b7de\u00b7te", ",", "Be\u00b7we\u00b7gung", "ru\u00b7het", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und zu der Arbeitsruh kehrst du gest\u00e4rkt nach Haus.", "tokens": ["Und", "zu", "der", "Ar\u00b7beits\u00b7ruh", "kehrst", "du", "ge\u00b7st\u00e4rkt", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und einen frischern Straus, als du mit Kunst geschm\u00fcckt", "tokens": ["Und", "ei\u00b7nen", "fri\u00b7schern", "Straus", ",", "als", "du", "mit", "Kunst", "ge\u00b7schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Daheim, bringst du nach Haus, auf Gottes Flur gepfl\u00fcckt.", "tokens": ["Da\u00b7heim", ",", "bringst", "du", "nach", "Haus", ",", "auf", "Got\u00b7tes", "Flur", "ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Auf Gottes sch\u00f6ner Flur o wollt' es lenzen nur", "tokens": ["Auf", "Got\u00b7tes", "sch\u00f6\u00b7ner", "Flur", "o", "wollt'", "es", "len\u00b7zen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "FM", "VMFIN", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sonst! doch von dem Lenz ist nirgend eine Spur.", "tokens": ["Wie", "sonst", "!", "doch", "von", "dem", "Lenz", "ist", "nir\u00b7gend", "ei\u00b7ne", "Spur", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$.", "ADV", "APPR", "ART", "NN", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Auf Gottes sch\u00f6ne Flur o bring im Herzen nur", "tokens": ["Auf", "Got\u00b7tes", "sch\u00f6\u00b7ne", "Flur", "o", "bring", "im", "Her\u00b7zen", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN", "FM", "VVFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Fr\u00fchling mit, so gehst du nicht auf Winterspur.", "tokens": ["Den", "Fr\u00fch\u00b7ling", "mit", ",", "so", "gehst", "du", "nicht", "auf", "Win\u00b7ter\u00b7spur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der Lenz im Herzen nur zeigt dir des Lenzes Spur", "tokens": ["Der", "Lenz", "im", "Her\u00b7zen", "nur", "zeigt", "dir", "des", "Len\u00b7zes", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "NN", "ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von au\u00dfen auch, und macht die Welt zur Gottesflur.", "tokens": ["Von", "au\u00b7\u00dfen", "auch", ",", "und", "macht", "die", "Welt", "zur", "Got\u00b7tes\u00b7flur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}