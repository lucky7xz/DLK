{"dta.poem.5945": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Bau der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir sprechen dies darauf: Die C\u00f6rper, welche sich", "tokens": ["Wir", "spre\u00b7chen", "dies", "da\u00b7rauf", ":", "Die", "C\u00f6r\u00b7per", ",", "wel\u00b7che", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PDS", "PAV", "$.", "ART", "NN", "$,", "PRELS", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um ihren Mittel-Punct in einem Creyse regen,", "tokens": ["Um", "ih\u00b7ren", "Mit\u00b7tel\u00b7Punct", "in", "ei\u00b7nem", "Crey\u00b7se", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ART", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Indem, (sie m\u00f6gen sich bewegen,", "tokens": ["In\u00b7dem", ",", "(", "sie", "m\u00f6\u00b7gen", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "$(", "PPER", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie oder stille stehn)", "tokens": ["Wie", "o\u00b7der", "stil\u00b7le", "stehn", ")"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "KON", "ADJA", "VVINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie nicht mehr Raum gebrauchen; drehn", "tokens": ["Sie", "nicht", "mehr", "Raum", "ge\u00b7brau\u00b7chen", ";", "drehn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "PTKNEG", "PIAT", "NN", "VVINF", "$.", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sich durch einander um: Es drehet allemal", "tokens": ["Sich", "durch", "ein\u00b7an\u00b7der", "um", ":", "Es", "dre\u00b7het", "al\u00b7le\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "PRF", "PTKVZ", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die gantze Masse sich, recht wie ein Crey\u00df aus Stahl,", "tokens": ["Die", "gant\u00b7ze", "Mas\u00b7se", "sich", ",", "recht", "wie", "ein", "Crey\u00df", "aus", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,", "ADJD", "KOKOM", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der glatt und wol polirt. Ob er gleich gantz erf\u00fcllt;", "tokens": ["Der", "glatt", "und", "wol", "po\u00b7lirt", ".", "Ob", "er", "gleich", "gantz", "er\u00b7f\u00fcllt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ADV", "VVPP", "$.", "KOUS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So h\u00f6rt gleichwohl sein Lauff,", "tokens": ["So", "h\u00f6rt", "gleich\u00b7wohl", "sein", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Wohin er sich auch dreht, deswegen doch nicht auf.", "tokens": ["Wo\u00b7hin", "er", "sich", "auch", "dreht", ",", "des\u00b7we\u00b7gen", "doch", "nicht", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,", "PAV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er dreht sich auf sich selbst, er folgt und jaget sich.", "tokens": ["Er", "dreht", "sich", "auf", "sich", "selbst", ",", "er", "folgt", "und", "ja\u00b7get", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PRF", "ADV", "$,", "PPER", "VVFIN", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Bewegt ist alles, voll ist alles, alles treibt", "tokens": ["Be\u00b7wegt", "ist", "al\u00b7les", ",", "voll", "ist", "al\u00b7les", ",", "al\u00b7les", "treibt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "PIS", "$,", "ADJD", "VAFIN", "PIS", "$,", "PIS", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sich in der Aendrung fort, da, welches sonderlich,", "tokens": ["Sich", "in", "der", "A\u00b7en\u00b7drung", "fort", ",", "da", ",", "wel\u00b7ches", "son\u00b7der\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKVZ", "$,", "KOUS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "In der Bewegung doch ein\u2019 Ordnung immer bleibt.", "tokens": ["In", "der", "Be\u00b7we\u00b7gung", "doch", "ein'", "Ord\u00b7nung", "im\u00b7mer", "bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ein C\u00f6rper gehet fort, ein andrer nimmt die Stelle.", "tokens": ["Ein", "C\u00f6r\u00b7per", "ge\u00b7het", "fort", ",", "ein", "an\u00b7drer", "nimmt", "die", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "ART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ein Fisch, indem er schwimmt, so giebt die Welle,", "tokens": ["Ein", "Fisch", ",", "in\u00b7dem", "er", "schwimmt", ",", "so", "giebt", "die", "Wel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Die sich im Creyse dreht, ihm Raum. Geht er nun fort;", "tokens": ["Die", "sich", "im", "Crey\u00b7se", "dreht", ",", "ihm", "Raum", ".", "Geht", "er", "nun", "fort", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "VVFIN", "$,", "PPER", "NN", "$.", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.18": {"text": "So ist zu gleicher Zeit der Ort,", "tokens": ["So", "ist", "zu", "glei\u00b7cher", "Zeit", "der", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Aus welchem er gekommen,", "tokens": ["Aus", "wel\u00b7chem", "er", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Von Circkeln, welche reg\u2019 und fl\u00fc\u00dfig, eingenommen.", "tokens": ["Von", "Cir\u00b7ckeln", ",", "wel\u00b7che", "reg'", "und", "fl\u00fc\u00b7\u00dfig", ",", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}