{"dta.poem.8945": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XiII.  \n  Serenate.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Mitgift. Dieses erfuhr Jeder und ich, Klagender, wei\u00df", "tokens": ["Mit\u00b7gift", ".", "Die\u00b7ses", "er\u00b7fuhr", "Je\u00b7der", "und", "ich", ",", "Kla\u00b7gen\u00b7der", ",", "wei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "PDS", "VVFIN", "PIS", "KON", "PPER", "$,", "NN", "$,", "VVFIN"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Zwar mir l\u00e4chelte manch freundlicher Blick s\u00fc\u00dfe Ver-", "tokens": ["Zwar", "mir", "l\u00e4\u00b7chel\u00b7te", "manch", "freund\u00b7li\u00b7cher", "Blick", "s\u00fc\u00b7\u00dfe", "Ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Zu; bald w\u00e4r' ich erh\u00f6rt, br\u00e4chte mir, ach! blinder Ge-", "tokens": ["Zu", ";", "bald", "w\u00e4r'", "ich", "er\u00b7h\u00f6rt", ",", "br\u00e4ch\u00b7te", "mir", ",", "ach", "!", "blin\u00b7der", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "$.", "ADV", "VAFIN", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$,", "ITJ", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Doch ich seufze ja nur Liebe zu dir, Liebe zu dir ja nur!", "tokens": ["Doch", "ich", "seuf\u00b7ze", "ja", "nur", "Lie\u00b7be", "zu", "dir", ",", "Lie\u00b7be", "zu", "dir", "ja", "nur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "NN", "APPR", "PPER", "$,", "NN", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+--+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.5": {"text": "Ach, und w\u00e4hrend ich hier klage, vielleicht dient ein Ge-", "tokens": ["Ach", ",", "und", "w\u00e4h\u00b7rend", "ich", "hier", "kla\u00b7ge", ",", "viel\u00b7leicht", "dient", "ein", "Ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "KOUS", "PPER", "ADV", "VVFIN", "$,", "ADV", "VVFIN", "ART", "TRUNC"], "meter": "+-+-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Als Wegweiser f\u00fcr Ihn, welcher den Arm \u00fcber die", "tokens": ["Als", "Weg\u00b7wei\u00b7ser", "f\u00fcr", "Ihn", ",", "wel\u00b7cher", "den", "Arm", "\u00fc\u00b7ber", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "APPR", "PPER", "$,", "PRELS", "ART", "NN", "APPR", "ART"], "meter": "+-+-+-+--++-+", "measure": "trochaic.septa.relaxed"}, "line.7": {"text": "Legt, und K\u00fcsse vielleicht, freudeberauscht, griechischen", "tokens": ["Legt", ",", "und", "K\u00fcs\u00b7se", "viel\u00b7leicht", ",", "freu\u00b7de\u00b7be\u00b7rauscht", ",", "grie\u00b7chi\u00b7schen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "$,", "KON", "NN", "ADV", "$,", "VVFIN", "$,", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}}}}}