{"dta.poem.20441": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Seneca Epist. 26.  \n Quid egeris, tunc apparebit, cum  \n animam ages.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Such armer/ wie du wilt/ zu deinen thaten nacht/", "tokens": ["Such", "ar\u00b7mer", "/", "wie", "du", "wilt", "/", "zu", "dei\u00b7nen", "tha\u00b7ten", "nacht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$(", "PWAV", "PPER", "VMFIN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verstelle sinn und hertz/ den nechsten zu betr\u00fcgen/", "tokens": ["Ver\u00b7stel\u00b7le", "sinn", "und", "hertz", "/", "den", "nechs\u00b7ten", "zu", "be\u00b7tr\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$(", "ART", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der mund beflei\u00dfe sich uns lugend vorzul\u00fcgen/", "tokens": ["Der", "mund", "be\u00b7flei\u00b7\u00dfe", "sich", "uns", "lu\u00b7gend", "vor\u00b7zu\u00b7l\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So unter schimpff und koth der laster schon verschmacht;", "tokens": ["So", "un\u00b7ter", "schimpff", "und", "koth", "der", "las\u00b7ter", "schon", "ver\u00b7schmacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "La\u00df in der augen glantz die freundligkeiten spielen/", "tokens": ["La\u00df", "in", "der", "au\u00b7gen", "glantz", "die", "freund\u00b7lig\u00b7kei\u00b7ten", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "NN", "ART", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Im busen wird doch gifft und rauch und unmuth w\u00fchlen/", "tokens": ["Im", "bu\u00b7sen", "wird", "doch", "gifft", "und", "rauch", "und", "un\u00b7muth", "w\u00fch\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "NN", "KON", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn itzt die seele soll von ihrem leibe ziehn/", "tokens": ["Wenn", "itzt", "die", "see\u00b7le", "soll", "von", "ih\u00b7rem", "lei\u00b7be", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und das gewissen wacht/ dein zeuge/ dein verr\u00e4ther/", "tokens": ["Und", "das", "ge\u00b7wis\u00b7sen", "wacht", "/", "dein", "zeu\u00b7ge", "/", "dein", "ver\u00b7r\u00e4\u00b7ther", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "$(", "PPOSAT", "VVFIN", "$(", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hilff GOtt/ was h\u00f6rt man da! du bist ein \u00fcbelth\u00e4ter!", "tokens": ["Hilff", "Gott", "/", "was", "h\u00f6rt", "man", "da", "!", "du", "bist", "ein", "\u00fc\u00b7belt\u00b7h\u00e4\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "PWS", "VVFIN", "PIS", "ADV", "$.", "PPER", "VAFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und must aus schreckens-angst f\u00fcr GOttes urtheil fliehn.", "tokens": ["Und", "must", "aus", "schre\u00b7ckens\u00b7angst", "f\u00fcr", "Got\u00b7tes", "ur\u00b7theil", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJD", "APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da k\u00f6mmt der h\u00e4nde werck und des gem\u00fcthes sinnen", "tokens": ["Da", "k\u00f6mmt", "der", "h\u00e4n\u00b7de", "werck", "und", "des", "ge\u00b7m\u00fc\u00b7thes", "sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ans licht/ es wird entdeckt dein heimliches beginnen;", "tokens": ["Ans", "licht", "/", "es", "wird", "ent\u00b7deckt", "dein", "heim\u00b7li\u00b7ches", "be\u00b7gin\u00b7nen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$(", "PPER", "VAFIN", "VVPP", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Nun ist der f\u00fcrhang weg/ hier steht das hertze blo\u00df/", "tokens": ["Nun", "ist", "der", "f\u00fcr\u00b7hang", "weg", "/", "hier", "steht", "das", "hert\u00b7ze", "blo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "PTKVZ", "$(", "ADV", "VVFIN", "ART", "NN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Das vor so lustig war/ die schalckheit zu verhelen/", "tokens": ["Das", "vor", "so", "lus\u00b7tig", "war", "/", "die", "schalck\u00b7heit", "zu", "ver\u00b7he\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADV", "ADJD", "VAFIN", "$(", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die glieder zancken sich mit ihrem gast der seelen/", "tokens": ["Die", "glie\u00b7der", "zan\u00b7cken", "sich", "mit", "ih\u00b7rem", "gast", "der", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "PPOSAT", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die itzt bekennen mu\u00df/ wo sie will qvitt und lo\u00df", "tokens": ["Die", "itzt", "be\u00b7ken\u00b7nen", "mu\u00df", "/", "wo", "sie", "will", "qvitt", "und", "lo\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVINF", "VMFIN", "$(", "PWAV", "PPER", "VMFIN", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Von ihren s\u00fcnden seyn: was hastu nun ver\u00fcbet?", "tokens": ["Von", "ih\u00b7ren", "s\u00fcn\u00b7den", "seyn", ":", "was", "has\u00b7tu", "nun", "ver\u00b7\u00fc\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VAINF", "$.", "PWS", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Weil auch dein leben selbst dir kein gut zeugni\u00df giebet.", "tokens": ["Weil", "auch", "dein", "le\u00b7ben", "selbst", "dir", "kein", "gut", "zeug\u00b7ni\u00df", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "VVFIN", "ADV", "PPER", "PIAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ach mensch! gedencke nicht/ da\u00df was verborgen bleibt/", "tokens": ["Ach", "mensch", "!", "ge\u00b7den\u00b7cke", "nicht", "/", "da\u00df", "was", "ver\u00b7bor\u00b7gen", "bleibt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "VVFIN", "PTKNEG", "$(", "KOUS", "PIS", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ein sterblich aug und ohr ist leichtlich zubeth\u00f6ren/", "tokens": ["Ein", "sterb\u00b7lich", "aug", "und", "ohr", "ist", "leicht\u00b7lich", "zu\u00b7bet\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "KON", "NN", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der himmel schlummert nicht/ sein ohr wird immer h\u00f6ren", "tokens": ["Der", "him\u00b7mel", "schlum\u00b7mert", "nicht", "/", "sein", "ohr", "wird", "im\u00b7mer", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auff deiner reden ziel: und was sein griffel schreibt/", "tokens": ["Auff", "dei\u00b7ner", "re\u00b7den", "ziel", ":", "und", "was", "sein", "grif\u00b7fel", "schreibt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVINF", "VVFIN", "$.", "KON", "PWS", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Lesch keine zeit nicht aus. Drum m\u00fch dich so zu leben/", "tokens": ["Lesch", "kei\u00b7ne", "zeit", "nicht", "aus", ".", "Drum", "m\u00fch", "dich", "so", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PTKNEG", "PTKVZ", "$.", "PAV", "ADJD", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Als wenn du t\u00e4glich GOtt soltst deine rechnung geben.", "tokens": ["Als", "wenn", "du", "t\u00e4g\u00b7lich", "Gott", "soltst", "dei\u00b7ne", "rech\u00b7nung", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}