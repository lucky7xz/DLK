{"dta.poem.20814": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den geneigten  \n Leser.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Do sprach die Keiserinne/", "tokens": ["Do", "sprach", "die", "Kei\u00b7se\u00b7rin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir Fr\u00f6wen han k\u00fcrzen Mut:", "tokens": ["Wir", "Fr\u00f6\u00b7wen", "han", "k\u00fcr\u00b7zen", "Mut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Swas eine iez nit im Sinne/", "tokens": ["Swas", "ei\u00b7ne", "iez", "nit", "im", "Sin\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NE", "PTKNEG", "APPRART", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Gern sie es moren tut.", "tokens": ["Gern", "sie", "es", "mo\u00b7ren", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da sprach die K\u00e4yserin/", "tokens": ["Da", "sprach", "die", "K\u00e4y\u00b7se\u00b7rin", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wir Frauen haben leichte Sinnen", "tokens": ["Wir", "Frau\u00b7en", "ha\u00b7ben", "leich\u00b7te", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "NN", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wozu man heute sie nicht \u00fcberreden k\u00f6nnen/", "tokens": ["Wo\u00b7zu", "man", "heu\u00b7te", "sie", "nicht", "\u00fc\u00b7berr\u00b7e\u00b7den", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ADV", "PPER", "PTKNEG", "VVINF", "VMINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da wollen morgen sie auch ungebeten hin.", "tokens": ["Da", "wol\u00b7len", "mor\u00b7gen", "sie", "auch", "un\u00b7ge\u00b7be\u00b7ten", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}