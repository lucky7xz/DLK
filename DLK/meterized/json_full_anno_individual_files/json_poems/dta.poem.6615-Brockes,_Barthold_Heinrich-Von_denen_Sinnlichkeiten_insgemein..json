{"dta.poem.6615": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So muste denn der SCHOEPFFER aller Sachen,", "tokens": ["So", "mus\u00b7te", "denn", "der", "ScHOE\u00b7PF\u00b7FER", "al\u00b7ler", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Um alles in Vollkommenheit zu bringen,", "tokens": ["Um", "al\u00b7les", "in", "Voll\u00b7kom\u00b7men\u00b7heit", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gesch\u00f6pffe, die vern\u00fcnfftig, machen,", "tokens": ["Ge\u00b7sch\u00f6pf\u00b7fe", ",", "die", "ver\u00b7n\u00fcnff\u00b7tig", ",", "ma\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Menschen, die von ihm emfingen", "tokens": ["Und", "Men\u00b7schen", ",", "die", "von", "ihm", "em\u00b7fin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "NN", "$,", "PRELS", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So Sinnen als Verstand. Erwegen und betrachten", "tokens": ["So", "Sin\u00b7nen", "als", "Ver\u00b7stand", ".", "Er\u00b7we\u00b7gen", "und", "be\u00b7trach\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "NN", "KOUS", "NN", "$.", "NN", "KON", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist unser Theil. Dies ist der Weisen Amt. Die Welt", "tokens": ["Ist", "un\u00b7ser", "Theil", ".", "Dies", "ist", "der", "Wei\u00b7sen", "Amt", ".", "Die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist blos zu diesem Zweck uns vorgestellt,", "tokens": ["Ist", "blos", "zu", "die\u00b7sem", "Zweck", "uns", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PDAT", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df wir darauf mit Freuden achten.", "tokens": ["Da\u00df", "wir", "da\u00b7rauf", "mit", "Freu\u00b7den", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PAV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Es w\u00e4r Ver\u00e4nderung und aller Sch\u00f6nheit Pracht,", "tokens": ["Es", "w\u00e4r", "Ver\u00b7\u00e4n\u00b7de\u00b7rung", "und", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Empf\u00e4nde man sie nicht, nichts, und umsonst gemacht.", "tokens": ["E\u00b7mpf\u00e4n\u00b7de", "man", "sie", "nicht", ",", "nichts", ",", "und", "um\u00b7sonst", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PPER", "PTKNEG", "$,", "PIS", "$,", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Nur unsre Sinnlichkeit belebt sie. Durch Verstand,", "tokens": ["Nur", "uns\u00b7re", "Sinn\u00b7lich\u00b7keit", "be\u00b7lebt", "sie", ".", "Durch", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Betrachtung, und Begriff, ist sie uns blo\u00df bekannt.", "tokens": ["Be\u00b7trach\u00b7tung", ",", "und", "Be\u00b7griff", ",", "ist", "sie", "uns", "blo\u00df", "be\u00b7kannt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}