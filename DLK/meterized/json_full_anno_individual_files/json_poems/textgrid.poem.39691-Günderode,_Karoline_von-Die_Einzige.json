re": "trochaic.tetra"}, "line.9": {"text": "Eines nur mein Sinn begehret,", "tokens": ["Ei\u00b7nes", "nur", "mein", "Sinn", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Eine Sehnsucht mich verzehret;", "tokens": ["Ei\u00b7ne", "Sehn\u00b7sucht", "mich", "ver\u00b7zeh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Eng ist meine Welt befangen,", "tokens": ["Eng", "ist", "mei\u00b7ne", "Welt", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nur vom einzigen Verlangen", "tokens": ["Nur", "vom", "ein\u00b7zi\u00b7gen", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Was ich liebe zu erlangen.", "tokens": ["Was", "ich", "lie\u00b7be", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}