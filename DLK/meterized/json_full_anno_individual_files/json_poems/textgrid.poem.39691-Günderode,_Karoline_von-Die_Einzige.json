{"textgrid.poem.39691": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "Die Einzige", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ist ganz mein Sinn befangen,", "tokens": ["Wie", "ist", "ganz", "mein", "Sinn", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer, Einer anzuhangen;", "tokens": ["Ei\u00b7ner", ",", "Ei\u00b7ner", "an\u00b7zu\u00b7han\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Eine zu umpfangen", "tokens": ["Die\u00b7se", "Ei\u00b7ne", "zu", "um\u00b7pfan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treibt mich einzig nur Verlangen;", "tokens": ["Treibt", "mich", "ein\u00b7zig", "nur", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freude kann mir nur gew\u00e4hren,", "tokens": ["Freu\u00b7de", "kann", "mir", "nur", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heimlich diesen Wunsch zu n\u00e4hren,", "tokens": ["Heim\u00b7lich", "die\u00b7sen", "Wunsch", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mich in Tr\u00e4umen zu beth\u00f6ren,", "tokens": ["Mich", "in", "Tr\u00e4u\u00b7men", "zu", "be\u00b7th\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mich in Sehnen zu verzehren,", "tokens": ["Mich", "in", "Seh\u00b7nen", "zu", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Was mich t\u00f6dtet zu geb\u00e4hren.", "tokens": ["Was", "mich", "t\u00f6d\u00b7tet", "zu", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Widerstand will mir nicht frommen,", "tokens": ["Wi\u00b7der\u00b7stand", "will", "mir", "nicht", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fliehen mu\u00df ich neu zu kommen,", "tokens": ["Flie\u00b7hen", "mu\u00df", "ich", "neu", "zu", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Z\u00fcrnen nur, mich zu vers\u00f6hnen,", "tokens": ["Z\u00fcr\u00b7nen", "nur", ",", "mich", "zu", "ver\u00b7s\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann mich Ihrer nicht entw\u00f6hnen,", "tokens": ["Kann", "mich", "Ih\u00b7rer", "nicht", "ent\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df im lauten Jubel st\u00f6hnen;", "tokens": ["Mu\u00df", "im", "lau\u00b7ten", "Ju\u00b7bel", "st\u00f6h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In den Becher fallen Thr\u00e4nen,", "tokens": ["In", "den", "Be\u00b7cher", "fal\u00b7len", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich versink in tr\u00e4umrisch W\u00e4hnen;", "tokens": ["Ich", "ver\u00b7sink", "in", "tr\u00e4um\u00b7risch", "W\u00e4h\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00f6re nicht der T\u00f6ne Reigen,", "tokens": ["H\u00f6\u00b7re", "nicht", "der", "T\u00f6\u00b7ne", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie sie auf und nieder steigen,", "tokens": ["Wie", "sie", "auf", "und", "nie\u00b7der", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wogend schwellen Well' in Welle;", "tokens": ["Wo\u00b7gend", "schwel\u00b7len", "Well'", "in", "Wel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sehe nicht der Farben Helle", "tokens": ["Se\u00b7he", "nicht", "der", "Far\u00b7ben", "Hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Str\u00f6men aus des Lichtes Quelle.", "tokens": ["Str\u00f6\u00b7men", "aus", "des", "Lich\u00b7tes", "Quel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich begr\u00fc\u00dfen Fr\u00fchlingsl\u00fcfte,", "tokens": ["Mich", "be\u00b7gr\u00fc\u00b7\u00dfen", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00fcssen leise Blumend\u00fcfte,", "tokens": ["K\u00fcs\u00b7sen", "lei\u00b7se", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch das all ist mir verlohren,", "tokens": ["Doch", "das", "all", "ist", "mir", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Ist f\u00fcr mich wie nicht gebohren,", "tokens": ["Ist", "f\u00fcr", "mich", "wie", "nicht", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "KOKOM", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Denn mein Geist ist eng umpfangen", "tokens": ["Denn", "mein", "Geist", "ist", "eng", "um\u00b7pfan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Von dem einzigen Verlangen", "tokens": ["Von", "dem", "ein\u00b7zi\u00b7gen", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Eine, Eine zu erlangen.", "tokens": ["Ei\u00b7ne", ",", "Ei\u00b7ne", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ART", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hungrig in der Zahl der G\u00e4ste", "tokens": ["Hung\u00b7rig", "in", "der", "Zahl", "der", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siz ich bei dem Freudenfeste,", "tokens": ["Siz", "ich", "bei", "dem", "Freu\u00b7den\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Das Natur der Erde spendet;", "tokens": ["Das", "Na\u00b7tur", "der", "Er\u00b7de", "spen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frage heimlich ob's bald endet?", "tokens": ["Fra\u00b7ge", "heim\u00b7lich", "ob's", "bald", "en\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NE", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich aus der G\u00e4ste Reigen", "tokens": ["Ob", "ich", "aus", "der", "G\u00e4s\u00b7te", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "D\u00fcrf' dem eklen Mahl entweichen,", "tokens": ["D\u00fcr\u00b7f'", "dem", "ek\u00b7len", "Mahl", "ent\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das verschwendrisch Andre n\u00e4hret:", "tokens": ["Das", "ver\u00b7schwend\u00b7risch", "And\u00b7re", "n\u00e4h\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mir nicht einen Wunsch gew\u00e4hret?", "tokens": ["Mir", "nicht", "ei\u00b7nen", "Wunsch", "ge\u00b7w\u00e4h\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Eines nur mein Sinn begehret,", "tokens": ["Ei\u00b7nes", "nur", "mein", "Sinn", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Eine Sehnsucht mich verzehret;", "tokens": ["Ei\u00b7ne", "Sehn\u00b7sucht", "mich", "ver\u00b7zeh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Eng ist meine Welt befangen,", "tokens": ["Eng", "ist", "mei\u00b7ne", "Welt", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Nur vom einzigen Verlangen", "tokens": ["Nur", "vom", "ein\u00b7zi\u00b7gen", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.13": {"text": "Was ich liebe zu erlangen.", "tokens": ["Was", "ich", "lie\u00b7be", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie ist ganz mein Sinn befangen,", "tokens": ["Wie", "ist", "ganz", "mein", "Sinn", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer, Einer anzuhangen;", "tokens": ["Ei\u00b7ner", ",", "Ei\u00b7ner", "an\u00b7zu\u00b7han\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PIS", "$,", "PIS", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese Eine zu umpfangen", "tokens": ["Die\u00b7se", "Ei\u00b7ne", "zu", "um\u00b7pfan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "ART", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treibt mich einzig nur Verlangen;", "tokens": ["Treibt", "mich", "ein\u00b7zig", "nur", "Ver\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "AD