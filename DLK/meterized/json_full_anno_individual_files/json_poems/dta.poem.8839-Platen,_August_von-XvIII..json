{"dta.poem.8839": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Als fielen alle Sterne vom Himmel, mich zu decken:", "tokens": ["Als", "fie\u00b7len", "al\u00b7le", "Ster\u00b7ne", "vom", "Him\u00b7mel", ",", "mich", "zu", "de\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIAT", "NN", "APPRART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es reizt die Welt mein Auge durch tausend pr\u00e4cht'ge", "tokens": ["Es", "reizt", "die", "Welt", "mein", "Au\u00b7ge", "durch", "tau\u00b7send", "pr\u00e4cht'\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wo soll vor diesem Drange, wie Saul, ich mich ver-", "tokens": ["Wo", "soll", "vor", "die\u00b7sem", "Dran\u00b7ge", ",", "wie", "Saul", ",", "ich", "mich", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "APPR", "PDAT", "NN", "$,", "PWAV", "NE", "$,", "PPER", "PRF", "TRUNC"], "meter": "-+-+-+--+-++", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Des Forschens Labyrinthe! Der Kunst Gestaltenzauber!", "tokens": ["Des", "For\u00b7schens", "La\u00b7by\u00b7rin\u00b7the", "!", "Der", "Kunst", "Ge\u00b7stal\u00b7ten\u00b7zau\u00b7ber", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Der V\u00f6lker That und Sage! Der L\u00e4nder sch\u00f6ne Strecken!", "tokens": ["Der", "V\u00f6l\u00b7ker", "That", "und", "Sa\u00b7ge", "!", "Der", "L\u00e4n\u00b7der", "sch\u00f6\u00b7ne", "Stre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$.", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Auf meinem Busen lastet unendliche Begierde", "tokens": ["Auf", "mei\u00b7nem", "Bu\u00b7sen", "las\u00b7tet", "un\u00b7end\u00b7li\u00b7che", "Be\u00b7gier\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Nach jenen Sch\u00e4tzen allen, die Lieb' und Lust erwecken!", "tokens": ["Nach", "je\u00b7nen", "Sch\u00e4t\u00b7zen", "al\u00b7len", ",", "die", "Lieb'", "und", "Lust", "er\u00b7we\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PIAT", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "So w\u00e4r' ich l\u00e4ngst erlegen; doch meine Blicke sollten", "tokens": ["So", "w\u00e4r'", "ich", "l\u00e4ngst", "er\u00b7le\u00b7gen", ";", "doch", "mei\u00b7ne", "Bli\u00b7cke", "soll\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$.", "ADV", "PPOSAT", "NN", "VMFIN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "In Einen Punkt verdichtet des Sch\u00f6nen All entdecken:", "tokens": ["In", "Ei\u00b7nen", "Punkt", "ver\u00b7dich\u00b7tet", "des", "Sch\u00f6\u00b7nen", "All", "ent\u00b7de\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Seitdem du mir erschienen, entsagt' ich diesem Schweifen", "tokens": ["Seit\u00b7dem", "du", "mir", "er\u00b7schie\u00b7nen", ",", "ent\u00b7sagt'", "ich", "die\u00b7sem", "Schwei\u00b7fen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PPER", "VVINF", "$,", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Nach allen Himmelswinkeln, nach allen Erdenecken.", "tokens": ["Nach", "al\u00b7len", "Him\u00b7mels\u00b7win\u00b7keln", ",", "nach", "al\u00b7len", "Er\u00b7de\u00b7ne\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Es dampft der Quell der Jugend vom Fels im Wirbel-", "tokens": ["Es", "dampft", "der", "Quell", "der", "Ju\u00b7gend", "vom", "Fels", "im", "Wir\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPRART", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Bis friedlich ihn und silbern umf\u00e4ngt der Liebe Becken.", "tokens": ["Bis", "fried\u00b7lich", "ihn", "und", "sil\u00b7bern", "um\u00b7f\u00e4ngt", "der", "Lie\u00b7be", "Be\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "KON", "NN", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}