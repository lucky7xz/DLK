{"textgrid.poem.25280": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gew\u00f6lk hat umgebracht", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gew\u00f6lk hat umgebracht", "tokens": ["Ge\u00b7w\u00f6lk", "hat", "um\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den letzten Sternenfunken;", "tokens": ["Den", "letz\u00b7ten", "Ster\u00b7nen\u00b7fun\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In rabenschwarze Nacht", "tokens": ["In", "ra\u00b7ben\u00b7schwar\u00b7ze", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist Fels und Tann versunken.", "tokens": ["Ist", "Fels", "und", "Tann", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich bin ein Erlenstumpf,", "tokens": ["Ich", "bin", "ein", "Er\u00b7len\u00b7stumpf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dran bleicher Moder glimmert,", "tokens": ["Dran", "blei\u00b7cher", "Mo\u00b7der", "glim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein g\u00e4rend fauler Sumpf,", "tokens": ["Ein", "g\u00e4\u00b7rend", "fau\u00b7ler", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo scheu das Irrlicht flimmert.", "tokens": ["Wo", "scheu", "das", "Irr\u00b7licht", "flim\u00b7mert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Unheimlich d\u00fcstre Welt,", "tokens": ["Un\u00b7heim\u00b7lich", "d\u00fcst\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Du Tummelplatz f\u00fcr Toren!", "tokens": ["Du", "Tum\u00b7mel\u00b7platz", "f\u00fcr", "To\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bin g\u00e4nzlich unbestellt", "tokens": ["Bin", "g\u00e4nz\u00b7lich", "un\u00b7be\u00b7stellt"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In dich hineingeboren.", "tokens": ["In", "dich", "hin\u00b7ein\u00b7ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sag an, was hast du f\u00fcr", "tokens": ["Sag", "an", ",", "was", "hast", "du", "f\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "PWS", "VAFIN", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mit deinem bangen Kinde?", "tokens": ["Mit", "dei\u00b7nem", "ban\u00b7gen", "Kin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hast du keine T\u00fcr,", "tokens": ["Und", "hast", "du", "kei\u00b7ne", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo ich den Ausgang finde?", "tokens": ["Wo", "ich", "den", "Aus\u00b7gang", "fin\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Gew\u00f6lk hat umgebracht", "tokens": ["Ge\u00b7w\u00f6lk", "hat", "um\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den letzten Sternenfunken;", "tokens": ["Den", "letz\u00b7ten", "Ster\u00b7nen\u00b7fun\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In rabenschwarze Nacht", "tokens": ["In", "ra\u00b7ben\u00b7schwar\u00b7ze", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist Fels und Tann versunken.", "tokens": ["Ist", "Fels", "und", "Tann", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mein Leben sch\u00e4umend rann,", "tokens": ["Mein", "Le\u00b7ben", "sch\u00e4u\u00b7mend", "rann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sturzbach zwischen Steinen.", "tokens": ["Ein", "Sturz\u00b7bach", "zwi\u00b7schen", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich dabei gewann?", "tokens": ["Was", "ich", "da\u00b7bei", "ge\u00b7wann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O bitter m\u00f6cht ich weinen!", "tokens": ["O", "bit\u00b7ter", "m\u00f6cht", "ich", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Einst ward ich schmuck und neu", "tokens": ["Einst", "ward", "ich", "schmuck", "und", "neu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als Menschlein eingekleidet.", "tokens": ["Als", "Mensc\u00b7hlein", "ein\u00b7ge\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch alles Fleisch ist Heu,", "tokens": ["Doch", "al\u00b7les", "Fleisch", "ist", "Heu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und horch, die Sense schneidet.", "tokens": ["Und", "horch", ",", "die", "Sen\u00b7se", "schnei\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach wohl, die Jugend reicht", "tokens": ["Ach", "wohl", ",", "die", "Ju\u00b7gend", "reicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den s\u00fc\u00dfen Taumelbecher.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Tau\u00b7mel\u00b7be\u00b7cher", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch Rausch und Minne weicht,", "tokens": ["Doch", "Rausch", "und", "Min\u00b7ne", "weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und Reue weckt den Zecher.", "tokens": ["Und", "Reu\u00b7e", "weckt", "den", "Ze\u00b7cher", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Um jeden Bissen Brot", "tokens": ["Um", "je\u00b7den", "Bis\u00b7sen", "Brot"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00df hart der Froner schanzen;", "tokens": ["Mu\u00df", "hart", "der", "Fro\u00b7ner", "schan\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sonst hockt die hagre Not", "tokens": ["Sonst", "hockt", "die", "hag\u00b7re", "Not"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Auf seinem leeren Ranzen.", "tokens": ["Auf", "sei\u00b7nem", "lee\u00b7ren", "Ran\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Mach dich nicht gar zu breit,", "tokens": ["Mach", "dich", "nicht", "gar", "zu", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "PTKA", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Du Herr im g\u00fcldnen Hause!", "tokens": ["Du", "Herr", "im", "g\u00fcld\u00b7nen", "Hau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ohn End ist Ewigkeit,", "tokens": ["Ohn", "End", "ist", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und schmal die letzte Klause.", "tokens": ["Und", "schmal", "die", "letz\u00b7te", "Klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Poch nicht auf Ehr und Zier!", "tokens": ["Poch", "nicht", "auf", "Ehr", "und", "Zier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Fortuna hat's geliehen.", "tokens": ["For\u00b7tu\u00b7na", "hat's", "ge\u00b7lie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "VVPP", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Der Hobler wird auch dir", "tokens": ["Der", "Hob\u00b7ler", "wird", "auch", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Linnenkleid anziehen,", "tokens": ["Ein", "Lin\u00b7nen\u00b7kleid", "an\u00b7zie\u00b7hen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Zum Pf\u00fchle untern Kopf", "tokens": ["Zum", "Pf\u00fch\u00b7le", "un\u00b7tern", "Kopf"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zwei Handvoll Sp\u00e4hne schieben ...", "tokens": ["Zwei", "Hand\u00b7voll", "Sp\u00e4h\u00b7ne", "schie\u00b7ben", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun denke nach, du Tropf,", "tokens": ["Nun", "den\u00b7ke", "nach", ",", "du", "Tropf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wie n\u00e4rrisch du's getrieben!", "tokens": ["Wie", "n\u00e4r\u00b7risch", "du's", "ge\u00b7trie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Gew\u00f6lk hat umgebracht", "tokens": ["Ge\u00b7w\u00f6lk", "hat", "um\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den letzten Sternenfunken;", "tokens": ["Den", "letz\u00b7ten", "Ster\u00b7nen\u00b7fun\u00b7ken", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In rabenschwarze Nacht", "tokens": ["In", "ra\u00b7ben\u00b7schwar\u00b7ze", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ist Fels und Tann versunken.", "tokens": ["Ist", "Fels", "und", "Tann", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Und wie ich ratlos bang", "tokens": ["Und", "wie", "ich", "rat\u00b7los", "bang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ins dunkle R\u00e4tsel staune,", "tokens": ["Ins", "dunk\u00b7le", "R\u00e4t\u00b7sel", "stau\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Horch, sanfter Wiegensang,", "tokens": ["Horch", ",", "sanf\u00b7ter", "Wie\u00b7gen\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein wogend Waldgeraune:", "tokens": ["Ein", "wo\u00b7gend", "Wald\u00b7ge\u00b7rau\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "\u00bbnur stille, Menschenkind!", "tokens": ["\u00bb", "nur", "stil\u00b7le", ",", "Men\u00b7schen\u00b7kind", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Was helfen deine Sorgen?", "tokens": ["Was", "hel\u00b7fen", "dei\u00b7ne", "Sor\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Augen schlie\u00dfe lind!", "tokens": ["Die", "Au\u00b7gen", "schlie\u00b7\u00dfe", "lind", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Derweilen w\u00e4chst das Morgen.\u00ab", "tokens": ["Der\u00b7wei\u00b7len", "w\u00e4chst", "das", "Mor\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Die Nacht hat ihren Tau,", "tokens": ["Die", "Nacht", "hat", "ih\u00b7ren", "Tau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Auf da\u00df der Maien bl\u00fche,", "tokens": ["Auf", "da\u00df", "der", "Mai\u00b7en", "bl\u00fc\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und aus dem Wolkengrau", "tokens": ["Und", "aus", "dem", "Wol\u00b7ken\u00b7grau"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Entsprie\u00dft die Purpurfr\u00fche.", "tokens": ["Ent\u00b7sprie\u00dft", "die", "Pur\u00b7pur\u00b7fr\u00fc\u00b7he", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Soll nicht der Sagenstein,", "tokens": ["Soll", "nicht", "der", "Sa\u00b7gen\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.