, "tokens": ["Ist", "Fels", "und", "Tann", "ver\u00b7sun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.25": {"line.1": {"text": "Mein Leben sch\u00e4umend rann,", "tokens": ["Mein", "Le\u00b7ben", "sch\u00e4u\u00b7mend", "rann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sturzbach zwischen Steinen.", "tokens": ["Ein", "Sturz\u00b7bach", "zwi\u00b7schen", "Stei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was ich dabei gewann?", "tokens": ["Was", "ich", "da\u00b7bei", "ge\u00b7wann", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PAV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "O bitter m\u00f6cht ich weinen!", "tokens": ["O", "bit\u00b7ter", "m\u00f6cht", "ich", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Einst ward ich schmuck und neu", "tokens": ["Einst", "ward", "ich", "schmuck", "und", "neu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Als Menschlein eingekleidet.", "tokens": ["Als", "Mensc\u00b7hlein", "ein\u00b7ge\u00b7klei\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch alles Fleisch ist Heu,", "tokens": ["Doch", "al\u00b7les", "Fleisc