{"textgrid.poem.36367": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der Ehre", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auch alle V\u00f6lker wanken,", "tokens": ["Wenn", "auch", "al\u00b7le", "V\u00f6l\u00b7ker", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh' die Erde ganz verl\u00e4\u00dft,", "tokens": ["Ruh'", "die", "Er\u00b7de", "ganz", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Rechte brechend schwanken,", "tokens": ["Al\u00b7le", "Rech\u00b7te", "bre\u00b7chend", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht die Ehre dennoch fest;", "tokens": ["Steht", "die", "Eh\u00b7re", "den\u00b7noch", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewig, wie der Nordstern milde", "tokens": ["E\u00b7wig", ",", "wie", "der", "Nords\u00b7tern", "mil\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Strahlet durch der Nacht Gefilde.", "tokens": ["Strah\u00b7let", "durch", "der", "Nacht", "Ge\u00b7fil\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heil dem Mann, der darnach handelt,", "tokens": ["Heil", "dem", "Mann", ",", "der", "dar\u00b7nach", "han\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PAV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diesen Stern im Auge h\u00e4lt,", "tokens": ["Die\u00b7sen", "Stern", "im", "Au\u00b7ge", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Stern der Ehre, der nie wandelt,", "tokens": ["Stern", "der", "Eh\u00b7re", ",", "der", "nie", "wan\u00b7delt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Fiel' in Tr\u00fcmmern auch die Welt!", "tokens": ["Fiel'", "in", "Tr\u00fcm\u00b7mern", "auch", "die", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Tode noch wird gr\u00fcnen", "tokens": ["Aus", "dem", "To\u00b7de", "noch", "wird", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hohe Siegeslust dem K\u00fchnen.", "tokens": ["Ho\u00b7he", "Sie\u00b7ges\u00b7lust", "dem", "K\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Denn es siegt ja doch die Ehre", "tokens": ["Denn", "es", "siegt", "ja", "doch", "die", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei dem edleren Geschlecht,", "tokens": ["Bei", "dem", "ed\u00b7le\u00b7ren", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie das blinde Gl\u00fcck auch mehre", "tokens": ["Wie", "das", "blin\u00b7de", "Gl\u00fcck", "auch", "meh\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ADV", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Siege sonder Ehr' und Recht.", "tokens": ["Sie\u00b7ge", "son\u00b7der", "Ehr'", "und", "Recht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ewig gl\u00e4nzt der Tugend Adel,", "tokens": ["E\u00b7wig", "gl\u00e4nzt", "der", "Tu\u00b7gend", "A\u00b7del", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Falscher Ruhm ist mehr nur Tadel.", "tokens": ["Fal\u00b7scher", "Ruhm", "ist", "mehr", "nur", "Ta\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "ADV", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum sei jener hochgepriesen,", "tokens": ["Drum", "sei", "je\u00b7ner", "hoch\u00b7ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "PDAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kaiser Er mit Recht genannt,", "tokens": ["Kai\u00b7ser", "Er", "mit", "Recht", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der des Gl\u00fcckes m\u00e4cht'gem Riesen", "tokens": ["Der", "des", "Gl\u00fc\u00b7ckes", "m\u00e4cht'\u00b7gem", "Rie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mutig leistet Widerstand,", "tokens": ["Mu\u00b7tig", "leis\u00b7tet", "Wi\u00b7der\u00b7stand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "An der Ehre Kraft noch glaubend,", "tokens": ["An", "der", "Eh\u00b7re", "Kraft", "noch", "glau\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die Zeit der Schmach entraubend.", "tokens": ["Und", "die", "Zeit", "der", "Schmach", "en\u00b7trau\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohl vertraut den m\u00e4cht'gen Ahnen", "tokens": ["Wohl", "ver\u00b7traut", "den", "m\u00e4cht'\u00b7gen", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er auf seinem V\u00f6lkerthron,", "tokens": ["Er", "auf", "sei\u00b7nem", "V\u00f6l\u00b7ker\u00b7thron", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An den Ruhm der Zukunft mahnen", "tokens": ["An", "den", "Ruhm", "der", "Zu\u00b7kunft", "mah\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihn, des Nordens hohen Sohn,", "tokens": ["Ihn", ",", "des", "Nor\u00b7dens", "ho\u00b7hen", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zeichen, strahlend durch die Zeiten,", "tokens": ["Zei\u00b7chen", ",", "strah\u00b7lend", "durch", "die", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Neu die Welt uns zu bereiten.", "tokens": ["Neu", "die", "Welt", "uns", "zu", "be\u00b7rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Leuchtend ob dem Eisgefilde", "tokens": ["Leuch\u00b7tend", "ob", "dem", "Eis\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wogen Feuer durch die Nacht,", "tokens": ["Wo\u00b7gen", "Feu\u00b7er", "durch", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "S\u00fchnend wird im Flammenbilde", "tokens": ["S\u00fch\u00b7nend", "wird", "im", "Flam\u00b7men\u00b7bil\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hier das Opfer dargebracht;", "tokens": ["Hier", "das", "Op\u00b7fer", "dar\u00b7ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "V\u00f6lker fluten im Gewimmel,", "tokens": ["V\u00f6l\u00b7ker", "flu\u00b7ten", "im", "Ge\u00b7wim\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4mpfend jauchzen sie zum Himmel.", "tokens": ["K\u00e4mp\u00b7fend", "jauch\u00b7zen", "sie", "zum", "Him\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00f6chte neu ein Reich zu gr\u00fcnden", "tokens": ["M\u00f6ch\u00b7te", "neu", "ein", "Reich", "zu", "gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADJD", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der Ehre festem Grund,", "tokens": ["Auf", "der", "Eh\u00b7re", "fes\u00b7tem", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Heldenherzen zu entz\u00fcnden,", "tokens": ["Hel\u00b7den\u00b7her\u00b7zen", "zu", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wieder Eins im alten Bund,", "tokens": ["Wie\u00b7der", "Eins", "im", "al\u00b7ten", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hoch als Sieger Ihm gelingen,", "tokens": ["Hoch", "als", "Sie\u00b7ger", "Ihm", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Alle bald den Retter singen.", "tokens": ["Al\u00b7le", "bald", "den", "Ret\u00b7ter", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Sind der Streiche, die uns trafen,", "tokens": ["Sind", "der", "Strei\u00b7che", ",", "die", "uns", "tra\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der Schmach noch nicht genug,", "tokens": ["Ist", "der", "Schmach", "noch", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll durch Gott uns h\u00e4rter strafen", "tokens": ["Soll", "durch", "Gott", "uns", "h\u00e4r\u00b7ter", "stra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "NN", "PPER", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch die Gei\u00dfel, die uns schlug;", "tokens": ["Noch", "die", "Gei\u00b7\u00dfel", ",", "die", "uns", "schlug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dennoch zu den fernsten Zeiten", "tokens": ["Den\u00b7noch", "zu", "den", "ferns\u00b7ten", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wirst du sch\u00f6nen Glanz verbreiten;", "tokens": ["Wirst", "du", "sch\u00f6\u00b7nen", "Glanz", "ver\u00b7brei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Lichter Stern, der uns erschienen,", "tokens": ["Lich\u00b7ter", "Stern", ",", "der", "uns", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stern der Ehr' in tr\u00fcber Nacht,", "tokens": ["Stern", "der", "Ehr'", "in", "tr\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der den Treuen, die ihr dienen,", "tokens": ["Der", "den", "Treu\u00b7en", ",", "die", "ihr", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoffnung wieder angefacht;", "tokens": ["Hoff\u00b7nung", "wie\u00b7der", "an\u00b7ge\u00b7facht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Stern der Ehr' aus jenem Norden,", "tokens": ["Stern", "der", "Ehr'", "aus", "je\u00b7nem", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch den frei die Erd' einst worden!", "tokens": ["Durch", "den", "frei", "die", "Erd'", "einst", "wor\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ART", "NN", "VVPP", "VAPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wenn auch alle V\u00f6lker wanken,", "tokens": ["Wenn", "auch", "al\u00b7le", "V\u00f6l\u00b7ker", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruh' die Erde ganz verl\u00e4\u00dft,", "tokens": ["Ruh'", "die", "Er\u00b7de", "ganz", "ver\u00b7l\u00e4\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Rechte brechend schwanken,", "tokens": ["Al\u00b7le", "Rech\u00b7te", "bre\u00b7chend", "schwan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steht die Ehre dennoch fest;", "tokens": ["Steht", "die", "Eh\u00b7re", "den\u00b7noch", "fest", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": 