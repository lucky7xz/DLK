{"dta.poem.1568": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Herrn Han\u0303s Friderichs von Schweinitz/ Chur  \n F\u00fcrstl. S\u00e4ch\u00df. Obrist-Lieutenants.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was nuzt die Tapfferkeit/ was hilfft der freye Mutt/ ", "tokens": ["Was", "nuzt", "die", "Tapf\u00b7fer\u00b7keit", "/", "was", "hilfft", "der", "frey\u00b7e", "Mutt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun Schwartzens schwartze Kunst mit Rauch und Re-", "tokens": ["Nun", "Schwart\u00b7zens", "schwart\u00b7ze", "Kunst", "mit", "Rauch", "und", "Re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "ADJA", "NN", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach Meuchelm\u00f6rders Art auff frische Helden zielt/", "tokens": ["Nach", "Meu\u00b7chel\u00b7m\u00f6r\u00b7ders", "Art", "auff", "fri\u00b7sche", "Hel\u00b7den", "zielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und offt ein feiger Knecht erlegt das k\u00fchnste Blutt?", "tokens": ["Und", "offt", "ein", "fei\u00b7ger", "Knecht", "er\u00b7legt", "das", "k\u00fchns\u00b7te", "Blutt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nun unter Wolck und Dampff der B\u00fcchsen und Granaten", "tokens": ["Nun", "un\u00b7ter", "Wolck", "und", "Dampff", "der", "B\u00fcch\u00b7sen", "und", "Gra\u00b7na\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verborgen m\u00fcssen seyn die besten Helden-Thaten?", "tokens": ["Ver\u00b7bor\u00b7gen", "m\u00fcs\u00b7sen", "seyn", "die", "bes\u00b7ten", "Hel\u00b7den\u00b7Tha\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich/ eines Helden Sohn/ hab als ein Held gelebt;", "tokens": ["Ich", "/", "ei\u00b7nes", "Hel\u00b7den", "Sohn", "/", "hab", "als", "ein", "Held", "ge\u00b7lebt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ART", "NN", "NN", "$(", "VAFIN", "KOKOM", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Kriegs-Spiel war mein Spiel von zarter Jugend an:", "tokens": ["Das", "Kriegs\u00b7Spiel", "war", "mein", "Spiel", "von", "zar\u00b7ter", "Ju\u00b7gend", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich lieff mit Freudigkeit die strenge Ritter-Bahn/", "tokens": ["Ich", "lieff", "mit", "Freu\u00b7dig\u00b7keit", "die", "stren\u00b7ge", "Rit\u00b7ter\u00b7Bahn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Auff der des Adels Fu\u00df nach wahrer Ehre strebt.", "tokens": ["Auff", "der", "des", "A\u00b7dels", "Fu\u00df", "nach", "wah\u00b7rer", "Eh\u00b7re", "strebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein Feld-Herr hat erkennt/ mein Feind hat selbst gepriesen", "tokens": ["Mein", "Feld\u00b7Herr", "hat", "er\u00b7kennt", "/", "mein", "Feind", "hat", "selbst", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie ich in Stadt und Feld mich unverzagt erwiesen.", "tokens": ["Wie", "ich", "in", "Stadt", "und", "Feld", "mich", "un\u00b7ver\u00b7zagt", "er\u00b7wie\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vors Vaterland hab ich mein Leben auffgesezt:", "tokens": ["Vors", "Va\u00b7ter\u00b7land", "hab", "ich", "mein", "Le\u00b7ben", "auff\u00b7ge\u00b7sezt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In meinen Diensten ward noch Schwei\u00df noch Blutt geschont/", "tokens": ["In", "mei\u00b7nen", "Diens\u00b7ten", "ward", "noch", "Schwei\u00df", "noch", "Blutt", "ge\u00b7schont", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ADV", "NN", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bi\u00df mir ein fr\u00fcher Todt/ wie andern Helden/ lohnt/", "tokens": ["Bi\u00df", "mir", "ein", "fr\u00fc\u00b7her", "Todt", "/", "wie", "an\u00b7dern", "Hel\u00b7den", "/", "lohnt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJA", "NN", "$(", "KOKOM", "ADJA", "NN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und mein bekr\u00f6ntes Haubt ein harter Schu\u00df verlezt/", "tokens": ["Und", "mein", "be\u00b7kr\u00f6n\u00b7tes", "Haubt", "ein", "har\u00b7ter", "Schu\u00df", "ver\u00b7lezt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch must ich noch den Sieg mit eigner Faust erlangen/", "tokens": ["Doch", "must", "ich", "noch", "den", "Sieg", "mit", "eig\u00b7ner", "Faust", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mein Feind ist vor im Tod/ ins Leben ich gegangen.", "tokens": ["Mein", "Feind", "ist", "vor", "im", "Tod", "/", "ins", "Le\u00b7ben", "ich", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "APPRART", "NN", "$(", "APPRART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Streut Palmen um mich her und nicht Cypressen aus!", "tokens": ["Streut", "Pal\u00b7men", "um", "mich", "her", "und", "nicht", "Cyp\u00b7res\u00b7sen", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPER", "PTKVZ", "KON", "PTKNEG", "NN", "PTKVZ", "$."], "meter": "-+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Mein Bruder starb von Hitz/ ich durch erhiztes Bley:", "tokens": ["Mein", "Bru\u00b7der", "starb", "von", "Hitz", "/", "ich", "durch", "er\u00b7hiz\u00b7tes", "Bley", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$(", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gleich ist der Ruhm und Tod/ die Art nicht einerley.", "tokens": ["Gleich", "ist", "der", "Ruhm", "und", "Tod", "/", "die", "Art", "nicht", "ei\u00b7ner\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "KON", "NN", "$(", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Tugend/ nicht die Zahl/ erh\u00e4lt/ erh\u00f6ht das Hau\u00df.", "tokens": ["Die", "Tu\u00b7gend", "/", "nicht", "die", "Zahl", "/", "er\u00b7h\u00e4lt", "/", "er\u00b7h\u00f6ht", "das", "Hau\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PTKNEG", "ART", "NN", "$(", "VVFIN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wer dentsch Gebl\u00fctte f\u00fchrt wird mich an Frantzen r\u00e4chen/", "tokens": ["Wer", "dentsch", "Ge\u00b7bl\u00fct\u00b7te", "f\u00fchrt", "wird", "mich", "an", "Frant\u00b7zen", "r\u00e4\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Lorbern auff mein Grab in ihren G\u00e4rten brechen.", "tokens": ["Und", "Lor\u00b7bern", "auff", "mein", "Grab", "in", "ih\u00b7ren", "G\u00e4r\u00b7ten", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}