{"textgrid.poem.55121": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Johanna Sebus", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich trage dich, Mutter, durch die Flut,", "tokens": ["\u00bb", "ich", "tra\u00b7ge", "dich", ",", "Mut\u00b7ter", ",", "durch", "die", "Flut", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch reicht sie nicht hoch, ich wate gut.\u00ab \u2013", "tokens": ["Noch", "reicht", "sie", "nicht", "hoch", ",", "ich", "wa\u00b7te", "gut", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbauch uns bedenke, bedr\u00e4ngt wie wir sind,", "tokens": ["\u00bb", "auch", "uns", "be\u00b7den\u00b7ke", ",", "be\u00b7dr\u00e4ngt", "wie", "wir", "sind", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Hausgenossin, drei arme Kind'!", "tokens": ["Die", "Haus\u00b7ge\u00b7nos\u00b7sin", ",", "drei", "ar\u00b7me", "Kind'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "ADJA", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.5": {"text": "Die schwache Frau!... Du gehst davon!\u00ab \u2013", "tokens": ["Die", "schwa\u00b7che", "Frau", "!", "...", "Du", "gehst", "da\u00b7von", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "PAV", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie tr\u00e4gt die Mutter durchs Wasser schon.", "tokens": ["Sie", "tr\u00e4gt", "die", "Mut\u00b7ter", "durchs", "Was\u00b7ser", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u00bbzum B\u00fchle da rettet euch! harret derweil;", "tokens": ["\u00bb", "zum", "B\u00fch\u00b7le", "da", "ret\u00b7tet", "euch", "!", "har\u00b7ret", "der\u00b7weil", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ADV", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Gleich kehr ich zur\u00fcck, uns allen ist Heil.", "tokens": ["Gleich", "kehr", "ich", "zu\u00b7r\u00fcck", ",", "uns", "al\u00b7len", "ist", "Heil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PIS", "VAFIN", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Zum B\u00fchl ist's noch trocken und wenige Schritt';", "tokens": ["Zum", "B\u00fchl", "ist's", "noch", "tro\u00b7cken", "und", "we\u00b7ni\u00b7ge", "Schritt'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADJD", "KON", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Doch nehmt auch mir meine Ziege mit!\u00ab", "tokens": ["Doch", "nehmt", "auch", "mir", "mei\u00b7ne", "Zie\u00b7ge", "mit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Sie setzt die Mutter auf sichres Land,", "tokens": ["Sie", "setzt", "die", "Mut\u00b7ter", "auf", "sich\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sch\u00f6n Suschen, gleich wieder zur Flut gewandt.", "tokens": ["Sch\u00f6n", "Su\u00b7schen", ",", "gleich", "wie\u00b7der", "zur", "Flut", "ge\u00b7wandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "\u00bbwohin? Wohin? Die Breite schwoll,", "tokens": ["\u00bb", "wo\u00b7hin", "?", "Wo\u00b7hin", "?", "Die", "Brei\u00b7te", "schwoll", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PWAV", "$.", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wassers ist h\u00fcben und dr\u00fcben voll.", "tokens": ["Des", "Was\u00b7sers", "ist", "h\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verwegen ins Tiefe willst du hinein !\u00ab \u2013", "tokens": ["Ver\u00b7we\u00b7gen", "ins", "Tie\u00b7fe", "willst", "du", "hin\u00b7ein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "VMFIN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Sch\u00f6n Suschen schreitet gewohnten Steg,", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "schrei\u00b7tet", "ge\u00b7wohn\u00b7ten", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Umstr\u00f6mt auch gleitet sie nicht vom Weg,", "tokens": ["Um\u00b7str\u00f6mt", "auch", "glei\u00b7tet", "sie", "nicht", "vom", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erreicht den B\u00fchl und die Nachbarin;", "tokens": ["Er\u00b7reicht", "den", "B\u00fchl", "und", "die", "Nach\u00b7ba\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch der und den Kindern kein Gewinn!", "tokens": ["Doch", "der", "und", "den", "Kin\u00b7dern", "kein", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "Da g\u00e4hnet und wirbelt der sch\u00e4umende Schlund", "tokens": ["Da", "g\u00e4h\u00b7net", "und", "wir\u00b7belt", "der", "sch\u00e4u\u00b7men\u00b7de", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und ziehet die Frau mit den Kindern zu Grund;", "tokens": ["Und", "zie\u00b7het", "die", "Frau", "mit", "den", "Kin\u00b7dern", "zu", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das Horn der Ziege fa\u00dft das ein',", "tokens": ["Das", "Horn", "der", "Zie\u00b7ge", "fa\u00dft", "das", "ein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sollten sie alle verloren sein!", "tokens": ["So", "soll\u00b7ten", "sie", "al\u00b7le", "ver\u00b7lo\u00b7ren", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sch\u00f6n Suschen steht noch strack und gut:", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "steht", "noch", "strack", "und", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer rettet das junge, das edelste Blut!", "tokens": ["Wer", "ret\u00b7tet", "das", "jun\u00b7ge", ",", "das", "e\u00b7dels\u00b7te", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Sch\u00f6n Suschen steht noch wie ein Stern;", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "steht", "noch", "wie", "ein", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch alle Werber sind alle fern.", "tokens": ["Doch", "al\u00b7le", "Wer\u00b7ber", "sind", "al\u00b7le", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Rings um sie her ist Wasserbahn,", "tokens": ["Rings", "um", "sie", "her", "ist", "Was\u00b7ser\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kein Schifflein schwimmet zu ihr heran.", "tokens": ["Kein", "Schif\u00b7flein", "schwim\u00b7met", "zu", "ihr", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Noch einmal blickt sie zum Himmel hinauf,", "tokens": ["Noch", "ein\u00b7mal", "blickt", "sie", "zum", "Him\u00b7mel", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Da nehmen die schmeichelnden Fluten sie auf.", "tokens": ["Da", "neh\u00b7men", "die", "schmei\u00b7cheln\u00b7den", "Flu\u00b7ten", "sie", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.6": {"line.1": {"text": "Bedeckt ist alles mit Wasserschwall;", "tokens": ["Be\u00b7deckt", "ist", "al\u00b7les", "mit", "Was\u00b7ser\u00b7schwall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch Suschens Bild schwebt \u00fcberall. \u2013", "tokens": ["Doch", "Su\u00b7schens", "Bild", "schwebt", "\u00fc\u00b7be\u00b7rall", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wasser sinkt, das Land erscheint,", "tokens": ["Das", "Was\u00b7ser", "sinkt", ",", "das", "Land", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcberall wird sch\u00f6n Suschen beweint. \u2013", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "wird", "sch\u00f6n", "Su\u00b7schen", "be\u00b7weint", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "NN", "VVPP", "$.", "$("], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Und dem sei, wer's nicht singt und sagt,", "tokens": ["Und", "dem", "sei", ",", "wer's", "nicht", "singt", "und", "sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "VAFIN", "PTKNEG", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Im Leben und Tod nicht nachgefragt!", "tokens": ["Im", "Le\u00b7ben", "und", "Tod", "nicht", "nach\u00b7ge\u00b7fragt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbich trage dich, Mutter, durch die Flut,", "tokens": ["\u00bb", "ich", "tra\u00b7ge", "dich", ",", "Mut\u00b7ter", ",", "durch", "die", "Flut", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Noch reicht sie nicht hoch, ich wate gut.\u00ab \u2013", "tokens": ["Noch", "reicht", "sie", "nicht", "hoch", ",", "ich", "wa\u00b7te", "gut", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADJD", "$,", "PPER", "VVFIN", "ADJD", "$.", "$(", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbauch uns bedenke, bedr\u00e4ngt wie wir sind,", "tokens": ["\u00bb", "auch", "uns", "be\u00b7den\u00b7ke", ",", "be\u00b7dr\u00e4ngt", "wie", "wir", "sind", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PPER", "VVFIN", "$,", "VVFIN", "KOKOM", "PPER", "VAFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Hausgenossin, drei arme Kind'!", "tokens": ["Die", "Haus\u00b7ge\u00b7nos\u00b7sin", ",", "drei", "ar\u00b7me", "Kind'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "CARD", "ADJA", "NN", "$."], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.5": {"text": "Die schwache Frau!... Du gehst davon!\u00ab \u2013", "tokens": ["Die", "schwa\u00b7che", "Frau", "!", "...", "Du", "gehst", "da\u00b7von", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "PAV", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie tr\u00e4gt die Mutter durchs Wasser schon.", "tokens": ["Sie", "tr\u00e4gt", "die", "Mut\u00b7ter", "durchs", "Was\u00b7ser", "schon", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u00bbzum B\u00fchle da rettet euch! harret derweil;", "tokens": ["\u00bb", "zum", "B\u00fch\u00b7le", "da", "ret\u00b7tet", "euch", "!", "har\u00b7ret", "der\u00b7weil", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ADV", "VVFIN", "PPER", "$.", "VVFIN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.8": {"text": "Gleich kehr ich zur\u00fcck, uns allen ist Heil.", "tokens": ["Gleich", "kehr", "ich", "zu\u00b7r\u00fcck", ",", "uns", "al\u00b7len", "ist", "Heil", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "PIS", "VAFIN", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Zum B\u00fchl ist's noch trocken und wenige Schritt';", "tokens": ["Zum", "B\u00fchl", "ist's", "noch", "tro\u00b7cken", "und", "we\u00b7ni\u00b7ge", "Schritt'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ADV", "ADJD", "KON", "PIAT", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.10": {"text": "Doch nehmt auch mir meine Ziege mit!\u00ab", "tokens": ["Doch", "nehmt", "auch", "mir", "mei\u00b7ne", "Zie\u00b7ge", "mit", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Sie setzt die Mutter auf sichres Land,", "tokens": ["Sie", "setzt", "die", "Mut\u00b7ter", "auf", "sich\u00b7res", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Sch\u00f6n Suschen, gleich wieder zur Flut gewandt.", "tokens": ["Sch\u00f6n", "Su\u00b7schen", ",", "gleich", "wie\u00b7der", "zur", "Flut", "ge\u00b7wandt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbwohin? Wohin? Die Breite schwoll,", "tokens": ["\u00bb", "wo\u00b7hin", "?", "Wo\u00b7hin", "?", "Die", "Brei\u00b7te", "schwoll", ","], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "$.", "PWAV", "$.", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Wassers ist h\u00fcben und dr\u00fcben voll.", "tokens": ["Des", "Was\u00b7sers", "ist", "h\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Verwegen ins Tiefe willst du hinein !\u00ab \u2013", "tokens": ["Ver\u00b7we\u00b7gen", "ins", "Tie\u00b7fe", "willst", "du", "hin\u00b7ein", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "APPRART", "NN", "VMFIN", "PPER", "PTKVZ", "$.", "$(", "$("], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Sch\u00f6n Suschen schreitet gewohnten Steg,", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "schrei\u00b7tet", "ge\u00b7wohn\u00b7ten", "Steg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Umstr\u00f6mt auch gleitet sie nicht vom Weg,", "tokens": ["Um\u00b7str\u00f6mt", "auch", "glei\u00b7tet", "sie", "nicht", "vom", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Erreicht den B\u00fchl und die Nachbarin;", "tokens": ["Er\u00b7reicht", "den", "B\u00fchl", "und", "die", "Nach\u00b7ba\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch der und den Kindern kein Gewinn!", "tokens": ["Doch", "der", "und", "den", "Kin\u00b7dern", "kein", "Ge\u00b7winn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "KON", "ART", "NN", "PIAT", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.11": {"line.1": {"text": "Da g\u00e4hnet und wirbelt der sch\u00e4umende Schlund", "tokens": ["Da", "g\u00e4h\u00b7net", "und", "wir\u00b7belt", "der", "sch\u00e4u\u00b7men\u00b7de", "Schlund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+---+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und ziehet die Frau mit den Kindern zu Grund;", "tokens": ["Und", "zie\u00b7het", "die", "Frau", "mit", "den", "Kin\u00b7dern", "zu", "Grund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "APPR", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Das Horn der Ziege fa\u00dft das ein',", "tokens": ["Das", "Horn", "der", "Zie\u00b7ge", "fa\u00dft", "das", "ein'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sollten sie alle verloren sein!", "tokens": ["So", "soll\u00b7ten", "sie", "al\u00b7le", "ver\u00b7lo\u00b7ren", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVPP", "VAINF", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Sch\u00f6n Suschen steht noch strack und gut:", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "steht", "noch", "strack", "und", "gut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer rettet das junge, das edelste Blut!", "tokens": ["Wer", "ret\u00b7tet", "das", "jun\u00b7ge", ",", "das", "e\u00b7dels\u00b7te", "Blut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "Sch\u00f6n Suschen steht noch wie ein Stern;", "tokens": ["Sch\u00f6n", "Su\u00b7schen", "steht", "noch", "wie", "ein", "Stern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "ADV", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch alle Werber sind alle fern.", "tokens": ["Doch", "al\u00b7le", "Wer\u00b7ber", "sind", "al\u00b7le", "fern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PIS", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Rings um sie her ist Wasserbahn,", "tokens": ["Rings", "um", "sie", "her", "ist", "Was\u00b7ser\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Kein Schifflein schwimmet zu ihr heran.", "tokens": ["Kein", "Schif\u00b7flein", "schwim\u00b7met", "zu", "ihr", "he\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Noch einmal blickt sie zum Himmel hinauf,", "tokens": ["Noch", "ein\u00b7mal", "blickt", "sie", "zum", "Him\u00b7mel", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Da nehmen die schmeichelnden Fluten sie auf.", "tokens": ["Da", "neh\u00b7men", "die", "schmei\u00b7cheln\u00b7den", "Flu\u00b7ten", "sie", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.12": {"line.1": {"text": "Bedeckt ist alles mit Wasserschwall;", "tokens": ["Be\u00b7deckt", "ist", "al\u00b7les", "mit", "Was\u00b7ser\u00b7schwall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PIS", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch Suschens Bild schwebt \u00fcberall. \u2013", "tokens": ["Doch", "Su\u00b7schens", "Bild", "schwebt", "\u00fc\u00b7be\u00b7rall", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Wasser sinkt, das Land erscheint,", "tokens": ["Das", "Was\u00b7ser", "sinkt", ",", "das", "Land", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcberall wird sch\u00f6n Suschen beweint. \u2013", "tokens": ["Und", "\u00fc\u00b7be\u00b7rall", "wird", "sch\u00f6n", "Su\u00b7schen", "be\u00b7weint", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADJD", "NN", "VVPP", "$.", "$("], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Und dem sei, wer's nicht singt und sagt,", "tokens": ["Und", "dem", "sei", ",", "wer's", "nicht", "singt", "und", "sagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "$,", "VAFIN", "PTKNEG", "VVFIN", "KON", "VVFIN", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Im Leben und Tod nicht nachgefragt!", "tokens": ["Im", "Le\u00b7ben", "und", "Tod", "nicht", "nach\u00b7ge\u