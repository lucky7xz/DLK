{"textgrid.poem.62767": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zween F\u00fcchse brachen einem P\u00e4chter", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zween F\u00fcchse brachen einem P\u00e4chter", "tokens": ["Zween", "F\u00fcch\u00b7se", "bra\u00b7chen", "ei\u00b7nem", "P\u00e4ch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Nachtzeit in sein H\u00fchnerhaus,", "tokens": ["Zur", "Nacht\u00b7zeit", "in", "sein", "H\u00fch\u00b7ner\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und \u00fcbten Mord und Todtschlag aus.", "tokens": ["Und", "\u00fcb\u00b7ten", "Mord", "und", "Todt\u00b7schlag", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein stolzer Hahn, des Hofes W\u00e4chter,", "tokens": ["Ein", "stol\u00b7zer", "Hahn", ",", "des", "Ho\u00b7fes", "W\u00e4ch\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Hennen Abgott, fiel im Straus", "tokens": ["Der", "Hen\u00b7nen", "Ab\u00b7gott", ",", "fiel", "im", "Straus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Held f\u00fcr seine Sultaninnen,", "tokens": ["Als", "Held", "f\u00fcr", "sei\u00b7ne", "Sul\u00b7ta\u00b7nin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und kurz, es konnte nichts entrinnen,", "tokens": ["Und", "kurz", ",", "es", "konn\u00b7te", "nichts", "ent\u00b7rin\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PPER", "VMFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was Federn auf dem Leibe trug.", "tokens": ["Was", "Fe\u00b7dern", "auf", "dem", "Lei\u00b7be", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Dieben frommt kein langes Weilen,", "tokens": ["Den", "Die\u00b7ben", "frommt", "kein", "lan\u00b7ges", "Wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Sie sagten: la\u00dft den Raub uns theilen,", "tokens": ["Sie", "sag\u00b7ten", ":", "la\u00dft", "den", "Raub", "uns", "thei\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Raps war schon alt und folglich klug.", "tokens": ["Raps", "war", "schon", "alt", "und", "folg\u00b7lich", "klug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Er sprach zum j\u00fcngern Spie\u00dfgesellen:", "tokens": ["Er", "sprach", "zum", "j\u00fcn\u00b7gern", "Spie\u00df\u00b7ge\u00b7sel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Mein Sohn, ich wei\u00df aus tausend F\u00e4llen,", "tokens": ["Mein", "Sohn", ",", "ich", "wei\u00df", "aus", "tau\u00b7send", "F\u00e4l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VVFIN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wie n\u00f6thig man zu sparen hat;", "tokens": ["Wie", "n\u00f6\u00b7thig", "man", "zu", "spa\u00b7ren", "hat", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ist heut mein Magen noch so satt,", "tokens": ["Ist", "heut", "mein", "Ma\u00b7gen", "noch", "so", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So will er morgen doch was haben.", "tokens": ["So", "will", "er", "mor\u00b7gen", "doch", "was", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "PWS", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Auch ist der Winter vor der Th\u00fcr,", "tokens": ["Auch", "ist", "der", "Win\u00b7ter", "vor", "der", "Th\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Darum, Herr Neffe, glaube mir,", "tokens": ["Da\u00b7rum", ",", "Herr", "Nef\u00b7fe", ",", "glau\u00b7be", "mir", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "$,", "NN", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "La\u00df uns den reichen Schatz vergraben,", "tokens": ["La\u00df", "uns", "den", "rei\u00b7chen", "Schatz", "ver\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Um lang uns noch damit zu laben.", "tokens": ["Um", "lang", "uns", "noch", "da\u00b7mit", "zu", "la\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJD", "PPER", "ADV", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Der junge Rips, ein lockrer Wicht,", "tokens": ["Der", "jun\u00b7ge", "Rips", ",", "ein", "lock\u00b7rer", "Wicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Versetzte mit ersticktem Lachen:", "tokens": ["Ver\u00b7setz\u00b7te", "mit", "er\u00b7stick\u00b7tem", "La\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Ich danke f\u00fcr den Unterricht", "tokens": ["Ich", "dan\u00b7ke", "f\u00fcr", "den", "Un\u00b7ter\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und will ihn mir zu Nutze machen.", "tokens": ["Und", "will", "ihn", "mir", "zu", "Nut\u00b7ze", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PPER", "PTKZU", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Hier fiel er auf den Proviant", "tokens": ["Hier", "fiel", "er", "auf", "den", "Pro\u00b7vi\u00b7ant"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Und schob ein H\u00fchnchen in den Rachen.", "tokens": ["Und", "schob", "ein", "H\u00fchn\u00b7chen", "in", "den", "Ra\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Ein zweytes ward ihm nachgesandt", "tokens": ["Ein", "zwey\u00b7tes", "ward", "ihm", "nach\u00b7ge\u00b7sandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Und mit dem Spiele fortgefahren,", "tokens": ["Und", "mit", "dem", "Spie\u00b7le", "fort\u00b7ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Bis alle, trotz der Homilie", "tokens": ["Bis", "al\u00b7le", ",", "trotz", "der", "Ho\u00b7mi\u00b7lie"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "APPR", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Des Oheims, rein verzehret waren.", "tokens": ["Des", "O\u00b7heims", ",", "rein", "ver\u00b7zeh\u00b7ret", "wa\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Nun suchte Rips mit saurer M\u00fch", "tokens": ["Nun", "such\u00b7te", "Rips", "mit", "sau\u00b7rer", "M\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Sich von der Tafel zu erheben.", "tokens": ["Sich", "von", "der", "Ta\u00b7fel", "zu", "er\u00b7he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Allein auf einmal wird ihm schwach;", "tokens": ["Al\u00b7lein", "auf", "ein\u00b7mal", "wird", "ihm", "schwach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Sein Athem stockt, die Kniee beben:", "tokens": ["Sein", "A\u00b7them", "stockt", ",", "die", "Kni\u00b7ee", "be\u00b7ben", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Er w\u00e4lzt sich, seufzet Weh und Ach,", "tokens": ["Er", "w\u00e4lzt", "sich", ",", "seuf\u00b7zet", "Weh", "und", "Ach", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Und sucht den Fra\u00df zur\u00fcck zu geben.", "tokens": ["Und", "sucht", "den", "Fra\u00df", "zu\u00b7r\u00fcck", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Der Oheim fuhr dem armen Gauch", "tokens": ["Der", "O\u00b7heim", "fuhr", "dem", "ar\u00b7men", "Gauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Mit einer Feder in die Kehle.", "tokens": ["Mit", "ei\u00b7ner", "Fe\u00b7der", "in", "die", "Keh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Allein umsonst, der volle Bauch", "tokens": ["Al\u00b7lein", "um\u00b7sonst", ",", "der", "vol\u00b7le", "Bauch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Zerbarst. Zevs helfe seiner Seele;", "tokens": ["Zer\u00b7barst", ".", "Zevs", "hel\u00b7fe", "sei\u00b7ner", "See\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Sprach Raps und schickte sich nun auch", "tokens": ["Sprach", "Raps", "und", "schick\u00b7te", "sich", "nun", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "KON", "VVFIN", "PRF", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Zum Siegesmahl. Er nagt die Fl\u00fcgel", "tokens": ["Zum", "Sie\u00b7ges\u00b7mahl", ".", "Er", "nagt", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.43": {"text": "Des Hahns in kleinen Bissen ab,", "tokens": ["Des", "Hahns", "in", "klei\u00b7nen", "Bis\u00b7sen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "H\u00f6lt f\u00fcr den Rest des Raubs ein Grab", "tokens": ["H\u00f6lt", "f\u00fcr", "den", "Rest", "des", "Raubs", "ein", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.45": {"text": "Und \u00fcberdeckt mit Moos den H\u00fcgel.", "tokens": ["Und", "\u00fc\u00b7berd\u00b7eckt", "mit", "Moos", "den", "H\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Nun schlendert er in kurzem Trab", "tokens": ["Nun", "schlen\u00b7dert", "er", "in", "kur\u00b7zem", "Trab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Nach Haus, und \u00fcberschl\u00e4gt im Gehen,", "tokens": ["Nach", "Haus", ",", "und", "\u00fc\u00b7bersc\u00b7hl\u00e4gt", "im", "Ge\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Wie weit der Vorrath reichen mag.", "tokens": ["Wie", "weit", "der", "Vor\u00b7rath", "rei\u00b7chen", "mag."], "token_info": ["word", "word", "word", "word", "word", "abbreviation"], "pos": ["PWAV", "ADJD", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.49": {"text": "Er rastet nicht, kaum graut der Tag,", "tokens": ["Er", "ras\u00b7tet", "nicht", ",", "kaum", "graut", "der", "Tag", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "So eilt er nach dem Schatz zu sehen.", "tokens": ["So", "eilt", "er", "nach", "dem", "Schatz", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Allein der P\u00e4chter, ein Pandur,", "tokens": ["Al\u00b7lein", "der", "P\u00e4ch\u00b7ter", ",", "ein", "Pan\u00b7dur", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.52": {"text": "Der nur zu fr\u00fch den Raub erfuhr,", "tokens": ["Der", "nur", "zu", "fr\u00fch", "den", "Raub", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKA", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.53": {"text": "Lie\u00df heimlich auf den Gaudieb lauren;", "tokens": ["Lie\u00df", "heim\u00b7lich", "auf", "den", "Gau\u00b7di\u00b7eb", "lau\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.54": {"text": "Auch war er kaum dem H\u00fcgel nah,", "tokens": ["Auch", "war", "er", "k