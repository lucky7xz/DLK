{"textgrid.poem.63597": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob wohl im Atem dieser Sommerluft,", "tokens": ["Ob", "wohl", "im", "A\u00b7tem", "die\u00b7ser", "Som\u00b7mer\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dieses Morgens sanftbewegter K\u00fchle", "tokens": ["In", "die\u00b7ses", "Mor\u00b7gens", "sanft\u00b7be\u00b7weg\u00b7ter", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Hauch schon mich umschwebt aus deiner Gruft?", "tokens": ["Ein", "Hauch", "schon", "mich", "um\u00b7schwebt", "aus", "dei\u00b7ner", "Gruft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So rein wie kindlich heitre Vorgef\u00fchle", "tokens": ["So", "rein", "wie", "kind\u00b7lich", "heit\u00b7re", "Vor\u00b7ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOKOM", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Weht von der Halde mich der Fr\u00fchwind an,", "tokens": ["Weht", "von", "der", "Hal\u00b7de", "mich", "der", "Fr\u00fch\u00b7wind", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Indes ich hier in meinen Schmerzen w\u00fchle;", "tokens": ["In\u00b7des", "ich", "hier", "in", "mei\u00b7nen", "Schmer\u00b7zen", "w\u00fch\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Da\u00df ich des Wahns mich nicht erwehren kann,", "tokens": ["Da\u00df", "ich", "des", "Wahns", "mich", "nicht", "er\u00b7weh\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Teil von dir, ein St\u00e4ubchen von dem Staube", "tokens": ["Ein", "Teil", "von", "dir", ",", "ein", "St\u00e4ub\u00b7chen", "von", "dem", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPER", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der dich, geliebtes Kind, zur\u00fcckgewann,", "tokens": ["Der", "dich", ",", "ge\u00b7lieb\u00b7tes", "Kind", ",", "zu\u00b7r\u00fcck\u00b7ge\u00b7wann", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Umwittre mich im Schatten dieser Laube,", "tokens": ["Um\u00b7witt\u00b7re", "mich", "im", "Schat\u00b7ten", "die\u00b7ser", "Lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Umschmeichle mein Gem\u00fct, da\u00df es hinfort", "tokens": ["Um\u00b7schmeich\u00b7le", "mein", "Ge\u00b7m\u00fct", ",", "da\u00df", "es", "hin\u00b7fort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Nicht mehr so bittrer Schwermut sei zum Raube.", "tokens": ["Nicht", "mehr", "so", "bit\u00b7trer", "Schwer\u00b7mut", "sei", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJA", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Im Tropfen Tau an jener Ranke dort", "tokens": ["Im", "Trop\u00b7fen", "Tau", "an", "je\u00b7ner", "Ran\u00b7ke", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "NN", "APPR", "PDAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Seh' ich den Schimmer deines Auges wieder,", "tokens": ["Seh'", "ich", "den", "Schim\u00b7mer", "dei\u00b7nes", "Au\u00b7ges", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Im Rieseln jenes Bachs h\u00f6r' ich dein Wort.", "tokens": ["Im", "Rie\u00b7seln", "je\u00b7nes", "Bachs", "h\u00f6r'", "ich", "dein", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Dein Lachen klingt mir aus den Wipfeln nieder,", "tokens": ["Dein", "La\u00b7chen", "klingt", "mir", "aus", "den", "Wip\u00b7feln", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Wo Tauben nisten; jeder Blumenspro\u00df", "tokens": ["Wo", "Tau\u00b7ben", "nis\u00b7ten", ";", "je\u00b7der", "Blu\u00b7men\u00b7spro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "$.", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Mahnt mich des Wuchses deiner schlanken Glieder.", "tokens": ["Mahnt", "mich", "des", "Wuch\u00b7ses", "dei\u00b7ner", "schlan\u00b7ken", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Nichts ist in aller Runde klein und gro\u00df,", "tokens": ["Nichts", "ist", "in", "al\u00b7ler", "Run\u00b7de", "klein", "und", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Das mir nicht dienen mu\u00df, dich zu verk\u00fcnden,", "tokens": ["Das", "mir", "nicht", "die\u00b7nen", "mu\u00df", ",", "dich", "zu", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Als w\u00e4re die Natur ein Spiegel blo\u00df,", "tokens": ["Als", "w\u00e4\u00b7re", "die", "Na\u00b7tur", "ein", "Spie\u00b7gel", "blo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Dein fr\u00fchverlornes Bild darin zu finden,", "tokens": ["Dein", "fr\u00fch\u00b7ver\u00b7lor\u00b7nes", "Bild", "da\u00b7rin", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Als b\u00e4rge jeder laut geheimen Sinn,", "tokens": ["Als", "b\u00e4r\u00b7ge", "je\u00b7der", "laut", "ge\u00b7hei\u00b7men", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PIS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Den nur wir beide, du und ich, verst\u00fcnden.", "tokens": ["Den", "nur", "wir", "bei\u00b7de", ",", "du", "und", "ich", ",", "ver\u00b7st\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "PPER", "PIS", "$,", "PPER", "KON", "PPER", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "O sprich mir weiter, s\u00fc\u00dfe Schl\u00e4ferin!", "tokens": ["O", "sprich", "mir", "wei\u00b7ter", ",", "s\u00fc\u00b7\u00dfe", "Schl\u00e4\u00b7fe\u00b7rin", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Aus deinem Traum sprich Liebliches zu mir,", "tokens": ["Aus", "dei\u00b7nem", "Traum", "sprich", "Lieb\u00b7li\u00b7ches", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "ADJA", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Der ich noch taub f\u00fcr Menschenstimme bin!", "tokens": ["Der", "ich", "noch", "taub", "f\u00fcr", "Men\u00b7schen\u00b7stim\u00b7me", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADJD", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Best\u00e4rke mir den Glauben: was ich hier", "tokens": ["Be\u00b7st\u00e4r\u00b7ke", "mir", "den", "Glau\u00b7ben", ":", "was", "ich", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "PWS", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "An Holdem seh' und h\u00f6re, sei dein Gr\u00fc\u00dfen,", "tokens": ["An", "Hol\u00b7dem", "seh'", "und", "h\u00f6\u00b7re", ",", "sei", "dein", "Gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "KON", "VVFIN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Und jedes Labsal stamme nur von dir.", "tokens": ["Und", "je\u00b7des", "Lab\u00b7sal", "stam\u00b7me", "nur", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.31": {"text": "So fr\u00fch hab' ich zur\u00fcck dich geben m\u00fcssen", "tokens": ["So", "fr\u00fch", "hab'", "ich", "zu\u00b7r\u00fcck", "dich", "ge\u00b7ben", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "PTKVZ", "PPER", "VVINF", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Ans All, aus dem du fl\u00fcchtig aufgetaucht;", "tokens": ["Ans", "All", ",", "aus", "dem", "du", "fl\u00fcch\u00b7tig", "auf\u00b7ge\u00b7taucht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Nun kann der Trost nur meinen Gram vers\u00fc\u00dfen,", "tokens": ["Nun", "kann", "der", "Trost", "nur", "mei\u00b7nen", "Gram", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Da\u00df aus dem All zur\u00fcck dein Wesen haucht.", "tokens": ["Da\u00df", "aus", "dem", "All", "zu\u00b7r\u00fcck", "dein", "We\u00b7sen", "haucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "PTKVZ", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob wohl im Atem dieser Sommerluft,", "tokens": ["Ob", "wohl", "im", "A\u00b7tem", "die\u00b7ser", "Som\u00b7mer\u00b7luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In dieses Morgens sanftbewegter K\u00fchle", "tokens": ["In", "die\u00b7ses", "Mor\u00b7gens", "sanft\u00b7be\u00b7weg\u00b7ter", "K\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Hauch schon mich umschwebt aus deiner Gruft?", "tokens": ["Ein", "Hauch", "schon", "mich", "um\u00b7schwebt", "aus", "dei\u00b7ner", "Gruft", "?"], "token