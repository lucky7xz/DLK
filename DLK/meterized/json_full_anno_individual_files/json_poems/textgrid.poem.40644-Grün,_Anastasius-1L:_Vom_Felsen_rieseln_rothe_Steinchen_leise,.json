{"textgrid.poem.40644": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom Felsen rieseln rothe Steinchen leise,", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "en:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Felsen rieseln rothe Steinchen leise,", "tokens": ["Vom", "Fel\u00b7sen", "rie\u00b7seln", "ro\u00b7the", "Stein\u00b7chen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als rinne Blut vom Eiland in die Fluthen;", "tokens": ["Als", "rin\u00b7ne", "Blut", "vom", "Ei\u00b7land", "in", "die", "Flut\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPRART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es stirbt langsamen Tod, wie jener Weise,", "tokens": ["Es", "stirbt", "lang\u00b7sa\u00b7men", "Tod", ",", "wie", "je\u00b7ner", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$,", "PWAV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Bad aus offnen Adern zu verbluten.", "tokens": ["Im", "Bad", "aus", "off\u00b7nen", "A\u00b7dern", "zu", "ver\u00b7blu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch grausam tr\u00e4g ist der Zerst\u00f6rung Reise,", "tokens": ["Doch", "grau\u00b7sam", "tr\u00e4g", "ist", "der", "Zer\u00b7st\u00f6\u00b7rung", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein rascher Untergang in Sturm und Gluthen!", "tokens": ["Kein", "ra\u00b7scher", "Un\u00b7ter\u00b7gang", "in", "Sturm", "und", "Glu\u00b7then", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Sturz, der einst kein Wellchen regt im Kreise, \u2013", "tokens": ["Ein", "Sturz", ",", "der", "einst", "kein", "Well\u00b7chen", "regt", "im", "Krei\u00b7se", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "APPRART", "NN", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie herbes Menschenloos will mich's gemuthen:", "tokens": ["Wie", "her\u00b7bes", "Men\u00b7schen\u00b7loos", "will", "mich's", "ge\u00b7mu\u00b7then", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "VMFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn langsam niederrieselt ins Vergessen", "tokens": ["Wenn", "lang\u00b7sam", "nie\u00b7der\u00b7rie\u00b7selt", "ins", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Dauerndste, was unser Herz besessen,", "tokens": ["Das", "Dau\u00b7ernds\u00b7te", ",", "was", "un\u00b7ser", "Herz", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn unser Bestes St\u00fcck um St\u00fcck verwittert!", "tokens": ["Wenn", "un\u00b7ser", "Bes\u00b7tes", "St\u00fcck", "um", "St\u00fcck", "ver\u00b7wit\u00b7tert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir m\u00fcssen erst die bitt're Welle trinken", "tokens": ["Wir", "m\u00fcs\u00b7sen", "erst", "die", "bitt'\u00b7re", "Wel\u00b7le", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der herben Fluth, eh' wir in sie versinken, \u2013", "tokens": ["Der", "her\u00b7ben", "Fluth", ",", "eh'", "wir", "in", "sie", "ver\u00b7sin\u00b7ken", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVINF", "$,", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wir sinken ein, und keine Welle zittert.", "tokens": ["Wir", "sin\u00b7ken", "ein", ",", "und", "kei\u00b7ne", "Wel\u00b7le", "zit\u00b7tert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Vom Felsen rieseln rothe Steinchen leise,", "tokens": ["Vom", "Fel\u00b7sen", "rie\u00b7seln", "ro\u00b7the", "Stein\u00b7chen", "lei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": [