}, "line.4": {"text": "So gro\u00df zu sein wie sie!", "tokens": ["So", "gro\u00df", "zu", "sein", "wie", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Wirf endlich diese Stelzen weg", "tokens": ["Wirf", "end\u00b7lich", "die\u00b7se", "Stel\u00b7zen", "weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PDAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vornehmer Gleisnerei:", "tokens": ["Vor\u00b7neh\u00b7mer", "Gleis\u00b7ne\u00b7rei", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wahr sei der Mensch, er krieche nicht,", "tokens": ["Wahr", "sei", "der", "Mensch", ",", "er", "krie\u00b7che", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst braucht es kein Gebet.", "tokens": ["Sonst", "braucht", "es", "kein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.31": {"line.1": {"text": "Im Herzen wohnt die Gottesfurcht,", "tokens": ["Im", "Her\u00b7zen", "wohnt", "die", "Got\u00b7tes\u00b7furcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und blo\u00df ein W\u00fcterich", "tokens": ["Und", "blo\u00df", "ein", "W\u00fc\u00b7te\u00b7rich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "(wir wurden's inne) breitet sie", "tokens": ["(", "wir", "wur\u00b7den's", "in\u00b7ne", ")", "brei\u00b7tet", "sie"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PTKVZ", "$(", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie einen Mantel aus!", "tokens": ["Wie", "ei\u00b7nen", "Man\u00b7tel", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.32": {"line.1": {"text": "Wann deiner S\u00f6hne jeglicher", "tokens": ["Wann", "dei\u00b7ner", "S\u00f6h\u00b7ne", "jeg\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN", "PIAT"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sein B\u00fcrgertum erkennt,", "tokens": ["Sein", "B\u00fcr\u00b7ger\u00b7tum", "er\u00b7kennt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sinkt vor dir Europas Schwert", "tokens": ["Dann", "sinkt", "vor", "dir", "Eu\u00b7ro\u00b7pas", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Asiens Henkerbeil!", "tokens": ["Und", "A\u00b7siens", "Hen\u00b7ker\u00b7beil", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}