{"textgrid.poem.60756": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du wachst; allein wer b\u00fcrgt daf\u00fcr,", "genre": "verse", "period": "N.A.", "pub_year": 1832, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wachst; allein wer b\u00fcrgt daf\u00fcr,", "tokens": ["Du", "wachst", ";", "al\u00b7lein", "wer", "b\u00fcrgt", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "PWS", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ob nie du schlafen wirst?", "tokens": ["Ob", "nie", "du", "schla\u00b7fen", "wirst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob Mut und Vaterlandsgef\u00fchl", "tokens": ["Ob", "Mut", "und", "Va\u00b7ter\u00b7lands\u00b7ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf ewig bleiben wach?", "tokens": ["Auf", "e\u00b7wig", "blei\u00b7ben", "wach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du ruhst an einem Bergesrand", "tokens": ["Du", "ruhst", "an", "ei\u00b7nem", "Ber\u00b7ges\u00b7rand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4hrlich \u00fcberaus,", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "\u00fc\u00b7be\u00b7raus", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wehe dir, sobald du schl\u00e4fst", "tokens": ["Und", "we\u00b7he", "dir", ",", "so\u00b7bald", "du", "schl\u00e4fst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Einen Augenblick!", "tokens": ["Nur", "Ei\u00b7nen", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Gedenke nicht des Augenblicks,", "tokens": ["Ge\u00b7den\u00b7ke", "nicht", "des", "Au\u00b7gen\u00b7blicks", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ins tiefre Werden sieh!", "tokens": ["Ins", "tief\u00b7re", "Wer\u00b7den", "sieh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die ganze Zukunft, liegt sie nicht", "tokens": ["Die", "gan\u00b7ze", "Zu\u00b7kunft", ",", "liegt", "sie", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner Brust allein?", "tokens": ["In", "dei\u00b7ner", "Brust", "al\u00b7lein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es sah die Welt Jahrhunderte", "tokens": ["Es", "sah", "die", "Welt", "Jahr\u00b7hun\u00b7der\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dumpfen Schlaf gesenkt,", "tokens": ["In", "dum\u00b7pfen", "Schlaf", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und einer wildbewegten Zeit", "tokens": ["Und", "ei\u00b7ner", "wild\u00b7be\u00b7weg\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Folgt eine tr\u00e4ge nach.", "tokens": ["Folgt", "ei\u00b7ne", "tr\u00e4\u00b7ge", "nach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer aber selbst in schlaffer Zeit,", "tokens": ["Wer", "a\u00b7ber", "selbst", "in", "schlaf\u00b7fer", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer, sprich, erhielt sich wach?", "tokens": ["Wer", ",", "sprich", ",", "er\u00b7hielt", "sich", "wach", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "ADJD", "$,", "VVFIN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es blieben selbst in schlaffer Zeit", "tokens": ["Es", "blie\u00b7ben", "selbst", "in", "schlaf\u00b7fer", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die freien V\u00f6lker wach!", "tokens": ["Die", "frei\u00b7en", "V\u00f6l\u00b7ker", "wach", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es ist die Freiheit jener Puls,", "tokens": ["Es", "ist", "die", "Frei\u00b7heit", "je\u00b7ner", "Puls", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der stets lebendig schl\u00e4gt,", "tokens": ["Der", "stets", "le\u00b7ben\u00b7dig", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der stets zum Kampfe treibt ein Volk", "tokens": ["Der", "stets", "zum", "Kamp\u00b7fe", "treibt", "ein", "Volk"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr seinen eignen Herd.", "tokens": ["F\u00fcr", "sei\u00b7nen", "eig\u00b7nen", "Herd", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nie fehlen ihr Verteidiger,", "tokens": ["Nie", "feh\u00b7len", "ihr", "Ver\u00b7tei\u00b7di\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie mangelt ihr ein Schwert,", "tokens": ["Nie", "man\u00b7gelt", "ihr", "ein", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wer sie recht gekostet hat,", "tokens": ["Und", "wer", "sie", "recht", "ge\u00b7kos\u00b7tet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geht in den Tod f\u00fcr sie!", "tokens": ["Geht", "in", "den", "Tod", "f\u00fcr", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "O w\u00e4r ich frei, wer raubte mir's?", "tokens": ["O", "w\u00e4r", "ich", "frei", ",", "wer", "raub\u00b7te", "mir's", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "$,", "PWS", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verl\u00f6r ich jede Hand,", "tokens": ["Ver\u00b7l\u00f6r", "ich", "je\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So hielt ich doch die Waffe noch", "tokens": ["So", "hielt", "ich", "doch", "die", "Waf\u00b7fe", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit meinen Z\u00e4hnen fest!", "tokens": ["Mit", "mei\u00b7nen", "Z\u00e4h\u00b7nen", "fest", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du f\u00fcrchtest diesen starken Wein,", "tokens": ["Du", "f\u00fcrch\u00b7test", "die\u00b7sen", "star\u00b7ken", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dieweil er m\u00e4chtig g\u00e4rt;", "tokens": ["Die\u00b7weil", "er", "m\u00e4ch\u00b7tig", "g\u00e4rt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch setze nur den Becher an,", "tokens": ["Doch", "set\u00b7ze", "nur", "den", "Be\u00b7cher", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er macht die Seelen stark!", "tokens": ["Er", "macht", "die", "See\u00b7len", "stark", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wenn du diesen Trieb erstickst", "tokens": ["Und", "wenn", "du", "die\u00b7sen", "Trieb", "er\u00b7stickst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "(du willst es nicht, ich wei\u00df!)", "tokens": ["(", "du", "willst", "es", "nicht", ",", "ich", "wei\u00df", "!", ")"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PTKNEG", "$,", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann stehst du nackt und waffenlos,", "tokens": ["Dann", "stehst", "du", "nackt", "und", "waf\u00b7fen\u00b7los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein entnervter Greis.", "tokens": ["Wie", "ein", "ent\u00b7nerv\u00b7ter", "Greis", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wann dieser Trieb erlischt, er ist", "tokens": ["Wann", "die\u00b7ser", "Trieb", "er\u00b7lischt", ",", "er", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PDAT", "NN", "VVPP", "$,", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erloschen manchem Volk,", "tokens": ["Er\u00b7lo\u00b7schen", "man\u00b7chem", "Volk", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du r\u00fcttelst dann die Leiche wohl", "tokens": ["Du", "r\u00fct\u00b7telst", "dann", "die", "Lei\u00b7che", "wohl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und r\u00fcttelst sie nicht auf!", "tokens": ["Und", "r\u00fct\u00b7telst", "sie", "nicht", "auf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Er sei bewahrt als Heiligtum,", "tokens": ["Er", "sei", "be\u00b7wahrt", "als", "Hei\u00b7lig\u00b7tum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KOKOM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ewigen Lampe Gleich,", "tokens": ["Der", "e\u00b7wi\u00b7gen", "Lam\u00b7pe", "Gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die hangend vor dem Hochaltar", "tokens": ["Die", "han\u00b7gend", "vor", "dem", "Hoc\u00b7hal\u00b7tar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Doms Gew\u00f6lb erhellt.", "tokens": ["Des", "Doms", "Ge\u00b7w\u00f6lb", "er\u00b7hellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Vergebens blickt Bewunderung", "tokens": ["Ver\u00b7ge\u00b7bens", "blickt", "Be\u00b7wun\u00b7de\u00b7rung"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf alte V\u00f6lker hin:", "tokens": ["Auf", "al\u00b7te", "V\u00f6l\u00b7ker", "hin", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bewundert nicht! Es liegt an euch,", "tokens": ["Be\u00b7wun\u00b7dert", "nicht", "!", "Es", "liegt", "an", "euch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So gro\u00df zu sein wie sie!", "tokens": ["So", "gro\u00df", "zu", "sein", "wie", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKZU", "VAINF", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Wirf endlich diese Stelzen weg", "tokens": ["Wirf", "end\u00b7lich", "die\u00b7se", "Stel\u00b7zen", "weg"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "PDAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vornehmer Gleisnerei:", "tokens": ["Vor\u00b7neh\u00b7mer", "Gleis\u00b7ne\u00b7rei", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wahr sei der Mensch, er krieche nicht,", "tokens": ["Wahr", "sei", "der", "Mensch", ",", "er", "krie\u00b7che", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sonst braucht es kein Gebet.", "tokens": ["Sonst", "braucht", "es", "kein", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.15": {"line.1": {"text": "Im Herzen wohnt die Gottesfurcht,", "tokens": ["Im", "Her\u00b7zen", "wohnt", "die", "Got\u00b7tes\u00b7furcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und blo\u00df ein W\u00fcterich", "tokens": ["Und", "blo\u00df", "ein", "W\u00fc\u00b7te\u00b7rich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "(wir wurden's inne) breitet sie", "tokens": ["(", "wir", "wur\u00b7den's", "in\u00b7ne", ")", "brei\u00b7tet", "sie"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PTKVZ", "$(", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie einen Mantel aus!", "tokens": ["Wie", "ei\u00b7nen", "Man\u00b7tel", "aus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Wann deiner S\u00f6hne jeglicher", "tokens": 