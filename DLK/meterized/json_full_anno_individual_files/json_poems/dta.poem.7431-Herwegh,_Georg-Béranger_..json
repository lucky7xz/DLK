{"dta.poem.7431": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "B\u00e9ranger   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling! Fr\u00fchling! Die Feder wird zur Schwinge,", "tokens": ["Fr\u00fch\u00b7ling", "!", "Fr\u00fch\u00b7ling", "!", "Die", "Fe\u00b7der", "wird", "zur", "Schwin\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Und jedes Elend eine Seligkeit!", "tokens": ["Und", "je\u00b7des", "E\u00b7lend", "ei\u00b7ne", "Se\u00b7lig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Fr\u00fchling! Fr\u00fchling! Der Griffel wird zur Klinge,", "tokens": ["Fr\u00fch\u00b7ling", "!", "Fr\u00fch\u00b7ling", "!", "Der", "Grif\u00b7fel", "wird", "zur", "Klin\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$.", "ART", "NN", "VAFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Die mutig die verj\u00fcngte Welt befreit!", "tokens": ["Die", "mu\u00b7tig", "die", "ver\u00b7j\u00fcng\u00b7te", "Welt", "be\u00b7freit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ein Lied mein Morgen- und mein Abendsegen,", "tokens": ["Ein", "Lied", "mein", "Mor\u00b7gen", "und", "mein", "A\u00b7bend\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "TRUNC", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein Lied f\u00fcr jeden Jubel, jedes Weh', \u2014", "tokens": ["Ein", "Lied", "f\u00fcr", "je\u00b7den", "Ju\u00b7bel", ",", "je\u00b7des", "Weh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch meiner Kr\u00e4nze sch\u00f6nsten la\u00dft mich legen", "tokens": ["Doch", "mei\u00b7ner", "Kr\u00e4n\u00b7ze", "sch\u00f6ns\u00b7ten", "la\u00dft", "mich", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ums Silberhaar heut' meinem ", "tokens": ["Ums", "Sil\u00b7ber\u00b7haar", "heut'", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "NN", "ADV", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er k\u00fc\u00dfte jede Freiheit in der Wiege,", "tokens": ["Er", "k\u00fc\u00df\u00b7te", "je\u00b7de", "Frei\u00b7heit", "in", "der", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er weinte jeder in die Grube nach;", "tokens": ["Er", "wein\u00b7te", "je\u00b7der", "in", "die", "Gru\u00b7be", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er war der zweite Held bei jedem Siege,", "tokens": ["Er", "war", "der", "zwei\u00b7te", "Held", "bei", "je\u00b7dem", "Sie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er rief den Donner f\u00fcr Tyrannen wach;", "tokens": ["Er", "rief", "den", "Don\u00b7ner", "f\u00fcr", "Ty\u00b7ran\u00b7nen", "wach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es wurde zur ersch\u00fctternden Lawine", "tokens": ["Es", "wur\u00b7de", "zur", "er\u00b7sch\u00fct\u00b7tern\u00b7den", "La\u00b7wi\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des holden Haubtes leichter Flockenschnee;", "tokens": ["Des", "hol\u00b7den", "Haub\u00b7tes", "leich\u00b7ter", "Flo\u00b7cken\u00b7schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Der Freiheit ewig unersch\u00f6pfte Mine,", "tokens": ["Der", "Frei\u00b7heit", "e\u00b7wig", "un\u00b7er\u00b7sch\u00f6pf\u00b7te", "Mi\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Es ist das Herz von meinem ", "tokens": ["Es", "ist", "das", "Herz", "von", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Die von der Heimat Boden sich verbannten,", "tokens": ["Die", "von", "der", "Hei\u00b7mat", "Bo\u00b7den", "sich", "ver\u00b7bann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo freier Seelen Opfer Nichts mehr n\u00fctzt,", "tokens": ["Wo", "frei\u00b7er", "See\u00b7len", "Op\u00b7fer", "Nichts", "mehr", "n\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "NN", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und Ihr, des Czaren reinste Diamanten,", "tokens": ["Und", "Ihr", ",", "des", "Cza\u00b7ren", "reins\u00b7te", "Di\u00b7a\u00b7man\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die er vor Dieben in Sibirien sch\u00fctzt,", "tokens": ["Die", "er", "vor", "Die\u00b7ben", "in", "Si\u00b7bi\u00b7ri\u00b7en", "sch\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PDAT", "APPR", "NE", "VVFIN", "$,"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Auf Deinen Bergen, k\u00fchner Suliote,", "tokens": ["Auf", "Dei\u00b7nen", "Ber\u00b7gen", ",", "k\u00fch\u00b7ner", "Su\u00b7li\u00b7o\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Du, T\u00fcrk', in Deiner luftigen Moschee,", "tokens": ["Du", ",", "T\u00fcrk'", ",", "in", "Dei\u00b7ner", "luf\u00b7ti\u00b7gen", "Mo\u00b7schee", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Theilt heute zwischen Ihm und Eurem Gotte,", "tokens": ["Theilt", "heu\u00b7te", "zwi\u00b7schen", "Ihm", "und", "Eu\u00b7rem", "Got\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Theilt zwischen Gott und meinem ", "tokens": ["Theilt", "zwi\u00b7schen", "Gott", "und", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "KON", "PPOSAT"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Wer lag am Boden, den Er nicht erhoben?", "tokens": ["Wer", "lag", "am", "Bo\u00b7den", ",", "den", "Er", "nicht", "er\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPRART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wessen Herz ist seinem Lied zu klein?", "tokens": ["Und", "wes\u00b7sen", "Herz", "ist", "sei\u00b7nem", "Lied", "zu", "klein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wo ist die H\u00fctte, drum er nicht gewoben", "tokens": ["Wo", "ist", "die", "H\u00fct\u00b7te", ",", "drum", "er", "nicht", "ge\u00b7wo\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "$,", "PAV", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "H\u00e4tt' einen Paradieses-Heil'genschein?", "tokens": ["H\u00e4tt'", "ei\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7Heil'\u00b7gen\u00b7schein", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du \u201eAlter Vagabund\u201c, den ich dem Grabe", "tokens": ["Du", "\u201e", "Al\u00b7ter", "Va\u00b7ga\u00b7bund", "\u201c", ",", "den", "ich", "dem", "Gra\u00b7be"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$(", "NN", "NE", "$(", "$,", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So grollend dort entgegenschleichen seh', \u2014", "tokens": ["So", "grol\u00b7lend", "dort", "ent\u00b7ge\u00b7gen\u00b7schlei\u00b7chen", "seh'", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "ADV", "VVINF", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Heil, dreifach Heil dem morschen Bettelstabe,", "tokens": ["Heil", ",", "drei\u00b7fach", "Heil", "dem", "mor\u00b7schen", "Bet\u00b7tel\u00b7sta\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "CARD", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Dem Aaronsstab von meinem ", "tokens": ["Dem", "Aa\u00b7rons\u00b7stab", "von", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Fr\u00fchling! die G\u00e4rten wollen Rosen tragen,", "tokens": ["Fr\u00fch\u00b7ling", "!", "die", "G\u00e4r\u00b7ten", "wol\u00b7len", "Ro\u00b7sen", "tra\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "NN", "VMFIN", "NN", "VVINF", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die ersten flugs hier meinem Mann ums Haubt!", "tokens": ["Die", "ers\u00b7ten", "flugs", "hier", "mei\u00b7nem", "Mann", "ums", "Haubt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Nachtigall, die f\u00fcr Freiheit hat geschlagen,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", ",", "die", "f\u00fcr", "Frei\u00b7heit", "hat", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Hat an die Liebe gl\u00fchend auch geglaubt.", "tokens": ["Hat", "an", "die", "Lie\u00b7be", "gl\u00fc\u00b7hend", "auch", "ge\u00b7glaubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch wollt' er einzig von der Liebe singen,", "tokens": ["Doch", "wollt'", "er", "ein\u00b7zig", "von", "der", "Lie\u00b7be", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df auch die Liebe bei der Freiheit steh', \u2014", "tokens": ["Da\u00df", "auch", "die", "Lie\u00b7be", "bei", "der", "Frei\u00b7heit", "steh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein Schwert mit Rosen wollen wir ihm bringen,", "tokens": ["Ein", "Schwert", "mit", "Ro\u00b7sen", "wol\u00b7len", "wir", "ihm", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Schwert mit Rosen meinem ", "tokens": ["Ein", "Schwert", "mit", "Ro\u00b7sen", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}