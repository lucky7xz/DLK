{"textgrid.poem.58942": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "161. Vernunft gilt nicht im Rat", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft Gott folgen und nicht fragen,", "tokens": ["Du", "mu\u00dft", "Gott", "fol\u00b7gen", "und", "nicht", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was andre Menschen dazu sagen;", "tokens": ["Was", "and\u00b7re", "Men\u00b7schen", "da\u00b7zu", "sa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wer mit Vernunft zu Rat will gehn,", "tokens": ["Wer", "mit", "Ver\u00b7nunft", "zu", "Rat", "will", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt stets in Furcht und Zweifel stehn.", "tokens": ["Bleibt", "stets", "in", "Furcht", "und", "Zwei\u00b7fel", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du mu\u00dft Gott folgen und nicht fragen,", "tokens": ["Du", "mu\u00dft", "Gott", "fol\u00b7gen", "und", "nicht", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "KON", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+