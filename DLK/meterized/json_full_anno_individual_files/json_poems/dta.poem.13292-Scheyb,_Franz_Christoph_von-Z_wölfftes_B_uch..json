{"dta.poem.13292": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z w\u00f6lfftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann in der Fr\u00fchlings-Zeit die Sonne die Gestallt", "tokens": ["Wann", "in", "der", "Fr\u00fch\u00b7lings\u00b7Zeit", "die", "Son\u00b7ne", "die", "Ge\u00b7stallt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der neu-begr\u00fcnten Au mit ihrem Morgen mahlt,", "tokens": ["Der", "neu\u00b7be\u00b7gr\u00fcn\u00b7ten", "Au", "mit", "ih\u00b7rem", "Mor\u00b7gen", "mahlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So pflegt die Munterkeit der V\u00f6gel sich zu regen,", "tokens": ["So", "pflegt", "die", "Mun\u00b7ter\u00b7keit", "der", "V\u00f6\u00b7gel", "sich", "zu", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PRF", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "120Und dem erweckten Sinn Vergn\u00fcgen einzupr\u00e4gen.", "tokens": ["dem", "er\u00b7weck\u00b7ten", "Sinn", "Ver\u00b7gn\u00fc\u00b7gen", "ein\u00b7zu\u00b7pr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVIZU", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Ein jeder lockt und pfeift, schl\u00e4gt, zwitschert, schn\u00e4belt, singt,", "tokens": ["Ein", "je\u00b7der", "lockt", "und", "pfeift", ",", "schl\u00e4gt", ",", "zwit\u00b7schert", ",", "schn\u00e4\u00b7belt", ",", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem er sich erfreut vom Baum auf B\u00e4ume schwingt;", "tokens": ["In\u00b7dem", "er", "sich", "er\u00b7freut", "vom", "Baum", "auf", "B\u00e4u\u00b7me", "schwingt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADJD", "APPRART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wie lebhaft r\u00fchrt er sich, wann er das Laub durchschlupfet,", "tokens": ["Wie", "leb\u00b7haft", "r\u00fchrt", "er", "sich", ",", "wann", "er", "das", "Laub", "durch\u00b7schlup\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "PRF", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den Kopf, den Hal\u00df, den Leib, dreht, d\u00e4hnet, hebt und hupfet?", "tokens": ["Den", "Kopf", ",", "den", "Hal\u00df", ",", "den", "Leib", ",", "dreht", ",", "d\u00e4h\u00b7net", ",", "hebt", "und", "hup\u00b7fet", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "125Ein hell vermischter Schall dringt aus der hohlen Brust,", "tokens": ["hell", "ver\u00b7mischter", "Schall", "dringt", "aus", "der", "hoh\u00b7len", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "Erwecket bey der Schaar Frolocken, Trost und Lust.", "tokens": ["Er\u00b7we\u00b7cket", "bey", "der", "Schaar", "Fro\u00b7lo\u00b7cken", ",", "Trost", "und", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie gurgelt sich so schnell die Fl\u00f6ten-volle Kehle?", "tokens": ["Wie", "gur\u00b7gelt", "sich", "so", "schnell", "die", "Fl\u00f6\u00b7ten\u00b7vol\u00b7le", "Keh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PRF", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Es scheint, als wann sein Herz die Fr\u00f6hlichkeit erz\u00e4hle.", "tokens": ["Es", "scheint", ",", "als", "wann", "sein", "Herz", "die", "Fr\u00f6h\u00b7lich\u00b7keit", "er\u00b7z\u00e4h\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Kommt diese Regung uns erg\u00f6zlich, lieblich vor;", "tokens": ["Kommt", "die\u00b7se", "Re\u00b7gung", "uns", "er\u00b7g\u00f6z\u00b7lich", ",", "lieb\u00b7lich", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PPER", "ADJD", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "130Wie viel annehmlicher ist dieser Tugend-Chor?", "tokens": ["viel", "an\u00b7nehm\u00b7li\u00b7cher", "ist", "die\u00b7ser", "Tu\u00b7gen\u00b7dChor", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.15": {"text": "Wo Stimme, Stirn und Aug auf einmahl mehr erkl\u00e4ren,", "tokens": ["Wo", "Stim\u00b7me", ",", "Stirn", "und", "Aug", "auf", "ein\u00b7mahl", "mehr", "er\u00b7kl\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "APPR", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Als wann die Blicke nichts als solche S\u00e4nger w\u00e4ren;", "tokens": ["Als", "wann", "die", "Bli\u00b7cke", "nichts", "als", "sol\u00b7che", "S\u00e4n\u00b7ger", "w\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWAV", "ART", "NN", "PIS", "KOKOM", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Dann keine war, die nicht bald hier, bald da, bald dort", "tokens": ["Dann", "kei\u00b7ne", "war", ",", "die", "nicht", "bald", "hier", ",", "bald", "da", ",", "bald", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PIAT", "VAFIN", "$,", "PRELS", "PTKNEG", "ADV", "ADV", "$,", "ADV", "ADV", "$,", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Gesch\u00e4fftig, munter, froh, mehr oder nur ein Wort", "tokens": ["Ge\u00b7sch\u00e4ff\u00b7tig", ",", "mun\u00b7ter", ",", "froh", ",", "mehr", "o\u00b7der", "nur", "ein", "Wort"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,", "ADV", "KON", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "135Laut oder heimlich sprach, auch etwan eine k\u00fc\u00dfte,", "tokens": ["o\u00b7der", "heim\u00b7lich", "sprach", ",", "auch", "et\u00b7wan", "ei\u00b7ne", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "ADV", "ADV", "ART", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Und ihres Herzens Lust durch den Beweis vers\u00fc\u00dfte.", "tokens": ["Und", "ih\u00b7res", "Her\u00b7zens", "Lust", "durch", "den", "Be\u00b7weis", "ver\u00b7s\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie stunden Reihen-wei\u00df; zerstreuet; Paar und Paar;", "tokens": ["Sie", "stun\u00b7den", "Rei\u00b7hen\u00b7wei\u00df", ";", "zer\u00b7streu\u00b7et", ";", "Paar", "und", "Paar", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "VVFIN", "$.", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nach dem es jedem Sinn lieb und gef\u00e4llig war:", "tokens": ["Nach", "dem", "es", "je\u00b7dem", "Sinn", "lieb", "und", "ge\u00b7f\u00e4l\u00b7lig", "war", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PIAT", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bis endlich einige sich nach der Ordnung sezten,", "tokens": ["Bis", "end\u00b7lich", "ei\u00b7ni\u00b7ge", "sich", "nach", "der", "Ord\u00b7nung", "sez\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "140Jedoch stets voller Lust von tausend Sachen schw\u00e4zten.", "tokens": ["stets", "vol\u00b7ler", "Lust", "von", "tau\u00b7send", "Sa\u00b7chen", "schw\u00e4z\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}