{"dta.poem.362": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Man will die manns\u00fcchtige  Talestris  \n im Bette sehen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Neugierig bin ich nicht/ doch dieses m\u00f6chte ich", "tokens": ["Neu\u00b7gie\u00b7rig", "bin", "ich", "nicht", "/", "doch", "die\u00b7ses", "m\u00f6ch\u00b7te", "ich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "PTKNEG", "$(", "ADV", "PDS", "VMFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von Hertzen gerne sehn wie die ", "tokens": ["Von", "Hert\u00b7zen", "ger\u00b7ne", "sehn", "wie", "die"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVINF", "KOKOM", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrem Bette stellt/ wenn ihr die L\u00fcste kommen/", "tokens": ["In", "ih\u00b7rem", "Bet\u00b7te", "stellt", "/", "wenn", "ihr", "die", "L\u00fcs\u00b7te", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ein verborgen Feur das Hertze eingenommen;", "tokens": ["Und", "ein", "ver\u00b7bor\u00b7gen", "Feur", "das", "Hert\u00b7ze", "ein\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PDS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich glaube/ ihr Gesicht wird dann mit Rosen bl\u00fchn", "tokens": ["Ich", "glau\u00b7be", "/", "ihr", "Ge\u00b7sicht", "wird", "dann", "mit", "Ro\u00b7sen", "bl\u00fchn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Lippen werden sich mit Scharlach \u00fcberziehn.", "tokens": ["Die", "Lip\u00b7pen", "wer\u00b7den", "sich", "mit", "Schar\u00b7lach", "\u00fc\u00b7ber\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der zarte Busen wird steiff in die H\u00f6he schwellen/", "tokens": ["Der", "zar\u00b7te", "Bu\u00b7sen", "wird", "steiff", "in", "die", "H\u00f6\u00b7he", "schwel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Und so ein Schwaanen-Bett der s\u00fcssen Lust vorstellen.", "tokens": ["Und", "so", "ein", "Schwaa\u00b7nen\u00b7Bett", "der", "s\u00fcs\u00b7sen", "Lust", "vor\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die angenehme Schoo\u00df wird denn voll Feuer seyn", "tokens": ["Die", "an\u00b7ge\u00b7neh\u00b7me", "Schoo\u00df", "wird", "denn", "voll", "Feu\u00b7er", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Indem daselbsten zieht ", "tokens": ["In\u00b7dem", "da\u00b7selbs\u00b7ten", "zieht"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PDS", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Die Flammen werden da als wie ein ", "tokens": ["Die", "Flam\u00b7men", "wer\u00b7den", "da", "als", "wie", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "KOUS", "KOKOM", "ART"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Daran ein l\u00fcstern Sinn sich wird erw\u00e4rmen k\u00f6nnen.", "tokens": ["Da\u00b7ran", "ein", "l\u00fcs\u00b7tern", "Sinn", "sich", "wird", "er\u00b7w\u00e4r\u00b7men", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "ADJA", "NN", "PRF", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "In diesen Stande wolt\u2019 ich wol ", "tokens": ["In", "die\u00b7sen", "Stan\u00b7de", "wolt'", "ich", "wol"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ich wett\u2019 sie ist alsdann noch tausend mahl so sch\u00f6n/", "tokens": ["Ich", "wett'", "sie", "ist", "als\u00b7dann", "noch", "tau\u00b7send", "mahl", "so", "sch\u00f6n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "ADV", "ADV", "CARD", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als wie sie sonsten ist/ wenn sich die Lilje leget", "tokens": ["Als", "wie", "sie", "sons\u00b7ten", "ist", "/", "wenn", "sich", "die", "Lil\u00b7je", "le\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "PPER", "ADV", "VAFIN", "$(", "KOUS", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-++", "measure": "unknown.measure.septa"}, "line.16": {"text": "Um ihre sch\u00f6ne Wang\u2019 und silber Fr\u00fcchte tr\u00e4get.", "tokens": ["Um", "ih\u00b7re", "sch\u00f6\u00b7ne", "Wang'", "und", "sil\u00b7ber", "Fr\u00fcch\u00b7te", "tr\u00e4\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Doch wozu dient der Wunsch? da es doch nicht geschicht/", "tokens": ["Doch", "wo\u00b7zu", "dient", "der", "Wunsch", "?", "da", "es", "doch", "nicht", "ge\u00b7schicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "$.", "KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Talestris sehe ich in ihren Bette nicht/", "tokens": ["Ta\u00b7le\u00b7stris", "se\u00b7he", "ich", "in", "ih\u00b7ren", "Bet\u00b7te", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.19": {"text": "Wer in den sch\u00f6nen Port sein Liebes-Schiff will lencken/", "tokens": ["Wer", "in", "den", "sch\u00f6\u00b7nen", "Port", "sein", "Lie\u00b7bes\u00b7Schiff", "will", "len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NE", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der mu\u00df zum Ancker-Gold viel ", "tokens": ["Der", "mu\u00df", "zum", "An\u00b7cker\u00b7Gold", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "APPRART", "NN", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}