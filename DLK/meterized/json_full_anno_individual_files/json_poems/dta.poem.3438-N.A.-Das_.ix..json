{"dta.poem.3438": {"metadata": {"author": {"name": "N.A.", "birth": "N.A.", "death": "N.A."}, "title": "Das .ix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1497", "urn": "urn:nbn:de:kobv:b4-30121-8", "language": ["de:0.99"], "booktitle": "Des Edlen Ritter Morgeners Walfart. Erfurt, 1497."}, "poem": {"stanza.1": {"line.1": {"text": "Der kammerer sprach gar tugentlich", "tokens": ["Der", "kam\u00b7me\u00b7rer", "sprach", "gar", "tu\u00b7gent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "ir edler ritter es te\u00fccht mich gut", "tokens": ["ir", "ed\u00b7ler", "rit\u00b7ter", "es", "te\u00fccht", "mich", "gut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "jr bleibt hie heim bei ewrem reich", "tokens": ["jr", "bleibt", "hie", "heim", "bei", "ew\u00b7rem", "reich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "APPR", "PPOSAT", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "die frawen haben einen kurtzen mut", "tokens": ["die", "fra\u00b7wen", "ha\u00b7ben", "ei\u00b7nen", "kurt\u00b7zen", "mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "in gantzen trewen ich e\u00fcch sag", "tokens": ["in", "gant\u00b7zen", "tre\u00b7wen", "ich", "e\u00b7\u00fcch", "sag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "VVFIN", "PPER", "ADJD", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "ich pfleg ewer frawen nicht", "tokens": ["ich", "pfleg", "e\u00b7wer