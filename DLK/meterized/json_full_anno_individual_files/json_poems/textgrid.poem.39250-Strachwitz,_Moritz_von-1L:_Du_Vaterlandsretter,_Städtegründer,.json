{"textgrid.poem.39250": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Vaterlandsretter, St\u00e4dtegr\u00fcnder,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Vaterlandsretter, St\u00e4dtegr\u00fcnder,", "tokens": ["Du", "Va\u00b7ter\u00b7lands\u00b7ret\u00b7ter", ",", "St\u00e4d\u00b7te\u00b7gr\u00fcn\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPER", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gro\u00df im Gewinnen, gr\u00f6\u00dfer im Bewahren,", "tokens": ["Gro\u00df", "im", "Ge\u00b7win\u00b7nen", ",", "gr\u00f6\u00b7\u00dfer", "im", "Be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sei mit gesegnet, Heiden\u00fcberwinder!", "tokens": ["Sei", "mit", "ge\u00b7seg\u00b7net", ",", "Hei\u00b7den\u00b7\u00fc\u00b7berw\u00b7in\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "VVPP", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Matt zuckte unterm S\u00e4bel der Barbaren", "tokens": ["Matt", "zuck\u00b7te", "un\u00b7term", "S\u00e4\u00b7bel", "der", "Bar\u00b7ba\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das Reich, und stampfend \u00fcber deutsche Saaten", "tokens": ["Das", "Reich", ",", "und", "stamp\u00b7fend", "\u00fc\u00b7ber", "deut\u00b7sche", "Saa\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinging das Ro\u00df des Wenden und Magyaren.", "tokens": ["Hin\u00b7ging", "das", "Ro\u00df", "des", "Wen\u00b7den", "und", "Ma\u00b7gy\u00b7a\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$."], "meter": "+--+-+--+-+-", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Von seinen F\u00fcrsten ward das Land verraten,", "tokens": ["Von", "sei\u00b7nen", "F\u00fcrs\u00b7ten", "ward", "das", "Land", "ver\u00b7ra\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die w\u00fcrgten sich und riefen sich zum Bunde", "tokens": ["Die", "w\u00fcrg\u00b7ten", "sich", "und", "rie\u00b7fen", "sich", "zum", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "KON", "VVFIN", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Den grimmen Heiden her zu grimmen Taten.", "tokens": ["Den", "grim\u00b7men", "Hei\u00b7den", "her", "zu", "grim\u00b7men", "Ta\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APZR", "PTKZU", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Durch \u00d6sterreich, da ritt in b\u00f6ser Stunde", "tokens": ["Durch", "\u00d6s\u00b7ter\u00b7reich", ",", "da", "ritt", "in", "b\u00f6\u00b7ser", "Stun\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die maulwurfs\u00e4ugige Kentaurenhorde,", "tokens": ["Die", "maul\u00b7wurf\u00b7s\u00e4u\u00b7gi\u00b7ge", "Ken\u00b7tau\u00b7ren\u00b7hor\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Volk des Attila, die Brut der Hunde.", "tokens": ["Das", "Volk", "des", "At\u00b7ti\u00b7la", ",", "die", "Brut", "der", "Hun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Als wie die S\u00fcndflut \u00fcber alle Borde", "tokens": ["Als", "wie", "die", "S\u00fcnd\u00b7flut", "\u00fc\u00b7ber", "al\u00b7le", "Bor\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOKOM", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinschwoll der Greuel durch das Land der V\u00e4ter,", "tokens": ["Hin\u00b7schwoll", "der", "Greu\u00b7el", "durch", "das", "Land", "der", "V\u00e4\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Das r\u00f6chelte im ungeheuren Morde.", "tokens": ["Das", "r\u00f6\u00b7chel\u00b7te", "im", "un\u00b7ge\u00b7heu\u00b7ren", "Mor\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Von Blut und Flammen widerschien der \u00c4ther,", "tokens": ["Von", "Blut", "und", "Flam\u00b7men", "wi\u00b7der\u00b7schien", "der", "\u00c4\u00b7ther", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nicht Einer kam, kein Retter und kein Ringer,", "tokens": ["Nicht", "Ei\u00b7ner", "kam", ",", "kein", "Ret\u00b7ter", "und", "kein", "Rin\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PIS", "VVFIN", "$,", "PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denn selbst der Priester wurde zum Verr\u00e4ter.", "tokens": ["Denn", "selbst", "der", "Pries\u00b7ter", "wur\u00b7de", "zum", "Ver\u00b7r\u00e4\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Und alle Jahre kamen die Bezwinger,", "tokens": ["Und", "al\u00b7le", "Jah\u00b7re", "ka\u00b7men", "die", "Be\u00b7zwin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und j\u00e4hrlich \u00e4rmer ward und j\u00e4hrlich schw\u00e4cher", "tokens": ["Und", "j\u00e4hr\u00b7lich", "\u00e4r\u00b7mer", "ward", "und", "j\u00e4hr\u00b7lich", "schw\u00e4\u00b7cher"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VAFIN", "KON", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das gro\u00dfe Reich der kleinen Karolinger. \u2013", "tokens": ["Das", "gro\u00b7\u00dfe", "Reich", "der", "klei\u00b7nen", "Ka\u00b7ro\u00b7lin\u00b7ger", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und \u00fcbersatt vom bittren Schmerzensbecher,", "tokens": ["Und", "\u00fc\u00b7ber\u00b7satt", "vom", "bit\u00b7tren", "Schmer\u00b7zens\u00b7be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf seinem Todbett lag Konrad der Franke,", "tokens": ["Auf", "sei\u00b7nem", "Tod\u00b7bett", "lag", "Kon\u00b7rad", "der", "Fran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der sprach: \u00bbIch will Euch k\u00fcren einen R\u00e4cher.\u00ab", "tokens": ["Der", "sprach", ":", "\u00bb", "Ich", "will", "Euch", "k\u00fc\u00b7ren", "ei\u00b7nen", "R\u00e4\u00b7cher", ".", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "$.", "$(", "PPER", "VMFIN", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "\u00bbich stritt mit ihm der Krone hier zu Danke,", "tokens": ["\u00bb", "ich", "stritt", "mit", "ihm", "der", "Kro\u00b7ne", "hier", "zu", "Dan\u00b7ke", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "APPR", "PTKANT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nun nehmt sie hin, es trage sie derselbe,", "tokens": ["Nun", "nehmt", "sie", "hin", ",", "es", "tra\u00b7ge", "sie", "der\u00b7sel\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PDAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er wird sie halten, ob im Sturm sie schwanke.\u00ab", "tokens": ["Er", "wird", "sie", "hal\u00b7ten", ",", "ob", "im", "Sturm", "sie", "schwan\u00b7ke", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "\u00bbund dieser Krone leuchtendes Gew\u00f6lbe,", "tokens": ["\u00bb", "und", "die\u00b7ser", "Kro\u00b7ne", "leuch\u00b7ten\u00b7des", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er l\u00e4\u00dft es flammen weit in aller F\u00e4hrde; \u2013", "tokens": ["Er", "l\u00e4\u00dft", "es", "flam\u00b7men", "weit", "in", "al\u00b7ler", "F\u00e4hr\u00b7de", ";", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist der Herzog von dem Land der Elbe.\u00ab \u2013", "tokens": ["Es", "ist", "der", "Her\u00b7zog", "von", "dem", "Land", "der", "El\u00b7be", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "ART", "NE", "$.", "$(", "$("], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.11": {"line.1": {"text": "In heil'ger Morgenluft am Vogelherde,", "tokens": ["In", "heil'\u00b7ger", "Mor\u00b7gen\u00b7luft", "am", "Vo\u00b7gel\u00b7her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da dr\u00fccken sie den Reif ihm in die Locken,", "tokens": ["Da", "dr\u00fc\u00b7cken", "sie", "den", "Reif", "ihm", "in", "die", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf hohem Berg vor aller deutscher Erde.", "tokens": ["Auf", "ho\u00b7hem", "Berg", "vor", "al\u00b7ler", "deut\u00b7scher", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und alle Lande staunten froh erschrocken,", "tokens": ["Und", "al\u00b7le", "Lan\u00b7de", "staun\u00b7ten", "froh", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn allw\u00e4rts warf die Krone ihre Strahlen,", "tokens": ["Denn", "all\u00b7w\u00e4rts", "warf", "die", "Kro\u00b7ne", "ih\u00b7re", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und rings von selber r\u00fchrten sich die Glocken.", "tokens": ["Und", "rings", "von", "sel\u00b7ber", "r\u00fchr\u00b7ten", "sich", "die", "Glo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ADV", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Sie schien allm\u00e4chtig zu den tiefsten Talen", "tokens": ["Sie", "schien", "all\u00b7m\u00e4ch\u00b7tig", "zu", "den", "tiefs\u00b7ten", "Ta\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und lie\u00df die Waffen in Demanten zittern,", "tokens": ["Und", "lie\u00df", "die", "Waf\u00b7fen", "in", "De\u00b7man\u00b7ten", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die W\u00e4lder sich mit gr\u00fcnem Gold bemalen.", "tokens": ["Die", "W\u00e4l\u00b7der", "sich", "mit", "gr\u00fc\u00b7nem", "Gold", "be\u00b7ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Es t\u00e4t der Aar die junge Sonne wittern,", "tokens": ["Es", "t\u00e4t", "der", "Aar", "die", "jun\u00b7ge", "Son\u00b7ne", "wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der deutsche Aar, der lag in Schmach und Frone,", "tokens": ["Der", "deut\u00b7sche", "Aar", ",", "der", "lag", "in", "Schmach", "und", "Fro\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da scholl sein Fl\u00fcgelschlag gleich Lenzgewittern,", "tokens": ["Da", "scholl", "sein", "Fl\u00fc\u00b7gel\u00b7schlag", "gleich", "Lenz\u00b7ge\u00b7wit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und zu dem neuen Licht der Kaiserkrone", "tokens": ["Und", "zu", "dem", "neu\u00b7en", "Licht", "der", "Kai\u00b7ser\u00b7kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Stieg er empor, das sieghaft und allm\u00e4chtig", "tokens": ["Stieg", "er", "em\u00b7por", ",", "das", "sieg\u00b7haft", "und", "all\u00b7m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinstrahlte von des Bergs gr\u00fcnsamt'nem Throne.", "tokens": ["Hins\u00b7trahl\u00b7te", "von", "des", "Bergs", "gr\u00fcn\u00b7samt'\u00b7nem", "Thro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Es stand der erste Heinrich ernst bed\u00e4chtig,", "tokens": ["Es", "stand", "der", "ers\u00b7te", "Hein\u00b7rich", "ernst", "be\u00b7d\u00e4ch\u00b7tig", ","], "token_info": ["word",