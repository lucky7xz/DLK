{"textgrid.poem.34495": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ganz nur im Banne der Kunst und M\u00e4nner wie Weiber verachtend,", "tokens": ["Ganz", "nur", "im", "Ban\u00b7ne", "der", "Kunst", "und", "M\u00e4n\u00b7ner", "wie", "Wei\u00b7ber", "ver\u00b7ach\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "ART", "NN", "KON", "NN", "KOKOM", "NN", "VVPP", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "stirbst du als Junggesell einst, wenigen Freunden getreu.", "tokens": ["stirbst", "du", "als", "Jung\u00b7ge\u00b7sell", "einst", ",", "we\u00b7ni\u00b7gen", "Freun\u00b7den", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "NN", "ADV", "$,", "PIAT", "NN", "ADJD", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Schm\u00e4hlich misslang der Versuch, dir die weibliche Bestie zu z\u00e4hmen \u2013", "tokens": ["Schm\u00e4h\u00b7lich", "miss\u00b7lang", "der", "Ver\u00b7such", ",", "dir", "die", "weib\u00b7li\u00b7che", "Be\u00b7stie", "zu", "z\u00e4h\u00b7men", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "PPER", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "+--+--+--+-+-+-+-", "measure": "dactylic.tri.plus"}, "line.4": {"text": "sage, wie kommt es da nur, dass wir dich Papa genannt?", "tokens": ["sa\u00b7ge", ",", "wie", "kommt", "es", "da", "nur", ",", "dass", "wir", "dich", "Pa\u00b7pa", "ge\u00b7n