{"textgrid.poem.40110": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Graf Richard, was jagst du und jagst durchs Feld,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Graf Richard, was jagst du und jagst durchs Feld,", "tokens": ["Graf", "Ric\u00b7hard", ",", "was", "jagst", "du", "und", "jagst", "durchs", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "als fliehst du vor deinem Gewissen?! \u2013", "tokens": ["als", "fliehst", "du", "vor", "dei\u00b7nem", "Ge\u00b7wis\u00b7sen", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es war deine Pflicht, es war dein Recht!", "tokens": ["Es", "war", "dei\u00b7ne", "Pflicht", ",", "es", "war", "dein", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "dein Weib beim Knecht:", "tokens": ["dein", "Weib", "beim", "Knecht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "das haben sie ", "tokens": ["das", "ha\u00b7ben", "sie"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Graf Richard, was fliehst du und fliehst durch die Nacht:", "tokens": ["Graf", "Ric\u00b7hard", ",", "was", "fliehst", "du", "und", "fliehst", "durch", "die", "Nacht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VVFIN", "PPER", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "die Tote liegt ", "tokens": ["die", "To\u00b7te", "liegt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "wie damals so still, wie damals so kalt,", "tokens": ["wie", "da\u00b7mals", "so", "still", ",", "wie", "da\u00b7mals", "so", "kalt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ADJD", "$,", "PWAV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "als mit Gewalt", "tokens": ["als", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "sie zogst zum Traualtare! \u2013 \u2013", "tokens": ["sie", "zogst", "zum", "Trau\u00b7al\u00b7ta\u00b7re", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dahin, dahin am Eichenhain!", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", "am", "Ei\u00b7chen\u00b7hain", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "herunter vom Feld! die Stra\u00dfe hinein!", "tokens": ["her\u00b7un\u00b7ter", "vom", "Feld", "!", "die", "Stra\u00b7\u00dfe", "hin\u00b7ein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$.", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "zur\u00fcck, zur\u00fcck zum Schlosse! \u2013", "tokens": ["zu\u00b7r\u00fcck", ",", "zu\u00b7r\u00fcck", "zum", "Schlos\u00b7se", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PTKVZ", "$,", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie schleichen die Nebel her\u00fcber vom Moor,", "tokens": ["Wie", "schlei\u00b7chen", "die", "Ne\u00b7bel", "her\u00b7\u00fc\u00b7ber", "vom", "Moor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "wie schaun aus dem Walde die Schatten hervor!", "tokens": ["wie", "schaun", "aus", "dem", "Wal\u00b7de", "die", "Schat\u00b7ten", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Dem Reiter wird ", "tokens": ["Dem", "Rei\u00b7ter", "wird"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Dahin, dahin mit h\u00e4ngendem Zaum!", "tokens": ["Da\u00b7hin", ",", "da\u00b7hin", "mit", "h\u00e4n\u00b7gen\u00b7dem", "Zaum", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "vor\u00fcber, vor\u00fcber an Baum und Baum!", "tokens": ["vor\u00b7\u00fc\u00b7ber", ",", "vor\u00b7\u00fc\u00b7ber", "an", "Baum", "und", "Baum", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "ADV", "APPR", "NE", "KON", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "will die Burg denn noch immer nicht ragen? \u2013", "tokens": ["will", "die", "Burg", "denn", "noch", "im\u00b7mer", "nicht", "ra\u00b7gen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "ART", "NN", "KON", "ADV", "ADV", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Noch Einmal k\u00fcssen in heimlicher Stund'", "tokens": ["Noch", "Ein\u00b7mal", "k\u00fcs\u00b7sen", "in", "heim\u00b7li\u00b7cher", "Stund'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "die blasse Wunde, den wei\u00dfen Mund!", "tokens": ["die", "blas\u00b7se", "Wun\u00b7de", ",", "den", "wei\u00b7\u00dfen", "Mund", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Ich \u2013 hab' sie \u2013 ", "tokens": ["Ich", "\u2013", "hab'", "sie", "\u2013"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["PPER", "$(", "VAFIN", "PPER", "$("], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.5": {"line.1": {"text": "Was taucht hervor aus Dunstes Wogen,", "tokens": ["Was", "taucht", "her\u00b7vor", "aus", "Duns\u00b7tes", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "was schimmert so sanft, so bleich?", "tokens": ["was", "schim\u00b7mert", "so", "sanft", ",", "so", "bleich", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "was kommt so lockend einhergezogen,", "tokens": ["was", "kommt", "so", "lo\u00b7ckend", "ein\u00b7her\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "was breitet die Arme so weich! \u2013", "tokens": ["was", "brei\u00b7tet", "die", "Ar\u00b7me", "so", "weich", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Allm\u00e4chtiger Vater, sie ", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "Va\u00b7ter", ",", "sie"], "token_info": ["word", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "PPER"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "nun bin ich erl\u00f6st, nun bin ich befreit!", "tokens": ["nun", "bin", "ich", "er\u00b7l\u00f6st", ",", "nun", "bin", "ich", "be\u00b7freit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Was schwebt zur\u00fcck, was schwebt entgegen", "tokens": ["Was", "schwebt", "zu\u00b7r\u00fcck", ",", "was", "schwebt", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKVZ", "$,", "PWS", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "vorbei an Stamm und Stamm?", "tokens": ["vor\u00b7bei", "an", "Stamm", "und", "Stamm", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "was wallt und winkt mit leisem Bewegen", "tokens": ["was", "wallt", "und", "winkt", "mit", "lei\u00b7sem", "Be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "herunter vom sichern Damm! \u2013", "tokens": ["her\u00b7un\u00b7ter", "vom", "si\u00b7chern", "Damm", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Halt stille doch, Liebchen! ich nehm' dich aufs Pferd!", "tokens": ["Halt", "stil\u00b7le", "doch", ",", "Lieb\u00b7chen", "!", "ich", "nehm'", "dich", "aufs", "Pferd", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "$,", "NN", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "ich hab' dich so lange, so hei\u00df begehrt! \u2013 \u2013", "tokens": ["ich", "hab'", "dich", "so", "lan\u00b7ge", ",", "so", "hei\u00df", "be\u00b7gehrt", "!", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$,", "ADV", "ADJD", "VVPP", "$.", "$(", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.7": {"line.1": {"text": "Ich will dich ", "tokens": ["Ich", "will", "dich"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PRF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "nicht l\u00e4nger ", "tokens": ["nicht", "l\u00e4n\u00b7ger"], "token_info": ["word", "word"], "pos": ["PTKNEG", "ADJD"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und gl\u00fchenden Blicks er sich vorw\u00e4rts bengt,", "tokens": ["Und", "gl\u00fc\u00b7hen\u00b7den", "Blicks", "er", "sich", "vor\u00b7w\u00e4rts", "bengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "und gl\u00fchenden Auges der Rappe keucht ...", "tokens": ["und", "gl\u00fc\u00b7hen\u00b7den", "Au\u00b7ges", "der", "Rap\u00b7pe", "keucht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Die Nebel quellen vom Moore.", "tokens": ["Die", "Ne\u00b7bel", "quel\u00b7len", "vom", "Moo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Willst wieder entweichen \u2013? so stirb, Geduld!", "tokens": ["Willst", "wie\u00b7der", "ent\u00b7wei\u00b7chen", "\u2013", "?", "so", "stirb", ",", "Ge\u00b7duld", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "$(", "$.", "ADV", "VVFIN", "$,", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "will l\u00e4nger nicht ", "tokens": ["will", "l\u00e4n\u00b7ger", "nicht"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ADJD", "PTKNEG"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "jetzt fasse ich dich! jetzt halt' ich die Braut \u2013 \u2013", "tokens": ["jetzt", "fas\u00b7se", "ich", "dich", "!", "jetzt", "halt'", "ich", "die", "Braut", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$.", "ADV", "VVFIN", "PPER", "ART", "NN", "$(", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Nebel rollen am Moore.", "tokens": ["Die", "Ne\u00b7bel", "rol\u00b7len", "am", "Moo\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Graf Richard, was jagst du und jagst durchs Feld,", "tokens": ["Graf", "Ric\u00b7hard", ",", "was", "jagst", "du", "und", "jagst", "durchs", "Feld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "VVFIN", "PPER", "KON", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "als fliehst du vor deinem Gewissen?! \u2013", "tokens": ["als", "fliehst", "du", "vor", "dei\u00b7nem", "Ge\u00b7wis\u00b7sen", "?!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Es war deine Pflicht, es war dein Recht!", "tokens": ["Es", "war", "dei\u00b7ne", "Pflicht", ",", "es", "war", "dein", "Recht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "N