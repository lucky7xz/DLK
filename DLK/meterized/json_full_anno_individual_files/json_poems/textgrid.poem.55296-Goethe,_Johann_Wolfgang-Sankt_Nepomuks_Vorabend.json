{"textgrid.poem.55296": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Sankt Nepomuks Vorabend", "genre": "verse", "period": "N.A.", "pub_year": 1820, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lichtlein schwimmen auf dem Strome,", "tokens": ["Licht\u00b7lein", "schwim\u00b7men", "auf", "dem", "Stro\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kinder singen auf der Br\u00fccken, Glocke,", "tokens": ["Kin\u00b7der", "sin\u00b7gen", "auf", "der", "Br\u00fc\u00b7cken", ",", "Glo\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gl\u00f6ckchen f\u00fcgt vom Dome", "tokens": ["Gl\u00f6ck\u00b7chen", "f\u00fcgt", "vom", "Do\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Sich der Andacht, dem Entz\u00fccken.", "tokens": ["Sich", "der", "An\u00b7dacht", ",", "dem", "Ent\u00b7z\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Lichtlein schwinden, Sterne schwinden;", "tokens": ["Licht\u00b7lein", "schwin\u00b7den", ",", "Ster\u00b7ne", "schwin\u00b7den", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Also l\u00f6ste sich die Seele", "tokens": ["Al\u00b7so", "l\u00f6s\u00b7te", "sich", "die", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsres Heil'gen, nicht verk\u00fcnden", "tokens": ["Uns\u00b7res", "Heil'\u00b7gen", ",", "nicht", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durft er anvertraute Fehle.", "tokens": ["Durft", "er", "an\u00b7ver\u00b7trau\u00b7te", "Feh\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Lichtlein, schwimmet! Spielt, ihr Kinder!", "tokens": ["Licht\u00b7lein", ",", "schwim\u00b7met", "!", "Spielt", ",", "ihr", "Kin\u00b7der", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$.", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kinderchor, o singe, singe!", "tokens": ["Kin\u00b7der\u00b7chor", ",", "o", "sin\u00b7ge", ",", "sin\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "FM", "FM", "$,", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verk\u00fcndiget nicht minder,", "tokens": ["Und", "ver\u00b7k\u00fcn\u00b7di\u00b7get", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was den Stern zu Sternen bringe.", "tokens": ["Was", "den", "Stern", "zu", "Ster\u00b7nen", "brin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "p