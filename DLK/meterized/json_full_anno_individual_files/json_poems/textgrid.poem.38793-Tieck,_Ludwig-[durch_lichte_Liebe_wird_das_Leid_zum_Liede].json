{"textgrid.poem.38793": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[durch lichte Liebe wird das Leid zum Liede]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch lichte Liebe wird das Leid zum Liede,", "tokens": ["Durch", "lich\u00b7te", "Lie\u00b7be", "wird", "das", "Leid", "zum", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Schnell fachen sich in Gluth an helle Kerzen,", "tokens": ["Schnell", "fa\u00b7chen", "sich", "in", "Gluth", "an", "hel\u00b7le", "Ker\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PRF", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das rothe Leben brennt im tiefen Herzen", "tokens": ["Das", "ro\u00b7the", "Le\u00b7ben", "brennt", "im", "tie\u00b7fen", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stumme Freude wird beredter Friede.", "tokens": ["Und", "stum\u00b7me", "Freu\u00b7de", "wird", "be\u00b7red\u00b7ter", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Willkommen denn! th\u00f6rigt, wer euch vermiede,", "tokens": ["Will\u00b7kom\u00b7men", "denn", "!", "th\u00f6\u00b7rigt", ",", "wer", "euch", "ver\u00b7mie\u00b7de", ","