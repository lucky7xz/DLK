{"dta.poem.18819": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Trunken m\u00fcssen wir alle seyn!", "tokens": ["Trun\u00b7ken", "m\u00fcs\u00b7sen", "wir", "al\u00b7le", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PIS", "VAINF", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Jugend ist Trunkenheit ohne Wein;", "tokens": ["Ju\u00b7gend", "ist", "Trun\u00b7ken\u00b7heit", "oh\u00b7ne", "Wein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "APPR", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Trinkt sich das Alter wieder zu Jugend,", "tokens": ["Trinkt", "sich", "das", "Al\u00b7ter", "wie\u00b7der", "zu", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "So ist es wundervolle Tugend.", "tokens": ["So", "ist", "es", "wun\u00b7der\u00b7vol\u00b7le", "Tu\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr Sorgen sorgt das liebe Leben", "tokens": ["F\u00fcr", "Sor\u00b7gen", "sorgt", "das", "lie\u00b7be", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und Sorgenbrecher sind die Reben.", "tokens": ["Und", "Sor\u00b7gen\u00b7bre\u00b7cher", "sind", "die", "Re\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da wird nicht mehr nachgefragt!", "tokens": ["Da", "wird", "nicht", "mehr", "nach\u00b7ge\u00b7fragt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wein ist ernstlich untersagt.", "tokens": ["Wein", "ist", "ernst\u00b7lich", "un\u00b7ter\u00b7sagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Soll denn doch getrunken seyn,", "tokens": ["Soll", "denn", "doch", "ge\u00b7trun\u00b7ken", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Trinke nur vom besten Wein:", "tokens": ["Trin\u00b7ke", "nur", "vom", "bes\u00b7ten", "Wein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doppelt w\u00e4rest du ein Ketzer", "tokens": ["Dop\u00b7pelt", "w\u00e4\u00b7rest", "du", "ein", "Ket\u00b7zer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In Verdammniss um den Kr\u00e4tzer.", "tokens": ["In", "Ver\u00b7damm\u00b7niss", "um", "den", "Kr\u00e4t\u00b7zer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}