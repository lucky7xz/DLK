{"dta.poem.2167": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Untersuchung der Unempfindlich-  \n keit \u00fcber unsere Gesund-  \n heit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sowohl, da\u00df sie sich wohl befindet, als, da\u00df sie auch", "tokens": ["So\u00b7wohl", ",", "da\u00df", "sie", "sich", "wohl", "be\u00b7fin\u00b7det", ",", "als", ",", "da\u00df", "sie", "auch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "KOUS", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "von Schmerzen frey,", "tokens": ["von", "Schmer\u00b7zen", "frey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So sonst die Krankheit ihr verursacht, und aller Plag", "tokens": ["So", "sonst", "die", "Krank\u00b7heit", "ihr", "ver\u00b7ur\u00b7sacht", ",", "und", "al\u00b7ler", "Plag"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVPP", "$,", "KON", "PIAT", "NN"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "entnommen sey.", "tokens": ["ent\u00b7nom\u00b7men", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "So angenehm wird jeder Mensch die Gabe der Gesund-", "tokens": ["So", "an\u00b7ge\u00b7nehm", "wird", "je\u00b7der", "Mensch", "die", "Ga\u00b7be", "der", "Ge\u00b7sun\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VAFIN", "PIAT", "NN", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "heit finden,", "tokens": ["heit", "fin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Wenn wir mit dem bese\u00dfnen Gl\u00fcck die Ueberlegung nur", "tokens": ["Wenn", "wir", "mit", "dem", "be\u00b7se\u00df\u00b7nen", "Gl\u00fcck", "die", "Ue\u00b7ber\u00b7le\u00b7gung", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "verbinden.", "tokens": ["ver\u00b7bin\u00b7den", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}