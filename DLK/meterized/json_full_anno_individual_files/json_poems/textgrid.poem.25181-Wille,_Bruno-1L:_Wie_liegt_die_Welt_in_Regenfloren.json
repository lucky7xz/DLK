{"textgrid.poem.25181": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie liegt die Welt in Regenfloren", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie liegt die Welt in Regenfloren", "tokens": ["Wie", "liegt", "die", "Welt", "in", "Re\u00b7gen\u00b7flo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So leichenhaft verloren!", "tokens": ["So", "lei\u00b7chen\u00b7haft", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel grau und greise,", "tokens": ["Der", "Him\u00b7mel", "grau", "und", "grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Erde runzlig greise;", "tokens": ["Die", "Er\u00b7de", "runz\u00b7lig", "grei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und beide weinen leise.", "tokens": ["Und", "bei\u00b7de", "wei\u00b7nen", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vergilbter Rasen, Moderlaub,", "tokens": ["Ver\u00b7gilb\u00b7ter", "Ra\u00b7sen", ",", "Mo\u00b7der\u00b7laub", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der B\u00e4ume schw\u00e4rzliches Ge\u00e4st \u2013", "tokens": ["Der", "B\u00e4u\u00b7me", "schw\u00e4rz\u00b7li\u00b7ches", "Ge\u00b7\u00e4st", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So tr\u00fcb verschwommen,", "tokens": ["So", "tr\u00fcb", "ver\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Gleich gramgetr\u00e4nkten Gr\u00fcbelein.", "tokens": ["Gleich", "gram\u00b7ge\u00b7tr\u00e4nk\u00b7ten", "Gr\u00fc\u00b7be\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Mein Haupt ist \u00f6de wie im Herbst ein Nest,", "tokens": ["Mein", "Haupt", "ist", "\u00f6\u00b7de", "wie", "im", "Herbst", "ein", "Nest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KOKOM", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und auf dem Herzen pre\u00dft", "tokens": ["Und", "auf", "dem", "Her\u00b7zen", "pre\u00dft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Mir kalt und schwer ein Leichenstein. \u2013 \u2013", "tokens": ["Mir", "kalt", "und", "schwer", "ein", "Lei\u00b7chen\u00b7stein", ".", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst lieg ich steif und hager", "tokens": ["Einst", "lieg", "ich", "steif", "und", "ha\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf dem Totenlager:", "tokens": ["Auf", "dem", "To\u00b7ten\u00b7la\u00b7ger", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "All meine Weisheit ist alsdann", "tokens": ["All", "mei\u00b7ne", "Weis\u00b7heit", "ist", "als\u00b7dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein B\u00fcschel Silberhaar,", "tokens": ["Ein", "B\u00fc\u00b7schel", "Sil\u00b7ber\u00b7haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und all mein Lied", "tokens": ["Und", "all", "mein", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein reglos bleiches Lippenpaar;", "tokens": ["Ein", "reg\u00b7los", "blei\u00b7ches", "Lip\u00b7pen\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "All meine Liebe", "tokens": ["All", "mei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein kaltes, starres Herz,", "tokens": ["Ein", "kal\u00b7tes", ",", "star\u00b7res", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und all mein Werk", "tokens": ["Und", "all", "mein", "Werk"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Zwei schwere H\u00e4nde auf dem starren Herzen", "tokens": ["Zwei", "schwe\u00b7re", "H\u00e4n\u00b7de", "auf", "dem", "star\u00b7ren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Fern stirbt der Stra\u00dfenl\u00e4rm, stumm schaun die d\u00fcstern W\u00e4nde", "tokens": ["Fern", "stirbt", "der", "Stra\u00b7\u00dfen\u00b7l\u00e4rm", ",", "stumm", "schaun", "die", "d\u00fcs\u00b7tern", "W\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADJD", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Auf meinen greisen Schatz,", "tokens": ["Auf", "mei\u00b7nen", "grei\u00b7sen", "Schatz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Der leise schluchzt \u2013 in seine welken H\u00e4nde.", "tokens": ["Der", "lei\u00b7se", "schluchzt", "\u2013", "in", "sei\u00b7ne", "wel\u00b7ken", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Dann \u2013 kommt der Leichenmann,", "tokens": ["Dann", "\u2013", "kommt", "der", "Lei\u00b7chen\u00b7mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Packt alles in den Sarg:", "tokens": ["Packt", "al\u00b7les", "in", "den", "Sarg", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Haare, Lippen, Herz und H\u00e4nde,", "tokens": ["Haa\u00b7re", ",", "Lip\u00b7pen", ",", "Herz", "und", "H\u00e4n\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und pre\u00dft den Deckel fest mit knirschenden Schrauben.", "tokens": ["Und", "pre\u00dft", "den", "De\u00b7ckel", "fest", "mit", "knir\u00b7schen\u00b7den", "Schrau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Nun mag ich tr\u00e4umen", "tokens": ["Nun", "mag", "ich", "tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Im Finstern, Stillen, K\u00fchlen ...", "tokens": ["Im", "Fins\u00b7tern", ",", "Stil\u00b7len", ",", "K\u00fch\u00b7len", "..."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "NN", "$,", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und ich tr\u00e4ume:", "tokens": ["Und", "ich", "tr\u00e4u\u00b7me", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ich bin ein zarter Keim", "tokens": ["Ich", "bin", "ein", "zar\u00b7ter", "Keim"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und grabe heimlich feine Wurzeln,", "tokens": ["Und", "gra\u00b7be", "heim\u00b7lich", "fei\u00b7ne", "Wur\u00b7zeln", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stemme mich r\u00fcstig wider die Krume", "tokens": ["Stem\u00b7me", "mich", "r\u00fcs\u00b7tig", "wi\u00b7der", "die", "Kru\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Und recke neugiervoll mein K\u00f6pfchen...", "tokens": ["Und", "re\u00b7cke", "neu\u00b7gier\u00b7voll", "mein", "K\u00f6pf\u00b7chen", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da blendet und umsp\u00fclt mich", "tokens": ["Da", "blen\u00b7det", "und", "um\u00b7sp\u00fclt", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Entz\u00fcckend goldnes Licht,", "tokens": ["Ent\u00b7z\u00fc\u00b7ckend", "gold\u00b7nes", "Licht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lau, so weich!", "tokens": ["So", "lau", ",", "so", "weich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Horch, wie jauchzend zwitschern", "tokens": ["Horch", ",", "wie", "jauch\u00b7zend", "zwit\u00b7schern"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Die behenden V\u00f6glein!", "tokens": ["Die", "be\u00b7hen\u00b7den", "V\u00f6\u00b7glein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mit ihren s\u00fc\u00dfen Kehlen", "tokens": ["Mit", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen", "Keh\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "H\u00fcpft mein Kinderherz.", "tokens": ["H\u00fcpft", "mein", "Kin\u00b7der\u00b7herz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Und sieh, auf Zweigen sitzen", "tokens": ["Und", "sieh", ",", "auf", "Zwei\u00b7gen", "sit\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Viel kleine runde Knospen.", "tokens": ["Viel", "klei\u00b7ne", "run\u00b7de", "Knos\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ich nicke ihnen l\u00e4chelnd zu;", "tokens": ["Ich", "ni\u00b7cke", "ih\u00b7nen", "l\u00e4\u00b7chelnd", "zu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie nicken wieder", "tokens": ["Sie", "ni\u00b7cken", "wie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Und wollen mit mir spielen.", "tokens": ["Und", "wol\u00b7len", "mit", "mir", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und wie wir spielen in warmer Sonne,", "tokens": ["Und", "wie", "wir", "spie\u00b7len", "in", "war\u00b7mer", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Da wachsen den lieben Kleinen", "tokens": ["Da", "wach\u00b7sen", "den", "lie\u00b7ben", "Klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Lauter Fl\u00fcgelchen wei\u00df und rosa,", "tokens": ["Lau\u00b7ter", "Fl\u00fc\u00b7gel\u00b7chen", "wei\u00df", "und", "ro\u00b7sa", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "KON", "NE", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "Und zwischen Zweigen und Bl\u00e4ttchen schweben sie,", "tokens": ["Und", "zwi\u00b7schen", "Zwei\u00b7gen", "und", "Bl\u00e4tt\u00b7chen", "schwe\u00b7ben", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Duftige Engelchenschw\u00e4rme.", "tokens": ["Duf\u00b7ti\u00b7ge", "En\u00b7gel\u00b7chen\u00b7schw\u00e4r\u00b7me", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Wie liegt die Welt in Regenfloren", "tokens": ["Wie", "liegt", "die", "Welt", "in", "Re\u00b7gen\u00b7flo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So leichenhaft verloren!", "tokens": ["So", "lei\u00b7chen\u00b7haft", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Himmel grau und greise,", "tokens": ["Der", "Him\u00b7mel", "grau", "und", "grei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Erde runzlig greise;", "tokens": ["Die", "Er\u00b7de", "runz\u00b7lig", "grei\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und beide weinen leise.", "tokens": ["Und", "bei\u00b7de", "wei\u00b7nen", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVINF", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vergilbter Rasen, Moderlaub,", "tokens": ["Ver\u00b7gilb\u00b7ter", "Ra\u00b7sen", ",", "Mo\u00b7der\u00b7laub", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der B\u00e4ume schw\u00e4rzliches Ge\u00e4st \u2013", "tokens": ["Der", "B\u00e4u\u00b7me", "schw\u00e4rz\u00b7li\u00b7ches", "Ge\u00b7\u00e4st", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So tr\u00fcb verschwommen,", "tokens": ["So", "tr\u00fcb", "ve