, "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "All meine Weisheit ist alsdann", "tokens": ["All", "mei\u00b7ne", "Weis\u00b7heit", "ist", "als\u00b7dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein B\u00fcschel Silberhaar,", "tokens": ["Ein", "B\u00fc\u00b7schel", "Sil\u00b7ber\u00b7haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und all mein Lied", "tokens": ["Und", "all", "mein", "Lied"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ein reglos bleiches Lippenpaar;", "tokens": ["Ein", "reg\u00b7los", "blei\u00b7ches", "Lip\u00b7pen\u00b7paar", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "All meine Liebe", "tokens": ["All", "mei\u00b7ne", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Ein kaltes, starres Herz,", "tokens": ["Ein", "kal\u00b7tes", ",", "star\u00b7res", "Herz", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Und all mein Werk", "tokens": ["Und", "all", "mein", "Werk"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Zwei schwere H\u00e4nde auf dem starren Herzen", "tokens": ["Zwei", "schwe\u00b7re", "H\u00e4n\u00b7de", "auf", "dem", "star\u00b7ren", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "w