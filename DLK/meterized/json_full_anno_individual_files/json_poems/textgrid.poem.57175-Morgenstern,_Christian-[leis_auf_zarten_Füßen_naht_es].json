{"textgrid.poem.57175": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "[leis auf zarten F\u00fc\u00dfen naht es]", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Leis auf zarten F\u00fc\u00dfen naht es,", "tokens": ["Leis", "auf", "zar\u00b7ten", "F\u00fc\u00b7\u00dfen", "naht", "es", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "vor dem Schlafen wie ein F\u00e4cheln:", "tokens": ["vor", "dem", "Schla\u00b7fen", "wie", "ein", "F\u00e4\u00b7cheln", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Horch, o Seele, meines Rates,", "tokens": ["Horch", ",", "o", "See\u00b7le", ",", "mei\u00b7nes", "Ra\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "la\u00df dir Gl\u00fcck und Tr\u00f6stung l\u00e4cheln \u2013:", "tokens": ["la\u00df", "dir", "Gl\u00fcck", "und", "Tr\u00f6s\u00b7tung", "l\u00e4\u00b7cheln", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "NN", "KON", "NN", "VVFIN", "$(", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die in Liebe dir verbunden,", "tokens": ["Die", "in", "Lie\u00b7be", "dir", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "meas