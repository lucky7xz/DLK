{"textgrid.poem.61125": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2. Hirsch und Reh", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf dem hohen Berg Olympos, wo der Wald von Tannen rauscht,", "tokens": ["Auf", "dem", "ho\u00b7hen", "Berg", "O\u00b7lym\u00b7pos", ",", "wo", "der", "Wald", "von", "Tan\u00b7nen", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "An dem Quell im hohen Kraute steht ein Hirsch, der talw\u00e4rts lauscht;", "tokens": ["An", "dem", "Quell", "im", "ho\u00b7hen", "Krau\u00b7te", "steht", "ein", "Hirsch", ",", "der", "tal\u00b7w\u00e4rts", "lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Tr\u00e4nen weint er, dicke Tr\u00e4nen, gro\u00df wie Beeren, rot wie Blut;", "tokens": ["Tr\u00e4\u00b7nen", "weint", "er", ",", "di\u00b7cke", "Tr\u00e4\u00b7nen", ",", "gro\u00df", "wie", "Bee\u00b7ren", ",", "rot", "wie", "Blut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "ADJD", "KOKOM", "NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie aus liebem Menschenauge str\u00f6met seine Tr\u00e4nenflut.", "tokens": ["Wie", "aus", "lie\u00b7bem", "Men\u00b7schen\u00b7au\u00b7ge", "str\u00f6\u00b7met", "sei\u00b7ne", "Tr\u00e4\u00b7nen\u00b7flut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Kommt ein Rehlein hergesprungen, Rehlein mit geflecktem Fell,", "tokens": ["Kommt", "ein", "Reh\u00b7lein", "her\u00b7ge\u00b7sprun\u00b7gen", ",", "Reh\u00b7lein", "mit", "ge\u00b7fleck\u00b7tem", "Fell", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVPP", "$,", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sieht des Hirsches Tr\u00e4nen fallen auf die Kr\u00e4uter, in den Quell,", "tokens": ["Sieht", "des", "Hir\u00b7sches", "Tr\u00e4\u00b7nen", "fal\u00b7len", "auf", "die", "Kr\u00e4u\u00b7ter", ",", "in", "den", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Spricht: \u00bbWas weinst du solche Tr\u00e4nen, gro\u00df wie Beeren, rot wie Blut?", "tokens": ["Spricht", ":", "\u00bb", "Was", "weinst", "du", "sol\u00b7che", "Tr\u00e4\u00b7nen", ",", "gro\u00df", "wie", "Bee\u00b7ren", ",", "rot", "wie", "Blut", "?"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PWS", "VVFIN", "PPER", "PIAT", "NN", "$,", "ADJD", "KOKOM", "NN", "$,", "ADJD", "KOKOM", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wie aus liebem Menschenauge str\u00f6met deine Tr\u00e4nenflut.\u00ab", "tokens": ["Wie", "aus", "lie\u00b7bem", "Men\u00b7schen\u00b7au\u00b7ge", "str\u00f6\u00b7met", "dei\u00b7ne", "Tr\u00e4\u00b7nen\u00b7flut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "\u00bbt\u00fcrken sind ins Tal gekommen. Als empor den Berg ich sprang,", "tokens": ["\u00bb", "t\u00fcr\u00b7ken", "sind", "ins", "Tal", "ge\u00b7kom\u00b7men", ".", "Als", "em\u00b7por", "den", "Berg", "ich", "sprang", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VAFIN", "APPRART", "NN", "VVPP", "$.", "KOUS", "NE", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sah ich ihrer S\u00e4bel Blitzen, h\u00f6rt' ich ihrer Trommeln Klang;", "tokens": ["Sah", "ich", "ih\u00b7rer", "S\u00e4\u00b7bel", "Blit\u00b7zen", ",", "h\u00f6rt'", "ich", "ih\u00b7rer", "Trom\u00b7meln", "Klang", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "H\u00f6rt' ich auch ein gro\u00dfes Bellen: denn sie haben sich zur Jagd", "tokens": ["H\u00f6rt'", "ich", "auch", "ein", "gro\u00b7\u00dfes", "Bel\u00b7len", ":", "denn", "sie", "ha\u00b7ben", "sich", "zur", "Jagd"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$.", "KON", "PPER", "VAFIN", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Aus der Stadt Konstantinopel sechzig Hunde mitgebracht.\u00ab", "tokens": ["Aus", "der", "Stadt", "Kons\u00b7tan\u00b7ti\u00b7no\u00b7pel", "sech\u00b7zig", "Hun\u00b7de", "mit\u00b7ge\u00b7bracht", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NE", "CARD", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Rehlein spricht: \u00bbDas gr\u00e4mt mich wenig; L\u00e4ufe hab' ich flink und gut,", "tokens": ["Reh\u00b7lein", "spricht", ":", "\u00bb", "Das", "gr\u00e4mt", "mich", "we\u00b7nig", ";", "L\u00e4u\u00b7fe", "hab'", "ich", "flink", "und", "gut", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "$(", "PDS", "VVFIN", "PPER", "ADV", "$.", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Jede Kluft zu \u00fcberspringen, zu durch schwimmen jede Flut,", "tokens": ["Je\u00b7de", "Kluft", "zu", "\u00fc\u00b7bers\u00b7prin\u00b7gen", ",", "zu", "durch", "schwim\u00b7men", "je\u00b7de", "Flut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$,", "APPR", "APPR", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Und vom Berg die Klephten haben Pulver, Kugeln und Gewehr,", "tokens": ["Und", "vom", "Berg", "die", "Kleph\u00b7ten", "ha\u00b7ben", "Pul\u00b7ver", ",", "Ku\u00b7geln", "und", "Ge\u00b7wehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Um die T\u00fcrken und die Hunde fortzujagen bis ans Meer.\u00ab", "tokens": ["Um", "die", "T\u00fcr\u00b7ken", "und", "die", "Hun\u00b7de", "fort\u00b7zu\u00b7ja\u00b7gen", "bis", "ans", "Meer", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUI", "ART", "NN", "KON", "ART", "NN", "VVFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "Aber als die Sonn' hinabging, lag das Rehlein schon im Staub,", "tokens": ["A\u00b7ber", "als", "die", "Sonn'", "hin\u00b7ab\u00b7ging", ",", "lag", "das", "Reh\u00b7lein", "schon", "im", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Blutig das gefleckte H\u00e4lschen, und sein Fleisch der Hunde Raub;", "tokens": ["Blu\u00b7tig", "das", "ge\u00b7fleck\u00b7te", "H\u00e4l\u00b7schen", ",", "und", "sein", "Fleisch", "der", "Hun\u00b7de", "Raub", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Eh' der Morgen wieder graute, war der stolze Hirsch erjagt,", "tokens": ["Eh'", "der", "Mor\u00b7gen", "wie\u00b7der", "grau\u00b7te", ",", "war", "der", "stol\u00b7ze", "Hirsch", "er\u00b7jagt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und die T\u00fcrken h\u00f6hnen jeden, der sie nach den Klephten fragt.", "tokens": ["Und", "die", "T\u00fcr\u00b7ken", "h\u00f6h\u00b7nen", "je\u00b7den", ",", "der", "sie", "nach", "den", "Kleph\u00b7ten", "fragt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PIAT", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.6": {"line.1": {"text": "Auf dem hohen Berg Olympos, wo der Wald von Tannen rauscht,", "tokens": ["Auf", "dem", "ho\u00b7hen", "Berg", "O\u00b7lym\u00b7pos", ",", "wo", "der", "Wald", "von", "Tan\u00b7nen", "rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NE", "$,", "PWAV", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "An dem Quell im hohen Kraute steht ein Hirsch, der talw\u00e4rts lauscht;", "tokens": ["An", "dem", "Quell", "im", "ho\u00b7hen", "Krau\u00b7te", "steht", "ein", "Hirsch", ",", "der", "tal\u00b7w\u00e4rts", "lauscht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Tr\u00e4nen weint er, dicke Tr\u00e4nen, gro\u00df wie Beeren, rot wie Blut;", "tokens": ["Tr\u00e4\u00b7nen", "weint", "er", ",", "di\u00b7cke", "Tr\u00e4\u00b7nen", ",", "gro\