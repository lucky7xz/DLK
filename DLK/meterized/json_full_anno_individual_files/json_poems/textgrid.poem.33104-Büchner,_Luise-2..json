{"textgrid.poem.33104": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Morgen graut im heim'schen Abendlande,", "tokens": ["Der", "Mor\u00b7gen", "graut", "im", "heim'\u00b7schen", "A\u00b7bend\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kauffartheischiff naht dem sich'ren Port,", "tokens": ["Ein", "Kauf\u00b7fart\u00b7hei\u00b7schiff", "naht", "dem", "sich'\u00b7ren", "Port", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Des Kaffees Frucht von jenes reichen Pflanzers", "tokens": ["Des", "Kaf\u00b7fees", "Frucht", "von", "je\u00b7nes", "rei\u00b7chen", "Pflan\u00b7zers"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Plantagen tr\u00e4gt es wohlverwahrt an Bord;", "tokens": ["Plan\u00b7ta\u00b7gen", "tr\u00e4gt", "es", "wohl\u00b7ver\u00b7wahrt", "an", "Bord", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Gepackt in S\u00e4cke, die das arme M\u00e4dchen", "tokens": ["Ge\u00b7packt", "in", "S\u00e4\u00b7cke", ",", "die", "das", "ar\u00b7me", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit den Genossen trauernd hat gewebt,", "tokens": ["Mit", "den", "Ge\u00b7nos\u00b7sen", "trau\u00b7ernd", "hat", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "An jedem eine bitt're Thr\u00e4nenquelle,", "tokens": ["An", "je\u00b7dem", "ei\u00b7ne", "bitt'\u00b7re", "Thr\u00e4\u00b7nen\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "An jedem banger Schrei nach Freiheit klebt!", "tokens": ["An", "je\u00b7dem", "ban\u00b7ger", "Schrei", "nach", "Frei\u00b7heit", "klebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und sieh, wie man auf Gr\u00e4ber Blumen pflanzet,", "tokens": ["Und", "sieh", ",", "wie", "man", "auf", "Gr\u00e4\u00b7ber", "Blu\u00b7men", "pflan\u00b7zet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PIS", "APPR", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "So, auf die rauhen S\u00e4cke, sonst verkannt,", "tokens": ["So", ",", "auf", "die", "rau\u00b7hen", "S\u00e4\u00b7cke", ",", "sonst", "ver\u00b7kannt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Streut wie vers\u