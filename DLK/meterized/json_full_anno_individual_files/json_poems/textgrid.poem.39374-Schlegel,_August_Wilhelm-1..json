{"textgrid.poem.39374": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wollte dieses Leben", "tokens": ["Ich", "woll\u00b7te", "die\u00b7ses", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch ein unendlich Streben", "tokens": ["Durch", "ein", "un\u00b7end\u00b7lich", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zur Ewigkeit erh\u00f6h'n.", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "er\u00b7h\u00f6h'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich fragte nicht nach dr\u00fcben,", "tokens": ["Ich", "frag\u00b7te", "nicht", "nach", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Hoffen und mein Lieben", "tokens": ["Mein", "Hof\u00b7fen", "und", "mein", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War mir hienieden sch\u00f6n.", "tokens": ["War", "mir", "hien\u00b7ie\u00b7den", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was die Natur gewoben,", "tokens": ["Was", "die", "Na\u00b7tur", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Menschen drauf erhoben,", "tokens": ["Was", "Men\u00b7schen", "drauf", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verband mir Poesie.", "tokens": ["Ver\u00b7band", "mir", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So w\u00e4hnt' ich klar zu l\u00f6sen", "tokens": ["So", "w\u00e4hnt'", "ich", "klar", "zu", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gute sammt dem B\u00f6sen", "tokens": ["Das", "Gu\u00b7te", "sammt", "dem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu hoher Harmonie.", "tokens": ["Zu", "ho\u00b7her", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Was pl\u00f6tzlich abgebrochen,", "tokens": ["Was", "pl\u00f6tz\u00b7lich", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War dennoch ausgesprochen", "tokens": ["War", "den\u00b7noch", "aus\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem ordnenden Gef\u00fchl:", "tokens": ["Dem", "ord\u00b7nen\u00b7den", "Ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Lied war mir die Jugend,", "tokens": ["Ein", "Lied", "war", "mir", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Fall der Heldentugend", "tokens": ["Der", "Fall", "der", "Hel\u00b7den\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein g\u00f6ttlich Trauerspiel.", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Trau\u00b7er\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch bald ist mir zerronnen", "tokens": ["Doch", "bald", "ist", "mir", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Muth, so die\u00df begonnen,", "tokens": ["Der", "Muth", ",", "so", "die\u00df", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Gn\u00fcgsamkeit in Dunst.", "tokens": ["Die", "Gn\u00fcg\u00b7sam\u00b7keit", "in", "Dunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gefe\u00dfelt vom Verh\u00e4ngni\u00df", "tokens": ["Ge\u00b7fe\u00b7\u00dfelt", "vom", "Ver\u00b7h\u00e4ng\u00b7ni\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Im irdischen Gef\u00e4ngni\u00df:", "tokens": ["Im", "ir\u00b7di\u00b7schen", "Ge\u00b7f\u00e4ng\u00b7ni\u00df", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was hilft mir weise Kunst?", "tokens": ["Was", "hilft", "mir", "wei\u00b7se", "Kunst", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Rose kaum entfaltet,", "tokens": ["Die", "Ro\u00b7se", "kaum", "ent\u00b7fal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch s\u00fc\u00dfer mir gestaltet", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", "mir", "ge\u00b7stal\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als aller Schmuck der Welt,", "tokens": ["Als", "al\u00b7ler", "Schmuck", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die hat ein Wurm gestochen,", "tokens": ["Die", "hat", "ein", "Wurm", "ge\u00b7sto\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die hat der Tod gebrochen,", "tokens": ["Die", "hat", "der", "Tod", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Die hat der Sturm gef\u00e4llt.", "tokens": ["Die", "hat", "der", "Sturm", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nun schau' ich zu den Sternen,", "tokens": ["Nun", "schau'", "ich", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zu jenen ew'gen Fernen,", "tokens": ["Zu", "je\u00b7nen", "ew'\u00b7gen", "Fer\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie tief aus \u00f6der Kluft;", "tokens": ["Wie", "tief", "aus", "\u00f6\u00b7der", "Kluft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und, ihre blauen Augen", "tokens": ["Und", ",", "ih\u00b7re", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem Himmel zu entsaugen,", "tokens": ["Dem", "Him\u00b7mel", "zu", "ent\u00b7sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "K\u00fc\u00df' ich die leere Luft.", "tokens": ["K\u00fc\u00df'", "ich", "die", "lee\u00b7re", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "O, werde mein Orakel,", "tokens": ["O", ",", "wer\u00b7de", "mein", "O\u00b7ra\u00b7kel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du, die du ohne Makel", "tokens": ["Du", ",", "die", "du", "oh\u00b7ne", "Ma\u00b7kel"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der falschen Welt entflohst!", "tokens": ["Der", "fal\u00b7schen", "Welt", "ent\u00b7flohst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sieh mich in meiner Demuth", "tokens": ["Sieh", "mich", "in", "mei\u00b7ner", "De\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und hauch in meine Wehmuth", "tokens": ["Und", "hauch", "in", "mei\u00b7ne", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der zarten Liebe Trost.", "tokens": ["Der", "zar\u00b7ten", "Lie\u00b7be", "Trost", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn dort die Ros' erbl\u00fchte,", "tokens": ["Wenn", "dort", "die", "Ros'", "er\u00b7bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So sei die heil'ge G\u00fcte", "tokens": ["So", "sei", "die", "heil'\u00b7ge", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Endlos gebenedeit.", "tokens": ["End\u00b7los", "ge\u00b7be\u00b7ne\u00b7deit", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Zwar sehnlich werd' ich schmachten,", "tokens": ["Zwar", "sehn\u00b7lich", "werd'", "ich", "schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Doch nicht verme\u00dfen trachten", "tokens": ["Doch", "nicht", "ver\u00b7me\u00b7\u00dfen", "trach\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Aus dieser Sterblichkeit.", "tokens": ["Aus", "die\u00b7ser", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Wo ich mich wiederfinde", "tokens": ["Wo", "ich", "mich", "wie\u00b7der\u00b7fin\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bei meinem s\u00fc\u00dfen Kinde,", "tokens": ["Bei", "mei\u00b7nem", "s\u00fc\u00b7\u00dfen", "Kin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df Heil sein, Wonn' und Licht.", "tokens": ["Mu\u00df", "Heil", "sein", ",", "Wonn'", "und", "Licht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "VAINF", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Sie wird, wenn meiner Zungen", "tokens": ["Sie", "wird", ",", "wenn", "mei\u00b7ner", "Zun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Klage Laut verklungen,", "tokens": ["Der", "Kla\u00b7ge", "Laut", "ver\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mein himmlisches Gedicht.", "tokens": ["Mein", "himm\u00b7li\u00b7sches", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nahm ich in grausem Dunkel", "tokens": ["Nahm", "ich", "in", "grau\u00b7sem", "Dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Schlange Tod vom Haupt.", "tokens": ["Der", "Schlan\u00b7ge", "Tod", "vom", "Haupt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will ihn bei mir tragen,", "tokens": ["Ich", "will", "ihn", "bei", "mir", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In allen Lebenstagen", "tokens": ["In", "al\u00b7len", "Le\u00b7bens\u00b7ta\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wird er mir nie geraubt.", "tokens": ["Wird", "er", "mir", "nie", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Ich wollte dieses Leben", "tokens": ["Ich", "woll\u00b7te", "die\u00b7ses", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Durch ein unendlich Streben", "tokens": ["Durch", "ein", "un\u00b7end\u00b7lich", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zur Ewigkeit erh\u00f6h'n.", "tokens": ["Zur", "E\u00b7wig\u00b7keit", "er\u00b7h\u00f6h'", "n."], "token_info": ["word", "word", "word", "abbreviation"], "pos": ["APPRART", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich fragte nicht nach dr\u00fcben,", "tokens": ["Ich", "frag\u00b7te", "nicht", "nach", "dr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Hoffen und mein Lieben", "tokens": ["Mein", "Hof\u00b7fen", "und", "mein", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "War mir hienieden sch\u00f6n.", "tokens": ["War", "mir", "hien\u00b7ie\u00b7den", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was die Natur gewoben,", "tokens": ["Was", "die", "Na\u00b7tur", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was Menschen drauf erhoben,", "tokens": ["Was", "Men\u00b7schen", "drauf", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Verband mir Poesie.", "tokens": ["Ver\u00b7band", "mir", "Poe\u00b7sie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "So w\u00e4hnt' ich klar zu l\u00f6sen", "tokens": ["So", "w\u00e4hnt'", "ich", "klar", "zu", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Gute sammt dem B\u00f6sen", "tokens": ["Das", "Gu\u00b7te", "sammt", "dem", "B\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu hoher Harmonie.", "tokens": ["Zu", "ho\u00b7her", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Was pl\u00f6tzlich abgebrochen,", "tokens": ["Was", "pl\u00f6tz\u00b7lich", "ab\u00b7ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War dennoch ausgesprochen", "tokens": ["War", "den\u00b7noch", "aus\u00b7ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dem ordnenden Gef\u00fchl:", "tokens": ["Dem", "ord\u00b7nen\u00b7den", "Ge\u00b7f\u00fchl", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ein Lied war mir die Jugend,", "tokens": ["Ein", "Lied", "war", "mir", "die", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Fall der Heldentugend", "tokens": ["Der", "Fall", "der", "Hel\u00b7den\u00b7tu\u00b7gend"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ein g\u00f6ttlich Trauerspiel.", "tokens": ["Ein", "g\u00f6tt\u00b7lich", "Trau\u00b7er\u00b7spiel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Doch bald ist mir zerronnen", "tokens": ["Doch", "bald", "ist", "mir", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Muth, so die\u00df begonnen,", "tokens": ["Der", "Muth", ",", "so", "die\u00df", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PDS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Gn\u00fcgsamkeit in Dunst.", "tokens": ["Die", "Gn\u00fcg\u00b7sam\u00b7keit", "in", "Dunst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line