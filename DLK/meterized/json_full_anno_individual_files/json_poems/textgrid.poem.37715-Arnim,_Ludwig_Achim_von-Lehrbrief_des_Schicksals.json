{"textgrid.poem.37715": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Lehrbrief des Schicksals", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie so gerne flie\u00dft ihr Thr\u00e4nen,", "tokens": ["Wie", "so", "ger\u00b7ne", "flie\u00dft", "ihr", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlt zur Freiheit himmlisch Sehnen,", "tokens": ["F\u00fchlt", "zur", "Frei\u00b7heit", "himm\u00b7lisch", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott aus Wasser schuf die Erde,", "tokens": ["Gott", "aus", "Was\u00b7ser", "schuf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df zu Wasser alles werde,", "tokens": ["Da\u00df", "zu", "Was\u00b7ser", "al\u00b7les", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles sinket drin in Schimmer,", "tokens": ["Al\u00b7les", "sin\u00b7ket", "drin", "in", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Goldne Schl\u00f6sser scheinen Tr\u00fcmmer.", "tokens": ["Gold\u00b7ne", "Schl\u00f6s\u00b7ser", "schei\u00b7nen", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Schatten streichen durch die Hallen,", "tokens": ["Schat\u00b7ten", "strei\u00b7chen", "durch", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Lichter sonst einfallen,", "tokens": ["Wo", "die", "Lich\u00b7ter", "sonst", "ein\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Viel Geschlechter ruhen stille,", "tokens": ["Viel", "Ge\u00b7schlech\u00b7ter", "ru\u00b7hen", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also ruhet auch mein Wille,", "tokens": ["Al\u00b7so", "ru\u00b7het", "auch", "mein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich suchte, ist vergessen,", "tokens": ["Was", "ich", "such\u00b7te", ",", "ist", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "All mein Hoffen war vermessen.", "tokens": ["All", "mein", "Hof\u00b7fen", "war", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Was geboren ist verloren,", "tokens": ["Was", "ge\u00b7bo\u00b7ren", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was verloren ist erkoren", "tokens": ["Was", "ver\u00b7lo\u00b7ren", "ist", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVPP", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem stets neuenden Geschicke,", "tokens": ["Dem", "stets", "neu\u00b7en\u00b7den", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wo irren unsre Blicke! \u2013", "tokens": ["Und", "wo", "ir\u00b7ren", "uns\u00b7re", "Bli\u00b7cke", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wir liebten, die verstorben,", "tokens": ["Ach", "wir", "lieb\u00b7ten", ",", "die", "ver\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Vorzeit unverdorben.", "tokens": ["Ei\u00b7ne", "Vor\u00b7zeit", "un\u00b7ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wehe, wen ", "tokens": ["We\u00b7he", ",", "wen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PWS"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Seine Zeit geht ihm verloren!", "tokens": ["Sei\u00b7ne", "Zeit", "geht", "ihm", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehe, wer nicht bessre kannte,", "tokens": ["We\u00b7he", ",", "wer", "nicht", "bess\u00b7re", "kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PTKNEG", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Lebenslicht verbrannte!", "tokens": ["Und", "sein", "Le\u00b7bens\u00b7licht", "ver\u00b7brann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wehe, wer die bessre kannte,", "tokens": ["We\u00b7he", ",", "wer", "die", "bess\u00b7re", "kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der vom Paradies Verbannte.", "tokens": ["Der", "vom", "Pa\u00b7ra\u00b7dies", "Ver\u00b7bann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nun er aus die Arme breitet,", "tokens": ["Nun", "er", "aus", "die", "Ar\u00b7me", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Bild noch sehnlich schreitet,", "tokens": ["Nach", "dem", "Bild", "noch", "sehn\u00b7lich", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehet er in Wolkenleere,", "tokens": ["Ste\u00b7het", "er", "in", "Wol\u00b7ken\u00b7lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niedersenket ihn die Schwere;", "tokens": ["Nie\u00b7der\u00b7sen\u00b7ket", "ihn", "die", "Schwe\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was geschieht, wer kann's verstehen,", "tokens": ["Was", "ge\u00b7schieht", ",", "wer", "kann's", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer kann durch den Panzer sehen.", "tokens": ["Wer", "kann", "durch", "den", "Pan\u00b7zer", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Schicksal in dem Eisenkleide,", "tokens": ["Schick\u00b7sal", "in", "dem", "Ei\u00b7sen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Winkst du mir zu neuem Leide?", "tokens": ["Winkst", "du", "mir", "zu", "neu\u00b7em", "Lei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welchen Brief willst du mir geben?", "tokens": ["Wel\u00b7chen", "Brief", "willst", "du", "mir", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ich auch das Siegel heben?", "tokens": ["Kann", "ich", "auch", "das", "Sie\u00b7gel", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Starre nicht mein Aug' gebrochen,", "tokens": ["Star\u00b7re", "nicht", "mein", "Aug'", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja der Brief ist nun erbrochen.", "tokens": ["Ja", "der", "Brief", "ist", "nun", "er\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bin ich nicht im Traum gewesen,", "tokens": ["Bin", "ich", "nicht", "im", "Traum", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wer kann im Traume lesen,", "tokens": ["Und", "wer", "kann", "im", "Trau\u00b7me", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihn behalten ist ein Wunder,", "tokens": ["Ihn", "be\u00b7hal\u00b7ten", "ist", "ein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Funken schl\u00e4gt er in den Zunder;", "tokens": ["Fun\u00b7ken", "schl\u00e4gt", "er", "in", "den", "Zun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flamme ist der Sonnenatmosph\u00e4re", "tokens": ["Flam\u00b7me", "ist", "der", "Son\u00b7ne\u00b7nat\u00b7mo\u00b7sph\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Durchbruch in die ird'sche Leere.", "tokens": ["Durch\u00b7bruch", "in", "die", "ird'\u00b7sche", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ohne Ausflucht, ohne Hoffen", "tokens": ["Oh\u00b7ne", "Aus\u00b7flucht", ",", "oh\u00b7ne", "Hof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag im Traum der Lehrbrief offen,", "tokens": ["Lag", "im", "Traum", "der", "Lehr\u00b7brief", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz auf wei\u00df, es stand darinnen:", "tokens": ["Schwarz", "auf", "wei\u00df", ",", "es", "stand", "da\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwenn in Andacht Thr\u00e4nen rinnen,", "tokens": ["\u00bb", "wenn", "in", "An\u00b7dacht", "Thr\u00e4\u00b7nen", "rin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thr\u00e4nen in die Arbeit rinnen,", "tokens": ["Thr\u00e4\u00b7nen", "in", "die", "Ar\u00b7beit", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag sie wohl den Preis gewinnen.\u00ab", "tokens": ["Mag", "sie", "wohl", "den", "Preis", "ge\u00b7win\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "\u00bbschwei\u00df vom Angesicht der Treuen", "tokens": ["\u00bb", "schwei\u00df", "vom", "An\u00b7ge\u00b7sicht", "der", "Treu\u00b7en"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird die d\u00fcrre Erd' erneuen,", "tokens": ["Wird", "die", "d\u00fcr\u00b7re", "Erd'", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ner als der Thau in Bl\u00fcthen", "tokens": ["Sch\u00f6\u00b7ner", "als", "der", "Thau", "in", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frischet er die Schwerbem\u00fchten,", "tokens": ["Fri\u00b7schet", "er", "die", "Schwer\u00b7be\u00b7m\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dies schmerzliche Geb\u00e4ren", "tokens": ["Und", "dies", "schmerz\u00b7li\u00b7che", "Ge\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4\u00dft die Liebe nicht verj\u00e4hren.\u00ab", "tokens": ["L\u00e4\u00dft", "die", "Lie\u00b7be", "nicht", "ver\u00b7j\u00e4h\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "\u00bbnicht zu Wasser wird die Erde,", "tokens": ["\u00bb", "nicht", "zu", "Was\u00b7ser", "wird", "die", "Er\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie ganz zu Feuer werde,", "tokens": ["Da\u00df", "sie", "ganz", "zu", "Feu\u00b7er", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Opfert sie sich zum Kometen,", "tokens": ["Op\u00b7fert", "sie", "sich", "zum", "Ko\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wird mit Drohn die Ferne r\u00f6then,", "tokens": ["Wird", "mit", "Drohn", "die", "Fer\u00b7ne", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kreutzend durch des Nordlichts Strahlen,", "tokens": ["Kreut\u00b7zend", "durch", "des", "Nord\u00b7lichts", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoch erh\u00f6het unter Qualen.\u00ab", "tokens": ["Hoch", "er\u00b7h\u00f6\u00b7het", "un\u00b7ter", "Qua\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie so gerne flie\u00dft ihr Thr\u00e4nen,", "tokens": ["Wie", "so", "ger\u00b7ne", "flie\u00dft", "ihr", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "F\u00fchlt zur Freiheit himmlisch Sehnen,", "tokens": ["F\u00fchlt", "zur", "Frei\u00b7heit", "himm\u00b7lisch", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gott aus Wasser schuf die Erde,", "tokens": ["Gott", "aus", "Was\u00b7ser", "schuf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df zu Wasser alles werde,", "tokens": ["Da\u00df", "zu", "Was\u00b7ser", "al\u00b7les", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PIS", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alles sinket drin in Schimmer,", "tokens": ["Al\u00b7les", "sin\u00b7ket", "drin", "in", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Goldne Schl\u00f6sser scheinen Tr\u00fcmmer.", "tokens": ["Gold\u00b7ne", "Schl\u00f6s\u00b7ser", "schei\u00b7nen", "Tr\u00fcm\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Schatten streichen durch die Hallen,", "tokens": ["Schat\u00b7ten", "strei\u00b7chen", "durch", "die", "Hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Lichter sonst einfallen,", "tokens": ["Wo", "die", "Lich\u00b7ter", "sonst", "ein\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Viel Geschlechter ruhen stille,", "tokens": ["Viel", "Ge\u00b7schlech\u00b7ter", "ru\u00b7hen", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Also ruhet auch mein Wille,", "tokens": ["Al\u00b7so", "ru\u00b7het", "auch", "mein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was ich suchte, ist vergessen,", "tokens": ["Was", "ich", "such\u00b7te", ",", "ist", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "All mein Hoffen war vermessen.", "tokens": ["All", "mein", "Hof\u00b7fen", "war", "ver\u00b7mes\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Was geboren ist verloren,", "tokens": ["Was", "ge\u00b7bo\u00b7ren", "ist", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VVPP", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was verloren ist erkoren", "tokens": ["Was", "ver\u00b7lo\u00b7ren", "ist", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVPP", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem stets neuenden Geschicke,", "tokens": ["Dem", "stets", "neu\u00b7en\u00b7den", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wo irren unsre Blicke! \u2013", "tokens": ["Und", "wo", "ir\u00b7ren", "uns\u00b7re", "Bli\u00b7cke", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach wir liebten, die verstorben,", "tokens": ["Ach", "wir", "lieb\u00b7ten", ",", "die", "ver\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Vorzeit unverdorben.", "tokens": ["Ei\u00b7ne", "Vor\u00b7zeit", "un\u00b7ver\u00b7dor\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wehe, wen ", "tokens": ["We\u00b7he", ",", "wen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "PWS"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Seine Zeit geht ihm verloren!", "tokens": ["Sei\u00b7ne", "Zeit", "geht", "ihm", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wehe, wer nicht bessre kannte,", "tokens": ["We\u00b7he", ",", "wer", "nicht", "bess\u00b7re", "kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PTKNEG", "VVFIN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und sein Lebenslicht verbrannte!", "tokens": ["Und", "sein", "Le\u00b7bens\u00b7licht", "ver\u00b7brann\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wehe, wer die bessre kannte,", "tokens": ["We\u00b7he", ",", "wer", "die", "bess\u00b7re", "kann\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "ART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der vom Paradies Verbannte.", "tokens": ["Der", "vom", "Pa\u00b7ra\u00b7dies", "Ver\u00b7bann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nun er aus die Arme breitet,", "tokens": ["Nun", "er", "aus", "die", "Ar\u00b7me", "brei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem Bild noch sehnlich schreitet,", "tokens": ["Nach", "dem", "Bild", "noch", "sehn\u00b7lich", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stehet er in Wolkenleere,", "tokens": ["Ste\u00b7het", "er", "in", "Wol\u00b7ken\u00b7lee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Niedersenket ihn die Schwere;", "tokens": ["Nie\u00b7der\u00b7sen\u00b7ket", "ihn", "die", "Schwe\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Was geschieht, wer kann's verstehen,", "tokens": ["Was", "ge\u00b7schieht", ",", "wer", "kann's", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWS", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wer kann durch den Panzer sehen.", "tokens": ["Wer", "kann", "durch", "den", "Pan\u00b7zer", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.16": {"line.1": {"text": "Schicksal in dem Eisenkleide,", "tokens": ["Schick\u00b7sal", "in", "dem", "Ei\u00b7sen\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Winkst du mir zu neuem Leide?", "tokens": ["Winkst", "du", "mir", "zu", "neu\u00b7em", "Lei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welchen Brief willst du mir geben?", "tokens": ["Wel\u00b7chen", "Brief", "willst", "du", "mir", "ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann ich auch das Siegel heben?", "tokens": ["Kann", "ich", "auch", "das", "Sie\u00b7gel", "he\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Starre nicht mein Aug' gebrochen,", "tokens": ["Star\u00b7re", "nicht", "mein", "Aug'", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ja der Brief ist nun erbrochen.", "tokens": ["Ja", "der", "Brief", "ist", "nun", "er\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "VAFIN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Bin ich nicht im Traum gewesen,", "tokens": ["Bin", "ich", "nicht", "im", "Traum", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wer kann im Traume lesen,", "tokens": ["Und", "wer", "kann", "im", "Trau\u00b7me", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ihn behalten ist ein Wunder,", "tokens": ["Ihn", "be\u00b7hal\u00b7ten", "ist", "ein", "Wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Funken schl\u00e4gt er in den Zunder;", "tokens": ["Fun\u00b7ken", "schl\u00e4gt", "er", "in", "den", "Zun\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Flamme ist der Sonnenatmosph\u00e4re", "tokens": ["Flam\u00b7me", "ist", "der", "Son\u00b7ne\u00b7nat\u00b7mo\u00b7sph\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Durchbruch in die ird'sche Leere.", "tokens": ["Durch\u00b7bruch", "in", "die", "ird'\u00b7sche", "Lee\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Ohne Ausflucht, ohne Hoffen", "tokens": ["Oh\u00b7ne", "Aus\u00b7flucht", ",", "oh\u00b7ne", "Hof\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "KOUI", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lag im Traum der Lehrbrief offen,", "tokens": ["Lag", "im", "Traum", "der", "Lehr\u00b7brief", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz auf wei\u00df, es stand darinnen:", "tokens": ["Schwarz", "auf", "wei\u00df", ",", "es", "stand", "da\u00b7rin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00bbwenn in Andacht Thr\u00e4nen rinnen,", "tokens": ["\u00bb", "wenn", "in", "An\u00b7dacht", "Thr\u00e4\u00b7nen", "rin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "APPR", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Thr\u00e4nen in die Arbeit rinnen,", "tokens": ["Thr\u00e4\u00b7nen", "in", "die", "Ar\u00b7beit", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mag sie wohl den Preis gewinnen.\u00ab", "tokens": ["Mag", "sie", "wohl", "den", "Preis", "ge\u00b7win\u00b7nen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "\u00bbschwei\u00df vom Angesicht der Treuen", "tokens": ["\u00bb", "schwei\u00df", "vom", "An\u00b7ge\u00b7sicht", "der", "Treu\u00b7en"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADJD", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird die d\u00fcrre Erd' erneuen,", "tokens": ["Wird", "die", "d\u00fcr\u00b7re", "Erd'", "er\u00b7neu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ner als der Thau in Bl\u00fcthen", "tokens": ["Sch\u00f6\u00b7ner", "als", "der", "Thau", "in", "Bl\u00fc\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Frischet er die Schwerbem\u00fchten,", "tokens": ["Fri\u00b7schet", "er", "die", "Schwer\u00b7be\u00b7m\u00fch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und dies schmerzliche Geb\u00e4ren", "tokens": ["Und", "dies", "schmerz\u00b7li\u00b7che", "Ge\u00b7b\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "L\u00e4\u00dft die Liebe nicht verj\u00e4hren.\u00ab", "tokens": ["L\u00e4\u00dft", "die", "Lie\u00b7be", "nicht", "ver\u00b7j\u00e4h\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "\u00bbnicht zu Wasser wird die Erde,", "tokens": ["\u00bb", "nicht", "zu", "Was\u00b7ser", "wird", "die", "Er\u00b7de", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "APPR", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie ganz zu Feuer werde,", "tokens": ["Da\u00df", "sie", "ganz", "zu", "Feu\u00b7er", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Opfert sie sich zum Kometen,", "tokens": ["Op\u00b7fert", "sie", "sich", "zum", "Ko\u00b7me\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Wird mit Drohn die Ferne r\u00f6then,", "tokens": ["Wird", "mit", "Drohn", "die", "Fer\u00b7ne", "r\u00f6\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Kreutzend durch des Nordlichts Strahlen,", "tokens": ["Kreut\u00b7zend", "durch", "des", "Nord\u00b7lichts", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hoch erh\u00f6het unter Qualen.\u00ab", "tokens": ["Hoch", "er\u00b7h\u00f6\u00b7het", "un\u00b7ter", "Qua\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}