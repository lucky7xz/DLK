{"dta.poem.7697": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir kam ein Freund, den ich nicht sah in langen Jahren,", "tokens": ["Mir", "kam", "ein", "Freund", ",", "den", "ich", "nicht", "sah", "in", "lan\u00b7gen", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der hatte nichts von mir, ich nichts von ihm erfahren.", "tokens": ["Der", "hat\u00b7te", "nichts", "von", "mir", ",", "ich", "nichts", "von", "ihm", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PIS", "APPR", "PPER", "$,", "PPER", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nun gieng er ohne da\u00df er viel von mir erfuhr,", "tokens": ["Nun", "gieng", "er", "oh\u00b7ne", "da\u00df", "er", "viel", "von", "mir", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil er von sich allein mich lie\u00df erfahren nur.", "tokens": ["Weil", "er", "von", "sich", "al\u00b7lein", "mich", "lie\u00df", "er\u00b7fah\u00b7ren", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRF", "ADV", "PPER", "VVFIN", "VVINF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es war ihm offenbar viel minder um mein Leben", "tokens": ["Es", "war", "ihm", "of\u00b7fen\u00b7bar", "viel", "min\u00b7der", "um", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu thun, als Kunde mir vom seinigen zu geben.", "tokens": ["Zu", "thun", ",", "als", "Kun\u00b7de", "mir", "vom", "sei\u00b7ni\u00b7gen", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "NN", "PPER", "APPRART", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So hat er denn von mir in Wahrheit nichts bekommen;", "tokens": ["So", "hat", "er", "denn", "von", "mir", "in", "Wahr\u00b7heit", "nichts", "be\u00b7kom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "NN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich habe, was von ihm zu brauchen war, genommen.", "tokens": ["Ich", "ha\u00b7be", ",", "was", "von", "ihm", "zu", "brau\u00b7chen", "war", ",", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PRELS", "APPR", "PPER", "PTKZU", "VVINF", "VAFIN", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}