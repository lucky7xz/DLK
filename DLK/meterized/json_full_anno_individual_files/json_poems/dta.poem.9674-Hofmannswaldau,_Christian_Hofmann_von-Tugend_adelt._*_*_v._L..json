{"dta.poem.9674": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tugend adelt.  \n * * v. L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gar viel edler ist ein edler/ den begl\u00e4ntzet tugendschein/", "tokens": ["Gar", "viel", "ed\u00b7ler", "ist", "ein", "ed\u00b7ler", "/", "den", "be\u00b7gl\u00e4nt\u00b7zet", "tu\u00b7gend\u00b7schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "VAFIN", "ART", "ADJA", "$(", "ART", "VVFIN", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Als der/ bey dem/ statt der tugend/ alte wapen m\u00fcssen seyn.", "tokens": ["Als", "der", "/", "bey", "dem", "/", "statt", "der", "tu\u00b7gend", "/", "al\u00b7te", "wa\u00b7pen", "m\u00fcs\u00b7sen", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "$(", "APPR", "ART", "$(", "APPR", "ART", "NN", "$(", "ADJA", "VVINF", "VMFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}