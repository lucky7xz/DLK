{"textgrid.poem.67155": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Komm! ins Offene, Freund! zwar gl\u00e4nzt ein Weniges heute", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm! ins Offene, Freund! zwar gl\u00e4nzt ein Weniges heute", "tokens": ["Komm", "!", "ins", "Of\u00b7fe\u00b7ne", ",", "Freund", "!", "zwar", "gl\u00e4nzt", "ein", "We\u00b7ni\u00b7ges", "heu\u00b7te"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPRART", "NN", "$,", "NN", "$.", "ADV", "VVFIN", "ART", "PIS", "ADV"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Nur herunter und eng schlie\u00dfet der Himmel uns ein.", "tokens": ["Nur", "her\u00b7un\u00b7ter", "und", "eng", "schlie\u00b7\u00dfet", "der", "Him\u00b7mel", "uns", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "KON", "ADJD", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Weder die Berge sind noch aufgegangen des Waldes", "tokens": ["We\u00b7der", "die", "Ber\u00b7ge", "sind", "noch", "auf\u00b7ge\u00b7gan\u00b7gen", "des", "Wal\u00b7des"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "ART", "NN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Gipfel nach Wunsch und leer ruht von Gesange die Luft.", "tokens": ["Gip\u00b7fel", "nach", "Wunsch", "und", "leer", "ruht", "von", "Ge\u00b7san\u00b7ge", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "ADJD", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "+--+-++--+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Tr\u00fcb ists heut, es schlummern die G\u00e4ng und die Gassen und fast will", "tokens": ["Tr\u00fcb", "ists", "heut", ",", "es", "schlum\u00b7mern", "die", "G\u00e4ng", "und", "die", "Gas\u00b7sen", "und", "fast", "will"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "$,", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "KON", "ADV", "VMFIN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.6": {"text": "Mir es scheinen, es sei, als in der bleiernen Zeit.", "tokens": ["Mir", "es", "schei\u00b7nen", ",", "es", "sei", ",", "als", "in", "der", "blei\u00b7er\u00b7nen", "Zeit", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Dennoch gelinget der Wunsch, Rechtglaubige zweifeln an ", "tokens": ["Den\u00b7noch", "ge\u00b7lin\u00b7get", "der", "Wunsch", ",", "Recht\u00b7glau\u00b7bi\u00b7ge", "zwei\u00b7feln", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Stunde nicht und der Lust bleibe geweihet der Tag.", "tokens": ["Stun\u00b7de", "nicht", "und", "der", "Lust", "blei\u00b7be", "ge\u00b7wei\u00b7het", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "KON", "ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Denn nicht wenig erfreut, was wir vom Himmel gewonnen,", "tokens": ["Denn", "nicht", "we\u00b7nig", "er\u00b7freut", ",", "was", "wir", "vom", "Him\u00b7mel", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADJD", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Wenn ers weigert und doch g\u00f6nnet den Kindern zuletzt.", "tokens": ["Wenn", "ers", "wei\u00b7gert", "und", "doch", "g\u00f6n\u00b7net", "den", "Kin\u00b7dern", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "KON", "ADV", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Nur da\u00df solcher Reden und auch der Schritt' und der M\u00fche", "tokens": ["Nur", "da\u00df", "sol\u00b7cher", "Re\u00b7den", "und", "auch", "der", "Schritt'", "und", "der", "M\u00fc\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIAT", "NN", "KON", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Wert der Gewinn und ganz wahr das Erg\u00f6tzliche sei.", "tokens": ["Wert", "der", "Ge\u00b7winn", "und", "ganz", "wahr", "das", "Er\u00b7g\u00f6tz\u00b7li\u00b7che", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "ADV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.13": {"text": "Darum hoff ich sogar, es werde, wenn das Gew\u00fcnschte", "tokens": ["Da\u00b7rum", "hoff", "ich", "so\u00b7gar", ",", "es", "wer\u00b7de", ",", "wenn", "das", "Ge\u00b7w\u00fcnschte"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.14": {"text": "Wir beginnen und erst unsere Zunge gel\u00f6st,", "tokens": ["Wir", "be\u00b7gin\u00b7nen", "und", "erst", "un\u00b7se\u00b7re", "Zun\u00b7ge", "ge\u00b7l\u00f6st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Und gefunden das Wort, und aufgegangen das Herz ist,", "tokens": ["Und", "ge\u00b7fun\u00b7den", "das", "Wort", ",", "und", "auf\u00b7ge\u00b7gan\u00b7gen", "das", "Herz", "ist", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "ART", "NN", "$,", "KON", "VVPP", "ART", "NN", "VAFIN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Und von trunkener Stirn h\u00f6her Besinnen entspringt,", "tokens": ["Und", "von", "trun\u00b7ke\u00b7ner", "Stirn", "h\u00f6\u00b7her", "Be\u00b7sin\u00b7nen", "ent\u00b7springt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.17": {"text": "Mit der unsern zugleich des Himmels Bl\u00fcte beginnen,", "tokens": ["Mit", "der", "un\u00b7sern", "zu\u00b7gleich", "des", "Him\u00b7mels", "Bl\u00fc\u00b7te", "be\u00b7gin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.18": {"text": "Und dem offenen Blick offen der Leuchtende sein.", "tokens": ["Und", "dem", "of\u00b7fe\u00b7nen", "Blick", "of\u00b7fen", "der", "Leuch\u00b7ten\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ADJD", "ART", "NN", "VAINF", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Denn nicht M\u00e4chtiges ists, zum Leben aber geh\u00f6rt es,", "tokens": ["Denn", "nicht", "M\u00e4ch\u00b7ti\u00b7ges", "ists", ",", "zum", "Le\u00b7ben", "a\u00b7ber", "ge\u00b7h\u00f6rt", "es", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "NN", "VAFIN", "$,", "APPRART", "NN", "ADV", "VVFIN", "PPER", "$,"], "meter": "--+-++-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was wir wollen, und scheint schicklich und freudig zugleich.", "tokens": ["Was", "wir", "wol\u00b7len", ",", "und", "scheint", "schick\u00b7lich", "und", "freu\u00b7dig", "zu\u00b7gleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VMFIN", "$,", "KON", "VVFIN", "ADJD", "KON", "ADJD", "ADV", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Aber kommen doch auch der segenbringenden Schwalben", "tokens": ["A\u00b7ber", "kom\u00b7men", "doch", "auch", "der", "se\u00b7gen\u00b7brin\u00b7gen\u00b7den", "Schwal\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "+-+--+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Immer einige noch, ehe der Sommer, ins Land.", "tokens": ["Im\u00b7mer", "ei\u00b7ni\u00b7ge", "noch", ",", "e\u00b7he", "der", "Som\u00b7mer", ",", "ins", "Land", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIS", "ADV", "$,", "KOUS", "ART", "NN", "$,", "APPRART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "N\u00e4mlich droben zu weihn bei guter Rede den Boden,", "tokens": ["N\u00e4m\u00b7lich", "dro\u00b7ben", "zu", "weihn", "bei", "gu\u00b7ter", "Re\u00b7de", "den", "Bo\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Wo den G\u00e4sten das Haus baut der verst\u00e4ndige Wirt;", "tokens": ["Wo", "den", "G\u00e4s\u00b7ten", "das", "Haus", "baut", "der", "ver\u00b7st\u00e4n\u00b7di\u00b7ge", "Wirt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Da\u00df sie kosten und schaun das Sch\u00f6nste, die F\u00fclle des Landes,", "tokens": ["Da\u00df", "sie", "kos\u00b7ten", "und", "schaun", "das", "Sch\u00f6ns\u00b7te", ",", "die", "F\u00fcl\u00b7le", "des", "Lan\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Da\u00df, wie das Herz es w\u00fcnscht, offen, dem Geiste gem\u00e4\u00df", "tokens": ["Da\u00df", ",", "wie", "das", "Herz", "es", "w\u00fcnscht", ",", "of\u00b7fen", ",", "dem", "Geis\u00b7te", "ge\u00b7m\u00e4\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$,", "ADJD", "$,", "ART", "NN", "ADJD"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Mahl und Tanz und Gesang und Stuttgarts Freude gekr\u00f6nt sei,", "tokens": ["Mahl", "und", "Tanz", "und", "Ge\u00b7sang", "und", "Stutt\u00b7garts", "Freu\u00b7de", "ge\u00b7kr\u00f6nt", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "KON", "NN", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Deshalb wollen wir heut w\u00fcnschend den H\u00fcgel hinauf.", "tokens": ["Des\u00b7halb", "wol\u00b7len", "wir", "heut", "w\u00fcn\u00b7schend", "den", "H\u00fc\u00b7gel", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "M\u00f6g ein Besseres noch das menschenfreundliche Mailicht", "tokens": ["M\u00f6g", "ein", "Bes\u00b7se\u00b7res", "noch", "das", "men\u00b7schen\u00b7freund\u00b7li\u00b7che", "Mai\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+--+-", "measure": "hexameter"}, "line.12": {"text": "Dr\u00fcber sprechen, von selbst bildsamen G\u00e4sten erkl\u00e4rt,", "tokens": ["Dr\u00fc\u00b7ber", "spre\u00b7chen", ",", "von", "selbst", "bild\u00b7sa\u00b7men", "G\u00e4s\u00b7ten", "er\u00b7kl\u00e4rt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVINF", "$,", "APPR", "ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Oder, wie sonst, wenns andern gef\u00e4llt, denn alt ist die Sitte,", "tokens": ["O\u00b7der", ",", "wie", "sonst", ",", "wenns", "an\u00b7dern", "ge\u00b7f\u00e4llt", ",", "denn", "alt", "ist", "die", "Sit\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADV", "$,", "KOUS", "PIS", "VVPP", "$,", "KON", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Und es schauen so oft l\u00e4chelnd die G\u00f6tter auf uns,", "tokens": ["Und", "es", "schau\u00b7en", "so", "oft", "l\u00e4\u00b7chelnd", "die", "G\u00f6t\u00b7ter", "auf", "uns", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "ADJD", "ART", "NN", "APPR", "PPER", "$,"], "meter": "--+--++--+-+-", "measure": "anapaest.di.plus"}, "line.15": {"text": "M\u00f6ge der Zimmermann vom Gipfel des Daches den Spruch tun,", "tokens": ["M\u00f6\u00b7ge", "der", "Zim\u00b7mer\u00b7mann", "vom", "Gip\u00b7fel", "des", "Da\u00b7ches", "den", "Spruch", "tun", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "APPRART", "NN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Wir, so gut es gelang, haben das Unsre getan.", "tokens": ["Wir", ",", "so", "gut", "es", "ge\u00b7lang", ",", "ha\u00b7ben", "das", "Uns\u00b7re", "ge\u00b7tan", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$,", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aber sch\u00f6n ist der Ort, wenn in Feiertagen des Fr\u00fchlings", "tokens": ["A\u00b7ber", "sch\u00f6n", "ist", "der", "Ort", ",", "wenn", "in", "Fei\u00b7er\u00b7ta\u00b7gen", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "KOUS", "APPR", "NN", "ART", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Aufgegangen das Tal, wenn mit dem Neckar herab", "tokens": ["Auf\u00b7ge\u00b7gan\u00b7gen", "das", "Tal", ",", "wenn", "mit", "dem", "Ne\u00b7ckar", "her\u00b7ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "APPR", "ART", "NN", "ADV"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Weiden gr\u00fcnend und Wald und all die gr\u00fcnenden B\u00e4ume", "tokens": ["Wei\u00b7den", "gr\u00fc\u00b7nend", "und", "Wald", "und", "all", "die", "gr\u00fc\u00b7nen\u00b7den", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "NN", "KON", "PIAT", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Zahllos, bl\u00fchend wei\u00df, wallen in wiegender Luft,", "tokens": ["Zahl\u00b7los", ",", "bl\u00fc\u00b7hend", "wei\u00df", ",", "wal\u00b7len", "in", "wie\u00b7gen\u00b7der", "Luft", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Aber mit W\u00f6lkchen bedeckt an Bergen herunter der Weinstock", "tokens": ["A\u00b7ber", "mit", "W\u00f6lk\u00b7chen", "be\u00b7deckt", "an", "Ber\u00b7gen", "her\u00b7un\u00b7ter", "der", "Wein\u00b7stock"], "token_info": ["word", "word", "word", "word", "word", "word", "