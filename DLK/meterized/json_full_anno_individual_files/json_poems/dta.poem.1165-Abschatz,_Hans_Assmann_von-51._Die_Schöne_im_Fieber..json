{"dta.poem.1165": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "51. Die Sch\u00f6ne im Fieber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Hitze lob' ich zwar/ die deine Brust entz\u00fcndet/ ", "tokens": ["Die", "Hit\u00b7ze", "lob'", "ich", "zwar", "/", "die", "dei\u00b7ne", "Brust", "ent\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$(", "ART", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die nat\u00fcrliche Bewegung dir verr\u00fcckt/", "tokens": ["Und", "die", "na\u00b7t\u00fcr\u00b7li\u00b7che", "Be\u00b7we\u00b7gung", "dir", "ver\u00b7r\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch glaube/ da\u00df sich nicht darauff mein W\u00fcnschen gr\u00fcndet/", "tokens": ["Doch", "glau\u00b7be", "/", "da\u00df", "sich", "nicht", "dar\u00b7auff", "mein", "W\u00fcn\u00b7schen", "gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PRF", "PTKNEG", "PAV", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df du mit solcher Qual solt lange seyn bestrickt.", "tokens": ["Da\u00df", "du", "mit", "sol\u00b7cher", "Qual", "solt", "lan\u00b7ge", "seyn", "be\u00b7strickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "VMFIN", "ADV", "VAINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn man dich aber in der Hitze sch\u00f6ner findet/", "tokens": ["Wenn", "man", "dich", "a\u00b7ber", "in", "der", "Hit\u00b7ze", "sch\u00f6\u00b7ner", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "APPR", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wenn dein mattes Aug\u2019 uns lindre Stralen schickt/", "tokens": ["Und", "wenn", "dein", "mat\u00b7tes", "Aug'", "uns", "lind\u00b7re", "Stra\u00b7len", "schickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So dencke/ da\u00df man thut/ worzu Natur verbindet/", "tokens": ["So", "den\u00b7cke", "/", "da\u00df", "man", "thut", "/", "wor\u00b7zu", "Na\u00b7tur", "ver\u00b7bin\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PIS", "VVFIN", "$(", "PWAV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sich selber g\u00f6nnt und hofft mehr so zu seyn begl\u00fcckt.", "tokens": ["Sich", "sel\u00b7ber", "g\u00f6nnt", "und", "hofft", "mehr", "so", "zu", "seyn", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "KON", "VVFIN", "ADV", "ADV", "PTKZU", "VAINF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Beklaget sich dein Mund/ gehn Seufftzer von dem Hertzen/", "tokens": ["Be\u00b7kla\u00b7get", "sich", "dein", "Mund", "/", "gehn", "Seufft\u00b7zer", "von", "dem", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$(", "VVFIN", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Thut dir verborgner Brand in allen Adern weh/", "tokens": ["Thut", "dir", "ver\u00b7borg\u00b7ner", "Brand", "in", "al\u00b7len", "A\u00b7dern", "weh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "APPR", "PIAT", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So hoff\u2019 ich Beyleid auch von dir bey gleichen Schmertzen.", "tokens": ["So", "hoff'", "ich", "Bey\u00b7leid", "auch", "von", "dir", "bey", "glei\u00b7chen", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADV", "APPR", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Kan ich in Hitz und Frost dich ungescheut verehren/", "tokens": ["Kan", "ich", "in", "Hitz", "und", "Frost", "dich", "un\u00b7ge\u00b7scheut", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "KON", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So mag die gantze Welt mir zum Vergn\u00fcgen h\u00f6ren:", "tokens": ["So", "mag", "die", "gant\u00b7ze", "Welt", "mir", "zum", "Ver\u00b7gn\u00fc\u00b7gen", "h\u00f6\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich lieb und hoffe stets/ es gehe wie es geh.", "tokens": ["Ich", "lieb", "und", "hof\u00b7fe", "stets", "/", "es", "ge\u00b7he", "wie", "es", "geh."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["PPER", "ADJD", "KON", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "KOKOM", "PPER", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}