{"textgrid.poem.33547": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00f6n, wie auf griechischen Inseln der Tag,", "tokens": ["Sch\u00f6n", ",", "wie", "auf", "grie\u00b7chi\u00b7schen", "In\u00b7seln", "der", "Tag", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "W\u00e4re mit dir mir das Leben,", "tokens": ["W\u00e4\u00b7re", "mit", "dir", "mir", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Doch nur dem Mut, der das K\u00fchnste vermag,", "tokens": ["Doch", "nur", "dem", "Mut", ",", "der", "das", "K\u00fchns\u00b7te", "ver\u00b7mag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wird auch die Bl\u00fcte, die sch\u00f6nste, gegeben.", "tokens": ["Wird", "auch", "die", "Bl\u00fc\u00b7te", ",", "die", "sch\u00f6ns\u00b7te", ",", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Mitten im Sturm, der die V\u00f6lker zerw\u00fchlt,", "tokens": ["Mit\u00b7ten", "im", "Sturm", ",", "der", "die", "V\u00f6l\u00b7