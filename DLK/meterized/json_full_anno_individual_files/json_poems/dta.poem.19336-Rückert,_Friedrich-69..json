{"dta.poem.19336": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Sohn, das Ehrgef\u00fchl ist eine Umgestaltung", "tokens": ["Mein", "Sohn", ",", "das", "Ehr\u00b7ge\u00b7f\u00fchl", "ist", "ei\u00b7ne", "Um\u00b7ge\u00b7stal\u00b7tung"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vom allgemeinen Trieb des Lebens, Selbsterhaltung.", "tokens": ["Vom", "all\u00b7ge\u00b7mei\u00b7nen", "Trieb", "des", "Le\u00b7bens", ",", "Selbs\u00b7ter\u00b7hal\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir f\u00fchlen unser Seyn gesteigert und gemehrt,", "tokens": ["Wir", "f\u00fch\u00b7len", "un\u00b7ser", "Seyn", "ge\u00b7stei\u00b7gert", "und", "ge\u00b7mehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem wir anerkannt uns sehen und geehrt,", "tokens": ["In\u00b7dem", "wir", "an\u00b7er\u00b7kannt", "uns", "se\u00b7hen", "und", "ge\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPER", "VVINF", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und m\u00f6gen billig bis von uns erworbne Leben", "tokens": ["Und", "m\u00f6\u00b7gen", "bil\u00b7lig", "bis", "von", "uns", "er\u00b7worb\u00b7ne", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADJD", "APPR", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertheidigen sogut wie das uns Gott gegeben.", "tokens": ["Ver\u00b7thei\u00b7di\u00b7gen", "so\u00b7gut", "wie", "das", "uns", "Gott", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "KOKOM", "PDS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}