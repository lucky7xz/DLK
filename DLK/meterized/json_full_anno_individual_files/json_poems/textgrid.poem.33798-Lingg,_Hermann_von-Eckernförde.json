{"textgrid.poem.33798": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Eckernf\u00f6rde", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie, wenn noch so alt ich werde,", "tokens": ["Nie", ",", "wenn", "noch", "so", "alt", "ich", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie vergess' ich jenen Tag,", "tokens": ["Nie", "ver\u00b7ge\u00b7ss'", "ich", "je\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jenen Tag von Eckernf\u00f6rde,", "tokens": ["Je\u00b7nen", "Tag", "von", "E\u00b7ckern\u00b7f\u00f6r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der D\u00e4nenstolz erlag;", "tokens": ["Da", "der", "D\u00e4\u00b7nen\u00b7stolz", "er\u00b7lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Am Gr\u00fcndonnerstag des Jahrs", "tokens": ["Am", "Gr\u00fcn\u00b7don\u00b7ners\u00b7tag", "des", "Jahrs"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Achtzehnhundertneunundvierzig war's.", "tokens": ["Acht\u00b7zehn\u00b7hun\u00b7dert\u00b7neun\u00b7und\u00b7vier\u00b7zig", "wa\u00b7r'", "s."], "token_info": ["word", "word", "abbreviation"], "pos": ["CARD", "XY", "XY"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Lustig war das Meer und lachte,", "tokens": ["Lus\u00b7tig", "war", "das", "Meer", "und", "lach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Segel voller Hohn", "tokens": ["Und", "die", "Se\u00b7gel", "vol\u00b7ler", "Hohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bl\u00e4hte Christian der Achte", "tokens": ["Bl\u00e4h\u00b7te", "Chris\u00b7ti\u00b7an", "der", "Ach\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die sch\u00f6ne Gefion.", "tokens": ["Und", "die", "sch\u00f6\u00b7ne", "Ge\u00b7fi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Er ein Kriegschiff stolz und reich,", "tokens": ["Er", "ein", "Krieg\u00b7schiff", "stolz", "und", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Sie der Morgenr\u00f6te gleich.", "tokens": ["Sie", "der", "Mor\u00b7gen\u00b7r\u00f6\u00b7te", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Gefion fing an zu tanzen,", "tokens": ["Ge\u00b7fi\u00b7on", "fing", "an", "zu", "tan\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Aber das bekam ihr schlecht;", "tokens": ["A\u00b7ber", "das", "be\u00b7kam", "ihr", "schlecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Musik von unsern Schanzen", "tokens": ["Die", "Mu\u00b7sik", "von", "un\u00b7sern", "Schan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Spielte ihr den Takt nicht recht.", "tokens": ["Spiel\u00b7te", "ihr", "den", "Takt", "nicht", "recht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, mein Herre Christian!", "tokens": ["Ach", ",", "mein", "Her\u00b7re", "Chris\u00b7ti\u00b7an", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Fing sie laut zu jammern an.", "tokens": ["Fing", "sie", "laut", "zu", "jam\u00b7mern", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, die deutschen Kugeln rei\u00dfen", "tokens": ["Ach", ",", "die", "deut\u00b7schen", "Ku\u00b7geln", "rei\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mir vom Leib mein Prachtgewand!", "tokens": ["Mir", "vom", "Leib", "mein", "Pracht\u00b7ge\u00b7wand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag nicht l\u00e4nger d\u00e4nisch hei\u00dfen,", "tokens": ["Mag", "nicht", "l\u00e4n\u00b7ger", "d\u00e4\u00b7nisch", "hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ADJD", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eilig fl\u00fccht' ich an den Strand;", "tokens": ["Ei\u00b7lig", "fl\u00fccht'", "ich", "an", "den", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Deutsche M\u00e4nner, seid mir hold,", "tokens": ["Deut\u00b7sche", "M\u00e4n\u00b7ner", ",", "seid", "mir", "hold", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Kleidet mich in Schwarzrotgold!", "tokens": ["Klei\u00b7det", "mich", "in", "Schwarz\u00b7rot\u00b7gold", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Wie die Wimpel lustig flogen,", "tokens": ["Wie", "die", "Wim\u00b7pel", "lus\u00b7tig", "flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als man ihr das Banner gab!", "tokens": ["Als", "man", "ihr", "das", "Ban\u00b7ner", "gab", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es wurden glatt die Wogen,", "tokens": ["Doch", "es", "wur\u00b7den", "glatt", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Farben bla\u00dften ab;", "tokens": ["Und", "die", "Far\u00b7ben", "bla\u00df\u00b7ten", "ab", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lang vor D\u00e4nen\u00fcbermut", "tokens": ["Lang", "vor", "D\u00e4\u00b7nen\u00b7\u00fc\u00b7ber\u00b7mut"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Hat des R\u00e4chers Schwert geruht.", "tokens": ["Hat", "des", "R\u00e4\u00b7chers", "Schwert", "ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Trittst du wieder aus der Wolke,", "tokens": ["Trittst", "du", "wie\u00b7der", "aus", "der", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deutscher Freiheit Morgenrot?", "tokens": ["Deut\u00b7scher", "Frei\u00b7heit", "Mor\u00b7gen\u00b7rot", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem einig gro\u00dfen Volke", "tokens": ["In", "dem", "ei\u00b7nig", "gro\u00b7\u00dfen", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Flammt ein m\u00e4chtig Aufgebot;", "tokens": ["Flammt", "ein", "m\u00e4ch\u00b7tig", "Auf\u00b7ge\u00b7bot", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "F\u00fcr das Recht mit Herz und Hand", "tokens": ["F\u00fcr", "das", "Recht", "mit", "Herz", "und", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Loht empor ein Flammenbrand.", "tokens": ["Loht", "em\u00b7por", "ein", "Flam\u00b7men\u00b7brand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Leuchte wie das Julfestfeuer", "tokens": ["Leuch\u00b7te", "wie", "das", "Jul\u00b7fest\u00b7feu\u00b7er"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die lange Nordlandsnacht!", "tokens": ["Durch", "die", "lan\u00b7ge", "Nord\u00b7lands\u00b7nacht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6r es, Bruderstamm, getreuer,", "tokens": ["H\u00f6r", "es", ",", "Bru\u00b7der\u00b7stamm", ",", "ge\u00b7treu\u00b7er", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "$,", "NN", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Tag ist auferwacht,", "tokens": ["Un\u00b7ser", "Tag", "ist", "au\u00b7fer\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und gez\u00fcckt ist schon das Schwert,", "tokens": ["Und", "ge\u00b7z\u00fcckt", "ist", "schon", "das", "Schwert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.6": {"text": "Das von euch die Knechtung wehrt.", "tokens": ["Das", "von", "euch", "die", "Knech\u00b7tung", "wehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Und zur\u00fcck nicht in die Scheide", "tokens": ["Und", "zu\u00b7r\u00fcck", "nicht", "in", "die", "Schei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll es fallen, bis ger\u00e4cht,", "tokens": ["Soll", "es", "fal\u00b7len", ",", "bis", "ge\u00b7r\u00e4cht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis gerettet sind die Eide,", "tokens": ["Bis", "ge\u00b7ret\u00b7tet", "sind", "die", "Ei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und gerettet ist das Recht;", "tokens": ["Und", "ge\u00b7ret\u00b7tet", "ist", "das", "Recht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcber Arglist und Verrat", "tokens": ["\u00dc\u00b7ber", "Arg\u00b7list", "und", "Ver\u00b7rat"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht den gro\u00dfen Schwur zur Tat!", "tokens": ["Macht", "den", "gro\u00b7\u00dfen", "Schwur", "zur", "Tat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nie, wenn noch so alt ich werde,", "tokens": ["Nie", ",", "wenn", "noch", "so", "alt", "ich", "wer\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ADV", "ADV", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nie vergess' ich jenen Tag,", "tokens": ["Nie", "ver\u00b7ge\u00b7ss'", "ich", "je\u00b7nen", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jenen Tag von Eckernf\u00f6rde,", "tokens": ["Je\u00b7nen", "Tag", "von", "E\u00b7ckern\u00b7f\u00f6r\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da der D\u00e4nenstolz erlag;", "tokens": ["Da", "der", "D\u00e4\u00b7nen\u00b7stolz", "er\u00b7lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Am Gr\u00fcndonnerstag des Jahrs", "tokens": ["Am", "Gr\u00fcn\u00b7don\u00b7ners\u00b7tag", "d