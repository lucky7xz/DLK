{"dta.poem.3939": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "29) Als sein Hertzens-Freund  Renandes  \n ihm eine plaudernde Magd abgemahlet,  \n hat solche sch\u00f6ne Zuschrifft in seinem Werck der  \n  Autor  erl\u00e4utert und einger\u00fcckt.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Plaudernde Tasche!", "tokens": ["Plau\u00b7dern\u00b7de", "Ta\u00b7sche", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sch\u00e4ndliche Wasche!", "tokens": ["Sch\u00e4nd\u00b7li\u00b7che", "Wa\u00b7sche", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Sage mir doch,", "tokens": ["Sa\u00b7ge", "mir", "doch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Was vor ein Loch", "tokens": ["Was", "vor", "ein", "Loch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Du dir erwehlet,", "tokens": ["Du", "dir", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Da du erzehlet", "tokens": ["Da", "du", "er\u00b7zeh\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Was du nur weist,", "tokens": ["Was", "du", "nur", "weist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Dazu beschmeist", "tokens": ["Da\u00b7zu", "be\u00b7schmeist"], "token_info": ["word", "word"], "pos": ["PAV", "VVFIN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Mit groben L\u00fcgen,", "tokens": ["Mit", "gro\u00b7ben", "L\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Die von dir fliegen.", "tokens": ["Die", "von", "dir", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Frauen und Herrn,", "tokens": ["Frau\u00b7en", "und", "Herrn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.12": {"text": "Das thust du gern.", "tokens": ["Das", "thust", "du", "gern", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "Du kommst nicht wieder,", "tokens": ["Du", "kommst", "nicht", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "He\u00dflich Gefieder!", "tokens": ["He\u00df\u00b7lich", "Ge\u00b7fie\u00b7der", "!"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.15": {"text": "Gifftiger Gaul,", "tokens": ["Giff\u00b7ti\u00b7ger", "Gaul", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "B\u00e4ndge das Maul!", "tokens": ["B\u00e4nd\u00b7ge", "das", "Maul", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.17": {"text": "Du b\u00f6se Klinge,", "tokens": ["Du", "b\u00f6\u00b7se", "Klin\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Nimmst dein Gedinge,", "tokens": ["Nimmst", "dein", "Ge\u00b7din\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Jssest dein Brod,", "tokens": ["Js\u00b7sest", "dein", "Brod", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.20": {"text": "Leidest nicht Noth,", "tokens": ["Lei\u00b7dest", "nicht", "Noth", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.21": {"text": "L\u00e4ssest die Sachen,", "tokens": ["L\u00e4s\u00b7sest", "die", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.22": {"text": "Welche zu machen,", "tokens": ["Wel\u00b7che", "zu", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "PTKZU", "VVINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Alle im Stich,", "tokens": ["Al\u00b7le", "im", "Stich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.24": {"text": "L\u00e4sterst nur mich,", "tokens": ["L\u00e4s\u00b7terst", "nur", "mich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.25": {"text": "Wenn ich was sage,", "tokens": ["Wenn", "ich", "was", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Sch\u00e4ndliche Plage!", "tokens": ["Sch\u00e4nd\u00b7li\u00b7che", "Pla\u00b7ge", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "He\u00dfliches Thier!", "tokens": ["He\u00df\u00b7li\u00b7ches", "Thier", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.28": {"text": "Siehe nur hier,", "tokens": ["Sie\u00b7he", "nur", "hier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Nichts wird gescheuret,", "tokens": ["Nichts", "wird", "ge\u00b7scheu\u00b7ret", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Alles verleuret", "tokens": ["Al\u00b7les", "ver\u00b7leu\u00b7ret"], "token_info": ["word", "word"], "pos": ["PIS", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Durch dich den Glantz,", "tokens": ["Durch", "dich", "den", "Glantz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.32": {"text": "Und modert gantz.", "tokens": ["Und", "mo\u00b7dert", "gantz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Was ich dir sage,", "tokens": ["Was", "ich", "dir", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.34": {"text": "Und schrifftlich klage,", "tokens": ["Und", "schrifft\u00b7lich", "kla\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.35": {"text": "Was ich geklagt,", "tokens": ["Was", "ich", "ge\u00b7klagt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.36": {"text": "Von meiner Magd,", "tokens": ["Von", "mei\u00b7ner", "Magd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Kennst du die lose?", "tokens": ["Kennst", "du", "die", "lo\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.38": {"text": "Die alte Rose.", "tokens": ["Die", "al\u00b7te", "Ro\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}