{"dta.poem.1951": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Uberschrifften und Sinn-  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Arm will meinen Leib/ dein Mund mein Hertz u", "tokens": ["Dein", "Arm", "will", "mei\u00b7nen", "Leib", "/", "dein", "Mund", "mein", "Hertz", "u"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schl\u00fcssen/", "tokens": ["schl\u00fcs\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und deine Rechte sich bey meiner Rechten wissen/", "tokens": ["Und", "dei\u00b7ne", "Rech\u00b7te", "sich", "bey", "mei\u00b7ner", "Rech\u00b7ten", "wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Im Fall noch k\u00f6mmt darzu der K\u00fcsse Freundligkeit/", "tokens": ["Im", "Fall", "noch", "k\u00f6mmt", "dar\u00b7zu", "der", "K\u00fcs\u00b7se", "Freund\u00b7lig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PAV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So heissestu mit Recht ein Indas unsrer Zeit.", "tokens": ["So", "heis\u00b7se\u00b7stu", "mit", "Recht", "e