{"dta.poem.471": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Der Hahnrey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Das Weib geh\u00f6rt mir zu/ ein ander bricht die Fr\u00fcchte/", "tokens": ["Das", "Weib", "ge\u00b7h\u00f6rt", "mir", "zu", "/", "ein", "an\u00b7der", "bricht", "die", "Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKZU", "$(", "ART", "ADJD", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So macht ihr Bienen nicht vor euch eur s\u00fc\u00df Gerichte.", "tokens": ["So", "macht", "ihr", "Bie\u00b7nen", "nicht", "vor", "euch", "eur", "s\u00fc\u00df", "Ge\u00b7rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Kinder sind mein Werck ehrt man den andern gleich", "tokens": ["Die", "Kin\u00b7der", "sind", "mein", "Werck", "ehrt", "man", "den", "an\u00b7dern", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "VVFIN", "PIS", "ART", "ADJA", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So heckt ihr Vogel auch die jungen nicht vor euch.", "tokens": ["So", "heckt", "ihr", "Vo\u00b7gel", "auch", "die", "jun\u00b7gen", "nicht", "vor", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NE", "ADV", "ART", "ADJA", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}