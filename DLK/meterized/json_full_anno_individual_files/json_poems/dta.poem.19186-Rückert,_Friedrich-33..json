{"dta.poem.19186": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol ist im Saamenkorn die Pflanze schon enthalten,", "tokens": ["Wol", "ist", "im", "Saa\u00b7men\u00b7korn", "die", "Pflan\u00b7ze", "schon", "ent\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Doch siehst du's ihm nicht an, wie sie sich wird entfalten.", "tokens": ["Doch", "siehst", "du's", "ihm", "nicht", "an", ",", "wie", "sie", "sich", "wird", "ent\u00b7fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "PTKNEG", "PTKVZ", "$,", "PWAV", "PPER", "PRF", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Viel gr\u00f6\u00dfer als der Kern des Apfels ist die Bohne,", "tokens": ["Viel", "gr\u00f6\u00b7\u00dfer", "als", "der", "Kern", "des", "Ap\u00b7fels", "ist", "die", "Boh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch Ranken gibt sie nur, er eines Baumes Krone.", "tokens": ["Doch", "Ran\u00b7ken", "gibt", "sie", "nur", ",", "er", "ei\u00b7nes", "Bau\u00b7mes", "Kro\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "p