{"dta.poem.8658": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Einem Knaben .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Was trauerst du, mein sch\u00f6ner Junge?", "tokens": ["Was", "trau\u00b7erst", "du", ",", "mein", "sch\u00f6\u00b7ner", "Jun\u00b7ge", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du Armer, sprich, was weinst du so?", "tokens": ["Du", "Ar\u00b7mer", ",", "sprich", ",", "was", "weinst", "du", "so", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADJD", "$,", "PWS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df treulos dir im raschen Schwunge", "tokens": ["Da\u00df", "treu\u00b7los", "dir", "im", "ra\u00b7schen", "Schwun\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein liebes V\u00f6gelein entfloh?", "tokens": ["Dein", "lie\u00b7bes", "V\u00f6\u00b7ge\u00b7lein", "ent\u00b7floh", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du blickest bald in deiner Trauer", "tokens": ["Du", "bli\u00b7ckest", "bald", "in", "dei\u00b7ner", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hin\u00fcber dort nach jenem Baum,", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "dort", "nach", "je\u00b7nem", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald wieder nach dem leeren Bauer", "tokens": ["Bald", "wie\u00b7der", "nach", "dem", "lee\u00b7ren", "Bau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Blickst du in deinem Kindestraum.", "tokens": ["Blickst", "du", "in", "dei\u00b7nem", "Kin\u00b7des\u00b7traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du legst so schlaff die kleinen H\u00e4nde", "tokens": ["Du", "legst", "so", "schlaff", "die", "klei\u00b7nen", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An deines Lieblings \u00f6des Haus;", "tokens": ["An", "dei\u00b7nes", "Lieb\u00b7lings", "\u00f6\u00b7des", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und pr\u00fcfest rings die Sprossenw\u00e4nde,", "tokens": ["Und", "pr\u00fc\u00b7fest", "rings", "die", "Spros\u00b7sen\u00b7w\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und fragst: \u201ewie kam er nur hinaus?\u201c", "tokens": ["Und", "fragst", ":", "\u201e", "wie", "kam", "er", "nur", "hin\u00b7aus", "?", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "An jenem Baume h\u00f6rst du singen", "tokens": ["An", "je\u00b7nem", "Bau\u00b7me", "h\u00f6rst", "du", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Fernen, den dein Herz verlor,", "tokens": ["Den", "Fer\u00b7nen", ",", "den", "dein", "Herz", "ver\u00b7lor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und unaufhaltsam eilig dringen", "tokens": ["Und", "un\u00b7auf\u00b7halt\u00b7sam", "ei\u00b7lig", "drin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die hei\u00dfen Thr\u00e4nen dir hervor.", "tokens": ["Die", "hei\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "dir", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gib acht, gib acht, o lieber Knabe,", "tokens": ["Gib", "acht", ",", "gib", "acht", ",", "o", "lie\u00b7ber", "Kna\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "$,", "VVIMP", "CARD", "$,", "FM", "ADV", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du nicht dastehst traurend einst,", "tokens": ["Da\u00df", "du", "nicht", "das\u00b7tehst", "trau\u00b7rend", "einst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und um die beste, sch\u00f6nste Habe", "tokens": ["Und", "um", "die", "bes\u00b7te", ",", "sch\u00f6ns\u00b7te", "Ha\u00b7be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Menschenlebens bitter weinst!", "tokens": ["Des", "Men\u00b7schen\u00b7le\u00b7bens", "bit\u00b7ter", "weinst", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da\u00df du die Hand, die sturmerprobte,", "tokens": ["Da\u00df", "du", "die", "Hand", ",", "die", "stur\u00b7mer\u00b7prob\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht legst, ein Mann an deine Brust,", "tokens": ["Nicht", "legst", ",", "ein", "Mann", "an", "dei\u00b7ne", "Brust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Darin so mancher Schmerz dir tobte,", "tokens": ["Da\u00b7rin", "so", "man\u00b7cher", "Schmerz", "dir", "tob\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir s\u00e4uselte so manche Lust;", "tokens": ["Dir", "s\u00e4u\u00b7sel\u00b7te", "so", "man\u00b7che", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da\u00df du die Hand in wildem Krampfe", "tokens": ["Da\u00df", "du", "die", "Hand", "in", "wil\u00b7dem", "Kramp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht dr\u00fcckst in deinen Busen ein,", "tokens": ["Nicht", "dr\u00fcckst", "in", "dei\u00b7nen", "Bu\u00b7sen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus dem die Unschuld dir im Kampfe", "tokens": ["Aus", "dem", "die", "Un\u00b7schuld", "dir", "im", "Kamp\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Entflohn, das scheue V\u00f6gelein.", "tokens": ["Ent\u00b7flohn", ",", "das", "scheu\u00b7e", "V\u00f6\u00b7ge\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Dann h\u00f6rst du fl\u00fcstern ihre leisen", "tokens": ["Dann", "h\u00f6rst", "du", "fl\u00fcs\u00b7tern", "ih\u00b7re", "lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ges\u00e4nge aus der Ferne her;", "tokens": ["Ge\u00b7s\u00e4n\u00b7ge", "aus", "der", "Fer\u00b7ne", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Neigst hin dich nach den s\u00fc\u00dfen Weisen;", "tokens": ["Neigst", "hin", "dich", "nach", "den", "s\u00fc\u00b7\u00dfen", "Wei\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das V\u00f6glein aber kehrt nicht mehr! \u2014", "tokens": ["Das", "V\u00f6\u00b7glein", "a\u00b7ber", "kehrt", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PTKNEG", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}