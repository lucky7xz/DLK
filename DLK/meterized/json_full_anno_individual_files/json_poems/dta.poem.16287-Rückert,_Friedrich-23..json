{"dta.poem.16287": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn einen Teller mehr hat auf den Tisch gesetzt", "tokens": ["Wenn", "ei\u00b7nen", "Tel\u00b7ler", "mehr", "hat", "auf", "den", "Tisch", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hausfrau, als am Tisch sich finden G\u00e4ste jetzt;", "tokens": ["Die", "Haus\u00b7frau", ",", "als", "am", "Tisch", "sich", "fin\u00b7den", "G\u00e4s\u00b7te", "jetzt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "APPRART", "NN", "PRF", "VVINF", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "So r\u00e4ume sie nur nicht den Teller wieder ein!", "tokens": ["So", "r\u00e4u\u00b7me", "sie", "nur", "nicht", "den", "Tel\u00b7ler", "wie\u00b7der", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein hungeriger Gast wird auf dem Wege seyn.", "tokens": ["Ein", "hun\u00b7ge\u00b7ri\u00b7ger", "Gast", "wird", "auf", "dem", "We\u00b7ge", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.h