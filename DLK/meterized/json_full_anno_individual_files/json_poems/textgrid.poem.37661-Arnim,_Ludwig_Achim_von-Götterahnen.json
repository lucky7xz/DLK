er": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zur Erde, der Sklavin, sie sehn,", "tokens": ["Zur", "Er\u00b7de", ",", "der", "Skla\u00b7vin", ",", "sie", "sehn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ART", "NN", "$,", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Junggesellen sie bleiben,", "tokens": ["Doch", "Jung\u00b7ge\u00b7sel\u00b7len", "sie", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Einander im Wege sie stehn.", "tokens": ["Ein\u00b7an\u00b7der", "im", "We\u00b7ge", "sie", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.19": {"line.1": {"text": "Sie halten Feuer in H\u00e4nden", "tokens": ["Sie", "hal\u00b7ten", "Feu\u00b7er", "in", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und theilen's der Liebenden aus,", "tokens": ["Und", "thei\u00b7len's", "der", "Lie\u00b7ben\u00b7den", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Doch ehe die Buhlschaft sie enden,", "tokens": ["Doch", "e\u00b7he", "die", "Buhl\u00b7schaft", "sie", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Fast gehet das Feuer schon aus.", "tokens": ["Fast",