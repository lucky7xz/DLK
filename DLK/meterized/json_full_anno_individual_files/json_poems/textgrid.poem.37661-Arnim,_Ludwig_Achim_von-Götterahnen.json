s"}}, "stanza.23": {"line.1": {"text": "Es brennen die heiligen Heerde", "tokens": ["Es", "bren\u00b7nen", "die", "hei\u00b7li\u00b7gen", "Heer\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und Sonne und Mond da erscheint,", "tokens": ["Und", "Son\u00b7ne", "und", "Mond", "da", "er\u00b7scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "ADV", "VVFIN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Erheben sich schnell von der Erde,", "tokens": ["Er\u00b7he\u00b7ben", "sich", "schnell", "von", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Erde gar bitterlich weint.", "tokens": ["Die", "Er\u00b7de", "gar", "bit\u00b7ter\u00b7lich", "weint", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.24": {"line.1": {"text": "Als sie das beide erblicket,", "tokens": ["Als", "sie", "das", "bei\u00b7de", "er\u00b7bli\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da sehen die Freunde sich an,", "tokens": ["Da", "se\u00b7hen", "die", "Freun\u00b7de", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Und steigen, die Hand sich zu dr\u00fccken,", "tokens": ["Und", "stei\u00b7gen", ",", "die", "Hand", "sich", "zu", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Wie feurige Kugeln hinan.", "tokens": ["Wie", "feu\u00b7ri\u00b7ge", "Ku\u00b7geln", "hi\u00b7nan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.25": {"line.1": {"text": "Und st\u00fcrzen nun hin auf die Erde,", "tokens": ["Und", "st\u00fcr\u00b7zen", "nun", "hin", "auf", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu ruhen im Schoo\u00dfe der Braut,", "tokens": ["Zu", "ru\u00b7hen", "im", "Schoo\u00b7\u00dfe", "der", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es fanden sie Hirten der Heerde,", "tokens": ["Es", "fan\u00b7den", "sie", "Hir\u00b7ten", "der", "Heer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "In Tempeln die Hohen ihr schaut.", "tokens": ["In", "Tem\u00b7peln", "die", "Ho\u00b7hen", "ihr", "schaut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "PPER", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.26": {"line.1": {"text": "Sie haben nicht Augen und sehen,", "tokens": ["Sie", "ha\u00b7ben", "nicht", "Au\u00b7gen", "und", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "KON", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie f\u00fchlen und haben kein Blut,", "tokens": ["Sie", "f\u00fch\u00b7len", "und", "ha\u00b7ben", "kein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Wir f\u00fchlen sie ungesehen,", "tokens": ["Wir", "f\u00fch\u00b7len", "sie", "un\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch thut uns der Anblick so gut.", "tokens": ["Doch", "thut", "uns", "der", "An\u00b7blick", "so", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.27": {"line.1": {"text": "Sie scheinen nun allen gelinder", "tokens": ["Sie", "schei\u00b7nen", "nun", "al\u00b7len", "ge\u00b7lin\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und sind wie Steine erstarrt,", "tokens": ["Und", "sind", "wie", "Stei\u00b7ne", "er\u00b7starrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch Ernst nur erziehet die Kinder,", "tokens": ["Doch", "Ernst", "nur", "er\u00b7zie\u00b7het", "die", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Ein g\u00f6ttliches Leben macht hart.", "tokens": ["Ein", "g\u00f6tt\u00b7li\u00b7ches", "Le\u00b7ben", "macht", "hart", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.28": {"line.1": {"text": "Der Erde Kinder vor ihnen", "tokens": ["Der", "Er\u00b7de", "Kin\u00b7der", "vor", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "APPR", "PPER"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ergreifet die kindliche Zeit,", "tokens": ["Er\u00b7grei\u00b7fet", "die", "kind\u00b7li\u00b7che", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Eh Ungl\u00fcck der Liebe erschienen,", "tokens": ["Eh", "Un\u00b7gl\u00fcck", "der", "Lie\u00b7be", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Eh Fr\u00fchling sie wieder befreit.", "tokens": ["Eh", "Fr\u00fch\u00b7ling", "sie", "wie\u00b7der", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.29": {"line.1": {"text": "Die Thronen, die leeren, sie steigen", "tokens": ["Die", "Thro\u00b7nen", ",", "die", "lee\u00b7ren", ",", "sie", "stei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "PPER", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Als Sonne, als Mond auf ab,", "tokens": ["Als", "Son\u00b7ne", ",", "als", "Mond", "auf", "ab", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "KOUS", "NN", "APPR", "PTKVZ", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch brennet das Feuer die Reigen", "tokens": ["Noch", "bren\u00b7net", "das", "Feu\u00b7er", "die", "Rei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Von g\u00f6ttlichen Zeiten herab.", "tokens": ["Von", "g\u00f6tt\u00b7li\u00b7chen", "Zei\u00b7ten", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.30": {"line.1": {"text": "Drum wendet euch nicht zu der Sonne,", "tokens": ["Drum", "wen\u00b7det", "euch", "nicht", "zu", "der", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht wendet euch dienend zum Mond,", "tokens": ["Nicht", "wen\u00b7det", "euch", "die\u00b7nend", "zum", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Auf Erden noch stehet besonnen", "tokens": ["Auf", "Er\u00b7den", "noch", "ste\u00b7het", "be\u00b7son\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVFIN", "VVINF"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Gottheit und strafet und lohnt.", "tokens": ["Die", "Got\u00b7theit", "und", "stra\u00b7fet", "und", "lohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}}, "stanza.31": {"line.1": {"text": "Und lie\u00dfen sie nichts vom Geschlechte,", "tokens": ["Und", "lie\u00b7\u00dfen", "sie", "nichts", "vom", "Ge\u00b7schlech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Sie schw\u00e4ngern im Geiste die Welt,", "tokens": ["Sie", "schw\u00e4n\u00b7gern", "im", "Geis\u00b7te", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "All jeden, der schaffet das Rechte", "tokens": ["All", "je\u00b7den", ",", "der", "schaf\u00b7fet", "das", "Rech\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "PIS", "$,", "PRELS", "VVFIN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Als reiner ung\u00f6ttlicher Held.", "tokens": ["Als", "rei\u00b7ner", "un\u00b7g\u00f6tt\u00b7li\u00b7cher", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.32": {"line.1": {"text": "So schwanket die Erde aus Wettern", "tokens": ["So", "schwan\u00b7ket", "die", "Er\u00b7de", "aus", "Wet\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Der dunklen chaotischen Zeit,", "tokens": ["Der", "dunk\u00b7len", "chao\u00b7ti\u00b7schen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Die Andacht erziehet zu G\u00f6ttern", "tokens": ["Die", "An\u00b7dacht", "er\u00b7zie\u00b7het", "zu", "G\u00f6t\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die treulich erleuchtet ihr Leid.", "tokens": ["Die", "treu\u00b7lich", "er\u00b7leuch\u00b7tet", "ihr", "Leid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}