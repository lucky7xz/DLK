{"textgrid.poem.24737": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Werden zu doppelter Lust nun doppelte Tage geboren?", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Werden zu doppelter Lust nun doppelte Tage geboren?", "tokens": ["Wer\u00b7den", "zu", "dop\u00b7pel\u00b7ter", "Lust", "nun", "dop\u00b7pel\u00b7te", "Ta\u00b7ge", "ge\u00b7bo\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADJA", "NN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ehe der eine versank, steigt schon der neue herauf!", "tokens": ["E\u00b7he", "der", "ei\u00b7ne", "ver\u00b7sank", ",", "steigt", "schon", "der", "neu\u00b7e", "her\u00b7auf", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PIS", "VVFIN", "$,", "VVFIN", "ADV", "ART", "ADJA", "PTKVZ", "$."], "meter": "+--+--+----+-+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Herrlich in Salben und Glanz, ged\u00e4chtnislos wie ein Halbgott,", "tokens": ["Herr\u00b7lich", "in", "Sal\u00b7ben", "und", "Glanz", ",", "ge\u00b7d\u00e4cht\u00b7nis\u00b7los", "wie", "ein", "Halb\u00b7gott", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Deckt er mir G\u00e4rten und See zu mit erstarrendem Prunk.", "tokens": ["Deckt", "er", "mir", "G\u00e4r\u00b7ten", "und", "See", "zu", "mit", "er\u00b7star\u00b7ren\u00b7dem", "Prunk", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "NN", "KON", "NN", "APPR", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Und der vertrauliche Baum wird fremd, fremd funkelt der Springbrunn,", "tokens": ["Und", "der", "ver\u00b7trau\u00b7li\u00b7che", "Baum", "wird", "fremd", ",", "fremd", "fun\u00b7kelt", "der", "Spring\u00b7brunn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-+++", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Fremde und dunkle Gewalt dr\u00e4ngt sich von au\u00dfen in mich.", "tokens": ["Frem\u00b7de", "und", "dunk\u00b7le", "Ge\u00b7walt", "dr\u00e4ngt", "sich", "von", "au\u00b7\u00dfen", "in", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVFIN", "PRF", "APPR", "ADV", "APPR", "PPER", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Sind dies die B\u00fcsche, darin die bunten Gedanken genistet?", "tokens": ["Sind", "dies", "die", "B\u00fc\u00b7sche", ",", "da\u00b7rin", "die", "bun\u00b7ten", "Ge\u00b7dan\u00b7ken", "ge\u00b7nis\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ART", "NN", "$,", "PAV", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Kaum mehr erkenn ich die Bank! Die ists? Die lauernde hier?", "tokens": ["Kaum", "mehr", "er\u00b7kenn", "ich", "die", "Bank", "!", "Die", "ists", "?", "Die", "lau\u00b7ern\u00b7de", "hier", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$.", "ART", "NE", "$.", "ART", "ADJA", "ADV", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Aber sie ists, denn im Netz der flei\u00dfigen, winzigen Spinne", "tokens": ["A\u00b7ber", "sie", "ists", ",", "denn", "im", "Netz", "der", "flei\u00b7\u00dfi\u00b7gen", ",", "win\u00b7zi\u00b7gen", "Spin\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KON", "APPRART", "NN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "H\u00e4ngt noch der schimmernde Punkt! Komm ich mir selber zur\u00fcck?", "tokens": ["H\u00e4ngt", "noch", "der", "schim\u00b7mern\u00b7de", "Punkt", "!", "Komm", "ich", "mir", "sel\u00b7ber", "zu\u00b7r\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$.", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Als dein Brief heut kam \u2013 ich ri\u00df mit zu hastigen Fingern", "tokens": ["Als", "dein", "Brief", "heut", "kam", "\u2013", "ich", "ri\u00df", "mit", "zu", "has\u00b7ti\u00b7gen", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "APPR", "ADJA", "NN"], "meter": "--+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Ungeduldig ihn auf \u2013, flogen die Teilchen hinweg", "tokens": ["Un\u00b7ge\u00b7dul\u00b7dig", "ihn", "auf", "\u2013", ",", "flo\u00b7gen", "die", "Teil\u00b7chen", "hin\u00b7weg"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "PPER", "APPR", "$(", "$,", "VVFIN", "ART", "NN", "APZR"], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Von dem zerrissenen Rand: sie spr\u00fchten wie Tropfen dem Trinker,", "tokens": ["Von", "dem", "zer\u00b7ris\u00b7se\u00b7nen", "Rand", ":", "sie", "spr\u00fch\u00b7ten", "wie", "Trop\u00b7fen", "dem", "Trin\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Wenn er zum Springbrunn sich dr\u00e4ngt, um den verd\u00fcrsteten Mund!", "tokens": ["Wenn", "er", "zum", "Spring\u00b7brunn", "sich", "dr\u00e4ngt", ",", "um", "den", "ver\u00b7d\u00fcrs\u00b7te\u00b7ten", "Mund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PRF", "VVFIN", "$,", "KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Ja, jetzt dr\u00e4ngt sichs heran und kommt \u00fcbers Wasser geschwommen,", "tokens": ["Ja", ",", "jetzt", "dr\u00e4ngt", "sichs", "he\u00b7ran", "und", "kommt", "\u00fc\u00b7bers", "Was\u00b7ser", "ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "PIS", "PTKVZ", "KON", "VVFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "--+-++-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Hebt sich mit lieblichem Arm rings aus dem Dunkel zu mir:", "tokens": ["Hebt", "sich", "mit", "lieb\u00b7li\u00b7chem", "Arm", "rings", "aus", "dem", "Dun\u00b7kel", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"