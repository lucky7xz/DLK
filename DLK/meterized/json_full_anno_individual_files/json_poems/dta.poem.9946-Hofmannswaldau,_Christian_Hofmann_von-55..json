{"dta.poem.9946": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein erz\u00fcrnt und sch\u00e4dlich geist wohnt in meinem kleinen leibe/", "tokens": ["Ein", "er\u00b7z\u00fcrnt", "und", "sch\u00e4d\u00b7lich", "geist", "wohnt", "in", "mei\u00b7nem", "klei\u00b7nen", "lei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVPP", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Wo ich immer komme hin/ furcht und schrecken ein ich treibe/", "tokens": ["Wo", "ich", "im\u00b7mer", "kom\u00b7me", "hin", "/", "furcht", "und", "schre\u00b7cken", "ein", "ich", "trei\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVFIN", "PTKVZ", "$(", "NN", "KON", "VVFIN", "ART", "PPER", "VVFIN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Alles r\u00e4um ich aus dem wege/ was verhindern kan und mag;", "tokens": ["Al\u00b7les", "r\u00e4um", "ich", "aus", "dem", "we\u00b7ge", "/", "was", "ver\u00b7hin\u00b7dern", "kan", "und", "mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "PWS", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Ob ich erst schon stille scheine/ knall ich wie ein donnerschlag.", "tokens": ["Ob", "ich", "erst", "schon", "stil\u00b7le", "schei\u00b7ne", "/", "knall", "ich", "wie", "ein", "don\u00b7ner\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJA", "VVFIN", "$(", "VMFIN", "PPER", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}