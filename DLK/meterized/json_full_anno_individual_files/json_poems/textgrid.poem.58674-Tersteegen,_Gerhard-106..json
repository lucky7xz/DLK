{"textgrid.poem.58674": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "106.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz ist ausgedorrt, ohn' Saft und Kraft und Leben,", "tokens": ["Mein", "Herz", "ist", "aus\u00b7ge\u00b7dorrt", ",", "ohn'", "Saft", "und", "Kraft", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gleichwie ein totes Bein, das unempfindlich liegt;", "tokens": ["Gleich\u00b7wie", "ein", "to\u00b7tes", "Bein", ",", "das", "un\u00b7emp\u00b7find\u00b7lich", "liegt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Ohnmacht dringt mich fast, die Hoffnung aufzugeben,", "tokens": ["Die", "Ohn\u00b7macht", "dringt", "mich", "fast", ",", "die", "Hoff\u00b7nung", "auf\u00b7zu\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein abgeschnittnes Bein wohl nimmer Leben kriegt.", "tokens": ["Ein", "ab\u00b7ge\u00b7schnitt\u00b7nes", "Bein", "wohl", "nim\u00b7mer", "Le\u00b7ben", "kriegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Herr, darum hilf mir selbst, weil es mit mir zum Ende,", "tokens": ["Herr", ",", "da\u00b7rum", "hilf", "mir", "selbst", ",", "weil", "es", "mit", "mir", "zum", "En\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PAV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "Hol du mich aus dem Grab, brich alle Todesband',", "tokens": ["Hol", "du", "mich", "aus", "dem", "Grab", ",", "brich", "al\u00b7le", "To\u00b7des\u00b7band'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "NN", "$,", "ADJD", "PIAT", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.7": {"text": "Nur deinen Lebensgeist mit Kraft ins Herz mir sende,", "tokens": ["Nur", "dei\u00b7nen", "Le\u00b7bens\u00b7geist", "mit", "Kraft", "ins", "Herz", "mir", "sen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So leb' ich dann vor dir in dir, mein rechtes Land!", "tokens": ["So", "leb'", "ich", "dann", "vor", "dir", "in", "dir", ",", "mein", "rech\u00b7tes", "Land", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein Herz ist ausgedorrt, ohn' Saft und Kraft und Leben,", "tokens":