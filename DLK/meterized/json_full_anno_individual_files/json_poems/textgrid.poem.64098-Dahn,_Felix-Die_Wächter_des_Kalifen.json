{"textgrid.poem.64098": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Die W\u00e4chter des Kalifen", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlummre furchtlos, mein Gebieter,", "tokens": ["Schlumm\u00b7re", "furcht\u00b7los", ",", "mein", "Ge\u00b7bie\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlafe sicher, o Har\u00fan:", "tokens": ["Schla\u00b7fe", "si\u00b7cher", ",", "o", "Ha\u00b7r\u00fan", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "FM", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wahrlich, deinem heil'gen Haupte", "tokens": ["Wahr\u00b7lich", ",", "dei\u00b7nem", "heil'\u00b7gen", "Haup\u00b7te"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll kein Hasser Leides tun!", "tokens": ["Soll", "kein", "Has\u00b7ser", "Lei\u00b7des", "tun", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn ob deinen Tr\u00e4umen wachen", "tokens": ["Denn", "ob", "dei\u00b7nen", "Tr\u00e4u\u00b7men", "wa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor der T\u00fcr der L\u00f6wen zwei:", "tokens": ["Vor", "der", "T\u00fcr", "der", "L\u00f6\u00b7wen", "zwei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und wer sagt es, wer von beiden", "tokens": ["Und", "wer", "sagt", "es", ",", "wer", "von", "bei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$,", "PWS", "APPR", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treuer oder st\u00e4rker sei? \u2013", "tokens": ["Treu\u00b7er", "o\u00b7der", "st\u00e4r\u00b7ker", "sei", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "KON", "ADJD", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Den Bem\u00e4hnten hat dein scharfes", "tokens": ["Den", "Be\u00b7m\u00e4hn\u00b7ten", "hat", "dein", "schar\u00b7fes"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schwert befreit am W\u00fcstenrand,", "tokens": ["Schwert", "be\u00b7freit", "am", "W\u00fcs\u00b7ten\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die f\u00fcrchterliche Schlange", "tokens": ["Als", "die", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Schlan\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schuppenringig ihn umwand.", "tokens": ["Schup\u00b7pen\u00b7rin\u00b7gig", "ihn", "um\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dankbar hat der W\u00fcstenk\u00f6nig", "tokens": ["Dank\u00b7bar", "hat", "der", "W\u00fcs\u00b7ten\u00b7k\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir zu F\u00fc\u00dfen sich gestreckt", "tokens": ["Dir", "zu", "F\u00fc\u00b7\u00dfen", "sich", "ge\u00b7streckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NN", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und gehorsam wie ein H\u00fcndlein", "tokens": ["Und", "ge\u00b7hor\u00b7sam", "wie", "ein", "H\u00fcnd\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Des Erretters Hand geleckt.", "tokens": ["Des", "Er\u00b7ret\u00b7ters", "Hand", "ge\u00b7leckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Nie mehr von der Ferse wich er", "tokens": ["Nie", "mehr", "von", "der", "Fer\u00b7se", "wich", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dir seither bei Nacht und Tag:", "tokens": ["Dir", "sei\u00b7ther", "bei", "Nacht", "und", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Oft dein Haupt auf seiner weichen", "tokens": ["Oft", "dein", "Haupt", "auf", "sei\u00b7ner", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4hne statt des Pf\u00fchles lag.", "tokens": ["M\u00e4h\u00b7ne", "statt", "des", "Pf\u00fch\u00b7les", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aber Arslan, mich, den zweiten", "tokens": ["A\u00b7ber", "Ars\u00b7lan", ",", "mich", ",", "den", "zwei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NE", "$,", "PPER", "$,", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deiner H\u00fcter, hast du dir", "tokens": ["Dei\u00b7ner", "H\u00fc\u00b7ter", ",", "hast", "du", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fester noch ans Herz gekettet", "tokens": ["Fes\u00b7ter", "noch", "ans", "Herz", "ge\u00b7ket\u00b7tet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Als das k\u00f6nigliche Tier.", "tokens": ["Als", "das", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Tier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dich zu morden, aus Arabien", "tokens": ["Dich", "zu", "mor\u00b7den", ",", "aus", "A\u00b7ra\u00b7bi\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "APPR", "NE"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Hatte mich mein Herr gesandt:", "tokens": ["Hat\u00b7te", "mich", "mein", "Herr", "ge\u00b7sandt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch als ich dein Antlitz schaute,", "tokens": ["Doch", "als", "ich", "dein", "Ant\u00b7litz", "schau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da versagte Dolch und Hand!", "tokens": ["Da", "ver\u00b7sag\u00b7te", "Dolch", "und", "Hand", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Und ich st\u00fcrzte dir zu F\u00fc\u00dfen", "tokens": ["Und", "ich", "st\u00fcrz\u00b7te", "dir", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und gestand den Plan, den Mord:", "tokens": ["Und", "ge\u00b7stand", "den", "Plan", ",", "den", "Mord", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in Flammen sollt' ich sterben", "tokens": ["Und", "in", "Flam\u00b7men", "sollt'", "ich", "ster\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nach der sieben Richter Wort.", "tokens": ["Nach", "der", "sie\u00b7ben", "Rich\u00b7ter", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "CARD", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"lin