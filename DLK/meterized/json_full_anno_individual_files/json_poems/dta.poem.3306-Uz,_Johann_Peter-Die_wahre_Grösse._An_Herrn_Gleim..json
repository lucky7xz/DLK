{"dta.poem.3306": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "Die wahre Gr\u00f6sse.  \n An Herrn Gleim.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "In meinen Adern tobt ein juvenalisch Feuer;", "tokens": ["In", "mei\u00b7nen", "A\u00b7dern", "tobt", "ein", "ju\u00b7ve\u00b7na\u00b7lisch", "Feu\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Unmuth reichet mir die scharfgestimmte Leyer:", "tokens": ["Der", "Un\u00b7muth", "rei\u00b7chet", "mir", "die", "scharf\u00b7ges\u00b7timm\u00b7te", "Le\u00b7yer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Ma\u00dft sich des P\u00f6bels Wahn", "tokens": ["Ma\u00dft", "sich", "des", "P\u00f6\u00b7bels", "Wahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Das Urtheil nicht von grossen Seelen an?", "tokens": ["Das", "Ur\u00b7theil", "nicht", "von", "gros\u00b7sen", "See\u00b7len", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sey Richter, liebster Gleim! der P\u00f6bel soll nicht", "tokens": ["Sey", "Rich\u00b7ter", ",", "liebs\u00b7ter", "Gleim", "!", "der", "P\u00f6\u00b7bel", "soll", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "ADJA", "NN", "$.", "ART", "NN", "VMFIN", "PTKNEG"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "richten,", "tokens": ["rich\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "O du, der iedes Herz mit lieblichen Gedichten", "tokens": ["O", "du", ",", "der", "ie\u00b7des", "Herz", "mit", "lieb\u00b7li\u00b7chen", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach Amors Willen lenkt,", "tokens": ["Nach", "A\u00b7mors", "Wil\u00b7len", "lenkt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der schalkhaft scherzt und frey und edel denkt!", "tokens": ["Der", "schalk\u00b7haft", "scherzt", "und", "frey", "und", "e\u00b7del", "denkt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ein Mann, der gl\u00fccklich k\u00fchn zur h\u00f6chsten W\u00fcr-", "tokens": ["Ein", "Mann", ",", "der", "gl\u00fcck\u00b7lich", "k\u00fchn", "zur", "h\u00f6chs\u00b7ten", "W\u00fcr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "ADJD", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "de flieget,", "tokens": ["de", "flie\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und, weil er Sklaven gleich, vor Grossen sich geschmieget,", "tokens": ["Und", ",", "weil", "er", "Skla\u00b7ven", "gleich", ",", "vor", "Gros\u00b7sen", "sich", "ge\u00b7schmie\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "NN", "ADV", "$,", "APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nun, als ein grosser Mann,", "tokens": ["Nun", ",", "als", "ein", "gros\u00b7ser", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch endlich selbst in Marmor wohnen kann:", "tokens": ["Auch", "end\u00b7lich", "selbst", "in", "Mar\u00b7mor", "woh\u00b7nen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der hei\u00dft beym P\u00f6bel gro\u00df, da ihn sein Herz ver-", "tokens": ["Der", "hei\u00dft", "beym", "P\u00f6\u00b7bel", "gro\u00df", ",", "da", "ihn", "sein", "Herz", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADJD", "$,", "KOUS", "PPER", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "dammet;", "tokens": ["dam\u00b7met", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und wann der B\u00fcrger Gold auf seinem Kleide flammet,", "tokens": ["Und", "wann", "der", "B\u00fcr\u00b7ger", "Gold", "auf", "sei\u00b7nem", "Klei\u00b7de", "flam\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So sieht die Schmeicheley", "tokens": ["So", "sieht", "die", "Schmei\u00b7che\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "F\u00fcr Schimmer nicht, wie klein die Seele sey.", "tokens": ["F\u00fcr", "Schim\u00b7mer", "nicht", ",", "wie", "klein", "die", "See\u00b7le", "sey", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Soll seines Nahmens Ruhm auf sp\u00e4te Nachwelt", "tokens": ["Soll", "sei\u00b7nes", "Nah\u00b7mens", "Ruhm", "auf", "sp\u00e4\u00b7te", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "gr\u00fcnen?", "tokens": ["gr\u00fc\u00b7nen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Dem Staate dient er nur, sich Sch\u00e4tze zu verdienen:", "tokens": ["Dem", "Staa\u00b7te", "dient", "er", "nur", ",", "sich", "Sch\u00e4t\u00b7ze", "zu", "ver\u00b7die\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "PRF", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bereichert ein Verrath,", "tokens": ["Be\u00b7rei\u00b7chert", "ein", "Ver\u00b7rath", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "So, zweifle nicht, verr\u00e4th er auch den Staat.", "tokens": ["So", ",", "zweif\u00b7le", "nicht", ",", "ver\u00b7r\u00e4\u00b7th", "er", "auch", "den", "Staat", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Absicht Niedrigkeit erniedrigt grosse Thaten:", "tokens": ["Der", "Ab\u00b7sicht", "Nied\u00b7rig\u00b7keit", "er\u00b7nied\u00b7rigt", "gros\u00b7se", "Tha\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wem Geiz und Ruhmbegier auch Herculs Werke rathen,", "tokens": ["Wem", "Geiz", "und", "Ruhm\u00b7be\u00b7gier", "auch", "Her\u00b7culs", "Wer\u00b7ke", "ra\u00b7then", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "ADV", "NE", "NN", "VVFIN", "$,"], "meter": "-+-+-+-++-+--", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Der hei\u00dft vergebens gro\u00df:", "tokens": ["Der", "hei\u00dft", "ver\u00b7ge\u00b7bens", "gro\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er schwingt sich nie vom Staub des P\u00f6bels los.", "tokens": ["Er", "schwingt", "sich", "nie", "vom", "Staub", "des", "P\u00f6\u00b7bels", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Zeuch, Alexander! hin bis zu den braunen Scythen;", "tokens": ["Zeuch", ",", "A\u00b7lex\u00b7an\u00b7der", "!", "hin", "bis", "zu", "den", "brau\u00b7nen", "Scyt\u00b7hen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "$.", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jrr um den tr\u00e4gen Phrat, wo heissre Sonnen w\u00fcthen,", "tokens": ["Jrr", "um", "den", "tr\u00e4\u00b7gen", "Phrat", ",", "wo", "heiss\u00b7re", "Son\u00b7nen", "w\u00fct\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und rei\u00df dein murrend Heer", "tokens": ["Und", "rei\u00df", "dein", "mur\u00b7rend", "Heer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zum Ganges hin, bis ans entfernte Meer!", "tokens": ["Zum", "Gan\u00b7ges", "hin", ",", "bis", "ans", "ent\u00b7fern\u00b7te", "Meer", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$,", "KOUS", "APPRART", "ADJA", "NN", "$."], "meter": "-+-----+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Du k\u00e4mpfest \u00fcberall und siegest, wo du k\u00e4mpfest,", "tokens": ["Du", "k\u00e4mp\u00b7fest", "\u00fc\u00b7be\u00b7rall", "und", "sie\u00b7gest", ",", "wo", "du", "k\u00e4mp\u00b7fest", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bis du der Barbarn Stolz, voll gr\u00f6ssern Stolzes, d\u00e4mp-", "tokens": ["Bis", "du", "der", "Bar\u00b7barn", "Stolz", ",", "voll", "gr\u00f6s\u00b7sern", "Stol\u00b7zes", ",", "d\u00e4mp"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "ADJD", "ADJA", "NN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "fest,", "tokens": ["fest", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-", "measure": "single.down"}, "line.4": {"text": "Und die verheerte Welt", "tokens": ["Und", "die", "ver\u00b7heer\u00b7te", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor ihrem Feind gefesselt niederf\u00e4llt.", "tokens": ["Vor", "ih\u00b7rem", "Feind", "ge\u00b7fes\u00b7selt", "nie\u00b7der\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Doch la\u00df dich immerhin der Menschheit nicht erbar-", "tokens": ["Doch", "la\u00df", "dich", "im\u00b7mer\u00b7hin", "der", "Menschheit", "nicht", "er\u00b7ba\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ART", "NN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "men!", "tokens": ["men", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Von deinem Haupte reisst, auch in des Sieges Armen,", "tokens": ["Von", "dei\u00b7nem", "Haup\u00b7te", "reisst", ",", "auch", "in", "des", "Sie\u00b7ges", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Tugend rauhe Hand", "tokens": ["Der", "Tu\u00b7gend", "rau\u00b7he", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die Lorbeern ab, die Ehrsucht ihr entwandt.", "tokens": ["Die", "Lor\u00b7beern", "ab", ",", "die", "Ehr\u00b7sucht", "ihr", "ent\u00b7wandt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Mit Lorbeern wird von ihr der bessre Held bekr\u00e4nzet,", "tokens": ["Mit", "Lor\u00b7beern", "wird", "von", "ihr", "der", "bess\u00b7re", "Held", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der f\u00fcr das Vaterland in furchtbarn Waffen gl\u00e4nzet,", "tokens": ["Der", "f\u00fcr", "das", "Va\u00b7ter\u00b7land", "in", "furcht\u00b7barn", "Waf\u00b7fen", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und \u00fcber Feinde siegt,", "tokens": ["Und", "\u00fc\u00b7ber", "Fein\u00b7de", "siegt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Feinde sucht, nicht unbeleidigt kriegt:", "tokens": ["Nicht", "Fein\u00b7de", "sucht", ",", "nicht", "un\u00b7be\u00b7lei\u00b7digt", "kriegt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "VVFIN", "$,", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Der Weise, der voll Muths, wann Aberglaube", "tokens": ["Der", "Wei\u00b7se", ",", "der", "voll", "Muths", ",", "wann", "A\u00b7berg\u00b7lau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "$,", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "schrecket,", "tokens": ["schre\u00b7cket", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und Wahn die halbe Welt mit schwarzen Fl\u00fcgeln decket,", "tokens": ["Und", "Wahn", "die", "hal\u00b7be", "Welt", "mit", "schwar\u00b7zen", "Fl\u00fc\u00b7geln", "de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein die Wahrheit ehrt,", "tokens": ["Al\u00b7lein", "die", "Wahr\u00b7heit", "ehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "lin