{"dta.poem.9844": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "O. F. v. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aber ihr verliebten ihr/", "tokens": ["A\u00b7ber", "ihr", "ver\u00b7lieb\u00b7ten", "ihr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Last die tugend euch f\u00fcr allen/", "tokens": ["Last", "die", "tu\u00b7gend", "euch", "f\u00fcr", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "APPR", "PIAT", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht des leibes eitle zier/", "tokens": ["Nicht", "des", "lei\u00b7bes", "eit\u00b7le", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht die wollust wohlgefallen/", "tokens": ["Nicht", "die", "wol\u00b7lust", "wohl\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Last die tugend blo\u00df allein", "tokens": ["Last", "die", "tu\u00b7gend", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Eures hertzens endzweck seyn.", "tokens": ["Eu\u00b7res", "hert\u00b7zens", "end\u00b7zweck", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nheit/ die uns haute ziert/", "tokens": ["Sch\u00f6n\u00b7heit", "/", "die", "uns", "hau\u00b7te", "ziert", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "PRELS", "PPER", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird vor morgen hingerissen/", "tokens": ["Wird", "vor", "mor\u00b7gen", "hin\u00b7ge\u00b7ris\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ADV", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn die einmahl sich verliehrt/", "tokens": ["Wenn", "die", "ein\u00b7mahl", "sich", "ver\u00b7liehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "PRF", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df sie allen glantz vermissen/", "tokens": ["Mu\u00df", "sie", "al\u00b7len", "glantz", "ver\u00b7mis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die abgewelckte pracht", "tokens": ["Und", "die", "ab\u00b7ge\u00b7welck\u00b7te", "pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird von aller welt verlacht.", "tokens": ["Wird", "von", "al\u00b7ler", "welt", "ver\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Nur die tugend wird allein", "tokens": ["Nur", "die", "tu\u00b7gend", "wird", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem tode nicht vergessen/", "tokens": ["Nach", "dem", "to\u00b7de", "nicht", "ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jhrer gaben hoher schein", "tokens": ["Ih\u00b7rer", "ga\u00b7ben", "ho\u00b7her", "schein"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird durch keine zeit gefressen;", "tokens": ["Wird", "durch", "kei\u00b7ne", "zeit", "ge\u00b7fres\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist sie gleich auch noch so alt/", "tokens": ["Ist", "sie", "gleich", "auch", "noch", "so", "alt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird sie doch nicht ungestalt.", "tokens": ["Wird", "sie", "doch", "nicht", "un\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Frommer himmel/ la\u00df mich doch", "tokens": ["From\u00b7mer", "him\u00b7mel", "/", "la\u00df", "mich", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$(", "VVIMP", "PPER", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses grosse gl\u00fcck erleben:", "tokens": 