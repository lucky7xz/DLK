{"textgrid.poem.33051": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ob ich dich liebe, wolltest du mich fragen \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ob ich dich liebe, wolltest du mich fragen \u2013", "tokens": ["Ob", "ich", "dich", "lie\u00b7be", ",", "woll\u00b7test", "du", "mich", "fra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was ich liebe, will ich treu dir sagen:", "tokens": ["Und", "was", "ich", "lie\u00b7be", ",", "will", "ich", "treu", "dir", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Bl\u00fcmchen lieb' ich, das die w\u00fcrz'gen D\u00fcfte", "tokens": ["Das", "Bl\u00fcm\u00b7chen", "lieb'", "ich", ",", "das", "die", "w\u00fcrz'\u00b7gen", "D\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ausstreuet in die lauen Fr\u00fchlingsl\u00fcfte,", "tokens": ["Aus\u00b7streu\u00b7et", "in", "die", "lau\u00b7en", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und doch sich tief verbirgt im dunklen Moos \u2013", "tokens": ["Und", "doch", "sich", "tief", "ver\u00b7birgt", "im", "dunk\u00b7len", "Moos", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "ADJD", "VVFIN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein Auge sieht der Heimath stillen Schoo\u00df.", "tokens": ["Kein", "Au\u00b7ge", "sieht", "der", "Hei\u00b7math", "stil\u00b7len", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Den See auch lieb' ich, de\u00df krystallner Quell", "tokens": ["Den", "See", "auch", "lieb'", "ich", ",", "de\u00df", "krys\u00b7tall\u00b7ner", "Quell"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Blick sich \u00f6ffnet bis zum Grunde hell,", "tokens": ["Dem", "Blick", "sich", "\u00f6ff\u00b7net", "bis", "zum", "Grun\u00b7de", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "APPR", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auf dessen Spiegel sich in sanftem Licht", "tokens": ["Auf", "des\u00b7sen", "Spie\u00b7gel", "sich", "in", "sanf\u00b7tem", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Getreu des Himmels milder Abglanz bricht.", "tokens": ["Ge\u00b7treu", "des", "Him\u00b7mels", "mil\u00b7der", "Ab\u00b