{"textgrid.poem.53595": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Morgenpost", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was bringt mir morgens so die Post?", "tokens": ["Was", "bringt", "mir", "mor\u00b7gens", "so", "die", "Post", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da liegt ein kleines H\u00e4uflein Briefe \u2013", "tokens": ["Da", "liegt", "ein", "klei\u00b7nes", "H\u00e4uf\u00b7lein", "Brie\u00b7fe", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "ich tue noch, als ob ich schliefe", "tokens": ["ich", "tue", "noch", ",", "als", "ob", "ich", "schlie\u00b7fe"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und dreh mich brummelnd wieder um . . .", "tokens": ["und", "dreh", "mich", "brum\u00b7melnd", "wie\u00b7der", "um", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Noch nicht, du Tag! Noch kein Spektakulum!", "tokens": ["Noch", "nicht", ",", "du", "Tag", "!", "Noch", "kein", "Spek\u00b7ta\u00b7ku\u00b7lum", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "$,", "PPER", "NN", "$.", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Es tickt die Uhr. Da kommts aus West und Ost . . .", "tokens": ["Es", "tickt", "die", "Uhr", ".", "Da", "kommts", "aus", "West", "und", "Ost", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$.", "ADV", "VVFIN", "APPR", "NE", "KON", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Was bringt mir morgens so die Post?", "tokens": ["Was", "bringt", "mir", "mor\u00b7gens", "so", "die", "Post", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ganz oben liegt ein gro\u00dfes Dings.", "tokens": ["Ganz", "o\u00b7ben", "liegt", "ein", "gro\u00b7\u00dfes", "Dings", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich \u00f6ffne. \u00bbB\u00fcrger!\u00ab mu\u00df ich lesen,", "tokens": ["Ich", "\u00f6ff\u00b7ne", ".", "\u00bb", "B\u00fcr\u00b7ger", "!", "\u00ab", "mu\u00df", "ich", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "$.", "$(", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsie sind doch auch Soldat gewesen!", "tokens": ["\u00bb", "sie", "sind", "doch", "auch", "Sol\u00b7dat", "ge\u00b7we\u00b7sen", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "NN", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einwohnerwehr! Sch\u00fctzt euer Haus!\u00ab", "tokens": ["Ein\u00b7woh\u00b7ner\u00b7wehr", "!", "Sch\u00fctzt", "eu\u00b7er", "Haus", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, Spiegelberg, so siehst du aus!", "tokens": ["Ach", ",", "Spie\u00b7gel\u00b7berg", ",", "so", "siehst", "du", "aus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein kleiner Tisch \u2013 er wackelt links \u2013", "tokens": ["Mein", "klei\u00b7ner", "Tisch", "\u2013", "er", "wa\u00b7ckelt", "links", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "ich st\u00fctz ihn nachher mit dem Dings.", "tokens": ["ich", "st\u00fctz", "ihn", "nach\u00b7her", "mit", "dem", "Dings", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein blaues Brieflein. Zarte Hand . . .", "tokens": ["Ein", "blau\u00b7es", "Brie\u00b7flein", ".", "Zar\u00b7te", "Hand", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O Minnie, ist es dir gelungen?", "tokens": ["O", "Min\u00b7nie", ",", "ist", "es", "dir", "ge\u00b7lun\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verlobt? So fingst du dir den Jungen?", "tokens": ["Ver\u00b7lobt", "?", "So", "fingst", "du", "dir", "den", "Jun\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$.", "ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Segen ruht auf diesem Paar.", "tokens": ["Mein", "Se\u00b7gen", "ruht", "auf", "die\u00b7sem", "Paar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich sage nichts von dem, was war.", "tokens": ["Ich", "sa\u00b7ge", "nichts", "von", "dem", ",", "was", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "ART", "$,", "PWS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sch\u00f6n ist Hymenaios Band", "tokens": ["Wie", "sch\u00f6n", "ist", "Hy\u00b7me\u00b7nai\u00b7os", "Band"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VAFIN", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "(f\u00fcr andre). Liebe zarte Hand . . .", "tokens": ["(", "f\u00fcr", "and\u00b7re", ")", ".", "Lie\u00b7be", "zar\u00b7te", "Hand", ".", ".", "."], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "APPR", "PIS", "$(", "$.", "NN", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da nahts. Der Aufdruck so vertraut \u2013", "tokens": ["Da", "nahts", ".", "Der", "Auf\u00b7druck", "so", "ver\u00b7traut", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbich habe\u00ab, schreibt S. J., \u00bbgebeten", "tokens": ["\u00bb", "ich", "ha\u00b7be", "\u00ab", ",", "schreibt", "S.", "J.", ",", "\u00bb", "ge\u00b7be\u00b7ten"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "abbreviation", "abbreviation", "punct", "punct", "word"], "pos": ["$(", "PPER", "VAFIN", "$(", "$,", "VVFIN", "NN", "NE", "$,", "$(", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "um ein Gedicht \u2013 Sie schickens nie!", "tokens": ["um", "ein", "Ge\u00b7dicht", "\u2013", "Sie", "schi\u00b7ckens", "nie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$(", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gebt ihr euch einmal f\u00fcr Poeten,", "tokens": ["Gebt", "ihr", "euch", "ein\u00b7mal", "f\u00fcr", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "so kommandiert die Poesie!\u00ab", "tokens": ["so", "kom\u00b7man\u00b7diert", "die", "Poe\u00b7sie", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und kommandiere ich auch noch so laut:", "tokens": ["Und", "kom\u00b7man\u00b7die\u00b7re", "ich", "auch", "noch", "so", "laut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die Muse ist doch schlie\u00dflich keine Braut!", "tokens": ["Die", "Mu\u00b7se", "ist", "doch", "schlie\u00df\u00b7lich", "kei\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein Schreiben noch. Im Eifer des Gefechts", "tokens": ["Ein", "Schrei\u00b7ben", "noch", ".", "Im", "Ei\u00b7fer", "des", "Ge\u00b7fechts"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$.", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "fiels auf den Boden. Viele Listen.", "tokens": ["fiels", "auf", "den", "Bo\u00b7den", ".", "Vie\u00b7le", "Lis\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$.", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verein der Antibolschewisten . . .", "tokens": ["Ver\u00b7ein", "der", "An\u00b7ti\u00b7bol\u00b7sche\u00b7wis\u00b7ten", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "ART", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nun steh ich auf. Ich wei\u00df Bescheid:", "tokens": ["Nun", "steh", "ich", "auf", ".", "Ich", "wei\u00df", "Be\u00b7scheid", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach jener winzigen, gro\u00dfen Zeit", "tokens": ["Nach", "je\u00b7ner", "win\u00b7zi\u00b7gen", ",", "gro\u00b7\u00dfen", "Zeit"]