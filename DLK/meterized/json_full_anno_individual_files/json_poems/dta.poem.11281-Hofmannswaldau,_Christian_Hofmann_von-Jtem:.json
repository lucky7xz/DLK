{"dta.poem.11281": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Jtem:", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr jungfern! habt ihr nichts? Zieht nur ins warme bad:", "tokens": ["Ihr", "jung\u00b7fern", "!", "habt", "ihr", "nichts", "?", "Zieht", "nur", "ins", "war\u00b7me", "bad", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$.", "VAFIN", "PPER", "PIS", "$.", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird aus nichts ein ding, das h\u00e4nd und f\u00fcsse hat.", "tokens": ["So", "wird", "aus", "nichts", "ein", "ding", ",", "das", "h\u00e4nd", "und", "f\u00fcs\u00b7se", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PIS", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}