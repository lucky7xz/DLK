in", ",", "und", "stets", "ge\u00b7hen", "der", "Gu\u00b7ten", "noch", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "T\u00e4glich verrinnen unsre Lieben. Sie kommen und gl\u00e4nzen", "tokens": ["T\u00e4g\u00b7lich", "ver\u00b7rin\u00b7nen", "uns\u00b7re", "Lie\u00b7ben", ".", "Sie", "kom\u00b7men", "und", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "$.", "PPER", "VVFIN", "KON", "VVINF"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und verschwinden, wie Thau gl\u00e4nzt in der Sonn' und verschwindt.", "tokens": ["Und", "ver\u00b7schwin\u00b7den", ",", "wie", "Thau", "gl\u00e4nzt", "in", "der", "Sonn'", "und", "ver\u00b7schwindt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.12": {"line.1": {"text": "D\u00fcster gelockter Freund, du J\u00fcngling von herrlichem Ausblick,", "tokens": ["D\u00fcs\u00b7ter", "ge\u00b7lock\u00b7ter", "Freund", ",", "du", "J\u00fcng\u00b7ling", "von", "herr\u00b7li\u00b7chem", "Aus\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Warum kamst du, und nahmst uns das Herze? Warum", "tokens": ["Wa\u00b7rum", "kamst", "du", ",", "und", "nahmst", "uns", "das", "Her\u00b7ze", "?", "Wa\u00b7rum"], "token_info": ["wor