{"textgrid.poem.36144": {"metadata": {"author": {"name": "Kosegarten, Gotthard Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Immer weniger wird der Edlen am Ricke! Zehn tausend", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Immer weniger wird der Edlen am Ricke! Zehn tausend", "tokens": ["Im\u00b7mer", "we\u00b7ni\u00b7ger", "wird", "der", "Ed\u00b7len", "am", "Ri\u00b7cke", "!", "Zehn", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$.", "CARD", "CARD"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Gingen dahin, und stets gehen der Guten noch mehr.", "tokens": ["Gin\u00b7gen", "da\u00b7hin", ",", "und", "stets", "ge\u00b7hen", "der", "Gu\u00b7ten", "noch", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "$,", "KON", "ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "T\u00e4glich verrinnen unsre Lieben. Sie kommen und gl\u00e4nzen", "tokens": ["T\u00e4g\u00b7lich", "ver\u00b7rin\u00b7nen", "uns\u00b7re", "Lie\u00b7ben", ".", "Sie", "kom\u00b7men", "und", "gl\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "ADJA", "$.", "PPER", "VVFIN", "KON", "VVINF"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und verschwinden, wie Thau gl\u00e4nzt in der Sonn' und verschwindt.", "tokens": ["Und", "ver\u00b7schwin\u00b7den", ",", "wie", "Thau", "gl\u00e4nzt", "in", "der", "Sonn'", "und", "ver\u00b7schwindt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "NN", "VVFIN", "APPR", "ART", "NN", "KON", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "D\u00fcster gelockter Freund, du J\u00fcngling von herrlichem Ausblick,", "tokens": ["D\u00fcs\u00b7ter", "ge\u00b7lock\u00b7ter", "Freund", ",", "du", "J\u00fcng\u00b7ling", "von", "herr\u00b7li\u00b7chem", "Aus\u00b7blick", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+-+-+", "measure": "iambic.septa.invert"}, "line.2": {"text": "Warum kamst du, und nahmst uns das Herze? Warum", "tokens": ["Wa\u00b7rum", "kamst", "du", ",", "und", "nahmst", "uns", "das", "Her\u00b7ze", "?", "Wa\u00b7rum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER", "PDS", "VVFIN", "$.", "PWAV"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.3": {"text": "Mu\u00dften wir dich ob deiner Milde und J\u00fcnglings-W\u00fcrde", "tokens": ["Mu\u00df\u00b7ten", "wir", "dich", "ob", "dei\u00b7ner", "Mil\u00b7de", "und", "J\u00fcng\u00b7lings\u00b7W\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PRF", "KOUS", "PPOSAT", "NN", "KON", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Liebgewinnen? \u2013 Du gehst, wandelst die Lieb' uns in Gram.", "tokens": ["Lieb\u00b7ge\u00b7win\u00b7nen", "?", "\u2013", "Du", "gehst", ",", "wan\u00b7delst", "die", "Lieb'", "uns", "in", "Gram", "."], "token_info": ["word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "$(", "PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "PPER", "APPR", "NE", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Deine Blicke die waren so freundlich, wie Abendsternstrahlen.", "tokens": ["Dei\u00b7ne", "Bli\u00b7cke", "die", "wa\u00b7ren", "so", "freund\u00b7lich", ",", "wie", "A\u00b7bends\u00b7terns\u00b7trah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VAFIN", "ADV", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "+-+--+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Deine Seel' ist des Stoffs edlerer Geister. Dir flammt", "tokens": ["Dei\u00b7ne", "Seel'", "ist", "des", "Stoffs", "ed\u00b7le\u00b7rer", "Geis\u00b7ter", ".", "Dir", "flammt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Tief im Busen, wie Gottesflamme, Geniusst\u00e4rke,", "tokens": ["Tief", "im", "Bu\u00b7sen", ",", "wie", "Got\u00b7tes\u00b7flam\u00b7me", ",", "Ge\u00b7ni\u00b7us\u00b7st\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "PWAV", "NN", "$,", "NN", "$,"], "meter": "+-+--+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Und der Musen geweiht Adytum steht dir enth\u00fcllt.", "tokens": ["Und", "der", "Mu\u00b7sen", "ge\u00b7weiht", "A\u00b7dy\u00b7tum", "steht", "dir", "ent\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVPP", "NE", "VVFIN", "PPER", "VVFIN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Aber, wehe! da gehst du nun, wandelst in Thr\u00e4nen die Wonne", "tokens": ["A\u00b7ber", ",", "we\u00b7he", "!", "da", "gehst", "du", "nun", ",", "wan\u00b7delst", "in", "Thr\u00e4\u00b7nen", "die", "Won\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Deiner Geliebten um dich, tauschest das Freudengestad',", "tokens": ["Dei\u00b7ner", "Ge\u00b7lieb\u00b7ten", "um", "dich", ",", "tau\u00b7schest", "das", "Freu\u00b7den\u00b7ge\u00b7stad'", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Unsrer Hylde um unwirthbarere Ufer, verl\u00e4ssest", "tokens": ["Uns\u00b7rer", "Hyl\u00b7de", "um", "un\u00b7wirth\u00b7ba\u00b7re\u00b7re", "U\u00b7fer", ",", "ver\u00b7l\u00e4s\u00b7sest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "All die Freuden, die dir gastfrei die Hylde gerauscht \u2013", "tokens": ["All", "die", "Freu\u00b7den", ",", "die", "dir", "gast\u00b7frei", "die", "Hyl\u00b7de", "ge\u00b7rauscht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Schau! da stehn wir nun all', im Brausen des Herbststurms, die Lieben", "tokens": ["Schau", "!", "da", "stehn", "wir", "nun", "all'", ",", "im", "Brau\u00b7sen", "des", "Herbst\u00b7sturms", ",", "die", "Lie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PIS", "$,", "APPRART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Deiner Seele. Wir stehn, harren mit hangendem Haupt", "tokens": ["Dei\u00b7ner", "See\u00b7le", ".", "Wir", "stehn", ",", "har\u00b7ren", "mit", "han\u00b7gen\u00b7dem", "Haupt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deines Scheidens. Die Scheidensthr\u00e4ne verflie\u00dft in den Regen-", "tokens": ["Dei\u00b7nes", "Schei\u00b7dens", ".", "Die", "Schei\u00b7denst\u00b7hr\u00e4\u00b7ne", "ver\u00b7flie\u00dft", "in", "den", "Re\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Schauer, der \u00fcber uns stiebt, unsere Locken zerst\u00f6rt.", "tokens": ["Schau\u00b7er", ",", "der", "\u00fc\u00b7ber", "uns", "stiebt", ",", "un\u00b7se\u00b7re", "Lo\u00b7cken", "zer\u00b7st\u00f6rt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}}, "stanza.6": {"line.1": {"text": "Schau! mit zerstreutem Haar, mit der Wange, durch Harm und des Herbstwinds", "tokens": ["Schau", "!", "mit", "zer\u00b7streu\u00b7tem", "Haar", ",", "mit", "der", "Wan\u00b7ge", ",", "durch", "Harm", "und", "des", "Herbst\u00b7winds"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "APPR", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Rauhes Blasen verbleicht, steht dein blau\u00e4ugiges Kind,", "tokens": ["Rau\u00b7hes", "Bla\u00b7sen", "ver\u00b7bleicht", ",", "steht", "dein", "blau\u00b7\u00e4u\u00b7gi\u00b7ges", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Eleonore, die Sanfterr\u00f6thende, winkt den Geliebten", "tokens": ["E\u00b7leo\u00b7no\u00b7re", ",", "die", "Sanf\u00b7ter\u00b7r\u00f6\u00b7then\u00b7de", ",", "winkt", "den", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihrer Seele und bebt unter dem herbstlichen Frost \u2013", "tokens": ["Ih\u00b7rer", "See\u00b7le", "und", "bebt", "un\u00b7ter", "dem", "herbst\u00b7li\u00b7chen", "Frost", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Aber, du achtest nicht des Rufes der Hylde-Freuden,", "tokens": ["A\u00b7ber", ",", "du", "ach\u00b7test", "nicht", "des", "Ru\u00b7fes", "der", "Hyl\u00b7de\u00b7Freu\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Nicht des Kummers der Schar, welche dein Scheiden so beugt,", "tokens": ["Nicht", "des", "Kum\u00b7mers", "der", "Schar", ",", "wel\u00b7che", "dein", "Schei\u00b7den", "so", "beugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Nicht der Seufzer des silbergelockten M\u00e4dchens, das unter", "tokens": ["Nicht", "der", "Seuf\u00b7zer", "des", "sil\u00b7ber\u00b7ge\u00b7lock\u00b7ten", "M\u00e4d\u00b7chens", ",", "das", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Tausend J\u00fcnglingen dich ihren Geliebten erkor \u2013", "tokens": ["Tau\u00b7send", "J\u00fcng\u00b7lin\u00b7gen", "dich", "ih\u00b7ren", "Ge\u00b7lieb\u00b7ten", "er\u00b7kor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "J\u00fcngling, J\u00fcngling, eile nicht so! Die Freudentage,", "tokens": ["J\u00fcng\u00b7ling", ",", "J\u00fcng\u00b7ling", ",", "ei\u00b7le", "nicht", "so", "!", "Die", "Freu\u00b7den\u00b7ta\u00b7ge", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "PTKNEG", "ADV", "$.", "ART", "NN", "$,"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Die du hier verlebt, kommen in Minnegestalt", "tokens": ["Die", "du", "hier", "ver\u00b7lebt", ",", "kom\u00b7men", "in", "Min\u00b7ne\u00b7ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "VVPP", "$,", "VVFIN", "APPR", "NN"], "meter": "--+-++--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Einst dir vor Augen, t\u00e4uschen im Traum dich, lassen im Busen", "tokens": ["Einst", "dir", "vor", "Au\u00b7gen", ",", "t\u00e4u\u00b7schen", "im", "Traum", "dich", ",", "las\u00b7sen", "im", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "NN", "$,", "VVFIN", "APPRART", "NN", "PPER", "$,", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Dir, ist schwunden dein Traum, krankes Erinnern zur\u00fcck. \u2013 \u2013 \u2013", "tokens": ["Dir", ",", "ist", "schwun\u00b7den", "dein", "Traum", ",", "kran\u00b7kes", "E\u00b7rin\u00b7nern", "zu\u00b7r\u00fcck", ".", "\u2013", "\u2013", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["PPER", "$,", "VAFIN", "ADJD", "PPOSAT", "NN", "$,", "ADJA", "NN", "PTKVZ", "$.", "$(", "$(", "$("], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Ha! braunlockiger Freund! Dein heiteres Auge w\u00f6lkt sich!", "tokens": ["Ha", "!", "braun\u00b7lo\u00b7cki\u00b7ger", "Freund", "!", "Dein", "hei\u00b7te\u00b7res", "Au\u00b7ge", "w\u00f6lkt", "sich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Deine m\u00e4nnliche Wang' rinnt die Thr\u00e4ne hinab! \u2013", "tokens": ["Dei\u00b7ne", "m\u00e4nn\u00b7li\u00b7che", "Wang'", "rinnt", "die", "Thr\u00e4\u00b7ne", "hin\u00b7ab", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sey uns geklagt! Du l\u00e4ssest nicht harmlos die Freuden der Hylde", "tokens": ["Sey", "uns", "ge\u00b7klagt", "!", "Du", "l\u00e4s\u00b7sest", "nicht", "harm\u00b7los", "die", "Freu\u00b7den", "der", "Hyl\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$.", "PPER", "VVFIN", "PTKNEG", "ADJD", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Und Eleonore, dein Kind, lieblich und silbergelockt.", "tokens": ["Und", "E\u00b7leo\u00b7no\u00b7re", ",", "dein", "Kind", ",", "lieb\u00b7lich", "und", "sil\u00b7ber\u00b7ge\u00b7lockt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPOSAT", "NN", "$,", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}}, "stanza.10": {"line.1": {"text": "Aber der Schickung Wuthschlu\u00df steht! \u2013 Da hallt sie, die dunkle", "tokens": ["A\u00b7ber", "der", "Schi\u00b7ckung", "Wuth\u00b7schlu\u00df", "steht", "!", "\u2013", "Da", "hallt", "sie", ",", "die", "dunk\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "$.", "$(", "ADV", "VVFIN", "PPER", "$,", "ART", "ADJA"], "meter": "+--+-+---+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Abschiedsstunde! Du fliehst? Bruder, Geliebter, du fliehst? \u2013 \u2013 \u2013", "tokens": ["Ab\u00b7schieds\u00