text": "Unsrer Hylde um unwirthbarere Ufer, verl\u00e4ssest", "tokens": ["Uns\u00b7rer", "Hyl\u00b7de", "um", "un\u00b7wirth\u00b7ba\u00b7re\u00b7re", "U\u00b7fer", ",", "ver\u00b7l\u00e4s\u00b7sest"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,", "VVFIN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "All die Freuden, die dir gastfrei die Hylde gerauscht \u2013", "tokens": ["All", "die", "Freu\u00b7den", ",", "die", "dir", "gast\u00b7frei", "die", "Hyl\u00b7de", "ge\u00b7rauscht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.15": {"line.1": {"text": "Schau! da stehn wir nun all', im Brausen des Herbststurms, die Lieben", "tokens": ["Schau", "!", "da", "stehn", "wir", "nun", "all'", ",", "im", "Brau\u00b7sen", "des", "Herbst\u00b7sturms", ",", "die", "Lie\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "PIS", "$,", "APPRART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Deiner Seele. Wir stehn, harren mit hangendem Haupt", "tokens": ["Dei\u00b7ner", "See\u00b7le", ".", "Wir", "stehn", ",", "har\u00b7ren", "mit", "han\u00b7gen\u00b7dem", "Haupt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Deines Scheidens. Die Scheidensthr\u00e4ne verflie\u00dft in den Regen-", "tokens": ["Dei\u00b7nes", "S