{"textgrid.poem.39125": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ritt durch klare Fr\u00fchlingspracht", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ritt durch klare Fr\u00fchlingspracht", "tokens": ["Ich", "ritt", "durch", "kla\u00b7re", "Fr\u00fch\u00b7lings\u00b7pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf sturmbehendem Pferde,", "tokens": ["Auf", "sturm\u00b7be\u00b7hen\u00b7dem", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da hab' ich bei mir selbst gedacht:", "tokens": ["Da", "hab'", "ich", "bei", "mir", "selbst", "ge\u00b7dacht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ist doch sch\u00f6n die Erde!", "tokens": ["Wie", "ist", "doch", "sch\u00f6n", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Renner sprang, der Renner schwang", "tokens": ["Der", "Ren\u00b7ner", "sprang", ",", "der", "Ren\u00b7ner", "schwang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich \u00fcber Gr\u00e4ben und Hecken,", "tokens": ["Sich", "\u00fc\u00b7ber", "Gr\u00e4\u00b7ben", "und", "He\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wohl \u00fcber den sonnigen Bergeshang", "tokens": ["Wohl", "\u00fc\u00b7ber", "den", "son\u00b7ni\u00b7gen", "Ber\u00b7ges\u00b7hang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und schattige Talesstrecken.", "tokens": ["Und", "schat\u00b7ti\u00b7ge", "Ta\u00b7les\u00b7stre\u00b7cken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie rannen im sausenden Luftgezisch", "tokens": ["Wie", "ran\u00b7nen", "im", "sau\u00b7sen\u00b7den", "Luft\u00b7ge\u00b7zisch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor\u00fcber die blauen Weiten,", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "die", "blau\u00b7en", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir ward so froh, so frei und frisch,", "tokens": ["Mir", "ward", "so", "froh", ",", "so", "frei", "und", "frisch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als wollt' ich gen Himmel reiten.", "tokens": ["Als", "wollt'", "ich", "gen", "Him\u00b7mel", "rei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Fort stob der Erde Pein und Weh',", "tokens": ["Fort", "stob", "der", "Er\u00b7de", "Pein", "und", "Weh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie unterm Hufe die Kiese,", "tokens": ["Wie", "un\u00b7term", "Hu\u00b7fe", "die", "Kie\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf stieg aus der Brust zur Wolkenh\u00f6h'", "tokens": ["Auf", "stieg", "aus", "der", "Brust", "zur", "Wol\u00b7ken\u00b7h\u00f6h'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Des Gedankens freudiger Riese.", "tokens": ["Des", "Ge\u00b7dan\u00b7kens", "freu\u00b7di\u00b7ger", "Rie\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Es klirrte der B\u00fcgel, es blitzte der Sporn,", "tokens": ["Es", "klirr\u00b7te", "der", "B\u00fc\u00b7gel", ",", "es", "blitz\u00b7te", "der", "Sporn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ich sa\u00df in stolzer Ermannung;", "tokens": ["Ich", "sa\u00df", "in", "stol\u00b7zer", "Er\u00b7man\u00b7nung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Wie st\u00f6hnte des Rosses Feuerzorn", "tokens": ["Wie", "st\u00f6hn\u00b7te", "des", "Ros\u00b7ses", "Feu\u00b7er\u00b7zorn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In kr\u00e4ftiger Schenkelspannung.", "tokens": ["In", "kr\u00e4f\u00b7ti\u00b7ger", "Schen\u00b7kel\u00b7span\u00b7nung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Und wie es st\u00fcrmte hinab, hinauf,", "tokens": ["Und", "wie", "es", "st\u00fcrm\u00b7te", "hin\u00b7ab", ",", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "PTKVZ", "$,", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gehetzt vom Stachel der Ferse,", "tokens": ["Ge\u00b7hetzt", "vom", "Sta\u00b7chel", "der", "Fer\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da ward zum Reime ein jedes Geschnauf,", "tokens": ["Da", "ward", "zum", "Rei\u00b7me", "ein", "je\u00b7des", "Ge\u00b7schnauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein jeder Hufschlag zum Verse:", "tokens": ["Ein", "je\u00b7der", "Huf\u00b7schlag", "zum", "Ver\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Gott gr\u00fc\u00dfe dich, tiefes Himmelsblau,", "tokens": ["Gott", "gr\u00fc\u00b7\u00dfe", "dich", ",", "tie\u00b7fes", "Him\u00b7mels\u00b7blau", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Euch zuckende Sonnenstrahlen,", "tokens": ["Euch", "zu\u00b7cken\u00b7de", "Son\u00b7nen\u00b7strah\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du rauschender Wald, du Wellentau;", "tokens": ["Du", "rau\u00b7schen\u00b7der", "Wald", ",", "du", "Wel\u00b7len\u00b7tau", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Gott gr\u00fc\u00df' euch zu tausend Malen!", "tokens": ["Gott", "gr\u00fc\u00df'", "euch", "zu", "tau\u00b7send", "Ma\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "So hab' ich gejubelt, geschw\u00e4rmt, gelacht,", "tokens": ["So", "hab'", "ich", "ge\u00b7ju\u00b7belt", ",", "ge\u00b7schw\u00e4rmt", ",", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Im freudigen J\u00fcnglingsmute,", "tokens": ["Im", "freu\u00b7di\u00b7gen", "J\u00fcng\u00b7lings\u00b7mu\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Indes unter mir mit Windesmacht", "tokens": ["In\u00b7des", "un\u00b7ter", "mir", "mit", "Win\u00b7des\u00b7macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Hinjagte die schlanke Stute.", "tokens": ["Hin\u00b7jag\u00b7te", "die", "schlan\u00b7ke", "Stu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.9": {"line.1": {"text": "Und als ich daheim beim Abendstrahl", "tokens": ["Und", "als", "ich", "da\u00b7heim", "beim", "A\u00b7bend\u00b7strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Abnahm den Sattel dem Pferde,", "tokens": ["Ab\u00b7nahm", "den", "Sat\u00b7tel", "dem", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Da sprach ich im Stillen noch einmal:", "tokens": ["Da", "sprach", "ich", "im", "Stil\u00b7len", "noch", "ein\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "ADV", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wie ist so sch\u00f6n die Erde!", "tokens": ["Wie", "ist", "so", "sch\u00f6n", "die", "Er\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stan