{"textgrid.poem.27495": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An die Freier", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freier! Seid ihr beleidigt? Hier ist der Bogen Odysseus'!", "tokens": ["Frei\u00b7er", "!", "Seid", "ihr", "be\u00b7lei\u00b7digt", "?", "Hier", "ist", "der", "Bo\u00b7gen", "O\u00b7dys\u00b7seus'", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VAIMP", "PPER", "VVPP", "$.", "ADV", "VAFIN", "ART", "NN", "NE", "$."], "meter": "+----+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Spannt ihn, wie wir ihn gespannt, schnellt durch die \u00c4xte den Pfeil.", "tokens": ["Spannt", "ihn", ",", "wie", "wir", "ihn", "ge\u00b7spannt", ",", "schnellt", "durch", "die", "\u00c4x\u00b7te", "den", "Pfeil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}}, "stanza.2": {"line.1": {"text": "Ein paar Jahre r\u00fchret euch nun, dann kommen wir wieder,", "tokens": ["Ein", "paar", "Jah\u00b7re", "r\u00fch\u00b7ret", "euch", "nun", ",", "dann", "kom\u00b7men", "wir", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ist uns g\u00fcnstig Apoll, munter und mutig wie heut.", "tokens": ["Ist", "uns", "g\u00fcns\u00b7tig", "A\u00b7poll", ",", "mun\u00b7