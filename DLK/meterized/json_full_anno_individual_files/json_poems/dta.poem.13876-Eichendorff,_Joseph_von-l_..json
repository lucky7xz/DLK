{"dta.poem.13876": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "l .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Nun, wir sehen, Ihr seid Meister.", "tokens": ["Nun", ",", "wir", "se\u00b7hen", ",", "Ihr", "seid", "Meis\u00b7ter", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPER", "VVINF", "$,", "PPER", "VAFIN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch wir sind heut liberal;", "tokens": ["Doch", "wir", "sind", "heut", "li\u00b7be\u00b7ral", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Hoffentlich, als sch\u00f6ne Geister,", "tokens": ["Hof\u00b7fent\u00b7lich", ",", "als", "sch\u00f6\u00b7ne", "Geis\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Treibt Ihr's etwas ideal.", "tokens": ["Treibt", "Ih\u00b7r's", "et\u00b7was", "i\u00b7deal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}