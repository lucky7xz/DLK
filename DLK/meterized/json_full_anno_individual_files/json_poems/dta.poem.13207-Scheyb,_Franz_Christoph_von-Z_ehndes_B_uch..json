{"dta.poem.13207": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich wei\u00df, ihr pfleget oft f\u00fcr Sorg und Flei\u00df zu schwizen,", "tokens": ["\u201e", "ich", "wei\u00df", ",", "ihr", "pfle\u00b7get", "oft", "f\u00fcr", "Sorg", "und", "Flei\u00df", "zu", "schwi\u00b7zen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADV", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "300\u201dDamit ihr manches Herz der Jugend k\u00f6nnt besizen.", "tokens": ["\"", "Da\u00b7mit", "ihr", "man\u00b7ches", "Herz", "der", "Ju\u00b7gend", "k\u00f6nnt", "be\u00b7si\u00b7zen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PIAT", "NN", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ejhr pr\u00fcffet Lieb und Zwang; absonderlich wo sie", "tokens": ["\u201e", "jhr", "pr\u00fcf\u00b7fet", "Lieb", "und", "Zwang", ";", "ab\u00b7son\u00b7der\u00b7lich", "wo", "sie"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "NN", "KON", "NN", "$.", "ADJD", "PWAV", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201emit Niedertr\u00e4chtigkeit, mit unbelobter M\u00fch", "tokens": ["\u201e", "mit", "Nie\u00b7der\u00b7tr\u00e4ch\u00b7tig\u00b7keit", ",", "mit", "un\u00b7be\u00b7lob\u00b7ter", "M\u00fch"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "APPR", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201eden Leidenschaften nach zum Hochmuth sich erheben;", "tokens": ["\u201e", "den", "Lei\u00b7den\u00b7schaf\u00b7ten", "nach", "zum", "Hoch\u00b7muth", "sich", "er\u00b7he\u00b7ben", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPR", "APPRART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edurch falsche Regungen nach Dunst und Larven streben.", "tokens": ["\u201e", "durch", "fal\u00b7sche", "Re\u00b7gun\u00b7gen", "nach", "Dunst", "und", "Lar\u00b7ven", "stre\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "305\u201dUm wie viel sch\u00e4zbarer mu\u00df euch das Herz nicht seyn,", "tokens": ["\"", "Um", "wie", "viel", "sch\u00e4z\u00b7ba\u00b7rer", "mu\u00df", "euch", "das", "Herz", "nicht", "seyn", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUI", "KOKOM", "PIAT", "ADJA", "VMFIN", "PPER", "ART", "NN", "PTKNEG", "VAINF", "$,"], "meter": "-+-----+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "\u201ein dessen Grund ich selbst bereits den ersten Stein", "tokens": ["\u201e", "in", "des\u00b7sen", "Grund", "ich", "selbst", "be\u00b7reits", "den", "ers\u00b7ten", "Stein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "APPR", "PRELAT", "NN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eder Tugend, die ihr mich gelehrt, hab eingegraben:", "tokens": ["\u201e", "der", "Tu\u00b7gend", ",", "die", "ihr", "mich", "ge\u00b7lehrt", ",", "hab", "ein\u00b7ge\u00b7gra\u00b7ben", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "PRF", "VVPP", "$,", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edas seynd die T\u00f6chter hier und diese beyde Knaben.", "tokens": ["\u201e", "das", "seynd", "die", "T\u00f6ch\u00b7ter", "hier", "und", "die\u00b7se", "bey\u00b7de", "Kna\u00b7ben", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ART", "NN", "ADV", "KON", "PDAT", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eso nehmet sie dann hin! ich hoffe da\u00df der Lohn", "tokens": ["\u201e", "so", "neh\u00b7met", "sie", "dann", "hin", "!", "ich", "hof\u00b7fe", "da\u00df", "der", "Lohn"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "310\u201dEuch angenehmer ist, als ich und meine Kron.", "tokens": ["\"", "Euch", "an\u00b7ge\u00b7neh\u00b7mer", "ist", ",", "als", "ich", "und", "mei\u00b7ne", "Kron", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}