{"dta.poem.14192": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Frau Venus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "So mich auch ruft ihr aus dem stillen Hause \u2014", "tokens": ["So", "mich", "auch", "ruft", "ihr", "aus", "dem", "stil\u00b7len", "Hau\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schmerzlich nun mu\u00df ich im Fr\u00fchling l\u00e4cheln,", "tokens": ["Und", "schmerz\u00b7lich", "nun", "mu\u00df", "ich", "im", "Fr\u00fch\u00b7ling", "l\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Versinkend zwischen Duft und Klang vor Sehnen.", "tokens": ["Ver\u00b7sin\u00b7kend", "zwi\u00b7schen", "Duft", "und", "Klang", "vor", "Seh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}