{"textgrid.poem.66111": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Beter", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der wache Wald begr\u00fc\u00dft den stillen Strand", "tokens": ["Der", "wa\u00b7che", "Wald", "be\u00b7gr\u00fc\u00dft", "den", "stil\u00b7len", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit erstem Vogelruf. Die Morgenwelle", "tokens": ["Mit", "ers\u00b7tem", "Vo\u00b7gel\u00b7ruf", ".", "Die", "Mor\u00b7gen\u00b7wel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "W\u00e4scht blanke Kiesel, spielt mit wei\u00dfen Muscheln.", "tokens": ["W\u00e4scht", "blan\u00b7ke", "Kie\u00b7sel", ",", "spielt", "mit", "wei\u00b7\u00dfen", "Mu\u00b7scheln", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Kein Laut ringsum, der den Gebeugten st\u00f6rt,", "tokens": ["Kein", "Laut", "ring\u00b7sum", ",", "der", "den", "Ge\u00b7beug\u00b7ten", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "APPR", "ADV", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Den Hingebeugten, der den braunen Nacken", "tokens": ["Den", "Hin\u00b7ge\u00b7beug\u00b7ten", ",", "der", "den", "brau\u00b7nen", "Na\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Anbetend neigt, bis in den Sand die Stirn.", "tokens": ["An\u00b7be\u00b7tend", "neigt", ",", "bis", "in", "den", "Sand", "die", "Stirn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und strahlend kommt der Tag her \u00fcbers Wasser,", "tokens": ["Und", "strah\u00b7lend", "kommt", "der", "Tag", "her", "\u00fc\u00b7bers", "Was\u00b7ser", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "APZR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die Wellen h\u00fcpfen unter seinem Schritt", "tokens": ["Die", "Wel\u00b7len", "h\u00fcp\u00b7fen", "un\u00b7ter", "sei\u00b7nem", "Schritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und leuchten. Sanft ergl\u00fchn die strengen Wipfel", "tokens": ["Und", "leuch\u00b7ten", ".", "Sanft", "er\u00b7gl\u00fchn", "die", "stren\u00b7gen", "Wip\u00b7fel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Des schwarzen Haines und erschauern leis.", "tokens": ["Des", "schwar\u00b7zen", "Hai\u00b7nes", "und", "er\u00b7schau\u00b7ern", "leis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "i