{"textgrid.poem.55003": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Schadenfreude", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In des Papillons Gestalt", "tokens": ["In", "des", "Pa\u00b7pil\u00b7lons", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Flattr' ich, nach den letzten Z\u00fcgen,", "tokens": ["Flat\u00b7tr'", "ich", ",", "nach", "den", "letz\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zu den vielgeliebten Stellen,", "tokens": ["Zu", "den", "viel\u00b7ge\u00b7lieb\u00b7ten", "Stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zeugen himmlischer Vergn\u00fcgen,", "tokens": ["Zeu\u00b7gen", "himm\u00b7li\u00b7scher", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ober Wiesen, an die Quellen,", "tokens": ["O\u00b7ber", "Wie\u00b7sen", ",", "an", "die", "Quel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um den H\u00fcgel, durch den Wald.", "tokens": ["Um", "den", "H\u00fc\u00b7gel", ",", "durch", "den", "Wald", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich belausch ein z\u00e4rtlich Paar;", "tokens": ["Ich", "be\u00b7lausch", "ein", "z\u00e4rt\u00b7lich", "Paar", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Von des sch\u00f6nen M\u00e4dchens Haupte", "tokens": ["Von", "des", "sch\u00f6\u00b7nen", "M\u00e4d\u00b7chens", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den Kr\u00e4nzen schau ich nieder;", "tokens": ["Aus", "den", "Kr\u00e4n\u00b7zen", "schau", "ich", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles, was der Tod mir raubte,", "tokens": ["Al\u00b7les", ",", "was", "der", "Tod", "mir", "raub\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seh ich hier im Bilde wieder,", "tokens": ["Seh", "ich", "hier", "im", "Bil\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bin so gl\u00fccklich, wie ich war.", "tokens": ["Bin", "so", "gl\u00fcck\u00b7lich", ",", "wie", "ich", "war", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "$,", "PWAV", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie umarmt ihn l\u00e4chelnd stumm,", "tokens": ["Sie", "um\u00b7armt", "ihn", "l\u00e4\u00b7chelnd", "stumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und sein Mund genie\u00dft der Stunde,", "tokens": ["Und", "sein", "Mund", "ge\u00b7nie\u00dft", "der", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die ihm g\u00fct'ge G\u00f6tter senden,", "tokens": ["Die", "ihm", "g\u00fct'\u00b7ge", "G\u00f6t\u00b7ter", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcpft vom Busen zu dem Munde,", "tokens": ["H\u00fcpft", "vom", "Bu\u00b7sen", "zu", "dem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Von dem Munde zu den H\u00e4nden,", "tokens": ["Von", "dem", "Mun\u00b7de", "zu", "den", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und ich h\u00fcpf um ihn herum.", "tokens": ["Und", "ich", "h\u00fcpf", "um", "ihn", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line