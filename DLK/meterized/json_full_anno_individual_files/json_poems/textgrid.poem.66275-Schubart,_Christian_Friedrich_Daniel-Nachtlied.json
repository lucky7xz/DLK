"meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Vom H\u00f6llengeist ge\u00e4fft,", "tokens": ["Vom", "H\u00f6l\u00b7len\u00b7geist", "ge\u00b7\u00e4fft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nie s\u00fcndige und fehle,", "tokens": ["Nie", "s\u00fcn\u00b7di\u00b7ge", "und", "feh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Auch wenn der K\u00f6rper schl\u00e4ft.", "tokens": ["Auch", "wenn", "der", "K\u00f6r\u00b7per", "schl\u00e4ft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wenn neben mir ein armer", "tokens": ["Wenn", "ne\u00b7ben", "mir", "ein", "ar\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "ART", "ADJA"], "meter": "+----+-", "measure": "dactylic.init"}, "line.2": {"text": "Gefangner Freund noch wacht;", "tokens": ["Ge\u00b7fang\u00b7ner", "Freund", "noch", "wacht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So st\u00e4rk ihn doch, Erbarmer!", "tokens": ["So", "st\u00e4rk", "ihn", "doch", ",", "Er\u00b7bar\u00b7mer", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit einer guten Nacht.", "tokens": ["Mit", "ei\u00b7ner", "gu\u00b7ten", "Nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"},