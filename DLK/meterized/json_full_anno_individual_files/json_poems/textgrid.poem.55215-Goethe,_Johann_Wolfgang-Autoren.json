{"textgrid.poem.55215": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Autoren", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00dcber die Wiese, den Bach herab,", "tokens": ["\u00dc\u00b7ber", "die", "Wie\u00b7se", ",", "den", "Bach", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Durch seinen Garten,", "tokens": ["Durch", "sei\u00b7nen", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Bricht er die j\u00fcngsten Blumen ab:", "tokens": ["Bricht", "er", "die", "j\u00fcng\u00b7sten", "Blu\u00b7men", "ab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm schl\u00e4gt das Herz vor Erwarten.", "tokens": ["Ihm", "schl\u00e4gt", "das", "Herz", "vor", "Er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sein M\u00e4dchen kommt \u2013 o Gewinst! o Gl\u00fcck!", "tokens": ["Sein", "M\u00e4d\u00b7chen", "kommt", "\u2013", "o", "Ge\u00b7winst", "!", "o", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$(", "FM", "NN", "$.", "FM", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "J\u00fcngling, tauschest deine Bl\u00fcten um einen Blick!", "tokens": ["J\u00fcng\u00b7ling", ",", "tau\u00b7schest", "dei\u00b7ne", "Bl\u00fc\u00b7ten", "um", "ei\u00b7nen", "Blick", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Nachbar G\u00e4rtner sieht herein", "tokens": ["Der", "Nach\u00b7bar", "G\u00e4rt\u00b7ner", "sieht", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00dcber die Hecke: \u00bbSo ein Tor m\u00f6cht ich sein!", "tokens": ["\u00dc\u00b7ber", "die", "He\u00b7cke", ":", "\u00bb", "So", "ein", "Tor", "m\u00f6cht", "ich", "sein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$(", "ADV", "ART", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hab Freude, meine Blumen zu n\u00e4hren,", "tokens": ["Hab", "Freu\u00b7de", ",", "mei\u00b7ne", "Blu\u00b7men", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die V\u00f6gel von meinen Fr\u00fcchten zu wehren;", "tokens": ["Die", "V\u00f6\u00b7gel", "von", "mei\u00b7nen", "Fr\u00fcch\u00b7ten", "zu", "weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Aber sind sie reif: Geld! guter Freund!", "tokens": ["A\u00b7ber", "sind", "sie", "reif", ":", "Geld", "!", "gu\u00b7ter", "Freund", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$.", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Soll ich meine M\u00fche verlieren?\u00ab", "tokens": ["Soll", "ich", "mei\u00b7ne", "M\u00fc\u00b7he", "ver\u00b7lie\u00b7ren", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Das sind Autoren, wie es scheint.", "tokens": ["Das", "sind", "Au\u00b7to\u00b7ren", ",", "wie", "es", "scheint", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine streut seine Freuden herum", "tokens": ["Der", "ei\u00b7ne", "streut", "sei\u00b7ne", "Freu\u00b7den", "he\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "PPOSAT", "NN", "APZR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Seinen Freunden, dem Publikum;", "tokens": ["Sei\u00b7nen", "Freun\u00b7den", ",", "dem", "Pub\u00b7li\u00b7kum", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Der andre l\u00e4\u00dft sich pr\u00e4numerieren.", "tokens": ["Der", "and\u00b7re", "l\u00e4\u00dft", "sich", "pr\u00e4\u00b7nu\u00b7me\u00b7rie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+----+