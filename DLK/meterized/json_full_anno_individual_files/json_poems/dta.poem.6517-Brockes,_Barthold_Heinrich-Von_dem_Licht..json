{"dta.poem.6517": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unsere Vernunft es recht bedencket,", "tokens": ["Wenn", "un\u00b7se\u00b7re", "Ver\u00b7nunft", "es", "recht", "be\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So scheinets, da\u00df das Licht sich ebenm\u00e4\u00dfig lencket,", "tokens": ["So", "schei\u00b7nets", ",", "da\u00df", "das", "Licht", "sich", "e\u00b7ben\u00b7m\u00e4\u00b7\u00dfig", "len\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie andre C\u00f6rper auch nach Regeln und Gesetze,", "tokens": ["Wie", "and\u00b7re", "C\u00f6r\u00b7per", "auch", "nach", "Re\u00b7geln", "und", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So die Bewegung giebt,", "tokens": ["So", "die", "Be\u00b7we\u00b7gung", "giebt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}