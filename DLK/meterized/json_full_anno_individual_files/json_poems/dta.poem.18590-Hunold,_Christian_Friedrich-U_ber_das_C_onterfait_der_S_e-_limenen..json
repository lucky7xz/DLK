{"dta.poem.18590": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "U ber das  C onterfait der  S e-  \n limenen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1702", "urn": "urn:nbn:de:kobv:b4-200905197766", "language": ["de:0.99"], "booktitle": "Hunold, Christian Friedrich: Die Edle Bem\u00fchung m\u00fcssiger Stunden. Hamburg, 1702."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Auge welches nur will rare Sch\u00f6nheit kennen/", "tokens": ["Ein", "Au\u00b7ge", "wel\u00b7ches", "nur", "will", "ra\u00b7re", "Sch\u00f6n\u00b7heit", "ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRELS", "ADV", "VMFIN", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Seh dieses Conterfait mit rechten blicken an/", "tokens": ["Seh", "die\u00b7ses", "Con\u00b7ter\u00b7fait", "mit", "rech\u00b7ten", "bli\u00b7cken", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPR", "ADJA", "VVFIN", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Der Augen Wunder-Strahl wird auch in Abri\u00df brennen/", "tokens": ["Der", "Au\u00b7gen", "Wun\u00b7der\u00b7Strahl", "wird", "auch", "in", "Ab\u00b7ri\u00df", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der lebend jedes Hertz wie Wachs zerschmeltzen kan.", "tokens": ["Der", "le\u00b7bend", "je\u00b7des", "Hertz", "wie", "Wachs", "zer\u00b7schmelt\u00b7zen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PIAT", "NN", "KOKOM", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch das Original hegt tausend Lieblichkeiten", "tokens": ["Doch", "das", "O\u00b7rig\u00b7i\u00b7nal", "hegt", "tau\u00b7send", "Lieb\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "CARD", "NN"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Daf\u00fcr Apelles Kunst in Schildern selbst zu klein.", "tokens": ["Da\u00b7f\u00fcr", "A\u00b7pel\u00b7les", "Kunst", "in", "Schil\u00b7dern", "selbst", "zu", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "NN", "APPR", "NN", "ADV", "PTKA", "ADJD", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und fragst du wer denn hier die Sch\u00f6nheit soll bedeuten/", "tokens": ["Und", "fragst", "du", "wer", "denn", "hier", "die", "Sch\u00f6n\u00b7heit", "soll", "be\u00b7deu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PWS", "ADV", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So spricht die Warheit selbst/ es soll die Venus seyn.", "tokens": ["So", "spricht", "die", "War\u00b7heit", "selbst", "/", "es", "soll", "die", "Ve\u00b7nus", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$(", "PPER", "VMFIN", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}