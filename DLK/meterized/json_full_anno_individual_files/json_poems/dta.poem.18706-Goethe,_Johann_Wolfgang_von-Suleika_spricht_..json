{"dta.poem.18706": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Suleika  \n spricht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Der Spiegel sagt mir ich bin sch\u00f6n!", "tokens": ["Der", "Spie\u00b7gel", "sagt", "mir", "ich", "bin", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr sagt: zu altern sey auch mein Geschick.", "tokens": ["Ihr", "sagt", ":", "zu", "al\u00b7tern", "sey", "auch", "mein", "Ge\u00b7schick", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKZU", "VVINF", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vor Gott muss alles ewig stehn,", "tokens": ["Vor", "Gott", "muss", "al\u00b7les", "e\u00b7wig", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In mir liebt Ihn, f\u00fcr diesen Augenblick.", "tokens": ["In", "mir", "liebt", "Ihn", ",", "f\u00fcr", "die\u00b7sen", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}