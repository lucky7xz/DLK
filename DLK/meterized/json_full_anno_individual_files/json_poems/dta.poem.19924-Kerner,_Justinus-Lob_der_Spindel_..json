{"dta.poem.19924": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Lob der Spindel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905193111", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Faust des Mannes zieret", "tokens": ["Die", "Faust", "des", "Man\u00b7nes", "zie\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein blank geschliffen Schwerdt,", "tokens": ["Ein", "blank", "ge\u00b7schlif\u00b7fen", "Schwerdt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das er in Treue f\u00fchret,", "tokens": ["Das", "er", "in", "Treu\u00b7e", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo er das Recht begehrt.", "tokens": ["Wo", "er", "das", "Recht", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sank er auf blut'ger Haide,", "tokens": ["Sank", "er", "auf", "blut'\u00b7ger", "Hai\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Den Ring, den Edelstein,", "tokens": ["Den", "Ring", ",", "den", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dies seiner Hand Geschmeide", "tokens": ["Dies", "sei\u00b7ner", "Hand", "Ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Grab' man mit ihm hinein.", "tokens": ["Grab'", "man", "mit", "ihm", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Des Eisens Wucht zu heben", "tokens": ["Des", "Ei\u00b7sens", "Wucht", "zu", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind Frauen nicht gewandt,", "tokens": ["Sind", "Frau\u00b7en", "nicht", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie leben stilles Leben", "tokens": ["Sie", "le\u00b7ben", "stil\u00b7les", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Spindel in der Hand.", "tokens": ["Die", "Spin\u00b7del", "in", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die zarte Hand der Sch\u00f6nen", "tokens": ["Die", "zar\u00b7te", "Hand", "der", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ziert ", "tokens": ["Ziert"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Sie tanzt mit s\u00fc\u00dfem T\u00f6nen,", "tokens": ["Sie", "tanzt", "mit", "s\u00fc\u00b7\u00dfem", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und singt der Frauen Flei\u00df.", "tokens": ["Und", "singt", "der", "Frau\u00b7en", "Flei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "In alter W\u00e4lder Dunkel,", "tokens": ["In", "al\u00b7ter", "W\u00e4l\u00b7der", "Dun\u00b7kel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Auf moosigem Gestein", "tokens": ["Auf", "moo\u00b7si\u00b7gem", "Ge\u00b7stein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sizt an krystallner Kunkel", "tokens": ["Sizt", "an", "krys\u00b7tall\u00b7ner", "Kun\u00b7kel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nachtfrau im Mondenschein.", "tokens": ["Nacht\u00b7frau", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "++-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Mondhelle F\u00e4den bringet", "tokens": ["Mond\u00b7hel\u00b7le", "F\u00e4\u00b7den", "brin\u00b7get"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr Finger zart hervor;", "tokens": ["Ihr", "Fin\u00b7ger", "zart", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Seltsam die Spindel singet;", "tokens": ["Selt\u00b7sam", "die", "Spin\u00b7del", "sin\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Es lauscht des Wandrers Ohr.", "tokens": ["Es", "lauscht", "des", "Wand\u00b7rers", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "In Schlo\u00df und Burgeshallen", "tokens": ["In", "Schlo\u00df", "und", "Bur\u00b7ges\u00b7hal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Spindel emsig sang;", "tokens": ["Die", "Spin\u00b7del", "em\u00b7sig", "sang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den deutschen Frauen allen", "tokens": ["Den", "deut\u00b7schen", "Frau\u00b7en", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "War sie ein lieber Klang.", "tokens": ["War", "sie", "ein", "lie\u00b7ber", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Gar sp\u00e4rlich Sammt und Seide", "tokens": ["Gar", "sp\u00e4r\u00b7lich", "Sammt", "und", "Sei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Umfieng der Frauen Leib.", "tokens": ["Um\u00b7fi\u00b7eng", "der", "Frau\u00b7en", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im selbstgesponn'nen Kleide", "tokens": ["Im", "selbst\u00b7ge\u00b7sponn'\u00b7nen", "Klei\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gieng da manch s\u00fc\u00dfes Weib.", "tokens": ["Gieng", "da", "manch", "s\u00fc\u00b7\u00dfes", "Weib", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Kaum da\u00df in armer Kammer,", "tokens": ["Kaum", "da\u00df", "in", "ar\u00b7mer", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In N\u00e4chten lang und bang,", "tokens": ["In", "N\u00e4ch\u00b7ten", "lang", "und", "bang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bey Thr\u00e4nen und bey Jammer", "tokens": ["Bey", "Thr\u00e4\u00b7nen", "und", "bey", "Jam\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch t\u00f6nt der Spindel Sang.", "tokens": ["Noch", "t\u00f6nt", "der", "Spin\u00b7del", "Sang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sing' nur! Du singst den Sorgen", "tokens": ["Sing'", "nur", "!", "Du", "singst", "den", "Sor\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Armuth endlich Tod.", "tokens": ["Der", "Ar\u00b7muth", "end\u00b7lich", "Tod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Steig auf, du lichter Morgen!", "tokens": ["Steig", "auf", ",", "du", "lich\u00b7ter", "Mor\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bring' das ersung'ne Brod.", "tokens": ["Bring'", "das", "er\u00b7sung'\u00b7ne", "Brod", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}