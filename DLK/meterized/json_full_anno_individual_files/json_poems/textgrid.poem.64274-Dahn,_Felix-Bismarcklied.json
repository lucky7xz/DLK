{"textgrid.poem.64274": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "Bismarcklied", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun tummle, Germania, dich stolz auf dem Pferd,", "tokens": ["Nun", "tumm\u00b7le", ",", "Ger\u00b7ma\u00b7nia", ",", "dich", "stolz", "auf", "dem", "Pferd", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NE", "$,", "PRF", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Darauf dich dein Bismarck geschwungen,", "tokens": ["Da\u00b7rauf", "dich", "dein", "Bis\u00b7marck", "ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auf dem Haupte den Helm, in der Rechten das Schwert", "tokens": ["Auf", "dem", "Haup\u00b7te", "den", "Helm", ",", "in", "der", "Rech\u00b7ten", "das", "Schwert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Und den Geist und den Mut unbezwungen.", "tokens": ["Und", "den", "Geist", "und", "den", "Mut", "un\u00b7be\u00b7zwun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "KON", "ART", "NN", "ADJD", "$."], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.5": {"text": "So sprenge durch Wetter und St\u00fcrme dahin,", "tokens": ["So", "spren\u00b7ge", "durch", "Wet\u00b7ter", "und", "St\u00fcr\u00b7me", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "KON", "NN", "PAV", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Du walk\u00fcrenverschwisterte Reiterin.", "tokens": ["Du", "wal\u00b7k\u00fc\u00b7ren\u00b7ver\u00b7schwis\u00b7ter\u00b7te", "Rei\u00b7te\u00b7rin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wohl Feinde ringsum! Doch \u00bbviel Feind', viel Ehr'\u00ab,", "tokens": ["Wohl", "Fein\u00b7de", "ring\u00b7sum", "!", "Doch", "\u00bb", "viel", "Feind'", ",", "viel", "Ehr'", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "ADV", "$.", "KON", "$(", "PIAT", "NN", "$,", "PIAT", "NN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Du vertraue getrost deinen Sternen;", "tokens": ["Du", "ver\u00b7trau\u00b7e", "ge\u00b7trost", "dei\u00b7nen", "Ster\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Dein Bismarck z\u00e4hlte der Feinde noch mehr,", "tokens": ["Dein", "Bis\u00b7marck", "z\u00e4hl\u00b7te", "der", "Fein\u00b7de", "noch", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Konnte doch das F\u00fcrchten nicht lernen.", "tokens": ["Konn\u00b7te", "doch", "das", "F\u00fcrch\u00b7ten", "nicht", "ler\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Und wir Deutschen haben getreu bewahrt", "tokens": ["Und", "wir", "Deut\u00b7schen", "ha\u00b7ben", "ge\u00b7treu", "be\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "NN", "VAFIN", "ADJD", "VVPP"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "\u2013 Es gruselt uns nicht! \u2013 die Siegfried-Art.", "tokens": ["\u2013", "Es", "gru\u00b7selt", "uns", "nicht", "!", "\u2013", "die", "Sieg\u00b7frie\u00b7dArt", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$(", "ART", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und ward er vom Steuer hinweggedr\u00e4ngt,", "tokens": ["Und", "ward", "er", "vom", "Steu\u00b7er", "hin\u00b7weg\u00b7ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Wohl mu\u00df es dich gr\u00e4men und schmerzen;", "tokens": ["Wohl", "mu\u00df", "es", "dich", "gr\u00e4\u00b7men", "und", "schmer\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "KON", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.15": {"text": "Doch es steht sein Bild, tief eingesenkt,", "tokens": ["Doch", "es", "steht", "sein", "Bild", ",", "tief", "ein\u00b7ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.16": {"text": "Sein gewaltiges Bild dir im Herzen.", "tokens": ["Sein", "ge\u00b7wal\u00b7ti\u00b7ges", "Bild", "dir", "im", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.17": {"text": "Es zerbr\u00f6ckelt der Stein, es verrostet das Erz,", "tokens": ["Es", "zer\u00b7br\u00f6\u00b7ckelt", "der", "Stein", ",", "es", "ver\u00b7ros\u00b7tet", "das", "Erz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Treu wahrt seine Sch\u00e4tze das deutsche Herz.", "tokens": ["Treu", "wahrt", "sei\u00b7ne", "Sch\u00e4t\u00b7ze", "das", "deut\u00b7sche", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.19": {"text": "Der den Staat dir schuf und geschirmt dein Recht", "tokens": ["Der", "den", "Staat", "dir", "schuf", "und", "ge\u00b7schirmt", "dein", "Recht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "PPER", "VVFIN", "KON", "VVPP", "PPOSAT", "NN"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.20": {"text": "Und dich hob zum Ruhm \u2013 aus der Schande, \u2013", "tokens": ["Und", "dich", "hob", "zum", "Ruhm", "\u2013", "aus", "der", "Schan\u00b7de", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "$(", "APPR", "ART", "NN", "$,", "$("], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Fort leb