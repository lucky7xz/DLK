{"textgrid.poem.26562": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seltsam f\u00fcrwahr,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltsam f\u00fcrwahr,", "tokens": ["Selt\u00b7sam", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ihr holden Frau'n, ist der Zwiespalt,", "tokens": ["Ihr", "hol\u00b7den", "Frau'n", ",", "ist", "der", "Zwies\u00b7palt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der jetzt eu're Brust bewegt!", "tokens": ["Der", "jetzt", "eu'\u00b7re", "Brust", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6n wollt ihr sein \u2013 sch\u00f6ner denn je,", "tokens": ["Sch\u00f6n", "wollt", "ihr", "sein", "\u2013", "sch\u00f6\u00b7ner", "denn", "je", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "PPOSAT", "$(", "ADJD", "KON", "ADV", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und erfinderischer im Schm\u00fccken des Leibes,", "tokens": ["Und", "er\u00b7fin\u00b7de\u00b7ri\u00b7scher", "im", "Schm\u00fc\u00b7cken", "des", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Als zur Zeit verruchtesten Het\u00e4renthums,", "tokens": ["Als", "zur", "Zeit", "ver\u00b7ruch\u00b7tes\u00b7ten", "He\u00b7t\u00e4\u00b7ren\u00b7thums", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Tragt ihr Reize zur Schau,", "tokens": ["Tragt", "ihr", "Rei\u00b7ze", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Entflammend das Auge \u2013", "tokens": ["Ent\u00b7flam\u00b7mend", "das", "Au\u00b7ge", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Entflammend die W\u00fcnsche des Mannes.", "tokens": ["Ent\u00b7flam\u00b7mend", "die", "W\u00fcn\u00b7sche", "des", "Man\u00b7nes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.10": {"text": "Und doch wie th\u00f6richt!", "tokens": ["Und", "doch", "wie", "th\u00f6\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Verletzt seid ihr, emp\u00f6rt,", "tokens": ["Ver\u00b7letzt", "seid", "ihr", ",", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Wenn er sucht, was ihr bietet.", "tokens": ["Wenn", "er", "sucht", ",", "was", "ihr", "bie\u00b7tet", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.13": {"text": "Zorngl\u00fchend fragt ihr,", "tokens": ["Zorn\u00b7gl\u00fc\u00b7hend", "fragt", "ihr", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.14": {"text": "Ob ihr geschaffen nur seid,", "tokens": ["Ob", "ihr", "ge\u00b7schaf\u00b7fen", "nur", "seid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "ADV", "VAFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Irdisch gemeine Lust zu stillen", "tokens": ["Ir\u00b7disch", "ge\u00b7mei\u00b7ne", "Lust", "zu", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.16": {"text": "Als entw\u00fcrdigte Sklavinnen:", "tokens": ["Als", "ent\u00b7w\u00fcr\u00b7dig\u00b7te", "Skla\u00b7vin\u00b7nen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Oder ob nicht auch in euch", "tokens": ["O\u00b7der", "ob", "nicht", "auch", "in", "euch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "ADV", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Frei und selbstbestimmend walte der Geist,", "tokens": ["Frei", "und", "selbst\u00b7be\u00b7stim\u00b7mend", "wal\u00b7te", "der", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.19": {"text": "Der jetzt den Mann zum Herrscher macht,", "tokens": ["Der", "jetzt", "den", "Mann", "zum", "Herr\u00b7scher", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wie fr\u00fcher die gewaltige St\u00e4rke des Arms? \u2013", "tokens": ["Wie", "fr\u00fc\u00b7her", "die", "ge\u00b7wal\u00b7ti\u00b7ge", "St\u00e4r\u00b7ke", "des", "Arms", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADJD", "ART", "ADJA", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Ja, er waltet in euch!", "tokens": ["Ja", ",", "er", "wal\u00b7tet", "in", "euch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Ringen k\u00f6nnt auch ihr", "tokens": ["Rin\u00b7gen", "k\u00f6nnt", "auch", "ihr"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "PPOSAT"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Mit erwachender Kraft", "tokens": ["Mit", "er\u00b7wa\u00b7chen\u00b7der", "Kraft"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.24": {"text": "Nach den G\u00fctern der Erde,", "tokens": ["Nach", "den", "G\u00fc\u00b7tern", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.25": {"text": "Nach B\u00fcrgerkronen und Lorbeerkr\u00e4nzen!", "tokens": ["Nach", "B\u00fcr\u00b7ger\u00b7kro\u00b7nen", "und", "Lor\u00b7beer\u00b7kr\u00e4n\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.26": {"text": "Aber nimmermehr w\u00e4hnt,", "tokens": ["A\u00b7ber", "nim\u00b7mer\u00b7mehr", "w\u00e4hnt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.27": {"text": "Da\u00df damit euch anbrechen werden", "tokens": ["Da\u00df", "da\u00b7mit", "euch", "an\u00b7bre\u00b7chen", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PPER", "VVINF", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "H\u00f6here, frohere Tage des Seins!", "tokens": ["H\u00f6\u00b7he\u00b7re", ",", "fro\u00b7he\u00b7re", "Ta\u00b7ge", "des", "Seins", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.29": {"text": "Gl\u00fccklich werdet ihr stets nur werden", "tokens": ["Gl\u00fcck\u00b7lich", "wer\u00b7det", "ihr", "stets", "nur", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "ADV", "VAINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.30": {"text": "Durch eu're Sch\u00f6nheit \u2013", "tokens": ["Durch", "eu'\u00b7re", "Sch\u00f6n\u00b7heit", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.31": {"text": "Gl\u00fccklich nur, so lang sie dauert.", "tokens": ["Gl\u00fcck\u00b7lich", "nur", ",", "so", "lang", "sie", "dau\u00b7ert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Denn zu tief verkn\u00fcpft mit der Natur", "tokens": ["Denn", "zu", "tief", "ver\u00b7kn\u00fcpft", "mit", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKA", "ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.33": {"text": "Ist euer Schicksal,", "tokens": ["Ist", "eu\u00b7er", "Schick\u00b7sal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.34": {"text": "Und zu dem alten Fluch des Geschlechts,", "tokens": ["Und", "zu", "dem", "al\u00b7ten", "Fluch", "des", "Ge\u00b7schlechts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.35": {"text": "Dem kaum Eine entgeht:", "tokens": ["Dem", "kaum", "Ei\u00b7ne", "ent\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.36": {"text": "Zu den Leiden get\u00e4uschter Liebe", "tokens": ["Zu", "den", "Lei\u00b7den", "ge\u00b7t\u00e4uschter", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Werden gesellen sich noch", "tokens": ["Wer\u00b7den", "ge\u00b7sel\u00b7len", "sich", "noch"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "PRF", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.38": {"text": "Get\u00e4uschter Ehrgeiz,", "tokens": ["Ge\u00b7t\u00e4uschter", "Ehr\u00b7geiz", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.39": {"text": "Verfehltes Wirken,", "tokens": ["Ver\u00b7fehl\u00b7tes", "Wir\u00b7ken", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Fruchtloses Ringen,", "tokens": ["Frucht\u00b7lo\u00b7ses", "Rin\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.41": {"text": "Die Qual des Denkens \u2013", "tokens": ["Die", "Qual", "des", "Den\u00b7kens", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Und all die tausend Leiden und K\u00e4mpfe des Mannes ...", "tokens": ["Und", "all", "die", "tau\u00b7send", "Lei\u00b7den", "und", "K\u00e4mp\u00b7fe", "des", "Man\u00b7nes", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ART", "CARD", "NN", "KON", "NN", "ART", "NN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Reizt euch das Ziel?", "tokens": ["Reizt", "euch", "das", "Ziel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Seltsam f\u00fcrwahr,", "tokens": ["Selt\u00b7sam", "f\u00fcr\u00b7wahr", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Ihr holden Frau'n, ist der Zwiespalt,", "tokens": ["Ihr", "hol\u00b7den", "Frau'n", ",", "ist", "der", "Zwies\u00b7palt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der jetzt eu're Brust bewegt!", "tokens": ["Der", "jetzt", "eu'\u00b7re", "Brust", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP",