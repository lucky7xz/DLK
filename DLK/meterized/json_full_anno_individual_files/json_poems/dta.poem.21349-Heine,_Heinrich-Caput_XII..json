{"dta.poem.21349": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie h\u00f6rten von meiner Ankunft gewi\u00df,", "tokens": ["Sie", "h\u00f6r\u00b7ten", "von", "mei\u00b7ner", "An\u00b7kunft", "ge\u00b7wi\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Bestien, und mir zu Ehre", "tokens": ["Die", "Be\u00b7sti\u00b7en", ",", "und", "mir", "zu", "Eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPER", "APPR", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Illuminirten sie den Wald,", "tokens": ["Il\u00b7lu\u00b7min\u00b7ir\u00b7ten", "sie", "den", "Wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und singen sie ihre Ch\u00f6re.", "tokens": ["Und", "sin\u00b7gen", "sie", "ih\u00b7re", "Ch\u00f6\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Das ist ein St\u00e4ndchen, ich merke es jetzt,", "tokens": ["Das", "ist", "ein", "St\u00e4nd\u00b7chen", ",", "ich", "mer\u00b7ke", "es", "jetzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich soll gefeyert werden!", "tokens": ["Ich", "soll", "ge\u00b7fey\u00b7ert", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich warf mich gleich in Positur", "tokens": ["Ich", "warf", "mich", "gleich", "in", "Po\u00b7si\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sprach mit ger\u00fchrten Gebehrden:", "tokens": ["Und", "sprach", "mit", "ge\u00b7r\u00fchr\u00b7ten", "Ge\u00b7behr\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "\u201emitw\u00f6lfe! Ich bin gl\u00fccklich heut", "tokens": ["\u201e", "mit\u00b7w\u00f6l\u00b7fe", "!", "Ich", "bin", "gl\u00fcck\u00b7lich", "heut"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$.", "PPER", "VAFIN", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Eurer Mitte zu weilen,", "tokens": ["In", "Eu\u00b7rer", "Mit\u00b7te", "zu", "wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wo so viel edle Gem\u00fcther mir", "tokens": ["Wo", "so", "viel", "ed\u00b7le", "Ge\u00b7m\u00fc\u00b7ther", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PIAT", "ADJA", "NN", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Mit Liebe entgegenheulen.", "tokens": ["Mit", "Lie\u00b7be", "ent\u00b7ge\u00b7gen\u00b7heu\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "\u201ewas ich in diesem Augenblick", "tokens": ["\u201e", "was", "ich", "in", "die\u00b7sem", "Au\u00b7gen\u00b7blick"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PWS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empfinde, ist unerme\u00dflich;", "tokens": ["Emp\u00b7fin\u00b7de", ",", "ist", "un\u00b7er\u00b7me\u00df\u00b7lich", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ach! diese sch\u00f6ne Stunde bleibt", "tokens": ["Ach", "!", "die\u00b7se", "sch\u00f6\u00b7ne", "Stun\u00b7de", "bleibt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PDAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir ewig unverge\u00dflich.", "tokens": ["Mir", "e\u00b7wig", "un\u00b7ver\u00b7ge\u00df\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}