{"textgrid.poem.59379": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Sch\u00e4fer spricht, wenn er fr\u00fchmorgens weidet:", "tokens": ["Der", "Sch\u00e4\u00b7fer", "spricht", ",", "wenn", "er", "fr\u00fch\u00b7mor\u00b7gens", "wei\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "\u00bbdort dr\u00fcben wohnt sie hinter Berg' und Fl\u00fcssen!\u00ab", "tokens": ["\u00bb", "dort", "dr\u00fc\u00b7ben", "wohnt", "sie", "hin\u00b7ter", "Ber\u00b7g'", "und", "Fl\u00fcs\u00b7sen", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "VVFIN", "PPER", "APPR", "NE", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Doch seine Wunden deckt sie gern mit K\u00fcssen,", "tokens": ["Doch", "sei\u00b7ne", "Wun\u00b7den", "deckt", "sie", "gern", "mit", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wann lauschend Licht am stillen Abend scheidet.", "tokens": ["Wann", "lau\u00b7schend", "Licht", "am", "stil\u00b7len", "A\u00b7bend", "schei\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ob neu der Morgenschmuck die Erde kleidet,", "tokens": ["Ob", "neu", "der", "Mor\u00b7gen\u00b7schmuck", "die", "Er\u00b7de", "klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ob Nachtigallen Nacht und Stern' begr\u00fc\u00dfen,", "tokens": ["Ob", "Nach\u00b7ti\u00b7gal\u00b7len", "Nacht", "und", "Stern'", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Stets fern und nah bleibt ", "tokens": ["Stets", "fern", "und", "nah", "bleibt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Die in dem Lenz mich ewig sucht und meidet. \u2013", "tokens": ["Die", "in", "dem", "Lenz", "mich", "e\u00b7wig", "sucht", "und", "mei\u00b7det", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "ADJD", "VVFIN", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Doch h\u00f6r ich wunderbare Stimmen sprechen:", "tokens": ["Doch", "h\u00f6r", "ich", "wun\u00b7der\u00b7ba\u00b7re", "Stim\u00b7men", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdie Perlen, die du treu geweint im Schmerze,", "tokens": ["\u00bb", "die", "Per\u00b7len", ",", "die", "du", "treu", "ge\u00b7weint", "im", "Schmer\u00b7ze", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wird sie sorglich all zusammenbinden,", "tokens": ["Sie", "wird", "sie", "sorg\u00b7lich", "all", "zu\u00b7sam\u00b7men\u00b7bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Mit eigner Kette so dich s\u00fc\u00df umwinden,", "tokens": ["Mit", "eig\u00b7ner", "Ket\u00b7te", "so", "dich", "s\u00fc\u00df", "um\u00b7win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinaufziehn dich an Mund und bl\u00fchend Herze \u2013", "tokens": ["Hin\u00b7auf\u00b7ziehn", "dich", "an", "Mund", "und", "bl\u00fc\u00b7hend", "Her\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was Himmel schlo\u00df, mag nicht der Himmel brechen.\u00ab", "tokens": ["Was", "Him\u00b7mel", "schlo\u00df", ",", "mag", "nicht", "der", "Him\u00b7mel", "bre\u00b7chen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "NN", "VVFIN", "$,", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Sch\u00e4fer spricht, wenn er fr\u00fchmorgens weidet:", "tokens": ["Der", "Sch\u00e4\u00b7fer", "spricht", ",", "wenn