{"textgrid.poem.59739": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Lebensbescheid", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wieviel tausend Sonnen und Regenbogen", "tokens": ["Wie\u00b7viel", "tau\u00b7send", "Son\u00b7nen", "und", "Re\u00b7gen\u00b7bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "CARD", "NN", "KON", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Sind an dir und \u00fcber dich hingezogen!", "tokens": ["Sind", "an", "dir", "und", "\u00fc\u00b7ber", "dich", "hin\u00b7ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "KON", "APPR", "PPER", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Wieviel tausend Scherz und Schmerz!", "tokens": ["Wie\u00b7viel", "tau\u00b7send", "Scherz", "und", "Schmerz", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprich ein Wort, du altes, krankes Herz.", "tokens": ["Sprich", "ein", "Wort", ",", "du", "al\u00b7tes", ",", "kran\u00b7kes", "Herz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PPER", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Frage rundum bei weisesten Christen und Heiden,", "tokens": ["Fra\u00b7ge", "run\u00b7dum", "bei", "wei\u00b7ses\u00b7ten", "Chris\u00b7ten", "und", "Hei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Frage rund, vernimm den Klang,", "tokens": ["Fra\u00b7ge", "rund", ",", "ver\u00b7nimm", "den", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "VVIMP", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ihn schon Homer und David sang.", "tokens": ["Wie", "ihn", "schon", "Ho\u00b7mer", "und", "Da\u00b7vid", "sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NE", "KON", "NE", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Soweit Menschen hier bl\u00fchen und verbl\u00fchen,", "tokens": ["So\u00b7weit", "Men\u00b7schen", "hier", "bl\u00fc\u00b7hen", "und", "ver\u00b7bl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "VVFIN", "KON", "VVINF", "$,"], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Leuchten die Sterne gleich \u00fcber Freuden und M\u00fchen \u2013", "tokens": ["Leuch\u00b7ten", "die", "Ster\u00b7ne", "gleich", "\u00fc\u00b7ber", "Freu\u00b7den", "und", "M\u00fc\u00b7hen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Wer spricht hier den letzten Spruch?", "tokens": ["Wer", "spricht", "hier", "den", "letz\u00b7ten", "Spruch", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wiss', ich hatte beides \u00fcbergnug.", "tokens": ["Wiss'", ",", "