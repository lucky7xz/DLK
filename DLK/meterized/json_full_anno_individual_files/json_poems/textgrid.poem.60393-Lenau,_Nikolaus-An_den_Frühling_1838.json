ord"], "pos": ["ART", "ART", "ADJA", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ihrem Feind entgegenh\u00e4lt,", "tokens": ["Ih\u00b7rem", "Feind", "ent\u00b7ge\u00b7gen\u00b7h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Das Marienbild.", "tokens": ["Das", "Ma\u00b7ri\u00b7en\u00b7bild", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "K\u00fcsse deinen letzten Ku\u00df,", "tokens": ["K\u00fcs\u00b7se", "dei\u00b7nen", "letz\u00b7ten", "Ku\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fchling, s\u00fc\u00df und warm!", "tokens": ["Fr\u00fch\u00b7ling", ",", "s\u00fc\u00df", "und", "warm", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Eiche und Maria mu\u00df", "tokens": ["Ei\u00b7che", "und", "Ma\u00b7ria", "mu\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NE", "VMFIN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Fort aus deinem Arm!", "tokens": ["Fort", "aus", "dei\u00b7nem", "Arm", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Pfeilgeschwind und schnurgerad,", "tokens": ["Pfeil\u00b7ge\u00b7schwind", "und", "schnur\u00b7ge\u00b7rad", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nimmt der Wagen bald", "tokens": ["Nimmt", "der", "Wa\u00b7gen", "bald"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Bl\u00fct und Andacht unters Rad,", "tokens": ["Bl\u00fct", "und", "An\u00b7dacht", "un\u00b7ters", "Rad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sausend durch den Wald.", "tokens": ["Sau\u00b7send", "durch", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Lieber Lenz, ich frage dich,", "tokens": ["Lie\u00b7ber", "Lenz", ",", "ich", "fra\u00b7ge", "dich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Holt, wie er vertraut,", "tokens": ["Holt", ",", "wie", "er", "ver\u00b7traut", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hier der Mensch die Freiheit sich,", "tokens": ["Hier", "der", "Mensch", "die", "Frei\u00b7heit", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die ersehnte Braut?", "tokens": ["Die", "er\u00b7sehn\u00b7te", "Braut", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Lohnt ein sch\u00f6ner Freudenkranz", "tokens": ["Lohnt", "ein", "sch\u00f6\u00b7ner", "Freu\u00b7den\