{"textgrid.poem.60393": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "An den Fr\u00fchling 1838", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lieber Fr\u00fchling, sage mir,", "tokens": ["Lie\u00b7ber", "Fr\u00fch\u00b7ling", ",", "sa\u00b7ge", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn du bist Prophet,", "tokens": ["Denn", "du", "bist", "Pro\u00b7phet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ob man auf dem Wege hier", "tokens": ["Ob", "man", "auf", "dem", "We\u00b7ge", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einst zum Heile geht?", "tokens": ["Einst", "zum", "Hei\u00b7le", "geht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Mitten durch den gr\u00fcnen Hain,", "tokens": ["Mit\u00b7ten", "durch", "den", "gr\u00fc\u00b7nen", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungest\u00fcmer Hast,", "tokens": ["Un\u00b7ge\u00b7st\u00fc\u00b7mer", "Hast", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Fri\u00dft die Eisenbahn herein,", "tokens": ["Fri\u00dft", "die