{"textgrid.poem.58299": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "337. Wer Gott kennt, der mu\u00df ihn lieben", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt, Menschenkinder, schmecket doch,", "tokens": ["Kommt", ",", "Men\u00b7schen\u00b7kin\u00b7der", ",", "schme\u00b7cket", "doch", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Gott so s\u00fc\u00df, wie sanft sein Joch;", "tokens": ["Wie", "Gott", "so", "s\u00fc\u00df", ",", "wie", "sanft", "sein", "Joch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kehrt in euch ein, so werd't ihr's sehen!", "tokens": ["Kehrt", "in", "euch", "ein", ",", "so", "werd't", "ih\u00b7r's", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "PIS", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Wer diesen Schatz nur wesentlich,", "tokens": ["Wer", "die\u00b7sen", "Schatz", "nur", "we\u00b7sent\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gott selbst, gefunden hat in sich,", "tokens": ["Gott", "selbst", ",", "ge\u00b7fun\u00b7den", "hat", "in", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["N