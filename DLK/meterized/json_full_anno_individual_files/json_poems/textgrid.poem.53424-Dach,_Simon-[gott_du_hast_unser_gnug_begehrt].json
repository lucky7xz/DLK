{"textgrid.poem.53424": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[gott du hast unser gnug begehrt]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gott du hast unser gnug begehrt,", "tokens": ["Gott", "du", "hast", "un\u00b7ser", "gnug", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VAFIN", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns gnug zu dir gelocket.", "tokens": ["Uns", "gnug", "zu", "dir", "ge\u00b7lo\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer hat sich gro\u00df zu dir bekehrt?", "tokens": ["Wer", "hat", "sich", "gro\u00df", "zu", "dir", "be\u00b7kehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PRF", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer nicht sein Hertz verstocket?", "tokens": ["Wer", "nicht", "sein", "Hertz", "ver\u00b7sto\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun ist dein Zorn auch angebrandt,", "tokens": ["Nun", "ist", "dein", "Zorn", "auch", "an\u00b7ge\u00b7brandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie wird es uns ergehen!", "tokens": ["Wie", "wird", "es", "uns", "er\u00b7ge\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dann deiner au\u00dfgereckten Hand", "tokens": ["Dann", "dei\u00b7ner", "au\u00df\u00b7ge\u00b7reck\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Taug nichts zu widerstehen.", "tokens": ["Taug", "nichts", "zu", "wi\u00b7der\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Pestilentze dein Gericht,", "tokens": ["Die", "Pes\u00b7ti\u00b7lent\u00b7ze", "dein", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greifft umb sich hin und wieder,", "tokens": ["Greifft", "umb", "sich", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "PTKVZ", "KON", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Glocken Klang-Lied feyret nicht,", "tokens": ["Der", "Glo\u00b7cken", "Klang\u00b7Lied", "fey\u00b7ret", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir fallen h\u00e4uffig nieder", "tokens": ["Wir", "fal\u00b7len", "h\u00e4uf\u00b7fig", "nie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und tragen f\u00fcr einander Scheu.", "tokens": ["Und", "tra\u00b7gen", "f\u00fcr", "ein\u00b7an\u00b7der", "Scheu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wir m\u00fcssen einsam leben", "tokens": ["Wir", "m\u00fcs\u00b7sen", "ein\u00b7sam", "le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und uns der alten Lieb und Treu", "tokens": ["Und", "uns", "der", "al\u00b7ten", "Lieb", "und", "Treu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus Todes-Furcht begeben.", "tokens": ["Aus", "To\u00b7des\u00b7Furcht", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir fliehen unsre Stadt und Hau\u00df,", "tokens": ["Wir", "flie\u00b7hen", "uns\u00b7re", "Stadt", "und", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und keiner seine S\u00fcnden,", "tokens": ["Und", "kei\u00b7ner", "sei\u00b7ne", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der schauet hie, der dort hinau\u00df,", "tokens": ["Der", "schau\u00b7et", "hie", ",", "der", "dort", "hin\u00b7au\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "ADV", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gesunde Lufft zu finden.", "tokens": ["Ge\u00b7sun\u00b7de", "Lufft", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo aber ist dem Leben Raht,", "tokens": ["Wo", "a\u00b7ber", "ist", "dem", "Le\u00b7ben", "Raht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wolt ich die Welt auch fliehen,", "tokens": ["Wolt", "ich", "die", "Welt", "auch", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wann ich nicht meiner Missethat", "tokens": ["Wann", "ich", "nicht", "mei\u00b7ner", "Mis\u00b7se\u00b7that"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PTKNEG", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Werd au\u00df dem Wege ziehen?", "tokens": ["Werd", "au\u00df", "dem", "We\u00b7ge", "zie\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ein Hirsch, dem schon des J\u00e4gers-Pfeil,", "tokens": ["Ein", "Hirsch", ",", "dem", "schon", "des", "J\u00e4\u00b7ger\u00b7sPfeil", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Hertz hat eingenommen,", "tokens": ["Das", "Hertz", "hat", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es lauff in Windgeschwinder Eil,", "tokens": ["Es", "lauff", "in", "Wind\u00b7ge\u00b7schwin\u00b7der", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird es dem Todt entkommen!", "tokens": ["Wird", "es", "dem", "Todt", "ent\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wol dem, der von der S\u00fcnden l\u00e4st,", "tokens": ["Wol", "dem", ",", "der", "von", "der", "S\u00fcn\u00b7den", "l\u00e4st", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Kein Gifft wird den erschleichen,", "tokens": ["Kein", "Gifft", "wird", "den", "er\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ja er wird beydes hie der Pest,", "tokens": ["Ja", "er", "wird", "bey\u00b7des", "hie", "der", "Pest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VAFIN", "PIS", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der H\u00f6llen dort entweichen.", "tokens": ["Der", "H\u00f6l\u00b7len", "dort", "ent\u00b7wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hilff, Vater, uns in diesem Lauff,", "tokens": ["Hilff", ",", "Va\u00b7ter", ",", "uns", "in", "die\u00b7sem", "Lauff", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Sohn, das wahre Leben,", "tokens": ["Dein", "Sohn", ",", "das", "wah\u00b7re", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Thu seine Seit und H\u00e4nd uns auff,", "tokens": ["Thu", "sei\u00b7ne", "Seit", "und", "H\u00e4nd", "uns", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dareinn wir uns begeben,", "tokens": ["Da\u00b7reinn", "wir", "uns", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und weil wir deines Eiffers Glut", "tokens": ["Und", "weil", "wir", "dei\u00b7nes", "Eif\u00b7fers", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "(wie recht) empfinden m\u00fcssen,", "tokens": ["(", "wie", "recht", ")", "emp\u00b7fin\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWAV", "ADJD", "$(", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Lehr uns die wolverdiente Ruht", "tokens": ["Lehr", "uns", "die", "wol\u00b7ver\u00b7dien\u00b7te", "Ruht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In tieffster Demuht k\u00fcssen.", "tokens": ["In", "tieffs\u00b7ter", "De\u00b7muht", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "La\u00df uns au\u00df gro\u00dfer Bangigkeit", "tokens": ["La\u00df", "uns", "au\u00df", "gro\u00b7\u00dfer", "Ban\u00b7gig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Todes nicht verzagen,", "tokens": ["Des", "To\u00b7des", "nicht", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dann wer hat nicht zu seiner Zeit", "tokens": ["Dann", "wer", "hat", "nicht", "zu", "sei\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWS", "VAFIN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schuld dir abzutragen?", "tokens": ["Die", "Schuld", "dir", "ab\u00b7zu\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und wol uns, da\u00df dein Krieges-Schwerdt", "tokens": ["Und", "wol", "uns", ",", "da\u00df", "dein", "Krie\u00b7ge\u00b7sSchwerdt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PPER", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht eindringt zu uns allen,", "tokens": ["Nicht", "ein\u00b7dringt", "zu", "uns", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "APPR", "PPER", "PIS", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Und wir gesch\u00e4tzt sind noch so werth,", "tokens": ["Und", "wir", "ge\u00b7sch\u00e4tzt", "sind", "noch", "so", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "VAFIN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In deine Hand zu fallen.", "tokens": ["In", "dei\u00b7ne", "Hand", "zu", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Breit \u00fcber uns in der Gefahr", "tokens": ["Breit", "\u00fc\u00b7ber", "uns", "in", "der", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Fl\u00fcgel deiner Gnaden,", "tokens": ["Die", "Fl\u00fc\u00b7gel", "dei\u00b7ner", "Gna\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nimm unser Tritt und Wege war,", "tokens": ["Nimm", "un\u00b7ser", "Tritt", "und", "We\u00b7ge", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wird kein Gifft uns schaden,", "tokens": ["So", "wird", "kein", "Gifft", "uns", "scha\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Trett' auch die H\u00f6ll in einen Sinn,", "tokens": ["Trett'", "auch", "die", "H\u00f6ll", "in", "ei\u00b7nen", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns feindlich zu bestreiten,", "tokens": ["Uns", "feind\u00b7lich", "zu", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und fielen tausend Scharen hin", "tokens": ["Und", "fie\u00b7len", "tau\u00b7send", "Scha\u00b7ren", "hin"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "CARD", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Umb uns auff allen Seiten.", "tokens": ["Umb", "uns", "auff", "al\u00b7len", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Geriethen wir dann in den Todt,", "tokens": ["Ge\u00b7rie\u00b7then", "wir", "dann", "in", "den", "Todt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hilff uns ihn bezwingen,", "tokens": ["So", "hilff", "uns", "ihn", "be\u00b7zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bleib bey uns in der letzten Noht", "tokens": ["Bleib", "bey", "uns", "in", "der", "letz\u00b7ten", "Noht"], "token_