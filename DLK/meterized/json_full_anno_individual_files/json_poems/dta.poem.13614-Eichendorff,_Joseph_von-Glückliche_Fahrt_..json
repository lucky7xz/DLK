{"dta.poem.13614": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gl\u00fcckliche Fahrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "W\u00fcnsche sich mit W\u00fcnschen schlagen,", "tokens": ["W\u00fcn\u00b7sche", "sich", "mit", "W\u00fcn\u00b7schen", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Gier wird nie gestillt.", "tokens": ["Und", "die", "Gier", "wird", "nie", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist in dem w\u00fcsten Jagen", "tokens": ["Wer", "ist", "in", "dem", "w\u00fcs\u00b7ten", "Ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da der J\u00e4ger, wer das Wild?", "tokens": ["Da", "der", "J\u00e4\u00b7ger", ",", "wer", "das", "Wild", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PWS", "ART", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.5": {"text": "Seelig, wer es fromm mag wagen,", "tokens": ["See\u00b7lig", ",", "wer", "es", "fromm", "mag", "wa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Durch das Treiben dumpf und wild", "tokens": ["Durch", "das", "Trei\u00b7ben", "dumpf", "und", "wild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "In der festen Brust zu tragen", "tokens": ["In", "der", "fes\u00b7ten", "Brust", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Heil'ger Sch\u00f6nheit hohes Bild!", "tokens": ["Heil'\u00b7ger", "Sch\u00f6n\u00b7heit", "ho\u00b7hes", "Bild", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, da brechen tausend Quellen", "tokens": ["Sieh", ",", "da", "bre\u00b7chen", "tau\u00b7send", "Quel\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "ADJA", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch die felsenharte Welt,", "tokens": ["Durch", "die", "fel\u00b7sen\u00b7har\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Strome wird ihr Schwellen,", "tokens": ["Und", "zum", "Stro\u00b7me", "wird", "ihr", "Schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der melodisch steigt und f\u00e4llt.", "tokens": ["Der", "me\u00b7lo\u00b7disch", "steigt", "und", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ringsum sich die Fernen hellen,", "tokens": ["Ring\u00b7sum", "sich", "die", "Fer\u00b7nen", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ART", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gottes Hauch die Segel schwellt \u2014", "tokens": ["Got\u00b7tes", "Hauch", "die", "Se\u00b7gel", "schwellt"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rettend sp\u00fclen Dich die Wellen", "tokens": ["Ret\u00b7tend", "sp\u00fc\u00b7len", "Dich", "die", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In des Herzens stille Welt.", "tokens": ["In", "des", "Her\u00b7zens", "stil\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}