{"dta.poem.2443": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Si Deus pro nobis, quis contra nos?", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Jst Gott nur Oberster und stehet uns zur Seiten/", "tokens": ["Ist", "Gott", "nur", "O\u00b7bers\u00b7ter", "und", "ste\u00b7het", "uns", "zur", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "NN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "S\u00f6 k\u00e4mpffen wir getrost; Tod/ Teufel/ S\u00fcnd\u2019 und", "tokens": ["S\u00f6", "k\u00e4mpf\u00b7fen", "wir", "ge\u00b7trost", ";", "Tod", "/", "Teu\u00b7fel", "/", "S\u00fcnd'", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "VVPP", "$.", "NN", "$(", "NE", "$(", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Welt/", "tokens": ["Welt", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Die richten nichtes aus wie hart sie immer streiten/", "tokens": ["Die", "rich\u00b7ten", "nich\u00b7tes", "aus", "wie", "hart", "sie", "im\u00b7mer", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PIS", "PTKVZ", "KOKOM", "ADJD", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir/ wir behalten doch das freye Siegesfeld.", "tokens": ["Wir", "/", "wir", "be\u00b7hal\u00b7ten", "doch", "das", "frey\u00b7e", "Sie\u00b7ges\u00b7feld", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Denn welcher seine H\u00fclff\u2019 auff Gott den H\u00f6chsten setzet/", "tokens": ["Denn", "wel\u00b7cher", "sei\u00b7ne", "H\u00fclff'", "auff", "Gott", "den", "H\u00f6chs\u00b7ten", "set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "PPOSAT", "NN", "APPR", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der wird sein Lebenlang durch keinen Feind verletzet.", "tokens": ["Der", "wird", "sein", "Le\u00b7ben\u00b7lang", "durch", "kei\u00b7nen", "Feind", "ver\u00b7let\u00b7zet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}