{"textgrid.poem.35735": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "60. Die Dichtkunst", "genre": "verse", "period": "N.A.", "pub_year": 1795, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nicht sch\u00e4met euch zu singen,", "tokens": ["Nicht", "sch\u00e4\u00b7met", "euch", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ob D\u00fcnkel h\u00f6hnt und grollt!", "tokens": ["Ob", "D\u00fcn\u00b7kel", "h\u00f6hnt", "und", "grollt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch goldner ist, als Gold,", "tokens": ["Noch", "gold\u00b7ner", "ist", ",", "als", "Gold", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJA", "VAFIN", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Gesang von edlen Dingen!", "tokens": ["Ge\u00b7sang", "von", "ed\u00b7len", "Din\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Gesang ward anvertraut,", "tokens": ["Ge\u00b7sang", "ward", "an\u00b7ver\u00b7traut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den starren Geist zu lindern", "tokens": ["Den", "star\u00b7ren", "Geist", "zu", "lin\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text