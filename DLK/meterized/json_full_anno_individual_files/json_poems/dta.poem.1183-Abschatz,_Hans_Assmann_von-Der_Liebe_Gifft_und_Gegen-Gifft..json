{"dta.poem.1183": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der Liebe Gifft und Gegen-Gifft.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Der klugen Aerzte Kunst wei\u00df allem Ubel Rath/ ", "tokens": ["Der", "klu\u00b7gen", "A\u00b7erz\u00b7te", "Kunst", "wei\u00df", "al\u00b7lem", "U\u00b7bel", "Rath", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "PIS", "NN", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Was fast zu finden ist in weiter Erde Schrancken:", "tokens": ["Was", "fast", "zu", "fin\u00b7den", "ist", "in", "wei\u00b7ter", "Er\u00b7de", "Schran\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PTKZU", "VVINF", "VAFIN", "APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie kommts/ da\u00df sie kein Mittel hat", "tokens": ["Wie", "kommts", "/", "da\u00df", "sie", "kein", "Mit\u00b7tel", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$(", "KOUS", "PPER", "PIAT", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr eine Noth/ daran fast alle Welt mu\u00df krancken?", "tokens": ["F\u00fcr", "ei\u00b7ne", "Noth", "/", "da\u00b7ran", "fast", "al\u00b7le", "Welt", "mu\u00df", "kran\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "PAV", "ADV", "PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Hertze/ welches sich von Liebe wund betrifft/", "tokens": ["Ein", "Hert\u00b7ze", "/", "wel\u00b7ches", "sich", "von", "Lie\u00b7be", "wund", "be\u00b7tr\u00b7ifft", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "$(", "PRELS", "PRF", "APPR", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.2": {"text": "Kan seine Hoffnung nicht auff ihre Kr\u00e4uter gr\u00fcnden:", "tokens": ["Kan", "sei\u00b7ne", "Hoff\u00b7nung", "nicht", "auff", "ih\u00b7re", "Kr\u00e4u\u00b7ter", "gr\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Lieb ist Gifft und Gegen-Gifft:", "tokens": ["Die", "Lieb", "ist", "Gifft", "und", "Ge\u00b7gen\u00b7Gifft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man mu\u00df den Scorpion auff seinen Schaden binden.", "tokens": ["Man", "mu\u00df", "den", "Scor\u00b7pi\u00b7on", "auff", "sei\u00b7nen", "Scha\u00b7den", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}