{"dta.poem.23813": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00e4ffer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Kom\u0303t la\u00dft uns wieder gehn/ und zu den Schaafen kehren/", "tokens": ["Kom\u0303t", "la\u00dft", "uns", "wie\u00b7der", "gehn", "/", "und", "zu", "den", "Schaa\u00b7fen", "keh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVIMP", "PPER", "ADV", "VVINF", "$(", "KON", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Liebe m\u00f6chte sonst uns alle Ruh verst\u00f6ren.", "tokens": ["Die", "Lie\u00b7be", "m\u00f6ch\u00b7te", "sonst", "uns", "al\u00b7le", "Ruh", "ver\u00b7st\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "PPER", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ey was vor sch\u00f6nes Volck kriegt man allhier zu sehn!", "tokens": ["Ey", "was", "vor", "sch\u00f6\u00b7nes", "Volck", "kriegt", "man", "all\u00b7hier", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "APPR", "ADJA", "NN", "VVFIN", "PIS", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Unschuld leidet Noht; kom\u0303t la\u00dft uns wieder gehn.", "tokens": ["Die", "Un\u00b7schuld", "lei\u00b7det", "Noht", ";", "kom\u0303t", "la\u00