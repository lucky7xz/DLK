{"dta.poem.18369": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Zum Ausgang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und roth von meinem Herzblut jedes Lied,", "tokens": ["Und", "roth", "von", "mei\u00b7nem", "Herz\u00b7blut", "je\u00b7des", "Lied", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit ihm stell ich mich k\u00fchn in Reih und Glied \u2014", "tokens": ["Mit", "ihm", "stell", "ich", "mich", "k\u00fchn", "in", "Reih", "und", "Glied"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "PRF", "ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Dichtkunst Segen ward in mir zum Fluch!", "tokens": ["Der", "Dicht\u00b7kunst", "Se\u00b7gen", "ward", "in", "mir", "zum", "Fluch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch sei's, ich trag's. Nicht w\u00e4r ich ein Poet,", "tokens": ["Doch", "sei's", ",", "ich", "trag'", "s.", "Nicht", "w\u00e4r", "ich", "ein", "Po\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "abbreviation", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "PPER", "VVFIN", "VVIMP", "PTKNEG", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wollt ich mich anders geben, als ich bin;", "tokens": ["Wollt", "ich", "mich", "an\u00b7ders", "ge\u00b7ben", ",", "als", "ich", "bin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Auch liegt ein Wort, ein altes, mir im Sinn:", "tokens": ["Auch", "liegt", "ein", "Wort", ",", "ein", "al\u00b7tes", ",", "mir", "im", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Oft hilft ein Fluch uns mehr als ein Gebet!", "tokens": ["Oft", "hilft", "ein", "Fluch", "uns", "mehr", "als", "ein", "Ge\u00b7bet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und wahrlich, diese Zeit gleicht jener nicht,", "tokens": ["Und", "wahr\u00b7lich", ",", "die\u00b7se", "Zeit", "gleicht", "je\u00b7ner", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PDAT", "NN", "VVFIN", "PDS", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die uns das Alterthum als goldne pries,", "tokens": ["Die", "uns", "das", "Al\u00b7ter\u00b7thum", "als", "gold\u00b7ne", "pries", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn jeder L\u00fcge lacht ein Paradies", "tokens": ["Denn", "je\u00b7der", "L\u00fc\u00b7ge", "lacht", "ein", "Pa\u00b7ra\u00b7dies"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Wahrheit droht ein Hochgericht!", "tokens": ["Und", "je\u00b7der", "Wahr\u00b7heit", "droht", "ein", "Hoch\u00b7ge\u00b7richt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Schon k\u00fc\u00dft die Welt ein bleiches Abendroth,", "tokens": ["Schon", "k\u00fc\u00dft", "die", "Welt", "ein", "blei\u00b7ches", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die alte Griechensonne des Homer", "tokens": ["Die", "al\u00b7te", "Grie\u00b7chen\u00b7son\u00b7ne", "des", "Ho\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NE"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Hat sich ertr\u00e4nkt ins teifundunkle Meer,", "tokens": ["Hat", "sich", "er\u00b7tr\u00e4nkt", "ins", "tei\u00b7fun\u00b7dunk\u00b7le", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und seine Sense sch\u00e4rft der schwarze Tod.", "tokens": ["Und", "sei\u00b7ne", "Sen\u00b7se", "sch\u00e4rft", "der", "schwar\u00b7ze", "Tod", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Kein Stern, der farbig durch die Wolken bricht,", "tokens": ["Kein", "Stern", ",", "der", "far\u00b7big", "durch", "die", "Wol\u00b7ken", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kein Traum, der k\u00fchlend um die Schl\u00e4fen weht,", "tokens": ["Kein", "Traum", ",", "der", "k\u00fch\u00b7lend", "um", "die", "Schl\u00e4\u00b7fen", "weht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Kein Lied, das Wunder thut wie ein Gebet,", "tokens": ["Kein", "Lied", ",", "das", "Wun\u00b7der", "thut", "wie", "ein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kein Herz, das heimlich mit sich selber spricht!", "tokens": ["Kein", "Herz", ",", "das", "heim\u00b7lich", "mit", "sich", "sel\u00b7ber", "spricht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PRELS", "ADJD", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Doch tappt sich h\u00fcstelnd durch die dunkle Nacht", "tokens": ["Doch", "tappt", "sich", "h\u00fcs\u00b7telnd", "durch", "die", "dunk\u00b7le", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein b\u00f6ses Ding und pocht an deine Th\u00fcr", "tokens": ["Ein", "b\u00f6\u00b7ses", "Ding", "und", "pocht", "an", "dei\u00b7ne", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und zischt wie eine Viper: \u201eKomm herf\u00fcr,", "tokens": ["Und", "zischt", "wie", "ei\u00b7ne", "Vi\u00b7per", ":", "\u201e", "Komm", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "NN", "$.", "$(", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich bin das Herz, womit die S\u00fcnde lacht!", "tokens": ["Ich", "bin", "das", "Herz", ",", "wo\u00b7mit", "die", "S\u00fcn\u00b7de", "lacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Ich wei\u00df, auch du bist mir ein Kind der Zeit,", "tokens": ["Ich", "wei\u00df", ",", "auch", "du", "bist", "mir", "ein", "Kind", "der", "Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PPER", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das mit der Welt und mit sich selber grollt;", "tokens": ["Das", "mit", "der", "Welt", "und", "mit", "sich", "sel\u00b7ber", "grollt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "KON", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber wate bis ans Knie in Gold", "tokens": ["Ich", "a\u00b7ber", "wa\u00b7te", "bis", "ans", "Knie", "in", "Gold"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und h\u00f6re, wie dein Herz nach Wollust schreit.", "tokens": ["Und", "h\u00f6\u00b7re", ",", "wie", "dein", "Herz", "nach", "Wol\u00b7lust", "schreit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPOSAT", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Komm mit, in meinem Lusthaus wohnt das Gl\u00fcck:", "tokens": ["Komm", "mit", ",", "in", "mei\u00b7nem", "Lust\u00b7haus", "wohnt", "das", "Gl\u00fcck", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du trittst hinein, und singend drehn um dich", "tokens": ["Du", "trittst", "hin\u00b7ein", ",", "und", "sin\u00b7gend", "drehn", "um", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "CARD", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vielhundert wei\u00dfe Dirnenleiber sich", "tokens": ["Viel\u00b7hun\u00b7dert", "wei\u00b7\u00dfe", "Dir\u00b7nen\u00b7lei\u00b7ber", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und schlank wirft sie mein Spiegel dir zur\u00fcck.", "tokens": ["Und", "schlank", "wirft", "sie", "mein", "Spie\u00b7gel", "dir", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "In dunkler Nische k\u00fc\u00dft es sich so sch\u00f6n!", "tokens": ["In", "dunk\u00b7ler", "Ni\u00b7sche", "k\u00fc\u00dft", "es", "sich", "so", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und folgst du, s\u00fc\u00dfer Junge, mir, dann klingt,", "tokens": ["Und", "folgst", "du", ",", "s\u00fc\u00b7\u00dfer", "Jun\u00b7ge", ",", "mir", ",", "dann", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,", "PPER", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn einst dein Herzschlag m\u00fcde wird und hinkt,", "tokens": ["Wenn", "einst", "dein", "Herz\u00b7schlag", "m\u00fc\u00b7de", "wird", "und", "hinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ADJD", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dein Todesr\u00f6cheln noch wie Lustgest\u00f6hn!\u201c", "tokens": ["Dein", "To\u00b7des\u00b7r\u00f6\u00b7cheln", "noch", "wie", "Lust\u00b7ge\u00b7st\u00f6hn", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "ADV", "KOKOM", "NE", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So bl\u00e4st es frech dir nachts durchs Schl\u00fcsselloch,", "tokens": ["So", "bl\u00e4st", "es", "frech", "dir", "nachts", "durchs", "Schl\u00fcs\u00b7sel\u00b7loch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Regen rinnt, ums Dachwerk heult der Sturm,", "tokens": ["Der", "Re\u00b7gen", "rinnt", ",", "ums", "Dach\u00b7werk", "heult", "der", "Sturm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dir aber war's, als ob ein feister Wurm", "tokens": ["Dir", "a\u00b7ber", "wa\u00b7r's", ",", "als", "ob", "ein", "feis\u00b7ter", "Wurm"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Dir todtkalt \u00fcbers warme Herz hin kroch.", "tokens": ["Dir", "todt\u00b7kalt", "\u00fc\u00b7bers", "war\u00b7me", "Herz", "hin", "kroch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Und zornig springst du auf und schl\u00e4gst dir Licht", "tokens": ["Und", "zor\u00b7nig", "springst", "du", "auf", "und", "schl\u00e4gst", "dir", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und prallst zur\u00fcck, geekelt und entsetzt,", "tokens": ["Und", "prallst", "zu\u00b7r\u00fcck", ",", "ge\u00b7e\u00b7kelt", "und", "ent\u00b7setzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn vor dir steht, trief\u00e4ugig und zerfetzt,", "tokens": ["Denn", "vor", "dir", "steht", ",", "trie\u00b7f\u00e4u\u00b7gig", "und", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$,", "ADJD", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein altes Weib und grinst dir ins Gesicht.", "tokens": ["Ein", "al\u00b7tes", "Weib", "und", "grinst", "dir", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Dann schreist du auf, denn dumpf hast du gef\u00fchlt,", "tokens": ["Dann", "schreist", "du", "auf", ",", "denn", "dumpf", "hast", "du", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "ADJD", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie dir ein Etwas kalt die Kehle pre\u00dft:", "tokens": ["Wie", "dir", "ein", "Et\u00b7was", "kalt", "die", "Keh\u00b7le", "pre\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADV", "ADJD", "ART", "NN", "VVFIN", "$."],