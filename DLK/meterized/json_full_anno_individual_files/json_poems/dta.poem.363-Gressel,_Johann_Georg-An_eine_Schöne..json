{"dta.poem.363": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An eine Sch\u00f6ne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Der Augen helle Pracht und alles ist galant,", "tokens": ["Der", "Au\u00b7gen", "hel\u00b7le", "Pracht", "und", "al\u00b7les", "ist", "ga\u00b7lant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "PIS", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum glaubt/ mein Engel/ man du seyst aus Engelland!", "tokens": ["Drum", "glaubt", "/", "mein", "En\u00b7gel", "/", "man", "du", "seyst", "aus", "En\u00b7gel\u00b7land", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$(", "PPOSAT", "NN", "$(", "PIS", "PPER", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch Jrrland soll bey dir nur mein Vergn\u00fcgen seyn", "tokens": ["Doch", "Jrr\u00b7land", "soll", "bey", "dir", "nur", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VMFIN", "APPR", "PPER", "ADV", "PPOSAT", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach nimm mich in die Schoo\u00df den weissen Schottland ein.", "tokens": ["Ach", "nimm", "mich", "in", "die", "Schoo\u00df", "den", "weis\u00b7sen", "Schott\u00b7land", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "wor