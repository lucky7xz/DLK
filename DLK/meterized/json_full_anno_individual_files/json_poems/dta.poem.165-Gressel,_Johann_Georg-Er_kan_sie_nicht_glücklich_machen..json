{"dta.poem.165": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er kan sie nicht gl\u00fccklich machen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mu\u00df! ach hartes Wort! ach \u00fcber grosse Pein!", "tokens": ["Ich", "mu\u00df", "!", "ach", "har\u00b7tes", "Wort", "!", "ach", "\u00fc\u00b7ber", "gros\u00b7se", "Pein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$.", "XY", "ADJA", "NN", "$.", "XY", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6nen ", "tokens": ["Der", "sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["ART", "ADJA"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Da\u00df ich hinf\u00fchro ihr nicht kan ergeben bleiben/", "tokens": ["Da\u00df", "ich", "hin\u00b7f\u00fch\u00b7ro", "ihr", "nicht", "kan", "er\u00b7ge\u00b7ben", "blei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "PTKNEG", "VMFIN", "VVPP", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es streut zu unser Lust der Neid den Wermuth ein.", "tokens": ["Es", "streut", "zu", "un\u00b7ser", "Lust", "der", "Neid", "den", "Wer\u00b7muth", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das Schicksahl g\u00f6nnet uns nicht seinen Gnaden Schein/", "tokens": ["Das", "Schick\u00b7sahl", "g\u00f6n\u00b7net", "uns", "nicht", "sei\u00b7nen", "Gna\u00b7den", "Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Freunde mit Gewalt das B\u00fcndni\u00df hintertreiben/", "tokens": ["Die", "Freun\u00b7de", "mit", "Ge\u00b7walt", "das", "B\u00fcn\u00b7dni\u00df", "hin\u00b7ter\u00b7trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich mu\u00df! ich soll hinweg/ man achtet nichts mein Streuben/", "tokens": ["Ich", "mu\u00df", "!", "ich", "soll", "hin\u00b7weg", "/", "man", "ach\u00b7tet", "nichts", "mein", "Streu\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$.", "PPER", "VMFIN", "ADV", "$(", "PIS", "VVFIN", "PIS", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ich soll ihr Liebster nicht/ noch sie mein Sch\u00e4tzgen seyn.", "tokens": ["Ich", "soll", "ihr", "Liebs\u00b7ter", "nicht", "/", "noch", "sie", "mein", "Sch\u00e4tz\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "$(", "ADV", "PPER", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Mein Licht/ sie straff mich nicht/ sie kennt mein treues Hertze", "tokens": ["Mein", "Licht", "/", "sie", "straff", "mich", "nicht", "/", "sie", "kennt", "mein", "treu\u00b7es", "Hert\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$(", "PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie wei\u00df da\u00df ich nicht schuld an diesen Bruche bin.", "tokens": ["Sie", "wei\u00df", "da\u00df", "ich", "nicht", "schuld", "an", "die\u00b7sen", "Bru\u00b7che", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOUS", "PPER", "PTKNEG", "ADJD", "APPR", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ich leide billig Quaal/ mich trifft gerechter Schmertze/", "tokens": ["Ich", "lei\u00b7de", "bil\u00b7lig", "Qua\u00b7al", "/", "mich", "trifft", "ge\u00b7rech\u00b7ter", "Schmert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "NN", "$(", "PPER", "VVFIN", "ADJD", "NN", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Nichts \u00e4ngstet mehr den Geist als ihr getreuer Sinn/", "tokens": ["Nichts", "\u00e4ngs\u00b7tet", "mehr", "den", "Geist", "als", "ihr", "ge\u00b7treu\u00b7er", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ART", "NN", "KOKOM", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und da\u00df sie ohne Schuld durch ihr getreues Lieben/", "tokens": ["Und", "da\u00df", "sie", "oh\u00b7ne", "Schuld", "durch", "ihr", "ge\u00b7treu\u00b7es", "Lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Kommt in Bek\u00fcmmerni\u00df und in ein solch Betr\u00fcben.", "tokens": ["Kommt", "in", "Be\u00b7k\u00fcm\u00b7mer\u00b7ni\u00df", "und", "in", "ein", "solch", "Be\u00b7tr\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "APPR", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+++-+-", "measure": "unknown.measure.septa"}}}}}