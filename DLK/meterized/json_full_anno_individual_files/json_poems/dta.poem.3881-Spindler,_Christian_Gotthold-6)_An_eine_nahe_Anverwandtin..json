{"dta.poem.3881": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "6) An eine nahe Anverwandtin.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Dir, Freundin! dir geh\u00f6rt di\u00df Blat,", "tokens": ["Dir", ",", "Freun\u00b7din", "!", "dir", "ge\u00b7h\u00f6rt", "di\u00df", "Blat", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$.", "PPER", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dir soll mein Kiel zu Ehren singen,", "tokens": ["Dir", "soll", "mein", "Kiel", "zu", "Eh\u00b7ren", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und scheint er dir villeicht sehr mat,", "tokens": ["Und", "scheint", "er", "dir", "vil\u00b7leicht", "sehr", "mat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So soll er doch sein Opffer bringen.", "tokens": ["So", "soll", "er", "doch", "sein", "Opf\u00b7fer", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nimm hin den wahren Freundschaffts-Zoll,", "tokens": ["Nimm", "hin", "den", "wah\u00b7ren", "Freund\u00b7schaffts\u00b7Zoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist ja Lieb und Anmuths voll.", "tokens": ["Du", "bist", "ja", "Lieb", "und", "An\u00b7muths", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich werffe es zu deinen F\u00fcssen,", "tokens": ["Ich", "werf\u00b7fe", "es", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So soll es dich in Ehrfurcht gr\u00fcssen.", "tokens": ["So", "soll", "es", "dich", "in", "Ehr\u00b7furcht", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch welch ein gro\u00dfmuths-volles Thun", "tokens": ["Doch", "welch", "ein", "gro\u00df\u00b7muths\u00b7vol\u00b7les", "Thun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAT", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erquicket die erstarrten Glieder.", "tokens": ["Er\u00b7quic\u00b7ket", "die", "er\u00b7starr\u00b7ten", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun kan ich erstlich wieder ruhn,", "tokens": ["Nun", "kan", "ich", "erst\u00b7lich", "wie\u00b7der", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nunmehro kommt mein Bothe wieder.", "tokens": ["Nun\u00b7meh\u00b7ro", "kommt", "mein", "Bo\u00b7the", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O Himmel! welche s\u00fcsse Lust", "tokens": ["O", "Him\u00b7mel", "!", "wel\u00b7che", "s\u00fcs\u00b7se", "Lust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "PWAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erquicket meine schwache Brust,", "tokens": ["Er\u00b7quic\u00b7ket", "mei\u00b7ne", "schwa\u00b7che", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dein anmuths-volles Antwort-Schreiben", "tokens": ["Dein", "an\u00b7muths\u00b7vol\u00b7les", "Ant\u00b7wor\u00b7tSchrei\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Hie\u00df mich bey nah entgeistert bleiben.", "tokens": ["Hie\u00df", "mich", "bey", "nah", "ent\u00b7geis\u00b7tert", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ADJD", "VVPP", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Nimm nun von meiner treuen Hand,", "tokens": ["Nimm", "nun", "von", "mei\u00b7ner", "treu\u00b7en", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich kan vor dich nichts w\u00fcrdigs finden,", "tokens": ["Ich", "kan", "vor", "dich", "nichts", "w\u00fcr\u00b7digs", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PIS", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein schlecht, doch wohlgemeintes Band,", "tokens": ["Ein", "schlecht", ",", "doch", "wohl\u00b7ge\u00b7mein\u00b7tes", "Band", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Nahmens-Feyer anzubinden.", "tokens": ["Zur", "Nah\u00b7mens\u00b7Fey\u00b7er", "an\u00b7zu\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du, wahrer Tugend \u00e4chtes Bild,", "tokens": ["Du", ",", "wah\u00b7rer", "Tu\u00b7gend", "\u00e4ch\u00b7tes", "Bild", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gedencke von mir was du wilt,", "tokens": ["Ge\u00b7den\u00b7cke", "von", "mir", "was", "du", "wilt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PWS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dir weyhe ich so Hertz als Jahre,", "tokens": ["Dir", "wey\u00b7he", "ich", "so", "Hertz", "als", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Bis auf die schwartze Todten-Bahre.", "tokens": ["Bis", "auf", "die", "schwart\u00b7ze", "Tod\u00b7ten\u00b7Bah\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So hast du denn von mir gesehn,", "tokens": ["So", "hast", "du", "denn", "von", "mir", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die redlich treuen Freundschaffts-Flammen,", "tokens": ["Die", "red\u00b7lich", "treu\u00b7en", "Freund\u00b7schaffts\u00b7Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O! soltest du es frey gestehn?", "tokens": ["O", "!", "sol\u00b7test", "du", "es", "frey", "ge\u00b7stehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kaum kanst du meine Schuld verdammen.", "tokens": ["Kaum", "kanst", "du", "mei\u00b7ne", "Schuld", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich wei\u00df dein frommes Hertze bricht,", "tokens": ["Ich", "wei\u00df", "dein", "from\u00b7mes", "Hert\u00b7ze", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und du verschm\u00e4hst mein Opffer nicht,", "tokens": ["Und", "du", "ver\u00b7schm\u00e4hst", "mein", "Opf\u00b7fer", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du wirst mein treugemeintes Dichten", "tokens": ["Du", "wirst", "mein", "treu\u00b7ge\u00b7mein\u00b7tes", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nach deiner Huld und Gro\u00dfmuth richten.", "tokens": ["Nach", "dei\u00b7ner", "Huld", "und", "Gro\u00df\u00b7muth", "rich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Letzt lebe gl\u00fccklich, sey vergn\u00fcgt,", "tokens": ["Letzt", "le\u00b7be", "gl\u00fcck\u00b7lich", ",", "sey", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "$,", "VAFIN", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nichts soll der Freundschafft B\u00fcndni\u00df trennen,", "tokens": ["Nichts", "soll", "der", "Freund\u00b7schafft", "B\u00fcn\u00b7dni\u00df", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bis einst mein Leib im Moder liegt,", "tokens": ["Bis", "einst", "mein", "Leib", "im", "Mo\u00b7der", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will ich mich deinen Vetter nennen.", "tokens": ["Will", "ich", "mich", "dei\u00b7nen", "Vet\u00b7ter", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gr\u00fc\u00df deiner Eltern liebes Hau\u00df,", "tokens": ["Gr\u00fc\u00df", "dei\u00b7ner", "El\u00b7tern", "lie\u00b7bes", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Danck ihnn vor letzt erwiesnen Schmau\u00df,", "tokens": ["Danck", "ihnn", "vor", "letzt", "er\u00b7wies\u00b7nen", "Schmau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vergieb dem wohlgemeinten Schreiben,", "tokens": ["Ver\u00b7gieb", "dem", "wohl\u00b7ge\u00b7mein\u00b7ten", "Schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df mich stets deinen Diener bleiben.", "tokens": ["La\u00df", "mich", "stets", "dei\u00b7nen", "Die\u00b7ner", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}