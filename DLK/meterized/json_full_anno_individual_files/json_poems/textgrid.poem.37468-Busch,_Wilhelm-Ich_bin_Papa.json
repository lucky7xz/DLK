{"textgrid.poem.37468": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Ich bin Papa", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mitunter schwitzen mu\u00df der Schreiner,", "tokens": ["Mi\u00b7tun\u00b7ter", "schwit\u00b7zen", "mu\u00df", "der", "Schrei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er st\u00f6\u00dft auf manchen harten Ast.", "tokens": ["Er", "st\u00f6\u00dft", "auf", "man\u00b7chen", "har\u00b7ten", "Ast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So geht es auch, wenn unsereiner", "tokens": ["So", "geht", "es", "auch", ",", "wenn", "un\u00b7ser\u00b7ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mit der Gr\u00fcbelei befa\u00dft.", "tokens": ["Sich", "mit", "der", "Gr\u00fc\u00b7be\u00b7lei", "be\u00b7fa\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zum Gl\u00fcck hat meine gute Frau,", "tokens": ["Zum", "Gl\u00fcck", "hat", "mei\u00b7ne", "gu\u00b7te", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die liebevoll an alles denkt,", "tokens": ["Die", "lie\u00b7be\u00b7voll", "an", "al\u00b7les", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mir einen kleinen Fritz geschenkt,", "tokens": ["Mir", "ei\u00b7nen", "klei\u00b7nen", "Fritz", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NE", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn oft erfreut mich dieser Knabe", "tokens": ["Denn", "oft", "er\u00b7freut", "mich", "die\u00b7ser", "Kna\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch seinen kindlichen Radau,", "tokens": ["Durch", "sei\u00b7nen", "kind\u00b7li\u00b7chen", "Ra\u00b7dau", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ich so meine Schrullen habe.", "tokens": ["Wenn", "ich", "so", "mei\u00b7ne", "Schrul\u00b7len", "ha\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Heut mittag gab es wieder mal", "tokens": ["Heut", "mit\u00b7tag", "gab", "es", "wie\u00b7der", "mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leibgericht, gespickten Aal,", "tokens": ["Mein", "Leib\u00b7ge\u00b7richt", ",", "ge\u00b7spick\u00b7ten", "Aal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie ich dann zur K\u00f6rperpflege,", "tokens": ["Und", "wie", "ich", "dann", "zur", "K\u00f6r\u00b7per\u00b7pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Weste auf, die Augen zu,", "tokens": ["Die", "Wes\u00b7te", "auf", ",", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die H\u00e4nde friedlich auf dem Magen", "tokens": ["Die", "H\u00e4n\u00b7de", "fried\u00b7lich", "auf", "dem", "Ma\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Im Polsterstuhl mich niederlege,", "tokens": ["Im", "Pols\u00b7ter\u00b7stuhl", "mich", "nie\u00b7der\u00b7le\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Oh weh, ein Schwarm von dummen Fragen", "tokens": ["Oh", "weh", ",", "ein", "Schwarm", "von", "dum\u00b7men", "Fra\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "$,", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Verscheucht die hei\u00dfersehnte Ruh.", "tokens": ["Ver\u00b7scheucht", "die", "hei\u00b7\u00dfer\u00b7sehn\u00b7te", "Ruh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach, wird es mir denn niemals klar,", "tokens": ["Ach", ",", "wird", "es", "mir", "denn", "nie\u00b7mals", "klar", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VAFIN", "PPER", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo ich gewesen, eh ich war?", "tokens": ["Wo", "ich", "ge\u00b7we\u00b7sen", ",", "eh", "ich", "war", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAPP", "$,", "KOUS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwamm ich, verkr\u00fcmelt in Atome,", "tokens": ["Schwamm", "ich", ",", "ver\u00b7kr\u00fc\u00b7melt", "in", "A\u00b7to\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VVFIN", "APPR", "NE", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gedankenlos im Wirbelstrome,", "tokens": ["Ge\u00b7dan\u00b7ken\u00b7los", "im", "Wir\u00b7bel\u00b7stro\u00b7me", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bis ich am Ende mich verdichtet", "tokens": ["Bis", "ich", "am", "En\u00b7de", "mich", "ver\u00b7dich\u00b7tet"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zu einer denkenden Person?", "tokens": ["Zu", "ei\u00b7ner", "den\u00b7ken\u00b7den", "Per\u00b7son", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--++", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und jetzt, was hab ich ausgerichtet?", "tokens": ["Und", "jetzt", ",", "was", "hab", "ich", "aus\u00b7ge\u00b7rich\u00b7tet", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was war der M\u00fche karger Lohn?", "tokens": ["Was", "war", "der", "M\u00fc\u00b7he", "kar\u00b7ger", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das Geld ist rar, die Kurse sinken,", "tokens": ["Das", "Geld", "ist", "rar", ",", "die", "Kur\u00b7se", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dagegen steigt der Preis der Schinken.", "tokens": ["Da\u00b7ge\u00b7gen", "steigt", "der", "Preis", "der", "Schin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Fast jeden Morgen klagt die Mutter:", "tokens": ["Fast", "je\u00b7den", "Mor\u00b7gen", "klagt", "die", "Mut\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Ach Herr, wie teuer ist die Butter!", "tokens": ["Ach", "Herr", ",", "wie", "teu\u00b7er", "ist", "die", "But\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ja, selbst der Vater wird ger\u00fchrt,", "tokens": ["Ja", ",", "selbst", "der", "Va\u00b7ter", "wird", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Wenn er sein kleines Br\u00f6tchen schmiert.", "tokens": ["Wenn", "er", "sein", "klei\u00b7nes", "Br\u00f6t\u00b7chen", "schmiert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und doch, trotz dieser Seelenleiden,", "tokens": ["Und", "doch", ",", "trotz", "die\u00b7ser", "See\u00b7len\u00b7lei\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Will keiner gern von hinnen scheiden.", "tokens": ["Will", "kei\u00b7ner", "gern", "von", "hin\u00b7nen", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Wer wei\u00df?", "tokens": ["Wer", "wei\u00df", "?"], "token_info": ["word", "word", "punct"], "pos": ["PWS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Ei sieh, wer kommt denn da?", "tokens": ["Ei", "sieh", ",", "wer", "kommt", "denn", "da", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PWS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hallo, der Fritz! Nun wird es heiter,", "tokens": ["Hal\u00b7lo", ",", "der", "Fritz", "!", "Nun", "wird", "es", "hei\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NE", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nun machen wir den Eselreiter.", "tokens": ["Nun", "ma\u00b7chen", "wir", "den", "E\u00b7sel\u00b7rei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flugs stell ich mich auf alle Viere,", "tokens": ["Flugs", "stell", "ich", "mich", "auf", "al\u00b7le", "Vie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Indem ich auf und ab marschiere,", "tokens": ["In\u00b7dem", "ich", "auf", "und", "ab", "mar\u00b7schie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und rufe kr\u00e4ftig mein Ih \u2013 ah!", "tokens": ["Und", "ru\u00b7fe", "kr\u00e4f\u00b7tig", "mein", "Ih", "\u2013", "ah", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "NN", "$(", "ITJ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vor W\u00e4hligkeit und \u00dcbermut.", "tokens": ["Vor", "W\u00e4h\u00b7lig\u00b7keit", "und", "\u00dc\u00b7ber\u00b7mut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ih \u2013 ah! Die Welt ist nicht so \u00fcbel.", "tokens": ["Ih", "\u2013", "ah", "!", "Die", "Welt", "ist", "nicht", "so", "\u00fc\u00b7bel", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "ITJ", "$.", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wozu das n\u00e4rrische Gegr\u00fcbel?", "tokens": ["Wo\u00b7zu", "das", "n\u00e4r\u00b7ri\u00b7sche", "Ge\u00b7gr\u00fc\u00b7bel", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin Papa, und damit gut.", "tokens": ["Ich", "bin", "Pa\u00b7pa", ",", "und", "da\u00b7mit", "gut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$,", "KON", "PAV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mitunter schwitzen mu\u00df der Schreiner,", "tokens": ["Mi\u00b7tun\u00b7ter", "schwit\u00b7zen", "mu\u00df", "der", "Schrei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er st\u00f6\u00dft auf manchen harten Ast.", "tokens": ["Er", "st\u00f6\u00dft", "auf", "man\u00b7chen", "har\u00b7ten", "Ast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So geht es auch, wenn unsereiner", "tokens": ["So", "geht", "es", "auch", ",", "wenn", "un\u00b7ser\u00b7ei\u00b7ner"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich mit der Gr\u00fcbelei befa\