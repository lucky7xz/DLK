{"dta.poem.2925": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "124.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wei\u00dft du, was Liebe sei? Da\u00df eine dir gefallen,", "tokens": ["Wei\u00dft", "du", ",", "was", "Lie\u00b7be", "sei", "?", "Da\u00df", "ei\u00b7ne", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWS", "NN", "VAFIN", "$.", "KOUS", "ART", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ists nicht, auch das nicht, da\u00df sie dir gefiel vor allen.", "tokens": ["Ists", "nicht", ",", "auch", "das", "nicht", ",", "da\u00df", "sie", "dir", "ge\u00b7fiel", "vor", "al\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "$,", "ADV", "PDS", "PTKNEG", "$,", "KOUS", "PPER", "PPER", "VVFIN", "APPR", "PIAT", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch andere zu sehn, und sch\u00f6ner sie zu finden,", "tokens": ["Doch", "an\u00b7de\u00b7re", "zu", "sehn", ",", "und", "sch\u00f6\u00b7ner", "sie", "zu", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PTKZU", "VVINF", "$,", "KON", "ADJA", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geistreicher auch, und doch nicht Lust noch Neid empfinden,", "tokens": ["Geist\u00b7rei\u00b7cher", "auch", ",", "und", "doch", "nicht", "Lust", "noch", "Neid", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KON", "ADV", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und f\u00fchlen, da\u00df es nur zur Einen hin dich zieht;", "tokens": ["Und", "f\u00fch\u00b7len", ",", "da\u00df", "es", "nur", "zur", "Ei\u00b7nen", "hin", "dich", "zieht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPRART", "ART", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Lieb' ist das, die f\u00fchlt, nicht denket oder sieht.", "tokens": ["Die", "Lieb'", "ist", "das", ",", "die", "f\u00fchlt", ",", "nicht", "den\u00b7ket", "o\u00b7der", "sieht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PDS", "$,", "PRELS", "VVFIN", "$,", "PTKNEG", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}