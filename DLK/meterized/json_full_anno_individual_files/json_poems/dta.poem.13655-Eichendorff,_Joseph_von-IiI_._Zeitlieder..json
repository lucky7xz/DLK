{"dta.poem.13655": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "IiI .   Zeitlieder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wo ruhig sich und wilder", "tokens": ["Wo", "ru\u00b7hig", "sich", "und", "wil\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PRF", "KON", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Unst\u00e4te Wellen theilen,", "tokens": ["Un\u00b7st\u00e4\u00b7te", "Wel\u00b7len", "thei\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Des Lebens sch\u00f6ne Bilder", "tokens": ["Des", "Le\u00b7bens", "sch\u00f6\u00b7ne", "Bil\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Kl\u00e4ng' verworren eilen,", "tokens": ["Und", "Kl\u00e4ng'", "ver\u00b7wor\u00b7ren", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo ist der sichre Halt? \u2014", "tokens": ["Wo", "ist", "der", "sich\u00b7re", "Halt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "So ferne, was wir sollen,", "tokens": ["So", "fer\u00b7ne", ",", "was", "wir", "sol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "So dunkel, was wir wollen,", "tokens": ["So", "dun\u00b7kel", ",", "was", "wir", "wol\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PRELS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Fa\u00dft alle die Gewalt.", "tokens": ["Fa\u00dft", "al\u00b7le", "die", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}