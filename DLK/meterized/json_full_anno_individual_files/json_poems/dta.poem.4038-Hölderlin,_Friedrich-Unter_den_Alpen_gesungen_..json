{"dta.poem.4038": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Unter den Alpen gesungen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Heilige Unschuld, Du der Menschen und der", "tokens": ["Hei\u00b7li\u00b7ge", "Un\u00b7schuld", ",", "Du", "der", "Men\u00b7schen", "und", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "ART", "NN", "KON", "ART"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "G\u00f6tter liebste Vertrauteste! Du magst im", "tokens": ["G\u00f6t\u00b7ter", "liebs\u00b7te", "Ver\u00b7trau\u00b7tes\u00b7te", "!", "Du", "magst", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$.", "PPER", "VMFIN", "APPRART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Hause oder drau\u00dfen ihnen zu F\u00fc\u00dfen", "tokens": ["Hau\u00b7se", "o\u00b7der", "drau\u00b7\u00dfen", "ih\u00b7nen", "zu", "F\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "PPER", "APPR", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Sitzen, den Alten,", "tokens": ["Sit\u00b7zen", ",", "den", "Al\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Immerzufriedener Weisheit voll; denn manches", "tokens": ["Im\u00b7mer\u00b7zu\u00b7frie\u00b7de\u00b7ner", "Weis\u00b7heit", "voll", ";", "denn", "man\u00b7ches"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "$.", "KON", "PIS"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Gute kennet der Mann, doch staunet er dem", "tokens": ["Gu\u00b7te", "ken\u00b7net", "der", "Mann", ",", "doch", "stau\u00b7net", "er", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ART"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wild gleich, oft zum Himmel, aber wie rein ist,", "tokens": ["Wild", "gleich", ",", "oft", "zum", "Him\u00b7mel", ",", "a\u00b7ber", "wie", "rein", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "ADV", "APPRART", "NN", "$,", "KON", "PWAV", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Reine, Dir alles!", "tokens": ["Rei\u00b7ne", ",", "Dir", "al\u00b7les", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "PIS", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Siehe! das rauhe Thier des Feldes, gerne", "tokens": ["Sie\u00b7he", "!", "das", "rau\u00b7he", "Thier", "des", "Fel\u00b7des", ",", "ger\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "$.", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dient und trauet es Dir, der stumme Wald spricht", "tokens": ["Dient", "und", "trau\u00b7et", "es", "Dir", ",", "der", "stum\u00b7me", "Wald", "spricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "PPER", "PPER", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wie vor Alters, seine Spr\u00fcche zu Dir, es", "tokens": ["Wie", "vor", "Al\u00b7ters", ",", "sei\u00b7ne", "Spr\u00fc\u00b7che", "zu", "Dir", ",", "es"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "PPOSAT", "NN", "APPR", "PPER", "$,", "PPER"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Lehren die Berge", "tokens": ["Leh\u00b7ren", "die", "Ber\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.4": {"line.1": {"text": "Heil'ge Gesetze Dich, und was noch jetzt uns", "tokens": ["Heil'\u00b7ge", "Ge\u00b7set\u00b7ze", "Dich", ",", "und", "was", "noch", "jetzt", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "$,", "KON", "PWS", "ADV", "ADV", "PPER"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Vielerfahrenen, offenbar der gro\u00dfe", "tokens": ["Vie\u00b7ler\u00b7fah\u00b7re\u00b7nen", ",", "of\u00b7fen\u00b7bar", "der", "gro\u00b7\u00dfe"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ADJD", "ART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Vater werden hei\u00dft, Du darfst es allein uns", "tokens": ["Va\u00b7ter", "wer\u00b7den", "hei\u00dft", ",", "Du", "darfst", "es", "al\u00b7lein", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAINF", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Helle verk\u00fcnden.", "tokens": ["Hel\u00b7le", "ver\u00b7k\u00fcn\u00b7den", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "So mit den Himmlischen allein zu seyn, und", "tokens": ["So", "mit", "den", "Himm\u00b7li\u00b7schen", "al\u00b7lein", "zu", "seyn", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "PTKZU", "VAINF", "$,", "KON"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Geht vor\u00fcber das Licht, und Strom und Wind, und", "tokens": ["Geht", "vor\u00b7\u00fc\u00b7ber", "das", "Licht", ",", "und", "Strom", "und", "Wind", ",", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "$,", "KON"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Zeit eilt sie zum Ort, vor ihnen ein stetes", "tokens": ["Zeit", "eilt", "sie", "zum", "Ort", ",", "vor", "ih\u00b7nen", "ein", "ste\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$,", "APPR", "PPER", "ART", "ADJA"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Auge zu haben,", "tokens": ["Au\u00b7ge", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VAINF", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Seliger wei\u00df und w\u00fcnsch' ich nichts, so lange", "tokens": ["Se\u00b7li\u00b7ger", "wei\u00df", "und", "w\u00fcn\u00b7sch'", "ich", "nichts", ",", "so", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "KON", "VVFIN", "PPER", "PIS", "$,", "ADV", "ADV"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Nicht auch mich, wie die Winde, fort die Flut nimmt,", "tokens": ["Nicht", "auch", "mich", ",", "wie", "die", "Win\u00b7de", ",", "fort", "die", "Flut", "nimmt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "$,", "PWAV", "ART", "NN", "$,", "PTKVZ", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df wohl aufgehoben, schlafend dahin ich", "tokens": ["Da\u00df", "wohl", "auf\u00b7ge\u00b7ho\u00b7ben", ",", "schla\u00b7fend", "da\u00b7hin", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "VVPP", "$,", "ADJD", "PAV", "PPER"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Mu\u00df in den Wogen;", "tokens": ["Mu\u00df", "in", "den", "Wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Aber es bleibt daheim gern, wer in treuem", "tokens": ["A\u00b7ber", "es", "bleibt", "da\u00b7heim", "gern", ",", "wer", "in", "treu\u00b7em"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV", "$,", "PWS", "APPR", "ADJA"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Busen G\u00f6ttliches h\u00e4lt, und frei will ich, so", "tokens": ["Bu\u00b7sen", "G\u00f6tt\u00b7li\u00b7ches", "h\u00e4lt", ",", "und", "frei", "will", "ich", ",", "so"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "NE", "VVFIN", "$,", "KON", "ADJD", "VMFIN", "PPER", "$,", "ADV"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Lang ich darf, euch all' ihr Sprachen des Himmels!", "tokens": ["Lang", "ich", "darf", ",", "euch", "all'", "ihr", "Spra\u00b7chen", "des", "Him\u00b7mels", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VMFIN", "$,", "PPER", "PIS", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Deuten und singen.", "tokens": ["Deu\u00b7ten", "und", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}}}}