{"textgrid.poem.32541": {"metadata": {"author": {"name": "Scheffner, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Denkzettelchen in Phyllis Schatzk\u00e4stlein.", "genre": "verse", "period": "N.A.", "pub_year": 1778, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ohn die die weiche Hand, die wei\u00dfe Brust zu k\u00fc\u00dfen,", "tokens": ["Ohn", "die", "die", "wei\u00b7che", "Hand", ",", "die", "wei\u00b7\u00dfe", "Brust", "zu", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab ich dich, ", "tokens": ["Hab", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "F\u00fcr mich, o M\u00e4dchen, welch ein tiefer Schmerz!", "tokens": ["F\u00fcr", "mich", ",", "o", "M\u00e4d\u00b7chen", ",", "welch", "ein", "tie\u00b7fer", "Schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "PWAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auf deinen Lippen wohnt allein mein Leben,", "tokens": ["Auf", "dei\u00b7nen", "Lip\u00b7pen", "wohnt", "al\u00b7lein", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn unter K\u00fc\u00dfen sich die Marmorh\u00fcgel heben,", "tokens": ["Wenn", "un\u00b7ter", "K\u00fc\u00b7\u00dfen", "sich", "die", "Mar\u00b7mor\u00b7h\u00fc\u00b7gel", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dann wall't auch Freude durch mein Herz \u2013", "tokens": ["Dann", "wall't", "auch", "Freu\u00b7de", "durch", "mein", "Herz", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und bald \u2013 bald werd ich Dich lang gar nicht sehen,", "tokens": ["Und", "bald", "\u2013", "bald", "werd", "ich", "Dich", "lang", "gar", "nicht", "se\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VAFIN", "PPER", "PRF", "ADJD", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ach dann wird wohl die Winterlust", "tokens": ["Ach", "dann", "wird", "wohl", "die", "Win\u00b7ter\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Dich zum Contretantz und Schlittenfahrten ruft", "tokens": ["Die", "Dich", "zum", "Con\u00b7tre\u00b7tantz", "und", "Schlit\u00b7ten\u00b7fahr\u00b7ten", "ruft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die kleine Flamme ganz verwehen,", "tokens": ["Die", "klei\u00b7ne", "Flam\u00b7me", "ganz", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Die Flamme die vielleicht zu meinem Gl\u00fcck", "tokens": ["Die", "Flam\u00b7me", "die", "viel\u00b7leicht", "zu", "mei\u00b7nem", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "in manchem sch\u00f6nen Augenblick", "tokens": ["in", "man\u00b7chem", "sch\u00f6\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Dein Herz noch w\u00e4rmt \u2013 dann wird der Sommer meines Lebens", "tokens": ["Dein", "Herz", "noch", "w\u00e4rmt", "\u2013", "dann", "wird", "der", "Som\u00b7mer", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "$(", "ADV", "VAFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nur Eine lange Klage seyn;", "tokens": ["Nur", "Ei\u00b7ne", "lan\u00b7ge", "Kla\u00b7ge", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Dann bl\u00fcht f\u00fcr mich die Welt vergebens,", "tokens": ["Dann", "bl\u00fcht", "f\u00fcr", "mich", "die", "Welt", "ver\u00b7ge\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Dann wird um mich ein ew'ger Winter seyn! \u2013", "tokens": ["Dann", "wird", "um", "mich", "ein", "ew'\u00b7ger", "Win\u00b7ter", "seyn", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PRF", "ART", "ADJA", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Sieh, ", "tokens": ["Sieh", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+", "measure": "single.up"}, "line.18": {"text": "Sie luden uns, so lang als Zephyrs Fl\u00fcgel", "tokens": ["Sie", "lu\u00b7den", "uns", ",", "so", "lang", "als", "Ze\u00b7phyrs", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ADV", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Ihr gr\u00fcn Geb\u00fcsch durchwehte zum Spaziergang ein,", "tokens": ["Ihr", "gr\u00fcn", "Ge\u00b7b\u00fcsch", "durch\u00b7weh\u00b7te", "zum", "Spa\u00b7zier\u00b7gang", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Doch jetzt umbrausen sie des Nordwinds Fl\u00fcgel,", "tokens": ["Doch", "jetzt", "um\u00b7brau\u00b7sen", "sie", "des", "Nord\u00b7winds", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Die B\u00fcsche trauren bl\u00e4tterleer,", "tokens": ["Die", "B\u00fc\u00b7sche", "trau\u00b7ren", "bl\u00e4t\u00b7ter\u00b7leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Da stehn sie jetzt verway\u00dft die majest\u00e4tschen H\u00fcgel \u2013", "tokens": ["Da", "stehn", "sie", "jetzt", "ver\u00b7way\u00dft", "die", "ma\u00b7jes\u00b7t\u00e4t\u00b7schen", "H\u00fc\u00b7gel", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "So werd ich auch die Marmorh\u00fcgel,", "tokens": ["So", "werd", "ich", "auch", "die", "Mar\u00b7mor\u00b7h\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Wo jetzt Empfindung wohnt und Rosenknospen bl\u00fchn", "tokens": ["Wo", "jetzt", "Emp\u00b7fin\u00b7dung", "wohnt", "und", "Ro\u00b7sen\u00b7knos\u00b7pen", "bl\u00fchn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Von weitem sehn, vor ihrer K\u00e4lte fliehn. \u2013", "tokens": ["Von", "wei\u00b7tem", "sehn", ",", "vor", "ih\u00b7rer", "K\u00e4l\u00b7te", "fliehn", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "VVINF", "$,", "APPR", "PPOSAT", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "O welch ein Gram f\u00fcr mich wenn diese Busenh\u00f6hen", "tokens": ["O", "welch", "ein", "Gram", "f\u00fcr", "mich", "wenn", "die\u00b7se", "Bu\u00b7sen\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PIAT", "ART", "NN", "APPR", "PPER", "KOUS", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Kein Lenz der Liebe mehr f\u00fcr mich umbl\u00fch't,", "tokens": ["Kein", "Lenz", "der", "Lie\u00b7be", "mehr", "f\u00fcr", "mich", "um\u00b7bl\u00fch't", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "ADV", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Wenn sie ein andrer k\u00fc\u00dft, und ihren Reitz zu sehen", "tokens": ["Wenn", "sie", "ein", "an\u00b7drer", "k\u00fc\u00dft", ",", "und", "ih\u00b7ren", "Reitz", "zu", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,", "KON", "PPOSAT", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Den seidnen Flor von wei\u00dfen Schultern zieht!", "tokens": ["Den", "seid\u00b7nen", "Flor", "von", "wei\u00b7\u00dfen", "Schul\u00b7tern", "zieht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "O M\u00e4dchen la\u00df doch nie entfernt von mir den Winter", "tokens": ["O", "M\u00e4d\u00b7chen", "la\u00df", "doch", "nie", "ent\u00b7fernt", "von", "mir", "den", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "VVIMP", "ADV", "ADV", "ADJD", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Dein Herz mit Ei\u00df f\u00fcr mich umziehn,", "tokens": ["Dein", "Herz", "mit", "Ei\u00df", "f\u00fcr", "mich", "um\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "APPR", "PPER", "VVIZU", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Wenn du mich wiedersiehst, dann wall' dein Blut geschwinder,", "tokens": ["Wenn", "du", "mich", "wie\u00b7der\u00b7siehst", ",", "dann", "wall'", "dein", "Blut", "ge\u00b7schwin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Und la\u00df auf deinen Wangen Liebe gl\u00fchn.", "tokens": ["Und", "la\u00df", "auf", "dei\u00b7nen", "Wan\u00b7gen", "Lie\u00b7be", "gl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ohn die die weiche Hand, die wei\u00dfe Brust zu k\u00fc\u00dfen,", "tokens": ["Ohn", "die", "die", "wei\u00b7che", "Hand", ",", "die", "wei\u00b7\u00dfe", "Brust", "zu", "k\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hab ich dich, ", "tokens": ["Hab", "ich", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "F\u00fcr mich, o M\u00e4dchen, welch ein tiefer Schmerz!", "tokens": ["F\u00fcr", "mich", ",", "o", "M\u00e4d\u00b7chen", ",", "welch", "ein", "tie\u00b7fer", "Schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "$,", "PWAT", "ART", "ADJA", "NN", "$."], "meter"