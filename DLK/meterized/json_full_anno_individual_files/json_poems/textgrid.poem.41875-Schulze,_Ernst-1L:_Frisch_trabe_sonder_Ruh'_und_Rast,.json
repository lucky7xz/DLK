"Bey", "ihr", "bald", "heil\u00b7ten", ",", "bald", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVINF", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fchlt' ich drey Tage nur den Schmerz,", "tokens": ["F\u00fchlt'", "ich", "drey", "Ta\u00b7ge", "nur", "den", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ach, die Freude mu\u00dft' ich missen!", "tokens": ["Und", "ach", ",", "die", "Freu\u00b7de", "mu\u00dft'", "ich", "mis\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Drum trabe muthig durch die Nacht!", "tokens": ["Drum", "tra\u00b7be", "mut\u00b7hig", "durch", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schwinden auch die dunkeln Bahnen,", "tokens": ["Und", "schwin\u00b7den", "auch", "die", "dun\u00b7keln", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Sehnsucht helles Auge wacht,", "tokens": ["Der", "Sehn\u00b7sucht", "hel\u00b7les", "Au\u00b7ge", "wacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sicher f\u00fchrt mich s\u00fc\u00dfes Ahnen.", "tokens": ["Und", "si\u00b7cher", "f\u00fchrt", "mich", "s\u00fc\u00b7\u00dfes", "Ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Weit sehn wir \u00fcber Land und See", "tokens": ["Weit", "sehn", "wir", "\u00fc\u00b7ber", "Land", "und", "See"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zur w\u00e4rmern Flur den Vogel fliegen;", "tokens": ["Zur", "w\u00e4r\u00b7mern", "Flur", "den", "Vo\u00b7gel", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wie sollte denn die Liebe je", "tokens": ["Wie", "soll\u00b7te", "denn", "die", "Lie\u00b7be", "je"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In ihrem Pfade sich betr\u00fcgen?", "tokens": ["In", "ih\u00b7rem", "Pfa\u00b7de", "sich", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}