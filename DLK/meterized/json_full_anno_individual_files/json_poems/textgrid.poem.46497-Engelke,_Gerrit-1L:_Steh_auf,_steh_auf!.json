{"textgrid.poem.46497": {"metadata": {"author": {"name": "Engelke, Gerrit", "birth": "N.A.", "death": "N.A."}, "title": "1L: Steh auf, steh auf!", "genre": "verse", "period": "N.A.", "pub_year": 1915, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steh auf, steh auf!", "tokens": ["Steh", "auf", ",", "steh", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Ich bin die ganze Nacht im Schnee gegangen \u2013", "tokens": ["Ich", "bin", "die", "gan\u00b7ze", "Nacht", "im", "Schnee", "ge\u00b7gan\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die m\u00fcden H\u00e4user lauschen mit im Bangen,", "tokens": ["Die", "m\u00fc\u00b7den", "H\u00e4u\u00b7ser", "lau\u00b7schen", "mit", "im", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur die blinden Stra\u00dfenlichter wachen \u2013", "tokens": ["Nur", "die", "blin\u00b7den", "Stra\u00b7\u00dfen\u00b7lich\u00b7ter", "wa\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Steh auf, und la\u00df uns Hochzeit machen!", "tokens": ["Steh", "auf", ",", "und", "la\u00df", "uns", "Hoch\u00b7zeit", "ma\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "KON", "VVIMP", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie lange hab ich meinen Durst ertragen,", "tokens": ["Wie", "lan\u00b7ge", "hab", "ich", "mei\u00b7nen", "Durst", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df Tage, St\u00e4dte weiterjagen.", "tokens": ["Lie\u00df", "Ta\u00b7ge", ",", "St\u00e4d\u00b7te", "wei\u00b7ter\u00b7ja\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch Sehnsucht hat mich hergetrieben \u2013", "tokens": ["Doch", "Sehn\u00b7sucht", "hat", "mich", "her\u00b7ge\u00b7trie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kann doch dich nur lieben.", "tokens": ["Ich", "kann", "doch", "dich", "nur", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie kann mich Schlaf zufrieden laben \u2013", "tokens": ["Wie", "kann", "mich", "Schlaf", "zu\u00b7frie\u00b7den", "la\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin von weit durch Nacht und Schnee gekommen,", "tokens": ["Ich", "bin", "von", "weit", "durch", "Nacht", "und", "Schnee", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Durch dieselben Gassen hergeklommen,", "tokens": ["Durch", "die\u00b7sel\u00b7ben", "Gas\u00b7sen", "her\u00b7ge\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Und will dich wieder, wieder haben!", "tokens": ["Und", "will", "dich", "wie\u00b7der", ",", "wie\u00b7der", "ha\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADV", "$,", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Steh auf, steh auf, die Stunden dringen!", "tokens": ["Steh", "auf", ",", "steh", "auf", ",", "die", "Stun\u00b7den", "drin\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rei\u00df die Gardinen auf und z\u00fcnde Licht,", "tokens": ["Rei\u00df", "die", "Gar\u00b7di\u00b7nen", "auf", "und", "z\u00fcn\u00b7de", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "KON", "ADJA", "NN", "$,"], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Steh auf, ich will die Hochzeit bringen! \u2013", "tokens": ["Steh", "auf", ",", "ich", "will", "die", "Hoch\u00b7zeit", "brin\u00b7gen", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "VMFIN", "ART", "NN", "VVINF", "$.", "$("],