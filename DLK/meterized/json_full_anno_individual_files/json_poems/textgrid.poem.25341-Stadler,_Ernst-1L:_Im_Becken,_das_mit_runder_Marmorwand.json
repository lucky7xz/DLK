{"textgrid.poem.25341": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Becken, das mit runder Marmorwand", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Becken, das mit runder Marmorwand", "tokens": ["Im", "Be\u00b7cken", ",", "das", "mit", "run\u00b7der", "Mar\u00b7mor\u00b7wand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "schl\u00e4frige Flut fa\u00dft, wellen\u00fcberfaucht", "tokens": ["schl\u00e4f\u00b7ri\u00b7ge", "Flut", "fa\u00dft", ",", "wel\u00b7le\u00b7n\u00fc\u00b7berf\u00b7aucht"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVFIN", "$,", "PWAT"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "vom Schwan, der tief den Kopf zum Spiegel taucht,", "tokens": ["vom", "Schwan", ",", "der", "tief", "den", "Kopf", "zum", "Spie\u00b7gel", "taucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "in dessen gr\u00fcnem Gl\u00e4nzen er sein Auge fand,", "tokens": ["in", "des\u00b7sen", "gr\u00fc\u00b7nem", "Gl\u00e4n\u00b7zen", "er", "sein", "Au\u00b7ge", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "w\u00f6lbt sich ihr Leib, erwartungsvoll gespannt.", "tokens": ["w\u00f6lbt", "sich", "ihr", "Leib", ",", "er\u00b7war\u00b7tungs\u00b7voll", "ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "$,", "ADJD", "VVPP", "$."], "meter": "++-+-+-+-+", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Den nackten Fu\u00df umsp\u00fclt des Wassers Blitzen,", "tokens": ["Den", "nack\u00b7ten", "Fu\u00df", "um\u00b7sp\u00fclt", "des", "Was\u00b7sers", "Blit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und schmachtend lehnt sie an den Muschelspitzen,", "tokens": ["und", "schmach\u00b7tend", "lehnt", "sie", "an", "den", "Mu\u00b7schel\u00b7spit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sehns\u00fcchtig suchend langt die starre Hand.", "tokens": ["sehn\u00b7s\u00fcch\u00b7tig", "su\u00b7chend", "langt", "die", "star\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und Schw\u00e4ne, die die Nymphe m\u00fcd umschweben ...", "tokens": ["Und", "Schw\u00e4\u00b7ne", ",", "die", "die", "Nym\u00b7phe", "m\u00fcd", "um\u00b7schwe\u00b7ben", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "PRELS", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es streift den Leib im Gleiten ihr Gefieder,", "tokens": ["Es", "streift", "den", "Leib", "im", "Glei\u00b7ten", "ihr", "Ge\u00b7fie\u00b7der",