{"textgrid.poem.41427": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Zeisig", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zeisig, der sein Nest nur eben angelegt,", "tokens": ["Ein", "Zei\u00b7sig", ",", "der", "sein", "Nest", "nur", "e\u00b7ben", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versang an einem heitern Morgen", "tokens": ["Ver\u00b7sang", "an", "ei\u00b7nem", "hei\u00b7tern", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Schlaf, die Bau- und Nahrungssorgen.", "tokens": ["Den", "Schlaf", ",", "die", "Bau", "und", "Nah\u00b7rungs\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm wuchs sein kleines Herz, durch West und Lust erregt.", "tokens": ["Ihm", "wuchs", "sein", "klei\u00b7nes", "Herz", ",", "durch", "West", "und", "Lust", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Waldgesang verehrte Licht und Sonne,", "tokens": ["Sein", "Wald\u00b7ge\u00b7sang", "ver\u00b7ehr\u00b7te", "Licht", "und", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Denn ihn begeisterte des sch\u00f6nen Himmels Wonne;", "tokens": ["Denn", "ihn", "be\u00b7geis\u00b7ter\u00b7te", "des", "sch\u00f6\u00b7nen", "Him\u00b7mels", "Won\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und, wie ein Fr\u00f6hlicher oft gern zu schwatzen pflegt,", "tokens": ["Und", ",", "wie", "ein", "Fr\u00f6h\u00b7li\u00b7cher", "oft", "gern", "zu", "schwat\u00b7zen", "pflegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wollt' auch er sich recht beredt erweisen,", "tokens": ["So", "wollt'", "auch", "er", "sich", "recht", "be\u00b7redt", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "PPER", "PRF", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Der Lerche diesen Tag vor allen anzupreisen.", "tokens": ["Der", "Ler\u00b7che", "die\u00b7sen", "Tag", "vor", "al\u00b7len", "an\u00b7zu\u00b7prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "APPR", "PIS", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Der Mittag k\u00f6mmt umw\u00f6lkt. Die grauen M\u00f6wen fliehn", "tokens": ["Der", "Mit\u00b7tag", "k\u00f6mmt", "um\u00b7w\u00f6lkt", ".", "Die", "grau\u00b7en", "M\u00f6\u00b7wen", "fliehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "VVPP", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit bangem Flug, und schrein, und n\u00e4hern sich dem Lande:", "tokens": ["Mit", "ban\u00b7gem", "Flug", ",", "und", "schrein", ",", "und", "n\u00e4\u00b7hern", "sich", "dem", "Lan\u00b7de", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "ADJD", "$,", "KON", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein und ungl\u00fccksvoll spaziert im trocknen Sande", "tokens": ["Al\u00b7lein", "und", "un\u00b7gl\u00fccks\u00b7voll", "spa\u00b7ziert", "im", "trock\u00b7nen", "San\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KON", "ADJD", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Die dunkle Kr\u00e4h', und scharrt; Gewitter, die verziehn,", "tokens": ["Die", "dunk\u00b7le", "Kr\u00e4h'", ",", "und", "scharrt", ";", "Ge\u00b7wit\u00b7ter", ",", "die", "ver\u00b7ziehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "VVFIN", "$.", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ruft sie mit Kr\u00e4chzen her. Tief um das Schilfgras streichen", "tokens": ["Ruft", "sie", "mit", "Kr\u00e4ch\u00b7zen", "her", ".", "Tief", "um", "das", "Schilf\u00b7gras", "strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$.", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.15": {"text": "Die Erdschwalb' und der Spatz: der H\u00e4her sucht die Eichen,", "tokens": ["Die", "Erd\u00b7schwalb'", "und", "der", "Spatz", ":", "der", "H\u00e4\u00b7her", "sucht", "die", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der Reiher hohe Luft, sein Bette Hirsch und Thier:", "tokens": ["Der", "Rei\u00b7her", "ho\u00b7he", "Luft", ",", "sein", "Bet\u00b7te", "Hirsch", "und", "Thier", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Mit aufgewecktem Hals schnauft der beklommne Stier:", "tokens": ["Mit", "auf\u00b7ge\u00b7weck\u00b7tem", "Hals", "schnauft", "der", "be\u00b7klomm\u00b7ne", "Stier", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die Pferde treiben sich, die St\u00e4lle zu erreichen.", "tokens": ["Die", "Pfer\u00b7de", "trei\u00b7ben", "sich", ",", "die", "St\u00e4l\u00b7le", "zu", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Schnell \u00fcberw\u00e4ltiget ein Wirbelwind den West,", "tokens": ["Schnell", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7ti\u00b7get", "ein", "Wir\u00b7bel\u00b7wind", "den", "West", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Der Hain erbebt, und heult: auf Ficht' und Tanne schossen", "tokens": ["Der", "Hain", "er\u00b7bebt", ",", "und", "heult", ":", "auf", "Ficht'", "und", "Tan\u00b7ne", "schos\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "$.", "APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Verw\u00fcstend der Orcan, der Regen und die Schlossen;", "tokens": ["Ver\u00b7w\u00fcs\u00b7tend", "der", "Or\u00b7can", ",", "der", "Re\u00b7gen", "und", "die", "Schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "ART", "NE", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.22": {"text": "Und so verlor der Zeisig auch sein Nest.", "tokens": ["Und", "so", "ver\u00b7lor", "der", "Zei\u00b7sig", "auch", "sein", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Der m\u00fcde Sturm h\u00f6rt auf zu toben.", "tokens": ["Der", "m\u00fc\u00b7de", "Sturm", "h\u00f6rt", "auf", "zu", "to\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Der nasse S\u00e4nger h\u00fcpft zu seiner Lerche hin,", "tokens": ["Der", "nas\u00b7se", "S\u00e4n\u00b7ger", "h\u00fcpft", "zu", "sei\u00b7ner", "Ler\u00b7che", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die ihm recht zugeh\u00f6rt, der guten Nachbarin.", "tokens": ["Die", "ihm", "recht", "zu\u00b7ge\u00b7h\u00f6rt", ",", "der", "gu\u00b7ten", "Nach\u00b7ba\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVPP", "$,", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Zum Gl\u00fcck war er bei ihr ganz sicher aufgehoben.", "tokens": ["Zum", "Gl\u00fcck", "war", "er", "bei", "ihr", "ganz", "si\u00b7cher", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPER", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.27": {"text": "Wi\u00dft, sprach er, da\u00df ich schon durch Schaden kl\u00fcger bin:", "tokens": ["Wi\u00dft", ",", "sprach", "er", ",", "da\u00df", "ich", "schon", "durch", "Scha\u00b7den", "kl\u00fc\u00b7ger", "bin", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Man mu\u00df den sch\u00f6nsten Tag nicht vor dem Abend loben.", "tokens": ["Man", "mu\u00df", "den", "sch\u00f6ns\u00b7ten", "Tag", "nicht", "vor", "dem", "A\u00b7bend", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Zeisig, der sein Nest nur eben angelegt,", "tokens": ["Ein", "Zei\u00b7sig", ",", "der", "sein", "Nest", "nur", "e\u00b7ben", "an\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Versang an einem heitern Morgen", "tokens": ["Ver\u00b7sang", "an", "ei\u00b7nem", "hei\u00b7tern", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Schlaf, die Bau- und Nahrungssorgen.", "tokens": ["Den", "Schlaf", ",", "die", "Bau", "und", "Nah\u00b7rungs\u00b7sor\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm wuchs sein kleines Herz, durch West und Lust erregt.", "tokens": ["Ihm", "wuchs", "sein", "klei\u00b7nes", "Herz", ",", "durch", "West", "und", "Lust", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sein Waldgesang verehrte Licht und Sonne,", "tokens": ["Sein", "Wald\u00b7ge\u00b7sang", "ver\u00b7ehr\u00b7te", "Licht", 