{"dta.poem.8228": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Duett.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.57", "af:0.42"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Neigt sich die Seele dir;", "tokens": ["Neigt", "sich", "die", "See\u00b7le", "dir", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "H\u00f6her schlagen die Wellen,", "tokens": ["H\u00f6\u00b7her", "schla\u00b7gen", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Fluthen die Pulse mir.", "tokens": ["Flut\u00b7hen", "die", "Pul\u00b7se", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Fliehen und Wiederfinden,", "tokens": ["Flie\u00b7hen", "und", "Wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wechselnde Melodie!", "tokens": ["Wech\u00b7seln\u00b7de", "Me\u00b7lo\u00b7die", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "La\u00df du die Seele schwinden,", "tokens": ["La\u00df", "du", "die", "See\u00b7le", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sterben in Harmonie.", "tokens": ["Ster\u00b7ben", "in", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "H\u00f6rst du den Ruf erklingen,", "tokens": ["H\u00f6rst", "du", "den", "Ruf", "er\u00b7klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "R\u00fchren dein tr\u00e4umend Ohr?", "tokens": ["R\u00fch\u00b7ren", "dein", "tr\u00e4u\u00b7mend", "Ohr", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wei\u00dfe blendende Schwingen", "tokens": ["Wei\u00b7\u00dfe", "blen\u00b7den\u00b7de", "Schwin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Tragen dich wehend empor.", "tokens": ["Tra\u00b7gen", "dich", "we\u00b7hend", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Selig, im Lichte zu schweben", "tokens": ["Se\u00b7lig", ",", "im", "Lich\u00b7te", "zu", "schwe\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "APPRART", "NN", "PTKZU", "VVINF"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ueber den Wolken hoch!", "tokens": ["Ue\u00b7ber", "den", "Wol\u00b7ken", "hoch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Lie\u00dft du das s\u00fc\u00dfe Leben,", "tokens": ["Lie\u00dft", "du", "das", "s\u00fc\u00b7\u00dfe", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kennst du die Erde noch?", "tokens": ["Kennst", "du", "die", "Er\u00b7de", "noch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Aber zum stillen Grunde", "tokens": ["A\u00b7ber", "zum", "stil\u00b7len", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Zieht es hernieder schon;", "tokens": ["Zieht", "es", "her\u00b7nie\u00b7der", "schon", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Heimlich von Mund zu Munde", "tokens": ["Heim\u00b7lich", "von", "Mund", "zu", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "APPR", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wechselt ein leiser Ton.", "tokens": ["Wech\u00b7selt", "ein", "lei\u00b7ser", "Ton", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.6": {"line.1": {"text": "Fernhin rauschen die Wogen,", "tokens": ["Fern\u00b7hin", "rau\u00b7schen", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sch\u00fctze mein pochend Herz!", "tokens": ["Sch\u00fct\u00b7ze", "mein", "po\u00b7chend", "Herz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJD", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schon kommt die Nacht gezogen \u2014", "tokens": ["Schon", "kommt", "die", "Nacht", "ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fchlst du den s\u00fc\u00dfen Schmerz?", "tokens": ["F\u00fchlst", "du", "den", "s\u00fc\u00b7\u00dfen", "Schmerz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}