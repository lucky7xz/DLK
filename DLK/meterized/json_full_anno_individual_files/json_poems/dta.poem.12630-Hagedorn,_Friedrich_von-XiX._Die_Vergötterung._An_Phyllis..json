{"dta.poem.12630": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "XiX.  \n  Die Verg\u00f6tterung.  \n An Phyllis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1744", "urn": "urn:nbn:de:kobv:b4-200905199128", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Holde Phyllis, die G\u00f6ttinnen\n(Traue mir die Wahrheit zu)", "tokens": ["Hol\u00b7de", "Phyl\u00b7lis", ",", "die", "G\u00f6t\u00b7tin\u00b7nen", "(", "Trau\u00b7e", "mir", "die", "Wahr\u00b7heit", "zu", ")"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "$(", "VVFIN", "PPER", "ART", "NN", "PTKZU", "$("], "meter": "+-+--+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Waren anfangs Sch\u00e4ferinnen", "tokens": ["Wa\u00b7ren", "an\u00b7fangs", "Sch\u00e4\u00b7fe\u00b7rin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADV", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oder M\u00e4dchen, so wie du.", "tokens": ["O\u00b7der", "M\u00e4d\u00b7chen", ",", "so", "wie", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine, die mit blauen Augen", "tokens": ["Ei\u00b7ne", ",", "die", "mit", "blau\u00b7en", "Au\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mehr als M\u00e4nner-Witz verband,", "tokens": ["Mehr", "als", "M\u00e4n\u00b7ner\u00b7Witz", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Konnte zur Minerva taugen", "tokens": ["Konn\u00b7te", "zur", "Mi\u00b7ner\u00b7va", "tau\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPRART", "NE", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.7": {"text": "Und erwarb den G\u00f6tter-Stand.", "tokens": ["Und", "er\u00b7warb", "den", "G\u00f6t\u00b7ter\u00b7Stand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Dichterinnen hiessen Musen", "tokens": ["Dich\u00b7te\u00b7rin\u00b7nen", "hies\u00b7sen", "Mu\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und entz\u00fcckten Herz und Ohr.", "tokens": ["Und", "ent\u00b7z\u00fcck\u00b7ten", "Herz", "und", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reiffer Sch\u00f6nen volle Busen", "tokens": ["Reif\u00b7fer", "Sch\u00f6\u00b7nen", "vol\u00b7le", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bildete die Ceres vor.", "tokens": ["Bil\u00b7de\u00b7te", "die", "Ce\u00b7res", "vor", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die durch Jugend uns ergetzte", "tokens": ["Die", "durch", "Ju\u00b7gend", "uns", "er\u00b7getz\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schien, mit Recht, des Tempels wehrt,", "tokens": ["Schien", ",", "mit", "Recht", ",", "des", "Tem\u00b7pels", "wehrt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Den man ihr, als Heben, setzte,", "tokens": ["Den", "man", "ihr", ",", "als", "He\u00b7ben", ",", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "PIS", "PPER", "$,", "KOUS", "NN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die der st\u00e4rkste Held verehrt.", "tokens": ["Die", "der", "st\u00e4rks\u00b7te", "Held", "ver\u00b7ehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Eine ward, in spr\u00f6der Bl\u00e4sse", "tokens": ["Ei\u00b7ne", "ward", ",", "in", "spr\u00f6\u00b7der", "Bl\u00e4s\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VAFIN", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in strenger H\u00e4uslichkeit,", "tokens": ["Und", "in", "stren\u00b7ger", "H\u00e4us\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "H\u00fcterinn der Feuer-Esse", "tokens": ["H\u00fc\u00b7te\u00b7rinn", "der", "Feu\u00b7er\u00b7Es\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die Vesta jener Zeit.", "tokens": ["Und", "die", "Ves\u00b7ta", "je\u00b7ner", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Die durch Reitz und Ungl\u00fccks-F\u00e4lle", "tokens": ["Die", "durch", "Reitz", "und", "Un\u00b7gl\u00fccks\u00b7F\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich den Raub der Grobheit sah", "tokens": ["Sich", "den", "Raub", "der", "Grob\u00b7heit", "sah"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ward in ihres Ehstands H\u00f6lle", "tokens": ["Ward", "in", "ih\u00b7res", "Eh\u00b7stands", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Kl\u00e4glich zur Proserpina.", "tokens": ["Kl\u00e4g\u00b7lich", "zur", "Pro\u00b7ser\u00b7pi\u00b7na", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.4": {"line.1": {"text": "Majest\u00e4tische Geberden,", "tokens": ["Ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7sche", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hoheit, die sich nie verga\u00df,", "tokens": ["Ho\u00b7heit", ",", "die", "sich", "nie", "ver\u00b7ga\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Liessen die zur Juno werden,", "tokens": ["Lies\u00b7sen", "die", "zur", "Ju\u00b7no", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "APPRART", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die so grossen Geist besa\u00df.", "tokens": ["Die", "so", "gros\u00b7sen", "Geist", "be\u00b7sa\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Krone, Scepter, Wolken, Pfauen", "tokens": ["Kro\u00b7ne", ",", "Scep\u00b7ter", ",", "Wol\u00b7ken", ",", "Pfau\u00b7en"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Musten ihren Muth erh\u00f6hn;", "tokens": ["Mus\u00b7ten", "ih\u00b7ren", "Muth", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Zum Exempel aller Frauen,", "tokens": ["Zum", "Ex\u00b7em\u00b7pel", "al\u00b7ler", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Die das Regiment verstehn.", "tokens": ["Die", "das", "Re\u00b7gi\u00b7ment", "ver\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Jhr so wohlgepaarten Beyde:", "tokens": ["Ihr", "so", "wohl\u00b7ge\u00b7paar\u00b7ten", "Bey\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nheit und Empfindlichkeit!", "tokens": ["Sch\u00f6n\u00b7heit", "und", "Emp\u00b7find\u00b7lich\u00b7keit", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auch du, o s\u00fcsse Freude!", "tokens": ["Und", "auch", "du", ",", "o", "s\u00fcs\u00b7se", "Freu\u00b7de", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "$,", "FM", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mund, der l\u00e4chelnd Lust gebeut;", "tokens": ["Mund", ",", "der", "l\u00e4\u00b7chelnd", "Lust", "ge\u00b7beut", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Rosen aufgebl\u00fchter Wangen;", "tokens": ["Ro\u00b7sen", "auf\u00b7ge\u00b7bl\u00fch\u00b7ter", "Wan\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlaue Blicke; lockigt Haar!", "tokens": ["Schlau\u00b7e", "Bli\u00b7cke", ";", "lo\u00b7ckigt", "Haar", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "VVFIN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jhr nur stellet dem Verlangen", "tokens": ["Ihr", "nur", "stel\u00b7let", "dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Venus oder Phyllis dar.", "tokens": ["Ve\u00b7nus", "o\u00b7der", "Phyl\u00b7lis", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Phyllis! ja: in jenen Zeiten,", "tokens": ["Phyl\u00b7lis", "!", "ja", ":", "in", "je\u00b7nen", "Zei\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADV", "$.", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der alten G\u00f6tter-Welt,", "tokens": ["In", "der", "al\u00b7ten", "G\u00f6t\u00b7ter\u00b7Welt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4ren deinen Trefflichkeiten", "tokens": ["W\u00e4\u00b7ren", "dei\u00b7nen", "Treff\u00b7lich\u00b7kei\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gleichfalls Opfer angestellt:", "tokens": ["Gleich\u00b7falls", "Op\u00b7fer", "an\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleichfalls w\u00fcrden deinen Wagen", "tokens": ["Gleich\u00b7falls", "w\u00fcr\u00b7den", "dei\u00b7nen", "Wa\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tauben oder Schw\u00e4ne ziehn,", "tokens": ["Tau\u00b7ben", "o\u00b7der", "Schw\u00e4\u00b7ne", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dich die Liebes-G\u00f6tter tragen", "tokens": ["Dich", "die", "Lie\u00b7bes\u00b7G\u00f6t\u00b7ter", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und mit dir nach Paphos fliehn.", "tokens": ["Und", "mit", "dir", "nach", "Pa\u00b7phos", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}