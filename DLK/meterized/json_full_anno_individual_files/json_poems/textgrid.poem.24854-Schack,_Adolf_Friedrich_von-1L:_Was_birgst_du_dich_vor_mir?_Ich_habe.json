{"textgrid.poem.24854": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was birgst du dich vor mir? Ich habe", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was birgst du dich vor mir? Ich habe", "tokens": ["Was", "birgst", "du", "dich", "vor", "mir", "?", "Ich", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Tr\u00e4umen schon als Knabe,", "tokens": ["In", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "schon", "als", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als J\u00fcngling schon dich oft geschaut,", "tokens": ["Als", "J\u00fcng\u00b7ling", "schon", "dich", "oft", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sanft deiner N\u00e4he Hauch empfunden", "tokens": ["Sanft", "dei\u00b7ner", "N\u00e4\u00b7he", "Hauch", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und morgens, wenn du mir entschwunden,", "tokens": ["Und", "mor\u00b7gens", ",", "wenn", "du", "mir", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Thr\u00e4nen meinen Pf\u00fchl betaut.", "tokens": ["Mit", "Thr\u00e4\u00b7nen", "mei\u00b7nen", "Pf\u00fchl", "be\u00b7taut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn n\u00e4chtlich unterm Sternendache", "tokens": ["Wenn", "n\u00e4cht\u00b7lich", "un\u00b7term", "Ster\u00b7nen\u00b7da\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Rufen mir, das tausendfache,", "tokens": ["Das", "Ru\u00b7fen", "mir", ",", "das", "tau\u00b7send\u00b7fa\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von Wald und Flur zum Ohre drang,", "tokens": ["Von", "Wald", "und", "Flur", "zum", "Oh\u00b7re", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Oft fernher durch der St\u00fcrme Brausen,", "tokens": ["Oft", "fern\u00b7her", "durch", "der", "St\u00fcr\u00b7me", "Brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Str\u00f6me Rauschen, in den Pausen", "tokens": ["Der", "Str\u00f6\u00b7me", "Rau\u00b7schen", ",", "in", "den", "Pau\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vernahm ich deiner Stimme Klang.", "tokens": ["Ver\u00b7nahm", "ich", "dei\u00b7ner", "Stim\u00b7me", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "In allem Hohen, allem Sch\u00f6nen", "tokens": ["In", "al\u00b7lem", "Ho\u00b7hen", ",", "al\u00b7lem", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "NN", "$,", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der alten Dichtung, in den T\u00f6nen,", "tokens": ["Der", "al\u00b7ten", "Dich\u00b7tung", ",", "in", "den", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mozarts und Webers h\u00f6rt' ich sie;", "tokens": ["Mo\u00b7zarts", "und", "We\u00b7bers", "h\u00f6rt'", "ich", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Orgelklang durch die Chor\u00e4le", "tokens": ["Beim", "Or\u00b7gel\u00b7klang", "durch", "die", "Cho\u00b7r\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erscholl sie mir, und meine Seele", "tokens": ["Er\u00b7scholl", "sie", "mir", ",", "und", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PPER", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Trank br\u00fcnstig ihre Melodie.", "tokens": ["Trank", "br\u00fcns\u00b7tig", "ih\u00b7re", "Me\u00b7lo\u00b7die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch, die du immer mich umschwebtest,", "tokens": ["Doch", ",", "die", "du", "im\u00b7mer", "mich", "um\u00b7schweb\u00b7test", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Oft fragt' ich zweifelnd, ob du lebtest,", "tokens": ["Oft", "fragt'", "ich", "zwei\u00b7felnd", ",", "ob", "du", "leb\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil keine dir auf Erden glich.", "tokens": ["Weil", "kei\u00b7ne", "dir", "auf", "Er\u00b7den", "glich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und, wie die wechselnden Gestalten", "tokens": ["Und", ",", "wie", "die", "wech\u00b7seln\u00b7den", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Des Lebens mir vor\u00fcberwallten,", "tokens": ["Des", "Le\u00b7bens", "mir", "vor\u00b7\u00fc\u00b7berw\u00b7all\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In jeder, jeder sucht' ich dich.", "tokens": ["In", "je\u00b7der", ",", "je\u00b7der", "sucht'", "ich", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PIS", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich sah sie kommen, sah sie schwinden,", "tokens": ["Ich", "sah", "sie", "kom\u00b7men", ",", "sah", "sie", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und konnte nie die eine finden,", "tokens": ["Und", "konn\u00b7te", "nie", "die", "ei\u00b7ne", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nach der das Herz mir einzig rang \u2013", "tokens": ["Nach", "der", "das", "Herz", "mir", "ein\u00b7zig", "rang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Haupt verh\u00fcllt' ich da voll Trauer", "tokens": ["Mein", "Haupt", "ver\u00b7h\u00fcllt'", "ich", "da", "voll", "Trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und f\u00fchlte, wie des Todes Schauer", "tokens": ["Und", "f\u00fchl\u00b7te", ",", "wie", "des", "To\u00b7des", "Schau\u00b7er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ART", "NN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch meine Glieder eisig drang.", "tokens": ["Durch", "mei\u00b7ne", "Glie\u00b7der", "ei\u00b7sig", "drang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Schon schwand vom Leben mir das Beste,", "tokens": ["Schon", "schwand", "vom", "Le\u00b7ben", "mir", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verdorrend sinken seine Aeste,", "tokens": ["Ver\u00b7dor\u00b7rend", "sin\u00b7ken", "sei\u00b7ne", "A\u00b7es\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welk seine Bl\u00e4tter nach und nach;", "tokens": ["Welk", "sei\u00b7ne", "Bl\u00e4t\u00b7ter", "nach", "und", "nach", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch wieder naht, im Sturm sich wiegend,", "tokens": ["Doch", "wie\u00b7der", "naht", ",", "im", "Sturm", "sich", "wie\u00b7gend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPRART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Fr\u00fchling, Grab und Tod besiegend,", "tokens": ["Der", "Fr\u00fch\u00b7ling", ",", "Grab", "und", "Tod", "be\u00b7sie\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und neu wird alte Hoffnung wach.", "tokens": ["Und", "neu", "wird", "al\u00b7te", "Hoff\u00b7nung", "wach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Komm denn, du, die mir immer fehlte,", "tokens": ["Komm", "denn", ",", "du", ",", "die", "mir", "im\u00b7mer", "fehl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Braut, der ich mich im Geist verm\u00e4hlte!", "tokens": ["Braut", ",", "der", "ich", "mich", "im", "Geist", "ver\u00b7m\u00e4hl\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Birg meinem Blick dich l\u00e4nger nicht!", "tokens": ["Birg", "mei\u00b7nem", "Blick", "dich", "l\u00e4n\u00b7ger", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "PPER", "ADJD", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit hohen, sehnsuchtschweren Schl\u00e4gen", "tokens": ["Mit", "ho\u00b7hen", ",", "sehn\u00b7sucht\u00b7schwe\u00b7ren", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Klopft zitternd dir mein Herz entgegen!", "tokens": ["Klopft", "zit\u00b7ternd", "dir", "mein", "Herz", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Komm, da\u00df es nicht in Jammer bricht!", "tokens": ["Komm", ",", "da\u00df", "es", "nicht", "in", "Jam\u00b7mer", "bricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Was birgst du dich vor mir? Ich habe", "tokens": ["Was", "birgst", "du", "dich", "vor", "mir", "?", "Ich", "ha\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In meinen Tr\u00e4umen schon als Knabe,", "tokens": ["In", "mei\u00b7nen", "Tr\u00e4u\u00b7men", "schon", "als", "Kna\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als J\u00fcngling schon dich oft geschaut,", "tokens": ["Als", "J\u00fcng\u00b7ling", "schon", "dich", "oft", "ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sanft deiner N\u00e4he Hauch empfunden", "tokens": ["Sanft", "dei\u00b7ner", "N\u00e4\u00b7he", "Hauch", "emp\u00b7fun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und morgens, wenn du mir entschwunden,", "tokens": ["Und", "mor\u00b7gens", ",", "wenn", "du", "mir", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "punc