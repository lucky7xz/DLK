rd", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Vernahm ich deiner Stimme Klang.", "tokens": ["Ver\u00b7nahm", "ich", "dei\u00b7ner", "Stim\u00b7me", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "In allem Hohen, allem Sch\u00f6nen", "tokens": ["In", "al\u00b7lem", "Ho\u00b7hen", ",", "al\u00b7lem", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PIS", "NN", "$,", "PIS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der alten Dichtung, in den T\u00f6nen,", "tokens": ["Der", "al\u00b7ten", "Dich\u00b7tung", ",", "in", "den", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mozarts und Webers h\u00f6rt' ich sie;", "tokens": ["Mo\u00b7zarts", "und", "We\u00b7bers", "h\u00f6rt'", "ich", "sie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Orgelklang durch die Chor\u00e4le", "tokens": ["Beim", "Or\u00b7gel\u00b7klang", "durch", "die", "Cho\u00b7r\u00e4\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erscholl sie mir, und meine Seele", "tokens": ["Er\u00b7scholl", "sie", "mir", ",", "und", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "PPER", "PPER", "$,", "KON", 