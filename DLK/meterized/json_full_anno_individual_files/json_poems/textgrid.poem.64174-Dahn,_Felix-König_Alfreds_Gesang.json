{"textgrid.poem.64174": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "K\u00f6nig Alfreds Gesang", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlachtfl\u00fcchtig sucht' ich den tiefsten Tann,", "tokens": ["Schlacht\u00b7fl\u00fcch\u00b7tig", "sucht'", "ich", "den", "tiefs\u00b7ten", "Tann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Wo die Dornen zusammen wachsen:", "tokens": ["Wo", "die", "Dor\u00b7nen", "zu\u00b7sam\u00b7men", "wach\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Ein m\u00fcder, wunder, verzweifelter Mann", "tokens": ["Ein", "m\u00fc\u00b7der", ",", "wun\u00b7der", ",", "ver\u00b7zwei\u00b7fel\u00b7ter", "Mann"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "NN", "$,", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und \u2013 der K\u00f6nig der Angelsachsen! \u2013", "tokens": ["Und", "\u2013", "der", "K\u00f6\u00b7nig", "der", "An\u00b7gel\u00b7sach\u00b7sen", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$(", "ART", "NN", "ART", "NN", "$.", "$("], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Fest hielt ich den Grund vor dem \u00dcberdrang,", "tokens": ["Fest", "hielt", "ich", "den", "Grund", "vor", "dem", "\u00dc\u00b7ber\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bis unter der Streitaxt Streichen", "tokens": ["Bis", "un\u00b7ter", "der", "Strei\u00b7taxt", "Strei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mir der Helm und der Schild und das Schwert zersprang: \u2013", "tokens": ["Mir", "der", "Helm", "und", "der", "Schild", "und", "das", "Schwert", "zer\u00b7sprang", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ART", "NN", "KON", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$.", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Da sank ich f\u00fcr tot auf die Leichen. \u2013", "tokens": ["Da", "sank", "ich", "f\u00fcr", "tot", "auf", "die", "Lei\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und \u00fcber den Strand blies Morgenwind:", "tokens": ["Und", "\u00fc\u00b7ber", "den", "Strand", "blies", "Mor\u00b7gen\u00b7wind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der weckte mich scharf und schaurig: \u2013", "tokens": ["Der", "weck\u00b7te", "mich", "scharf", "und", "schau\u00b7rig", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "VVFIN", "KON", "ADJD", "$.", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da wich ich zu Walde, von Stirnblut blind,", "tokens": ["Da", "wich", "ich", "zu", "Wal\u00b7de", ",", "von", "Stirn\u00b7blut", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und zum Sterben matt und traurig. \u2013", "tokens": ["Und", "zum", "Ster\u00b7ben", "matt", "und", "trau\u00b7rig", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPRART", "NN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "O, wie sie nun \u00fcber mein Volk, mein Land,", "tokens": ["O", ",", "wie", "sie", "nun", "\u00fc\u00b7ber", "mein", "Volk", ",", "mein", "Land", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Hinw\u00fcten mit Feuer und Speeren: \u2013", "tokens": ["Hin\u00b7w\u00fc\u00b7ten", "mit", "Feu\u00b7er", "und", "Spee\u00b7ren", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Weh, Glockengeheul und D\u00f6rferbrand \u2013", "tokens": ["Weh", ",", "Glo\u00b7cken\u00b7ge\u00b7heul", "und", "D\u00f6r\u00b7fer\u00b7brand", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und ich kann es nicht wenden noch wehren!", "tokens": ["Und", "ich", "kann", "es", "nicht", "wen\u00b7den", "noch", "weh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "VVINF", "$."], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Alditha, mein Weib, mit den Augen klar,", "tokens": ["Al\u00b7di\u00b7tha", ",", "mein", "Weib", ",", "mit", "den", "Au\u00b7gen", "klar", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Mit den s\u00fc\u00dfen, den lallenden Kinden,", "tokens": ["Mit", "den", "s\u00fc\u00b7\u00dfen", ",", "den", "lal\u00b7len\u00b7den", "Kin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ART", "ADJA", "NN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Mit dem goldenen Herzen und goldenen Haar: \u2013 \u2013", "tokens": ["Mit", "dem", "gol\u00b7de\u00b7nen", "Her\u00b7zen", "und", "gol\u00b7de\u00b7nen", "Haar", ":", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "$.", "$(", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Wann werd' ich dich wieder finden?", "tokens": ["Wann", "werd'", "ich", "dich", "wie\u00b7der", "fin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PRF", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, ich hab' es im Brausen der Wipfel erlauscht,", "tokens": ["Ja", ",", "ich", "hab'", "es", "im", "Brau\u00b7sen", "der", "Wip\u00b7fel", "er\u00b7lauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "PPER", "APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wann bitter mich brannte die Wunde,", "tokens": ["Wann", "bit\u00b7ter", "mich", "brann\u00b7te", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Wann die Tannen gesaust und die Brandung gerauscht, \u2013", "tokens": ["Wann", "die", "Tan\u00b7nen", "ge\u00b7saust", "und", "die", "Bran\u00b7dung", "ge\u00b7rauscht", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "KON", "ART", "NN", "VVPP", "$,", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Aufrei\u00df' ich mein Volk vom Grunde!", "tokens": ["Auf\u00b7rei\u00b7\u00df'", "ich", "mein", "Volk", "vom", "Grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Bei Aldithens Jammer gelob' ich's und schw\u00f6r's: \u2013", "tokens": ["Bei", "Al\u00b7di\u00b7thens", "Jam\u00b7mer", "ge\u00b7lob'", "ich's", "und", "schw\u00f6r's", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "PIS", "KON", "VVFIN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Bei der Schande der d\u00e4nischen Ketten: \u2013", "tokens": ["Bei", "der", "Schan\u00b7de", "der", "d\u0