{"textgrid.poem.53021": {"metadata": {"author": {"name": "Pyra, Jakob Immanuel", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohin, wohin, ihr s\u00fcssen Rasereyen,", "genre": "verse", "period": "N.A.", "pub_year": 1729, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, wohin, ihr s\u00fcssen Rasereyen,", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "ihr", "s\u00fcs\u00b7sen", "Ra\u00b7se\u00b7rey\u00b7en", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des feurigen Vergn\u00fcgens starcke Lust?", "tokens": ["Des", "feu\u00b7ri\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gens", "star\u00b7cke", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohin, wohin, entrei\u00dft ihr meinen Geist?", "tokens": ["Wo\u00b7hin", ",", "wo\u00b7hin", ",", "ent\u00b7rei\u00dft", "ihr", "mei\u00b7nen", "Geist", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O selige Th\u00e4ler!", "tokens": ["O", "se\u00b7li\u00b7ge", "Th\u00e4\u00b7ler", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Wer rufet mich, wer k\u00f6mmt mir schon entgegen?", "tokens": ["Wer", "ru\u00b7fet", "mich", ",", "wer", "k\u00f6mmt", "mir", "schon", "ent\u00b7ge\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wer ists, als du, mein Damon, meine Lust!", "tokens": ["Wer", "ists", ",", "als", "du", ",", "mein", "Da\u00b7mon", ",", "mei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "KOUS", "PPER", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich seh dich schon, und Hilas an der Brust;", "tokens": ["Ich", "seh", "dich", "schon", ",", "und", "Hi\u00b7las", "an", "der", "Brust", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O himmlische Doris!", "tokens": ["O", "himm\u00b7li\u00b7sche", "Do\u00b7ris", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Welch grauser Blick! Ach fliehet diese Th\u00e4ler,", "tokens": ["Welch", "grau\u00b7ser", "Blick", "!", "Ach", "flie\u00b7het", "die\u00b7se", "Th\u00e4\u00b7ler", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$.", "ITJ", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kommt, flieht mit mir! O rauchend blutges Feld!", "tokens": ["Kommt", ",", "flieht", "mit", "mir", "!", "O", "rau\u00b7chend", "blut\u00b7ges", "Feld", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "APPR", "PPER", "$.", "NE", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mars triumphiret mit bespr\u00fctztem Schwerdt", "tokens": ["Mars", "tri\u00b7um\u00b7phi\u00b7ret", "mit", "be\u00b7spr\u00fctz\u00b7tem", "Schwerdt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "In demantnen Waffen.", "tokens": ["In", "de\u00b7mant\u00b7nen", "Waf\u00b7fen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Gebt mir die Hand! Komm Damon, komm o Doris,", "tokens": ["Gebt", "mir", "die", "Hand", "!", "Komm", "Da\u00b7mon", ",", "komm", "o", "Do\u00b7ris", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$.", "VVFIN", "NE", "$,", "VVFIN", "FM", "NE", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Kommt, ewiglich geliebte Zwey, ach kommt!", "tokens": ["Kommt", ",", "e\u00b7wig\u00b7lich", "ge\u00b7lieb\u00b7te", "Zwey", ",", "ach", "kommt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJD", "ADJA", "NN", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Errettet euch, mein Hilas sey die Last", "tokens": ["Er\u00b7ret\u00b7tet", "euch", ",", "mein", "Hi\u00b7las", "sey", "die", "Last"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der willigen Arme.", "tokens": ["Der", "wil\u00b7li\u00b7gen", "Ar\u00b7me", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.5": {"line.1": {"text": "Schau, Doris, schau, die grausen W\u00fcsteneyen", "tokens": ["Schau", ",", "Do\u00b7ris", ",", "schau", ",", "die", "grau\u00b7sen", "W\u00fcs\u00b7te\u00b7ne\u00b7yen"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NE", "$,", "PTKVZ", "$,", "ART", "ADJA", "NN"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Bedeckt dein Blick mit aller Blumen Pracht.", "tokens": ["Be\u00b7deckt", "dein", "Blick", "mit", "al\u00b7ler", "Blu\u00b7men", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du singst, o Freund, es kommen zum Pallast", "tokens": ["Du", "singst", ",", "o", "Freund", ",", "es", "kom\u00b7men", "zum", "Pal\u00b7last"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Felsen gerollet.", "tokens": ["Die", "Fel\u00b7sen", "ge\u00b7rol\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "O! was sind die\u00df vor unbekannte Wiesen?", "tokens": ["O", "!", "was", "sind", "die\u00df", "vor", "un\u00b7be\u00b7kann\u00b7te", "Wie\u00b7sen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWS", "VAFIN", "PDS", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In derer Einsamkeit kein falscher Freund uns st\u00f6hrt?", "tokens": ["In", "de\u00b7rer", "Ein\u00b7sam\u00b7keit", "kein", "fal\u00b7scher", "Freund", "uns", "st\u00f6hrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "NN", "PIAT", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Wo h\u00f6rt man mich, euch, einem Sch\u00e4ffer gleich,", "tokens": ["Wo", "h\u00f6rt", "man", "mich", ",", "euch", ",", "ei\u00b7nem", "Sch\u00e4f\u00b7fer", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PIS", "PPER", "$,", "PPER", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gantz fr\u00f6lich besingen?", "tokens": ["Gantz", "fr\u00f6\u00b7lich", "be\u00b7sin\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.7": {"line.1": {"text": "Wie ist mir, ach! erwach ich aus dem Schlafe?", "tokens": ["Wie", "ist", "mir", ",", "ach", "!", "er\u00b7wach", "ich", "aus", "dem", "Schla\u00b7fe", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ITJ", "$.", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Welch lieblicher, jedoch auch schwerer Traum", "tokens": ["Welch", "lieb\u00b7li\u00b7cher", ",", "je\u00b7doch", "auch", "schwe\u00b7rer", "Traum"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "$,", "ADV", "ADV", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Umnebelte mein wachendes Gehirn,", "tokens": ["Um\u00b7ne\u00b7bel\u00b7te", "mein", "wa\u00b7chen\u00b7des", "Ge\u00b7hirn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und dr\u00fcckte mein Hertz?", "tokens": ["Und", "dr\u00fcck\u00b7te", "mein", "Hertz", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.8": {"line.1": {"text": "Ach Damon, Freund, ach Doris, sollt ihr beyde,", "tokens": ["Ach", "Da\u00b7mon", ",", "Freund", ",", "ach", "Do\u00b7ris", ",", "sollt", "ihr", "bey\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "$,", "XY", "NE", "$,", "VMFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verjagt und arm, mit mir in W\u00fcsten ziehn?", "tokens": ["Ver\u00b7jagt", "und", "arm", ",", "mit", "mir", "in", "W\u00fcs\u00b7ten", "ziehn", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Soll deinen zarten Fu\u00df, o Doris, und dein Kind", "tokens": ["Soll", "dei\u00b7nen", "zar\u00b7ten", "Fu\u00df", ",", "o", "Do\u00b7ris", ",", "und", "dein", "Kind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "ADJA", "NN", "$,", "FM", "NE", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Die W\u00fcste verbrennen?", "tokens": ["Die", "W\u00fcs\u00b7te", "ver\u00b7bren\u00b7nen", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.9": {"line.1": {"text": "Ach sollt ich nicht in eurem heilgen Haine,", "tokens": ["Ach", "sollt", "ich", "nicht", "in", "eu\u00b7rem", "heil\u00b7gen", "Hai\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit euch allein, in d\u00fcstrer Einsamkeit,", "tokens": ["Mit", "euch", "al\u00b7lein", ",", "in", "d\u00fcst\u00b7rer", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich, Doris, freundlich l\u00e4cheln, lieblich schertzen sehn,", "tokens": ["Dich", ",", "Do\u00b7ris", ",", "freund\u00b7lich", "l\u00e4\u00b7cheln", ",", "lieb\u00b7lich", "schert\u00b7zen", "sehn", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "ADJD", "VVFIN", "$,", "ADJD", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dich, Damon, singen h\u00f6ren?", "tokens": ["Dich", ",", "Da\u00b7mon", ",", "sin\u00b7gen", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NE", "$,", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ach solltest du, o Hoffnung, mich betr\u00fcgen?", "tokens": ["Ach", "soll\u00b7test", "du", ",", "o", "Hoff\u00b7nung", ",", "mich", "be\u00b7tr\u00fc\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "$,", "FM", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein theures Paar, ich werd, ich werd euch sehn;", "tokens": ["Mein", "theu\u00b7res", "Paar", ",", "ich", "werd", ",", "ich", "werd", "euch", "sehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VAFIN", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr werdet mich mit eurem ofnen Arm", "tokens": ["Ihr", "wer\u00b7det", "mich", "mit", "eu\u00b7rem", "of\u00b7nen", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bald keuchend umfangen.", "tokens": ["Bald", "keu\u00b7chend", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.11": {"line.1": {"text": "Der Wanderstab steht wartend an der Pfoste,", "tokens": ["Der", "Wan\u00b7der\u00b7stab", "steht", "war\u00b7tend", "an", "der", "Pfos\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und mein Ger\u00e4th liegt auf den Weg bereit.", "tokens": ["Und", "mein", "Ge\u00b7r\u00e4\u00b7th", "liegt", "auf", "den", "Weg", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Erbittet mir zu meiner Reise nur", "tokens": ["Er\u00b7bit\u00b7tet", "mir", "zu", "mei\u00b7ner", "Rei\u00b7se", "nur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den fr\u00f6lichen Morgen.", "tokens": ["Den", "fr\u00f6\u00b7li\u00b7chen", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.12": {"line.1": {"text": "Sobald als nur die gl\u00fchend gr\u00f6ssre Sonne,", "tokens": ["So\u00b7bald", "als", "nur", "die", "gl\u00fc\u00b7hend", "gr\u00f6ss\u00b7re", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ADV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit strahlendem und halben Angesicht,", "tokens": ["Mit", "strah\u00b7len\u00b7dem", "und", "hal\u00b7ben", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach zwoen N\u00e4chten noch, dort \u00fcber jener H\u00f6h", "tokens": ["Nach", "zwoen", "N\u00e4ch\u00b7ten", "noch", ",", "dort", "\u00fc\u00b7ber", "je\u00b7ner", "H\u00f6h"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADV", "$,", "ADV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Auf unser Thal wird blicken;", "tokens": ["Auf", "un\u00b7ser", "Thal", "wird", "bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Sobald werd ich, vor Lust unausgeschlafen,", "tokens": ["So\u00b7bald", "werd", "ich", ",", "vor", "Lust", "un\u00b7aus\u00b7ge\u00b7schla