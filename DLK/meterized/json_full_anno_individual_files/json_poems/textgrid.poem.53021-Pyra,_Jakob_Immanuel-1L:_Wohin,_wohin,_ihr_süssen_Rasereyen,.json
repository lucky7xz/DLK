-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So werd ich schon von fern dein Dorf und euch", "tokens": ["So", "werd", "ich", "schon", "von", "fern", "dein", "Dorf", "und", "euch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "ADJD", "PPOSAT", "NN", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Jauchzen begr\u00fcssen.", "tokens": ["Mit", "Jauch\u00b7zen", "be\u00b7gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.15": {"line.1": {"text": "Doch qu\u00e4le mich mit traurigen Gedancken", "tokens": ["Doch", "qu\u00e4\u00b7le", "mich", "mit", "trau\u00b7ri\u00b7gen", "Ge\u00b7dan\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von dir, o Freund und Doris, fort nicht mehr.", "tokens": ["Von", "dir", ",", "o", "Freund", "und", "Do\u00b7ris", ",", "fort", "nicht", "mehr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NN", "KON", "NE", "$,", "PTKVZ", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bey eurer Lust leb ich allein vergn\u00fcget,", "tokens": ["Bey", "eu\u00b7rer", "Lust", "leb", "ich", "al\u00b7lein", "ver\u00b7gn\u00fc\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit euch auch sterb ich traurig.", "tokens": ["Mit", "euch", "auch", "sterb", "ich", "trau\u00b7rig", "."], "to