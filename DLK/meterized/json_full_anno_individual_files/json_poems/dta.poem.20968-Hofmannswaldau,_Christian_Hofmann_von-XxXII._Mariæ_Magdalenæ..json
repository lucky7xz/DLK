{"dta.poem.20968": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxXII.  \n Mari\u00e6 Magdalen\u00e6.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Hie ruht das sch\u00f6ne Haupt/ hie ruht die sch\u00f6ne", "tokens": ["Hie", "ruht", "das", "sch\u00f6\u00b7ne", "Haupt", "/", "hie", "ruht", "die", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$(", "ADV", "VVFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Scho\u00df/", "tokens": ["Scho\u00df", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Au\u00df der die Liebligkeit mit reichen Str\u00f6men flo\u00df.", "tokens": ["Au\u00df", "der", "die", "Lieb\u00b7lig\u00b7keit", "mit", "rei\u00b7chen", "Str\u00f6\u00b7men", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Nach dem di\u00df zarte Weib verlie\u00df den Huren-Orden/", "tokens": ["Nach", "dem", "di\u00df", "zar\u00b7te", "Weib", "ver\u00b7lie\u00df", "den", "Hu\u00b7ren\u00b7Or\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PDS", "ADJA", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So sind die Engel selbst derselben Buler worden.", "tokens": ["So", "sind", "die", "En\u00b7gel", "selbst", "der\u00b7sel\u00b7ben", "Bu\u00b7ler", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "PDAT", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}