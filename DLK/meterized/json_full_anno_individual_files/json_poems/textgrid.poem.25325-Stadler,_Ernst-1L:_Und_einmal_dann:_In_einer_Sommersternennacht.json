{"textgrid.poem.25325": {"metadata": {"author": {"name": "Stadler, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und einmal dann: In einer Sommersternennacht", "genre": "verse", "period": "N.A.", "pub_year": 1898, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und einmal dann: In einer Sommersternennacht", "tokens": ["Und", "ein\u00b7mal", "dann", ":", "In", "ei\u00b7ner", "Som\u00b7mers\u00b7ter\u00b7nen\u00b7nacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "$.", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "wenn alles Leben wie gel\u00f6st", "tokens": ["wenn", "al\u00b7les", "Le\u00b7ben", "wie", "ge\u00b7l\u00f6st"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "NN", "KOKOM", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "in sammetweiche Schwermut liegt", "tokens": ["in", "sam\u00b7met\u00b7wei\u00b7che", "Schwer\u00b7mut", "liegt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und \u00fcberm Forst noch der spr\u00fchende Goldschein h\u00e4ngt", "tokens": ["und", "\u00fc\u00b7berm", "Forst", "noch", "der", "spr\u00fc\u00b7hen\u00b7de", "Gold\u00b7schein", "h\u00e4ngt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "zitternd wie bla\u00df aufglimmernde Gewebe", "tokens": ["zit\u00b7ternd", "wie", "bla\u00df", "auf\u00b7glim\u00b7mern\u00b7de", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "KOKOM", "ADJD", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.6": {"text": "und zart wie Flaum: Dann wird ein langer Ruf", "tokens": ["und", "zart", "wie", "Flaum", ":", "Dann", "wird", "ein", "lan\u00b7ger", "Ruf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "NN", "$.", "ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "aus Traum und Schlummer ladend uns erl\u00f6sen.", "tokens": ["aus", "Traum", "und", "Schlum\u00b7mer", "la\u00b7dend", "uns", "er\u00b7l\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann ziehen wir indes der Feuerschein", "tokens": ["Dann", "zie\u00b7hen", "wir", "in\u00b7des", "der", "Feu\u00b7er\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "sich dichter um uns schlie\u00dft in dunklen Haufen", "tokens": ["sich", "dich\u00b7ter", "um", "uns", "schlie\u00dft", "in", "dunk\u00b7len", "Hau\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "die Stirn mit Laub geg\u00fcrtet \u00fcber Schollen", "tokens": ["die", "Stirn", "mit", "Laub", "ge\u00b7g\u00fcr\u00b7tet", "\u00fc\u00b7ber", "Schol\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sprossender \u00c4cker in das sinkende Licht.", "tokens": ["spros\u00b7sen\u00b7der", "\u00c4\u00b7cker", "in", "das", "sin\u00b7ken\u00b7de", "Licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Uns rei\u00dft des wilden Lebens j\u00e4he s\u00fc\u00dfe", "tokens": ["Uns", "rei\u00dft", "des", "wil\u00b7den", "Le\u00b7bens", "j\u00e4\u00b7he", "s\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "bet\u00f6rend lockende Zigeunerweise", "tokens": ["be\u00b7t\u00f6\u00b7rend", "lo\u00b7cken\u00b7de", "Zi\u00b7geu\u00b7ner\u00b7wei\u00b7se"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "in Nacht und Duft. Schon gl\u00e4nzt aus letzter Glut", "tokens": ["in", "Nacht", "und", "Duft", ".", "Schon", "gl\u00e4nzt", "aus", "letz\u00b7ter", "Glut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$.", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "die \u00fcber der erloschnen Haide funkelt", "tokens": ["die", "\u00fc\u00b7ber", "der", "er\u00b7lo\u00b7schnen", "Hai\u00b7de", "fun\u00b7kelt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "das gro\u00dfe Ziel. Schon schlingen sich die Reihn", "tokens": ["das", "gro\u00b7\u00dfe", "Ziel", ".", "Schon", "schlin\u00b7gen", "sich", "die", "Reihn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "vom Takt gef\u00fcgt. Schon st\u00fcrmen jauchzend", "tokens": ["vom", "Takt", "ge\u00b7f\u00fcgt", ".", "Schon", "st\u00fcr\u00b7men", "jauch\u00b7zend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$.", "ADV", "VVINF", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "die Vordersten in losgelassnem Tanz", "tokens": ["die", "Vor\u00b7ders\u00b7ten", "in", "los\u00b7ge\u00b7lass\u00b7nem", "Tanz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.8": {"text": "und eine Kette wirrer hei\u00dfer Stimmen w\u00e4lzt", "tokens": ["und", "ei\u00b7ne", "Ket\u00b7te", "wir\u00b7rer", "hei\u00b7\u00dfer", "Stim\u00b7men", "w\u00e4lzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "der Jubel schwer sich durch die Massen. Fackeln spritzen", "tokens": ["der", "Ju\u00b7bel", "schwer", "sich", "durch", "die", "Mas\u00b7sen", ".", "Fa\u00b7ckeln", "sprit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADJD", "PRF", "APPR", "ART", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "flackernde Flecken auf die schwarze Wand der \u00c4ste.", "tokens": ["fla\u00b7ckern\u00b7de", "Fle\u00b7cken", "auf", "die", "schwar\u00b7ze", "Wand", "der", "\u00c4s\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.11": {"text": "Auftaumelnd st\u00fcrzen Schatten. M\u00e4dchen schwenken", "tokens": ["Auf\u00b7tau\u00b7melnd", "st\u00fcr\u00b7zen", "Schat\u00b7ten", ".", "M\u00e4d\u00b7chen", "schwen\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "VVFIN", "NN", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "flitternde Birkenb\u00fcschel Frauen l\u00f6sen", "tokens": ["flit\u00b7tern\u00b7de", "Bir\u00b7ken\u00b7b\u00fc\u00b7schel", "Frau\u00b7en", "l\u00f6\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "VVINF"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.13": {"text": "die raschelnden Gewande tanzen nackt", "tokens": ["die", "ra\u00b7scheln\u00b7den", "Ge\u00b7wan\u00b7de", "tan\u00b7zen", "nackt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "vom Diadem der Haare \u00fcberstr\u00f6mt", "tokens": ["vom", "Di\u00b7a\u00b7dem", "der", "Haa\u00b7re", "\u00fc\u00b7ber\u00b7str\u00f6mt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.15": {"text": "ins Licht und ihre hei\u00dfen Augen schillern", "tokens": ["ins", "Licht", "und", "ih\u00b7re", "hei\u00b7\u00dfen", "Au\u00b7gen", "schil\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "unst\u00e4t wie Feuerglanz auf Abendlachen.", "tokens": ["un\u00b7st\u00e4t", "wie", "Feu\u00b7er\u00b7glanz", "auf", "A\u00b7bend\u00b7la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.17": {"text": "Und wilder glei\u00dft das tolle s\u00fc\u00dfe Lied.", "tokens": ["Und", "wil\u00b7der", "glei\u00dft", "das", "tol\u00b7le", "s\u00fc\u00b7\u00dfe", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Und wilder rast und st\u00fcrmt der hei\u00dfe Tanz.", "tokens": ["Und", "wil\u00b7der", "rast", "und", "st\u00fcrmt", "der", "hei\u00b7\u00dfe", "Tanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Und Wunder steigen auf wie Herbstnachtnebel.", "tokens": ["Und", "Wun\u00b7der", "stei\u00b7gen", "auf", "wie", "Herbst\u00b7nacht\u00b7ne\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "KOKOM", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Schon rollt das gro\u00dfe Leben wie ein Meer", "tokens": ["Schon", "rollt", "das", "gro\u00b7\u00dfe", "Le\u00b7ben", "wie", "ein", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "das gischtend gegen nackte Felsen b\u00e4umt", "tokens": ["das", "gischtend", "ge\u00b7gen", "nack\u00b7te", "Fel\u00b7sen", "b\u00e4umt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.22": {"text": "von br\u00e4unlich goldner D\u00e4mmerung umloht.", "tokens": ["von", "br\u00e4un\u00b7l