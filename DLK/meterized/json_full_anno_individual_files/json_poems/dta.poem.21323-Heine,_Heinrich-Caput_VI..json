{"dta.poem.21323": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput VI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201edu bist der Richter, der B\u00fcttel bin ich,", "tokens": ["\u201e", "du", "bist", "der", "Rich\u00b7ter", ",", "der", "B\u00fct\u00b7tel", "bin", "ich", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+--+-++", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mit dem Gehorsam des Knechtes", "tokens": ["Und", "mit", "dem", "Ge\u00b7hor\u00b7sam", "des", "Knech\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}, "line.3": {"text": "Vollstreck\u2019 ich das Urtheil, das du gef\u00e4llt,", "tokens": ["Voll\u00b7streck'", "ich", "das", "Ur\u00b7theil", ",", "das", "du", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Und sey es ein ungerechtes.", "tokens": ["Und", "sey", "es", "ein", "un\u00b7ge\u00b7rech\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "ADJA", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "\u201edem Consul trug man ein Beil voran,", "tokens": ["\u201e", "dem", "Con\u00b7sul", "trug", "man", "ein", "Beil", "vo\u00b7ran", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PIS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Zu Rom, in alten Tagen.", "tokens": ["Zu", "Rom", ",", "in", "al\u00b7ten", "Ta\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auch du hast deinen Liktor, doch wird", "tokens": ["Auch", "du", "hast", "dei\u00b7nen", "Lik\u00b7tor", ",", "doch", "wird"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Beil dir nachgetragen.", "tokens": ["Das", "Beil", "dir", "nach\u00b7ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201eich bin dein Liktor, und ich geh\u2019", "tokens": ["\u201e", "ich", "bin", "dein", "Lik\u00b7tor", ",", "und", "ich", "geh'"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Best\u00e4ndig mit dem blanken", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "mit", "dem", "blan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Richtbeile hinter dir \u2013 ich bin", "tokens": ["Richt\u00b7bei\u00b7le", "hin\u00b7ter", "dir", "\u2013", "ich", "bin"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "PPER", "$(", "PPER", "VAFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Die That von deinem Gedanken.\u201c", "tokens": ["Die", "That", "von", "dei\u00b7nem", "Ge\u00b7dan\u00b7ken", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}