{"dta.poem.19728": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Der Geist beym verborgnen Schatze .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe einen Schatz und den mu\u00df ich meiden,               ", "tokens": ["Ich", "ha\u00b7be", "ei\u00b7nen", "Schatz", "und", "den", "mu\u00df", "ich", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ART", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mu\u00df von ihm gehn, kein Wort mit ihm zu reden,", "tokens": ["Mu\u00df", "von", "ihm", "gehn", ",", "kein", "Wort", "mit", "ihm", "zu", "re\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPER", "VVINF", "$,", "PIAT", "NN", "APPR", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Herze in dem Leibe m\u00f6chte mir vergehn,", "tokens": ["Das", "Her\u00b7ze", "in", "dem", "Lei\u00b7be", "m\u00f6ch\u00b7te", "mir", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den Sonntag, den Montag in aller fruh,", "tokens": ["Den", "Sonn\u00b7tag", ",", "den", "Mon\u00b7tag", "in", "al\u00b7ler", "fruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Schickt mir mein Schatz die traurige Botschaft zu,", "tokens": ["Schickt", "mir", "mein", "Schatz", "die", "trau\u00b7ri\u00b7ge", "Bot\u00b7schaft", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Ich sollte ihn begleiten bis in das k\u00fchle Grab,", "tokens": ["Ich", "soll\u00b7te", "ihn", "be\u00b7glei\u00b7ten", "bis", "in", "das", "k\u00fch\u00b7le", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "Dieweil er mich so treulich geliebet hat.", "tokens": ["Die\u00b7weil", "er", "mich", "so", "treu\u00b7lich", "ge\u00b7lie\u00b7bet", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Ich habe ein Herz, ist h\u00e4rter als ein Stein,", "tokens": ["Ich", "ha\u00b7be", "ein", "Herz", ",", "ist", "h\u00e4r\u00b7ter", "als", "ein", "Stein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VAFIN", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Wo tausend Seufzer verborgen seyn,", "tokens": ["Wo", "tau\u00b7send", "Seuf\u00b7zer", "ver\u00b7bor\u00b7gen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "VVPP", "VAINF", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Viel lieber w\u00e4r mirs, ich l\u00e4g in einem Grab,", "tokens": ["Viel", "lie\u00b7ber", "w\u00e4r", "mirs", ",", "ich", "l\u00e4g", "in", "ei\u00b7nem", "Grab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NE", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "So k\u00e4m ich ja von allem meinem Trauren ab.", "tokens": ["So", "k\u00e4m", "ich", "ja", "von", "al\u00b7lem", "mei\u00b7nem", "Trau\u00b7ren", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PIS", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}