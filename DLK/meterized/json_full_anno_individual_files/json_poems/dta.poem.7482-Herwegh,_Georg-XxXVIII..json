{"dta.poem.7482": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XxXVIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O h\u00e4tten sie mir doch ihr Ohr geliehen", "tokens": ["O", "h\u00e4t\u00b7ten", "sie", "mir", "doch", "ihr", "Ohr", "ge\u00b7lie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "PPER", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In jenen ersten ungl\u00fccksel'gen Stunden,", "tokens": ["In", "je\u00b7nen", "ers\u00b7ten", "un\u00b7gl\u00fcck\u00b7sel'\u00b7gen", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da ich die Spur der Herrlichen gefunden,", "tokens": ["Da", "ich", "die", "Spur", "der", "Herr\u00b7li\u00b7chen", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sprach: Ihr Freunde, la\u00dft mich weiter ziehen!", "tokens": ["Und", "sprach", ":", "Ihr", "Freun\u00b7de", ",", "la\u00dft", "mich", "wei\u00b7ter", "zie\u00b7hen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "NN", "$,", "VVIMP", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie lachten aber meiner nur und schrieen:", "tokens": ["Sie", "lach\u00b7ten", "a\u00b7ber", "mei\u00b7ner", "nur", "und", "schri\u00b7een", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADV", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pah! ein ", "tokens": ["Pah", "!", "ein"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$.", "ART"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der Vogel ist nun einmal festgebunden,", "tokens": ["Der", "Vo\u00b7gel", "ist", "nun", "ein\u00b7mal", "fest\u00b7ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und soll so bald nicht wieder uns entfliehen.", "tokens": ["Und", "soll", "so", "bald", "nicht", "wie\u00b7der", "uns", "ent\u00b7flie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PTKNEG", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt wollen Alle die Gefahr erkennen;", "tokens": ["Jetzt", "wol\u00b7len", "Al\u00b7le", "die", "Ge\u00b7fahr", "er\u00b7ken\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sie f\u00fchren mir den Engel aus dem Haus,", "tokens": ["Sie", "f\u00fch\u00b7ren", "mir", "den", "En\u00b7gel", "aus", "dem", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da mir die Kraft versagt, um mich zu trennen.", "tokens": ["Da", "mir", "die", "Kraft", "ver\u00b7sagt", ",", "um", "mich", "zu", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,", "KOUI", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Lauft darauf alle Weisheit denn hinaus?", "tokens": ["Lauft", "da\u00b7rauf", "al\u00b7le", "Weis\u00b7heit", "denn", "hin\u00b7aus", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PIAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr la\u00dft den Schmetterling getrost verbrennen,", "tokens": ["Ihr", "la\u00dft", "den", "Schmet\u00b7ter\u00b7ling", "ge\u00b7trost", "ver\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und l\u00f6scht voll Mitleid dann die Kerzen aus!", "tokens": ["Und", "l\u00f6scht", "voll", "Mit\u00b7leid", "dann", "die", "Ker\u00b7zen", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}