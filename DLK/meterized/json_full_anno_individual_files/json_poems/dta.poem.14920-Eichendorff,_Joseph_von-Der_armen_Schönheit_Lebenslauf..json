{"dta.poem.14920": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der armen Sch\u00f6nheit Lebenslauf.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Die Augen, die so furchtsam waren,", "tokens": ["Die", "Au\u00b7gen", ",", "die", "so", "furcht\u00b7sam", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die haben jetzt so freien Lauf,", "tokens": ["Die", "ha\u00b7ben", "jetzt", "so", "frei\u00b7en", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fort ist das Kr\u00e4nzlein aus den Haaren,", "tokens": ["Fort", "ist", "das", "Kr\u00e4nz\u00b7lein", "aus", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hohe Federn steh'n darauf.", "tokens": ["Und", "ho\u00b7he", "Fe\u00b7dern", "steh'n", "da\u00b7rauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}