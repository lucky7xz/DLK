{"textgrid.poem.64789": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "12. Grabschrift.", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Geist von Kenntni\u00df voll, ein Herz voll edler Triebe,", "tokens": ["Ein", "Geist", "von", "Kennt\u00b7ni\u00df", "voll", ",", "ein", "Herz", "voll", "ed\u00b7ler", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Gott gef\u00e4llig macht, was die Vernunft verehrt,", "tokens": ["Was", "Gott", "ge\u00b7f\u00e4l\u00b7lig", "macht", ",", "was", "die", "Ver\u00b7nunft", "ver\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "ADJD", "VVFIN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erhob die - - -: das Gl\u00fcck von ihrer Liebe", "tokens": ["Er\u00b7hob", "die", ":", "das", "Gl\u00fcck", "von", "ih\u00b7rer", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "$(", "$(", "$(", "$.", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ward nur drey kurze Jahr' Dem, der sie klagt, gew\u00e4hrt.", "tokens": ["Ward", "nur", "drey", "kur\u00b7ze", "Jahr'", "Dem", ",", "der", "sie", "klagt", ",", "ge\u00b7w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN",