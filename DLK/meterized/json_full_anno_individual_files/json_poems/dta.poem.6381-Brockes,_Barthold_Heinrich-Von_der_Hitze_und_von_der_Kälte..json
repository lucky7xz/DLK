{"dta.poem.6381": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Hitze und von der K\u00e4lte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus erste Element mu\u00df man hiebey gedencken,", "tokens": ["Aus", "ers\u00b7te", "E\u00b7le\u00b7ment", "mu\u00df", "man", "hie\u00b7bey", "ge\u00b7den\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PIS", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wodurch, mit wallenden Bewegen,", "tokens": ["Wo\u00b7durch", ",", "mit", "wal\u00b7len\u00b7den", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wasser-Theilchen stets sich regen.", "tokens": ["Die", "Was\u00b7ser\u00b7T\u00b7heil\u00b7chen", "stets", "sich", "re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn es sich zwischen Eis und Saltz kan ferner sencken,", "tokens": ["Wenn", "es", "sich", "zwi\u00b7schen", "Eis", "und", "Saltz", "kan", "fer\u00b7ner", "sen\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "NN", "KON", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So eilt es schnell dadurch: wodurch sodann die Fluht,", "tokens": ["So", "eilt", "es", "schnell", "da\u00b7durch", ":", "wo\u00b7durch", "so\u00b7dann", "die", "Fluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PAV", "$.", "PWAV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Indem ihr leichter Trieb und Wallen sich verliehrt,", "tokens": ["In\u00b7dem", "ihr", "leich\u00b7ter", "Trieb", "und", "Wal\u00b7len", "sich", "ver\u00b7liehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jm Sommer eben so gefriert,", "tokens": ["Jm", "Som\u00b7mer", "e\u00b7ben", "so", "ge\u00b7friert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Als wie sie sonst im Winter thut.", "tokens": ["Als", "wie", "sie", "sonst", "im", "Win\u00b7ter", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}