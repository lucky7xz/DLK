, "tokens": ["Nie", "mocht'", "ein", "Men\u00b7schen\u00b7au\u00b7ge", "mich", "be\u00b7gl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "So tief in Lieb' und seligem Entz\u00fccken,", "tokens": ["So", "tief", "in", "Lieb'", "und", "se\u00b7li\u00b7gem", "Ent\u00b7z\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Als ich in deines Himmels Bl\u00e4ue seh'!", "tokens": ["Als", "ich", "in", "dei\u00b7nes", "Him\u00b7mels", "Bl\u00e4u\u00b7e", "seh'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "O, nimmer t\u00e4uschest du! du kehrest wieder", "tokens": ["O", ",", "nim\u00b7mer", "t\u00e4u\u00b7schest", "du", "!", "du", "keh\u00b7rest", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und neue Sch\u00f6nheit, neu erwachte Lieder", "tokens": ["Und", "neu\u00b7e", "Sch\u00f6n\u00b7heit", ",", "neu", "er\u00b7wach\u00b7te", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Verscheuchen jedes Leid und jedes Weh!", "tokens": ["Ver\u00b7scheu\u00b7chen", "je\u00b7des", "Leid", "und", "je\u00b7des", "Weh", "!"], "token_info": ["word", "word", "word",