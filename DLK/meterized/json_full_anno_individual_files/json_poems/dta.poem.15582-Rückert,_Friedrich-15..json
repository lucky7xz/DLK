{"dta.poem.15582": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Arbeiter dingt der Herr f\u00fcr seinen Arbeitstag,", "tokens": ["Ar\u00b7bei\u00b7ter", "dingt", "der", "Herr", "f\u00fcr", "sei\u00b7nen", "Ar\u00b7beits\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Abends jedem gibt er seines Lohns Betrag.", "tokens": ["Und", "A\u00b7bends", "je\u00b7dem", "gibt", "er", "sei\u00b7nes", "Lohns", "Be\u00b7trag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIS", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nur einem einz'gen gibt er einen Ueberschu\u00df;", "tokens": ["Nur", "ei\u00b7nem", "einz'\u00b7gen", "gibt", "er", "ei\u00b7nen", "Ue\u00b7ber\u00b7schu\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das sehn die anderen Arbeiter mit Verdru\u00df,", "tokens": ["Das", "sehn", "die", "an\u00b7de\u00b7ren", "Ar\u00b7bei\u00b7ter", "mit", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und sprechen: Haben wir nicht gleich wie er und eben", "tokens": ["Und", "spre\u00b7chen", ":", "Ha\u00b7ben", "wir", "nicht", "gleich", "wie", "er", "und", "e\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$.", "VAFIN", "PPER", "PTKNEG", "ADV", "KOKOM", "PPER", "KON", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Soviel geschafft? warum hast du ihm mehr gegeben?", "tokens": ["So\u00b7viel", "ge\u00b7schafft", "?", "wa\u00b7rum", "hast", "du", "ihm", "mehr", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVPP", "$.", "PWAV", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da sprach er: Habet ihr zuwenig denn empfangen,", "tokens": ["Da", "sprach", "er", ":", "Ha\u00b7bet", "ihr", "zu\u00b7we\u00b7nig", "denn", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und brach ich einem ab vom Lohn, den wir bedangen?", "tokens": ["Und", "brach", "ich", "ei\u00b7nem", "ab", "vom", "Lohn", ",", "den", "wir", "be\u00b7dan\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "APPR", "APPRART", "NN", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie sprachen: Nein! Er sprach: So nehmt und schweiget still;", "tokens": ["Sie", "spra\u00b7chen", ":", "Nein", "!", "Er", "sprach", ":", "So", "nehmt", "und", "schwei\u00b7get", "still", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PTKANT", "$.", "PPER", "VVFIN", "$.", "ADV", "VVFIN", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Ueberschu\u00df der Gnad' ertheil' ich, wem ich will.", "tokens": ["Den", "Ue\u00b7ber\u00b7schu\u00df", "der", "Gnad'", "ert\u00b7heil'", "ich", ",", "wem", "ich", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}