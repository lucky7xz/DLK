{"textgrid.poem.38366": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Die Abc-Sch\u00fctzen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rathe, was ich habe vernommen,", "tokens": ["Ra\u00b7the", ",", "was", "ich", "ha\u00b7be", "ver\u00b7nom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Es sind achtzehn fremde Gesellen ins Land gekommen,", "tokens": ["Es", "sind", "acht\u00b7zehn", "frem\u00b7de", "Ge\u00b7sel\u00b7len", "ins", "Land", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "ADJA", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Zu mahlen sch\u00f6n und s\u00e4uberlich,", "tokens": ["Zu", "mah\u00b7len", "sch\u00f6n", "und", "s\u00e4u\u00b7ber\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch keiner einem andern glich,", "tokens": ["Doch", "kei\u00b7ner", "ei\u00b7nem", "an\u00b7dern", "glich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "All ohne Fehler und Gebrechen,", "tokens": ["All", "oh\u00b7ne", "Feh\u00b7ler", "und", "Ge\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nur konnte keiner ein Wort sprechen,", "tokens": ["Nur", "konn\u00b7te", "kei\u00b7ner", "ein", "Wort", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und damit man sie sollte verstehn,", "tokens": ["Und", "da\u00b7mit", "man", "sie", "soll\u00b7te", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "PPER", "VMFIN", "VVINF", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Hatten sie f\u00fcnf Dolmetscher mit sich gehn,", "tokens": ["Hat\u00b7ten", "sie", "f\u00fcnf", "Dol\u00b7met\u00b7scher", "mit", "sich", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "CARD", "NN", "APPR", "PRF", "VVINF", "$,"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Das waren hochgelehrte Leut,", "tokens": ["Das", "wa\u00b7ren", "hoch\u00b7ge\u00b7lehr\u00b7te", "Leut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der erst erstaunt, rei\u00dfts Maul auf weit,", "tokens": ["Der", "erst", "er\u00b7staunt", ",", "rei\u00dfts", "Maul", "auf", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "$,", "VVFIN", "NN", "APPR", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Der zweite wie ein Kindlein schreit,", "tokens": ["Der", "zwei\u00b7te", "wie", "ein", "Kin\u00b7dlein", "schreit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "