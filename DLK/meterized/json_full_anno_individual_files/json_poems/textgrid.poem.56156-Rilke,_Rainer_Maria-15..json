{"textgrid.poem.56156": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "15.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Brunnen-Mund, du gebender, du Mund,", "tokens": ["O", "Brun\u00b7nen\u00b7Mund", ",", "du", "ge\u00b7ben\u00b7der", ",", "du", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der unersch\u00f6pflich Eines, Reines, spricht, \u2013", "tokens": ["der", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "Ei\u00b7nes", ",", "Rei\u00b7nes", ",", "spricht", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "ADJD", "PIS", "$,", "NE", "$,", "VVFIN", "$,", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "du, vor des Wassers flie\u00dfendem Gesicht,", "tokens": ["du", ",", "vor", "des", "Was\u00b7sers", "flie\u00b7\u00dfen\u00b7dem", "Ge\u00b7sicht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "marmorne Maske. Und im Hintergrund", "tokens": ["mar\u00b7mor\u00b7ne", "Mas\u00b7ke", ".", "Und", "im", "Hin\u00b7ter\u00b7grund"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "KON", "APPRART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "der Aqu\u00e4dukte Herkunft. Weither an", "tokens": ["der", "A\u00b7qu\u00e4d\u00b7uk\u00b7te", "Her\u00b7kunft", ".", "Weit\u00b7her", "an"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gr\u00e4bern vorbei, vom Hang des Apennins", "tokens": ["Gr\u00e4\u00b7bern", "vor\u00b7bei", ",", "vom", "Hang", "des", "A\u00b7pen\u00b7nins"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "APPRART", "NN", "ART", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "tragen sie dir dein Sagen zu, das dann", "tokens": ["tra\u00b7gen", "sie", "dir", "dein", "Sa\u00b7gen", "zu", ",", "das", "dann"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "PRELS", "ADV"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "am schwarzen Altern deines Kinns", "tokens": ["am", "schwar\u00b7zen", "Al\u00b7tern", "dei\u00b7nes", "Kinns"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "vor\u00fcberf\u00e4llt in das Gef\u00e4\u00df davor.", "tokens": ["vor\u00b7\u00fc\u00b7berf\u00b7\u00e4llt", "in", "das", "Ge\u00b7f\u00e4\u00df", "da\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dies ist das schlafend hingelegte Ohr,", "tokens": ["Dies", "ist", "das", "schla\u00b7fend", "hin\u00b7ge\u00b7leg\u00b7te", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJD", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "das Marmorohr, in das du immer sprichst.", "tokens": ["das", "Mar\u00b7mo\u00b7rohr", ",", "in", "das", "du", "im\u00b7mer", "sprichst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Ohr der Erde. Nur mit sich allein", "tokens": ["Ein", "Ohr", "der", "Er\u00b7de", ".", "Nur", "mit", "sich", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADV", "APPR", "PRF", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "redet sie also. Schiebt ein Krug sich ein,", "tokens": ["re\u00b7det", "sie", "al\u00b7so", ".", "Schiebt", "ein", "Krug", "sich", "ein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$.", "VVFIN", "ART", "NN", "PRF", "PTKVZ", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.3": {"text": "so scheint es ihr, da\u00df du sie unterbrichst.", "tokens": ["so", "scheint", "es", "ihr", ",", "da\u00df", "du", "sie", "un\u00b7ter\u00b7brichst", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O Brunnen-Mund, du gebender, du Mund,", "tokens": ["O", "Brun\u00b7nen\u00b7Mund", ",", "du", "ge\u00b7ben\u00b7der", ",", "du", "Mund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "der unersch\u00f6pflich Eines, Reines, spricht, \u2013", "tokens": ["der", "un\u00b7er\u00b7sch\u00f6pf\u00b7lich", "Ei\u00b7nes", ",", "Rei\u00b7nes", ",", "spricht", ",", "\u20