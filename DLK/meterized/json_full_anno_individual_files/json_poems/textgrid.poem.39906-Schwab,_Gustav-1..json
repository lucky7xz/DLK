{"textgrid.poem.39906": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und wie sollt' ich dein vergessen,", "tokens": ["Und", "wie", "sollt'", "ich", "dein", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VMFIN", "PPER", "PPOSAT", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du getreue Musenstadt,", "tokens": ["Du", "ge\u00b7treu\u00b7e", "Mu\u00b7sen\u00b7stadt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die mein ganzes Herz besessen", "tokens": ["Die", "mein", "gan\u00b7zes", "Herz", "be\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mich wohl gepfleget hat.", "tokens": ["Und", "mich", "wohl", "ge\u00b7pfle\u00b7get", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Von dir singen, von dir sagen", "tokens": ["Von", "dir", "sin\u00b7gen", ",", "von", "dir", "sa\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "$,", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "K\u00f6nnt' ich gar viel Leid und Freud',", "tokens": ["K\u00f6nnt'", "ich", "gar", "viel", "Leid", "und", "Freud'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch, nicht ist's aus fernen Tagen,", "tokens": ["Doch", ",", "nicht", "ist's", "aus", "fer\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PTKNEG", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach! mir ist, als w\u00e4r's erst heut!", "tokens": ["Ach", "!", "mir", "ist", ",", "als", "w\u00e4r's", "erst", "heut", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber heute gieb mir Kunde", "tokens": ["A\u00b7ber", "heu\u00b7te", "gieb", "mir", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVIMP", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief aus deiner alten Zeit,", "tokens": ["Tief", "aus", "dei\u00b7ner", "al\u00b7ten", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als dich von dem schw\u00e4b'schen Bunde", "tokens": ["Als", "dich", "von", "dem", "schw\u00e4b'\u00b7schen", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ulrich, unser Herr, befreit.", "tokens": ["Ul\u00b7rich", ",", "un\u00b7ser", "Herr", ",", "be\u00b7freit", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Zwar er kam in schwerem Zorne,", "tokens": ["Zwar", "er", "kam", "in", "schwe\u00b7rem", "Zor\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlug dir ein dein zagend Schlo\u00df,", "tokens": ["Schlug", "dir", "ein", "dein", "za\u00b7gend", "Schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "PPOSAT", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df die Str\u00e4ucher und die Dorne", "tokens": ["Da\u00df", "die", "Str\u00e4u\u00b7cher", "und", "die", "Dor\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Standen auf den Tr\u00fcmmern blo\u00df.", "tokens": ["Stan\u00b7den", "auf", "den", "Tr\u00fcm\u00b7mern", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch er hat es neu erbauet,", "tokens": ["Doch", "er", "hat", "es", "neu", "er\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stark und f\u00fcrstlich es erh\u00f6ht;", "tokens": ["Stark", "und", "f\u00fcrst\u00b7lich", "es", "er\u00b7h\u00f6ht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Blickt, ihr Enkel, auf und schauet,", "tokens": ["Blickt", ",", "ihr", "En\u00b7kel", ",", "auf", "und", "schau\u00b7et", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie es noch so stattlich steht!", "tokens": ["Wie", "es", "noch", "so", "statt\u00b7lich", "steht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Stolz auf seinem schlanken Renner", "tokens": ["Stolz", "auf", "sei\u00b7nem", "schlan\u00b7ken", "Ren\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ritt der Herzog mitten ein,", "tokens": ["Ritt", "der", "Her\u00b7zog", "mit\u00b7ten", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hoher Rat der weisen M\u00e4nner", "tokens": ["Ho\u00b7her", "Rat", "der", "wei\u00b7sen", "M\u00e4n\u00b7ner"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zog gem\u00e4chlich hinterdrein.", "tokens": ["Zog", "ge\u00b7m\u00e4ch\u00b7lich", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aus den Zellen, aus den Schenken,", "tokens": ["Aus", "den", "Zel\u00b7len", ",", "aus", "den", "Schen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word"