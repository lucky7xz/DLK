{"dta.poem.12694": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen Vater \u00fcber das absterben  \n seiner letzten tochter.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! das scheint fast zu viel! So dencket fleisch und blut,", "tokens": ["Ach", "!", "das", "scheint", "fast", "zu", "viel", "!", "So", "den\u00b7cket", "fleisch", "und", "blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDS", "VVFIN", "ADV", "PTKA", "PIS", "$.", "ADV", "VVFIN", "ADJD", "KON", "NN", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Jtzt, da sein letztes kind, das hertz-blat unter allen,", "tokens": ["Jtzt", ",", "da", "sein", "letz\u00b7tes", "kind", ",", "das", "hertz\u00b7blat", "un\u00b7ter", "al\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "APPR", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der rest von dem geschlecht, ist vollends abgefallen:", "tokens": ["Der", "rest", "von", "dem", "ge\u00b7schlecht", ",", "ist", "vol\u00b7lends", "ab\u00b7ge\u00b7fal\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "VVPP", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er aber lebe nur! so ist es alles gut.", "tokens": ["Er", "a\u00b7ber", "le\u00b7be", "nur", "!", "so", "ist", "es", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$.", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das edle rathhau\u00df wird ihn einen Vater nennen:", "tokens": ["Das", "ed\u00b7le", "rath\u00b7hau\u00df", "wird", "ihn", "ei\u00b7nen", "Va\u00b7ter", "nen\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So kan er diese stadt selbst vor sein kind erkennen.", "tokens": ["So", "kan", "er", "die\u00b7se", "stadt", "selbst", "vor", "sein", "kind", "er\u00b7ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}