{"dta.poem.11305": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die vielen kupffer-bilder.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vor diesem war das hertz der Teutschen stahl und eisen,", "tokens": ["Vor", "die\u00b7sem", "war", "das", "hertz", "der", "Teut\u00b7schen", "stahl", "und", "ei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jtzt l\u00e4st man hertz und kopff in kupffer-bl\u00e4ttern weisen.", "tokens": ["Jtzt", "l\u00e4st", "man", "hertz", "und", "kopff", "in", "kupf\u00b7fer\u00b7bl\u00e4t\u00b7tern", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "KON", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich zweiffle nicht, mein schlu\u00df stimmt mit der wahrheit ein:", "tokens": ["Ich", "zweiff\u00b7le", "nicht", ",", "mein", "schlu\u00df", "stimmt", "mit", "der", "wahr\u00b7heit", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das, was sonst st\u00e4hlern war, mu\u00df itzt papieren seyn.", "tokens": ["Das", ",", "was", "sonst", "st\u00e4h\u00b7lern", "war", ",", "mu\u00df", "itzt", "pa\u00b7pie\u00b7ren", "seyn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,", "VMFIN", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}