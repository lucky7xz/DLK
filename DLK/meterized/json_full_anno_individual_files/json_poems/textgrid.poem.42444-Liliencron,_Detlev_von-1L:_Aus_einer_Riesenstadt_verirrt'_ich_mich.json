{"textgrid.poem.42444": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Aus einer Riesenstadt verirrt' ich mich", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus einer Riesenstadt verirrt' ich mich", "tokens": ["Aus", "ei\u00b7ner", "Rie\u00b7sen\u00b7stadt", "ver\u00b7irrt'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einen weit entlegnen kleinen Bahnhof.", "tokens": ["Auf", "ei\u00b7nen", "weit", "ent\u00b7leg\u00b7nen", "klei\u00b7nen", "Bahn\u00b7hof", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein St\u00e4dtchen wird vielleicht von hier erreicht", "tokens": ["Ein", "St\u00e4dt\u00b7chen", "wird", "viel\u00b7leicht", "von", "hier", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von M\u00e4nnern, die vom Morgen an viel Stunden", "tokens": ["Von", "M\u00e4n\u00b7nern", ",", "die", "vom", "Mor\u00b7gen", "an", "viel", "Stun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "APPRART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Pult, in L\u00e4den und Kanzlei gesessen,", "tokens": ["Am", "Pult", ",", "in", "L\u00e4\u00b7den", "und", "Kanz\u00b7lei", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und nun den Abend im Familienkreise", "tokens": ["Und", "nun", "den", "A\u00b7bend", "im", "Fa\u00b7mi\u00b7li\u00b7en\u00b7krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "APPRART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Den Staub absch\u00fctteln wollen vom \u00bbGesch\u00e4ft.\u00ab", "tokens": ["Den", "Staub", "ab\u00b7sch\u00fct\u00b7teln", "wol\u00b7len", "vom", "\u00bb", "Ge\u00b7sch\u00e4ft", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "APPRART", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein gl\u00fchend hei\u00dfer Sommertag schlo\u00df ab.", "tokens": ["Ein", "gl\u00fc\u00b7hend", "hei\u00b7\u00dfer", "Som\u00b7mer\u00b7tag", "schlo\u00df", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war die Zeit der Mitteld\u00e4mmerung.", "tokens": ["Es", "war", "die", "Zeit", "der", "Mit\u00b7tel\u00b7d\u00e4m\u00b7me\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der neue Mond schob wie ein Komma sich", "tokens": ["Der", "neu\u00b7e", "Mond", "schob", "wie", "ein", "Kom\u00b7ma", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KOKOM", "ART", "NN", "PRF"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Just zwischen zwei bepackte G\u00fcterwagen.", "tokens": ["Just", "zwi\u00b7schen", "zwei", "be\u00b7pack\u00b7te", "G\u00fc\u00b7ter\u00b7wa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Im Westen lag der stumme Abendhimmel", "tokens": ["Im", "Wes\u00b7ten", "lag", "der", "stum\u00b7me", "A\u00b7bend\u00b7him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "In ganz verbla\u00dfter milchiggelber Farbe.", "tokens": ["In", "ganz", "ver\u00b7bla\u00df\u00b7ter", "mil\u00b7chig\u00b7gel\u00b7ber", "Far\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und diesem Himmel stand wie abgeschnitten", "tokens": ["Und", "die\u00b7sem", "Him\u00b7mel", "stand", "wie", "ab\u00b7ge\u00b7schnit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "VVFIN", "KOKOM", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Haufen Schornsteint\u00fcrme vor der Helle.", "tokens": ["Ein", "Hau\u00b7fen", "Schorn\u00b7stein\u00b7t\u00fcr\u00b7me", "vor", "der", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Aus allen Schloten qualmte dicker Rauch,", "tokens": ["Aus", "al\u00b7len", "Schlo\u00b7ten", "qualm\u00b7te", "di\u00b7cker", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Erst grad' zur H\u00f6h', dann wie gebrochen bald,", "tokens": ["Erst", "grad'", "zur", "H\u00f6h'", ",", "dann", "wie", "ge\u00b7bro\u00b7chen", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,", "ADV", "KOKOM", "VVPP", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Beinah' im rechten Winkel, einem Windzug", "tokens": ["Bei\u00b7nah'", "im", "rech\u00b7ten", "Win\u00b7kel", ",", "ei\u00b7nem", "Wind\u00b7zug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Nachgebend, der hier Oberhand gewonnen.", "tokens": ["Nach\u00b7ge\u00b7bend", ",", "der", "hier", "O\u00b7ber\u00b7hand", "ge\u00b7won\u00b7nen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "ADV", "NN", "VVPP", "$."], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.13": {"text": "In wunderlich geformten Oefen dort,", "tokens": ["In", "wun\u00b7der\u00b7lich", "ge\u00b7form\u00b7ten", "O\u00b7e\u00b7fen", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Die offne Stellen zeigten, lohte ruhig,", "tokens": ["Die", "off\u00b7ne", "Stel\u00b7len", "zeig\u00b7ten", ",", "loh\u00b7te", "ru\u00b7hig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ganz ruhig, ohne jeden Flackerzug,", "tokens": ["Ganz", "ru\u00b7hig", ",", "oh\u00b7ne", "je\u00b7den", "Fla\u00b7cker\u00b7zug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUI", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Ein dunkelblauer starker Flammenmantel ...", "tokens": ["Ein", "dun\u00b7kel\u00b7blau\u00b7er", "star\u00b7ker", "Flam\u00b7men\u00b7man\u00b7tel", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Und aus der gro\u00dfen Stadt klang dumpf Ger\u00e4usch,", "tokens": ["Und", "aus", "der", "gro\u00b7\u00dfen", "Stadt", "klang", "dumpf", "Ge\u00b7r\u00e4usch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ein brodelnd Kochen, das ich einmal schon", "tokens": ["Ein", "bro\u00b7delnd", "Ko\u00b7chen", ",", "das", "ich", "ein\u00b7mal", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Geh\u00f6rt, als vor Paris wir Deutschen ruhten,", "tokens": ["Ge\u00b7h\u00f6rt", ",", "als", "vor", "Pa\u00b7ris", "wir", "Deut\u00b7schen", "ruh\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "KOUS", "APPR", "NE", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Indessen drinnen die Kommune sich", "tokens": ["In\u00b7des\u00b7sen", "drin\u00b7nen", "die", "Kom\u00b7mu\u00b7ne", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Im H\u00f6llenl\u00e4rme blutige Wangen wusch.", "tokens": ["Im", "H\u00f6l\u00b7len\u00b7l\u00e4r\u00b7me", "blu\u00b7ti\u00b7ge", "Wan\u00b7gen", "wusch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Das fiel mir ein in diesem Augenblick.", "tokens": ["Das", "fiel", "mir", "ein", "in", "die\u00b7sem", "Au\u00b7gen\u00b7blick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wie auch damals, kam ein Bild von neuem:", "tokens": ["Und", "wie", "auch", "da\u00b7mals", ",", "kam", "ein", "Bild", "von", "neu\u00b7em", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ADV", "$,", "VVFIN", "ART", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Scharf, wie geputztes Messing blank, ergl\u00e4nzte", "tokens": ["Scharf", ",", "wie", "ge\u00b7putz\u00b7tes", "Mes\u00b7sing", "blank", ",", "er\u00b7gl\u00e4nz\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "PWAV", "ADJA", "NN", "PTKVZ", "$,", "VVFIN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Hoch \u00fcber allem Zank der Jupiter.", "tokens": ["Hoch", "\u00fc\u00b7ber", "al\u00b7lem", "Zank", "der", "Ju\u00b7pi\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PIS", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und heut wie einst: Der Jupiter stand oben,", "tokens": ["Und", "heut", "wie", "einst", ":", "Der", "Ju\u00b7pi\u00b7ter", "stand", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADV", "$.", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von allen Sternen er allein zu sehn,", "tokens": ["Von", "al\u00b7len", "Ster\u00b7nen", "er", "al\u00b7lein", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und schaute auf den ewigen Erdenkampf,", "tokens": ["Und", "schau\u00b7te", "auf", "den", "e\u00b7wi\u00b7gen", "Er\u00b7den\u00b7kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Der mir so w\u00fcst in dieser Stunde schien \u2013", "tokens": ["Der", "mir", "so", "w\u00fcst", "in", "die\u00b7ser", "Stun\u00b7de", "schien", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wie bezwungen sprach ich vor mich hin", "tokens": ["Und", "wie", "be\u00b7zwun\u00b7gen", "sprach", "ich", "vor", "mich", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "VVPP", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mit leiser Lippe: Zwanzigstes Jahrhundert.", "tokens": ["Mit", "lei\u00b7ser", "Lip\u00b7pe", ":", "Zwan\u00b7zigs\u00b7tes", "Jahr\u00b7hun\u00b7dert", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-++-++-", "measure": "unknown.measure.hexa"}}, "stanza.4": {"line.1": {"text": "Um mich war's leer; ein letzter Zug hielt fertig,", "tokens": ["Um", "mich", "wa\u00b7r's", "leer", ";", "ein", "letz\u00b7ter", "Zug", "hielt", "fer\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "VAFIN", "ADJD", "$.", "ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die letzten Arbeitsm\u00fcden zu erwarten.", "tokens": ["Die", "letz\u00b7ten", "Ar\u00b7beits\u00b7m\u00fc\u00b7den", "zu", "er\u00b7war\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Bahnbeamter mit knallroter M\u00fctze", "tokens": ["Ein", "Bahn\u00b7be\u00b7am\u00b7ter", "mit", "knall\u00b7ro\u00b7ter", "M\u00fct\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Scho\u00df mir vorbei mit Eilgutformularen.", "tokens": ["Scho\u00df", "mir", "vor\u00b7bei", "mit", "Eil\u00b7gut\u00b7for\u00b7mu\u00b7la\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Sonst nichts \u2013 nur oben stand der Jupiter.", "tokens": ["Sonst", "nichts", "\u2013", "nur", "o\u00b7ben", "stand", "der", "Ju\u00b7pi\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$(", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Die blauen Flammen lohten geisterhaft,", "tokens": ["Die", "blau\u00b7en", "Flam\u00b7men", "loh\u00b7ten", "geis\u00b7ter\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und aus der Stadt her drang verworrner Ton.", "tokens": ["Und", "aus", "der", "Stadt", "her", "drang", "ver\u00b7worr\u00b7ner", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "APZR", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Aus einer Riesenstadt verirrt' ich mich", "tokens": ["Aus", "ei\u00b7ner", "Rie\u00b7sen\u00b7stadt", "ver\u00b7irrt'", "ich", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf einen weit entlegnen kleinen Bahnhof.", "tokens": ["Auf", "ei\u00b7nen", "weit", "ent\u00b7leg\u00b7nen", "klei\u00b7nen", "Bahn\u00b7hof", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein St\u00e4dtchen wird vielleicht von hier erreicht", "tokens": ["Ein", "St\u00e4dt\u00b7chen", "wird", "viel\u00b7leicht", "von", "hier", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Von M\u00e4nnern, die vom Morgen an viel Stunden", "tokens": ["Von", "M\u00e4n\u00b7nern", ",", "die", "vom", "Mor\u00b7gen", "an", "viel", "