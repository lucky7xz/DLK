{"textgrid.poem.47356": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Liebesgedanken", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich von dir, Geliebte! mu\u00dfte wanken,", "tokens": ["Als", "ich", "von", "dir", ",", "Ge\u00b7lieb\u00b7te", "!", "mu\u00df\u00b7te", "wan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,", "NN", "$.", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lie\u00df ich zur\u00fcck die H\u00e4lfte der Gedanken,", "tokens": ["Lie\u00df", "ich", "zu\u00b7r\u00fcck", "die", "H\u00e4lf\u00b7te", "der", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die kleinre H\u00e4lfte nahm ich nur mit mir,", "tokens": ["Die", "klein\u00b7re", "H\u00e4lf\u00b7te", "nahm", "ich", "nur", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die mir's nun gar nicht danken,", "tokens": ["Die", "mir's", "nun", "gar", "nicht", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie nicht sind bei dir.", "tokens": ["Da\u00df", "sie", "nicht", "sind", "bei", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["