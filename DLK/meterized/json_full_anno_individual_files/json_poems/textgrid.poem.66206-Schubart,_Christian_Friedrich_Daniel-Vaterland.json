{"textgrid.poem.66206": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Teutonia, mein gro\u00dfes Vaterland,", "tokens": ["Teu\u00b7to\u00b7nia", ",", "mein", "gro\u00b7\u00dfes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Du Riesentochter, schwingst schon wieder", "tokens": ["Du", "Rie\u00b7sen\u00b7toch\u00b7ter", ",", "schwingst", "schon", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Schlachtgeschosse in der Hand?", "tokens": ["Die", "Schlacht\u00b7ge\u00b7schos\u00b7se", "in", "der", "Hand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Reiz'st Br\u00fcder gegen Br\u00fcder?", "tokens": ["Rei\u00b7z'st", "Br\u00fc\u00b7der", "ge\u00b7gen", "Br\u00fc\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Soll wieder Blut \u2013 ach, deiner S\u00f6hne Blut", "tokens": ["Soll", "wie\u00b7der", "Blut", "\u2013", "ach", ",", "dei\u00b7ner", "S\u00f6h\u00b7ne", "Blut"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "NN", "$(", "XY", "$,", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das Gras, die Wiesenblume f\u00e4rben?", "tokens": ["Das", "Gras", ",", "die", "Wie\u00b7sen\u00b7blu\u00b7me", "f\u00e4r\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Siehst du so gerne, schwarz vor Wuth,", "tokens": ["Siehst", "du", "so", "ger\u00b7ne", ",", "schwarz", "vor", "Wuth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den wunden Krieger sterben?", "tokens": ["Den", "wun\u00b7den", "Krie\u00b7ger", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Schon h\u00f6r' ich's, wie der Brennen blaue Schaar", "tokens": ["Schon", "h\u00f6r'", "ich's", ",", "wie", "der", "Bren\u00b7nen", "blau\u00b7e", "Schaar"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "PWAV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Daher mit Donnerschritten schreitet,", "tokens": ["Da\u00b7her", "mit", "Don\u00b7ner\u00b7schrit\u00b7ten", "schrei\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von ihrem Sch\u00fctzer unsichtbar,", "tokens": ["Von", "ih\u00b7rem", "Sch\u00fct\u00b7zer", "un\u00b7sicht\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von Friedrichs Geist begleitet.", "tokens": ["Von", "Fried\u00b7richs", "Geist", "be\u00b7glei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gleich Meereswogen w\u00e4lzt ein wei\u00dfes Heer", "tokens": ["Gleich", "Mee\u00b7res\u00b7wo\u00b7gen", "w\u00e4lzt", "ein", "wei\u00b7\u00dfes", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den blauen Schaaren sich entgegen.", "tokens": ["Den", "blau\u00b7en", "Schaa\u00b7ren", "sich", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Pulverwetter drohen schwer", "tokens": ["Die", "Pul\u00b7ver\u00b7wet\u00b7ter", "dro\u00b7hen", "schwer"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit m\u00f6rderischen Schl\u00e4gen.", "tokens": ["Mit", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O du, auf den der Blick der Welt gekehrt,", "tokens": ["O", "du", ",", "auf", "den", "der", "Blick", "der", "Welt", "ge\u00b7kehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "APPR", "ART", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O Leopold, der Menschen Freude,", "tokens": ["O", "Leo\u00b7pold", ",", "der", "Men\u00b7schen", "Freu\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sprich zu dem blanken W\u00fcrgeschwert:", "tokens": ["Sprich", "zu", "dem", "blan\u00b7ken", "W\u00fcr\u00b7ge\u00b7schwert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u00bbfahr wieder in die Scheide.\u00ab", "tokens": ["\u00bb", "fahr", "wie\u00b7der", "in", "die", "Schei\u00b7de", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Auch Friedrich Wilhelm ist dem Frieden hold,", "tokens": ["Auch", "Fried\u00b7rich", "Wil\u00b7helm", "ist", "dem", "Frie\u00b7den", "hold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "NE", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Feind vom m\u00f6rderischen W\u00fcthen.", "tokens": ["Ein", "Feind", "vom", "m\u00f6r\u00b7de\u00b7ri\u00b7schen", "W\u00fct\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er wird die Rechte, Leopold,", "tokens": ["Er", "wird", "die", "Rech\u00b7te", ",", "Leo\u00b7pold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dir gern, als Bruder, bieten.", "tokens": ["Dir", "gern", ",", "als", "Bru\u00b7der", ",", "bie\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es ist so sch\u00f6n, so himmlisch k\u00f6niglich,", "tokens": ["Es", "ist", "so", "sch\u00f6n", ",", "so", "himm\u00b7lisch", "k\u00f6\u00b7nig\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Im Frieden seinen Thron besteigen,", "tokens": ["Im", "Frie\u00b7den", "sei\u00b7nen", "Thron", "be\u00b7stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und allen Erdenv\u00f6lkern sich", "tokens": ["Und", "al\u00b7len", "Er\u00b7den\u00b7v\u00f6l\u00b7kern", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als Freund der Menschheit zeigen.", "tokens": ["Als", "Freund", "der", "Menschheit", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Der Aufruhr, den die H\u00f6ll' heraufgesandt,", "tokens": ["Der", "Auf\u00b7ruhr", ",", "den", "die", "H\u00f6ll'", "her\u00b7auf\u00b7ge\u00b7sandt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Nationen zu erregen,", "tokens": ["Die", "Na\u00b7ti\u00b7o\u00b7nen", "zu", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird bald aus hochgehobner Hand", "tokens": ["Wird", "bald", "aus", "hoch\u00b7ge\u00b7hob\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Flammengei\u00dfel legen.", "tokens": ["Die", "Flam\u00b7men\u00b7gei\u00b7\u00dfel", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Doch soll des Krieges Wetterwolkennacht,", "tokens": ["Doch", "soll", "des", "Krie\u00b7ges", "Wet\u00b7ter\u00b7wol\u00b7ken\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Teutonia, die Welt best\u00fcrmen,", "tokens": ["Teu\u00b7to\u00b7nia", ",", "die", "Welt", "be\u00b7st\u00fcr\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "So droh mit aller deiner Macht", "tokens": ["So", "droh", "mit", "al\u00b7ler", "dei\u00b7ner", "Macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den stolzen sieben Th\u00fcrmen.", "tokens": ["Den", "stol\u00b7zen", "sie\u00b7ben", "Th\u00fcr\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nur m\u00f6ge nie des Deutschen starke Hand", "tokens": ["Nur", "m\u00f6\u00b7ge", "nie", "des", "Deut\u00b7schen", "star\u00b7ke", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ADV", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Bruder bei der Kehle packen,", "tokens": ["Den", "Bru\u00b7der", "bei", "der", "Keh\u00b7le", "pa\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch schickt sich besser Mord und Brand", "tokens": ["Auch", "schickt", "sich", "bes\u00b7ser", "Mord", "und", "Brand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr thierische Kosaken.", "tokens": ["F\u00fcr", "thie\u00b7ri\u00b7sche", "Ko\u00b7sa\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Im Friedenslispel schalle bald der Ruf:", "tokens": ["Im", "Frie\u00b7dens\u00b7li\u00b7spel", "schal\u00b7le", "bald", "der", "Ruf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kaiser sitzt auf deutschem Throne!", "tokens": ["Ein", "Kai\u00b7ser", "sitzt", "auf", "deut\u00b7schem", "Thro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Gott zu unsrem Haupte ", "tokens": ["Den", "Gott", "zu", "uns\u00b7rem",