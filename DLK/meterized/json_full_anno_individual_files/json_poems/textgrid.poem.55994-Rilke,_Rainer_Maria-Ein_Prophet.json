{"textgrid.poem.55994": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Ein Prophet", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ausgedehnt von riesigen Gesichten,", "tokens": ["Aus\u00b7ge\u00b7dehnt", "von", "rie\u00b7si\u00b7gen", "Ge\u00b7sich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "hell vom Feuerschein aus dem Verlauf", "tokens": ["hell", "vom", "Feu\u00b7er\u00b7schein", "aus", "dem", "Ver\u00b7lauf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPRART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "der Gerichte, die ihn nie vernichten, \u2013", "tokens": ["der", "Ge\u00b7rich\u00b7te", ",", "die", "ihn", "nie", "ver\u00b7nich\u00b7ten", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVINF", "$,", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "sind die Augen, schauend unter dichten", "tokens": ["sind", "die", "Au\u00b7gen", ",", "schau\u00b7end", "un\u00b7ter", "dich\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "ADJD", "APPR", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Brauen. Und in seinem Innern richten", "tokens": ["Brau\u00b7en", ".", "Und", "in", "sei\u00b7nem", "In\u00b7nern", "rich\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "li