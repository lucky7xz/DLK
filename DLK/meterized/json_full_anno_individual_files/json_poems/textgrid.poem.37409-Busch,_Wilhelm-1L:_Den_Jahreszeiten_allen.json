{"textgrid.poem.37409": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den Jahreszeiten allen", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den Jahreszeiten allen", "tokens": ["Den", "Jah\u00b7res\u00b7zei\u00b7ten", "al\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Selbviert sei Preis und Ehr!", "tokens": ["Selb\u00b7viert", "sei", "Preis", "und", "Ehr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nur sag ich: Mir gefallen", "tokens": ["Nur", "sag", "ich", ":", "Mir", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NE", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sie minder oder mehr.", "tokens": ["Sie", "min\u00b7der", "o\u00b7der", "mehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Fr\u00fchling wird ja immer", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wird", "ja", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ger\u00fchmt, wie sich's geb\u00fchrt,", "tokens": ["Ge\u00b7r\u00fchmt", ",", "wie", "sich's", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Weil er mit gr\u00fcnem Schimmer", "tokens": ["Weil", "er", "mit", "gr\u00fc\u00b7nem", "Schim\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die graue Welt verziert.", "tokens": ["Die", "grau\u00b7e", "Welt", "ver\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch hat in unsrer Zone", "tokens": ["Doch", "hat", "in", "uns\u00b7rer", "Zo\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er durch den Reif der Nacht", "tokens": ["Er", "durch", "den", "Reif", "der", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Schon manche gr\u00fcne Bohne", "tokens": ["Schon", "man\u00b7che", "gr\u00fc\u00b7ne", "Boh\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und Gurke umgebracht.", "tokens": ["Und", "Gur\u00b7ke", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Stets wird auch Ruhm erwerben", "tokens": ["Stets", "wird", "auch", "Ruhm", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Herbst, vorausgesetzt,", "tokens": ["Der", "Herbst", ",", "vor\u00b7aus\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df er mit vollen K\u00f6rben", "tokens": ["Da\u00df", "er", "mit", "vol\u00b7len", "K\u00f6r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Uns Aug und Mund erg\u00f6tzt.", "tokens": ["Uns", "Aug", "und", "Mund", "er\u00b7g\u00f6tzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Indes durch leises Tupfen", "tokens": ["In\u00b7des", "durch", "lei\u00b7ses", "Tup\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Gemahnt er uns bereits:", "tokens": ["Ge\u00b7mahnt", "er", "uns", "be\u00b7reits", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bald, Kinder, kommt der Schnupfen", "tokens": ["Bald", ",", "Kin\u00b7der", ",", "kommt", "der", "Schnup\u00b7fen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und 's Gripperl seinerseits.", "tokens": ["Und", "'s", "Grip\u00b7perl", "sei\u00b7ner\u00b7seits", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Winter kommt. Es blasen", "tokens": ["Der", "Win\u00b7ter", "kommt", ".", "Es", "bla\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Winde scharf und k\u00fchl;", "tokens": ["Die", "Win\u00b7de", "scharf", "und", "k\u00fchl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rot werden alle Nasen,", "tokens": ["Rot", "wer\u00b7den", "al\u00b7le", "Na\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und Kohlen braucht man viel.", "tokens": ["Und", "Koh\u00b7len", "braucht", "man", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PIS", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Nein, mir gef\u00e4llt am besten", "tokens": ["Nein", ",", "mir", "ge\u00b7f\u00e4llt", "am", "bes\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VVPP", "APPRART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das, was der Sommer bringt,", "tokens": ["Das", ",", "was", "der", "Som\u00b7mer", "bringt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn auf belaubten \u00c4sten", "tokens": ["Wenn", "auf", "be\u00b7laub\u00b7ten", "\u00c4s\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schar der V\u00f6glein singt.", "tokens": ["Die", "Schar", "der", "V\u00f6\u00b7glein", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wenn Rosen, zahm und wilde,", "tokens": ["Wenn", "Ro\u00b7sen", ",", "zahm", "und", "wil\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In vollster Bl\u00fcte stehn,", "tokens": ["In", "volls\u00b7ter", "Bl\u00fc\u00b7te", "stehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn \u00fcber Lustgefilde", "tokens": ["Wenn", "\u00fc\u00b7ber", "Lust\u00b7ge\u00b7fil\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zephire kosend wehn.", "tokens": ["Ze\u00b7phi\u00b7re", "ko\u00b7send", "wehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$."], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Und wollt' mich einer fragen,", "tokens": ["Und", "wollt'", "mich", "ei\u00b7ner", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann's mir im Sommer dann", "tokens": ["Wann's", "mir", "im", "Som\u00b7mer", "dann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Besonders t\u00e4t behagen,", "tokens": ["Be\u00b7son\u00b7ders", "t\u00e4t", "be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Den Juni g\u00e4b ich an.", "tokens": ["Den", "Ju\u00b7ni", "g\u00e4b", "ich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und wieder dann darunter", "tokens": ["Und", "wie\u00b7der", "dann", "da\u00b7run\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "PAV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den selben Tag gerad,", "tokens": ["Den", "sel\u00b7ben", "Tag", "ge\u00b7rad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo einst ein Kindlein munter", "tokens": ["Wo", "einst", "ein", "Kin\u00b7dlein", "mun\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zuerst zutage trat.", "tokens": ["Zu\u00b7erst", "zu\u00b7ta\u00b7ge", "trat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drum flattert dies Gedichtchen", "tokens": ["Drum", "flat\u00b7tert", "dies", "Ge\u00b7dicht\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PDS", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Jetzt \u00fcber Berg und Tal", "tokens": ["Jetzt", "\u00fc\u00b7ber", "Berg", "und", "Tal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und gr\u00fc\u00dft das liebe Nichtchen", "tokens": ["Und", "gr\u00fc\u00dft", "das", "lie\u00b7be", "Nicht\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"},