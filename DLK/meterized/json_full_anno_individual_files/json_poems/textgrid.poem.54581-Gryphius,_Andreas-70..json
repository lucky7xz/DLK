{"textgrid.poem.54581": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "70.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Man f\u00e4ngt das Neue Jahr mit Wunsch und Gaben an/", "tokens": ["Man", "f\u00e4ngt", "das", "Neu\u00b7e", "Jahr", "mit", "Wunsch", "und", "Ga\u00b7ben", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mein Hertz/ ihr hab ich selbst zu eigen mich gegeben/", "tokens": ["Mein", "Hertz", "/", "ihr", "hab", "ich", "selbst", "zu", "ei\u00b7gen", "mich", "ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und bin nicht weiter frey/ mein ihr verpflichtet Leben", "tokens": ["Und", "bin", "nicht", "wei\u00b7ter", "frey", "/", "mein", "ihr", "ver\u00b7pflich\u00b7tet", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "ADJD", "$(", "PPOSAT", "PPER", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat nichts/ zu dem Sie nicht schon Anspruch haben kan.", "tokens": ["Hat", "nichts", "/", "zu", "dem", "Sie", "nicht", "schon", "An\u00b7spruch", "ha\u00b7ben", "kan", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$(", "APPR", 