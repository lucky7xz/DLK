{"textgrid.poem.42039": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Abenteuer", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun hat der weite Weg mich ganz verloren,", "tokens": ["Nun", "hat", "der", "wei\u00b7te", "Weg", "mich", "ganz", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie floh mich Anfang, Ende und die Mitte!", "tokens": ["Wie", "floh", "mich", "An\u00b7fang", ",", "En\u00b7de", "und", "die", "Mit\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin in einem tiefen Kreis geboren,", "tokens": ["Ich", "bin", "in", "ei\u00b7nem", "tie\u00b7fen", "Kreis", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich h\u00f6re meine leichten Schritte", "tokens": ["Ich", "h\u00f6\u00b7re", "mei\u00b7ne", "leich\u00b7ten", "Schrit\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "In einem fernen Lande widerhallen,", "tokens": ["In", "ei\u00b7nem", "fer\u00b7nen", "Lan\u00b7de", "wi\u00b7der\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich h\u00f6re meine leisen Worte", "tokens": ["Ich", "h\u00f6\u00b7re", "mei\u00b7ne", "lei\u00b7sen", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "In eine dunkle Stille niederfallen,", "tokens": ["In", "ei\u00b7ne", "dunk\u00b7le", "Stil\u00b7le", "nie\u00b7der\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ich schreite durch viel fremde Orte,", "tokens": ["Ich", "schrei\u00b7te", "durch", "viel", "frem\u00b7de", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "F\u00fchl mich gehalten, stille, wie zu Haus,", "tokens": ["F\u00fchl", "mich", "ge\u00b7hal\u00b7ten", ",", "stil\u00b7le", ",", "wie", "zu", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVPP", "$,", "VVFIN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und mu\u00df doch gehn, und gehe wie f\u00fcr immer,", "tokens": ["Und", "mu\u00df", "doch", "gehn", ",", "und", "ge\u00b7he", "wie", "f\u00fcr", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "VVINF", "$,", "KON", "VVFIN", "KOKOM", "APPR", "ADV", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Und schau nach meiner R\u00fcckkehr l\u00e4chelnd aus \u2013", "tokens": ["Und", "schau", "nach", "mei\u00b7ner", "R\u00fcck\u00b7kehr", "l\u00e4\u00b7chelnd", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPOSAT", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ich wei\u00df so viel: Ich kenne schon das Zimmer,", "tokens": ["Ich", "wei\u00df", "so", "viel", ":", "Ich", "ken\u00b7ne", "schon", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der blauen Ampel s\u00fc\u00df gestilltes Licht,", "tokens": ["Der", "blau\u00b7en", "Am\u00b7pel", "s\u00fc\u00df", "ge\u00b7still\u00b7tes", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Ich h\u00f6rte schon vor tausend Jahren diese Stimme,", "tokens": ["Ich", "h\u00f6r\u00b7te", "schon", "vor", "tau\u00b7send", "Jah\u00b7ren", "die\u00b7se", "Stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "CARD", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wie sie mir zitternd das Willkommen spricht.", "tokens": ["Wie", "sie", "mir", "zit\u00b7ternd", "das", "Will\u00b7kom\u00b7men", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun hat der weite Weg mich ganz verloren,", "tokens": ["Nun", "hat", "der", "wei\u00b7te", "We