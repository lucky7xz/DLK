{"textgrid.poem.61134": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Das M\u00e4dchen von Paros", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Denkst du des Abends noch, des hellen,", "tokens": ["Denkst", "du", "des", "A\u00b7bends", "noch", ",", "des", "hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da mich der Winde leiser Zug", "tokens": ["Da", "mich", "der", "Win\u00b7de", "lei\u00b7ser", "Zug"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sanft \u00fcber die entschlafnen Wellen", "tokens": ["Sanft", "\u00fc\u00b7ber", "die", "ent\u00b7schlaf\u00b7nen", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An diese stille K\u00fcste trug?", "tokens": ["An", "die\u00b7se", "stil\u00b7le", "K\u00fcs\u00b7te", "trug", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da ich, erm\u00fcdet vom Gew\u00fchle,", "tokens": ["Da", "ich", ",", "er\u00b7m\u00fc\u00b7det", "vom", "Ge\u00b7w\u00fch\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das drau\u00dfen toset fr\u00fch und spat,", "tokens": ["Das", "drau\