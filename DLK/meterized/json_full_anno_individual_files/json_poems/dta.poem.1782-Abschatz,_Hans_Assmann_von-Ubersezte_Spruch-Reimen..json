{"dta.poem.1782": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "S\u00fcsser Freunde Gegenwart dient zu Felde vor den Wagen/", "tokens": ["S\u00fcs\u00b7ser", "Freun\u00b7de", "Ge\u00b7gen\u00b7wart", "dient", "zu", "Fel\u00b7de", "vor", "den", "Wa\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Ist in St\u00e4dten unsre Lust/ unser bestes Wohlbehagen.", "tokens": ["Ist", "in", "St\u00e4d\u00b7ten", "uns\u00b7re", "Lust", "/", "un\u00b7ser", "bes\u00b7tes", "Wohl\u00b7be\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "PPOSAT", "NN", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Stehet uns auff Gastereyen vor den theuren G\u00f6tter-Wein/", "tokens": ["Ste\u00b7het", "uns", "auff", "Gas\u00b7te\u00b7re\u00b7yen", "vor", "den", "theu\u00b7ren", "G\u00f6t\u00b7ter\u00b7Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Kan gewisser Trost in Schmertzen/ Linderung in Trauren", "tokens": ["Kan", "ge\u00b7wis\u00b7ser", "Trost", "in", "Schmert\u00b7zen", "/", "Lin\u00b7de\u00b7rung", "in", "Trau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADJA", "NN", "APPR", "NN", "$(", "NN", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "seyn.", "tokens": ["seyn", "."], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+", "measure": "single.up"}}}}}