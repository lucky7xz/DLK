{"dta.poem.20921": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Judith an Balduin.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Kan Balduin denn noch der Judith nicht", "tokens": ["Kan", "Bal\u00b7du\u00b7in", "denn", "noch", "der", "Ju\u00b7dith", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "NE", "KON", "ADV", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "vergessen/", "tokens": ["ver\u00b7ges\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Und streicht sein alter Wunsch auch endlich", "tokens": ["Und", "streicht", "sein", "al\u00b7ter", "Wunsch", "auch", "end\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "\u00fcber See!", "tokens": ["\u00fc\u00b7ber", "See", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Du bist in frembder Lufft und weit von mir gesessen/", "tokens": ["Du", "bist", "in", "fremb\u00b7der", "Lufft", "und", "weit", "von", "mir", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "KON", "ADJD", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch st\u00f6hrt dein k\u00fchner Brieff mein heisses Ach und", "tokens": ["Doch", "st\u00f6hrt", "dein", "k\u00fch\u00b7ner", "Brieff", "mein", "heis\u00b7ses", "Ach", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Weh.", "tokens": ["Weh", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Wer bey der Leich", "tokens": ["Wer", "bey", "der", "Leich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Es schickt sich Alo", "tokens": ["Es", "schickt", "sich", "A\u00b7lo"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NE"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Es solte deine Brunst nicht meine Seuffzer st\u00f6ren/", "tokens": ["Es", "sol\u00b7te", "dei\u00b7ne", "Brunst", "nicht", "mei\u00b7ne", "Seuff\u00b7zer", "st\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Noch deiner K\u00fchnheit Trieb verr\u00fccken meine Pflicht.", "tokens": ["Noch", "dei\u00b7ner", "K\u00fchn\u00b7heit", "Trieb", "ver\u00b7r\u00fc\u00b7cken", "mei\u00b7ne", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ich lasse mich itzund mit Trauerflor \u00fcmschl\u00fcssen/", "tokens": ["Ich", "las\u00b7se", "mich", "it\u00b7zund", "mit", "Trau\u00b7er\u00b7flor", "\u00fcm\u00b7schl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So streut dein kecker Geist verliebte Blumen aus/", "tokens": ["So", "streut", "dein", "ke\u00b7cker", "Geist", "ver\u00b7lieb\u00b7te", "Blu\u00b7men", "aus", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "VVFIN", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und weil mein Auge l\u00e4\u00dft die Wasserperlen fl\u00fcssen/", "tokens": ["Und", "weil", "mein", "Au\u00b7ge", "l\u00e4\u00dft", "die", "Was\u00b7ser\u00b7per\u00b7len", "fl\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "VVFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "So dringt dein freyer Schertz in mein betr\u00fcbtes Hau\u00df.", "tokens": ["So", "dringt", "dein", "frey\u00b7er", "Schertz", "in", "mein", "be\u00b7tr\u00fcb\u00b7tes", "Hau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Kein Freuden Pflaster dient vor die gekr\u00e4nckten Hertzen/", "tokens": ["Kein", "Freu\u00b7den", "Pflas\u00b7ter", "dient", "vor", "die", "ge\u00b7kr\u00e4nck\u00b7ten", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die Wehmuth st\u00fcnde dir am allerbesten an/ ", "tokens": ["Die", "Weh\u00b7muth", "st\u00fcn\u00b7de", "dir", "am", "al\u00b7ler\u00b7bes\u00b7ten", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein Freund/ d\u2019 sich ergetzt bey seiner Freundin Schmer-", "tokens": ["Ein", "Freund", "/", "d'", "sich", "er\u00b7getzt", "bey", "sei\u00b7ner", "Freun\u00b7din", "Schmer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "NE", "PRF", "VVPP", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Hat zwar nach seiner Lust/ doch nicht nach Pflicht ge-", "tokens": ["Hat", "zwar", "nach", "sei\u00b7ner", "Lust", "/", "doch", "nicht", "nach", "Pflicht", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$(", "ADV", "PTKNEG", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "than", "tokens": ["than"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "So geht es Balduin; was liebst du? deine L\u00fcste;", "tokens": ["So", "geht", "es", "Bal\u00b7du\u00b7in", ";", "was", "liebst", "du", "?", "dei\u00b7ne", "L\u00fcs\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "PWS", "VVFIN", "PPER", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mein Ruhm/ ja ich dazu/ mag bleiben wo ich will/", "tokens": ["Mein", "Ruhm", "/", "ja", "ich", "da\u00b7zu", "/", "mag", "blei\u00b7ben", "wo", "ich", "will", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "PPER", "PAV", "$(", "VMFIN", "VVINF", "PWAV", "PPER", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du suchst ein Freuden Feld und l\u00e4st mich in der W\u00fcste/", "tokens": ["Du", "suchst", "ein", "Freu\u00b7den", "Feld", "und", "l\u00e4st", "mich", "in", "der", "W\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die\u00df/ was Vergn\u00fcgung heist/ ist dein erwehltes Ziel.", "tokens": ["Die\u00df", "/", "was", "Ver\u00b7gn\u00fc\u00b7gung", "heist", "/", "ist", "dein", "er\u00b7wehl\u00b7tes", "Ziel", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$(", "PWS", "NN", "VVFIN", "$(", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du lachest/ ob die Welt auf meinen Nahmen fluchet/", "tokens": ["Du", "la\u00b7chest", "/", "ob", "die", "Welt", "auf", "mei\u00b7nen", "Nah\u00b7men", "flu\u00b7chet", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Du schaust auf deine Lust/ nicht meinen Ehren Ruhm/", "tokens": ["Du", "schaust", "auf", "dei\u00b7ne", "Lust", "/", "nicht", "mei\u00b7nen", "Eh\u00b7ren", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$(", "PTKNEG", "PPOSAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Wann Balduin erhitzt die Freuden Rosen suchet/", "tokens": ["Wann", "Bal\u00b7du\u00b7in", "er\u00b7hitzt", "die", "Freu\u00b7den", "Ro\u00b7sen", "su\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So meint er/ Lust und Leid sey gleiches Eigenthum.", "tokens": ["So", "meint", "er", "/", "Lust", "und", "Leid", "sey", "glei\u00b7ches", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$(", "NN", "KON", "NN", "VAFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Darff ich die Warheit hier mit rechten Nahmen nen-", "tokens": ["Darff", "ich", "die", "War\u00b7heit", "hier", "mit", "rech\u00b7ten", "Nah\u00b7men", "nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "(doch dieses stehet mir bey meinem trauren frey/) (nen/", "tokens": ["(", "doch", "die\u00b7ses", "ste\u00b7het", "mir", "bey", "mei\u00b7nem", "trau\u00b7ren", "frey", "/", ")", "(", "nen", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct"], "pos": ["$(", "KON", "PDS", "VVFIN", "PPER", "APPR", "PPOSAT", "VVFIN", "ADJD", "$(", "$(", "$(", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So mu\u00df ich nur f\u00fcr dir und aller Welt bekennen/", "tokens": ["So", "mu\u00df", "ich", "nur", "f\u00fcr", "dir", "und", "al\u00b7ler", "Welt", "be\u00b7ken\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPER", "KON", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df auf der M\u00e4nner Wort nicht viel zubauen sey.", "tokens": ["Da\u00df", "auf", "der", "M\u00e4n\u00b7ner", "Wort", "nicht", "viel", "zu\u00b7bau\u00b7en", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PTKNEG", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Was liebt ihr? euch/ nicht uns; ihr spielt mit Schwu", "tokens": ["Was", "liebt", "ihr", "?", "euch", "/", "nicht", "uns", ";", "ihr", "spielt", "mit", "Schwu"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPER", "$(", "PTKNEG", "PPER", "$.", "PPER", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.34": {"text": "und Eyde/", "tokens": ["und", "Ey\u00b7de", "/"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.35": {"text": "Und sucht durch Falschheit Wind den Hafen euer Lust/", "tokens": ["Und", "sucht", "durch", "Falschheit", "Wind", "den", "Ha\u00b7fen", "eu\u00b7er", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.36": {"text": "Jhr kleidet euer Wort in schwanenweisse Seyde/", "tokens": ["Ihr", "klei\u00b7det", "eu\u00b7er", "Wort", "in", "schwa\u00b7nen\u00b7weis\u00b7se", "Sey\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "In dem der Bo\u00dfheit Ru\u00df erf\u00fcllet eure Brust.", "tokens": ["In", "dem", "der", "Bo\u00df\u00b7heit", "Ru\u00df", "er\u00b7f\u00fcl\u00b7let", "eu\u00b7re", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Jhr w\u00fcnscht das Gottes Zorn euch schleunig soll ver-", "tokens": ["Ihr", "w\u00fcnscht", "das", "Got\u00b7tes", "Zorn", "euch", "schleu\u00b7nig", "soll", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PPER", "ADJD", "VMFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "zehren/", "tokens": ["zeh\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Dafern ein Tropfen List verg\u00e4llet euren Sinn/", "tokens": ["Da\u00b7fern", "ein", "Trop\u00b7fen", "List", "ver\u00b7g\u00e4l\u00b7let", "eu\u00b7ren", "Sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NE", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und gebet da und dort vertiefft in solchen Schweren", "tokens": ["Und", "ge\u00b7bet", "da", "und", "dort", "ver\u00b7tiefft", "in", "sol\u00b7chen", "Schwe\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Vor einen halben Ku\u00df den gantzen Himmel hin.", "tokens": ["Vor", "ei\u00b7nen", "hal\u00b7ben", "Ku\u00df", "den", "gant\u00b7zen", "Him\u00b7mel", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Jhr bauet mit Gefahr auf unsers Ruhmes Grunde/", "tokens": ["Ihr", "bau\u00b7et", "mit", "Ge\u00b7fahr", "auf", "un\u00b7sers", "Ruh\u00b7mes", "Grun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Der oftmals sehr beschwert in tausend St\u00fccken bricht/", "tokens": ["Der", "oft\u00b7mals", "sehr", "be\u00b7schwert", "in", "tau\u00b7send", "St\u00fc\u00b7cken", "bricht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADJD", "APPR", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Jhr blaset falschen Dunst aus eurem geilen Munde", "tokens": ["Ihr", "bla\u00b7set", "fal\u00b7schen", "Dunst", "aus", "eu\u00b7rem", "gei\u00b7len", "Mun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Und schont in eurer Gluth der reinsten Seelen 