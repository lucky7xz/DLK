{"dta.poem.15799": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Tadler ehr' ich, der die Richtigkeit des Zieles", "tokens": ["Den", "Tad\u00b7ler", "ehr'", "ich", ",", "der", "die", "Rich\u00b7tig\u00b7keit", "des", "Zie\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mir zugibt, fehle gleich noch zur Erreichung vieles.", "tokens": ["Mir", "zu\u00b7gibt", ",", "feh\u00b7le", "gleich", "noch", "zur", "Er\u00b7rei\u00b7chung", "vie\u00b7les", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "ADV", "APPRART", "NN", "PIS", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn der Tadler gar mir kann die Wege weisen,", "tokens": ["Und", "wenn", "der", "Tad\u00b7ler", "gar", "mir", "kann", "die", "We\u00b7ge", "wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie's zu erreichen sei, dann will ich erst ihn preisen.", "tokens": ["Wie's", "zu", "er\u00b7rei\u00b7chen", "sei", ",", "dann", "will", "ich", "erst", "ihn", "prei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "VAFIN", "$,