{"dta.poem.7050": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mit welcher Wunder-Kunst und Pracht,", "tokens": ["Mit", "wel\u00b7cher", "Wun\u00b7der\u00b7Kunst", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der unausdr\u00fccklichsten Vollkommenheit,", "tokens": ["Der", "un\u00b7aus\u00b7dr\u00fcck\u00b7lichs\u00b7ten", "Voll\u00b7kom\u00b7men\u00b7heit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Mu\u00df unsers Sch\u00f6pffers Thron gemacht,", "tokens": ["Mu\u00df", "un\u00b7sers", "Sch\u00f6pf\u00b7fers", "Thron", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit welchem seel\u2019gen Schein,", "tokens": ["Mit", "wel\u00b7chem", "seel'\u00b7gen", "Schein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit welchem Wunder-Glantz erf\u00fcllet seyn!", "tokens": ["Mit", "wel\u00b7chem", "Wun\u00b7der\u00b7Glantz", "er\u00b7f\u00fcl\u00b7let", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}