rd", "punct"], "pos": ["ADV", "NE", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Schneewei\u00df, wie die Nu\u00df im Kerne;", "tokens": ["Schnee\u00b7wei\u00df", ",", "wie", "die", "Nu\u00df", "im", "Ker\u00b7ne", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Montreux' graue D\u00e4cher dort,", "tokens": ["Mon\u00b7treux'", "grau\u00b7e", "D\u00e4\u00b7cher", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Chillons Zwinger in der Ferne.", "tokens": ["Chil\u00b7lons", "Zwin\u00b7ger", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Meiner Sehnsucht Traumgebiet,", "tokens": ["Mei\u00b7ner", "Sehn\u00b7sucht", "Traum\u00b7ge\u00b7biet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegst du vor mir duftumschleiert?", "tokens": ["Liegst", "du", "vor", "mir", "duf\u00b7tumsc\u00b7hlei\u00b7ert", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zauberwelt, in Sag' und Lied", "tokens": ["Zau\u00b7ber\u00b7welt", ",", "in", "Sag'", "und", "Lied"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Von Unsterblichen gefeiert!", "tokens": ["Von", "U\u00b7nsterb\u00b7li\u00b7chen", "ge\u00b7fei\u00b7ert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Doch indes ich schau' entz\u00fcckt,", "tokens": ["Doch", "in\u00b7des", "ich", "schau'", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie die H\u00f6hn mit Gold sich kr\u00f6nen,", "tokens": ["Wie", "die", "H\u00f6hn", "mit", "Gold", "sich", "kr\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sitzen vor sich hin geb\u00fcckt", "tokens": ["Sit\u00b7zen", "vor", "sich", "hin", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwei von Albions blonden S\u00f6hnen.", "tokens": ["Zwei", "von", "Al\u00b7bions", "blon\u00b7den", "S\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Ihren Murray sehr vertieft", "tokens": ["Ih\u00b7ren", "Mur\u00b7ray", "sehr", "ver\u00b7tieft"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben sie zur Hand genommen,", "tokens": ["Ha\u00b7ben", "sie", "zur", "Hand", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob er's ihnen auch verbrieft,", "tokens": ["Ob", "er's", "ih\u00b7nen", "auch", "ver\u00b7brieft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut in Bern noch anzukommen.", "tokens": ["Heut", "in", "Bern", "noch", "an\u00b7zu\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "ADV", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Still emp\u00f6rt wend' ich mich ab,", "tokens": ["Still", "em\u00b7p\u00f6rt", "wend'", "ich", "mich", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und auf einmal mu\u00df ich lachen:", "tokens": ["Und", "auf", "ein\u00b7mal", "mu\u00df", "ich", "la\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "VMFIN", "PPER", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Pflegen wir's bis an das Grab", "tokens": ["Pfle\u00b7gen", "wir's", "bis", "an", "das", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kl\u00fcger mit dem Gl\u00fcck zu machen?", "tokens": ["Kl\u00fc\u00b7ger", "mit", "dem", "Gl\u00fcck", "zu", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Hast du nie der Gegenwart", "tokens":