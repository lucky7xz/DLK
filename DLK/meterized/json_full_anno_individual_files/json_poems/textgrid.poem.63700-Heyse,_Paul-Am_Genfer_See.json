{"textgrid.poem.63700": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Am Genfer See", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Abendlich vergl\u00fchen still", "tokens": ["A\u00b7bend\u00b7lich", "ver\u00b7gl\u00fc\u00b7hen", "still"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVINF", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort die Berge von Savoyen.", "tokens": ["Dort", "die", "Ber\u00b7ge", "von", "Sa\u00b7vo\u00b7yen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sch\u00f6ner See, noch einmal will", "tokens": ["Sch\u00f6\u00b7ner", "See", ",", "noch", "ein\u00b7mal", "will"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADV", "ADV", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich an dir mein Herz erfreuen.", "tokens": ["Ich", "an", "dir", "mein", "Herz", "er\u00b7freu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4hrend sacht der Bahnzug f\u00e4hrt", "tokens": ["W\u00e4h\u00b7rend", "sacht", "der", "Bahn\u00b7zug", "f\u00e4hrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf der H\u00f6he von Lausanne,", "tokens": ["Auf", "der", "H\u00f6\u00b7he", "von", "Lau\u00b7san\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR"