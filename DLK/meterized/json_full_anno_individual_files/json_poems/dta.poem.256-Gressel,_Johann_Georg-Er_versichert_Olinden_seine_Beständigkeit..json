{"dta.poem.256": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er versichert  Olinden  seine Best\u00e4ndigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "So wie ein Fels bey Wind-undWind- und Meeres-St\u00fcrmen", "tokens": ["So", "wie", "ein", "Fels", "bey", "Win\u00b7d\u00b7und", "Win\u00b7d", "und", "Mee\u00b7res\u00b7St\u00fcr\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "APPR", "NN", "TRUNC", "KON", "NN"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Durch eigne Krafft sich selbsten pflegt zu schirmen/", "tokens": ["Durch", "eig\u00b7ne", "Krafft", "sich", "selbs\u00b7ten", "pflegt", "zu", "schir\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PRF", "ADV", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So will ich auch in meiner Liebe stehn", "tokens": ["So", "will", "ich", "auch", "in", "mei\u00b7ner", "Lie\u00b7be", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Eur Auge mag noch so erz\u00fcrnt aussehn.", "tokens": ["Eur", "Au\u00b7ge", "mag", "noch", "so", "er\u00b7z\u00fcrnt", "aus\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich will/ mein Schatz/ euch unver\u00e4ndert lieben/", "tokens": ["Ich", "will", "/", "mein", "Schatz", "/", "euch", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "lie\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "PPOSAT", "NN", "$(", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eur Zorn heist mir noch gr\u00f6sser Treu ver\u00fcben/", "tokens": ["Eur", "Zorn", "heist", "mir", "noch", "gr\u00f6s\u00b7ser", "Treu", "ver\u00b7\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die stehest fest/ f\u00e4lt schon der Himmel ein/", "tokens": ["Die", "ste\u00b7hest", "fest", "/", "f\u00e4lt", "schon", "der", "Him\u00b7mel", "ein", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$(", "VVFIN", "ADV", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein zornig Blick kan ihr nicht sch\u00e4dlich seyn.", "tokens": ["Ein", "zor\u00b7nig", "Blick", "kan", "ihr", "nicht", "sch\u00e4d\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VMFIN", "PPER", "PTKNEG", "ADJD", "VAINF", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Z\u00fcrnt nur/ z\u00fcrnt nur/ ich bleib euch doch ergeben/", "tokens": ["Z\u00fcrnt", "nur", "/", "z\u00fcrnt", "nur", "/", "ich", "bleib", "euch", "doch", "er\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$(", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "PPER", "ADV", "VVPP", "$("], "meter": "+----+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Eur zornig Aug kan meine Treu nicht heben/", "tokens": ["Eur", "zor\u00b7nig", "Aug", "kan", "mei\u00b7ne", "Treu", "nicht", "he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Und brennt eur Zorn gleich noch vielmahl so sehr/", "tokens": ["Und", "brennt", "eur", "Zorn", "gleich", "noch", "viel\u00b7mahl", "so", "sehr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADV", "ADV", "ADV", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So lieb ich euch dennoch/ mein Kind/ vielmehr.", "tokens": ["So", "lieb", "ich", "euch", "den\u00b7noch", "/", "mein", "Kind", "/", "viel\u00b7mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Will gleich eur Zorn auf meine Liebe rasen/", "tokens": ["Will", "gleich", "eur", "Zorn", "auf", "mei\u00b7ne", "Lie\u00b7be", "ra\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch soll Bestand in meine Seegel blasen/", "tokens": ["Doch", "soll", "Be\u00b7stand", "in", "mei\u00b7ne", "See\u00b7gel", "bla\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich will getreu bey allen St\u00fcrmen seyn", "tokens": ["Ich", "will", "ge\u00b7treu", "bey", "al\u00b7len", "St\u00fcr\u00b7men", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJD", "APPR", "PIAT", "NN", "VAINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein zornig Blick reist nicht die Hoffnung ein.", "tokens": ["Ein", "zor\u00b7nig", "Blick", "reist", "nicht", "die", "Hoff\u00b7nung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PTKNEG", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Eur Zorn vermehrt noch ferner meine Liebe/", "tokens": ["Eur", "Zorn", "ver\u00b7mehrt", "noch", "fer\u00b7ner", "mei\u00b7ne", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADV", "ADV", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Man stirbt nicht gleich/ ob schon der Himmel tr\u00fcbe/", "tokens": ["Man", "stirbt", "nicht", "gleich", "/", "ob", "schon", "der", "Him\u00b7mel", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "ADV", "$(", "KOUS", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Blitzen folgt ein froher Sonnen-Schein/", "tokens": ["Auf", "Blit\u00b7zen", "folgt", "ein", "fro\u00b7her", "Son\u00b7nen\u00b7Schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Liebe pflegt im Zorn verstrickt zu seyn.", "tokens": ["Und", "Lie\u00b7be", "pflegt", "im", "Zorn", "ver\u00b7strickt", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Drum z\u00fcrnet nur annehmliche ", "tokens": ["Drum", "z\u00fcr\u00b7net", "nur", "an\u00b7nehm\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "ADJA"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Je mehr ihr z\u00fcrnt/ je mehr ich mich entz\u00fcnde", "tokens": ["Je", "mehr", "ihr", "z\u00fcrnt", "/", "je", "mehr", "ich", "mich", "ent\u00b7z\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "VVFIN", "$(", "ADV", "ADV", "PPER", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von Strahlen/ die eur d\u00fcsters Auge zeigt/", "tokens": ["Von", "Strah\u00b7len", "/", "die", "eur", "d\u00fcs\u00b7ters", "Au\u00b7ge", "zeigt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die gleich dem Schein/ der aus den Wolcken steigt.", "tokens": ["Die", "gleich", "dem", "Schein", "/", "der", "aus", "den", "Wol\u00b7cken", "steigt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}