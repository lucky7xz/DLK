{"textgrid.poem.40597": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Welch ein Bl\u00fchen, Duften, Quellen", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Bl\u00fchen, Duften, Quellen", "tokens": ["Welch", "ein", "Bl\u00fc\u00b7hen", ",", "Duf\u00b7ten", ",", "Quel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des K\u00f6nigs Artus Garten!", "tokens": ["In", "des", "K\u00f6\u00b7nigs", "Ar\u00b7tus", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fcchte aller Zonen schwellen", "tokens": ["Fr\u00fcch\u00b7te", "al\u00b7ler", "Zo\u00b7nen", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Bl\u00fcthen aller Arten.", "tokens": ["Zwi\u00b7schen", "Bl\u00fc\u00b7then", "al\u00b7ler", "Ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur am Teiche eine Weide", "tokens": ["Nur", "am", "Tei\u00b7che", "ei\u00b7ne", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht gebeugt in stummer Klage,", "tokens": ["Steht", "ge\u00b7beugt", "in", "stum\u00b7mer", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie versenkt in tiefem Leide,", "tokens": ["Wie", "ver\u00b7senkt", "in", "tie\u00b7fem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie nicht auch Fr\u00fcchte trage.", "tokens": ["Da\u00df", "sie", "nicht", "auch", "Fr\u00fcch\u00b7te", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Die gel\u00f6sten Haare fallen", "tokens": ["Die", "ge\u00b7l\u00f6s\u00b7ten", "Haa\u00b7re", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder ihr, ein gr\u00fcn Verstecke,", "tokens": ["Nie\u00b7der", "ihr", ",", "ein", "gr\u00fcn", "Ver\u00b7ste\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dran die K\u00f6n'gin fand Gefallen", "tokens": ["Dran", "die", "K\u00f6n'\u00b7gin", "fand", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auch Lanzelot, der Kecke.", "tokens": ["Und", "auch", "Lan\u00b7ze\u00b7lot", ",", "der", "Ke\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Auf dem Baum sitzt jetzt der K\u00f6nig,", "tokens": ["Auf", "dem", "Baum", "sitzt", "jetzt", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Gezweig sich wohl versteckend,", "tokens": ["Im", "Ge\u00b7zweig", "sich", "wohl", "ver\u00b7ste\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein gesalbtes Haupt ein wenig", "tokens": ["Sein", "ge\u00b7salb\u00b7tes", "Haupt", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzuweit hervor nur streckend.", "tokens": ["All\u00b7zu\u00b7weit", "her\u00b7vor", "nur", "stre\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Traun, das hat er fein ersonnen!", "tokens": ["Traun", ",", "das", "hat", "er", "fein", "er\u00b7son\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier will er das Paar belauschen,", "tokens": ["Hier", "will", "er", "das", "Paar", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn, so h\u00f6rt' er, hier am Bronnen", "tokens": ["Denn", ",", "so", "h\u00f6rt'", "er", ",", "hier", "am", "Bron\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflegt es Ku\u00df um Ku\u00df zu tauschen.", "tokens": ["Pflegt", "es", "Ku\u00df", "um", "Ku\u00df", "zu", "tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh, die K\u00f6n'gin naht der Stelle;", "tokens": ["Sieh", ",", "die", "K\u00f6n'\u00b7gin", "naht", "der", "Stel\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sie sieht die Weide prangen", "tokens": ["Doch", "sie", "sieht", "die", "Wei\u00b7de", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Widerschein der Welle,", "tokens": ["In", "dem", "Wi\u00b7der\u00b7schein", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die seltne Frucht dran hangen.", "tokens": ["Und", "die", "selt\u00b7ne", "Frucht", "dran", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Ha, zu ihr zu lagern wagte", "tokens": ["Ha", ",", "zu", "ihr", "zu", "la\u00b7gern", "wag\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich schon Lanzelot im Moose;", "tokens": ["Sich", "schon", "Lan\u00b7ze\u00b7lot", "im", "Moo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber schlau zum Ritter sagte", "tokens": ["A\u00b7ber", "schlau", "zum", "Rit\u00b7ter", "sag\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laut Ginevra jetzt, die Lose:", "tokens": ["Laut", "Gi\u00b7nev\u00b7ra", "jetzt", ",", "die", "Lo\u00b7se", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "\u00bbseht die Weid' im Teiche strahlen,", "tokens": ["\u00bb", "seht", "die", "Weid'", "im", "Tei\u00b7che", "strah\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenkt das Aug' drauf, doch genaue;", "tokens": ["Lenkt", "das", "Aug'", "drauf", ",", "doch", "ge\u00b7nau\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob euch's nennt der Bl\u00e4tter Zahlen?", "tokens": ["Ob", "euch's", "nennt", "der", "Bl\u00e4t\u00b7ter", "Zah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob es Fr\u00fcchte dran erschaue?", "tokens": ["Ob", "es", "Fr\u00fcch\u00b7te", "dran", "er\u00b7schau\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Eher tr\u00e4gt wohl Frucht die Weide,", "tokens": ["E\u00b7her", "tr\u00e4gt", "wohl", "Frucht", "die", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' z\u00e4hlt ihr der Bl\u00e4tter Masse,", "tokens": ["Eh'", "z\u00e4hlt", "ihr", "der", "Bl\u00e4t\u00b7ter", "Mas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als ich breche Lieb' und Eide,", "tokens": ["Als", "ich", "bre\u00b7che", "Lieb'", "und", "Ei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Herrn und Gatten lasse.", "tokens": ["Mei\u00b7nen", "Herrn", "und", "Gat\u00b7ten", "las\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie die Weid' auf Wellent\u00e4nzen,", "tokens": ["Wie", "die", "Weid'", "auf", "Wel\u00b7len\u00b7t\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruht sein Bild in meinem Herzen,", "tokens": ["Ruht", "sein", "Bild", "in", "mei\u00b7nem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich will's mit Liebe kr\u00e4nzen,", "tokens": ["Und", "ich", "will's", "mit", "Lie\u00b7be", "kr\u00e4n\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihr's schirmt mit Stahl und Erzen!\u00ab", "tokens": ["Wie", "ih\u00b7r's", "schirmt", "mit", "Stahl", "und", "Er\u00b7zen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Drauf der Ritter: \u00bbHa, wie zeigen", "tokens": ["Drauf", "der", "Rit\u00b7ter", ":", "\u00bb", "Ha", ",", "wie", "zei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["PAV", "ART", "NN", "$.", "$(", "ITJ", "$,", "PWAV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wellenspiegel doch genaue,", "tokens": ["Wel\u00b7len\u00b7spie\u00b7gel", "doch", "ge\u00b7nau\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sogar ich in den Zweigen", "tokens": ["Da\u00df", "so\u00b7gar", "ich", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch ein nistend V\u00f6glein schaue.", "tokens": ["Hoch", "ein", "nis\u00b7tend", "V\u00f6\u00b7glein", "schau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "VVPP", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.12": {"line.1": {"text": "Eh' wird Mensch die\u00df V\u00f6glein werden", "tokens": ["Eh'", "wird", "Mensch", "die\u00df", "V\u00f6\u00b7glein", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "PDS", "NN", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Menschenworten sprechen,", "tokens": ["Und", "in", "Men\u00b7schen\u00b7wor\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als dem K\u00f6nig je auf Erden", "tokens": ["Als", "dem", "K\u00f6\u00b7nig", "je", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflicht und Treu' ich k\u00f6nnte brechen.", "tokens": ["Pflicht", "und", "Treu'", "ich", "k\u00f6nn\u00b7te", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "So ist unserm Bund die Weihe", "tokens": ["So", "ist", "un\u00b7serm", "Bund", "die", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fcr des K\u00f6nigs Heil beschieden:", "tokens": ["F\u00fcr", "des", "K\u00f6\u00b7nigs", "Heil", "be\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fctzt im Kampf ihn meine Treue,", "tokens": ["Sch\u00fctzt", "im", "Kampf", "ihn", "mei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fcckt ihn eure Lieb' im Frieden.\u00ab", "tokens": ["Schm\u00fcckt", "ihn", "eu\u00b7re", "Lieb'", "im", "Frie\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Artus nickt als wangenrother", "tokens": ["Ar\u00b7tus", "nickt", "als", "wan\u00b7gen\u00b7ro\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Apfel froh aus Zweigeshallen,", "tokens": ["Ap\u00b7fel", "froh", "aus", "Zwei\u00b7ges\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und fast vor Entz\u00fccken droht er", "tokens": ["Und", "fast", "vor", "Ent\u00b7z\u00fc\u00b7cken", "droht", "er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueberreif vom Baum zu fallen.", "tokens": ["Ue\u00b7berr\u00b7eif", "vom", "Baum", "zu", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Sp\u00e4t im Zwielicht, m\u00fcden Leibes,", "tokens": ["Sp\u00e4t", "im", "Zwie\u00b7licht", ",", "m\u00fc\u00b7den", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stiehlt er heimlich sich nach Hause;", "tokens": ["Stiehlt", "er", "heim\u00b7lich", "sich", "nach", "Hau\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Verleumder seines Weibes", "tokens": ["Die", "Ver\u00b7leum\u00b7der", "sei\u00b7nes", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sperrt er tief in Thurmesklause.", "tokens": ["Sperrt", "er", "tief", "in", "Thur\u00b7mes\u00b7klau\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Und du darfst nun nimmer klagen,", "tokens": ["Und", "du", "darfst", "nun", "nim\u00b7mer", "kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6ne Weide, da du heute", "tokens": ["Sch\u00f6\u00b7ne", "Wei\u00b7de", ",", "da", "du", "heu\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Frucht von seltner Art getragen,", "tokens": ["Frucht", "von", "selt\u00b7ner", "Art", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dran sich manches Herz erfreute.", "tokens": ["Dran", "sich", "man\u00b7ches", "Herz", "er\u00b7freu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Welch ein Bl\u00fchen, Duften, Quellen", "tokens": ["Welch", "ein", "Bl\u00fc\u00b7hen", ",", "Duf\u00b7ten", ",", "Quel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "ART", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In des K\u00f6nigs Artus Garten!", "tokens": ["In", "des", "K\u00f6\u00b7nigs", "Ar\u00b7tus", "Gar\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fcchte aller Zonen schwellen", "tokens": ["Fr\u00fcch\u00b7te", "al\u00b7ler", "Zo\u00b7nen", "schwel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zwischen Bl\u00fcthen aller Arten.", "tokens": ["Zwi\u00b7schen", "Bl\u00fc\u00b7then", "al\u00b7ler", "Ar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PIAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Nur am Teiche eine Weide", "tokens": ["Nur", "am", "Tei\u00b7che", "ei\u00b7ne", "Wei\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Steht gebeugt in stummer Klage,", "tokens": ["Steht", "ge\u00b7beugt", "in", "stum\u00b7mer", "Kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie versenkt in tiefem Leide,", "tokens": ["Wie", "ver\u00b7senkt", "in", "tie\u00b7fem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie nicht auch Fr\u00fcchte trage.", "tokens": ["Da\u00df", "sie", "nicht", "auch", "Fr\u00fcch\u00b7te", "tra\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.19": {"line.1": {"text": "Die gel\u00f6sten Haare fallen", "tokens": ["Die", "ge\u00b7l\u00f6s\u00b7ten", "Haa\u00b7re", "fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nieder ihr, ein gr\u00fcn Verstecke,", "tokens": ["Nie\u00b7der", "ihr", ",", "ein", "gr\u00fcn", "Ver\u00b7ste\u00b7cke", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dran die K\u00f6n'gin fand Gefallen", "tokens": ["Dran", "die", "K\u00f6n'\u00b7gin", "fand", "Ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und auch Lanzelot, der Kecke.", "tokens": ["Und", "auch", "Lan\u00b7ze\u00b7lot", ",", "der", "Ke\u00b7cke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "NN", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Auf dem Baum sitzt jetzt der K\u00f6nig,", "tokens": ["Auf", "dem", "Baum", "sitzt", "jetzt", "der", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Im Gezweig sich wohl versteckend,", "tokens": ["Im", "Ge\u00b7zweig", "sich", "wohl", "ver\u00b7ste\u00b7ckend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein gesalbtes Haupt ein wenig", "tokens": ["Sein", "ge\u00b7salb\u00b7tes", "Haupt", "ein", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "PIS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Allzuweit hervor nur streckend.", "tokens": ["All\u00b7zu\u00b7weit", "her\u00b7vor", "nur", "stre\u00b7ckend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.21": {"line.1": {"text": "Traun, das hat er fein ersonnen!", "tokens": ["Traun", ",", "das", "hat", "er", "fein", "er\u00b7son\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier will er das Paar belauschen,", "tokens": ["Hier", "will", "er", "das", "Paar", "be\u00b7lau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Denn, so h\u00f6rt' er, hier am Bronnen", "tokens": ["Denn", ",", "so", "h\u00f6rt'", "er", ",", "hier", "am", "Bron\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "VVFIN", "PPER", "$,", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflegt es Ku\u00df um Ku\u00df zu tauschen.", "tokens": ["Pflegt", "es", "Ku\u00df", "um", "Ku\u00df", "zu", "tau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.22": {"line.1": {"text": "Sieh, die K\u00f6n'gin naht der Stelle;", "tokens": ["Sieh", ",", "die", "K\u00f6n'\u00b7gin", "naht", "der", "Stel\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sie sieht die Weide prangen", "tokens": ["Doch", "sie", "sieht", "die", "Wei\u00b7de", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Widerschein der Welle,", "tokens": ["In", "dem", "Wi\u00b7der\u00b7schein", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die seltne Frucht dran hangen.", "tokens": ["Und", "die", "selt\u00b7ne", "Frucht", "dran", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PAV", "VVINF", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.23": {"line.1": {"text": "Ha, zu ihr zu lagern wagte", "tokens": ["Ha", ",", "zu", "ihr", "zu", "la\u00b7gern", "wag\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "PPER", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich schon Lanzelot im Moose;", "tokens": ["Sich", "schon", "Lan\u00b7ze\u00b7lot", "im", "Moo\u00b7se", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber schlau zum Ritter sagte", "tokens": ["A\u00b7ber", "schlau", "zum", "Rit\u00b7ter", "sag\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Laut Ginevra jetzt, die Lose:", "tokens": ["Laut", "Gi\u00b7nev\u00b7ra", "jetzt", ",", "die", "Lo\u00b7se", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "ADV", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.24": {"line.1": {"text": "\u00bbseht die Weid' im Teiche strahlen,", "tokens": ["\u00bb", "seht", "die", "Weid'", "im", "Tei\u00b7che", "strah\u00b7len", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lenkt das Aug' drauf, doch genaue;", "tokens": ["Lenkt", "das", "Aug'", "drauf", ",", "doch", "ge\u00b7nau\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKVZ", "$,", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ob euch's nennt der Bl\u00e4tter Zahlen?", "tokens": ["Ob", "euch's", "nennt", "der", "Bl\u00e4t\u00b7ter", "Zah\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ob es Fr\u00fcchte dran erschaue?", "tokens": ["Ob", "es", "Fr\u00fcch\u00b7te", "dran", "er\u00b7schau\u00b7e", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PAV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.25": {"line.1": {"text": "Eher tr\u00e4gt wohl Frucht die Weide,", "tokens": ["E\u00b7her", "tr\u00e4gt", "wohl", "Frucht", "die", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' z\u00e4hlt ihr der Bl\u00e4tter Masse,", "tokens": ["Eh'", "z\u00e4hlt", "ihr", "der", "Bl\u00e4t\u00b7ter", "Mas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als ich breche Lieb' und Eide,", "tokens": ["Als", "ich", "bre\u00b7che", "Lieb'", "und", "Ei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meinen Herrn und Gatten lasse.", "tokens": ["Mei\u00b7nen", "Herrn", "und", "Gat\u00b7ten", "las\u00b7se", "."], "token_info": ["word", "word", 