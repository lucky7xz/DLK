, ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sogar ich in den Zweigen", "tokens": ["Da\u00df", "so\u00b7gar", "ich", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hoch ein nistend V\u00f6glein schaue.", "tokens": ["Hoch", "ein", "nis\u00b7tend", "V\u00f6\u00b7glein", "schau\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "VVPP", "NN", "VVFIN", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}}, "stanza.28": {"line.1": {"text": "Eh' wird Mensch die\u00df V\u00f6glein werden", "tokens": ["Eh'", "wird", "Mensch", "die\u00df", "V\u00f6\u00b7glein", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "NN", "PDS", "NN", "VAINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und in Menschenworten sprechen,", "tokens": ["Und", "in", "Men\u00b7schen\u00b7wor\u00b7ten", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als dem K\u00f6nig je auf Erden", "tokens": ["Als", "dem", "K\u00f6\u00b7nig", "je", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Pflicht und Treu' ich k\u00f6nnte brechen.", "tokens": ["Pflicht", "und", "Treu'", "ich", "k\u00f6nn\u00b7te", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.29": {"line.1": {"text": "So ist unserm Bund die Weihe", "tokens": ["So", "ist", "un\u00b7serm", "Bund", "die", "Wei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "F\u00fcr des K\u00f6nigs Heil beschieden:", "tokens": ["F\u00fcr", "des", "K\u00f6\u00b7nigs", "Heil", "be\u00b7schie\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00fctzt im Kampf ihn meine Treue,", "tokens": ["Sch\u00fctzt", "im", "Kampf", "ihn", "mei\u00b7ne", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schm\u00fcckt ihn eure Lieb' im Frieden.\u00ab", "tokens": ["Schm\u00fcckt", "ihn", "eu\u00b7re", "Lieb'", "im", "Frie\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.30": {"line.1": {"text": "Artus nickt als wangenrother", "tokens": ["Ar\u00b7tus", "nickt", "als", "wan\u00b7gen\u00b7ro\u00b7ther"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Apfel froh aus Zweigeshallen,", "tokens": ["Ap\u00b7fel", "froh", "aus", "Zwei\u00b7ges\u00b7hal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und fast vor Entz\u00fccken droht er", "tokens": ["Und", "