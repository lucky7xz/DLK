{"textgrid.poem.34991": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Michel nach dem M\u00e4rz", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Solang ich den deutschen Michel gekannt,", "tokens": ["So\u00b7lang", "ich", "den", "deut\u00b7schen", "Mi\u00b7chel", "ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "War er ein B\u00e4renh\u00e4uter;", "tokens": ["War", "er", "ein", "B\u00e4\u00b7ren\u00b7h\u00e4u\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich dachte im M\u00e4rz, er hat sich ermannt", "tokens": ["Ich", "dach\u00b7te", "im", "M\u00e4rz", ",", "er", "hat", "sich", "er\u00b7mannt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPER", "VAFIN", "PRF", "VVFIN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und handelt f\u00fcrder gescheuter.", "tokens": ["Und", "han\u00b7delt", "f\u00fcr\u00b7der", "ge\u00b7scheu\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie stolz erhob er das blonde Haupt", "tokens": ["Wie", "stolz", "er\u00b7hob", "er", "das", "blon\u00b7de", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Vor seinen Landesv\u00e4tern!", "tokens": ["Vor", "sei\u00b7nen", "Lan\u00b7des\u00b7v\u00e4\u00b7tern", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie sprach er \u2013 was doch unerlaubt \u2013", "tokens": ["Wie", "sprach", "er", "\u2013", "was", "doch", "un\u00b7er\u00b7laubt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$(", "PWS", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von hohen Landesverr\u00e4tern.", "tokens": ["Von", "ho\u00b7hen", "Lan\u00b7des\u00b7ver\u00b7r\u00e4\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Das klang so s\u00fc\u00df zu meinem Ohr", "tokens": ["Das", "klang", "so", "s\u00fc\u00df", "zu", "mei\u00b7nem", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie m\u00e4rchenhafte Sagen,", "tokens": ["Wie", "m\u00e4r\u00b7chen\u00b7haf\u00b7te", "Sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich f\u00fchlte, wie ein junger Tor,", "tokens": ["Ich", "f\u00fchl\u00b7te", ",", "wie", "ein", "jun\u00b7ger", "Tor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz mir wieder schlagen.", "tokens": ["Das", "Herz", "mir", "wie\u00b7der", "schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Doch als die schwarzrotgoldne Fahn',", "tokens": ["Doch", "als", "die", "schwarz\u00b7rot\u00b7gold\u00b7ne", "Fahn'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der altgermanische Plunder,", "tokens": ["Der", "alt\u00b7ger\u00b7ma\u00b7ni\u00b7sche", "Plun\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aufs neu' erschien, da schwand mein Wahn", "tokens": ["Aufs", "neu'", "er\u00b7schien", ",", "da", "schwand", "mein", "Wahn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "$,", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und die s\u00fc\u00dfen M\u00e4rchenwunder.", "tokens": ["Und", "die", "s\u00fc\u00b7\u00dfen", "M\u00e4r\u00b7chen\u00b7wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich kannte die Farben in diesem Panier", "tokens": ["Ich", "kann\u00b7te", "die", "Far\u00b7ben", "in", "die\u00b7sem", "Pa\u00b7nier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und ihre Vorbedeutung:", "tokens": ["Und", "ih\u00b7re", "Vor\u00b7be\u00b7deu\u00b7tung", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von deutscher Freiheit brachten sie mir", "tokens": ["Von", "deut\u00b7scher", "Frei\u00b7heit", "brach\u00b7ten", "sie", "mir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die schlimmste Hiobszeitung.", "tokens": ["Die", "schlimms\u00b7te", "Hiobs\u00b7zei\u00b7tung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.6": {"line.1": {"text": "Schon sah ich den Arndt, den Vater Jahn \u2013", "tokens": ["Schon", "sah", "ich", "den", "Arndt", ",", "den", "Va\u00b7ter", "Jahn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "$,", "ART", "NN", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Helden aus andern Zeiten", "tokens": ["Die", "Hel\u00b7den", "aus", "an\u00b7dern", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Aus ihren Gr\u00e4bern wieder nahn", "tokens": ["Aus", "ih\u00b7ren", "Gr\u00e4\u00b7bern", "wie\u00b7der", "nahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ADJA"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und f\u00fcr den Kaiser streiten.", "tokens": ["Und", "f\u00fcr", "den", "Kai\u00b7ser", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Die Burschenschaftler allesamt", "tokens": ["Die", "Bur\u00b7schen\u00b7schaft\u00b7ler", "al\u00