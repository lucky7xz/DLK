{"dta.poem.10493": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "L.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "M\u00e4dchen mit dem rothen M\u00fcndchen,", "tokens": ["M\u00e4d\u00b7chen", "mit", "dem", "ro\u00b7then", "M\u00fcnd\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit den Aeuglein s\u00fc\u00df und klar,", "tokens": ["Mit", "den", "A\u00b7e\u00b7u\u00b7glein", "s\u00fc\u00df", "und", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Du mein liebes, kleines M\u00e4dchen,", "tokens": ["Du", "mein", "lie\u00b7bes", ",", "klei\u00b7nes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner denk' ich immerdar.", "tokens": ["Dei\u00b7ner", "denk'", "ich", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lang ist heut der Winterabend,", "tokens": ["Lang", "ist", "heut", "der", "Win\u00b7ter\u00b7a\u00b7bend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich m\u00f6chte bei dir seyn,", "tokens": ["Und", "ich", "m\u00f6ch\u00b7te", "bei", "dir", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei dir sitzen, mit dir schwatzen,", "tokens": ["Bei", "dir", "sit\u00b7zen", ",", "mit", "dir", "schwat\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im vertrauten K\u00e4mmerlein.", "tokens": ["Im", "ver\u00b7trau\u00b7ten", "K\u00e4m\u00b7mer\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "An die Lippen wollt' ich pressen", "tokens": ["An", "die", "Lip\u00b7pen", "wollt'", "ich", "pres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deine kleine, wei\u00dfe Hand,", "tokens": ["Dei\u00b7ne", "klei\u00b7ne", ",", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und mit Thr\u00e4nen sie benetzen,", "tokens": ["Und", "mit", "Thr\u00e4\u00b7nen", "sie", "be\u00b7net\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine kleine, wei\u00dfe Hand.", "tokens": ["Dei\u00b7ne", "klei\u00b7ne", ",", "wei\u00b7\u00dfe", "Hand", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}