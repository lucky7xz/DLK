, "See", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "denn makt de Swager sick parat,", "tokens": ["denn", "makt", "de", "Swa\u00b7ger", "sick", "pa\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "un d\u00f6rch dat Holt bl\u00f6\u00dft he.", "tokens": ["un", "d\u00f6rch", "dat", "Holt", "bl\u00f6\u00dft", "he", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "PAV", "ART", "NN", "VVFIN", "NE", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Dat klingt so s\u00f6t, so eegen jo", "tokens": ["Dat", "klingt", "so", "s\u00f6t", ",", "so", "e\u00b7e\u00b7gen", "jo"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "VVFIN", "$,", "ADV", "ADJA", "NE"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "as Sang un \u00d6rgel her;", "tokens": ["as", "Sang", "un", "\u00d6r\u00b7gel", "her", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "so nipp h\u00fcrt denn de Ollsche to,", "tokens": ["so", "nipp", "h\u00fcrt", "denn", "de", "Oll\u00b7sche", "to", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "as bl\u00f6s' he man f\u00f6r ehr.", "tokens": ["as", "bl\u00f6s'", "he", "man", "f\u00f6r", "ehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM