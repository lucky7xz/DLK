{"textgrid.poem.63467": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Ollendeel", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "De Ollsch hett nu dat Ollendeel,", "tokens": ["De", "Ollsch", "hett", "nu", "dat", "Ol\u00b7len\u00b7deel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "l\u00fctt Kamer bi den Bur'n,", "tokens": ["l\u00fctt", "Ka\u00b7mer", "bi", "den", "Bur'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "nich \u0153werschrag, nich \u0153werv\u00e4l \u2013", "tokens": ["nich", "\u0153wer\u00b7schrag", ",", "nich", "\u0153wer\u00b7v\u00e4l", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PTKNEG", "NE", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "up't Johr s\u00f6\u00df Sch\u00e4pel Kurn.", "tokens": ["up't", "Johr", "s\u00f6\u00df", "Sch\u00e4\u00b7pel", "Kurn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ehr Mann is dot, ehr S\u0153hn is dot,", "tokens": ["Ehr", "Mann", "is", "dot", ",", "ehr", "S\u0153hn", "is", "dot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "beid \u00fcnner eenen Steen;", "tokens": ["b