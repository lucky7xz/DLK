{"dta.poem.19484": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Auf Goldgrund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Durch den Bildersaal bin ich gegangen", "tokens": ["Durch", "den", "Bil\u00b7der\u00b7saal", "bin", "ich", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VAFIN", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der letzten Stunde noch, der sp\u00e4ten,", "tokens": ["In", "der", "letz\u00b7ten", "Stun\u00b7de", "noch", ",", "der", "sp\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wo, von schimmernd goldnem Grund umfangen,", "tokens": ["Wo", ",", "von", "schim\u00b7mernd", "gold\u00b7nem", "Grund", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Heil'ge mit gehobnen H\u00e4nden beten.", "tokens": ["Heil'\u00b7ge", "mit", "ge\u00b7hob\u00b7nen", "H\u00e4n\u00b7den", "be\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dann durchs blache Feld bin ich geschritten.", "tokens": ["Dann", "durchs", "bla\u00b7che", "Feld", "bin", "ich", "ge\u00b7schrit\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Letzter Sommerabendgluth entgegen,", "tokens": ["Letz\u00b7ter", "Som\u00b7mer\u00b7a\u00b7bend\u00b7gluth", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die heut das reife Korn geschnitten,", "tokens": ["Und", "die", "heut", "das", "rei\u00b7fe", "Korn", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sah ich Garben auf den Wagen legen.", "tokens": ["Sah", "ich", "Gar\u00b7ben", "auf", "den", "Wa\u00b7gen", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Rasch gedieh das Werk der braunen Arme,", "tokens": ["Rasch", "ge\u00b7dieh", "das", "Werk", "der", "brau\u00b7nen", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Um den Schnitter und die dunkle Garbe", "tokens": ["Um", "den", "Schnit\u00b7ter", "und", "die", "dunk\u00b7le", "Gar\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Flo\u00df das Abendlicht, das gl\u00fchend warme,", "tokens": ["Flo\u00df", "das", "A\u00b7bend\u00b7licht", ",", "das", "gl\u00fc\u00b7hend", "war\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Mit der wunderbaren Goldesfarbe.", "tokens": ["Mit", "der", "wun\u00b7der\u00b7ba\u00b7ren", "Gol\u00b7des\u00b7far\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Unter B\u00fcrden schwankende Gestalten", "tokens": ["Un\u00b7ter", "B\u00fcr\u00b7den", "schwan\u00b7ken\u00b7de", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Lautlos in der stillen Feierstunde!", "tokens": ["Laut\u00b7los", "in", "der", "stil\u00b7len", "Fei\u00b7er\u00b7stun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "M\u00fcder Arme unerm\u00fcdlich Walten,", "tokens": ["M\u00fc\u00b7der", "Ar\u00b7me", "un\u00b7er\u00b7m\u00fcd\u00b7lich", "Wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Auch auf schimmernd heilig-goldnem Grunde!", "tokens": ["Auch", "auf", "schim\u00b7mernd", "hei\u00b7lig\u00b7gold\u00b7nem", "Grun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}