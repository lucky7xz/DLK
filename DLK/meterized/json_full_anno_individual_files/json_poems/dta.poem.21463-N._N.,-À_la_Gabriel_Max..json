{"dta.poem.21463": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "\u00c0 la  Gabriel Max.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.85", "pl:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O la\u00df mich, la\u00df mich,", "tokens": ["O", "la\u00df", "mich", ",", "la\u00df", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "$,", "VVIMP", "PPER", "$,"], "meter": "++-+-", "measure": "iambic.di"}, "line.2": {"text": "Du blasse Dirne,", "tokens": ["Du", "blas\u00b7se", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Du so hei\u00df begehrtes,", "tokens": ["Du", "so", "hei\u00df", "be\u00b7gehr\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So schn\u00f6de verdammtes", "tokens": ["So", "schn\u00f6\u00b7de", "ver\u00b7damm\u00b7tes"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Kind der S\u00fcnde!", "tokens": ["Kind", "der", "S\u00fcn\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Was soll das L\u00e4cheln,", "tokens": ["Was", "soll", "das", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Das sinnverwirrende,", "tokens": ["Das", "sinn\u00b7ver\u00b7wir\u00b7ren\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.8": {"text": "Das den reizenden", "tokens": ["Das", "den", "rei\u00b7zen\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PDS", "ART", "ADJA"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Kleinen schwellenden Mund", "tokens": ["Klei\u00b7nen", "schwel\u00b7len\u00b7den", "Mund"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.10": {"text": "Dir so lieblich umknospet?", "tokens": ["Dir", "so", "lieb\u00b7lich", "um\u00b7knos\u00b7pet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.11": {"text": "Was soll deiner gro\u00dfen", "tokens": ["Was", "soll", "dei\u00b7ner", "gro\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPOSAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Nachtschwarzen Kinderaugen", "tokens": ["Nacht\u00b7schwar\u00b7zen", "Kin\u00b7der\u00b7au\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Wehm\u00fcthige R\u00e4thselfrage,", "tokens": ["Weh\u00b7m\u00fct\u00b7hi\u00b7ge", "R\u00e4th\u00b7sel\u00b7fra\u00b7ge", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "All\u2019 die bachantische Glut,", "tokens": ["All'", "die", "ba\u00b7ch\u00b7an\u00b7ti\u00b7sche", "Glut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "All\u2019 das lustsatte Leid,", "tokens": ["All'", "das", "lust\u00b7sat\u00b7te", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Das dein m\u00fcdes Gesichtchen", "tokens": ["Das", "dein", "m\u00fc\u00b7des", "Ge\u00b7sicht\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "PPOSAT", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Mir wechselnd k\u00fcndet?", "tokens": ["Mir", "wech\u00b7selnd", "k\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$."], "meter": "++-+-", "measure": "iambic.di"}, "line.18": {"text": "Ich kann dich nicht retten", "tokens": ["Ich", "kann", "dich", "nicht", "ret\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.19": {"text": "Aus dem Pfuhl der Verderbni\u00df,", "tokens": ["Aus", "dem", "Pfuhl", "der", "Ver\u00b7der\u00b7bni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.20": {"text": "Du sch\u00f6ne Verlorene! . .", "tokens": ["Du", "sch\u00f6\u00b7ne", "Ver\u00b7lo\u00b7re\u00b7ne", "!", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "$.", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.21": {"text": "Nicht darf ich mehr bergen", "tokens": ["Nicht", "darf", "ich", "mehr", "ber\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.22": {"text": "Dein s\u00fc\u00dfes Lockenhaupt", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Lo\u00b7cken\u00b7haupt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "An meine starke", "tokens": ["An", "mei\u00b7ne", "star\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.24": {"text": "Pochende M\u00e4nnerbrust,", "tokens": ["Po\u00b7chen\u00b7de", "M\u00e4n\u00b7ner\u00b7brust", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.25": {"text": "Nicht mehr mit zitternden Fingern", "tokens": ["Nicht", "mehr", "mit", "zit\u00b7tern\u00b7den", "Fin\u00b7gern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.26": {"text": "Voll seliger Trunkenheit", "tokens": ["Voll", "se\u00b7li\u00b7ger", "Trun\u00b7ken\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.27": {"text": "W\u00fchlen in deinem Seidenhaar.", "tokens": ["W\u00fch\u00b7len", "in", "dei\u00b7nem", "Sei\u00b7den\u00b7haar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.28": {"text": "Ich lieb\u2019 eine Andere! . .", "tokens": ["Ich", "lieb'", "ei\u00b7ne", "An\u00b7de\u00b7re", "!", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "$.", "$.", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.29": {"text": "Wie du mich liebst", "tokens": ["Wie", "du", "mich", "liebst"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.30": {"text": "Mit all\u2019 der St\u00e4rke und Reine", "tokens": ["Mit", "all'", "der", "St\u00e4r\u00b7ke", "und", "Rei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIS", "ART", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.31": {"text": "Und thaufrischen Fr\u00fchlingsempfindung", "tokens": ["Und", "thauf\u00b7ri\u00b7schen", "Fr\u00fch\u00b7ling\u00b7semp\u00b7fin\u00b7dung"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.32": {"text": "All\u2019 der herzf\u00fcllenden Leidenschaft", "tokens": ["All'", "der", "herz\u00b7f\u00fcl\u00b7len\u00b7den", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "word", "word", "word"], "pos": ["PIS", "ART", "ADJA", "NN"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.33": {"text": "Der wahren Liebe! \u2014 \u2014 \u2014", "tokens": ["Der", "wah\u00b7ren", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "$(", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.34": {"text": "Ach, nicht deiner Seele", "tokens": ["Ach", ",", "nicht", "dei\u00b7ner", "See\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PTKNEG", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "Holdes Geheimni\u00df", "tokens": ["Hol\u00b7des", "Ge\u00b7heim\u00b7ni\u00df"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Suchte ich br\u00fcnstig,", "tokens": ["Such\u00b7te", "ich", "br\u00fcns\u00b7tig", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.37": {"text": "Aug\u2019 in Auge gesenkt", "tokens": ["Aug'", "in", "Au\u00b7ge", "ge\u00b7senkt"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVPP"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.38": {"text": "Lippe hangend an Lippe", "tokens": ["Lip\u00b7pe", "han\u00b7gend", "an", "Lip\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVPP", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.39": {"text": "In der Wollustumarmung", "tokens": ["In", "der", "Wol\u00b7lus\u00b7tu\u00b7mar\u00b7mung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.40": {"text": "Wildlodernder K\u00fcsse,", "tokens": ["Wild\u00b7lo\u00b7dern\u00b7der", "K\u00fcs\u00b7se", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+---+-", "measure": "dactylic.init"}, "line.41": {"text": "Nur deines Leibes", "tokens": ["Nur", "dei\u00b7nes", "Lei\u00b7bes"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Jungfr\u00e4ulich herber", "tokens": ["Jung\u00b7fr\u00e4u\u00b7lich", "her\u00b7ber"], "token_info": ["word", "word"], "pos": ["ADJD", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.43": {"text": "Berauschender Dufthauch", "tokens": ["Be\u00b7rau\u00b7schen\u00b7der", "Duf\u00b7thauch"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Trieb mich fiebernd", "tokens": ["Trieb", "mich", "fie\u00b7bernd"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "VVPP"], "meter": "+-+-", "measure": "trochaic.di"}, "line.45": {"text": "In deine weichen Arme,", "tokens": ["In", "dei\u00b7ne", "wei\u00b7chen", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Da\u00df ich wild an mich pre\u00dfte", "tokens": ["Da\u00df", "ich", "wild", "an", "mich", "pre\u00df\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "PPER", "VVFIN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.47": {"text": "Deiner wei\u00dfwogenden Br\u00fcste", "tokens": ["Dei\u00b7ner", "wei\u00df\u00b7wo\u00b7gen\u00b7den", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "Schimmernde F\u00fclle,", "tokens": ["Schim\u00b7mern\u00b7de", "F\u00fcl\u00b7le", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.49": {"text": "Zu s\u00e4ttigen der Sinne", "tokens": ["Zu", "s\u00e4t\u00b7ti\u00b7gen", "der", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Ewig rege D\u00e4monen \u2026", "tokens": ["E\u00b7wig", "re\u00b7ge", "D\u00e4\u00b7mo\u00b7nen", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.51": {"text": "Ich kann dich nicht retten \u2026", "tokens": ["Ich", "kann", "dich", "nicht", "ret\u00b7ten", "\u2026"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.52": {"text": "O fluche nicht dem Unseligen!", "tokens": ["O", "flu\u00b7che", "nicht", "dem", "Un\u00b7se\u00b7li\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.53": {"text": "Auch ich bin gebannt", "tokens": ["Auch", "ich", "bin", "ge\u00b7bannt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.54": {"text": "In sternlose Nacht", "tokens": ["In", "stern\u00b7lo\u00b7se", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.55": {"text": "Wie du;", "tokens": ["Wie", "du", ";"], "token_info": ["word", "word", "punct"], "pos": ["PWAV", "PPER", "$."], "meter": "-+", "measure": "iambic.single"}, "line.56": {"text": "Unst\u00e4t und fl\u00fcchtig", "tokens": ["Un\u00b7st\u00e4t", "und", "fl\u00fcch\u00b7tig"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.57": {"text": "Mu\u00df ich weiter irren", "tokens": ["Mu\u00df", "ich", "wei\u00b7ter", "ir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ADV", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.58": {"text": "Durch pfadleere W\u00fcste,", "tokens": ["Durch", "pfad\u00b7lee\u00b7re", "W\u00fcs\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.59": {"text": "Stumm weiterschleppen", "tokens": ["Stumm", "wei\u00b7ter\u00b7schlep\u00b7pen"], "token_info": ["word", "word"], "pos": ["ADJD", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.60": {"text": "Die Qualenlast", "tokens": ["Die", "Qua\u00b7len\u00b7last"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.61": {"text": "Nie gestillter Sehnsucht.", "tokens": ["Nie", "ge\u00b7still\u00b7ter", "Sehn\u00b7sucht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}