{"dta.poem.21058": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Schl\u00e4fer erwach.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193300", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schl&#228;fer erwach!               ", "tokens": ["Schl", "&#228;", "fer", "er\u00b7wach", "!"], "token_info": ["word", "XML_entity", "word", "word", "punct"], "pos": ["NE", "$(", "ADJD", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schau, die Pforten des Osten", "tokens": ["Schau", ",", "die", "Pfor\u00b7ten", "des", "Os\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00d6nffet der r\u00f6thliche Morgen,", "tokens": ["\u00d6nf\u00b7fet", "der", "r\u00f6th\u00b7li\u00b7che", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Streuet Rosen und Krokos", "tokens": ["Streu\u00b7et", "Ro\u00b7sen", "und", "Kro\u00b7kos"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Von der leuchtenden Tochter des Himmels daher \u2014", "tokens": ["Von", "der", "leuch\u00b7ten\u00b7den", "Toch\u00b7ter", "des", "Him\u00b7mels", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "PAV", "$("], "meter": "--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.7": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schau, die Tochter des Himmels", "tokens": ["Schau", ",", "die", "Toch\u00b7ter", "des", "Him\u00b7mels"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ART", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "\u00d6ffnet die goldenen Wimper.", "tokens": ["\u00d6ff\u00b7net", "die", "gol\u00b7de\u00b7nen", "Wim\u00b7per", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Freundlich nickt sie,", "tokens": ["Freund\u00b7lich", "nickt", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Liebend blickt sie", "tokens": ["Lie\u00b7bend", "blickt", "sie"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Auf die thr\u00e4nenblitzende Flur.", "tokens": ["Auf", "die", "thr\u00e4\u00b7nen\u00b7blit\u00b7zen\u00b7de", "Flur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Schau, wie blitzen die Fluren!", "tokens": ["Schau", ",", "wie", "blit\u00b7zen", "die", "Flu\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schau, wie funkeln die G\u00e4rten!", "tokens": ["Schau", ",", "wie", "fun\u00b7keln", "die", "G\u00e4r\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Horch, wie lispelts im Hayn!", "tokens": ["Horch", ",", "wie", "lis\u00b7pelts", "im", "Hayn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "NE", "APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Jeder wankende Grashalm", "tokens": ["Je\u00b7der", "wan\u00b7ken\u00b7de", "Gras\u00b7halm"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Eine Perlenschnur!", "tokens": ["Ei\u00b7ne", "Per\u00b7len\u00b7schnur", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Jeder nickende Wipfel", "tokens": ["Je\u00b7der", "ni\u00b7cken\u00b7de", "Wip\u00b7fel"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.9": {"text": "Ein Juwelenstrauss!", "tokens": ["Ein", "Ju\u00b7we\u00b7len\u00b7strauss", "!"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Schl\u00e4fer erwach!", "tokens": ["Schl\u00e4\u00b7fer", "er\u00b7wach", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}}}}