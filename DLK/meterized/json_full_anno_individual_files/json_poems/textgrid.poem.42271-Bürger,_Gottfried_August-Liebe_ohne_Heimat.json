{"textgrid.poem.42271": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Liebe ohne Heimat", "genre": "verse", "period": "N.A.", "pub_year": 1770, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Liebe, lange wie die Taube", "tokens": ["Mei\u00b7ne", "Lie\u00b7be", ",", "lan\u00b7ge", "wie", "die", "Tau\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Von dem Falken hin und her gescheucht,", "tokens": ["Von", "dem", "Fal\u00b7ken", "hin", "und", "her", "ge\u00b7scheucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "KON", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "W\u00e4hnte froh, sie hab' ihr Nest erreicht", "tokens": ["W\u00e4hn\u00b7te", "froh", ",", "sie", "hab'", "ihr", "Nest", "er\u00b7reicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In den Zweigen einer G\u00f6tterlaube.", "tokens": ["In", "den", "Zwei\u00b7gen", "ei\u00b7ner", "G\u00f6t\u00b7ter\u00b7lau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Armes T\u00e4ubchen! Hart get\u00e4uschter Glaube!", "tokens": ["Ar\u00b7mes", "T\u00e4ub\u00b7chen", "!", "Hart", "ge\u00b7t\u00e4uschter", "Glau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NE", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Herbes Schicksal, dem kein andres gleicht!", "tokens": ["Her\u00b7bes", "Schick\u00b7sal", ",", "dem", "kein", "and\u00b7res", "gleicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "PIAT", "PIS", "VVFIN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Ihre Heimat, kaum dem Blick gezeigt,", "tokens": ["Ih\u00b7re", "Hei\u00b7mat", ",", "kaum", "dem", "Blick", "ge\u00b7zeigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wurde schnell dem Wetterstrahl zum Raube.", "tokens": ["Wur\u00b7de", "schnell", "dem", "Wet\u00b7ter\u00b7strahl", "zum", "Rau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach, nun irrt sie wieder hin und her!", "tokens": ["Ach", ",", "nun", "irrt", "sie", "wie\u00b7der", "hin", "und", "her", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "KON", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Zwischen Erd' und Himmel schwebt die Arme,", "tokens": ["Zwi\u00b7schen", "Erd'", "und", "Him\u00b7mel", "schwebt", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Sonder Ziel f\u00fcr ihres Flugs Beschwer.", "tokens": ["Son\u00b7der", "Ziel", "f\