 {"text": "Das deine H\u00e4nde zugericht.", "tokens": ["Das", "dei\u00b7ne", "H\u00e4n\u00b7de", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Der HErr ist unaufh\u00f6rlich K\u00f6nig;", "tokens": ["Der", "Herr", "ist", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "K\u00f6\u00b7nig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Es ist Jhm alles unterth\u00e4nig,", "tokens": ["Es", "ist", "Jhm", "al\u00b7les", "un\u00b7ter\u00b7th\u00e4\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und ewig wird sein Reich bestehn,", "tokens": ["Und", "e\u00b7wig", "wird", "sein", "Reich", "be\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch nicht wie Pharao vergehn;", "tokens": ["Auch", "nicht", "wie", "Pha\u00b7rao", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "KOKOM", "NN", "VVINF", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Er zog mit Wagen, Reitern, Rossen", "tokens": ["Er", "zog", "mit", "Wa\u00b7gen", ",", "Rei\u00b7tern", ",", "Ros\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ins Meer, da gab der HErr Befehl,", "tokens": ["Ins", "Meer", ",", "da", "g