{"dta.poem.13403": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der Lobgesang Mosis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Der HERR ist! Diesem will ich singen!", "tokens": ["Der", "HeRR", "ist", "!", "Die\u00b7sem", "will", "ich", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$.", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Loblied soll von Jhm erklingen:", "tokens": ["Mein", "Lob\u00b7lied", "soll", "von", "Jhm", "er\u00b7klin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn Er hat eine That gethan,", "tokens": ["Denn", "Er", "hat", "ei\u00b7ne", "That", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die man nicht gnug erheben kann;", "tokens": ["Die", "man", "nicht", "gnug", "er\u00b7he\u00b7ben", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKNEG", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Kriegesheere, Rosse, Wagen", "tokens": ["Der", "Krie\u00b7ges\u00b7hee\u00b7re", ",", "Ros\u00b7se", ",", "Wa\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt in das Meer gest\u00fcrzet hat,", "tokens": ["Jetzt", "in", "das", "Meer", "ge\u00b7st\u00fcr\u00b7zet", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von diesem Wunder will ich sagen,", "tokens": ["Von", "die\u00b7sem", "Wun\u00b7der", "will", "ich", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Entz\u00fccket r\u00fchmt mein Geist die That!", "tokens": ["Ent\u00b7z\u00fc\u00b7cket", "r\u00fchmt", "mein", "Geist", "die", "That", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der HErr ist meine Kraft und St\u00e4rke,", "tokens": ["Der", "Herr", "ist", "mei\u00b7ne", "Kraft", "und", "St\u00e4r\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Lobgesang preist seine Werke,", "tokens": ["Mein", "Lob\u00b7ge\u00b7sang", "preist", "sei\u00b7ne", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der HErr! der H\u00f6chste! GOtt! mein Heyl!", "tokens": ["Der", "Herr", "!", "der", "H\u00f6chs\u00b7te", "!", "Gott", "!", "mein", "Heyl", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "ART", "NN", "$.", "NN", "$.", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O du mein auserw\u00e4hlter Theil!", "tokens": ["O", "du", "mein", "au\u00b7ser\u00b7w\u00e4hl\u00b7ter", "Theil", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das ist mein GOtt! ich will Jhn preisen,", "tokens": ["Das", "ist", "mein", "Gott", "!", "ich", "will", "Jhn", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Auch meines Vaters GOtt ist Er,", "tokens": ["Auch", "mei\u00b7nes", "Va\u00b7ters", "Gott", "ist", "Er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Jhm will ich dankbar mich erweisen,", "tokens": ["Jhm", "will", "ich", "dank\u00b7bar", "mich", "er\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Erheben will ich Jhn noch mehr!", "tokens": ["Er\u00b7he\u00b7ben", "will", "ich", "Jhn", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der HErr, das ist der rechte Streiter,", "tokens": ["Der", "Herr", ",", "das", "ist", "der", "rech\u00b7te", "Strei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "HeRR ist sein Nahme! Wagen, Reiter,", "tokens": ["HeRR", "ist", "sein", "Nah\u00b7me", "!", "Wa\u00b7gen", ",", "Rei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$.", "NN", "$,", "NE", "$,"], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Pharaonis Macht, sein Heer", "tokens": ["Des", "Pha\u00b7rao\u00b7nis", "Macht", ",", "sein", "Heer"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "PPOSAT", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Warf Er allein jetzt in das Meer,", "tokens": ["Warf", "Er", "al\u00b7lein", "jetzt", "in", "das", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.5": {"text": "Die H\u00e4upter, so sich der erlesen,", "tokens": ["Die", "H\u00e4up\u00b7ter", ",", "so", "sich", "der", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PRF", "ART", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Helden sah man erst noch hie,", "tokens": ["Die", "Hel\u00b7den", "sah", "man", "erst", "noch", "hie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nun aber sind sie da gewesen,", "tokens": ["Nun", "a\u00b7ber", "sind", "sie", "da", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Jm Schilfmeer dort versunken sie.", "tokens": ["Jm", "Schilf\u00b7meer", "dort", "ver\u00b7sun\u00b7ken", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Tiefe deckt sie diese Stunde,", "tokens": ["Die", "Tie\u00b7fe", "deckt", "sie", "die\u00b7se", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fielen allesammt zu Grunde,", "tokens": ["Sie", "fie\u00b7len", "al\u00b7le\u00b7sammt", "zu", "Grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Steine pl\u00f6tzlich und geschwind", "tokens": ["Wie", "Stei\u00b7ne", "pl\u00f6tz\u00b7lich", "und", "ge\u00b7schwind"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Jm Wasser untergangen sind;", "tokens": ["Jm", "Was\u00b7ser", "un\u00b7ter\u00b7gan\u00b7gen", "sind", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Herr! was f\u00fcr \u00fcbergrosse Wunder", "tokens": ["Herr", "!", "was", "f\u00fcr", "\u00fc\u00b7berg\u00b7ros\u00b7se", "Wun\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWS", "APPR", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Verrichtet deine rechte Hand!", "tokens": ["Ver\u00b7rich\u00b7tet", "dei\u00b7ne", "rech\u00b7te", "Hand", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Feinde hat sie erst jetzunder", "tokens": ["Die", "Fein\u00b7de", "hat", "sie", "erst", "je\u00b7tzun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Begraben in des Meeres Sand.", "tokens": ["Be\u00b7gra\u00b7ben", "in", "des", "Mee\u00b7res", "Sand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Widersacher sind gest\u00fcrzet,", "tokens": ["Die", "Wi\u00b7der\u00b7sa\u00b7cher", "sind", "ge\u00b7st\u00fcr\u00b7zet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Raserey ist abgek\u00fcrzet", "tokens": ["Die", "Ra\u00b7se\u00b7rey", "ist", "ab\u00b7ge\u00b7k\u00fcr\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch deine grosse Herrlichkeit,", "tokens": ["Durch", "dei\u00b7ne", "gros\u00b7se", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O HErr GOTT! m\u00e4chtig in dem Streit!", "tokens": ["O", "Herr", "GoTT", "!", "m\u00e4ch\u00b7tig", "in", "dem", "Streit", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "NE", "$.", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Indem Du deinen Grimm ausliessest,", "tokens": ["In\u00b7dem", "Du", "dei\u00b7nen", "Grimm", "aus\u00b7lies\u00b7sest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "War ihre Macht schon umgekehrt,", "tokens": ["War", "ih\u00b7re", "Macht", "schon", "um\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Eh Du Dich noch gewaltig wiesest;", "tokens": ["Eh", "Du", "Dich", "noch", "ge\u00b7wal\u00b7tig", "wie\u00b7sest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Wie Stoppeln hat Er sie verzehrt.", "tokens": ["Wie", "Stop\u00b7peln", "hat", "Er", "sie", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du bliesest, und die Wasser flohen,", "tokens": ["Du", "blie\u00b7sest", ",", "und", "die", "Was\u00b7ser", "flo\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Allm\u00e4chtiger! vor deinem Drohen,", "tokens": ["All\u00b7m\u00e4ch\u00b7ti\u00b7ger", "!", "vor", "dei\u00b7nem", "Dro\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Fluthen stunden in dem Lauf,", "tokens": ["Die", "Flut\u00b7hen", "stun\u00b7den", "in", "dem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hielten ihre Wellen auf,", "tokens": ["Und", "hiel\u00b7ten", "ih\u00b7re", "Wel\u00b7len", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es wallete die grosse Tiefe", "tokens": ["Es", "wal\u00b7le\u00b7te", "die", "gros\u00b7se", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ganz von einander in dem Meer,", "tokens": ["Ganz", "von", "ein\u00b7an\u00b7der", "in", "dem", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Weil das Gew\u00e4sser nicht mehr liefe;", "tokens": ["Weil", "das", "Ge\u00b7w\u00e4s\u00b7ser", "nicht", "mehr", "lie\u00b7fe", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Berge stund es um uns her.", "tokens": ["Wie", "Ber\u00b7ge", "stund", "es", "um", "uns", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da sprach das feindliche Verlangen:", "tokens": ["Da", "sprach", "das", "feind\u00b7li\u00b7che", "Ver\u00b7lan\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun jag ich nach, und will sie fangen,", "tokens": ["Nun", "jag", "ich", "nach", ",", "und", "will", "sie", "fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nun ist in meiner Hand ihr Gut;", "tokens": ["Nun", "ist", "in", "mei\u00b7ner", "Hand", "ihr", "Gut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich aber k\u00fchle meinen Muth,", "tokens": ["Ich", "a\u00b7ber", "k\u00fch\u00b7le", "mei\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und meine Hand soll sie verderben,", "tokens": ["Und", "mei\u00b7ne", "Hand", "soll", "sie", "ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich ziehe das gewetzte Schwerd,", "tokens": ["Ich", "zie\u00b7he", "das", "ge\u00b7wetz\u00b7te", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Durch dieses soll der Haufe sterben,", "tokens": ["Durch", "die\u00b7ses", "soll", "der", "Hau\u00b7fe", "ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDS", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nichts anders ist die Rotte werth.", "tokens": ["Nichts", "an\u00b7ders", "ist", "die", "Rot\u00b7te", "werth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "VAFIN", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Allein da bliesen deine Winde,", "tokens": ["Al\u00b7lein", "da", "blie\u00b7sen", "dei\u00b7ne", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gleich bedeckte die\u00df Gesinde", "tokens": ["Und", "gleich", "be\u00b7deck\u00b7te", "die\u00df", "Ge\u00b7sin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PDS", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Meer, so \u00fcber sie sich go\u00df,", "tokens": ["Das", "Meer", ",", "so", "\u00fc\u00b7ber", "sie", "sich", "go\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "APPR", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wieder als vorhero flo\u00df;", "tokens": ["Und", "wie\u00b7der", "als", "vor\u00b7he\u00b7ro", "flo\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADV", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Jhr pl\u00f6tzlich Sinken war noch gr\u00f6sser,", "tokens": ["Ihr", "pl\u00f6tz\u