{"dta.poem.21356": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput XIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.71", "en:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des M\u00f6rders Todesurtheil war", "tokens": ["Des", "M\u00f6r\u00b7ders", "To\u00b7des\u00b7urt\u00b7heil", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Genagelt am Weidenstamme;", "tokens": ["Ge\u00b7na\u00b7gelt", "am", "Wei\u00b7dens\u00b7tam\u00b7me", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das haben die R\u00e4cher der Vehme gethan \u2013", "tokens": ["Das", "ha\u00b7ben", "die", "R\u00e4\u00b7cher", "der", "Veh\u00b7me", "ge\u00b7than", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Sonne, du klagende Flamme!", "tokens": ["Son\u00b7ne", ",", "du", "kla\u00b7gen\u00b7de", "Flam\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne war Kl\u00e4ger, sie hatte bewirkt,", "tokens": ["Die", "Son\u00b7ne", "war", "Kl\u00e4\u00b7ger", ",", "sie", "hat\u00b7te", "be\u00b7wirkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Da\u00df man den M\u00f6rder verdamme.", "tokens": ["Da\u00df", "man", "den", "M\u00f6r\u00b7der", "ver\u00b7dam\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Otilie hatte sterbend geschrien:", "tokens": ["O\u00b7ti\u00b7lie", "hat\u00b7te", "ster\u00b7bend", "ge\u00b7schri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Sonne, du klagende Flamme!", "tokens": ["Son\u00b7ne", ",", "du", "kla\u00b7gen\u00b7de", "Flam\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Und denk ich des Liedes, so denk\u2019 ich auch", "tokens": ["Und", "denk", "ich", "des", "Lie\u00b7des", ",", "so", "denk'", "ich", "auch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Der Amme, der lieben Alten;", "tokens": ["Der", "Am\u00b7me", ",", "der", "lie\u00b7ben", "Al\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich sehe wieder ihr braunes Gesicht,", "tokens": ["Ich", "se\u00b7he", "wie\u00b7der", "ihr", "brau\u00b7nes", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mit allen Runzeln und Falten.", "tokens": ["Mit", "al\u00b7len", "Run\u00b7zeln", "und", "Fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie war geboren im M\u00fcnsterland,", "tokens": ["Sie", "war", "ge\u00b7bo\u00b7ren", "im", "M\u00fcns\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und wu\u00dfte, in gro\u00dfer Menge,", "tokens": ["Und", "wu\u00df\u00b7te", ",", "in", "gro\u00b7\u00dfer", "Men\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gespenstergeschichten, grausenhaft,", "tokens": ["Ge\u00b7spens\u00b7ter\u00b7ge\u00b7schich\u00b7ten", ",", "grau\u00b7sen\u00b7haft", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und M\u00e4hrchen und Volksges\u00e4nge.", "tokens": ["Und", "M\u00e4hr\u00b7chen", "und", "Volks\u00b7ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}