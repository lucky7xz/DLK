tokens": ["Und", "vom", "Va\u00b7let\u00b7schu\u00df", "kracht", "die", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es ist die allerletzte Stunde,", "tokens": ["Es", "ist", "die", "al\u00b7ler\u00b7letz\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch nicht das allerletzte Weh.", "tokens": ["Doch", "nicht", "das", "al\u00b7ler\u00b7letz\u00b7te", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Wimpel weht mit frohen Gr\u00fc\u00dfen", "tokens": ["Der", "Wim\u00b7pel", "weht", "mit", "fro\u00b7hen", "Gr\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verst\u00e4ndlich von des Mastes Knauf;", "tokens": ["Ver\u00b7st\u00e4nd\u00b7lich", "von", "des", "Mas\u00b7tes", "Knauf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Noch einmal sink' ich Dir zu F\u00fc\u00dfen,", "tokens": ["Noch", "ein\u00b7mal", "sink'", "ich", "Dir", "zu", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch einmal seh' ich zu Dir auf.", "tokens": ["Noch", "ein\u00b7mal", "seh'", "ich", "zu", "Dir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich hab' es lang' genug ertragen,", "tokens": ["Ich", "hab'", "es", "lang'", "ge\u00b7nug", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stunde fliegt mit Schwalbenflug,", "tokens": ["Die", "Stun\u00b7de", "fliegt", "mit", "Schwal\u00b7ben\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich mu\u00df Dir alles, alles sagen,", "tokens": ["Ich", "mu\u00df", "Dir", "al\u00b7les", ",", "al\u00b7les", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "$,", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was ich f\u00fcr Dich im Herzen trug.", "tokens": ["Was", "ich", "f\u00fcr", "Dich", "im", "Her\u00b7zen", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df nicht, ob Du mich verstanden,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ob", "Du", "mich", "ver\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich wei\u00df nicht, ob Du mich erh\u00f6rt,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "ob", "Du", "mich", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob meiner Lieder wildes Branden", "tokens": ["Ob", "mei\u00b7ner", "Lie\u00b7der", "wil\u00b7des", "Bran\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich hat ger\u00fchrt, Dich hat emp\u00f6rt.", "tokens": ["Dich", "hat", "ge\u00b7r\u00fchrt", ",", "Dich", "hat", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich wei\u00df es nicht, in welche Fernen", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "in", "wel\u00b7che", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich bald von Dir die Woge rollt;", "tokens": ["Mich", "bald", "von", "Dir", "die", "Wo\u00b7ge", "rollt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das aber m\u00f6cht' ich wissen lernen,", "tokens": ["Das", "a\u00b7ber", "m\u00f6cht'", "ich", "wis\u00b7sen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob Du mir jemals wohl gewollt,", "tokens": ["Ob", "Du", "mir", "je\u00b7mals", "wohl", "ge\u00b7wollt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VMPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ob Dich ein einzig Wort ersch\u00fcttert,", "tokens": ["Ob", "Dich", "ein", "ein\u00b7zig", "Wort", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ob Dich ein einz'ger Blick durchdrang,", "tokens": ["Ob", "Dich", "ein", "einz'\u00b7ger", "Blick", "durch\u00b7drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob Dich von mir ein Traum umzittert,", "tokens": ["Ob", "Dich", "von", "mir", "ein", "Traum", "um\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob Dir ein Lied zum Herzen klang.", "tokens": ["Ob", "Dir", "ein", "Lied", "zum", "Her\u00b7zen", "klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Ob Dir mein Werben nicht verha\u00dft;", "tokens": ["Ob", "Dir", "mein", "Wer\u00b7ben", "nicht", "ver\u00b7ha\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df mich die gr\u00fcne Flagge hissen,", "tokens": ["La\u00df", "mich", "die", "gr\u00fc\u00b7ne", "Flag\u00b7ge", "his\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hoffnungsflagge, auf den Mast!", "tokens": ["Die", "Hoff\u00b7nungs\u00b7flag\u00b7ge", ",", "auf", "den", "Mast", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und wie der K\u00e4mpfer speerdurchstochen", "tokens": ["Und", "wie", "der", "K\u00e4mp\u00b7fer", "speer\u00b7durch\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich in die Fahne h\u00fcllt hinein,", "tokens": ["Sich", "in", "die", "Fah\u00b7ne", "h\u00fcllt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So soll, wenn einst dies Herz gebrochen,", "tokens": ["So", "soll", ",", "wenn", "einst", "dies", "Herz", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUS", "ADV", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Bahrtuch diese Flagge sein.", "tokens": ["Mein", "Bahr\u00b7tuch", "die\u00b7se", "Flag\u0