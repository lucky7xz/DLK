,", "ob", "Du", "mich", "er\u00b7h\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ob meiner Lieder wildes Branden", "tokens": ["Ob", "mei\u00b7ner", "Lie\u00b7der", "wil\u00b7des", "Bran\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich hat ger\u00fchrt, Dich hat emp\u00f6rt.", "tokens": ["Dich", "hat", "ge\u00b7r\u00fchrt", ",", "Dich", "hat", "em\u00b7p\u00f6rt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ich wei\u00df es nicht, in welche Fernen", "tokens": ["Ich", "wei\u00df", "es", "nicht", ",", "in", "wel\u00b7che", "Fer\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "APPR", "PWAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich bald von Dir die Woge rollt;", "tokens": ["Mich", "bald", "von", "Dir", "die", "Wo\u00b7ge", "rollt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das aber m\u00f6cht' ich wissen lernen,", "tokens": ["Das", "a\u00b7ber", "m\u00f6cht'", "ich", "wis\u00b7sen", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VMFIN", "PPER", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob Du mir jema