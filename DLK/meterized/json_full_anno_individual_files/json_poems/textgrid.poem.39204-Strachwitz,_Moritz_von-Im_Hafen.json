, "pos": ["KOUS", "PPER", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00df mich die gr\u00fcne Flagge hissen,", "tokens": ["La\u00df", "mich", "die", "gr\u00fc\u00b7ne", "Flag\u00b7ge", "his\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Hoffnungsflagge, auf den Mast!", "tokens": ["Die", "Hoff\u00b7nungs\u00b7flag\u00b7ge", ",", "auf", "den", "Mast", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.18": {"line.1": {"text": "Und wie der K\u00e4mpfer speerdurchstochen", "tokens": ["Und", "wie", "der", "K\u00e4mp\u00b7fer", "speer\u00b7durch\u00b7sto\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich in die Fahne h\u00fcllt hinein,", "tokens": ["Sich", "in", "die", "Fah\u00b7ne", "h\u00fcllt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So soll, wenn einst dies Herz gebrochen,", "tokens": ["So", "soll", ",", "wenn", "einst", "dies", "Herz", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "$,", "KOUS", "ADV", "PDS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mein Bahrtuch diese Flagge sein.", "tokens": ["Mein", "Bahr\u00b7tuch", "die\u00b7se", "Flag\u00b7ge", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "