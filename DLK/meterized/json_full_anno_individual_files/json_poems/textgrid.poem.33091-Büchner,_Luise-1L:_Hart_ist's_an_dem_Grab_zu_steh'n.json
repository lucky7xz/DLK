{"textgrid.poem.33091": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hart ist's an dem Grab zu steh'n", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hart ist's an dem Grab zu steh'n", "tokens": ["Hart", "ist's", "an", "dem", "Grab", "zu", "steh'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "APPR", "ART", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Derer, die du hei\u00df geliebet,", "tokens": ["De\u00b7rer", ",", "die", "du", "hei\u00df", "ge\u00b7lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Hart auch, wie am Fels der Zeit", "tokens": ["Hart", "auch", ",", "wie", "am", "Fels", "der", "Zeit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PWAV", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Traum um Traum in Nichts zerstiebet.", "tokens": ["Traum", "um", "Traum", "in", "Nichts", "zer\u00b7stie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Bittrer als des Todes Raub,", "tokens": ["Bit\u00b7trer", "als", "des", "To\u00b7des", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und was kalt die Zeit entwendet,", "tokens": ["Und", "was", "kalt", "die", "Zeit", "ent\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ist's, wenn du dein best Gef\u00fchl", "tokens": ["Ist's", ",", "wenn", "du", "dein", "best", "Ge\u00b7f\u00fchl"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PPER", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "An Unw\u00fcrdige verschwendet.", "tokens": ["An", "Un\u00b7w\u00fcr\u00b7di\u00b7ge", "ver\u00b7schwen\u00b7det", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie ein Bettler stehst du da,", "tokens": ["Wie", "ein", "Bett\u00b7ler", "stehst", "du", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sein Alles hingegeben,", "tokens": ["Der", "sein", "Al\u00b7les", "hin\u00b7ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VAINF", "PIS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem nichts blieb von seinem Schatz,", "tokens": ["Dem", "nichts", "blieb", "von", "sei\u00b7nem", "Schatz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Als das nackte, arme Leben.", "tokens": ["Als", "das", "nack\u00b7te", ",", "ar\u00b7me", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie, von roher Hand gest\u00fcrzt,", "tokens": ["Wie", ",", "von", "ro\u00b7her", "Hand", "ge\u00b7st\u00fcrzt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt ein G\u00f6tterbild im Staube,", "tokens": ["Liegt", "ein", "G\u00f6t\u00b7ter\u00b7bild", "im", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Also ist ein Tr\u00fcmmerhauf'", "tokens": ["Al\u00b7so", "ist", "ein", "Tr\u00fcm\u00b7mer\u00b7hauf'"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deines Herzens sch\u00f6nster Glaube!", "tokens": ["Dei\u00b7nes", "Her\u00b7zens", "sch\u00f6ns\u00b7ter", "Glau\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Neue Rosen bringt die Zeit,", "tokens": ["Neu\u00b7e", "Ro\u00b7sen", "bringt", "die", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frisches Gr\u00fcn das Grab umkleidet,", "tokens": ["Fri\u00b7sches", "Gr\u00fcn", "das", "Grab", "um\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aber \u00f6d' bleibt dieser Platz", "tokens": ["A\u00b7ber", "\u00f6d'", "bleibt", "die\u00b7ser", "