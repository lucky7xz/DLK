{"textgrid.poem.61963": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gold'ne Sonne mit den Strahlen,", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gold'ne Sonne mit den Strahlen,", "tokens": ["Gold'\u00b7ne", "Son\u00b7ne", "mit", "den", "Strah\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Komm und nimm mich an Dein Herz", "tokens": ["Komm", "und", "nimm", "mich", "an", "Dein", "Herz"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVIMP", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und von Deinem Licht getragen,", "tokens": ["Und", "von", "Dei\u00b7nem", "Licht", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Steige mit mir himmelw\u00e4rts!", "tokens": ["Stei\u00b7ge", "mit", "mir", "him\u00b7mel\u00b7w\u00e4rts", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Zeige mir dort Deine Wesen,", "tokens": ["Zei\u00b7ge", "mir", "dort", "Dei\u00b7ne", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Deinen gro\u00dfen Wunderraum,", "tokens": ["Dei\u00b7nen", "gro\u00b7\u00dfen", "Wun\u00b7der\u00b7raum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und damit ich's nicht verrate,", "tokens": ["Und", "da\u00b7mit", "ich's", "nicht", "ver\u00b7ra\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PIS", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df mich's schauen wie im Traum!", "tokens": ["La\u00df", "mich's", "schau\u00b7en", "wie", "im", "Traum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "KOKOM", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Oder nimm mich in die H\u00f6he", "tokens": ["O\u00b7der", "nimm", "mich", "in", "die", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nur ein tausend Meilen fern,", "tokens": ["Nur", "ein", "tau\u00b7send", "Mei\u00b7len", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "CARD", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df ich ihn von dort aus sehe:", "tokens": ["Da\u00df", "ich", "ihn", "von", "dort", "aus", "se\u00b7he", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "ADV", "APZR", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Unsern kleinen Erdenstern.", "tokens": ["Un\u00b7sern", "klei\u00b7nen", "Er\u00b7dens\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Gold'ne Sonne mit den Str