{"textgrid.poem.66748": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seltene Wollust sog ich aus jenen Gef\u00fchlen,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seltene Wollust sog ich aus jenen Gef\u00fchlen,", "tokens": ["Sel\u00b7te\u00b7ne", "Wol\u00b7lust", "sog", "ich", "aus", "je\u00b7nen", "Ge\u00b7f\u00fch\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die den T\u00e4uschern der Worte verschlossen sind \u2013", "tokens": ["Die", "den", "T\u00e4u\u00b7schern", "der", "Wor\u00b7te", "ver\u00b7schlos\u00b7sen", "sind", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "ART", "NN", "VVPP", "VAFIN", "$("], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Meine Seele barg sich in stillen Asylen,", "tokens": ["Mei\u00b7ne", "See\u00b7le", "barg", "sich", "in", "stil\u00b7len", "A\u00b7sy\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Fern dem tauben Schall und dem hohlen Wind.", "tokens": ["Fern", "dem", "tau\u00b7ben", "Schall", "und", "dem", "hoh\u00b7len", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, aus Bitternissen des Lebens quollen", "tokens": ["Ach", ",", "aus", "Bit\u00b7ter\u00b7nis\u00b7sen", "des", "Le\u00b7bens", "quol\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "APPR", "NN", "ART", "NN", "VVINF"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "S\u00fc\u00dfe Zufluchtstunden der D\u00e4mmerung,", "tokens": ["S\u00fc\u00b7\u00dfe", "Zu\u00b7flucht\u00b7stun\u00b7den", "der", "D\u00e4m\u00b7me\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Land der scheinenden L\u00fcge lag verschollen,", "tokens": ["Land", "der", "schei\u00b7nen\u00b7den", "L\u00fc\u00b7ge", "lag", "ver\u00b7schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Und es kam wie Wonne der Heiligung.", "tokens": ["Und", "es", "kam", "wie", "Won\u00b7ne", "der", "Hei\u00b7li\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KOKOM", "NN", "ART", "NN", "$."], "meter": "--+-+--+--", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Alles Leid der tiefer erschlossenen Dinge,", "tokens": ["Al\u00b7les", "Leid", "der", "tie\u00b7fer", "er\u00b7schlos\u00b7se\u00b7nen", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Alles l\u00f6sende Lachen der letzten Macht,", "tokens": ["Al\u00b7les", "l\u00f6\u00b7sen\u00b7de", "La\u00b7chen", "der", "letz\u00b7ten", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Alles leise Schweben auf ewiger Schwinge", "tokens": ["Al\u00b7les", "lei\u00b7se", "Schwe\u00b7ben", "auf", "e\u00b7wi\u00b7ger", "Schwin\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Jenseits mutzermalmender Niedertracht \u2013", "tokens": ["Jen\u00b7seits", "mut\u00b7zer\u00b7mal\u00b7men\u00b7der", "Nie\u00b7der\u00b7tracht", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Was wie Schluchzen an quellenden Mutterbr\u00fcsten,", "tokens": ["Was", "wie", "Schluch\u00b7zen", "an", "quel\u00b7len\u00b7den", "Mut\u00b7ter\u00b7br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOKOM", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Was wie Jauchzen der siegenden Liebe war,", "tokens": ["Was", "wie", "Jauch\u00b7zen", "der", "sie\u00b7gen\u00b7den", "Lie\u00b7be", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "KOKOM", "NN", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Wo die Seelen heimlich-erhaben sich k\u00fc\u00dften", "tokens": ["Wo", "die", "See\u00b7len", "heim\u00b7lich\u00b7er\u00b7ha\u00b7ben", "sich", "k\u00fc\u00df\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PRF", "VVFIN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vor dem unverletzlichen Brautaltar ...", "tokens": ["Vor"