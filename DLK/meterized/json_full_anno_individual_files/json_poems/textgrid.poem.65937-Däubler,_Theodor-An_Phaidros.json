{"textgrid.poem.65937": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "An Phaidros", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Taumbild, \u00fcber mir in Schwebe,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Taum\u00b7bild", ",", "\u00fc\u00b7ber", "mir", "in", "Schwe\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vielleicht mein Zwilling, sternhin abgeneigt", "tokens": ["Viel\u00b7leicht", "mein", "Zwil\u00b7ling", ",", "stern\u00b7hin", "ab\u00b7ge\u00b7neigt"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von diesem Wesenswuchs, der sich verzweigt,", "tokens": ["Von", "die\u00b7sem", "We\u00b7sens\u00b7wuchs", ",", "der", "sich", "ver\u00b7zweigt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch fromm macht, da\u00df der Blick sich dirzu hebe,", "tokens": ["Doch", "fromm", "macht", ",", "da\u00df", "der", "Blick", "sich", "dir\u00b7zu", "he\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,", "KOUS", "ART", "NN", "PRF", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich wei\u00df wohl, wie ich flehend elend lebe!", "tokens": ["Ich", "wei\u00df", "wohl", ",", "wie", "ich", "fle\u00b7hend", "e\u00b7lend", "le\u00b7be", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Bruderflug, der immer steiler steigt,", "tokens": ["Ein", "Bru\u00b7der\u00b7flug", ",", "der", "im\u00b7mer", "stei\u00b7ler", "steigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich bald als Ares, ein Gestirn, umreigt,", "tokens": ["Mich", "bald", "als", "A\u00b7res", ",", "ein", "Ge\u00b7stirn", ",", "um\u00b7reigt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "NN", "$,", "ART", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Entkreist, weil ich dem Schmachten mich ergebe.", "tokens": ["Ent\u00b7kreist", ",", "weil", "ich", "dem", "Schmach\u00b7ten", "mich", "er\u00b7ge\u00b7be", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auf seinem Boden weilt der stolze Bauer,", "tokens": ["Auf", "sei\u00b7nem", "Bo\u00b7den", "weilt", "der", "stol\u00b7ze", "Bau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bringt blo\u00df den G\u00f6ttern hohe Opfer dar,", "tokens": ["Bringt", "blo\u00df", "den", "G\u00f6t\u00b7tern", "ho\u00b7he", "Op\u00b7fer", "dar", ","], "token_info": ["word", "wo