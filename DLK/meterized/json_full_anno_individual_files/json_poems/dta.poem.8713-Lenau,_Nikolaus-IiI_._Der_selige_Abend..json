{"dta.poem.8713": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "IiI .  \n  Der selige Abend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Schnell versammelt um die Felsen", "tokens": ["Schnell", "ver\u00b7sam\u00b7melt", "um", "die", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben Wolken sich und Winde,", "tokens": ["Ha\u00b7ben", "Wol\u00b7ken", "sich", "und", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PRF", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um den neuen Gast zu gr\u00fc\u00dfen,", "tokens": ["Um", "den", "neu\u00b7en", "Gast", "zu", "gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seines Kummers Spielgesinde;", "tokens": ["Sei\u00b7nes", "Kum\u00b7mers", "Spiel\u00b7ge\u00b7sin\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ausgeloschen ist das Mondlicht", "tokens": ["Aus\u00b7ge\u00b7lo\u00b7schen", "ist", "das", "Mond\u00b7licht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und der Sterne helles Flimmern,", "tokens": ["Und", "der", "Ster\u00b7ne", "hel\u00b7les", "Flim\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die enge Fensterspalte", "tokens": ["Durch", "die", "en\u00b7ge", "Fens\u00b7ter\u00b7spal\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00f6rt der Prinz die L\u00fcfte wimmern.", "tokens": ["H\u00f6rt", "der", "Prinz", "die", "L\u00fcf\u00b7te", "wim\u00b7mern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Traurig sinnend blickt Johannes", "tokens": ["Trau\u00b7rig", "sin\u00b7nend", "blickt", "Jo\u00b7han\u00b7nes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VVFIN", "NE"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In die dunkle Ferne nieder,", "tokens": ["In", "die", "dunk\u00b7le", "Fer\u00b7ne", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es flattern seine Locken", "tokens": ["Und", "es", "flat\u00b7tern", "sei\u00b7ne", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Windgeschaukelt hin und wieder,", "tokens": ["Wind\u00b7ge\u00b7schau\u00b7kelt", "hin", "und", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Flattern um die blasse Stirne,", "tokens": ["Flat\u00b7tern", "um", "die", "blas\u00b7se", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie das Laub der Trauerweiden", "tokens": ["Wie", "das", "Laub", "der", "Trau\u00b7er\u00b7wei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Um die bleiche Marmortafel", "tokens": ["Um", "die", "blei\u00b7che", "Mar\u00b7mor\u00b7ta\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ueber den begrabnen Freuden.", "tokens": ["Ue\u00b7ber", "den", "be\u00b7grab\u00b7nen", "Freu\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Er gedenket eines Abends,", "tokens": ["Er", "ge\u00b7den\u00b7ket", "ei\u00b7nes", "A\u00b7bends", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eines seligen vor allen,", "tokens": ["Ei\u00b7nes", "se\u00b7li\u00b7gen", "vor", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als in Martigues er gelandet", "tokens": ["Als", "in", "Mar\u00b7ti\u00b7gues", "er", "ge\u00b7lan\u00b7det"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "PPER", "VVPP"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mit den Freunden und Vasallen.", "tokens": ["Mit", "den", "Freun\u00b7den", "und", "Va\u00b7sal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "KON", "NN", "$."], "meter": "--+--+--", "measure": "anapaest.di.plus"}}, "stanza.6": {"line.1": {"text": "Ruhig lag die sturmerprobte", "tokens": ["Ru\u00b7hig", "lag", "die", "stur\u00b7mer\u00b7prob\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Genuesische Galeere,", "tokens": ["Ge\u00b7nu\u00b7e\u00b7si\u00b7sche", "Ga\u00b7lee\u00b7re", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Lustig flogen ihre Wimpel,", "tokens": ["Lus\u00b7tig", "flo\u00b7gen", "ih\u00b7re", "Wim\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und der Tag versank im Meere;", "tokens": ["Und", "der", "Tag", "ver\u00b7sank", "im", "Mee\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}}, "stanza.7": {"line.1": {"text": "Scheidend warf er seine Strahlen", "tokens": ["Schei\u00b7dend", "warf", "er", "sei\u00b7ne", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Wellen bunt Gedr\u00e4nge,", "tokens": ["In", "der", "Wel\u00b7len", "bunt", "Ge\u00b7dr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie ein K\u00f6nig, goldverstreuend,", "tokens": ["Wie", "ein", "K\u00f6\u00b7nig", ",", "gold\u00b7ver\u00b7streu\u00b7end", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Scheidet von der frohen Menge.", "tokens": ["Schei\u00b7det", "von", "der", "fro\u00b7hen", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nach dem Sturme lag die See nun", "tokens": ["Nach", "dem", "Stur\u00b7me", "lag", "die", "See", "nun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6n in ihrer stillen Gr\u00f6\u00dfe,", "tokens": ["Sch\u00f6n", "in", "ih\u00b7rer", "stil\u00b7len", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur noch manchmal an das Ufer", "tokens": ["Nur", "noch", "manch\u00b7mal", "an", "das", "U\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "T\u00f6nten bange Wellenst\u00f6\u00dfe:", "tokens": ["T\u00f6n\u00b7ten", "ban\u00b7ge", "Wel\u00b7len\u00b7st\u00f6\u00b7\u00dfe", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ist auch schon das Auge heiter,", "tokens": ["Ist", "auch", "schon", "das", "Au\u00b7ge", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und verstummt des Mundes Klage;", "tokens": ["Und", "ver\u00b7stummt", "des", "Mun\u00b7des", "Kla\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch es zuckt nach starkem Weinen", "tokens": ["Doch", "es", "zuckt", "nach", "star\u00b7kem", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch das Herz im bangen Schlage.", "tokens": ["Noch", "das", "Herz", "im", "ban\u00b7gen", "Schla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Lieblich war der L\u00fcfte S\u00e4useln", "tokens": ["Lieb\u00b7lich", "war", "der", "L\u00fcf\u00b7te", "S\u00e4u\u00b7seln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nach dem rauhen Sturmestosen,", "tokens": ["Nach", "dem", "rau\u00b7hen", "Stur\u00b7mes\u00b7to\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Auf der Meeresruhe schwebten", "tokens": ["Auf", "der", "Mee\u00b7res\u00b7ru\u00b7he", "schweb\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die Ges\u00e4nge der Matrosen. \u2014 \u2014", "tokens": ["Die", "Ge\u00b7s\u00e4n\u00b7ge", "der", "Mat\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Dicht am Strande, schmuck und wirthlich,", "tokens": ["Dicht", "am", "Stran\u00b7de", ",", "schmuck", "und", "wirth\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Winkt der Gasthof mit dem Schilde", "tokens": ["Winkt", "der", "Gast\u00b7hof", "mit", "dem", "Schil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dreier Lilien, einzukehren", "tokens": ["Drei\u00b7er", "Li\u00b7li\u00b7en", ",", "ein\u00b7zu\u00b7keh\u00b7ren"], "token_info": ["word", "word", "punct", "word"], "pos": ["CARD", "NN", "$,", "VVIZU"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Zu dem sch\u00f6nen Engelbilde:", "tokens": ["Zu", "dem", "sch\u00f6\u00b7nen", "En\u00b7gel\u00b7bil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Klara Hebert, weit gepriesen", "tokens": ["Kla\u00b7ra", "He\u00b7bert", ",", "weit", "ge\u00b7prie\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rings im Lande ob der Bl\u00fcthe", "tokens": ["Rings", "im", "Lan\u00b7de", "ob", "der", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "KOUS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrer Sch\u00f6nheit, weit im Lande", "tokens": ["Ih\u00b7rer", "Sch\u00f6n\u00b7heit", ",", "weit", "im", "Lan\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ob des Herzens Wunderg\u00fcte.", "tokens": ["Ob", "des", "Her\u00b7zens", "Wun\u00b7der\u00b7g\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Laut mit ungest\u00fcmer Freude", "tokens": ["Laut", "mit", "un\u00b7ge\u00b7st\u00fc\u00b7mer", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tritt der Seemann in das Zimmer,", "tokens": ["Tritt", "der", "See\u00b7mann", "in", "das", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dringend heischt er nach dem Becher;", "tokens": ["Drin\u00b7gend", "heischt", "er", "nach", "dem", "Be\u00b7cher", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch sein Muth wird stiller immer:", "tokens": ["Doch", "sein", "Muth", "wird", "stil\u00b7ler", "im\u00b7mer", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Ihm kredenzt der Wirthin Tochter", "tokens": ["Ihm", "kre\u00b7denzt", "der", "Wirt\u00b7hin", "Toch\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Freundlich mit den zarten H\u00e4nden,", "tokens": ["Freund\u00b7lich", "mit", "den", "zar\u00b7ten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er l\u00e4\u00dft den Becher stehen,", "tokens": ["Und", "er", "l\u00e4\u00dft", "den", "Be\u00b7cher", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kann sein Auge nimmer wenden;", "tokens": ["Kann", "sein", "Au\u00b7ge", "nim\u00b7mer", "wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nun sie seinem Blick entschwunden,", "tokens": ["Nun", "sie", "sei\u00b7nem", "Blick", "ent\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trinkt er aus mit raschem Zuge,", "tokens": ["Trinkt", "er", "aus", "mit", "ra\u00b7schem", "Zu\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df sie ihn noch einmal f\u00fclle,", "tokens": ["Da\u00df", "sie", "ihn", "noch", "ein\u00b7mal", "f\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", 