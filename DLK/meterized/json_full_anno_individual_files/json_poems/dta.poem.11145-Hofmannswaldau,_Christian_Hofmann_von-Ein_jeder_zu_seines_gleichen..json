{"dta.poem.11145": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ein jeder zu seines gleichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mops k\u00fc\u00dfte seine magd; sie sprach: Herr! irrt euch nicht,", "tokens": ["Mops", "k\u00fc\u00df\u00b7te", "sei\u00b7ne", "magd", ";", "sie", "sprach", ":", "Herr", "!", "irrt", "euch", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es solte sonst wohin, und ihr kommt ins gesicht.", "tokens": ["Es", "sol\u00b7te", "sonst", "wo\u00b7hin", ",", "und", "ihr", "kommt", "ins", "ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PWAV", "$,", "KON", "PPER", "VVFIN", "APPRART", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}