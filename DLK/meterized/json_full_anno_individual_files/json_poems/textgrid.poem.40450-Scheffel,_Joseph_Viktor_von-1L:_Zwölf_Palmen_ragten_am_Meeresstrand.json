{"textgrid.poem.40450": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zw\u00f6lf Palmen ragten am Meeresstrand", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zw\u00f6lf Palmen ragten am Meeresstrand", "tokens": ["Zw\u00f6lf", "Pal\u00b7men", "rag\u00b7ten", "am", "Mee\u00b7res\u00b7strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um eine alte Zisterne;", "tokens": ["Um", "ei\u00b7ne", "al\u00b7te", "Zis\u00b7ter\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der Wagen knarrte im Ufersand,", "tokens": ["Der", "Wa\u00b7gen", "knarr\u00b7te", "im", "U\u00b7fer\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sonne versank in der Ferne.", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7sank", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.2": {"line.1": {"text": "Still einsam war's. Die Flut begann", "tokens": ["Still", "ein\u00b7sam", "wa\u00b7r'", "s.", "Die", "Flut", "be\u00b7gann"], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "word"], "pos": ["ADJD", "ADJD", "VAFIN", "VVIMP", "ART", "NN", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich im Abendpurpur zu f\u00e4rben,", "tokens": ["Sich", "im", "A\u00b7bend\u00b7pur\u00b7pur", "zu", "f\u00e4r\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Da rannte der Tod mich pl\u00f6tzlich an,", "tokens": ["Da", "rann\u00b7te", "der", "Tod", "mich", "pl\u00f6tz\u00b7lich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Da\u00df ich vermeinte zu sterben.", "tokens": ["Da\u00df", "ich", "ver\u00b7mein\u00b7te", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Herzschlag stockte, es stockte das Blut,", "tokens": ["Der", "Herz\u00b7schlag", "stock\u00b7te", ",", "es", "stock\u00b7te", "das", "Blut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Glieder wollten ermatten,", "tokens": ["Die", "Glie\u00b7der", "woll\u00b7ten", "er\u00b7mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Freunde trugen mit tr\u00fcbem Mut", "tokens": ["Die", "Freun\u00b7de", "tru\u00b7gen", "mit", "tr\u00fc\u00b7bem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hinab mich in k\u00fchlenden Schatten.", "tokens": ["Hin\u00b7ab", "mich", "in", "k\u00fch\u00b7len\u00b7den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Da sprach ich ruhig: \u00bbO la\u00dft mich hier,", "tokens": ["Da", "sprach", "ich", "ru\u00b7hig", ":", "\u00bb", "O", "la\u00dft", "mich", "hier", ","], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$.", "$(", "NE", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will nichts von der Heimfahrt mehr wissen;", "tokens": ["Will", "nichts", "von", "der", "Heim\u00b7fahrt", "mehr", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Sie fragten dort dr\u00fcben noch nie nach mir,", "tokens": ["Sie", "frag\u00b7ten", "dort", "dr\u00fc\u00b7ben", "noch", "nie", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADV", "APPR", "PPER", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "K\u00f6nnen auch meine Asche vermissen.", "tokens": ["K\u00f6n\u00b7nen", "auch", "mei\u00b7ne", "A\u00b7sche", "ver\u00b7mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.5": {"line.1": {"text": "Hier umgl\u00e4nzt mich die alte blaugoldne Pracht,", "tokens": ["Hier", "um\u00b7gl\u00e4nzt", "mich", "die", "al\u00b7te", "blau\u00b7gold\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Die der Jugend Leid mir vers\u00fc\u00dfte,", "tokens": ["Die", "der", "Ju\u00b7gend", "Leid", "mir", "ver\u00b7s\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hier murmelt das Meer so tr\u00e4umerisch sacht,", "tokens": ["Hier", "mur\u00b7melt", "das", "Meer", "so", "tr\u00e4u\u00b7me\u00b7risch", "sacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Als ob Sorrento mich gr\u00fc\u00dfte.", "tokens": ["Als", "ob", "Sor\u00b7ren\u00b7to", "mich", "gr\u00fc\u00df\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "NE", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier umstehn, eine alt befreundete Schar,", "tokens": ["Hier", "um\u00b7stehn", ",", "ei\u00b7ne", "alt", "be\u00b7freun\u00b7de\u00b7te", "Schar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Mein Schmerzenslager die Palmen,", "tokens": ["Mein", "Schmer\u00b7zens\u00b7la\u00b7ger", "die", "Pal\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Im F\u00e4cherdach rauscht's voll und klar", "tokens": ["Im", "F\u00e4\u00b7cher\u00b7dach", "rauscht's", "voll", "und", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie tr\u00f6stende Sterbepsalmen.", "tokens": ["Wie", "tr\u00f6s\u00b7ten\u00b7de", "Ster\u00b7be\u00b7psal\u00b7men", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Hier fand ich Sch\u00f6nheit und Liebe und Gl\u00fcck", "tokens": ["Hier", "fand", "ich", "Sch\u00f6n\u00b7heit", "und", "Lie\u00b7be", "und", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Fern allen Toren und Laffen;", "tokens": ["Fern", "al\u00b7len", "To\u00b7ren", "und", "Laf\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Gern kehrt die Seele von hier zur\u00fcck", "tokens": ["Gern", "kehrt", "die", "See\u00b7le", "von", "hier", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zu dem, der das Sch\u00f6ne geschaffen.\u00ab", "tokens": ["Zu", "dem", ",", "der", "das", "Sch\u00f6\u00b7ne", "ge\u00b7schaf\u00b7fen", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.8": {"line.1": {"text": "Der Tod aber rief von der Stra\u00dfe her:", "tokens": ["Der", "Tod", "a\u00b7ber", "rief", "von", "der", "Stra\u00b7\u00dfe", "her", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbgemach, das hat keine Eile;", "tokens": ["\u00bb", "ge\u00b7mach", ",", "das", "hat", "kei\u00b7ne", "Ei\u00b7le", ";"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "PDS", "VAFIN", "PIAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch immer magst du im Lebensmeer", "tokens": ["Noch", "im\u00b7mer", "magst", "du", "im", "Le\u00b7bens\u00b7meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Abzappeln dich eine Weile.", "tokens": ["Ab\u00b7zap\u00b7peln", "dich", "ei\u00b7ne", "Wei\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Kein \u00fcbler Geschmack: so am Palmenstrand", "tokens": ["Kein", "\u00fcb\u00b7ler", "Ge\u00b7schmack", ":", "so", "am", "Pal\u00b7mens\u00b7trand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "$.", "ADV", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ein Grab in italischer Erden!", "tokens": ["Ein", "Grab", "in", "i\u00b7ta\u00b7li\u00b7scher", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du mu\u00dft, o Freund, erst im deutschen Land", "tokens": ["Du", "mu\u00dft", ",", "o", "Freund", ",", "erst", "im", "deut\u00b7schen", "Land"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "FM", "NN", "$,", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Lebendig zur Mumie werden.\u00ab", "tokens": ["Le\u00b7ben\u00b7dig", "zur", "Mu\u00b7mie", "wer\u00b7den", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "APPRART", "NN", "VAINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Zw\u00f6lf Palmen ragten am Meeresstrand", "tokens": ["Zw\u00f6lf", "Pal\u00b7men", "rag\u00b7ten", "am", "Mee\u00b7res\u00b7strand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um eine alte Zisterne;", "tokens": ["Um", "ei\u00b7ne", "al\u00b7te", "Zis\u00b7ter\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der Wagen knarrte im Ufersand,", "tokens": ["Der", "Wa\u00b7gen", "knarr\u00b7te", "im", "U\u00b7fer\u00b7sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sonne versank in der Ferne.", "tokens": ["Die", "Son\u00b7ne", "ver\u00b7sank", "in", "der", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.11": {"line.1": 