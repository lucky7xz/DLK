, "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": "Dein jrrthum ist nicht schlecht/ ", "tokens": ["Dein", "jrr\u00b7thum", "ist", "nicht", "schlecht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dann denck jhm nach/ du zeuchst/ wilt du die ", "tokens": ["Dann", "denck", "jhm", "nach", "/", "du", "zeuchst", "/", "wilt", "du", "die"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "APPR", "$(", "PPER", "VVFIN", "$(", "VMFIN", "PPER", "ART"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Vnd Sie vor diesen ", "tokens": ["Vnd", "Sie", "vor", "die\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PDAT"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}}}}}