{"textgrid.poem.56536": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist so dunkel, als die Nacht,", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist so dunkel, als die Nacht,", "tokens": ["Du", "bist", "so", "dun\u00b7kel", ",", "als", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sternenlos sie ihren schwarzen Mantel breitet.", "tokens": ["Wenn", "ster\u00b7nen\u00b7los", "sie", "ih\u00b7ren", "schwar\u00b7zen", "Man\u00b7tel", "brei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das letzte Licht ertr\u00e4nkt in tiefstem Schacht;", "tokens": ["Das", "letz\u00b7te", "Licht", "er\u00b7tr\u00e4nkt", "in", "tiefs\u00b7tem", "Schacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nein, dunkler noch, im grausten Dunkel gleitet", "tokens": ["Nein", ",", "dunk\u00b7ler", "noch", ",", "im", "graus\u00b7ten", "Dun\u00b7kel", "glei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADJA", "ADV", "$,", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dein wei\u00dfes, bleiches Angesicht", "tokens": ["Dein", "wei\u00b7\u00dfes", ",", "blei\u00b7ches", "An\u00b7ge\u00b7si