{"textgrid.poem.43217": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das Pantherfell um die athlet'schen Glieder", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das Pantherfell um die athlet'schen Glieder", "tokens": ["Das", "Pan\u00b7ther\u00b7fell", "um", "die", "ath\u00b7let'\u00b7schen", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Weinlaub um das dunkle Haupt geschlungen,", "tokens": ["Und", "Wein\u00b7laub", "um", "das", "dunk\u00b7le", "Haupt", "ge\u00b7schlun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Betritt er lachend jenen K\u00e4fig wieder,", "tokens": ["Be\u00b7tritt", "er", "la\u00b7chend", "je\u00b7nen", "K\u00e4\u00b7fig", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In dem die L\u00f6win haust mit ihren Jungen.", "tokens": ["In", "dem", "die", "L\u00f6\u00b7win", "haust", "mit", "ih\u00b7ren", "Jun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das schwere Eisengitter rasselt nieder,", "tokens": ["Das", "schwe\u00b7re", "Ei\u00b7sen\u00b7git\u00b7ter", "ras\u00b7selt", "nie\u00b7der", ","], "token_info": ["word", "word", "word"