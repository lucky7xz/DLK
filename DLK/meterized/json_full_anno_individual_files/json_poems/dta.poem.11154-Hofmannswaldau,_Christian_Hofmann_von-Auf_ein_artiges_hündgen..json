{"dta.poem.11154": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein artiges h\u00fcndgen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Labelle! du bist werth, da\u00df England dich gezeugt,", "tokens": ["La\u00b7bel\u00b7le", "!", "du", "bist", "werth", ",", "da\u00df", "En\u00b7gland", "dich", "ge\u00b7zeugt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VAFIN", "ADJD", "$,", "KOUS", "NE", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df dich Bolonien mit seiner milch ges\u00e4ugt.", "tokens": ["Da\u00df", "dich", "Bo\u00b7lo\u00b7ni\u00b7en", "mit", "sei\u00b7ner", "milch", "ge\u00b7s\u00e4ugt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NE", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ja w\u00e4r nicht allbereit am himmel ein solch zeichen,", "tokens": ["Ja", "w\u00e4r", "nicht", "all\u00b7be\u00b7reit", "am", "him\u00b7mel", "ein", "solch", "zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So soltest du zu erst dergleichen stell erreichen.", "tokens": ["So", "sol\u00b7test", "du", "zu", "erst", "derg\u00b7lei\u00b7chen", "stell", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "ADV", "PIS", "AD