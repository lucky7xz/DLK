{"dta.poem.12370": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Rosmarien .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1806", "urn": "urn:nbn:de:kobv:b4-20090519157", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wollt die Jungfrau fr\u00fch aufstehn,               ", "tokens": ["Es", "wollt", "die", "Jung\u00b7frau", "fr\u00fch", "auf\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt in des Vaters Garten gehn,", "tokens": ["Wollt", "in", "des", "Va\u00b7ters", "Gar\u00b7ten", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Roth R\u00f6slein wollt sie brechen ab,", "tokens": ["Roth", "R\u00f6s\u00b7lein", "wollt", "sie", "bre\u00b7chen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Davon wollt sie sich machen,", "tokens": ["Da\u00b7von", "wollt", "sie", "sich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Kr\u00e4nzelein wohl sch\u00f6n.", "tokens": ["Ein", "Kr\u00e4n\u00b7zel\u00b7ein", "wohl", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sollt ihr Hochzeitskr\u00e4nzlein seyn:", "tokens": ["Es", "sollt", "ihr", "Hoch\u00b7zeits\u00b7kr\u00e4nz\u00b7lein", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edem feinen Knab, dem Knaben mein,", "tokens": ["\u201e", "dem", "fei\u00b7nen", "Knab", ",", "dem", "Kna\u00b7ben", "mein", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eihr R\u00f6slein roth, ich brech euch ab,", "tokens": ["\u201e", "ihr", "R\u00f6s\u00b7lein", "roth", ",", "ich", "brech", "euch", "ab", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADJD", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201edavon will ich mir winden,", "tokens": ["\u201e", "da\u00b7von", "will", "ich", "mir", "win\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PAV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201eein Kr\u00e4nzelein so sch\u00f6n.\u201c", "tokens": ["\u201e", "ein", "Kr\u00e4n\u00b7zel\u00b7ein", "so", "sch\u00f6n", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Sie gieng im Gr\u00fcnen her und hin,", "tokens": ["Sie", "gieng", "im", "Gr\u00fc\u00b7nen", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt R\u00f6slein fand sie Rosmarien:", "tokens": ["Statt", "R\u00f6s\u00b7lein", "fand", "sie", "Ros\u00b7ma\u00b7ri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u201eso bist du, mein Getreuer hin!", "tokens": ["\u201e", "so", "bist", "du", ",", "mein", "Ge\u00b7treu\u00b7er", "hin", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201ekein R\u00f6slein ist zu finden,", "tokens": ["\u201e", "kein", "R\u00f6s\u00b7lein", "ist", "zu", "fin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u201ekein Kr\u00e4nzelein so sch\u00f6n.\u201c", "tokens": ["\u201e", "kein", "Kr\u00e4n\u00b7zel\u00b7ein", "so", "sch\u00f6n", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "NN", "ADV", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie gieng im Garten her und hin,", "tokens": ["Sie", "gieng", "im", "Gar\u00b7ten", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt R\u00f6slein brach sie Rosmarien:", "tokens": ["Statt", "R\u00f6s\u00b7lein", "brach", "sie", "Ros\u00b7ma\u00b7ri\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "PPER", "NE", "$."], "meter": "-+-+-----", "measure": "unknown.measure.di"}, "line.3": {"text": "\u201edas nimm du, mein Getreuer, hin!", "tokens": ["\u201e", "das", "nimm", "du", ",", "mein", "Ge\u00b7treu\u00b7er", ",", "hin", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "\u201elieg bei dir unter Linden,", "tokens": ["\u201e", "lieg", "bei", "dir", "un\u00b7ter", "Lin\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "PPER", "APPR", "NE", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "\u201emein Todtenkr\u00e4nzlein sch\u00f6n.\u201c", "tokens": ["\u201e", "mein", "Tod\u00b7ten\u00b7kr\u00e4nz\u00b7lein", "sch\u00f6n", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}