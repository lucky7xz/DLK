{"dta.poem.21124": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Amintas .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du sahest sie, als in Kleanthens Armen", "tokens": ["Du", "sa\u00b7hest", "sie", ",", "als", "in", "Kle\u00b7an\u00b7thens", "Ar\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "APPR", "NE", "NN"], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dein z\u00e4rtlich Herz dem Freund entgegen klopfte,", "tokens": ["Dein", "z\u00e4rt\u00b7lich", "Herz", "dem", "Freund", "ent\u00b7ge\u00b7gen", "klopf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "ART", "NN", "PTKVZ", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und deinen Wunsch die Freundschaft ganz erf\u00fcllte.", "tokens": ["Und", "dei\u00b7nen", "Wunsch", "die", "Freund\u00b7schaft", "ganz", "er\u00b7f\u00fcll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sahest Sie \u2014 Mein Herz nennt mir sie ewig!", "tokens": ["Du", "sa\u00b7hest", "Sie", "Mein", "Herz", "nennt", "mir", "sie", "e\u00b7wig", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "----+-+-+-+", "measure": "unknown.measure.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr holder Blick drang unter dunkeln Thr\u00e4nen", "tokens": ["Ihr", "hol\u00b7der", "Blick", "drang", "un\u00b7ter", "dun\u00b7keln", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch sanft hervor, und l\u00e4chelte voll Unschuld.", "tokens": ["Doch", "sanft", "her\u00b7vor", ",", "und", "l\u00e4\u00b7chel\u00b7te", "voll", "Un\u00b7schuld", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie an der Brust ein fr\u00fch ungl\u00fccklich M\u00e4dchen", "tokens": ["Wie", "an", "der", "Brust", "ein", "fr\u00fch", "un\u00b7gl\u00fcck\u00b7lich", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJD", "ADJD", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem blanken Stahl des wilden M\u00f6rders l\u00e4chelt.", "tokens": ["Dem", "blan\u00b7ken", "Stahl", "des", "wil\u00b7den", "M\u00f6r\u00b7ders", "l\u00e4\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O! mein Amint, du liebst, und liebest gl\u00fccklich!", "tokens": ["O", "!", "mein", "A\u00b7mint", ",", "du", "liebst", ",", "und", "lie\u00b7best", "gl\u00fcck\u00b7lich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch du kennst auch der Liebe bittre Schmerzen.", "tokens": ["Doch", "du", "kennst", "auch", "der", "Lie\u00b7be", "bitt\u00b7re", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Beklag ein Herz, der Z\u00e4rtlichkeit geschaffen,", "tokens": ["Be\u00b7klag", "ein", "Herz", ",", "der", "Z\u00e4rt\u00b7lich\u00b7keit", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch nur geliebt zum Ungl\u00fcck und Verderben.", "tokens": ["Doch", "nur", "ge\u00b7liebt", "zum", "Un\u00b7gl\u00fcck", "und", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "APPRART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dir will ich oft die schweren Thr\u00e4nen weinen,", "tokens": ["Dir", "will", "ich", "oft", "die", "schwe\u00b7ren", "Thr\u00e4\u00b7nen", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Freundschaft heischt und reine Liebe fordert.", "tokens": ["Die", "Freund\u00b7schaft", "heischt", "und", "rei\u00b7ne", "Lie\u00b7be", "for\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch schon mein Damon ist geliebt und gl\u00fccklich;", "tokens": ["Doch", "schon", "mein", "Da\u00b7mon", "ist", "ge\u00b7liebt", "und", "gl\u00fcck\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sollt ich mich nicht in Deinem Gl\u00fccke tr\u00f6sten?", "tokens": ["Sollt", "ich", "mich", "nicht", "in", "Dei\u00b7nem", "Gl\u00fc\u00b7cke", "tr\u00f6s\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}