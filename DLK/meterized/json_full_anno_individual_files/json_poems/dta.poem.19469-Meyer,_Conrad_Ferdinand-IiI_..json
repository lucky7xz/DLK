{"dta.poem.19469": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "IiI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Fr\u00fchling mit der V\u00f6glein Laut", "tokens": ["Fr\u00fch\u00b7ling", "mit", "der", "V\u00f6\u00b7glein", "Laut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Allerenden, allerorten!", "tokens": ["Al\u00b7le\u00b7ren\u00b7den", ",", "al\u00b7le\u00b7ror\u00b7ten", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["PIS", "$,", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fr\u00fchling, der die Welt umblaut,", "tokens": ["Fr\u00fch\u00b7ling", ",", "der", "die", "Welt", "um\u00b7blaut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Deine bl\u00fch'nden Siegespforten", "tokens": ["Dei\u00b7ne", "bl\u00fch'n\u00b7den", "Sie\u00b7ge\u00b7spfor\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Hast du niedrig aufgebaut!", "tokens": ["Hast", "du", "nied\u00b7rig", "auf\u00b7ge\u00b7baut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ueber alle Pfade her", "tokens": ["Ue\u00b7ber", "al\u00b7le", "Pfa\u00b7de", "her"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APZR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schie\u00dfen bl\u00fcthenschwere Zweige", "tokens": ["Schie\u00b7\u00dfen", "bl\u00fc\u00b7then\u00b7schwe\u00b7re", "Zwei\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ungeb\u00e4ndigt, kreuz und quer,", "tokens": ["Un\u00b7ge\u00b7b\u00e4n\u00b7digt", ",", "kreuz", "und", "quer", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df dir jedes Haupt sich neige,", "tokens": ["Da\u00df", "dir", "je\u00b7des", "Haupt", "sich", "nei\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und die Demuth ist nicht schwer.", "tokens": ["Und", "die", "De\u00b7muth", "ist", "nicht", "schwer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}