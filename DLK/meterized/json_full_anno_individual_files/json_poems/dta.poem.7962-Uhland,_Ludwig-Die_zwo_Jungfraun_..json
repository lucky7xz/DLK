{"dta.poem.7962": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Die zwo Jungfraun .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Zwo Jungfraun sah ich auf dem H\u00fcgel droben,", "tokens": ["Zwo", "Jung\u00b7fraun", "sah", "ich", "auf", "dem", "H\u00fc\u00b7gel", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich lieblich von Gesicht, von zartem Baue;", "tokens": ["Gleich", "lieb\u00b7lich", "von", "Ge\u00b7sicht", ",", "von", "zar\u00b7tem", "Bau\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie blickten in die abendlichen Gaue,", "tokens": ["Sie", "blick\u00b7ten", "in", "die", "a\u00b7bend\u00b7li\u00b7chen", "Gau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie sa\u00dfen traut und schwesterlich verwoben.", "tokens": ["Sie", "sa\u00b7\u00dfen", "traut", "und", "schwes\u00b7ter\u00b7lich", "ver\u00b7wo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Eine hielt den rechten Arm erhoben,", "tokens": ["Die", "Ei\u00b7ne", "hielt", "den", "rech\u00b7ten", "Arm", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hindeutend auf Gebirg und Strom und Aue;", "tokens": ["Hin\u00b7deu\u00b7tend", "auf", "Ge\u00b7birg", "und", "Strom", "und", "Au\u00b7e", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Andre hielt, damit sie besser schaue,", "tokens": ["Die", "And\u00b7re", "hielt", ",", "da\u00b7mit", "sie", "bes\u00b7ser", "schau\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die linke Hand der Sonne vorgeschoben.", "tokens": ["Die", "lin\u00b7ke", "Hand", "der", "Son\u00b7ne", "vor\u00b7ge\u00b7scho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Kein Wunder, da\u00df Verlangen mich bestrickte", "tokens": ["Kein", "Wun\u00b7der", ",", "da\u00df", "Ver\u00b7lan\u00b7gen", "mich", "be\u00b7strick\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$,", "KOUS", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und da\u00df in mir der s\u00fc\u00dfe Wunsch ergl\u00fchte:", "tokens": ["Und", "da\u00df", "in", "mir", "der", "s\u00fc\u00b7\u00dfe", "Wunsch", "er\u00b7gl\u00fch\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O s\u00e4\u00df\u2019 ich doch an Einer Platz von Beiden!", "tokens": ["O", "s\u00e4\u00df'", "ich", "doch", "an", "Ei\u00b7ner", "Platz", "von", "Bei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wie ich l\u00e4nger nach den Trauten blickte,", "tokens": ["Doch", "wie", "ich", "l\u00e4n\u00b7ger", "nach", "den", "Trau\u00b7ten", "blick\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gedacht\u2019 ich im bes\u00e4nftigten Gem\u00fcthe:", "tokens": ["Ge\u00b7dacht'", "ich", "im", "be\u00b7s\u00e4nf\u00b7tig\u00b7ten", "Ge\u00b7m\u00fc\u00b7the", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nein! wahrlich, S\u00fcnde w\u00e4r\u2019 es, sie zu scheiden!", "tokens": ["Nein", "!", "wahr\u00b7lich", ",", "S\u00fcn\u00b7de", "w\u00e4r'", "es", ",", "sie", "zu", "schei\u00b7den", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "$,", "NN", "VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}