{"textgrid.poem.53168": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Heinrich Knobloch und Magdalene Boy", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wol dem, der dieser Vorjahrs-Lust", "tokens": ["Wol", "dem", ",", "der", "die\u00b7ser", "Vor\u00b7jahr\u00b7sLust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PDAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nach Gn\u00fcge kan geniessen,", "tokens": ["Nach", "Gn\u00fc\u00b7ge", "kan", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der, keiner Kranckheit jhm bewust,", "tokens": ["Der", ",", "kei\u00b7ner", "Kran\u00b7ck\u00b7heit", "jhm", "be\u00b7wust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht stets sich ein-mus-schliessen:", "tokens": ["Nicht", "stets", "sich", "ein\u00b7mus\u00b7schlies\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bald f\u00e4hrt er, bald spatziert er aus,", "tokens": ["Bald", "f\u00e4hrt", "er", ",", "bald", "spat\u00b7ziert", "er", "aus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd l\u00e4sst daheim die Schmertzen,", "tokens": ["Vnd", "l\u00e4sst", "da\u00b7heim", "die", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Besieht des freyen Himmels Hau\u00df", "tokens": ["Be\u00b7sieht", "des", "frey\u00b7en", "Him\u00b7mels", "Hau\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit unbesorgtem Hertzen.", "tokens": ["Mit", "un\u00b7be\u00b7sorg\u00b7tem", "Hert\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er l\u00e4sst Gep\u00fcsche, Berg' und Thal'", "tokens": ["Er", "l\u00e4sst", "Ge\u00b7p\u00fcsc\u00b7he", ",", "Ber\u00b7g'", "und", "Thal'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ihm tausent Frewde bringen,", "tokens": ["Ihm", "tau\u00b7sent", "Frew\u00b7de", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd h\u00f6rt die s\u00fcsse Nachtigall", "tokens": ["Vnd", "h\u00f6rt", "die", "s\u00fcs\u00b7se", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So sch\u00f6n und k\u00fcnstlich singen,", "tokens": ["So", "sch\u00f6n", "und", "k\u00fcnst\u00b7lich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sucht einen Baum, der lieblich k\u00fchlt", "tokens": ["Sucht", "ei\u00b7nen", "Baum", ",", "der", "lieb\u00b7lich", "k\u00fchlt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vnd Schatten zu-kan-neigen,", "tokens": ["Vnd", "Schat\u00b7ten", "zu\u00b7kan\u00b7nei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Hie h\u00f6rt er, wie der Westwind spielt", "tokens": ["Hie", "h\u00f6rt", "er", ",", "wie", "der", "West\u00b7wind", "spielt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auff den belaubten Zweigen.", "tokens": ["Auff", "den", "be\u00b7laub\u00b7ten", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "F\u00fchrt er sein Liebchen an der Hand,", "tokens": ["F\u00fchrt", "er", "sein", "Lieb\u00b7chen", "an", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die newlich sein ist worden,", "tokens": ["Die", "new\u00b7lich", "sein", "ist", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VAINF", "VAFIN", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie schwebt er doch durch solchen Stand", "tokens": ["Wie", "schwebt", "er", "doch", "durch", "sol\u00b7chen", "Stand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fast in der G\u00f6tter Orden!", "tokens": ["Fast", "in", "der", "G\u00f6t\u00b7ter", "Or\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie l\u00e4chelt jhn, sie sieht jhn an,", "tokens": ["Sie", "l\u00e4\u00b7chelt", "jhn", ",", "sie", "sieht", "jhn", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vmbarmt jhn auch daneben,", "tokens": ["Vm\u00b7barmt", "jhn", "auch", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.7": {"text": "Der Lentz ergetzt sie, mehr jhr Mann,", "tokens": ["Der", "Lentz", "er\u00b7getzt", "sie", ",", "mehr", "jhr", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ist jhr Hertz und Leben.", "tokens": ["Der", "ist", "jhr", "Hertz", "und", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er setzt sich mit jhr an ein Quell,", "tokens": ["Er", "setzt", "sich", "mit", "jhr", "an", "ein", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht sich die Nymphen baden,", "tokens": ["Sieht", "sich", "die", "Nym\u00b7phen", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie \u00fcbertrifft zart, sauber, hell,", "tokens": ["Sie", "\u00fc\u00b7bert\u00b7rifft", "zart", ",", "sau\u00b7ber", ",", "hell", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Die Sch\u00f6nheit der Dryaden,", "tokens": ["Die", "Sch\u00f6n\u00b7heit", "der", "Dry\u00b7a\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Pan schleicht jhr nach und m\u00f6chte schier", "tokens": ["Pan", "schleicht", "jhr", "nach", "und", "m\u00f6ch\u00b7te", "schier"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "APPR", "KON", "VMFIN", "ADJD"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.6": {"text": "Vor Mi\u00dfgunst gantz zerspringen,", "tokens": ["Vor", "Mi\u00df\u00b7gunst", "gantz", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Indessen kan der Nymphen Zier", "tokens": ["In\u00b7des\u00b7sen", "kan", "der", "Nym\u00b7phen", "Zier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Frey tantzen, spielen, singen.", "tokens": ["Frey", "tant\u00b7zen", ",", "spie\u00b7len", ",", "sin\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun die\u00df wird den Verliebten auch", "tokens": ["Nun", "die\u00df", "wird", "den", "Ver\u00b7lieb\u00b7ten", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PDS", "VAFIN", "ART", "NN", "ADV"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Im kurtzen wiederfahren,", "tokens": ["Im", "kurt\u00b7zen", "wie\u00b7der\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die nach gemeinem Christen-Brauch", "tokens": ["Die", "nach", "ge\u00b7mei\u00b7nem", "Chris\u00b7ten\u00b7Brauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich heute lassen paren;", "tokens": ["Sich", "heu\u00b7te", "las\u00b7sen", "pa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Nacht bricht an, sie liegen bey,", "tokens": ["Die", "Nacht", "bricht", "an", ",", "sie", "lie\u00b7gen", "bey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gott spreche seinen Segen,", "tokens": ["Gott", "spre\u00b7che", "sei\u00b7nen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df Ihrer EhZucht \u00e4hnlich sey", "tokens": ["Da\u00df", "Ih\u00b7rer", "Eh\u00b7Zucht", "\u00e4hn\u00b7lich", "sey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Morgen-Thaw und Regen!", "tokens": ["Dem", "Mor\u00b7gen\u00b7Thaw", "und", "Re\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was thun sie denn nach diesem bald?", "tokens": ["Was", "thun", "sie", "denn", "nach", "die\u00b7sem", "bald", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ohn zweiffel wird auch jhnen", "tokens": ["Ohn", "zweif\u00b7fel", "wird", "auch", "jh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein' Gart', ein Schattenreicher Wald", "tokens": ["Ein'", "Gart'", ",", "ein", "Schat\u00b7ten\u00b7rei\u00b7cher", "Wald"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nach wolgefallen dienen,", "tokens": ["Nach", "wol\u00b7ge\u00b7fal\u00b7len", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie haben Mittel, allen Wust", "tokens": ["Sie", "ha\u00b7ben", "Mit\u00b7tel", ",", "al\u00b7len", "Wust"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Sorgen au\u00df-zu-schliessen;", "tokens": ["Der", "Sor\u00b7gen", "au\u00df\u00b7zu\u00b7schlies\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Wol dem, der so der Vorjahrs-Lust", "tokens": ["Wol", "dem", ",", "der", "so", "der", "Vor\u00b7jahr\u00b7sLust"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ohn Kranckheit kan geniessen.", "tokens": ["Ohn", "Kran\u00b7ck\u00b7heit", "kan", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wol dem, der dieser Vorjahrs-Lust", "tokens": ["Wol", "dem", ",", "der", "die\u00b7ser", "Vor\u00b7jahr\u00b7sLust"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "$,", "PRELS", "PDAT", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Nach Gn\u00fcge kan geniessen,", "tokens": ["Nach", "Gn\u00fc\u00b7ge", "kan", "ge\u00b7nies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der, keiner Kranckheit jhm bewust,", "tokens": ["Der", ",", "kei\u00b7ner", "Kran\u00b7ck\u00b7heit", "jhm", "be\u00b7wust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nicht stets sich ein-mus-schliessen:", "tokens": ["Nicht", "s