{"dta.poem.16010": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "64.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sind wir zum Lebensmahl berufen, um zu fasten?", "tokens": ["Sind", "wir", "zum", "Le\u00b7bens\u00b7mahl", "be\u00b7ru\u00b7fen", ",", "um", "zu", "fas\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,", "KOUI", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O nein! da w\u00e4re schlimm bei unserm Wirthe gasten.", "tokens": ["O", "nein", "!", "da", "w\u00e4\u00b7re", "schlimm", "bei", "un\u00b7serm", "Wirt\u00b7he", "gas\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKANT", "$.", "ADV", "VAFIN", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Zum Fasten lud uns nicht der Herr zu seinem Feste,", "tokens": ["Zum", "Fas\u00b7ten", "lud", "uns", "nicht", "der", "Herr", "zu", "sei\u00b7nem", "Fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er freut sich, da\u00df des Mahls sich freuen seine G\u00e4ste.", "tokens": ["Er", "freut", "sich", ",", "da\u00df", "des", "Mahls", "sich", "freu\u00b7en", "sei\u00b7ne", "G\u00e4s\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "$,", "KOUS", "ART", "NN", "PRF", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "F\u00fcrlieb nur nehmen sollt ihr, nicht euch \u00fcbernehmen,", "tokens": ["F\u00fcr\u00b7lieb", "nur", "neh\u00b7men", "sollt", "ihr", ",", "nicht", "euch", "\u00fc\u00b7ber\u00b7neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVINF", "VMFIN", "PPER", "$,", "PTKNEG", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vertr\u00e4glich jeder auch dem Nachbar sich bequemen,", "tokens": ["Ver\u00b7tr\u00e4g\u00b7lich", "je\u00b7der", "auch", "dem", "Nach\u00b7bar", "sich", "be\u00b7que\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "ADV", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Mit sinnigem Gespr\u00e4ch des Wirthes Tafel w\u00fcrzen,", "tokens": ["Mit", "sin\u00b7ni\u00b7gem", "Ge\u00b7spr\u00e4ch", "des", "Wirt\u00b7hes", "Ta\u00b7fel", "w\u00fcr\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wenn ihr satt seid, euch zum Abzug dankbar sch\u00fcrzen.", "tokens": ["Und", "wenn", "ihr", "satt", "seid", ",", "euch", "zum", "Ab\u00b7zug", "dank\u00b7bar", "sch\u00fcr\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VAFIN", "$,", "PPER", "APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}