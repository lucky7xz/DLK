{"textgrid.poem.54298": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bekommst du, Margaris, ists m\u00f6glich? einen Mann,", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bekommst du, Margaris, ists m\u00f6glich? einen Mann,", "tokens": ["Be\u00b7kommst", "du", ",", "Mar\u00b7ga\u00b7ris", ",", "ists", "m\u00f6g\u00b7lich", "?", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "VAFIN", "ADJD", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dich als seine Frau best\u00e4ndig lieben kan?", "tokens": ["Der", "dich", "als", "sei\u00b7ne", "Frau", "be\u00b7st\u00e4n\u00b7dig", "lie\u00b7ben", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An L\u00f6ffel-Knechten hat es zwar dir nie gemangelt,", "tokens": ["An", "L\u00f6f\u00b7fel\u00b7Knech\u00b7ten", "hat", "es", "zwar", "dir", "nie", "ge\u00b7man\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie mancher N\u00e4scher hat bey dir bereits geangelt;", "tokens": ["Wie", "man\u00b7cher", "N\u00e4\u00b7scher", "hat", "bey", "dir", "be\u00b7reits", "ge\u00b7an\u00b7gelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenns zum nehmen kam, da war niemand zu Hau\u00df,", "tokens": ["Doch", "wenns", "zum", "neh\u00b7men", "kam", ",", "da", "war", "nie\u00b7mand", "zu", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "VVINF", "VVFIN", "$,", "ADV", "VAFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da sah es ziemlich leer, betr\u00fcbt und windig aus.", "tokens": ["Da", "sah", "es", "ziem\u00b7lich", "leer", ",", "be\u00b7tr\u00fcbt", "und", "win\u00b7dig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "VVPP", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es solten, dachtest du, die Minen und Gebehrden,", "tokens": ["Es", "sol\u00b7ten", ",", "dach\u00b7test", "du", ",", "die", "Mi\u00b7nen", "und", "Ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil alles reitzend hie\u00df, zu Fang und Sprenckeln werden.", "tokens": ["Weil", "al\u00b7les", "reit\u00b7zend", "hie\u00df", ",", "zu", "Fang", "und", "Spren\u00b7ckeln", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du f\u00e4rbtest Wang und Mund, so sch\u00f6n man immer kan,", "tokens": ["Du", "f\u00e4rb\u00b7test", "Wang", "und", "Mund", ",", "so", "sch\u00f6n", "man", "im\u00b7mer", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$,", "ADV", "ADJD", "PIS", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dieweil dir Jesabel die Kunst gelernet, an.", "tokens": ["Die\u00b7weil", "dir", "Je\u00b7sa\u00b7bel", "die", "Kunst", "ge\u00b7ler\u00b7net", ",", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Du suchtest alles vor, was zu charmiren n\u00f6thig,", "tokens": ["Du", "such\u00b7test", "al\u00b7les", "vor", ",", "was", "zu", "char\u00b7mi\u00b7ren", "n\u00f6\u00b7thig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "PRELS", "PTKZU", "VVINF", "ADJD", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "Und unter allen war kein einiger erb\u00f6thig", "tokens": ["Und", "un\u00b7ter", "al\u00b7len", "war", "kein", "ei\u00b7ni\u00b7ger", "er\u00b7b\u00f6t\u00b7hig"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIS", "VAFIN", "PIAT", "PIAT", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sein Hertze dir zu weyhn, daher es w\u00fcrcklich schien,", "tokens": ["Sein", "Hert\u00b7ze", "dir", "zu", "weyhn", ",", "da\u00b7her", "es", "w\u00fcrck\u00b7lich", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "PAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Du w\u00fcrdest gantz gewi\u00df das Hospital beziehn,", "tokens": ["Du", "w\u00fcr\u00b7dest", "gantz", "ge\u00b7wi\u00df", "das", "Hos\u00b7pi\u00b7tal", "be\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und die Kleinodien vom Jungfr\u00e4ulichen Leben,", "tokens": ["Und", "die", "Klein\u00b7o\u00b7di\u00b7en", "vom", "Jung\u00b7fr\u00e4u\u00b7li\u00b7chen", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-++--+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Den W\u00fcrmern dermahleinst, als ein Legatum, geben.", "tokens": ["Den", "W\u00fcr\u00b7mern", "der\u00b7mah\u00b7le\u00b7inst", ",", "als", "ein", "Le\u00b7ga\u00b7tum", ",", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "KOUS", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Doch nun besch\u00e4mest du daf\u00fcr die gantze Stadt,", "tokens": ["Doch", "nun", "be\u00b7sch\u00e4\u00b7mest", "du", "da\u00b7f\u00fcr", "die", "gant\u00b7ze", "Stadt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Die solchen tollen Wahn bisher geheget hat.", "tokens": ["Die", "sol\u00b7chen", "tol\u00b7len", "Wahn", "bis\u00b7her", "ge\u00b7he\u00b7get", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ADJA", "NN", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du hast nunmehr gesiegt, es hat sich was gefangen,", "tokens": ["Du", "hast", "nun\u00b7mehr", "ge\u00b7siegt", ",", "es", "hat", "sich", "was", "ge\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,", "PPER", "VAFIN", "PRF", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn Coridon ist doch noch in dein Netz gegangen.", "tokens": ["Denn", "Co\u00b7ri\u00b7don", "ist", "doch", "noch", "in", "dein", "Netz", "ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Begl\u00fcckter Coridon, du hast dir was erwehlt,", "tokens": ["Be\u00b7gl\u00fcck\u00b7ter", "Co\u00b7ri\u00b7don", ",", "du", "hast", "dir", "was", "er\u00b7wehlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPER", "VAFIN", "PPER", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das lange Jahre schon die Stunden abgezehlt,", "tokens": ["Das", "lan\u00b7ge", "Jah\u00b7re", "schon", "die", "Stun\u00b7den", "ab\u00b7ge\u00b7zehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wenn einst ein Freyher k\u00e4m; dein sch\u00f6n lacquirtes Weibgen", "tokens": ["Wenn", "einst", "ein", "Frey\u00b7her", "k\u00e4m", ";", "dein", "sch\u00f6n", "lac\u00b7quir\u00b7tes", "Weib\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "VVFIN", "$.", "PPOSAT", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Schmei\u00dft Nest und Cr\u00e4ntzgen hin, und k\u00fc\u00dft das Weiber-H\u00e4ubgen.", "tokens": ["Schmei\u00dft", "Nest", "und", "Cr\u00e4ntz\u00b7gen", "hin", ",", "und", "k\u00fc\u00dft", "das", "Wei\u00b7ber\u00b7H\u00e4ub\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "KON", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wohl wissend, da\u00df die Tracht ihr lange schon geb\u00fchrt,", "tokens": ["Wohl", "wis\u00b7send", ",", "da\u00df", "die", "Tracht", "ihr", "lan\u00b7ge", "schon", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ihr stiller Wandel hat ohnfehlbar dich verf\u00fchrt.", "tokens": ["Ihr", "stil\u00b7ler", "Wan\u00b7del", "hat", "ohn\u00b7fehl\u00b7bar", "dich", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es mu\u00df dich alle Welt in diesem St\u00fccke loben,", "tokens": ["Es", "mu\u00df", "dich", "al\u00b7le", "Welt", "in", "die\u00b7sem", "St\u00fc\u00b7cke", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Da\u00df du der Margaris dein Hertz hast aufgehoben.", "tokens": ["Da\u00df", "du", "der", "Mar\u00b7ga\u00b7ris", "dein", "Hertz", "hast", "auf\u00b7ge\u00b7ho\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NE", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Findst du gleich nicht den Kern, der andern war geweyht,", "tokens": ["Findst", "du", "gleich", "nicht", "den", "Kern", ",", "der", "an\u00b7dern", "war", "ge\u00b7weyht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$,", "PRELS", "PIS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "So sp\u00fchret man an dir doch die Vergn\u00fcgsamkeit.", "tokens": ["So", "sp\u00fch\u00b7ret", "man", "an", "dir", "doch", "die", "Ver\u00b7gn\u00fcg\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PPER", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die Liebe suchet dich mit Datteln abzuspeisen,", "tokens": ["Die", "Lie\u00b7be", "su\u00b7chet", "dich", "mit", "Dat\u00b7teln", "ab\u00b7zu\u00b7spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Die Schaalen schmecken s\u00fc\u00df, der Kern ist wegzuschmeissen.", "tokens": ["Die", "Schaa\u00b7len", "schme\u00b7cken", "s\u00fc\u00df", ",", "der", "Kern", "ist", "weg\u00b7zu\u00b7schmeis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Doch siehe dich ja vor, da\u00df dir di\u00df liebe Kind", "tokens": ["Doch", "sie\u00b7he", "dich", "ja", "vor", ",", "da\u00df", "dir", "di\u00df", "lie\u00b7be", "Kind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "ADV", "PTKVZ", "$,", "KOUS", "PPER", "PDS", "VVFIN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Act\u00e4ons Feder-Busch nicht auf dein H\u00fcthgen bind.", "tokens": ["Ac\u00b7t\u00e4ons", "Fe\u00b7der\u00b7Busch", "nicht", "auf", "dein", "H\u00fcth\u00b7gen", "bind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.35": {"text": "Du must, wofern du wilst der Hahn im Korbe bleiben,", "tokens": ["Du", "must", ",", "wo\u00b7fern", "du", "wilst", "der", "Hahn", "im", "Kor\u00b7be", "blei\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Das vorige Gewerb der Frau nicht lassen treiben.", "tokens": ["Das", "vo\u00b7ri\u00b7ge", "Ge\u00b7werb", "der", "Frau", "nicht", "las\u00b7sen", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKNEG", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Bekommst du, Margaris, ists m\u00f6glich? einen Mann,", "tokens": ["Be\u00b7kommst", "du", ",", "Mar\u00b7ga\u00b7ris", ",", "ists", "m\u00f6g\u00b7lich", "?", "ei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NE", "$,", "VAFIN", "ADJD", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der dich als seine Frau best\u00e4ndig lieben kan?", "tokens": ["Der", "dich", "als", "sei\u00b7ne", "Frau", "be\u00b7st\u00e4n\u00b7dig", "lie\u00b7ben", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "PPOSAT", "NN", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An L\u00f6ffel-Knechten hat es zwar dir nie gemangelt,", "tokens": ["An", "L\u00f6f\u00b7fel\u00b7Knech\u00b7ten", "hat", "es", "zwar", "dir", "nie", "ge\u00b7man\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie mancher N\u00e4scher hat bey dir bereits geangelt;", "tokens": ["Wie", "man\u00b7cher", "N\u00e4\u00b7scher", "hat", "bey", "dir", "be\u00b7reits", "ge\u00b7an\u00b7gelt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch wenns zum nehmen kam, da war niemand zu Hau\u00df,", "tokens": ["Doch", "wenns", "zum", "neh\u00b7men", "kam", ",", "da", "war", "nie\u00b7mand", "zu", "Hau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPRART", "VVINF", "VVFIN", "$,", "ADV", "VAFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Da sah es ziemlich leer, betr\u00fcbt und windig aus.", "tokens": ["Da", "sah", "es", "ziem\u00b7lich", "leer", ",", "be\u00b7tr\u00fcbt", "und", "win\u00b7dig", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,", "VVPP", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es solten, dachtest du, die Minen und Gebehrden,", "tokens": ["Es", "sol\u00b7ten", ",", "dach\u00b7test", "du", ",", "die", "Mi\u00b7nen", "und", "Ge\u00b7behr\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "VVFIN", "PPER", "$,", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil alles reitzend hie\u00df, zu Fang und Sprenckeln werden.", "tokens": ["Weil", "al\u00b7les", "reit\u00b7zend", "hie\u00df", ",", "zu", "Fang", "und", "Spren\u00b7ckeln", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Du f\u00e4rbtest Wang und Mund, so sch\u00f6n man immer kan,", "tokens": ["Du", "f\u00e4