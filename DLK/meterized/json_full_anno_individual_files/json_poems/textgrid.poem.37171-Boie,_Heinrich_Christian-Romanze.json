{"textgrid.poem.37171": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Dirnen, die ihr spr\u00f6de thut,", "tokens": ["Ihr", "Dir\u00b7nen", ",", "die", "ihr", "spr\u00f6\u00b7de", "thut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sch\u00e4umt euer j\u00fcngferliches Blut", "tokens": ["Sch\u00e4umt", "eu\u00b7er", "j\u00fcng\u00b7fer\u00b7li\u00b7ches", "Blut"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Gleich oft zum \u00fcberkochen,", "tokens": ["Gleich", "oft", "zum", "\u00fc\u00b7ber\u00b7ko\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00f6rt, wie in Sch\u00f6nbeck l\u00e4sterlich", "tokens": ["H\u00f6rt", ",", "wie", "in", "Sch\u00f6n\u00b7beck", "l\u00e4s\u00b7ter\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "APPR", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "An einem Kammerk\u00e4tzchen sich", "tokens": ["An", "ei\u00b7nem", "Kam\u00b7mer\u00b7k\u00e4tz\u00b7chen", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das spr\u00f6dethun gerochen.", "tokens": ["Das", "spr\u00f6\u00b7det\u00b7hun", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als Aeffchen ihrer gn\u00e4dgen Fra", "tokens": ["Als", "A\u00b7ef\u00b7fchen", "ih\u00b7rer", "gn\u00e4d\u00b7gen", "Fra"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schminkt sie sich salva venia", "tokens": ["Schminkt", "sie", "sich", "sal\u00b7va", "ve\u00b7nia"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "FM", "FM"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit rothen Hasenf\u00fc\u00dfchen;", "tokens": ["Mit", "ro\u00b7then", "Ha\u00b7sen\u00b7f\u00fc\u00df\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Belockt sich wie ein Hoffr\u00e4ulein", "tokens": ["Be\u00b7lockt", "sich", "wie", "ein", "Hof\u00b7fr\u00e4u\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und schn\u00fcrt sich d\u00fcnn und lispelt fein", "tokens": ["Und", "schn\u00fcrt", "sich", "d\u00fcnn", "und", "lis\u00b7pelt", "fein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und nimmt mit grace ein Prieschen.", "tokens": ["Und", "nimmt", "mit", "gra\u00b7ce", "ein", "Prie\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Als einmal sie Gevatter stund,", "tokens": ["Als", "ein\u00b7mal", "sie", "Ge\u00b7vat\u00b7ter", "stund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da zog und spizte sie den Mund", "tokens": ["Da", "zog", "und", "spiz\u00b7te", "sie", "den", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mon dieu wie mannigfaltig!", "tokens": ["Mon", "di\u00b7eu", "wie", "man\u00b7nig\u00b7fal\u00b7tig", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schmieds Friedrich warf ihr einen Schmatz", "tokens": ["Schmieds", "Fried\u00b7rich", "warf", "ihr", "ei\u00b7nen", "Schmatz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und trank ihr zu: \u00bbMamsell, Ihr Schatz!\u00ab", "tokens": ["Und", "trank", "ihr", "zu", ":", "\u00bb", "Mam\u00b7sell", ",", "Ihr", "Schatz", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$.", "$(", "NN", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Drob brummte sie gewaltig.", "tokens": ["Drob", "brumm\u00b7te", "sie", "ge\u00b7wal\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbein Schatz, parbleu! welch dummer Schnack!", "tokens": ["\u00bb", "ein", "Schatz", ",", "par\u00b7bleu", "!", "welch", "dum\u00b7mer", "Schnack", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "ADJD", "$.", "PWAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bleib er bei seinem Kohlensack", "tokens": ["Bleib", "er", "bei", "sei\u00b7nem", "Koh\u00b7len\u00b7sack"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und la\u00df er meines gleichen!\u00ab", "tokens": ["Und", "la\u00df", "er", "mei\u00b7nes", "glei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbnun, nun, Mamsell, nur kein Gekreisch!", "tokens": ["\u00bb", "nun", ",", "nun", ",", "Mam\u00b7sell", ",", "nur", "kein", "Ge\u00b7kreisch", "!"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "$,", "NN", "$,", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schwernoth! Ihr juckt wohl auch das Fleisch", "tokens": ["Schwer\u00b7noth", "!", "Ihr", "juckt", "wohl", "auch", "das", "Fleisch"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach mir und meines gleichen!\u00ab", "tokens": ["Nach", "mir", "und", "mei\u00b7nes", "glei\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "KON", "PPOSAT", "ADJA", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbdu bist der rechte, schrie sie, Du!", "tokens": ["\u00bb", "du", "bist", "der", "rech\u00b7te", ",", "schrie", "sie", ",", "Du", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "ADJA", "$,", "VVFIN", "PPER", "$,", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Solch grobes Mannsvolk stinkt mir zu,", "tokens": ["Solch", "gro\u00b7bes", "Manns\u00b7volk", "stinkt", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie Theer an alten Achsen.", "tokens": ["Wie", "Theer", "an", "al\u00b7ten", "Ach\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verfiel mein Gusto je aufs frein,", "tokens": ["Ver\u00b7fiel", "mein", "Gus\u00b7to", "je", "aufs", "frein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Soll diese Nacht zum Augenschein", "tokens": ["Soll", "die\u00b7se", "Nacht", "zum", "Au\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein schwarzer Bart mir wachsen!\u00ab", "tokens": ["Ein", "schwar\u00b7zer", "Bart", "mir", "wach\u00b7sen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie schlug ein Schnippchen, schnupft' und trank,", "tokens": ["Sie", "schlug", "ein", "Schnipp\u00b7chen", ",", "schnupft'", "und", "trank", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch klopft ihr gleich das Herz so bang.", "tokens": ["Doch", "klopft", "ihr", "gleich", "das", "Herz", "so", "bang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein b\u00f6s Ding ums Gewissen!", "tokens": ["Ein", "b\u00f6s", "Ding", "ums", "Ge\u00b7wis\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ihr graute nachts, schon juckt es ihr", "tokens": ["Ihr", "grau\u00b7te", "nachts", ",", "schon", "juckt", "es", "ihr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Um Wang' und Kinn, sie konnte schier", "tokens": ["Um", "Wang'", "und", "Kinn", ",", "sie", "konn\u00b7te", "schier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUI", "NN", "KON", "NN", "$,", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Angst kein Auge schlie\u00dfen.", "tokens": ["Vor", "Angst", "kein", "Au\u00b7ge", "schlie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Der Sturmwind saust' die Nacht hindurch,", "tokens": ["Der", "Sturm\u00b7wind", "saust'", "die", "Nacht", "hin\u00b7durch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Eule heulet auf der Burg,", "tokens": ["Die", "Eu\u00b7le", "heu\u00b7let", "auf", "der", "Burg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Wehklag' in den Eichen.", "tokens": ["Die", "Weh\u00b7klag'", "in", "den", "Ei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bang zirpen Grillen, Katzen maun,", "tokens": ["Bang", "zir\u00b7pen", "Gril\u00b7len", ",", "Kat\u00b7zen", "maun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "$,", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sieht ums Bette voller Graun", "tokens": ["Sie", "sieht", "ums", "Bet\u00b7te", "vol\u00b7ler", "Graun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Unterirdschen schleichen.", "tokens": ["Die", "Un\u00b7ter\u00b7ird\u00b7schen", "schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Als fr\u00fch sie vor den Spiegel trat,", "tokens": ["Als", "fr\u00fch", "sie", "vor", "den", "Spie\u00b7gel", "trat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da einen lauten Schrei sie that,", "tokens": ["Da", "ei\u00b7nen", "lau\u00b7ten", "Schrei", "sie", "that", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O scheusliches Geschicke!", "tokens": ["O", "scheus\u00b7li\u00b7ches", "Ge\u00b7schi\u00b7cke", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Wangen Kinn und Lippen zart", "tokens": ["Die", "Wan\u00b7gen", "Kinn", "und", "Lip\u00b7pen", "zart"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "KON", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umzog ein schwarzer Judenbart.", "tokens": ["Um\u00b7zog", "ein", "schwar\u00b7zer", "Ju\u00b7den\u00b7bart", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie fiel wie todt zur\u00fccke.", "tokens": ["Sie", "fiel", "wie", "todt", "zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Als sie erwacht, o Jemini!", "tokens": ["Als", "sie", "er\u00b7wacht", ",", "o", "Je\u00b7mi\u00b7ni", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sch\u00e4umte, knirschte, krazte sie,", "tokens": ["Wie", "sch\u00e4um\u00b7te", ",", "knirschte", ",", "kraz\u00b7te", "sie", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Das Scheusal auszurotten.", "tokens": ["Das", "Scheu\u00b7sal", "aus\u00b7zu\u00b7rot\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbnun Friedrich komm und lache mein!", "tokens": ["\u00bb", "nun", "Fried\u00b7rich", "komm", "und", "la\u00b7che", "mein", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "NE", "VVFIN", "KON", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun wird der schlechtste Kerl mich scheun", "tokens": ["Nun", "wird", "der", "schlechts\u00b7te", "Kerl", "mich", "scheun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und alle Hurren spotten!\u00ab", "tokens": ["Und", "al\u00b7le", "Hur\u00b7ren", "spot\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Sie legt umsonst Pechhauben an,", "tokens": ["Sie", "legt", "um\u00b7sonst", "Pech\u00b7hau\u00b7ben", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zang ihr auch nicht helfen kann,", "tokens": ["Die", "Zang", "ihr", "auch", "nicht", "hel\u00b7fen", "kann", ","], "token_info": ["word", "word", "word", "word", "w