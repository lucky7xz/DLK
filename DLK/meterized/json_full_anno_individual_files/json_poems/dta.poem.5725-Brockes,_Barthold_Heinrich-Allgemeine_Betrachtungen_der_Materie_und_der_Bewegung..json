{"dta.poem.5725": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgemeine Betrachtungen der Materie  \n und der Bewegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dergleichen Wahrheit, wie wir lesen,", "tokens": ["Derg\u00b7lei\u00b7chen", "Wahr\u00b7heit", ",", "wie", "wir", "le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist niemals unbekannt gewesen.", "tokens": ["Ist", "nie\u00b7mals", "un\u00b7be\u00b7kannt", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es sind von je her die Poeten,", "tokens": ["Es", "sind", "von", "je", "her", "die", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "APZR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihren Sinn- und Anmuth-reichen Bildern,", "tokens": ["In", "ih\u00b7ren", "Sinn", "und", "An\u00b7muth\u00b7rei\u00b7chen", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "TRUNC", "KON", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Als grosse Meister,", "tokens": ["Als", "gros\u00b7se", "Meis\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Bem\u00fcht gewesen, sie zu schildern,", "tokens": ["Be\u00b7m\u00fcht", "ge\u00b7we\u00b7sen", ",", "sie", "zu", "schil\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "VAPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie liehen der Philosophie", "tokens": ["Sie", "lie\u00b7hen", "der", "Phi\u00b7lo\u00b7so\u00b7phie"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Den Pinsel, zum Gebrauch. Dieselben edlen Geister,", "tokens": ["Den", "Pin\u00b7sel", ",", "zum", "Ge\u00b7brauch", ".", "Die\u00b7sel\u00b7ben", "ed\u00b7len", "Geis\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPRART", "NN", "$.", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Ehre Griechenlandes, die", "tokens": ["Die", "Eh\u00b7re", "Grie\u00b7chen\u00b7lan\u00b7des", ",", "die"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "NN", "$,", "PRELS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jhr Wissen auf des Pindus-H\u00f6hen", "tokens": ["Ihr", "Wis\u00b7sen", "auf", "des", "Pin\u00b7dus\u00b7H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Aus dessen Born gesch\u00f6pfft; und die der Musen Schaar", "tokens": ["Aus", "des\u00b7sen", "Born", "ge\u00b7sch\u00f6pfft", ";", "und", "die", "der", "Mu\u00b7sen", "Schaar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "NN", "VVPP", "$.", "KON", "ART", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Errichteten so Tempel als Altar,", "tokens": ["Er\u00b7rich\u00b7te\u00b7ten", "so", "Tem\u00b7pel", "als", "Al\u00b7tar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}