{"textgrid.poem.34684": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Wilknitz", "genre": "verse", "period": "N.A.", "pub_year": 1802, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Steig', Fl\u00fcgelro\u00df, den Sturm in deinen M\u00e4hnen!", "tokens": ["Steig'", ",", "Fl\u00fc\u00b7gel\u00b7ro\u00df", ",", "den", "Sturm", "in", "dei\u00b7nen", "M\u00e4h\u00b7nen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fleug auf, mein Lied, mit deinem k\u00fchnsten Schwung!", "tokens": ["Fleug", "auf", ",", "mein", "Lied", ",", "mit", "dei\u00b7nem", "k\u00fchns\u00b7ten", "Schwung", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zu dir, mein Held, zu dir, des Liedes Sehnen,", "tokens": ["Zu", "dir", ",", "mein", "Held", ",", "zu", "dir", ",", "des", "Lie\u00b7des", "Seh\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PPER", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu deinem Licht aus meiner D\u00e4mmerung!", "tokens": ["Zu", "dei\u00b7nem", "Licht", "aus", "mei\u00b7ner", "D\u00e4m\u00b7me\u00b7rung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Und f\u00fcllen gleich die Augen sich mit Thr\u00e4nen,", "tokens": ["Und", "f\u00fcl\u00b7len", "gleich", "die", "Au\u00b7gen", "sich", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dir gleich zu sein, bleibt doch mein stolzes W\u00e4hnen.", "tokens": ["Dir", "gleich", "zu", "sein", ",", "bleibt", "doch", "mein", "stol\u00b7zes", "W\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VAINF", "$,", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auf deinem Grabe mag zum Lohne schweben", "tokens": ["Auf", "dei\u00b7nem", "Gra\u00b7be", "mag", "zum", "Loh\u00b7ne", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Eichenkrone, die dein Volk dir schlang;", "tokens": ["Die", "Ei\u00b7chen\u00b7kro\u00b7ne", ",", "die", "dein", "Volk", "dir", "schlang", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begeist'rung will in Liedesgluten beben,", "tokens": ["Be\u00b7geist'\u00b7rung", "will", "in", "Lie\u00b7des\u00b7glu\u00b7ten", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und einem Helden ziemet Bardensang:", "tokens": ["Und", "ei\u00b7nem", "Hel\u00b7den", "zie\u00b7met", "Bar\u00b7den\u00b7sang", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich ruf' es jubelnd unserm Vaterlande,", "tokens": ["Ich", "ruf'", "es", "ju\u00b7belnd", "un\u00b7serm", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich dich, Heldenseele, Bruder nannte!", "tokens": ["Da\u00df", "ich", "dich", ",", "Hel\u00b7den\u00b7see\u00b7le", ",", "Bru\u00b7der", "nann\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "$,", "NN", "$,", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du wirst unsterblich in den Herzen leben,", "tokens": ["Du", "wirst", "uns\u00b7terb\u00b7lich", "in", "den", "Her\u00b7zen", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wo Freiheit noch und Kraft in Liedern klingt.", "tokens": ["Wo", "Frei\u00b7heit", "noch", "und", "Kraft", "in", "Lie\u00b7dern", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "KON", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In sch\u00f6nen Augen wird die Thr\u00e4ne leben,", "tokens": ["In", "sch\u00f6\u00b7nen", "Au\u00b7gen", "wird", "die", "Thr\u00e4\u00b7ne", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn man die That des k\u00fchnen Leuen singt,", "tokens": ["Wenn", "man", "die", "That", "des", "k\u00fch\u00b7nen", "Leu\u00b7en", "singt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und in des Volkes teurer Heldensage", "tokens": ["Und", "in", "des", "Vol\u00b7kes", "teu\u00b7rer", "Hel\u00b7den\u00b7sa\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dem Enkel leuchten aus dem Kampf der Tage.", "tokens": ["Dem", "En\u00b7kel", "leuch\u00b7ten", "aus", "dem", "Kampf", "der", "Ta\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Noch seh' ich dich in den treulosen Scharen,", "tokens": ["Noch", "seh'", "ich", "dich", "in", "den", "treu\u00b7lo\u00b7sen", "Scha\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie deine Faust die M\u00f6rder niedersticht,", "tokens": ["Wie", "dei\u00b7ne", "Faust", "die", "M\u00f6r\u00b7der", "nie\u00b7der\u00b7sticht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "[wie ihre Dolche durch die Brust dir fahren", "tokens": ["wie", "ih\u00b7re", "Dol\u00b7che", "durch", "die", "Brust", "dir", "fah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOKOM", "PPOSAT", "NN", "APPR", "ART", "NN", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und dir erbla\u00dft das Heldenangesicht.]", "tokens": ["Und", "dir", "er\u00b7bla\u00dft", "das", "Hel\u00b7den\u00b7an\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da treibt sie T\u00fccke, helfend dir zu nahen,", "tokens": ["Da", "treibt", "sie", "T\u00fc\u00b7cke", ",", "hel\u00b7fend", "dir", "zu", "na\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,", "ADJD", "PPER", "APPR", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "[solch einen Helden noch im Netz zu fahen.]", "tokens": ["solch", "ei\u00b7nen", "Hel\u00b7den", "noch", "im", "Netz", "zu", "fa\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PIAT", "ART", "NN", "ADV", "APPRART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "[du wirst aus dem Get\u00fcmmel weggetragen,", "tokens": ["du", "wirst", "aus", "dem", "Ge\u00b7t\u00fcm\u00b7mel", "weg\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Man reicht dir Wein und legt Verbande an,", "tokens": ["Man", "reicht", "dir", "Wein", "und", "legt", "Ver\u00b7ban\u00b7de", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "KON", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch du hast kaum die Augen aufgeschlagen:]", "tokens": ["Doch", "du", "hast", "kaum", "die", "Au\u00b7gen", "auf\u00b7ge\u00b7schla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbaus Bubenhand nehm' ich kein Leben an!\u00ab", "tokens": ["\u00bb", "aus", "Bu\u00b7ben\u00b7hand", "nehm'", "ich", "kein", "Le\u00b7ben", "an", "!", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "PPER", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Zorngl\u00fchend rufst du's aus, zerrei\u00dfest die Verbande,", "tokens": ["Zorn\u00b7gl\u00fc\u00b7hend", "rufst", "du's", "aus", ",", "zer\u00b7rei\u00b7\u00dfest", "die", "Ver\u00b7ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PIS", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und deine Seele fliegt zum Vaterlande.", "tokens": ["Und", "dei\u00b7ne", "See\u00b7le", "fliegt", "zum", "Va\u00b7ter\u00b7lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch kann ich nichts als deine Gr\u00f6\u00dfe singen;", "tokens": ["Noch", "kann", "ich", "nichts", "als", "dei\u00b7ne", "Gr\u00f6\u00b7\u00dfe", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "KOKOM", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wenn die Schlachtenlosung niederf\u00e4llt,", "tokens": ["Doch", "wenn", "die", "Schlach\u00b7ten\u00b7lo\u00b7sung", "nie\u00b7der\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn die Trompeten todeslustig klingen", "tokens": ["Wenn", "die", "Trom\u00b7pe\u00b7ten", "to\u00b7des\u00b7lus\u00b7tig", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "---+-+-+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Und der W\u00fcrgengel seine Hochzeit h\u00e4lt:", "tokens": ["Und", "der", "W\u00fcr\u00b7gen\u00b7gel", "sei\u00b7ne", "Hoch\u00b7zeit", "h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Dann darf ich dir den Stahl der Rache schwingen;", "tokens": ["Dann", "darf", "ich", "dir", "den", "Stahl", "der", "Ra\u00b7che", "schwin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der erste, der sich meinem Schwerte stellt,", "tokens": ["Der", "ers\u00b7te", ",", "der", "sich", "mei\u00b7nem", "Schwer\u00b7te", "stellt", ","], "token_info": [