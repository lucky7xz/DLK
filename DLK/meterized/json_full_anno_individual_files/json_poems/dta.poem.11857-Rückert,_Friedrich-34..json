{"dta.poem.11857": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "34.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seelen waren in der Weltseel' einst beisammen,", "tokens": ["Die", "See\u00b7len", "wa\u00b7ren", "in", "der", "Welt\u00b7seel'", "einst", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie Tropfen in dem Meer, alswie im Feuer Flammen.", "tokens": ["Wie", "Trop\u00b7fen", "in", "dem", "Meer", ",", "als\u00b7wie", "im", "Feu\u00b7er", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "$,", "KOKOM", "APPRART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Weltleib halfen sie beseelen und beleben,", "tokens": ["Den", "Welt\u00b7leib", "hal\u00b7fen", "sie", "be\u00b7see\u00b7len", "und", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von ihnen keiner war ein eigner Leib gegeben.", "tokens": ["Von", "ih\u00b7nen", "kei\u00b7ner", "war", "ein", "eig\u00b7ner", "Leib", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sie aber w\u00fcnschten nun ein eigenes Gebiet,", "tokens": ["Sie", "a\u00b7ber", "w\u00fcnschten", "nun", "ein", "ei\u00b7ge\u00b7nes", "Ge\u00b7biet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+---+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Darin zu herrschen, wie der eigne Trieb es rieth.", "tokens": ["Da\u00b7rin", "zu", "herr\u00b7schen", ",", "wie", "der", "eig\u00b7ne", "Trieb", "es", "rieth", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PTKZU", "VVINF", "$,", "PWAV", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Und abgegrenzet ward ein Weichbild so f\u00fcr jede,", "tokens": ["Und", "ab\u00b7ge\u00b7gren\u00b7zet", "ward", "ein", "Weich\u00b7bild", "so", "f\u00fcr", "je\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VAFIN", "ART", "NN", "ADV", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df zwischen ihnen nicht Verwirrung werd' und Fehde.", "tokens": ["Da\u00df", "zwi\u00b7schen", "ih\u00b7nen", "nicht", "Ver\u00b7wir\u00b7rung", "werd'", "und", "Feh\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPER", "PTKNEG", "NN", "VAFIN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nun wirkt gesondert jed' in ihrem eignen Leibe,", "tokens": ["Nun", "wirkt", "ge\u00b7son\u00b7dert", "jed'", "in", "ih\u00b7rem", "eig\u00b7nen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVPP", "PIAT", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie mit der Weltseel' einst in Sonn- und Mondenscheibe.", "tokens": ["Wie", "mit", "der", "Welt\u00b7seel'", "einst", "in", "Sonn", "und", "Mon\u00b7den\u00b7schei\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ADV", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Sonn- und Mondenscheib' ist nicht dadurch verglommen,", "tokens": ["Die", "Sonn", "und", "Mon\u00b7den\u00b7scheib'", "ist", "nicht", "da\u00b7durch", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "TRUNC", "KON", "NN", "VAFIN", "PTKNEG", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch sch\u00f6ne Glieder sind zum Vorschein hier gekommen.", "tokens": ["Doch", "sch\u00f6\u00b7ne", "Glie\u00b7der", "sind", "zum", "Vor\u00b7schein", "hier", "ge\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "APPRART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Darum gesegnet sei der Seele Trieb, zu walten", "tokens": ["Da\u00b7rum", "ge\u00b7seg\u00b7net", "sei", "der", "See\u00b7le", "Trieb", ",", "zu", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PAV", "VVPP", "VAFIN", "ART", "NN", "NN", "$,", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In einem Leib, und sch\u00f6n des Ird'sche zu gestalten.", "tokens": ["In", "ei\u00b7nem", "Leib", ",", "und", "sch\u00f6n", "des", "Ird'\u00b7sche", "zu", "ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}