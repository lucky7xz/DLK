{"textgrid.poem.38896": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Himmelblau", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie alle umschlie\u00df' ich mit Armen linde,", "tokens": ["Sie", "al\u00b7le", "um\u00b7schlie\u00df'", "ich", "mit", "Ar\u00b7men", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "APPR", "NN", "ADJA", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Sie alle tr\u00e4nk' ich an meinen Br\u00fcsten", "tokens": ["Sie", "al\u00b7le", "tr\u00e4nk'", "ich", "an", "mei\u00b7nen", "Br\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit L\u00fcsten,", "tokens": ["Mit", "L\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Ich sende die k\u00fchlenden Winde,", "tokens": ["Ich", "sen\u00b7de", "die", "k\u00fch\u00b7len\u00b7den", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ich schaue tief auf sie hinunter,", "tokens": ["Ich", "schau\u00b7e", "tief", "auf", "sie", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie alle schauen hoch zu mir daher,", "tokens": ["Sie", "al\u00b7le", "schau\u00b7en", "hoc