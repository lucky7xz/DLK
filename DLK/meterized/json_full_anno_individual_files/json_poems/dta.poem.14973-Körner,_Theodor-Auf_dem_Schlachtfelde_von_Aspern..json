{"dta.poem.14973": {"metadata": {"author": {"name": "K\u00f6rner, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Schlachtfelde von Aspern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1814", "urn": "urn:nbn:de:kobv:b4-200905193258", "language": ["de:0.99"], "booktitle": "K\u00f6rner, Theodor: Leyer und Schwerdt. Berlin, 1814."}, "poem": {"stanza.1": {"line.1": {"text": "Wo der Deutsche seine Kraft verb\u00fcrgte,", "tokens": ["Wo", "der", "Deut\u00b7sche", "sei\u00b7ne", "Kraft", "ver\u00b7b\u00fcrg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Heil'ger Boden! dich gr\u00fc\u00dft mein Gesang!", "tokens": ["Heil'\u00b7ger", "Bo\u00b7den", "!", "dich", "gr\u00fc\u00dft", "mein", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Frankreichs stolze Adler sahst du zittern,", "tokens": ["Fran\u00b7kreichs", "stol\u00b7ze", "Ad\u00b7ler", "sahst", "du", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Sahst des W\u00fcthrichs Eisenkraft zersplittern,", "tokens": ["Sahst", "des", "W\u00fcth\u00b7richs", "Ei\u00b7sen\u00b7kraft", "zer\u00b7split\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Die sich frech die halbe Welt bezwang. \u2014", "tokens": ["Die", "sich", "frech", "die", "hal\u00b7be", "Welt", "be\u00b7zwang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PRF", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Euch! ihr Manen der gefallnen Helden,", "tokens": ["Euch", "!", "ihr", "Ma\u00b7nen", "der", "ge\u00b7fall\u00b7nen", "Hel\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$.", "PPOSAT", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Deren Blick im Siegesdonner brach,", "tokens": ["De\u00b7ren", "Blick", "im", "Sie\u00b7ges\u00b7don\u00b7ner", "brach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Ruf' ich in den Fr\u00fchling eurer Welten", "tokens": ["Ruf'", "ich", "in", "den", "Fr\u00fch\u00b7ling", "eu\u00b7rer", "Wel\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Meines Herzens ganzen Jubel nach.", "tokens": ["Mei\u00b7nes", "Her\u00b7zens", "gan\u00b7zen", "Ju\u00b7bel", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich damals nicht bei euch gestanden! \u2014", "tokens": ["Da\u00df", "ich", "da\u00b7mals", "nicht", "bei", "euch", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "APPR", "PPER", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Da\u00df, wo Br\u00fcder Sieg und Freiheit fanden,", "tokens": ["Da\u00df", ",", "wo", "Br\u00fc\u00b7der", "Sieg", "und", "Frei\u00b7heit", "fan\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "PWAV", "NN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ich trotz Kraft und Jugend doch gefehlt!", "tokens": ["Ich", "trotz", "Kraft", "und", "Ju\u00b7gend", "doch", "ge\u00b7fehlt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Gl\u00fcckliche, die ihr den Tag erfochten,", "tokens": ["Gl\u00fcck\u00b7li\u00b7che", ",", "die", "ihr", "den", "Tag", "er\u00b7foch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Ew'ge Lorbeern habt ihr euch geflochten,", "tokens": ["Ew'\u00b7ge", "Lor\u00b7beern", "habt", "ihr", "euch", "ge\u00b7floch\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Zum Triumph des Vaterlands erw\u00e4hlt. \u2014", "tokens": ["Zum", "Tri\u00b7umph", "des", "Va\u00b7ter\u00b7lands", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Schwarz und traurig wie auf Grabestr\u00fcmmern", "tokens": ["Schwarz", "und", "trau\u00b7rig", "wie", "auf", "Gra\u00b7be\u00b7str\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADJD", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "W\u00e4lzt auf Deutschland sich des Schicksals Macht,", "tokens": ["W\u00e4lzt", "auf", "Deutschland", "sich", "des", "Schick\u00b7sals", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NE", "PRF", "ART", "NN", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.9": {"text": "Doch begeisternd wie mit Sternesschimmern", "tokens": ["Doch", "be\u00b7geis\u00b7ternd", "wie", "mit", "Ster\u00b7nes\u00b7schim\u00b7mern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "KOKOM", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Bricht der eine Tag durch unsre Nacht.", "tokens": ["Bricht", "der", "ei\u00b7ne", "Tag", "durch", "uns\u00b7re", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sonnenhauch in d\u00fcstern Nebeljahren!", "tokens": ["Son\u00b7nen\u00b7hauch", "in", "d\u00fcs\u00b7tern", "Ne\u00b7bel\u00b7jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Strahlen la\u00df uns treu bewahren,", "tokens": ["Dei\u00b7ne", "Strah\u00b7len", "la\u00df", "uns", "treu", "be\u00b7wah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Als Verm\u00e4chtni\u00df einer stolzen Zeit.", "tokens": ["Als", "Ver\u00b7m\u00e4cht\u00b7ni\u00df", "ei\u00b7ner", "stol\u00b7zen", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ueberall im gro\u00dfen Vaterlande", "tokens": ["Ue\u00b7be\u00b7rall", "im", "gro\u00b7\u00dfen", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Von der Ostsee bis zum Donaustrande,", "tokens": ["Von", "der", "Ost\u00b7see", "bis", "zum", "Do\u00b7naus\u00b7tran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Macht dein Name alle Herzen weit.", "tokens": ["Macht", "dein", "Na\u00b7me", "al\u00b7le", "Her\u00b7zen", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aspern klingts, und Karl klingts siegestrunken,", "tokens": ["As\u00b7pern", "klingts", ",", "und", "Karl", "klingts", "sie\u00b7ge\u00b7strun\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "KON", "NE", "VVFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wo nur deutsch die Lippe lallen kann.", "tokens": ["Wo", "nur", "deutsch", "die", "Lip\u00b7pe", "lal\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Nein Germanien ist nicht gesunken,", "tokens": ["Nein", "Ger\u00b7ma\u00b7ni\u00b7en", "ist", "nicht", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Hat noch ", "tokens": ["Hat", "noch"], "token_info": ["word", "word"], "pos": ["VAFIN", "ADV"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Und so lange deutsche Str\u00f6me sausen,", "tokens": ["Und", "so", "lan\u00b7ge", "deut\u00b7sche", "Str\u00f6\u00b7me", "sau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und so lange deutsche Lieder brausen,", "tokens": ["Und", "so", "lan\u00b7ge", "deut\u00b7sche", "Lie\u00b7der", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Gelten diese Namen ihren Klang.", "tokens": ["Gel\u00b7ten", "die\u00b7se", "Na\u00b7men", "ih\u00b7ren", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Was die Tage auch zerschmettert haben", "tokens": ["Was", "die", "Ta\u00b7ge", "auch", "zer\u00b7schmet\u00b7tert", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "ADV", "VVPP", "VAINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Karl und Aspern ist ins Herz gegraben", "tokens": ["Karl", "und", "As\u00b7pern", "ist", "ins", "Herz", "ge\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VAFIN", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Karl und Aspern donnert im Gesang.", "tokens": ["Karl", "und", "As\u00b7pern", "don\u00b7nert", "im", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Mag der Staub gefallner Helden modern,", "tokens": ["Mag", "der", "Staub", "ge\u00b7fall\u00b7ner", "Hel\u00b7den", "mo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die dem gro\u00dfen Tode sich geweiht,", "tokens": ["Die", "dem", "gro\u00b7\u00dfen", "To\u00b7de", "sich", "ge\u00b7weiht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Ihres Ruhmes Flammenz\u00fcge lodern,", "tokens": ["Ih\u00b7res", "Ruh\u00b7mes", "Flam\u00b7men\u00b7z\u00fc\u00b7ge", "lo\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "In dem Tempel der Unsterblichkeit.", "tokens": ["In", "dem", "Tem\u00b7pel", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}}, "stanza.5": {"line.1": {"text": "Aber nicht, wie sie die Nachwelt richte,", "tokens": ["A\u00b7ber", "nicht", ",", "wie", "sie", "die", "Nach\u00b7welt", "rich\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht die ew'ge Stimme der Geschichte,", "tokens": ["Nicht", "die", "ew'\u00b7ge", "Stim\u00b7me", "der", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Rei\u00dft der Mitwelt gro\u00dfe Schuld entzwei.", "tokens": ["Rei\u00dft", "der", "Mit\u00b7welt", "gro\u00b7\u00dfe", "Schuld", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihre Todesweihe lebt im Liede,", "tokens": ["Ih\u00b7re", "To\u00b7des\u00b7wei\u00b7he", "lebt", "im", "Lie\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Doch umsonst such ich die Pyramide,", "tokens": ["Doch", "um\u00b7sonst", "such", "ich", "die", "Py\u00b7ra\u00b7mi\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.6": {"text": "Die der Denkstein ihrer Gr\u00f6\u00dfe sey.", "tokens": ["Die", "der", "Denk\u00b7stein", "ih\u00b7rer", "Gr\u00f6\u00b7\u00dfe", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPOSAT", "NN", "VAFIN", "$."], "meter": "--+---+-+", "measure": "anapaest.init"}, "line.7": {"text": "Auf dem Wahlplatz heiligten die Ahnen,", "tokens": ["Auf", "dem", "Wahl\u00b7platz", "hei\u00b7lig\u00b7ten", "die", "Ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihrer Eichen stolze Riesenpracht,", "tokens": ["Ih\u00b7rer", "Ei\u00b7chen", "stol\u00b7ze", "Rie\u00b7sen\u00b7pracht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und die Irmens\u00e4ule der Germanen,", "tokens": ["Und", "die", "Ir\u00b7men\u00b7s\u00e4u\u00b7le", "der", "Ger\u00b7ma\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Sprach von der geschlag'nen R\u00f6merschlacht.", "tokens": ["Sprach", "von", "der", "ge\u00b7schlag'\u00b7nen", "R\u00f6\u00b7mer\u00b7schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "In dem blutgen Thal der Thermopylen,", "tokens": ["In", "dem", "blut\u00b7gen", "Thal", "der", "Ther\u00b7mo\u00b7py\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Griechen freie Schaaren fielen,", "tokens": ["Wo", "der", "Grie\u00b7chen", "frei\u00b7e", "Schaa\u00b7ren", "fie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Grub's in Marmor ihrer Br\u00fcder Dank:", "tokens": ["Grub's", "in", "Mar\u00b7mor", "ih\u00b7rer", "Br\u00fc\u00b7der", "Dank", ":"], "token_info": ["word", "word", "word", "word", "word", "wo