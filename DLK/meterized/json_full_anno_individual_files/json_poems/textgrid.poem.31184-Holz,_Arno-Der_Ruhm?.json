{"textgrid.poem.31184": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Der Ruhm?", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ruhm?", "tokens": ["Der", "Ruhm", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ein Ding, das unter sogenannten Br\u00fcdern", "tokens": ["Ein", "Ding", ",", "das", "un\u00b7ter", "so\u00b7ge\u00b7nann\u00b7ten", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Fast so reel wie eine Seifenblase?", "tokens": ["Fast", "so", "reel", "wie", "ei\u00b7ne", "Sei\u00b7fen\u00b7bla\u00b7se", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Geh, lass dir deine Nase putzen, Junge!", "tokens": ["Geh", ",", "lass", "dir", "dei\u00b7ne", "Na\u00b7se", "put\u00b7zen", ",", "Jun\u00b7ge", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein Rollmops, den die Mitwelt mit mir theilt,", "tokens": ["Ein", "Roll\u00b7mops", ",", "den", "die", "Mit\u00b7welt", "mit", "mir", "theilt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.6": {"text": "Wird mir unendlich schmeichelhafter sein,", "tokens": ["Wird", "mir", "un\u00b7end\u00b7lich", "schmei\u00b7chel\u00b7haf\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als tausend stilgerechte Mausoleen.", "tokens": ["Als", "tau\u00b7send", "stil\u00b7ge\u00b7rech\u00b7te", "Mau\u00b7so\u00b7leen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die enthusiastisch mir die Nachwelt baut.", "tokens": ["Die", "en\u00b7thu\u00b7si\u00b7as\u00b7tisch", "mir", "die", "Nach\u00b7welt", "baut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.9": {"text": "Auch ist es L\u00fcge, dass die Liebe sich", "tokens": ["Auch", "ist", "es", "L\u00fc\u00b7ge", ",", "dass", "die", "Lie\u00b7be", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$,", "KOUS", "ART", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mitunter auf ein Rosenblatt verirrt.", "tokens": ["Mi\u00b7tun\u00b7ter", "auf", "ein", "Ro\u00b7sen\u00b7blatt", "ver\u00b7irrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Auf dieses Monstrum hab ich Jagd gemacht", "tokens": ["Auf", "die\u00b7ses", "Monst\u00b7rum", "hab", "ich", "Jagd", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wie ein Professor, der Botanik liest,", "tokens": ["Wie", "ein", "Pro\u00b7fes\u00b7sor", ",", "der", "Bo\u00b7ta\u00b7nik", "liest", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Vom Brocken bis zum Popokatepetl.", "tokens": ["Vom", "Bro\u00b7cken", "bis", "zum", "Po\u00b7po\u00b7ka\u00b7te\u00b7petl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Doch, was die Dichter mir auch vorgefaselt,", "tokens": ["Doch", ",", "was", "die", "Dich\u00b7ter", "mir", "auch", "vor\u00b7ge\u00b7fa\u00b7selt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "ART", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.15": {"text": "Ich fand sie all mein Lebtag nur im Kuhdreck!", "tokens": ["Ich", "fand", "sie", "all", "mein", "Leb\u00b7tag", "nur", "im", "Kuh\u00b7dreck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Ruhm?", "tokens": ["Der", "Ruhm", "?"], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ein Ding, das unter sogenannten Br\u00fcdern", "tokens": ["Ein", "Ding", ",", "das", "un\u00b7ter", "so\u00b7ge\u00b7nann\u00b7ten", "Br\u00fc\u00b7dern"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"],