{"textgrid.poem.28557": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "72.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der redliche Piast, begabt mit fromer G\u00fcte,", "tokens": ["Der", "red\u00b7li\u00b7che", "Pi\u00b7ast", ",", "be\u00b7gabt", "mit", "fro\u00b7mer", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gerechtigkeit und Treu, lud alles Volcks Gem\u00fcte", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "und", "Treu", ",", "lud", "al\u00b7les", "Volcks", "Ge\u00b7m\u00fc\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "VVFIN", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Wolthat ihm zur Gunst und trug die Polsche Kron", "tokens": ["Durch", "Wolt\u00b7hat", "ihm", "zur", "Gunst", "und", "trug", "die", "Pol\u00b7sche", "Kron"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "PPER", "APPRART", "NN", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "(vielmehr, was ewig ist, unsterblich Lob) davon.", "tokens": ["(", "viel\u00b7mehr", ",", "was", "e\u00b7wig", "ist", ",", "uns\u00b7terb\u00b7lich", "Lob", ")", "da\u00b7von", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "$,", "PRELS", "ADJD", "VAFIN", "$,", "ADJD", "NN", "$(", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.i