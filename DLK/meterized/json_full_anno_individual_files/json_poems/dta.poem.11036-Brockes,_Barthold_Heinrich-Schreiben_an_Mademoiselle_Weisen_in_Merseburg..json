{"dta.poem.11036": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Schreiben  \n an  Mademoiselle  Weisen  \n in Merseburg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da sie in der Copie von Gottes Wunderwerken,", "tokens": ["Da", "sie", "in", "der", "Co\u00b7pie", "von", "Got\u00b7tes", "Wun\u00b7der\u00b7wer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So viel Vergn\u00fcgen f\u00fchlt und zeigt; wie wei\u00df, wie rein;", "tokens": ["So", "viel", "Ver\u00b7gn\u00fc\u00b7gen", "f\u00fchlt", "und", "zeigt", ";", "wie", "wei\u00df", ",", "wie", "rein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "KON", "VVFIN", "$.", "PWAV", "VVFIN", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weise mu\u00df der ", "tokens": ["Und", "wei\u00b7se", "mu\u00df", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da sie dem, welcher nur davon was gutes denket,", "tokens": ["Da", "sie", "dem", ",", "wel\u00b7cher", "nur", "da\u00b7von", "was", "gu\u00b7tes", "den\u00b7ket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "$,", "PRELS", "ADV", "PAV", "PWS", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durchs Urbild recht ger\u00fchrt, so was vollkommnes schenket.", "tokens": ["Durchs", "Ur\u00b7bild", "recht", "ge\u00b7r\u00fchrt", ",", "so", "was", "voll\u00b7komm\u00b7nes", "schen\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVPP", "$,", "ADV", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}