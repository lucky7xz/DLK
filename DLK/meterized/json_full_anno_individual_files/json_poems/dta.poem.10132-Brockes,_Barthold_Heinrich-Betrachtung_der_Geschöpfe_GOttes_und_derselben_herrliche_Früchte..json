{"dta.poem.10132": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung der Gesch\u00f6pfe GOttes  \n und derselben herrliche Fr\u00fcchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bet', o GOTT, in Deinen Wercken,", "tokens": ["Ich", "bet'", ",", "o", "GoTT", ",", "in", "Dei\u00b7nen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "FM", "NE", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Voll heiliger Verwunderung, Dich an.", "tokens": ["Voll", "hei\u00b7li\u00b7ger", "Ver\u00b7wun\u00b7de\u00b7rung", ",", "Dich", "an", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$,", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ach! la\u00df mich doch, so viel ich kann,", "tokens": ["Ach", "!", "la\u00df", "mich", "doch", ",", "so", "viel", "ich", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVIMP", "PPER", "ADV", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihnen, Dir zur Ehr, in froher Andacht mercken!", "tokens": ["Von", "ih\u00b7nen", ",", "Dir", "zur", "Ehr", ",", "in", "fro\u00b7her", "An\u00b7dacht", "mer\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "APPRART", "NN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Seele, die Du uns gegeben,", "tokens": ["Die", "See\u00b7le", ",", "die", "Du", "uns", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kann ihres Denckens Eigenschaft,", "tokens": ["Kann", "ih\u00b7res", "Den\u00b7ckens", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ihres Wesens beste Krafft,", "tokens": ["Als", "ih\u00b7res", "We\u00b7sens", "bes\u00b7te", "Krafft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu keinem edlern Zweck\u2019, in diesem Leben,", "tokens": ["Zu", "kei\u00b7nem", "ed\u00b7lern", "Zweck'", ",", "in", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wol anzuwenden, sich bestreben;", "tokens": ["Wol", "an\u00b7zu\u00b7wen\u00b7den", ",", "sich", "be\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "$,", "PRF", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Als wenn sie sich auf Dein Gesch\u00f6pfe lencket,", "tokens": ["Als", "wenn", "sie", "sich", "auf", "Dein", "Ge\u00b7sch\u00f6p\u00b7fe", "len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Desselben Sch\u00f6nheit, Nutz und Ordnung \u00fcberdencket,", "tokens": ["Des\u00b7sel\u00b7ben", "Sch\u00f6n\u00b7heit", ",", "Nutz", "und", "Ord\u00b7nung", "\u00fc\u00b7ber\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und, in bewundernder Betrachtung, sich,", "tokens": ["Und", ",", "in", "be\u00b7wun\u00b7dern\u00b7der", "Be\u00b7trach\u00b7tung", ",", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPR", "ADJA", "NN", "$,", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O Allmachts-voller GOTT, in Dich,", "tokens": ["O", "All\u00b7machts\u00b7vol\u00b7ler", "GoTT", ",", "in", "Dich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Als aller Sch\u00f6nheit Sch\u00f6pfer, sencket.", "tokens": ["Als", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "Sch\u00f6p\u00b7fer", ",", "sen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Durch dieses seelige Gesch\u00e4ffte", "tokens": ["Durch", "die\u00b7ses", "see\u00b7li\u00b7ge", "Ge\u00b7sch\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermehren sich in ihr das Wollen und die Kr\u00e4ffte,", "tokens": ["Ver\u00b7meh\u00b7ren", "sich", "in", "ihr", "das", "Wol\u00b7len", "und", "die", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des weisen, m\u00e4chtigen, und g\u00fctgen Sch\u00f6pfers Willen,", "tokens": ["Des", "wei\u00b7sen", ",", "m\u00e4ch\u00b7ti\u00b7gen", ",", "und", "g\u00fct\u00b7gen", "Sch\u00f6p\u00b7fers", "Wil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "$,", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In allen Handlungen und Pflichten, zu erf\u00fcllen.", "tokens": ["In", "al\u00b7len", "Hand\u00b7lun\u00b7gen", "und", "Pflich\u00b7ten", ",", "zu", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}}, "stanza.4": {"line.1": {"text": "Die, in der herrlichen Gesch\u00f6pfe Wunder-Pracht", "tokens": ["Die", ",", "in", "der", "herr\u00b7li\u00b7chen", "Ge\u00b7sch\u00f6p\u00b7fe", "Wun\u00b7der\u00b7Pracht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch immer mehr und mehr erkannte weise Macht,", "tokens": ["Noch", "im\u00b7mer", "mehr", "und", "mehr", "er\u00b7kann\u00b7te", "wei\u00b7se", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Noch immer mehr und mehr versp\u00fchrte Vater-Liebe,", "tokens": ["Noch", "im\u00b7mer", "mehr", "und", "mehr", "ver\u00b7sp\u00fchr\u00b7te", "Va\u00b7ter\u00b7Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erzielen Lust und Danck, erzielen Andachts-Triebe,", "tokens": ["Er\u00b7zie\u00b7len", "Lust", "und", "Danck", ",", "er\u00b7zie\u00b7len", "An\u00b7dachts\u00b7Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jm Gl\u00fcck ein frohes Hertz; ein sehnliches Verlangen,", "tokens": ["Jm", "Gl\u00fcck", "ein", "fro\u00b7hes", "Hertz", ";", "ein", "sehn\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr so viel Gnad\u2019 und Huld, die wir von Jhm empfangen,", "tokens": ["F\u00fcr", "so", "viel", "Gnad'", "und", "Huld", ",", "die", "wir", "von", "Jhm", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN", "$,", "PRELS", "PPER", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu thun, was Jhm gef\u00e4llt: In Wiederw\u00e4rtigkeit,", "tokens": ["Zu", "thun", ",", "was", "Jhm", "ge\u00b7f\u00e4llt", ":", "In", "Wie\u00b7der\u00b7w\u00e4r\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PWS", "PPER", "VVPP", "$.", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Jhm gef\u00e4llige Gelassenheit,", "tokens": ["Die", "Jhm", "ge\u00b7f\u00e4l\u00b7li\u00b7ge", "Ge\u00b7las\u00b7sen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vertrauen und Gedult. Denn wenn ichs recht verstehe,", "tokens": ["Ver\u00b7trau\u00b7en", "und", "Ge\u00b7dult", ".", "Denn", "wenn", "ichs", "recht", "ver\u00b7ste\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$.", "KON", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df eine m\u00e4chtige und weise Lieb\u2019 allein", "tokens": ["Da\u00df", "ei\u00b7ne", "m\u00e4ch\u00b7ti\u00b7ge", "und", "wei\u00b7se", "Lieb'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "KON", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Das, was geschicht, befiehlt; so geh es, wie es gehe;", "tokens": ["Das", ",", "was", "ge\u00b7schicht", ",", "be\u00b7fiehlt", ";", "so", "geh", "es", ",", "wie", "es", "ge\u00b7he", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "VVPP", "$,", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein GOTT vertrauend Hertz wird leicht getr\u00f6stet seyn.", "tokens": ["Ein", "GoTT", "ver\u00b7trau\u00b7end", "Hertz", "wird", "leicht", "ge\u00b7tr\u00f6s\u00b7tet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "NN", "VAFIN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}