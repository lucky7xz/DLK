{"textgrid.poem.37259": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[kinder, lasset uns besingen]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kinder, lasset uns besingen,", "tokens": ["Kin\u00b7der", ",", "las\u00b7set", "uns", "be\u00b7sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aber ohne allen Neid,", "tokens": ["A\u00b7ber", "oh\u00b7ne", "al\u00b7len", "Neid", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Onkel Kaspers rote Nase,", "tokens": ["On\u00b7kel", "Kas\u00b7pers", "ro\u00b7te", "Na\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die uns schon so oft erfreut.", "tokens": ["Die", "uns", "schon", "so", "oft", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Einst ward sie als zarte Pflanze", "tokens": ["Einst", "ward", "sie", "als", "zar\u00b7te", "Pflan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "KOUS", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ihm von der Natur geschenkt;", "tokens": ["Ihm", "von", "der", "Na\u00b7tur", "ge\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Flei\u00dfig hat er sie begossen,", "tokens": ["Flei\u00b7\u00dfig", "hat", "er", "sie", "be\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sie mit Wein und Schnaps getr\u00e4nkt.", "tokens": ["Sie", "mit", "Wein", "und", "Schnaps", "ge\u00b7tr\u00e4nkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald bemerkte er mit Freuden,", "tokens": ["Bald", "be\u00b7merk\u00b7te", "er", "mit", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df die junge Knospe schwoll,", "tokens": ["Da\u00df", "die", "jun\u00b7ge", "Knos\u00b7pe", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bis es eine Rose wurde,", "tokens": ["Bis", "es", "ei\u00b7ne", "Ro\u00b7se", "wur\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dunkelrot und wundervoll.", "tokens": ["Dun\u00b7kel\u00b7rot", "und", "wun\u00b7der\u00b7voll", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Alle Rosen haben Dornen,", "tokens": ["Al\u00b7le", "Ro\u00b7sen", "ha\u00b7ben", "Dor\u00b7nen", ","], "token_i