{"textgrid.poem.37033": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Erde traurt im wei\u00dfen Todtenkleide,", "genre": "verse", "period": "N.A.", "pub_year": 1769, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Erde traurt im wei\u00dfen Todtenkleide,", "tokens": ["Die", "Er\u00b7de", "traurt", "im", "wei\u00b7\u00dfen", "Tod\u00b7ten\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcbergiebt sich tr\u00e4ger Ruh.", "tokens": ["Und", "\u00fc\u00b7berg\u00b7iebt", "sich", "tr\u00e4\u00b7ger", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Westwind haucht dem Wandrer Scherz und Freude", "tokens": ["Kein", "West\u00b7wind", "haucht", "dem", "Wand\u00b7rer", "Scherz", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit frischen Veilchend\u00fcften zu.", "tokens": ["Mit", "fri\u00b7schen", "Veil\u00b7chen\u00b7d\u00fcf\u00b7ten", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Str\u00f6me und der B\u00e4che Urnen schlie\u00dfet", "tokens": ["Der", "Str\u00f6\u00b7me", "und", "der", "B\u00e4\u00b7che", "Ur\u00b7nen", "schlie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des wilden Winters kalte Hand;", "tokens": ["Des", "wil\u00b7den", "Win\u00b7ters", "kal\u00b7te", "Hand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Boreas durchw\u00fchlt die Luft, und gie\u00dfet", "tokens": ["Und", "Bo\u00b7re\u00b7as", "durch\u00b7w\u00fchlt", "die", "Luft", ",", "und", "gie\u00b7\u00dfet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein Meer von Flocken auf das Land.", "tokens": ["Ein", "Meer", "von", "Flo\u00b7cken", "auf", "das", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun sinken auf die W\u00e4lder Silberh\u00fcllen,", "tokens": ["Nun", "sin\u00b7ken", "auf", "die", "W\u00e4l\u00b7der", "Sil\u00b7ber\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und auf das fahle H\u00fcttendach", "tokens": ["Und", "auf", "das", "fah\u00b7le", "H\u00fct\u00b7ten\u00b7dach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Landmanns. Hohe Schneegeb\u00fcrge schwillen", "tokens": ["Des", "Land\u00b7manns", ".", "Ho\u00b7he", "Schnee\u00b7ge\u00b7b\u00fcr\u00b7ge", "schwil\u00b7len"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Rings um den kleinen Wiesenbach.", "tokens": ["Rings", "um", "den", "klei\u00b7nen", "Wie\u00b7sen\u00b7bach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Er murmelt keine Wonne durch die Fluren,", "tokens": ["Er", "mur\u00b7melt", "kei\u00b7ne", "Won\u00b7ne", "durch", "die", "Flu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie er im jungen Fr\u00fchling that.", "tokens": ["Wie", "er", "im", "jun\u00b7gen", "Fr\u00fch\u00b7ling", "that", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An seinem Ufer schlummern welke Spuren", "tokens": ["An", "sei\u00b7nem", "U\u00b7fer", "schlum\u00b7mern", "wel\u00b7ke", "Spu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PWAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Blume, die der Frost zertrat.", "tokens": ["Der", "Blu\u00b7me", ",", "die", "der", "Frost", "zer\u00b7trat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Der Landschaft vormahls bunte Scenen liegen", "tokens": ["Der", "Land\u00b7schaft", "vor\u00b7mahls", "bun\u00b7te", "Sce\u00b7nen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Entstellt. Ein finstrer Schleyr umzieht", "tokens": ["Ent\u00b7stellt", ".", "Ein", "finst\u00b7rer", "Schleyr", "um\u00b7zieht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Tages Antlitz. Neue Flocken fliegen", "tokens": ["Des", "Ta\u00b7ges", "Ant\u00b7litz", ".", "Neu\u00b7e", "Flo\u00b7cken", "flie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Luftraum, wo kein Phoebus gl\u00fcht.", "tokens": ["Im", "Luf\u00b7traum", ",", "wo", "kein", "Phoe\u00b7bus", "gl\u00fcht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sey mir, du Flur, du wei\u00dfgeschleyrte Erde", "tokens": ["Sey", "mir", ",", "du", "Flur", ",", "du", "wei\u00df\u00b7ge\u00b7schleyr\u00b7te", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PPER", "NN", "$,", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gegr\u00fc\u00dfet! Deine Majest\u00e4t", "tokens": ["Ge\u00b7gr\u00fc\u00b7\u00dfet", "!", "Dei\u00b7ne", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bezaubert mich, wiewohl jetzt keine Herde", "tokens": ["Be\u00b7zau\u00b7bert", "mich", ",", "wie\u00b7wohl", "jetzt", "kei\u00b7ne", "Her\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf deinen \u00f6den Triften geht,", "tokens": ["Auf", "dei\u00b7nen", "\u00f6\u00b7den", "Trif\u00b7ten", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und keine Harmonie die Schatteng\u00e4nge", "tokens": ["Und", "kei\u00b7ne", "Har\u00b7mo\u00b7nie", "die", "Schat\u00b7ten\u00b7g\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Waldes f\u00fcllt. Ich liebe dich", "tokens": ["Des", "Wal\u00b7des", "f\u00fcllt", ".", "Ich", "lie\u00b7be", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mehr als den Flitterprunk, und das Gedr\u00e4nge", "tokens": ["Mehr", "als", "den", "Flit\u00b7ter\u00b7prunk", ",", "und", "das", "Ge\u00b7dr\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Stadt, von der die Ruhe wich.", "tokens": ["Der", "Stadt", ",", "von", "der", "die", "Ru\u00b7he", "wich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Sch\u00f6nen wandeln hier im Hermeline", "tokens": ["Die", "Sch\u00f6\u00b7nen", "wan\u00b7deln", "hier", "im", "Her\u00b7me\u00b7li\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den B\u00e4llen zu, und Chloe f\u00e4ngt", "tokens": ["Den", "B\u00e4l\u00b7len", "zu", ",", "und", "Chloe", "f\u00e4ngt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit ihrem Busen, ihrer Zaubermiene", "tokens": ["Mit", "ih\u00b7rem", "Bu\u00b7sen", ",", "ih\u00b7rer", "Zau\u00b7ber\u00b7mie\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Stutzer, der ihr Weyhrauch schenkt.", "tokens": ["Den", "Stut\u00b7zer", ",", "der", "ihr", "Wey\u00b7hrauch", "schenkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Siegerin! Die M\u00e4nnerblicke hangen", "tokens": ["Die", "Sie\u00b7ge\u00b7rin", "!", "Die", "M\u00e4n\u00b7ner\u00b7bli\u00b7cke", "han\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An ihrem Haar, an ihrer Brust,", "tokens": ["An", "ih\u00b7rem", "Haar", ",", "an", "ih\u00b7rer", "Brust", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die immer wallt, an ihren Rosenwangen,", "tokens": ["Die", "im\u00b7mer", "wallt", ",", "an", "ih\u00b7ren", "Ro\u00b7sen\u00b7wan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sie ist ihres Siegs bewu\u00dft.", "tokens": ["Und", "sie", "ist", "ih\u00b7res", "Siegs", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.10": {"line.1": {"text": "Nun rollen, gleich des Windes Fl\u00fcgeln, Schlitten", "tokens": ["Nun", "rol\u00b7len", ",", "gleich", "des", "Win\u00b7des", "Fl\u00fc\u00b7geln", ",", "Schlit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVINF", "$,", "ADV", "ART", "NN", "NN", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch des gedr\u00e4ngten P\u00f6bels Schwall;", "tokens": ["Durch", "des", "ge\u00b7dr\u00e4ng\u00b7ten", "P\u00f6\u00b7bels", "Schwall", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Stentor trabt mit abgeme\u00dfnen Schritten,", "tokens": ["Und", "Sten\u00b7tor", "trabt", "mit", "ab\u00b7ge\u00b7me\u00df\u00b7nen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sobald der Abend winkt, dem Ball.", "tokens": ["So\u00b7bald", "der", "A\u00b7bend", "winkt", ",", "dem", "Ball", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Entgegen, wo sein Lockenbau und Weste", "tokens": ["Ent\u00b7ge\u00b7gen", ",", "wo", "sein", "Lo\u00b7cken\u00b7bau", "und", "Wes\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Sch\u00f6nen Augen auf sich rei\u00dft.", "tokens": ["Der", "Sch\u00f6\u00b7nen", "Au\u00b7gen", "auf", "sich", "rei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sein Federhut verr\u00e4th, er sey der gr\u00f6\u00dfte", "tokens": ["Sein", "Fe\u00b7der\u00b7hut", "ver\u00b7r\u00e4\u00b7th", ",", "er", "sey", "der", "gr\u00f6\u00df\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Erfindungsvollste, feinste Geist.", "tokens": ["Er\u00b7fin\u00b7dungs\u00b7volls\u00b7te", ",", "feins\u00b7te", "Geist", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Hier dreht man sich im Tanze,", "tokens": ["Hier", "dreht", "man", "sich", "im", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der labyrinthisch sich verstrickt,", "tokens": ["Der", "la\u00b7by\u00b7rin\u00b7thisch", "sich", "ver\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und von der jungen Sch\u00f6nen Myrtenkranze", "tokens": ["Und", "von", "der", "jun\u00b7gen", "Sch\u00f6\u00b7nen", "Myr\u00b7ten\u00b7kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird oft ein Bl\u00e4tchen abgepfl\u00fcckt.", "tokens": ["Wird", "oft", "ein", "Bl\u00e4t\u00b7chen", "ab\u00b7ge\u00b7pfl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Die Erde traurt im wei\u00dfen Todtenkleide,", "tokens": ["Die", "Er\u00b7de", "traurt", "im", "wei\u00b7\u00dfen", "Tod\u00b7ten\u00b7klei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und \u00fcbergiebt sich tr\u00e4ger Ruh.", "tokens": ["Und", "\u00fc\u00b7berg\u00b7iebt", "sich", "tr\u00e4\u00b7ger", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Westwind haucht dem Wandrer Scherz und Freude", "tokens": ["Kein", "West\u00b7wind", "haucht", "dem", "Wand\u00b7rer", "Scherz", "und", "Freu\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit frischen Veilchend\u00fcften zu.", "tokens": ["Mit", "fri\u00b7schen", "Veil\u00b