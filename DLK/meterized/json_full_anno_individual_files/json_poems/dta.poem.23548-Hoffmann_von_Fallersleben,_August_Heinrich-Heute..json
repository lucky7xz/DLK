{"dta.poem.23548": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Heute.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nur nicht \u00e4ngstlich! nur nicht klagen!", "tokens": ["Nur", "nicht", "\u00e4ngst\u00b7lich", "!", "nur", "nicht", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJD", "$.", "ADV", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df doch, was Dir nicht behagt!", "tokens": ["La\u00df", "doch", ",", "was", "Dir", "nicht", "be\u00b7hagt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "PRELS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Willst du nach dem Schicksal fragen?", "tokens": ["Willst", "du", "nach", "dem", "Schick\u00b7sal", "fra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat es je nach Dir gefragt?", "tokens": ["Hat", "es", "je", "nach", "Dir", "ge\u00b7fragt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Willst Du wie die Kinder scherzen?", "tokens": ["Willst", "Du", "wie", "die", "Kin\u00b7der", "scher\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "S\u00fc\u00df ist wohl der Kindheit Lust;", "tokens": ["S\u00fc\u00df", "ist", "wohl", "der", "Kind\u00b7heit", "Lust", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "W\u00e4rst Du Dir im M\u00e4nnerherzen", "tokens": ["W\u00e4rst", "Du", "Dir", "im", "M\u00e4n\u00b7ner\u00b7her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jener Freuden auch bewusst?", "tokens": ["Je\u00b7ner", "Freu\u00b7den", "auch", "be\u00b7wusst", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "In der Wiege des Bewusstseins", "tokens": ["In", "der", "Wie\u00b7ge", "des", "Be\u00b7wusst\u00b7seins"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liegt der S\u00e4ugling Deines Gl\u00fccks;", "tokens": ["Liegt", "der", "S\u00e4ug\u00b7ling", "Dei\u00b7nes", "Gl\u00fccks", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wirf den Gaukel alles Lustscheins", "tokens": ["Wirf", "den", "Gau\u00b7kel", "al\u00b7les", "Lust\u00b7scheins"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der Erinnerung hinterr\u00fccks!", "tokens": ["Der", "E\u00b7rin\u00b7ne\u00b7rung", "hin\u00b7ter\u00b7r\u00fccks", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}}}}