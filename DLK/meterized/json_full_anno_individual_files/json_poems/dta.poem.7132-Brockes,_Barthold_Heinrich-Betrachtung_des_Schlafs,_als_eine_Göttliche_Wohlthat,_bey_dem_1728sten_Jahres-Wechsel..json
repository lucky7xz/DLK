{"dta.poem.7132": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung des Schlafs,  \n als  \n eine G\u00f6ttliche Wohlthat,  \n bey dem 1728sten Jahres-Wechsel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein Traum geschickt uns zu entdecken,", "tokens": ["Es", "ist", "ein", "Traum", "ge\u00b7schickt", "uns", "zu", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von welcher Art die Feuchtigkeiten seyn,", "tokens": ["Von", "wel\u00b7cher", "Art", "die", "Feuch\u00b7tig\u00b7kei\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die in des C\u00f6rpers Mischung stecken.", "tokens": ["Die", "in", "des", "C\u00f6r\u00b7pers", "Misc\u00b7hung", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich deucht ich treff\u2019 hierinnen etwas an,", "tokens": ["Mich", "deucht", "ich", "treff'", "hie\u00b7rin\u00b7nen", "et\u00b7was", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PAV", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das uns zu herrlichen Gedancken leiten kan.", "tokens": ["Das", "uns", "zu", "herr\u00b7li\u00b7chen", "Ge\u00b7dan\u00b7cken", "lei\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}