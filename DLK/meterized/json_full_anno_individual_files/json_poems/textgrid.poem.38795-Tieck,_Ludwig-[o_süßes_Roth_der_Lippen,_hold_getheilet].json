{"textgrid.poem.38795": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "[o s\u00fc\u00dfes Roth der Lippen, hold getheilet]", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O s\u00fc\u00dfes Roth der Lippen, hold getheilet,", "tokens": ["O", "s\u00fc\u00b7\u00dfes", "Roth", "der", "Lip\u00b7pen", ",", "hold", "ge\u00b7thei\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ART", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein liebend Paar, in ungetrennten K\u00fcssen,", "tokens": ["Ein", "lie\u00b7bend", "Paar", ",", "in", "un\u00b7ge\u00b7trenn\u00b7ten", "K\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Blumenlager, wo die Sorgen m\u00fcssen", "tokens": ["Du", "Blu\u00b7men\u00b7la\u00b7ger", ",", "wo", "die", "Sor\u00b7gen", "m\u00fcs\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "NN", "$,", "PWAV", "ART", "NN", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im Spiel mit Liebesg\u00f6ttern fliehn geheilet.", "tokens": ["Im", "Spiel", "mit", "Lie\u00b7bes\u00b7g\u00f6t\u00b7tern", "fliehn", "ge\u00b7hei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVINF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie \u00fcber diesen Flammenweg hineilet", "tokens": ["Wie", "\u00fc\u00b7ber", "die\u00b7sen", "Flam\u00b7men\u00b7weg", "hi\u00b7n