{"textgrid.poem.37120": {"metadata": {"author": {"name": "Boie, Heinrich Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das Gewitter", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbchloe, siehst du nicht voll Grausen", "tokens": ["\u00bb", "chloe", ",", "siehst", "du", "nicht", "voll", "Grau\u00b7sen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort die Donnerwolken ziehn?", "tokens": ["Dort", "die", "Don\u00b7ner\u00b7wol\u00b7ken", "ziehn", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "H\u00f6rst du nicht die Winde brausen?", "tokens": ["H\u00f6rst", "du", "nicht", "die", "Win\u00b7de", "brau\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "La\u00df, Geliebte, la\u00df uns fliehn.", "tokens": ["La\u00df", ",", "Ge\u00b7lieb\u00b7te", ",", "la\u00df", "uns", "fliehn", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "NN", "$,", "VVIMP", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo das breite Dach der Buchen", "tokens": ["Wo", "das", "brei\u00b7te", "Dach", "der", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eine Zuflucht uns verspricht,", "tokens": ["Ei\u00b7ne", "Zu\u00b7flucht", "uns", "ver\u00b7spricht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Eile sie mit mir zu suchen!\u00ab \u2013", "tokens": ["Ei\u00b7le", "sie", "mit", "mir", "zu", "su\u00b7chen", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "PTKZU", "VVINF", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Chloe schwieg und eilte nicht.", "tokens": ["Chloe", "schwieg", "und", "eil\u00b7te", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Eine Hirtin, die die Liebe,", "tokens": ["Ei\u00b7ne", "Hir\u00b7tin", ",", "die", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich und ihren Sch\u00e4fer kennt,", "tokens": ["Sich", "und", "ih\u00b7ren", "Sch\u00e4\u00b7fer", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gerne treu der Tugend bliebe", "tokens": ["Ger\u00b7ne", "treu", "der", "Tu\u00b7gend", "blie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und doch heimlich f\u00fcr ihn brennt,", "tokens": ["Und", "doch", "heim\u00b7lich", "f\u00fcr", "ihn", "brennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Siehet \u00fcberall Gefahren,", "tokens": ["Sie\u00b7het", "\u00fc\u00b7be\u00b7rall", "Ge\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Trauet nie des Sch\u00e4fers Wort.", "tokens": ["Trau\u00b7et", "nie", "des", "Sch\u00e4\u00b7fers", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn hier Blitze schrecklich waren,", "tokens": ["Wenn", "hier", "Blit\u00b7ze", "schreck\u00b7lich", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "ADJD", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War es ihr Alexis dort.", "tokens": ["War", "es", "ihr", "A\u00b7le\u00b7xis", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "NE", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.3": {"line.1": {"text": "Aber schwarz und schw\u00e4rzer immer", "tokens": ["A\u00b7ber", "schwarz", "und", "schw\u00e4r\u00b7zer", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADJA", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht das Wetter sich herauf.", "tokens": ["Zieht", "das", "Wet\u00b7ter", "sich", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alles ist ein falber Schimmer,", "tokens": ["Al\u00b7les", "ist", "ein", "fal\u00b7ber", "Schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange Donner folgen drauf.", "tokens": ["Lan\u00b7ge", "Don\u00b7ner", "fol\u00b7gen", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zweifelnd noch in dem Entschlu\u00dfe", "tokens": ["Zwei\u00b7felnd", "noch", "in", "dem", "Ent\u00b7schlu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht sie, bleibt sie wieder stehn:", "tokens": ["Geht", "sie", ",", "bleibt", "sie", "wie\u00b7der", "stehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Furcht hei\u00dft sie mit einem Fu\u00dfe,", "tokens": ["Furcht", "hei\u00dft", "sie", "mit", "ei\u00b7nem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Liebe mit dem andern gehn.", "tokens": ["Lie\u00b7be", "mit", "dem", "an\u00b7dern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jetzo schon auf halbem Wege", "tokens": ["Jet\u00b7zo", "schon", "auf", "hal\u00b7bem", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt 