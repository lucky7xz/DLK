ra"}}, "stanza.5": {"line.1": {"text": "Endlich bei des Mondes Scheine", "tokens": ["End\u00b7lich", "bei", "des", "Mon\u00b7des", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehrte mit verst\u00f6rtem Blick,", "tokens": ["Kehr\u00b7te", "mit", "ver\u00b7st\u00f6r\u00b7tem", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Chloe langsam aus dem Haine", "tokens": ["Chloe", "lang\u00b7sam", "aus", "dem", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Alexis Arm zur\u00fcck.", "tokens": ["An", "A\u00b7le\u00b7xis", "Arm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nachtigallen sangen Lieder,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "san\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Duftend lag die Flur umher,", "tokens": ["Duf\u00b7tend", "lag", "die", "Flur", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruhig war der Himmel wieder,", "tokens": ["Ru\u00b7hig", "war", "der", "Him\u00b7mel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "