 "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange Donner folgen drauf.", "tokens": ["Lan\u00b7ge", "Don\u00b7ner", "fol\u00b7gen", "drauf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zweifelnd noch in dem Entschlu\u00dfe", "tokens": ["Zwei\u00b7felnd", "noch", "in", "dem", "Ent\u00b7schlu\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Geht sie, bleibt sie wieder stehn:", "tokens": ["Geht", "sie", ",", "bleibt", "sie", "wie\u00b7der", "stehn", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Furcht hei\u00dft sie mit einem Fu\u00dfe,", "tokens": ["Furcht", "hei\u00dft", "sie", "mit", "ei\u00b7nem", "Fu\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Liebe mit dem andern gehn.", "tokens": ["Lie\u00b7be", "mit", "dem", "an\u00b7dern", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Jetzo schon auf halbem Wege", "tokens": ["Jet\u00b7zo", "schon", "auf", "hal\u00b7bem", "We\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4lt sie pl\u00f6tzlich wieder ein.", "tokens": ["H\u00e4lt", "sie", "pl\u00f6tz\u00b7lich", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Regen, Sturm und Donnerschl\u00e4ge", "tokens": ["Re\u00b7gen", ",", "Sturm", "und", "Don\u00b7ner\u00b7schl\u00e4\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Treiben sie zuletzt hinein.", "tokens": ["Trei\u00b7ben", "sie", "zu\u00b7letzt", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lachend sieht sie Amor eilen", "tokens": ["La\u00b7chend", "sieht", "sie", "A\u00b7mor", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "NE", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und sein Blick begleitet sie.", "tokens": ["Und", "sein", "Blick", "be\u00b7glei\u00b7tet", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Man entgeht des Blitzes Pfeilen,", "tokens": ["Man", "ent\u00b7geht", "des", "Blit\u00b7zes", "Pfei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber Amors Pfeilen nie.", "tokens": ["A\u00b7ber", "A\u00b7mors", "Pfei\u00b7len", "nie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Endlich bei des Mondes Scheine", "tokens": ["End\u00b7lich", "bei", "des", "Mon\u00b7des", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kehrte mit verst\u00f6rtem Blick,", "tokens": ["Kehr\u00b7te", "mit", "ver\u00b7st\u00f6r\u00b7tem", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Chloe langsam aus dem Haine", "tokens": ["Chloe", "lang\u00b7sam", "aus", "dem", "Hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An Alexis Arm zur\u00fcck.", "tokens": ["An", "A\u00b7le\u00b7xis", "Arm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nachtigallen sangen Lieder,", "tokens": ["Nach\u00b7ti\u00b7gal\u00b7len", "san\u00b7gen", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Duftend lag die Flur umher,", "tokens": ["Duf\u00b7tend", "lag", "die", "Flur", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ruhig war der Himmel wieder,", "tokens": ["Ru\u00b7hig", "war", "der", "Him\u00b7mel", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur ihr Herz war es nicht mehr.", "tokens": ["Nur", "ihr", "Herz", "war", "es", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-++--+", "measure": "iambic.tetra.chol"}}}}}