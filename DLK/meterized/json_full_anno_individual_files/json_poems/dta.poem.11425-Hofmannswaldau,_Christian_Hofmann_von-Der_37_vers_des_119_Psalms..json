{"dta.poem.11425": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der 37 vers des 119 Psalms.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unser' augen sind wie sterne, die best\u00e4ndig irre gehn;", "tokens": ["Un\u00b7ser'", "au\u00b7gen", "sind", "wie", "ster\u00b7ne", ",", "die", "be\u00b7st\u00e4n\u00b7dig", "ir\u00b7re", "gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "KOKOM", "ADV", "$,", "PRELS", "ADJD", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Doch HErr! der du ewig bleibest, mache, da\u00df sie", "tokens": ["Doch", "Herr", "!", "der", "du", "e\u00b7wig", "blei\u00b7best", ",", "ma\u00b7che", ",", "da\u00df", "sie"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["KON", "NN", "$.", "ART", "PPER", "ADJD", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "stille stehn!", "tokens": ["stil\u00b7le", "stehn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "La\u00df sie nicht nach lehren gaffen, die mehr b\u00f6s\u2019 als n\u00fctzlich sind:", "tokens": ["La\u00df", "sie", "nicht", "nach", "leh\u00b7ren", "gaf\u00b7fen", ",", "die", "mehr", "b\u00f6s'", "als", "n\u00fctz\u00b7lich", "sind", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "VVINF", "VVINF", "$,", "PRELS", "ADV", "ADJD", "KOKOM", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Deine liebe n\u00fctzt und bessert, und viel wissen macht nur blind.", "tokens": ["Dei\u00b7ne", "lie\u00b7be", "n\u00fctzt", "und", "bes\u00b7sert", ",", "und", "viel", "wis\u00b7sen", "macht", "nur", "blind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "KON", "VVPP", "$,", "KON", "ADV", "VVINF", "VVFIN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}