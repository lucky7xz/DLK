{"dta.poem.21598": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Wei\u00dfe Rose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Ach ich glaube,", "tokens": ["Ach", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Da\u00df voreinst mir,", "tokens": ["Da\u00df", "vor\u00b7e\u00b7inst", "mir", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da ich Kind war,", "tokens": ["Da", "ich", "Kind", "war", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Allererst sich", "tokens": ["Al\u00b7le\u00b7rerst", "sich"], "token_info": ["word", "word"], "pos": ["ADV", "PRF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Was da sch\u00f6n sei,", "tokens": ["Was", "da", "sch\u00f6n", "sei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Offenbart hat", "tokens": ["Of\u00b7fen\u00b7bart", "hat"], "token_info": ["word", "word"], "pos": ["NE", "VAFIN"], "meter": "+---", "measure": "dactylic.init"}, "line.7": {"text": "In dem Duftbild", "tokens": ["In", "dem", "Duft\u00b7bild"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wei\u00dfer Rose.", "tokens": ["Wei\u00b7\u00dfer", "Ro\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Ach ich glaube,", "tokens": ["Ach", "ich", "glau\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Da\u00df der J\u00fcngling", "tokens": ["Da\u00df", "der", "J\u00fcng\u00b7ling"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Noch den Zauber", "tokens": ["Noch", "den", "Zau\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Alles Sch\u00f6nen", "tokens": ["Al\u00b7les", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Nicht gelernt hat", "tokens": ["Nicht", "ge\u00b7lernt", "hat"], "token_info": ["word", "word", "word"], "pos": ["PTKNEG", "VVPP", "VAFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Unterscheiden", "tokens": ["Un\u00b7ter\u00b7schei\u00b7den"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Von dem Zauber", "tokens": ["Von", "dem", "Zau\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wei\u00dfer Rose.", "tokens": ["Wei\u00b7\u00dfer", "Ro\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Ach ich glaube \u2014", "tokens": ["Ach", "ich", "glau\u00b7be"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "PPER", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Zwar der Herbst kam,", "tokens": ["Zwar", "der", "Herbst", "kam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Und die Schwestern", "tokens": ["Und", "die", "Schwes\u00b7tern"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Auf den Fluren", "tokens": ["Auf", "den", "Flu\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Welkten lang schon \u2014", "tokens": ["Welk\u00b7ten", "lang", "schon"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Weiter bl\u00fchst du", "tokens": ["Wei\u00b7ter", "bl\u00fchst", "du"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mir im Herzen,", "tokens": ["Mir", "im", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wei\u00dfe Rose.", "tokens": ["Wei\u00b7\u00dfe", "Ro\u00b7se", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}