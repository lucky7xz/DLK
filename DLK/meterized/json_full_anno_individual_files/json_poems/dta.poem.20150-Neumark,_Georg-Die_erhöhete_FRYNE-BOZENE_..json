{"dta.poem.20150": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die erh\u00f6hete  \n  FRYNE-BOZENE .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "Ein rechter Tugendgeist und trefliches\nGem\u00fchte/", "tokens": ["Ein", "rech\u00b7ter", "Tu\u00b7gend\u00b7geist", "und", "tref\u00b7li\u00b7ches", "Ge\u00b7m\u00fch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sicht nicht nach grossem Stand\u2019 und", "tokens": ["Sicht", "nicht", "nach", "gros\u00b7sem", "Stand'", "und"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "APPR", "ADJA", "NN", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dieweil es in gemein auf Pracht und Hoch-", "tokens": ["Die\u00b7weil", "es", "in", "ge\u00b7mein", "auf", "Pracht", "und", "Hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "APPR", "NN", "KON", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und seinen blinden Sinn auf schn\u00f6de Wollust", "tokens": ["Und", "sei\u00b7nen", "blin\u00b7den", "Sinn", "auf", "schn\u00f6\u00b7de", "Wol\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wenn ungeschminkte Ziehr sich bey die Tugend", "tokens": ["Wenn", "un\u00b7ge\u00b7schmink\u00b7te", "Ziehr", "sich", "bey", "die", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So ist ein edler Sinn von Hertzen wol vergn\u00fcget/", "tokens": ["So", "ist", "ein", "ed\u00b7ler", "Sinn", "von", "Hert\u00b7zen", "wol", "ver\u00b7gn\u00fc\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er fragt nicht viel darnach woher ihr Ursprung", "tokens": ["Er", "fragt", "nicht", "viel", "dar\u00b7nach", "wo\u00b7her", "ihr", "Ur\u00b7sprung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "PAV", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Jm fall Sie nur mit Ehr und Redligkeit ge-", "tokens": ["Jm", "fall", "Sie", "nur", "mit", "Ehr", "und", "Red\u00b7lig\u00b7keit", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPER", "ADV", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Die Tugend nim\u0303et oft aus armen Sch\u00e4ferh\u00fcrden/", "tokens": ["Die", "Tu\u00b7gend", "nim\u0303et", "oft", "aus", "ar\u00b7men", "Sch\u00e4\u00b7fer\u00b7h\u00fcr\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Ein schlecht-geschetztes Mensch und bringt sie", "tokens": ["Ein", "schlecht\u00b7ge\u00b7schetz\u00b7tes", "Mensch", "und", "bringt", "sie"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nehmt hter ein Beyspiel ab: Ein\u2019 arme Sch\u00e4-", "tokens": ["Nehmt", "hter", "ein", "Bey\u00b7spiel", "ab", ":", "Ein'", "ar\u00b7me", "Sch\u00e4"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$.", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wird durch der Tugendkraft zu einer Hertzo-", "tokens": ["Wird", "durch", "der", "Tu\u00b7gend\u00b7kraft", "zu", "ei\u00b7ner", "Hert\u00b7zo"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Hertzog  Huldenreich/ der F\u00fcrste der\nBohemen/", "tokens": ["Der", "Hert\u00b7zog", "Hul\u00b7den\u00b7reich", "/", "der", "F\u00fcrs\u00b7te", "der", "Bo\u00b7he\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NE", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der Herr von jenem Land\u2019 in dem man andern", "tokens": ["Der", "Herr", "von", "je\u00b7nem", "Land'", "in", "dem", "man", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "APPR", "PRELS", "PIS", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Der Elbenflu\u00df entspringt/ war aller Hoffart", "tokens": ["Der", "El\u00b7ben\u00b7flu\u00df", "ent\u00b7springt", "/", "war", "al\u00b7ler", "Hof\u00b7fart"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$(", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und gleichsam von Natur ein \u00e4dler Tugend-", "tokens": ["Und", "gleich\u00b7sam", "von", "Na\u00b7tur", "ein", "\u00e4d\u00b7ler", "Tu\u00b7gen\u00b7d"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Er trug von jugend auf zur J\u00e4gerey Belieben/", "tokens": ["Er", "trug", "von", "ju\u00b7gend", "auf", "zur", "J\u00e4\u00b7ge\u00b7rey", "Be\u00b7lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "APPRART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Mit welcher er sehr oft die lange Zeit vertrieben/", "tokens": ["Mit", "wel\u00b7cher", "er", "sehr", "oft", "die", "lan\u00b7ge", "Zeit", "ver\u00b7trie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das st\u00e4dtische Gepr\u00e4ng war ihm ein lautrer", "tokens": ["Das", "st\u00e4d\u00b7ti\u00b7sche", "Ge\u00b7pr\u00e4ng", "war", "ihm", "ein", "laut\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Ein sch\u00f6nbewachsner Wald war seine h\u00f6chste", "tokens": ["Ein", "sch\u00f6n\u00b7be\u00b7wachs\u00b7ner", "Wald", "war", "sei\u00b7ne", "h\u00f6chs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Er ritt\u2019 auf eine Zeit/ als F\u0153bus freundlich straal-", "tokens": ["Er", "ritt'", "auf", "ei\u00b7ne", "Zeit", "/", "als", "F\u0153\u00b7bus", "freund\u00b7lich", "straa\u00b7l"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KOUS", "NN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.22": {"text": "Und das verj\u00fcngte Feld mit g\u00f6ldnen Straalen", "tokens": ["Und", "das", "ver\u00b7j\u00fcng\u00b7te", "Feld", "mit", "g\u00f6ld\u00b7nen", "Straa\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Mit Dienern auf die Jagt/ Er kam bey ein Ge-", "tokens": ["Mit", "Die\u00b7nern", "auf", "die", "Jagt", "/", "Er", "kam", "bey", "ein", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "An einen sch\u00f6nen Ohrt der fast dem ", "tokens": ["An", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Ohrt", "der", "fast", "dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Dort in ", "tokens": ["Dort", "in"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.26": {"text": "Ein abgehaltes Feld voll sch\u00f6ner Lieblichkeiten/", "tokens": ["Ein", "ab\u00b7ge\u00b7hal\u00b7tes", "Feld", "voll", "sch\u00f6\u00b7ner", "Lieb\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Die Wiese war mit Klee und Blumen unter-", "tokens": ["Die", "Wie\u00b7se", "war", "mit", "Klee", "und", "Blu\u00b7men", "un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Auf der das Wollenvieh in satter Wollust tischt\u2019.", "tokens": ["Auf", "der", "das", "Wol\u00b7len\u00b7vieh", "in", "sat\u00b7ter", "Wol\u00b7lust", "tischt'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Auf einer Seiten stund\u2019 ein felsenreicher H\u00fcgel/", "tokens": ["Auf", "ei\u00b7ner", "Sei\u00b7ten", "stund'", "ein", "fel\u00b7sen\u00b7rei\u00b7cher", "H\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der voll Gep\u00fcsche war; das singende Gefl\u00fcgel", "tokens": ["Der", "voll", "Ge\u00b7p\u00fcsc\u00b7he", "war", ";", "das", "sin\u00b7gen\u00b7de", "Ge\u00b7fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VAFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.31": {"text": "War da in grosser Meng; die schlaue Nach-", "tokens": ["War", "da", "in", "gros\u00b7ser", "Meng", ";", "die", "schlau\u00b7e", "Nach"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "$.", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Die schlug mal \u00fcbermal; ", "tokens": ["Die", "schlug", "mal", "\u00fc\u00b7ber\u00b7mal", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "War auch zu h\u00f6ren da; Der Fels war \u00fcberzogen", "tokens": ["War", "auch", "zu", "h\u00f6\u00b7ren", "da", ";", "Der", "Fels", "war", "\u00fc\u00b7berz\u00b7o\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF", "PTKVZ", "$.", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mit gr\u00fcnendem Gestr\u00e4uch\u2019/ und schien/ als wie", "tokens": ["Mit", "gr\u00fc\u00b7nen\u00b7dem", "Ge\u00b7str\u00e4uch'", "/", "und", "schien", "/", "als", "wie"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "VVFIN", "$(", "KOUS", "KOKOM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Von Meistern ausgewelbt/ darein man kunte", "tokens": ["Von", "Meis\u00b7tern", "aus\u00b7ge\u00b7welbt", "/", "da\u00b7rein", "man", "kun\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "$(", "PAV", "PIS", "VMFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Jmfall man einen Sturm und Wetter sah ent-", "tokens": ["Jm\u00b7fall", "man", "ei\u00b7nen", "Sturm", "und", "Wet\u00b7ter", "sah", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "ART", "NN", "KON", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Von diesem Klippen scho\u00df ein Wasser wiel Kri-", "tokens": ["Von", "die\u00b7sem", "Klip\u00b7pen", "scho\u00df", "ein", "Was\u00b7ser", "wiel", "Kri"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN", "VVFIN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.38": {"text": "Welchs ein Ger\u00e4usche gab in seinem steilen fallen/", "tokens": ["Welchs", "ein", "Ge\u00b7r\u00e4u\u00b7sche", "gab", "in", "sei\u00b7nem", "stei\u00b7len", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es war sehr klar und hell/ erquikkend/ s\u00fc\u00df und", "tokens": ["Es", "war", "sehr", "klar", "und", "hell", "/", "er\u00b7quik\u00b7kend", "/", "s\u00fc\u00df", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$(", "VVPP", "$(", "ADJD", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Und rieselte mit Lust durch seine Myrtenp\u00fcsch\u2019/", "tokens": ["Und", "rie\u00b7sel\u00b7te", "mit", "Lust", "durch", "sei\u00b7ne", "Myr\u00b7ten\u00b7p\u00fcsch'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Es samlete sich da bald in ein Thal zusammen/", "tokens": ["Es", "sam\u00b7le\u00b7te", "sich", "da", "bald", "in", "ein", "Thal", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.42": {"text": "In dem der matte Mensch/ wenn er durch heisse", "tokens": ["In", "dem", "der", "mat\u00b7te", "Mensch", "/", "wenn", "er", "durch", "heis\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "$(", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Der Sonnen abgeschw\u00e4cht/ sich wieder k\u00fchlen", "tokens": ["Der", "Son\u00b7nen", "ab\u00b7ge\u00b7schw\u00e4cht", "/", "sich", "wie\u00b7der", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$(", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "In dem manch Sch\u00e4ferknecht sich Sommers-", "tokens": ["In", "dem", "manch", "Sch\u00e4\u00b7fer\u00b7knecht", "sich", "Som\u00b7mer\u00b7s"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "PRF", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.45": {"text": "Hier eben dieses Ohrts sa\u00df eine Sch\u00e4ferinne", "tokens": ["Hier", "e\u00b7ben", "die\u00b7ses", "Ohrts", "sa\u00df", "ei\u00b7ne", "Sch\u00e4\u00b7fe\u00b7rin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Zwar armer Leute Kind/ doch gleichwol keuscher", "tokens": ["Zwar", "ar\u00b7mer", "Leu\u00b7te", "Kind", "/", "doch", "gleich\u00b7wol", "keu\u00b7scher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "NN", "$(", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.47": {"text": "Und tref