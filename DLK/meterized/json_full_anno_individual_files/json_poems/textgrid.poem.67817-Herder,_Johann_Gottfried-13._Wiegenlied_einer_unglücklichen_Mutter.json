{"textgrid.poem.67817": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "13. Wiegenlied einer ungl\u00fccklichen Mutter", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schlaf sanft, mein Kind, schlaf sanft und sch\u00f6n!", "tokens": ["Schlaf", "sanft", ",", "mein", "Kind", ",", "schlaf", "sanft", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mich dauerts sehr, dich weinen sehn,", "tokens": ["Mich", "dau\u00b7erts", "sehr", ",", "dich", "wei\u00b7nen", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PRF", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und schl\u00e4fst du sanft, bin ich so froh,", "tokens": ["Und", "schl\u00e4fst", "du", "sanft", ",", "bin", "ich", "so", "froh", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wimmerst du \u2013 das schmerzt mich so!", "tokens": ["Und", "wim\u00b7merst", "du", "\u2013", "das", "schmerzt", "mich", "so", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$(", "PDS", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schlaf sanft, du kleines Mutterherz,", "tokens": ["Schlaf", "sanft", ",", "du", "klei\u00b7nes", "Mut\u00b7ter\u00b7herz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Vater macht mir bittern Schmerz.", "tokens": ["Dein", "Va\u00b7ter", "macht", "mir", "bit\u00b7tern", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schlaf sanft, mein Kind, schlaf sanft und sch\u00f6n!", "tokens": ["Schlaf", "sanft", ",", "mein", "Kind", ",", "schlaf", "sanft", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich dauerts sehr, dich weinen sehn.", "tokens": ["Mich", "dau\u00b7erts", "sehr", ",", "dich", "wei\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Dein Vater, als er zu mir trat,", "tokens": ["Dein", "Va\u00b7ter", ",", "als", "er", "zu", "mir", "trat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und s\u00fc\u00df, so s\u00fc\u00df um Liebe bat,", "tokens": ["Und", "s\u00fc\u00df", ",", "so", "s\u00fc\u00df", "um", "Lie\u00b7be", "bat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADV", "ADJD", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da kannt ich noch sein Truggesicht", "tokens": ["Da", "kannt", "ich", "noch", "sein", "Trug\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch seine s\u00fcsse Falschheit nicht.", "tokens": ["Noch", "sei\u00b7ne", "s\u00fcs\u00b7se", "Falschheit", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nun, leider! seh ichs, seh ichs ein,", "tokens": ["Nun", ",", "lei\u00b7der", "!", "seh", "ichs", ",", "seh", "ichs", "ein", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "$.", "VVFIN", "PIS", "$,", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie nichts wir ihm nun beyde seyn.", "tokens": ["Wie", "nichts", "wir", "ihm", "nun", "bey\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "PPER", "ADV", "PIS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schlaf sanft, mein Kind, schlaf sanft und sch\u00f6n!", "tokens": ["Schlaf", "sanft", ",", "mein", "Kind", ",", "schlaf", "sanft", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich dauerts sehr, dich weinen sehn.", "tokens": ["Mich", "dau\u00b7erts", "sehr", ",", "dich", "wei\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ruh sanft, mein S\u00fcsser, schlafe noch!", "tokens": ["Ruh", "sanft", ",", "mein", "S\u00fcs\u00b7ser", ",", "schla\u00b7fe", "noch", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wenn du aufwachst, l\u00e4chle doch,", "tokens": ["Und", "wenn", "du", "auf\u00b7wachst", ",", "l\u00e4ch\u00b7le", "doch", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch nicht, wie einst dein Vater that,", "tokens": ["Doch", "nicht", ",", "wie", "einst", "dein", "Va\u00b7ter", "that", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der l\u00e4chelnd mich so trogen hat.", "tokens": ["Der", "l\u00e4\u00b7chelnd", "mich", "so", "tro\u00b7gen", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beh\u00fct dich Gott! \u2013 Doch machts mir Schmerz,", "tokens": ["Be\u00b7h\u00fct", "dich", "Gott", "!", "\u2013", "Doch", "machts", "mir", "Schmerz", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$.", "$(", "KON", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du auch tr\u00e4gst sein G'sicht und Herz.", "tokens": ["Da\u00df", "du", "auch", "tr\u00e4gst", "sein", "G'\u00b7sicht", "und", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Schlaf sanft, mein Kind, schlaf sanft und sch\u00f6n!", "tokens": ["Schlaf", "sanft", ",", "mein", "Kind", ",", "schlaf", "sanft", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich dauerts sehr, dich weinen sehn.", "tokens": ["Mich", "dau\u00b7erts", "sehr", ",", "dich", "wei\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was kann ich thun? Eins kann ich noch.", "tokens": ["Was", "kann", "ich", "thun", "?", "Eins", "kann", "ich", "noch", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVINF", "$.", "NN", "VMFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihn lieben will ich immer doch!", "tokens": ["Ihn", "lie\u00b7ben", "will", "ich", "im\u00b7mer", "doch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo er geh und steh nah und fern,", "tokens": ["Wo", "er", "geh", "und", "steh", "nah", "und", "fern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Mein Herz soll folgen ihm so gern.", "tokens": ["Mein", "Herz", "soll", "fol\u00b7gen", "ihm", "so", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In Wohl und Weh, wie's um ihn sey,", "tokens": ["In", "Wohl", "und", "Weh", ",", "wie's", "um", "ihn", "sey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VVFIN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herz noch imm'r ihm wohne bei.", "tokens": ["Mein", "Herz", "noch", "im\u00b7m'r", "ihm", "woh\u00b7ne", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "PPER", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Schlaf sanft, mein Kind, schlaf sanft und sch\u00f6n!", "tokens": ["Schlaf", "sanft", ",", "mein", "Kind", ",", "schlaf", "sanft", "und", "sch\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mich dauerts sehr, dich weinen sehn.", "tokens": ["Mich", "dau\u00b7erts", "sehr", ",", "dich", "wei\u00b7nen", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "$,", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein, sch\u00f6ner Kleiner, thu es nie;", "tokens": ["Nein", ",", "sch\u00f6\u00b7ner", "Klei\u00b7ner", ",", "thu", "es", "nie", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein Herz zur Falschheit neige nie;", "tokens": ["Dein", "Herz", "zur", "Falschheit", "nei\u00b7ge", "nie", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "ADV", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sey treuer Liebe immer treu,", "tokens": ["Sey", "treu\u00b7er", "Lie\u00b7be", "im\u00b7mer", "treu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verla\u00df sie nicht, zu w\u00e4hlen neu;", "tokens": ["Ver\u00b7la\u00df", "sie", "nicht", ",", "zu", "w\u00e4h\u00b7len", "neu", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PTKZU", "VVINF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dir gut und hold, verla\u00df sie nie \u2013", "tokens": ["Dir", "gut", "und", "hold", ",", "ver\u00b7la\u00df", "sie", "nie", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "$,", "VVIMP", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Angstseufzer, schrecklich dr\u00fccken sie!", "tokens": ["A\u00b7ngstseuf\u00b7zer", ",", "schreck\u00b7lich", "dr\u00fc\u00b7cken", "sie", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos":