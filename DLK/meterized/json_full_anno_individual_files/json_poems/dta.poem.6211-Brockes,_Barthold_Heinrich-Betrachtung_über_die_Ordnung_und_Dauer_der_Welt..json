{"dta.poem.6211": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber die Ordnung  \n und Dauer der Welt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Betrachten wir nun so, vom Grossen All, den Bau;", "tokens": ["Be\u00b7trach\u00b7ten", "wir", "nun", "so", ",", "vom", "Gros\u00b7sen", "All", ",", "den", "Bau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wird vom Lehr Geb\u00e4u, das einfach, gantz genau,", "tokens": ["So", "wird", "vom", "Lehr", "Ge\u00b7b\u00e4u", ",", "das", "ein\u00b7fach", ",", "gantz", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPRART", "NN", "NN", "$,", "PRELS", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein deutlicher Zusammenhang gefunden.", "tokens": ["Ein", "deut\u00b7li\u00b7cher", "Zu\u00b7sam\u00b7men\u00b7hang", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es weis\u2019t von seinem Band und Ordnungen die Spur,", "tokens": ["Es", "weis't", "von", "sei\u00b7nem", "Band", "und", "Ord\u00b7nun\u00b7gen", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wodurch der ", "tokens": ["Wo\u00b7durch", "der"], "token_info": ["word", "word"], "pos": ["PWAV", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.6": {"text": "Von dieser weiten Welt, die C\u00f6rper all\u2019verbunden.", "tokens": ["Von", "die\u00b7ser", "wei\u00b7ten", "Welt", ",", "die", "C\u00f6r\u00b7per", "all'\u00b7ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}