{"dta.poem.19866": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Winterlied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Der Winter hat mit kalter Hand               ", "tokens": ["Der", "Win\u00b7ter", "hat", "mit", "kal\u00b7ter", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Pappel abgelaubt;", "tokens": ["Die", "Pap\u00b7pel", "ab\u00b7ge\u00b7laubt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hat das gr\u00fcne Maigewand", "tokens": ["Und", "hat", "das", "gr\u00fc\u00b7ne", "Mai\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der armen Flur geraubt;", "tokens": ["Der", "ar\u00b7men", "Flur", "ge\u00b7raubt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hat Bl\u00fcmchen, blau und rot und weis,", "tokens": ["Hat", "Bl\u00fcm\u00b7chen", ",", "blau", "und", "rot", "und", "weis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$,", "ADJD", "KON", "ADJD", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Begraben unter Schnee und Eis.", "tokens": ["Be\u00b7gra\u00b7ben", "un\u00b7ter", "Schnee", "und", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch liebe Bl\u00fcmchen hoffet nicht", "tokens": ["Doch", "lie\u00b7be", "Bl\u00fcm\u00b7chen", "hof\u00b7fet", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von mir ein Sterbelied.", "tokens": ["Von", "mir", "ein", "Ster\u00b7be\u00b7lied", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich weis ein lieblich Angesicht,", "tokens": ["Ich", "weis", "ein", "lieb\u00b7lich", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Worauf ihr alle bl\u00fcht.", "tokens": ["Wo\u00b7rauf", "ihr", "al\u00b7le", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Blau ist des Augensternes Rund,", "tokens": ["Blau", "ist", "des", "Au\u00b7gens\u00b7ter\u00b7nes", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die Stirne weis, und rot der Mund.", "tokens": ["Die", "Stir\u00b7ne", "weis", ",", "und", "rot", "der", "Mund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "KON", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was k\u00fcmmert mich die Nachtigal,", "tokens": ["Was", "k\u00fcm\u00b7mert", "mich", "die", "Nach\u00b7ti\u00b7gal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im aufgebl\u00fchten Hain?", "tokens": ["Im", "auf\u00b7ge\u00b7bl\u00fch\u00b7ten", "Hain", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Liebchen trillert hundertmal", "tokens": ["Mein", "Lieb\u00b7chen", "tril\u00b7lert", "hun\u00b7dert\u00b7mal"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So f\u00fcs und silberrein.", "tokens": ["So", "f\u00fcs", "und", "sil\u00b7ber\u00b7rein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ihr Athem ist, wie Fr\u00fchlingsluft,", "tokens": ["Ihr", "A\u00b7them", "ist", ",", "wie", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erf\u00fclt mit Hyazinthenduft.", "tokens": ["Er\u00b7f\u00fclt", "mit", "Hy\u00b7a\u00b7zin\u00b7then\u00b7duft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Vol f\u00fcr den Mund, und w\u00fcrzereich,", "tokens": ["Vol", "f\u00fcr", "den", "Mund", ",", "und", "w\u00fcr\u00b7ze\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KON", "ADJD", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Und alerfrischend ist,", "tokens": ["Und", "a\u00b7ler\u00b7fri\u00b7schend", "ist", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der aufgeschwolnen Erdbeer gleich,", "tokens": ["Der", "auf\u00b7ge\u00b7schwol\u00b7nen", "Erd\u00b7beer", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Kus, den sie mir k\u00fcst. \u2014", "tokens": ["Der", "Kus", ",", "den", "sie", "mir", "k\u00fcst", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O Mai, was frag\u2019 ich viel nach dir?", "tokens": ["O", "Mai", ",", "was", "frag'", "ich", "viel", "nach", "dir", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Fr\u00fchling lebt und webt in ihr.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "lebt", "und", "webt", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}