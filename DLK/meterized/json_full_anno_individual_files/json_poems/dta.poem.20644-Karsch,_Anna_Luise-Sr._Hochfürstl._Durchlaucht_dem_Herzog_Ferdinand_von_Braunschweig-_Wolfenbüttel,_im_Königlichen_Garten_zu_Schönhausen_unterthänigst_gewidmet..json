{"dta.poem.20644": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Sr. Hochf\u00fcrstl. Durchlaucht  \n dem  \n  Herzog Ferdinand von Braunschweig-  \n Wolfenb\u00fcttel,  \n im K\u00f6niglichen Garten zu Sch\u00f6nhausen  \n unterth\u00e4nigst gewidmet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dugro\u00dfer Ferdinand, ich brannte Dich zu sehn                 ", "tokens": ["Du\u00b7gro\u00b7\u00dfer", "Fer\u00b7di\u00b7nand", ",", "ich", "brann\u00b7te", "Dich", "zu", "sehn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dort wo die K\u00f6niginn jezt wohnet", "tokens": ["Dort", "wo", "die", "K\u00f6\u00b7ni\u00b7ginn", "jezt", "woh\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PWAV", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mu\u00dfte mit Gesang erst zu dem Gotte flehn,", "tokens": ["Und", "mu\u00df\u00b7te", "mit", "Ge\u00b7sang", "erst", "zu", "dem", "Got\u00b7te", "flehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der durch Gesundheit Deine Tugenden belohnet.", "tokens": ["Der", "durch", "Ge\u00b7sund\u00b7heit", "Dei\u00b7ne", "Tu\u00b7gen\u00b7den", "be\u00b7loh\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "\u201esohn des Apollo, (rief mein \u00e4ngstlich bittend Lied)", "tokens": ["\u201e", "sohn", "des", "A\u00b7pol\u00b7lo", ",", "(", "rief", "mein", "\u00e4ngst\u00b7lich", "bit\u00b7tend", "Lied", ")"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "NE", "$,", "$(", "VVFIN", "PPOSAT", "ADJD", "VVPP", "NN", "$("], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "\u201eo mache mich gesund, verstatte", "tokens": ["\u201e", "o", "ma\u00b7che", "mich", "ge\u00b7sund", ",", "ver\u00b7stat\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word"], "pos": ["$(", "FM", "VVFIN", "PPER", "VVPP", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eda\u00df bald mein Auge den ber\u00fchmten Guelfen sieht,", "tokens": ["\u201e", "da\u00df", "bald", "mein", "Au\u00b7ge", "den", "be\u00b7r\u00fchm\u00b7ten", "Guel\u00b7fen", "sieht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ADV", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eder deines Vaters Schutz in Kriegsgefahren", "tokens": ["\u201e", "der", "dei\u00b7nes", "Va\u00b7ters", "Schutz", "in", "Kriegs\u00b7ge\u00b7fah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "PPOSAT", "NN", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "hatte.", "tokens": ["hat\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Da half mir Aesculap, des Fiebers T\u00fcck und Macht", "tokens": ["Da", "half", "mir", "A\u00b7e\u00b7scu\u00b7lap", ",", "des", "Fie\u00b7bers", "T\u00fcck", "und", "Macht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NE", "$,", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Entwich vor einem seiner Blicke;", "tokens": ["Ent\u00b7wich", "vor", "ei\u00b7nem", "sei\u00b7ner", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So wich vor Deinem Blick in mehr als einer Schlacht", "tokens": ["So", "wich", "vor", "Dei\u00b7nem", "Blick", "in", "mehr", "als", "ei\u00b7ner", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "APPR", "PIAT", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Aus Deinen Kriegern Furcht und Zagemuth", "tokens": ["Aus", "Dei\u00b7nen", "Krie\u00b7gern", "Furcht", "und", "Za\u00b7ge\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "zur\u00fccke.", "tokens": ["zu\u00b7r\u00fc\u00b7cke", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.4": {"line.1": {"text": "Ich fand mich stark genug, und flog dem Garten zu,", "tokens": ["Ich", "fand", "mich", "stark", "ge\u00b7nug", ",", "und", "flog", "dem", "Gar\u00b7ten", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "ADV", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo Deine Schwestern Dich genossen;", "tokens": ["Wo", "Dei\u00b7ne", "Schwes\u00b7tern", "Dich", "ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sahe Dich, und sprach: O Muse, sage du,", "tokens": ["Und", "sa\u00b7he", "Dich", ",", "und", "sprach", ":", "O", "Mu\u00b7se", ",", "sa\u00b7ge", "du", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KON", "VVFIN", "$.", "NE", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "War diese lichte Stirn mit Heldenschwei\u00df um-", "tokens": ["War", "die\u00b7se", "lich\u00b7te", "Stirn", "mit", "Hel\u00b7den\u00b7schwei\u00df", "um"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "flossen?", "tokens": ["flos\u00b7sen", "?"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "Hat dieses Augenpaar, worinnen Liebe sitzt,", "tokens": ["Hat", "die\u00b7ses", "Au\u00b7gen\u00b7paar", ",", "wo\u00b7rin\u00b7nen", "Lie\u00b7be", "sitzt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Freundlichkeit und holde G\u00fcte", "tokens": ["Und", "Freund\u00b7lich\u00b7keit", "und", "hol\u00b7de", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Feinde Schrecken in sein stolzes Herz geblitzt,", "tokens": ["Dem", "Fein\u00b7de", "Schre\u00b7cken", "in", "sein", "stol\u00b7zes", "Herz", "ge\u00b7blitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So, da\u00df er waffenlos vor seinem Sieger kniete?", "tokens": ["So", ",", "da\u00df", "er", "waf\u00b7fen\u00b7los", "vor", "sei\u00b7nem", "Sie\u00b7ger", "knie\u00b7te", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Muse l\u00e4chelte und sprach: \u201eIst nicht Apoll", "tokens": ["Die", "Mu\u00b7se", "l\u00e4\u00b7chel\u00b7te", "und", "sprach", ":", "\u201e", "Ist", "nicht", "A\u00b7poll"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$.", "$(", "VAFIN", "PTKNEG", "NN"], "meter": "-+-+-+-++-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "\u201eauch freundlich auf des Tages Wagen,", "tokens": ["\u201e", "auch", "freund\u00b7lich", "auf", "des", "Ta\u00b7ges", "Wa\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "\u201eund schrecklich wenn er z\u00fcrnt, und seinen K\u00f6cher voll", "tokens": ["\u201e", "und", "schreck\u00b7lich", "wenn", "er", "z\u00fcrnt", ",", "und", "sei\u00b7nen", "K\u00f6\u00b7cher", "voll"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "ADJD", "KOUS", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Todespfeilen auf der Schulter pflegt zu", "tokens": ["Mit", "To\u00b7des\u00b7pfei\u00b7len", "auf", "der", "Schul\u00b7ter", "pflegt", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVFIN", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "tragen?", "tokens": ["tra\u00b7gen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}