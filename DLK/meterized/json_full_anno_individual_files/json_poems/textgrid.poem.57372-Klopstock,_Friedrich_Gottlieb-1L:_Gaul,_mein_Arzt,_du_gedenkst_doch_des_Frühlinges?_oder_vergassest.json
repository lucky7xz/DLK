{"textgrid.poem.57372": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gaul, mein Arzt, du gedenkst doch des Fr\u00fchlinges? oder vergassest", "genre": "verse", "period": "N.A.", "pub_year": 1794, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gaul, mein Arzt, du gedenkst doch des Fr\u00fchlinges? oder vergassest", "tokens": ["Gaul", ",", "mein", "Arzt", ",", "du", "ge\u00b7denkst", "doch", "des", "Fr\u00fch\u00b7lin\u00b7ges", "?", "o\u00b7der", "ver\u00b7gas\u00b7sest"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$.", "KON", "VVFIN"], "meter": "+-+--+---+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du den lieblichen May,", "tokens": ["Du", "den", "lieb\u00b7li\u00b7chen", "May", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Unter den Mayen allen seit J\u00fcnglingsalter den sch\u00f6nsten?", "tokens": ["Un\u00b7ter", "den", "Ma\u00b7yen", "al\u00b7len", "seit", "J\u00fcng\u00b7lings\u00b7al\u00b7ter", "den", "sch\u00f6ns\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "APPR", "NN", "ART", "ADJA", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Nein, du vergassest ihn nicht;", "tokens": ["Nein", ",", "du", "ver\u00b7gas\u00b7sest", "ihn", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Denn du wiehertest mir. Der May ist wiedergekommen,", "tokens": ["Denn", "du", "wie\u00b7her\u00b7test", "mir", ".", "Der", "May", "ist", "wie\u00b7der\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ob er gleich September sich nent.", "tokens": ["Ob", "er", "gleich", "Sep\u00b7tem\u00b7ber", "sich", "nent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beyde gleichen sich, wie ein Haberkorn in der vollen,", "tokens": ["Bey\u00b7de", "glei\u00b7chen", "sich", ",", "wie", "ein", "Ha\u00b7ber\u00b7korn", "in", "der", "vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PWAV", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Reifen \u00c4hre dem anderen gleicht.", "tokens": ["Rei\u00b7fen", "\u00c4h\u00b7re", "dem", "an\u00b7de\u00b7ren", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Niese nur fort; ich versteh dich: du niesest mir fr\u00f6hlichen Beyfall,", "tokens": ["Nie\u00b7se", "nur", "fort", ";", "ich", "ver\u00b7steh", "dich", ":", "du", "nie\u00b7sest", "mir", "fr\u00f6h\u00b7li\u00b7chen", "Bey\u00b7fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Durch die \u00c4hre ger\u00fchrt.", "tokens": ["Durch", "die", "\u00c4h\u00b7re", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Also ist jetzo Septembermay, wie du meinst, und ich meine.", "tokens": ["Al\u00b7so", "ist", "jet\u00b7zo", "Sep\u00b7tem\u00b7ber\u00b7may", ",", "wie", "du", "meinst", ",", "und", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NE", "$,", "PWAV", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Angenehm ist es, wenn Zween", "tokens": ["An\u00b7ge\u00b7nehm", "ist", "es", ",", "wenn", "Zween"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Eben die Meinung vereint; da schallt der entheiternde Strauss nicht,", "tokens": ["E\u00b7ben", "die", "Mei\u00b7nung", "ver\u00b7eint", ";", "da", "schallt", "der", "en\u00b7thei\u00b7tern\u00b7de", "Strauss", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Da ist Lebensgenuss.", "tokens": ["Da", "ist", "Le\u00b7bens\u00b7ge\u00b7nuss", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Lass uns geniessen, du in dem Schatten, zu dem ich dich leuke,", "tokens": ["Lass", "uns", "ge\u00b7nies\u00b7sen", ",", "du", "in", "dem", "Schat\u00b7ten", ",", "zu", "dem", "ich", "dich", "leu\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Frisches, k\u00fchlendes Gras,", "tokens": ["Fri\u00b7sches", ",", "k\u00fch\u00b7len\u00b7des", "Gras", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Von der weisslichen Blume durchwebt, und der goldnen; auch hebt dort", "tokens": ["Von", "der", "weiss\u00b7li\u00b7chen", "Blu\u00b7me", "durch\u00b7webt", ",", "und", "der", "gold\u00b7nen", ";", "auch", "hebt", "dort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "$.", "ADV", "VVFIN", "ADV"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Dein erkohrnes Gew\u00fcrz,", "tokens": ["Dein", "er\u00b7kohr\u00b7nes", "Ge\u00b7w\u00fcrz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Heilende Wermuth ihr Haupt. Ich schau geniessend den hellern,", "tokens": ["Hei\u00b7len\u00b7de", "Wer\u00b7muth", "ihr", "Haupt", ".", "Ich", "schau", "ge\u00b7nies\u00b7send", "den", "hel\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$.", "PPER", "ADJD", "VVPP", "ART", "ADJA", "$,"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Blaueren Himmel, des Sees", "tokens": ["Blau\u00b7e\u00b7ren", "Him\u00b7mel", ",", "des", "Sees"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Ebnen Kristall, und umschwebt von ziehenden Metten, vergess' ich", "tokens": ["Eb\u00b7nen", "Kris\u00b7tall", ",", "und", "um\u00b7schwebt", "von", "zie\u00b7hen\u00b7den", "Met\u00b7ten", ",", "ver\u00b7ge\u00b7ss'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+---+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Fast der Bl\u00fcthe, die nun", "tokens": ["Fast", "der", "Bl\u00fc\u00b7the", ",", "die", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Fruchtet, und mit vielfarbiger Last, den biegsamen Zweig kr\u00fcmt.", "tokens": ["Fruch\u00b7tet", ",", "und", "mit", "viel\u00b7far\u00b7bi\u00b7ger", "Last", ",", "den", "bieg\u00b7sa\u00b7men", "Zweig", "kr\u00fcmt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+--+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Also trink' ich die reinere Lust,", "tokens": ["Al\u00b7so", "trink'", "ich", "die", "rei\u00b7ne\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Und ein sanftes frohes Gef\u00fchl des Lebens berauscht mich!", "tokens": ["Und", "ein", "sanf\u00b7tes", "fro\u00b7hes", "Ge\u00b7f\u00fchl", "des", "Le\u00b7bens", "be\u00b7rauscht", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Aber du st\u00f6rest mich ja!", "tokens": ["A\u00b7ber", "du", "st\u00f6\u00b7rest", "mich", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Schmause doch nicht so gierig; sie legten dir Xenophons Zaum an,", "tokens": ["Schmau\u00b7se", "doch", "nicht", "so", "gie\u00b7rig", ";", "sie", "leg\u00b7ten", "dir", "Xe\u00b7no\u00b7phons", "Zaum", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.28": {"text": "Dessen Gebiss durch Ringe dich z\u00e4hmt:", "tokens": ["Des\u00b7sen", "Ge\u00b7biss", "durch", "Rin\u00b7ge", "dich", "z\u00e4hmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "O du k\u00f6ntest durch ihn dir so leicht die Zunge verwunden,", "tokens": ["O", "du", "k\u00f6n\u00b7test", "durch", "ihn", "dir", "so", "leicht", "die", "Zun\u00b7ge", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "APPR", "PPER", "PPER", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "F\u00e4rben die Halme mit Blut.", "tokens": ["F\u00e4r\u00b7ben", "die", "Hal\u00b7me", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "Doch du gehorsamest nicht. So steh denn gehobenes Halses,", "tokens": ["Doch", "du", "ge\u00b7hor\u00b7sa\u00b7mest", "nicht", ".", "So", "steh", "denn", "ge\u00b7ho\u00b7be\u00b7nes", "Hal\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$.", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Athme die Weste, wie ich.", "tokens": ["Ath\u00b7me", "die", "Wes\u00b7te", ",", "wie", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "Spitze nicht horchend das Ohr. Die Nachtigall ist mit dem zweyten", "tokens": ["Spit\u00b7ze", "nicht", "hor\u00b7chend", "das", "Ohr", ".", "Die", "Nach\u00b7ti\u00b7gall", "ist", "mit", "dem", "zwey\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADJD", "ART", "NN", "$.", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.34": {"text": "Lenze nicht wiedergekehrt:", "tokens": ["Len\u00b7ze", "nicht", "wie\u00b7der\u00b7ge\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "Kehret auch mit dem k\u00fcnftigen May nicht wieder; ist Fabel", "tokens": ["Keh\u00b7ret", "auch", "mit", "dem", "k\u00fcnf\u00b7ti\u00b7gen", "May", "nicht", "wie\u00b7der", ";", "ist", "Fa\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$.", "VAFIN", "NN"], "meter": "+----+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Nicht, was man staunend erz\u00e4hlt.", "tokens": ["Nicht", ",", "was", "man", "stau\u00b7nend", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Gallische Wilde, sagen sie, sind gekommen, und haben", "tokens": ["Gal\u00b7li\u00b7sche", "Wil\u00b7de", ",", "sa\u00b7gen", "sie", ",", "sind", "ge\u00b7kom\u00b7men", ",", "und", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$,", "KON", "VAFIN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Ihre Nester entdeckt;", "tokens": ["Ih\u00b7re", "Nes\u00b7ter", "ent\u00b7deckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.39": {"text": "Haben die Kinderchen ihr mit Geyerklauen entrissen,", "tokens": ["Ha\u00b7ben", "die", "Kin\u00b7der\u00b7chen", "ihr", "mit", "Ge\u00b7yer\u00b7klau\u00b7en", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+--+---+-+-+-+-", "measure": "dactylic.di.plus"}, "line.40": {"text": "Und sie samt dem Gefieder verzehrt.", "tokens": ["Und", "sie", "samt", "dem", "Ge\u00b7fie\u00b7der", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.41": {"text": "Hast du sie nicht geh\u00f6ret der M\u00fctter Klage? Sie schwankten", "tokens": ["Hast", "du", "sie", "nicht", "ge\u00b7h\u00f6\u00b7ret", "der", "M\u00fct\u00b7ter", "Kla\u00b7ge", "?", "Sie", "schwank\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Au den Zweigen; ihr Lied", "tokens": ["Au", "den", "Zwei\u00b7gen", ";", "ihr", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.43": {"text": "Jammerte N\u00e4chte lang: bald sunk das Fl\u00f6ten der Wehmuth", "tokens": ["Jam\u00b7mer\u00b7te", "N\u00e4ch\u00b7te", "lang", ":", "bald", "sunk", "das", "Fl\u00f6\u00b7ten", "der", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+--++", "measure": "iambic.septa.invert"}, "line.44": {"text": "Immer tiefer, und bald", "tokens": ["Im\u00b7mer", "tie\u00b7fer", ",", "und", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.45": {"text": "Schien's zu verstummen, verstumte. So hab' ich nie sie vernommen;", "tokens": ["Schien's", "zu", "ver\u00b7stum\u00b7men", ",", "ver\u00b7stum\u00b7te", ".", "So", "hab'", "ich", "nie", "sie", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$,", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+--+--+-++--+-+-", "measure": "dactylic.di.plus"}, "line.46": {"text": "Aber es war auch Leichengesang!", "tokens": ["A\u00b7ber", "es", "war", "auch", "Lei\u00b7chen\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.47": {"text": "Stamst du vielleicht von den Rossen Achills? Denn du senkst ja die M\u00e4hne", "tokens": ["Stamst", "du", "viel\u00b7leicht", "von", "den", "Ros\u00b7sen", "A\u00b7chills", "?", "Denn", "du", "senkst", "ja", "die", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NE", "$.", "KON", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.48": {"text": "Erdw\u00e4rts, und in den Staub", "tokens": ["Erd\u00b7w\u00e4rts", ",", "und", "in", "den", "Staub"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "St\u00fcrzen dir heisse Thr\u00e4nen hinab; so hat dich der kleinen", "tokens": ["St\u00fcr\u00b7zen", "dir", "heis\u00b7se", "Thr\u00e4\u00b7nen", "hin\u00b7ab", ";", "so", "hat", "dich", "der", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJA", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.50": {"text": "S\u00e4nger kl\u00e4gliches Ende ger\u00fchrt.", "tokens": ["S\u00e4n\u00b7ger", "kl\u00e4g\u00b7li\u00b7ches", "En\u00b7de", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Gaul, mein Arzt, du gedenkst doch des Fr\u00fchlinges? oder vergassest", "tokens": ["Gaul", ",", "mein", "Arzt", ",", "du", "ge\u00b7denkst", "doch", "des", "Fr\u00fch\u00b7lin\u00b7ges", "?", "o\u00b7der", "ver\u00b7gas\u00b7sest"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ADV", "ART", "NN", "$.", "KON", "VVFIN"], "meter": "+-+--+---+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Du den lieblichen May,", "tokens": ["Du", "den", "lieb\u00b7li\u00b7chen", "May", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Unter den Mayen allen seit J\u00fcnglingsalter den sch\u00f6nsten?", "tokens": ["Un\u00b7ter", "den", "Ma\u00b7yen", "al\u00b7len", "seit", "J\u00fcng\u00b7lings\u00b7al\u00b7ter", "den", "sch\u00f6ns\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PIAT", "APPR", "NN", "ART", "ADJA", "$."], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Nein, du vergassest ihn nicht;", "tokens": ["Nein", ",", "du", "ver\u00b7gas\u00b7sest", "ihn", "nicht", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Denn du wiehertest mir. Der May ist wiedergekommen,", "tokens": ["Denn", "du", "wie\u00b7her\u00b7test", "mir", ".", "Der", "May", "ist", "wie\u00b7der\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$.", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ob er gleich September sich nent.", "tokens": ["Ob", "er", "gleich", "Sep\u00b7tem\u00b7ber", "sich", "nent", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Beyde gleichen sich, wie ein Haberkorn in der vollen,", "tokens": ["Bey\u00b7de", "glei\u00b7chen", "sich", ",", "wie", "ein", "Ha\u00b7ber\u00b7korn", "in", "der", "vol\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "$,", "PWAV", "ART", "NN", "APPR", "ART", "ADJA", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Reifen \u00c4hre dem anderen gleicht.", "tokens": ["Rei\u00b7fen", "\u00c4h\u00b7re", "dem", "an\u00b7de\u00b7ren", "gleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "ADJA", "VVFIN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Niese nur fort; ich versteh dich: du niesest mir fr\u00f6hlichen Beyfall,", "tokens": ["Nie\u00b7se", "nur", "fort", ";", "ich", "ver\u00b7steh", "dich", ":", "du", "nie\u00b7sest", "mir", "fr\u00f6h\u00b7li\u00b7chen", "Bey\u00b7fall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.10": {"text": "Durch die \u00c4hre ger\u00fchrt.", "tokens": ["Durch", "die", "\u00c4h\u00b7re", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Also ist jetzo Septembermay, wie du meinst, und ich meine.", "tokens": ["Al\u00b7so", "ist", "jet\u00b7zo", "Sep\u00b7tem\u00b7ber\u00b7may", ",", "wie", "du", "meinst", ",", "und", "ich", "mei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NE", "$,", "PWAV", "PPER", "ADV", "$,", "KON", "PPER", "VVFIN", "$."], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Angenehm ist es, wenn Zween", "tokens": ["An\u00b7ge\u00b7nehm", "ist", "es", ",", "wenn", "Zween"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Eben die Meinung vereint; da schallt der entheiternde Strauss nicht,", "tokens": ["E\u00b7ben", "die", "Mei\u00b7nung", "ver\u00b7eint", ";", "da", "schallt", "der", "en\u00b7thei\u00b7tern\u00b7de", "Strauss", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Da ist Lebensgenuss.", "tokens": ["Da", "ist", "Le\u00b7bens\u00b7ge\u00b7nuss", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Lass uns geniessen, du in dem Schatten, zu dem ich dich leuke,", "tokens": ["Lass", "uns", "ge\u00b7nies\u00b7sen", ",", "du", "in", "dem", "Schat\u00b7ten", ",", "zu", "dem", "ich", "dich", "leu\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "$,", "PPER", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Frisches, k\u00fchlendes Gras,", "tokens": ["Fri\u00b7sches", ",", "k\u00fch\u00b7len\u00b7des", "Gras", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.17": {"text": "Von der weisslichen Blume durchwebt, und der goldnen; auch hebt dort", "tokens": ["Von", "der", "weiss\u00b7li\u00b7chen", "Blu\u00b7me", "durch\u00b7webt", ",", "und", "der", "gold\u00b7nen", ";", "auch", "hebt", "dort"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,", "KON", "ART", "ADJA", "$.", "ADV", "VVFIN", "ADV"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Dein erkohrnes Gew\u00fcrz,", "tokens": ["Dein", "er\u00b7kohr\u00b7nes", "Ge\u00b7w\u00fcrz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Heilende Wermuth ihr Haupt. Ich schau geniessend den hellern,", "tokens": ["Hei\u00b7len\u00b7de", "Wer\u00b7muth", "ihr", "Haupt", ".", "Ich", "schau", "ge\u00b7nies\u00b7send", "den", "hel\u00b7lern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "$.", "PPER", "ADJD", "VVPP", "ART", "ADJA", "$,"], "meter": "+---+-+-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.20": {"text": "Blaueren Himmel, des Sees", "tokens": ["Blau\u00b7e\u00b7ren", "Him\u00b7mel", ",", "des", "Sees"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "ART", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.21": {"text": "Ebnen Kristall, und umschwebt von ziehenden Metten, vergess' ich", "tokens": ["Eb\u00b7nen", "Kris\u00b7tall", ",", "und", "um\u00b7schwebt", "von", "zie\u00b7hen\u00b7den", "Met\u00b7ten", ",", "ver\u00b7ge\u00b7ss'", "ich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER"], "meter": "+-+---+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Fast der Bl\u00fcthe, die nun", "tokens": ["Fast", "der", "Bl\u00fc\u00b7the", ",", "die", "nun"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.23": {"text": "Fruchtet, und mit vielfarbiger Last, den biegsamen Zweig kr\u00fcmt.", "tokens": ["Fruch\u00b7tet", ",", "und", "mit", "viel\u00b7far\u00b7bi\u00b7ger", "Last", ",", "den", "bieg\u00b7sa\u00b7men", "Zweig", "kr\u00fcmt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+--+-++-+-", "measure": "trochaic.septa.relaxed"}, "line.24": {"text": "Also trink' ich die reinere Lust,", "tokens": ["Al\u00b7so", "trink'", "ich", "die", "rei\u00b7ne\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.25": {"text": "Und ein sanftes frohes Gef\u00fchl des Lebens berauscht mich!", "tokens": ["Und", "ein", "sanf\u00b7tes", "fro\u00b7hes", "Ge\u00b7f\u00fchl", "des", "Le\u00b7bens", "be\u00b7rauscht", "mich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Aber du st\u00f6rest mich ja!", "tokens": ["A\u00b7ber", "du", "st\u00f6\u00b7rest", "mich", "ja", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.27": {"text": "Schmause doch nicht so gierig; sie legten dir Xenophons Zaum an,", "tokens": ["Schmau\u00b7se", "doch", "nicht", "so", "gie\u00b7rig", ";", "sie", "leg\u00b7ten", "dir", "Xe\u00b7no\u00b7phons", "Zaum", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKNEG", "ADV", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+--+-+-+", "measure": "iambic.septa.invert"}, "line.28": {"text": "Dessen Gebiss durch Ringe dich z\u00e4hmt:", "tokens": ["Des\u00b7sen", "Ge\u00b7biss", "durch", "Rin\u00b7ge", "dich", "z\u00e4hmt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.29": {"text": "O du k\u00f6ntest durch ihn dir so leicht die Zunge verwunden,", "tokens": ["O", "du", "k\u00f6n\u00b7test", "durch", "ihn", "dir", "so", "leicht", "die", "Zun\u00b7ge", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "VMFIN", "APPR", "PPER", "PPER", "ADV", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "F\u00e4rben die Halme mit Blut.", "tokens": ["F\u00e4r\u00b7ben", "die", "Hal\u00b7me", "mit", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.31": {"text": "Doch du gehorsamest nicht. So steh denn gehobenes Halses,", "tokens": ["Doch", "du", "ge\u00b7hor\u00b7sa\u00b7mest", "nicht", ".", "So", "steh", "denn", "ge\u00b7ho\u00b7be\u00b7nes", "Hal\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PTKNEG", "$.", "ADV", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "Athme die Weste, wie ich.", "tokens": ["Ath\u00b7me", "die", "Wes\u00b7te", ",", "wie", "ich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PWAV", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.33": {"text": "Spitze nicht horchend das Ohr. Die Nachtigall ist mit dem zweyten", "tokens": ["Spit\u00b7ze", "nicht", "hor\u00b7chend", "das", "Ohr", ".", "Die", "Nach\u00b7ti\u00b7gall", "ist", "mit", "dem", "zwey\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADJD", "ART", "NN", "$.", "ART", "NN", "VAFIN", "APPR", "ART", "ADJA"], "meter": "+--+--+-+-+-+-+-", "measure": "dactylic.di.plus"}, "line.34": {"text": "Lenze nicht wiedergekehrt:", "tokens": ["Len\u00b7ze", "nicht", "wie\u00b7der\u00b7ge\u00b7kehrt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "VVPP", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.35": {"text": "Kehret auch mit dem k\u00fcnftigen May nicht wieder; ist Fabel", "tokens": ["Keh\u00b7ret", "auch", "mit", "dem", "k\u00fcnf\u00b7ti\u00b7gen", "May", "nicht", "wie\u00b7der", ";", "ist", "Fa\u00b7bel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "PTKNEG", "ADV", "$.", "VAFIN", "NN"], "meter": "+----+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.36": {"text": "Nicht, was man staunend erz\u00e4hlt.", "tokens": ["Nicht", ",", "was", "man", "stau\u00b7nend", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.37": {"text": "Gallische Wilde, sagen sie, sind gekommen, und haben", "tokens": ["Gal\u00b7li\u00b7sche", "Wil\u00b7de", ",", "sa\u00b7gen", "sie", ",", "sind", "ge\u00b7kom\u00b7men", ",", "und", "ha\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "VVPP", "$,", "KON", "VAFIN"], "meter": "+--+-+-+--+--+-", "measure": "hexameter"}, "line.38": {"text": "Ihre Nester entdeckt;", "tokens": ["Ih\u00b7re", "Nes\u00b7ter", "ent\u00b7deckt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.39": {"text": "Haben die Kinderchen ihr mit Geyerklauen entrissen,", "tokens": ["Ha\u00b7ben", "die", "Kin\u00b7der\u00b7chen", "ihr", "mit", "Ge\u00b7yer\u00b7klau\u00b7en", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+--+---+-+-+-+-", "measure": "dactylic.di.plus"}, "line.40": {"text": "Und sie samt dem Gefieder verzehrt.", "tokens": ["Und", "sie", "samt", "dem", "Ge\u00b7fie\u00b7der", "ver\u00b7zehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.41": {"text": "Hast du sie nicht geh\u00f6ret der M\u00fctter Klage? Sie schwankten", "tokens": ["Hast", "du", "sie", "nicht", "ge\u00b7h\u00f6\u00b7ret", "der", "M\u00fct\u00b7ter", "Kla\u00b7ge", "?", "Sie", "schwank\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN"], "meter": "-+-+-+--+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.42": {"text": "Au den Zweigen; ihr Lied", "tokens": ["Au", "den", "Zwei\u00b7gen", ";", "ihr", "Lied"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.43": {"text": "Jammerte N\u00e4chte lang: bald sunk das Fl\u00f6ten der Wehmuth", "tokens": ["Jam\u00b7mer\u00b7te", "N\u00e4ch\u00b7te", "lang", ":", "bald", "sunk", "das", "Fl\u00f6\u00b7ten", "der", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+-+--++", "measure": "iambic.septa.invert"}, "line.44": {"text": "Immer tiefer, und bald", "tokens": ["Im\u00b7mer", "tie\u00b7fer", ",", "und", "bald"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "ADV"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.45": {"text": "Schien's zu verstummen, verstumte. So hab' ich nie sie vernommen;", "tokens": ["Schien's", "zu", "ver\u00b7stum\u00b7men", ",", "ver\u00b7stum\u00b7te", ".", "So", "hab'", "ich", "nie", "sie", "ver\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKZU", "VVINF", "$,", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "PPER", "VVPP", "$."], "meter": "+--+--+-++--+-+-", "measure": "dactylic.di.plus"}, "line.46": {"text": "Aber es war auch Leichengesang!", "tokens": ["A\u00b7ber", "es", "war", "auch", "Lei\u00b7chen\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.47": {"text": "Stamst du vielleicht von den Rossen Achills? Denn du senkst ja die M\u00e4hne", "tokens": ["Stamst", "du", "viel\u00b7leicht", "von", "den", "Ros\u00b7sen", "A\u00b7chills", "?", "Denn", "du", "senkst", "ja", "die", "M\u00e4h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "NE", "$.", "KON", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.48": {"text": "Erdw\u00e4rts, und in den Staub", "tokens": ["Erd\u00b7w\u00e4rts", ",", "und", "in", "den", "Staub"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.49": {"text": "St\u00fcrzen dir heisse Thr\u00e4nen hinab; so hat dich der kleinen", "tokens": ["St\u00fcr\u00b7zen", "dir", "heis\u00b7se", "Thr\u00e4\u00b7nen", "hin\u00b7ab", ";", "so", "hat", "dich", "der", "klei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJA", "NN", "PTKVZ", "$.", "ADV", "VAFIN", "PPER", "ART", "ADJA"], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.50": {"text": "S\u00e4nger kl\u00e4gliches Ende ger\u00fchrt.", "tokens": ["S\u00e4n\u00b7ger", "kl\u00e4g\u00b7li\u00b7ches", "En\u00b7de", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVPP", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}}}}