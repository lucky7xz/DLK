{"textgrid.poem.33614": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Abend auf den Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fern hinunter in die Flut", "tokens": ["Fern", "hin\u00b7un\u00b7ter", "in", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPO", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Taucht das Licht, sich nochmals wendend", "tokens": ["Taucht", "das", "Licht", ",", "sich", "noch\u00b7mals", "wen\u00b7dend"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "PRF", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu den Bergen, eine Glut", "tokens": ["Zu", "den", "Ber\u00b7gen", ",", "ei\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihren Alpenblumen sendend.", "tokens": ["Ih\u00b7ren", "Al\u00b7pen\u00b7blu\u00b7men", "sen\u00b7dend", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da schon Dunkel liegt im Tal,", "tokens": ["Da", "schon", "Dun\u00b7kel", "liegt", "im", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Flattern hier noch Schmetterlinge,", "tokens": ["Flat\u00b7tern", "hier", "noch", "Schmet\u00b7ter\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "NN", "