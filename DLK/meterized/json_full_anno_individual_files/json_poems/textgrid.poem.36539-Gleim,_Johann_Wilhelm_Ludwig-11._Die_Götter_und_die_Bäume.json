{"textgrid.poem.36539": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "11. Die G\u00f6tter und die B\u00e4ume", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Vater aller G\u00f6tter wollte,", "tokens": ["Der", "Va\u00b7ter", "al\u00b7ler", "G\u00f6t\u00b7ter", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df jeder Gott und jede G\u00f6ttin sich", "tokens": ["Da\u00df", "je\u00b7der", "Gott", "und", "je\u00b7de", "G\u00f6t\u00b7tin", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "KON", "PIAT", "NN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Von allen B\u00e4umen einen Baum", "tokens": ["Von", "al\u00b7len", "B\u00e4u\u00b7men", "ei\u00b7nen", "Baum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erw\u00e4hlen und besch\u00fctzen sollte.", "tokens": ["Er\u00b7w\u00e4h\u00b7len", "und", "be\u00b7sch\u00fct\u00b7zen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der Eichbaum, sprach er, ist f\u00fcr mich!", "tokens": ["Der", "Eich\u00b7baum", ",", "sprach", "er", ",", "ist", "f\u00fcr", "mich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "$,", "VAFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Apollo nahm den Lorbeerbaum;", "tokens": ["A\u00b7pol\u00b7lo", "nahm", "den", "Lor\u00b7beer\u00b7baum", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Musen tanzten einen Tanz,", "tokens": ["Die", "Mu\u00b7sen", "tanz\u00b7ten", "ei\u00b7nen", "Tanz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und warfen ihm den ersten Lorbeerkranz", "tokens": ["Und", "war\u00b7fen", "ihm", "den", "ers\u00b7ten", "Lor\u00b7beer\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Um sein gelehrtes Haupt.", "tokens": ["Um", "sein", "ge\u00b7lehr\u00b7tes", "Haupt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die hohe Pappel, sch\u00f6n belaubt,", "tokens": ["Die", "ho\u00b7he", "Pap\u00b7pel", ",", "sch\u00f6n", "be\u00b7laubt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erw\u00e4hlte Herkules; gelehnt auf seine Keule,", "tokens": ["Er\u00b7w\u00e4hl\u00b7te", "Her\u00b7ku\u00b7les", ";", "ge\u00b7lehnt", "auf", "sei\u00b7ne", "Keu\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$.", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Sprach er: Ich leide keine Beile!", "tokens": ["Sprach", "er", ":", "Ich", "lei\u00b7de", "kei\u00b7ne", "Bei\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Cybele tritt herein, die Mutter aller G\u00f6tter;", "tokens": ["Cy\u00b7be\u00b7le", "tritt", "her\u00b7ein", ",", "die", "Mut\u00b7ter", "al\u00b7ler", "G\u00f6t\u00b7ter", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "ART", "NN", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die G\u00f6tter neigten sich dem gr\u00fc\u00dfenden Gesichte;", "tokens": ["Die", "G\u00f6t\u00b7ter", "neig\u00b7ten", "sich", "dem", "gr\u00fc\u00b7\u00dfen\u00b7den", "Ge\u00b7sich\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie spricht: Gebt mir den Baum, der ohne Bl\u00e4tter", "tokens": ["Sie", "spricht", ":", "Gebt", "mir", "den", "Baum", ",", "der", "oh\u00b7ne", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dem alten Winter trotzt, die immer gr\u00fcne Fichte!", "tokens": ["Dem", "al\u00b7ten", "Win\u00b7ter", "trotzt", ",", "die", "im\u00b7mer", "gr\u00fc\u00b7ne", "Fich\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Komm her, du kleine Myrthe, komm her in meinen Schutz,", "tokens": ["Komm", "her", ",", "du", "klei\u00b7ne", "Myr\u00b7the", ",", "komm", "her", "in", "mei\u00b7nen", "Schutz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Sagt Venus, dich besinget Adonis, oder Uz!", "tokens": ["Sagt", "Ve\u00b7nus", ",", "dich", "be\u00b7sin\u00b7get", "A\u00b7do\u00b7nis", ",", "o\u00b7der", "Uz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "PPER", "VVFIN", "NE", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}}, "stanza.5": {"line.1": {"text": "Was aber sagt Minerva? Sie l\u00e4chelt kleinen Spott,", "tokens": ["Was", "a\u00b7ber", "sagt", "Mi\u00b7ner\u00b7va", "?", "Sie", "l\u00e4\u00b7chelt", "klei\u00b7nen", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "NE", "$.", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sagt zum Zeus: Ich w\u00e4hl' den Oelbaum, den kein Gott", "tokens": ["Und", "sagt", "zum", "Zeus", ":", "Ich", "w\u00e4hl'", "den", "O\u00b7el\u00b7baum", ",", "den", "kein", "Gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "NE", "$.", "PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "Und keine G\u00f6ttin w\u00e4hlte, der ist an Fr\u00fcchten reich.", "tokens": ["Und", "kei\u00b7ne", "G\u00f6t\u00b7tin", "w\u00e4hl\u00b7te", ",", "der", "ist", "an", "Fr\u00fcch\u00b7ten", "reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,", "PRELS", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die unfruchtbaren B\u00e4ume, die, G\u00f6tter, la\u00df ich euch!", "tokens": ["Die", "un\u00b7frucht\u00b7ba\u00b7ren", "B\u00e4u\u00b7me", ",", "die", ",", "G\u00f6t\u00b7ter", ",", "la\u00df", "ich", "euch", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "$,", "NN", "$,", "VVIMP", "PPER", "PPER", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Da zankten sich die G\u00f6tter, und Zeus entschied den Zank,", "tokens": ["Da", "zank\u00b7ten", "sich", "die", "G\u00f6t\u00b7ter", ",", "und", "Zeus", "ent\u00b7schied", "den", "Zank", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "$,", "KON", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Umarmte seine Tochter, sang ihren Lobgesang.", "tokens": ["Um\u00b7arm\u00b7te", "sei\u00b7ne", "Toch\u00b7ter", ",", "sang", "ih\u00b7ren", "Lob\u00b7ge\u00b7sang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Er sang, Apollo horchte, Minerva hat gewonnen,", "tokens": ["Er", "sang", ",", "A\u00b7pol\u00b7lo", "horch\u00b7te", ",", "Mi\u00b7ner\u00b7va", "hat", "ge\u00b7won\u00b7nen", ","], "token