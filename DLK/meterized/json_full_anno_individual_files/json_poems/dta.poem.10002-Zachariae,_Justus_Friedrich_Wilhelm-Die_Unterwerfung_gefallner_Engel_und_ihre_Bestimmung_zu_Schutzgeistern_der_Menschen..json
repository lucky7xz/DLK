{"dta.poem.10002": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n Unterwerfung gefallner Engel  \n und ihre Bestimmung  \n zu  \n Schutzgeistern der Menschen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Also sprach er, und eilte sogleich zur Erde Be-", "tokens": ["Al\u00b7so", "sprach", "er", ",", "und", "eil\u00b7te", "sog\u00b7leich", "zur", "Er\u00b7de", "Be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VVFIN", "ADV", "APPRART", "NN", "TRUNC"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Aber Orion, und seine Gef\u00e4hrten, voll tiefen Gehorsams,", "tokens": ["A\u00b7ber", "O\u00b7rion", ",", "und", "sei\u00b7ne", "Ge\u00b7f\u00e4hr\u00b7ten", ",", "voll", "tie\u00b7fen", "Ge\u00b7hor\u00b7sams", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Liessen sich auf die hohen Geb\u00fcrge des Mondes hernieder.", "tokens": ["Lies\u00b7sen", "sich", "auf", "die", "ho\u00b7hen", "Ge\u00b7b\u00fcr\u00b7ge", "des", "Mon\u00b7des", "her\u00b7nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+--+--+--+-", "measure": "hexameter"}}}}}