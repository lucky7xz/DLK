{"dta.poem.13212": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Thron nim\u0303t selbst in Lust den Trieb der Tugend-Schaar", "tokens": ["Der", "Thron", "nim\u0303t", "selbst", "in", "Lust", "den", "Trieb", "der", "Tu\u00b7gen\u00b7dSchaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "NN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und den bey dem Geschenck entstandnen Eifer wahr.", "tokens": ["Und", "den", "bey", "dem", "Ge\u00b7schenck", "ent\u00b7stand\u00b7nen", "Ei\u00b7fer", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ART", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Fast niemand bleibt zur\u00fcck; man dr\u00e4ngt sich hin und eilet", "tokens": ["Fast", "nie\u00b7mand", "bleibt", "zu\u00b7r\u00fcck", ";", "man", "dr\u00e4ngt", "sich", "hin", "und", "ei\u00b7let"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "PTKVZ", "$.", "PIS", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "360So nah man kann, zum Thron, wo man das Pfand ertheilet.", "tokens": ["nah", "man", "kann", ",", "zum", "Thron", ",", "wo", "man", "das", "Pfand", "er\u00b7thei\u00b7let", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIS", "VMFIN", "$,