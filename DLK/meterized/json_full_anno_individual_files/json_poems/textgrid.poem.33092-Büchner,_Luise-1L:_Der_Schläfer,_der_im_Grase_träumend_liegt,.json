{"textgrid.poem.33092": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Schl\u00e4fer, der im Grase tr\u00e4umend liegt,", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Schl\u00e4fer, der im Grase tr\u00e4umend liegt,", "tokens": ["Der", "Schl\u00e4\u00b7fer", ",", "der", "im", "Gra\u00b7se", "tr\u00e4u\u00b7mend", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bis hoch am Himmel schon der Sonnenwagen,", "tokens": ["Bis", "hoch", "am", "Him\u00b7mel", "schon", "der", "Son\u00b7nen\u00b7wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "APPRART", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er kann, wenn dann empor sein Auge fliegt,", "tokens": ["Er", "kann", ",", "wenn", "dann", "em\u00b7por", "sein", "Au\u00b7ge", "fliegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "ADV", "PTKVZ", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Lichtes helle Klarheit nicht ertragen,", "tokens": ["Des", "Lich\u00b7tes", "hel\u00b7le", "Klar\u00b7heit", "nicht", "er\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Es blendet ihm wie Fackellicht die Augen,", "tokens": ["Es", "blen\u00b7det", "ihm", "wie", "Fa\u00b7ckel\u00b7licht", "die", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "KOKOM", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die, weggewandt, nicht mehr zum Sehen taugen.", "tokens": ["Die", ",", "weg\u00b7ge\u00b7wandt", ",", "nicht", "mehr", "zum", "Se\u00b7hen", "tau\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "VVPP", "$,", "PTKNEG", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht so der Andre, der vom Morgengrau'n", "tokens": ["Nicht", "so", "der", "And\u00b7re", ",", "der", "vom", "Mor\u00b7gen\u00b7grau'n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit wachem Auge folgt des Lichtes Spuren,", "tokens": ["Mit", "wa\u00b7chem", "Au\u00b7ge", "folgt", "des", "Lich\u00b7tes", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihn blendet's nicht, er kann es offen schau'n,", "tokens": ["Ihn", "blen\u00b7det's", "nicht", ",", "er", "kann", "es", "of\u00b7fen", "schau'n", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PPER", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wenn rings sein Schimmer strahlt auf allen Fluren;", "tokens": ["Wenn", "rings", "sein", "Schim\u00b7mer", "strahlt", "auf", "al\u00b7len", "Flu\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Am Quell des Lichtes darf er furchtlos hangen,", "tokens": ["Am", "Quell", "des", "Lich\u00b7tes", "darf", "er", "furcht\u00b7los", "han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der nie verwirrt, wer stets ihm nachgegangen.", "tokens": ["Der", "nie", "ver\u00b7wirrt", ",", "wer", "stets", "ihm", "nach\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "$,", "PWS", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O du, der Wahrheit und Erkenntni\u00df sucht,", "tokens": ["O", "du", ",", "der", "Wahr\u00b7heit", "und", "Er\u00b7kennt\u00b7ni\u00df", "sucht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So streb' ihr nach vom ersten Tagesgrauen,", "tokens": ["So", "streb'", "ihr", "nach", "vom", "ers\u00b7ten", "Ta\u00b7ges\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nach und nach dir reift der Klarheit Frucht,", "tokens": ["Da\u00df", "nach", "und", "nach", "dir", "reift", "der", "Klar\u00b7heit", "Frucht", ","], "token_info": ["word", "word", "word", "word", "wor