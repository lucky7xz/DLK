{"dta.poem.10267": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "St\u00fcck in Esther.  \n Kapitel 4. Vers 3\u201414.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Einer wundersch\u00f6nen J\u00fcdin", "tokens": ["Ei\u00b7ner", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "J\u00fc\u00b7din"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sa\u00df ich heute gegen\u00fcber,", "tokens": ["Sa\u00df", "ich", "heu\u00b7te", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus den gro\u00dfen braunen Augen", "tokens": ["Aus", "den", "gro\u00b7\u00dfen", "brau\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Klagte scheu Jerusalem.", "tokens": ["Klag\u00b7te", "scheu", "Je\u00b7ru\u00b7sa\u00b7lem", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NE", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Eingeschlagen wie zwei N\u00e4gel,", "tokens": ["Ein\u00b7ge\u00b7schla\u00b7gen", "wie", "zwei", "N\u00e4\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "CARD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Blitzten in den kleinen Ohren", "tokens": ["Blitz\u00b7ten", "in", "den", "klei\u00b7nen", "Oh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Diamanten reinsten Wassers,", "tokens": ["Di\u00b7a\u00b7man\u00b7ten", "reins\u00b7ten", "Was\u00b7sers", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blitzten l\u00fcstern mir ins Herz.", "tokens": ["Blitz\u00b7ten", "l\u00fcs\u00b7tern", "mir", "ins", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Unausstehlich war ihr Gatte,", "tokens": ["Un\u00b7aus\u00b7steh\u00b7lich", "war", "ihr", "Gat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Grau, gelangweilt, Z\u00e4hne stochernd,", "tokens": ["Grau", ",", "ge\u00b7lang\u00b7weilt", ",", "Z\u00e4h\u00b7ne", "sto\u00b7chernd", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "NN", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Dachte an Priorit\u00e4ten,", "tokens": ["Dach\u00b7te", "an", "Pri\u00b7o\u00b7ri\u00b7t\u00e4\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eifers\u00fcchtig schien er auch.", "tokens": ["Ei\u00b7fer\u00b7s\u00fcch\u00b7tig", "schien", "er", "auch", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und ich hob den vollen R\u00f6mer,", "tokens": ["Und", "ich", "hob", "den", "vol\u00b7len", "R\u00f6\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ihn an die Lippen setzend,", "tokens": ["Und", "ihn", "an", "die", "Lip\u00b7pen", "set\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Fand ich ihre braunen Augen,", "tokens": ["Fand", "ich", "ih\u00b7re", "brau\u00b7nen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Trank ihn aus auf einen Zug:", "tokens": ["Trank", "ihn", "aus", "auf", "ei\u00b7nen", "Zug", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Haman, Esther, hie\u00df der Unhold,", "tokens": ["Ha\u00b7man", ",", "E\u00b7sther", ",", "hie\u00df", "der", "Un\u00b7hold", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Der dich einst vernichten wollte,", "tokens": ["Der", "dich", "einst", "ver\u00b7nich\u00b7ten", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Haman nenn\u2019 ich deinen Gatten,", "tokens": ["Ha\u00b7man", "nenn'", "ich", "dei\u00b7nen", "Gat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ich will Artaxerxes sein.", "tokens": ["Ich", "will", "Ar\u00b7ta\u00b7xe\u00b7rxes", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Haman schaukelte bei Susan", "tokens": ["Ha\u00b7man", "schau\u00b7kel\u00b7te", "bei", "Su\u00b7san"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Bald an einem hohen Galgen,", "tokens": ["Bald", "an", "ei\u00b7nem", "ho\u00b7hen", "Gal\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Als den K\u00f6nig Artaxerxes", "tokens": ["Als", "den", "K\u00f6\u00b7nig", "Ar\u00b7ta\u00b7xe\u00b7rxes"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Du verliebt in dich gemacht.", "tokens": ["Du", "ver\u00b7liebt", "in", "dich", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Dein Genosse Haman aber", "tokens": ["Dein", "Ge\u00b7nos\u00b7se", "Ha\u00b7man", "a\u00b7ber"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "NE", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Soll im Saal am Leuchter h\u00e4ngen,", "tokens": ["Soll", "im", "Saal", "am", "Leuch\u00b7ter", "h\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in Artaxerxes Armen", "tokens": ["Und", "in", "Ar\u00b7ta\u00b7xe\u00b7rxes", "Ar\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ruht die sch\u00f6ne K\u00f6nigin.", "tokens": ["Ruht", "die", "sch\u00f6\u00b7ne", "K\u00f6\u00b7ni\u00b7gin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}