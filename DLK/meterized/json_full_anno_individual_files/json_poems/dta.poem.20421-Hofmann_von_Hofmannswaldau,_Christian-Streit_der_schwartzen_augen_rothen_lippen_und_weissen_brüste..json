{"dta.poem.20421": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Streit der schwartzen augen/ rothen  \n lippen/ und weissen br\u00fcste.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn unsre kugeln nicht mit s\u00fcssem triebe schertzen/", "tokens": ["Wenn", "uns\u00b7re", "ku\u00b7geln", "nicht", "mit", "s\u00fcs\u00b7sem", "trie\u00b7be", "schert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "ADJA", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dieser weisse schild der m\u00e4nner freyheit legt/", "tokens": ["Und", "die\u00b7ser", "weis\u00b7se", "schild", "der", "m\u00e4n\u00b7ner", "frey\u00b7heit", "legt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So stellt die Venus ja vergebens auff die hertzen/", "tokens": ["So", "stellt", "die", "Ve\u00b7nus", "ja", "ver\u00b7ge\u00b7bens", "auff", "die", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und selten wird ein brand durch unsre krafft erregt.", "tokens": ["Und", "sel\u00b7ten", "wird", "ein", "brand", "durch", "uns\u00b7re", "krafft", "er\u00b7regt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Das beben/ so man stets um unsre grentzen sp\u00fcret/", "tokens": ["Das", "be\u00b7ben", "/", "so", "man", "stets", "um", "uns\u00b7re", "grent\u00b7zen", "sp\u00fc\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "$(", "ADV", "PIS", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bl\u00e4st tausend flammen auff/ und leget feuer an/", "tokens": ["Bl\u00e4st", "tau\u00b7send", "flam\u00b7men", "auff", "/", "und", "le\u00b7get", "feu\u00b7er", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "CARD", "VVFIN", "APPR", "$(", "KON", "VVFIN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ja dieses/ was bey uns verborgen wird gef\u00e4hret/", "tokens": ["Ja", "die\u00b7ses", "/", "was", "bey", "uns", "ver\u00b7bor\u00b7gen", "wird", "ge\u00b7f\u00e4h\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PDAT", "$(", "PWS", "APPR", "PPER", "VVPP", "VAFIN", "VVPP", "$("], "meter": "-+-++--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Hat offtmahls mehr/ als das/ was sich gezeigt/ gethan.", "tokens": ["Hat", "offt\u00b7mahls", "mehr", "/", "als", "das", "/", "was", "sich", "ge\u00b7zeigt", "/", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "$(", "KOUS", "ART", "$(", "PWS", "PRF", "VVPP", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}