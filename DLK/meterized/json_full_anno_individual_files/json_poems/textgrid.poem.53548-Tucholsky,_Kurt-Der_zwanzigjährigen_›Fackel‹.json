{"textgrid.poem.53548": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Der zwanzigj\u00e4hrigen \u203aFackel\u2039", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast zwanzig Jahr ins Land gestrahlt.", "tokens": ["Du", "hast", "zwan\u00b7zig", "Jahr", "ins", "Land", "ge\u00b7strahlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "CARD", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Du hast manchen Schatten an die Wand gemalt \u2013", "tokens": ["Du", "hast", "man\u00b7chen", "Schat\u00b7ten", "an", "die", "Wand", "ge\u00b7malt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Rauchlos helle Flamme!", "tokens": ["Rauch\u00b7los", "hel\u00b7le", "Flam\u00b7me", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Und wir sprachen zu den feinen R\u00f6cken,", "tokens": ["Und", "wir", "spra\u00b7chen", "zu", "den", "fei\u00b7nen", "R\u00f6\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "und wir sprachen zu den kleinen Schm\u00f6cken:", "tokens": ["und", "wir", "spra\u00b7chen", "zu", "den", "klei\u00b7nen", "Schm\u00f6\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "ADJA",