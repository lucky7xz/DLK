{"dta.poem.11808": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem st\u00e4rkern Feind entgeht der schwache mit der Hilfe", "tokens": ["Dem", "st\u00e4r\u00b7kern", "Feind", "ent\u00b7geht", "der", "schwa\u00b7che", "mit", "der", "Hil\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des schwachen, wie der Frosch dem Krokodill im Schilfe.", "tokens": ["Des", "schwa\u00b7chen", ",", "wie", "der", "Frosch", "dem", "Kro\u00b7ko\u00b7dill", "im", "Schil\u00b7fe", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PWAV", "ART", "NN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn der Verschlinger droht im Strom dem armen Frosche,", "tokens": ["Wenn", "der", "Ver\u00b7schlin\u00b7ger", "droht", "im", "Strom", "dem", "ar\u00b7men", "Fro\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nimmt der ein breites Schilf geschwind in seine Gosche.", "tokens": ["Nimmt", "der", "ein", "brei\u00b7tes", "Schilf", "ge\u00b7schwind", "in", "sei\u00b7ne", "Go\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ART", "ADJA", "NN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das quere Schilfrohr geht nicht in den weiten Rachen,", "tokens": ["Das", "que\u00b7re", "Schilf\u00b7rohr", "geht", "nicht", "in", "den", "wei\u00b7ten", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ungef\u00e4rdet l\u00e4\u00dft das Ungeth\u00fcm den schwachen.", "tokens": ["Und", "un\u00b7ge\u00b7f\u00e4r\u00b7det", "l\u00e4\u00dft", "das", "Un\u00b7ge\u00b7th\u00fcm", "den", "schwa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Nun sitzt der Quaker dort und klagt sein Leid im Schilfe,", "tokens": ["Nun", "sitzt", "der", "Qua\u00b7ker", "dort", "und", "klagt", "sein", "Leid", "im", "Schil\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "KON", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df man in solchem Strom hat n\u00f6thig solche Hilfe.", "tokens": ["Da\u00df", "man", "in", "sol\u00b7chem", "Strom", "hat", "n\u00f6\u00b7thig", "sol\u00b7che", "Hil\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PIAT", "NN", "VAFIN", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Gelungen ists, ich bin dem Schlinger nun entsprungen,", "tokens": ["Ge\u00b7lun\u00b7gen", "ists", ",", "ich", "bin", "dem", "Schlin\u00b7ger", "nun", "ent\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch so dem Schlingen nah ist schlimmer als verschlungen.", "tokens": ["Doch", "so", "dem", "Schlin\u00b7gen", "nah", "ist", "schlim\u00b7mer", "als", "ver\u00b7schlun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ADJD", "VAFIN", "ADJD", "KOKOM", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}