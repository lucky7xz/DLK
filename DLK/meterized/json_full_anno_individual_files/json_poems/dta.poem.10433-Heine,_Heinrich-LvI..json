{"dta.poem.10433": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LvI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' im Traum' geweinet,", "tokens": ["Ich", "hab'", "im", "Traum'", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir tr\u00e4umte du l\u00e4gest im Grab'.", "tokens": ["Mir", "tr\u00e4um\u00b7te", "du", "l\u00e4\u00b7gest", "im", "Grab'", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich wachte auf und die Thr\u00e4ne", "tokens": ["Ich", "wach\u00b7te", "auf", "und", "die", "Thr\u00e4\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Flo\u00df noch von der Wange herab.", "tokens": ["Flo\u00df", "noch", "von", "der", "Wan\u00b7ge", "her\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "+---+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Ich hab' im Traum' geweinet,", "tokens": ["Ich", "hab'", "im", "Traum'", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir tr\u00e4umt' du verlie\u00dfest mich.", "tokens": ["Mir", "tr\u00e4umt'", "du", "ver\u00b7lie\u00b7\u00dfest", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich wachte auf, und ich weinte", "tokens": ["Ich", "wach\u00b7te", "auf", ",", "und", "ich", "wein\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch lange bitterlich.", "tokens": ["Noch", "lan\u00b7ge", "bit\u00b7ter\u00b7lich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich hab' im Traum' geweinet,", "tokens": ["Ich", "hab'", "im", "Traum'", "ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir tr\u00e4umte du w\u00e4rst mir noch gut.", "tokens": ["Mir", "tr\u00e4um\u00b7te", "du", "w\u00e4rst", "mir", "noch", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Ich wachte auf, und noch immer", "tokens": ["Ich", "wach\u00b7te", "auf", ",", "und", "noch", "im\u00b7mer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Str\u00f6mt meine Thr\u00e4nenfluth.", "tokens": ["Str\u00f6mt", "mei\u00b7ne", "Thr\u00e4\u00b7nen\u00b7fluth", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}