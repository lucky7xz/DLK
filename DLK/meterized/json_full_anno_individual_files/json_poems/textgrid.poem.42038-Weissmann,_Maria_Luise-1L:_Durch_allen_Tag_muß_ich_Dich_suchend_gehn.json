{"textgrid.poem.42038": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch allen Tag mu\u00df ich Dich suchend gehn", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch allen Tag mu\u00df ich Dich suchend gehn", "tokens": ["Durch", "al\u00b7len", "Tag", "mu\u00df", "ich", "Dich", "su\u00b7chend", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PRF", "ADJD", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und ist so viel, was rings Dich mir verhei\u00dft,", "tokens": ["Und", "ist", "so", "viel", ",", "was", "rings", "Dich", "mir", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "$,", "PRELS", "ADV", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mich mit Gewi\u00dfheit Deiner schimmernd speist:", "tokens": ["Mich", "mit", "Ge\u00b7wi\u00df\u00b7heit", "Dei\u00b7ner", "schim\u00b7mernd", "speist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PPOSAT", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Vogelrufen, Glanz des Golds, Kakteen,", "tokens": ["Ein", "Vo\u00b7gel\u00b7ru\u00b7fen", ",", "Glanz", "des", "Golds", ",", "Kak\u00b7te\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ART", "NN", "$,", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Schnee, ach, und Geige, die gesehn Dich haben,", "tokens": ["Schnee", ",", "ach", ",", "und", "Gei\u00b7ge", ",", "die", "ge\u00b7sehn", "Dich", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "KON", "NN", "$,", "PRELS", "VVPP", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Fahnen der blanken St\u00e4dte, Windeswehn \u2013", "tokens": ["Fah\u00b7nen", "der", "blan\u00b7ken", "St\u00e4d\u00b7te", ",", "Win\u00b7des\u00b7wehn", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "NN", "$("], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Starbst Du in einer Sonne Untergehn?", "tokens": ["Starbst", "Du", "in", "ei\u00b7ner", "Son\u00b7ne", "Un\u00b7ter\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "War dies Dein Schrei in wehem Spiel der Knaben?", "tokens": ["War", "dies", "Dein", "Schrei", "in", "we\u00b7hem", "Spiel", "der", "Kna\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PPOSAT", "NN", "APPR", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich wandre durch Taifun, kristallnen Strahl der Seen,", "tokens": ["Ich", "wand\u00b7re", "durch", "Tai\u00b7fun", ",", "kris\u00b7tall\u00b7nen", "Strahl", "der", "Seen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u2013 Vielleicht, da\u00df Dich ein Duft gefunden mach