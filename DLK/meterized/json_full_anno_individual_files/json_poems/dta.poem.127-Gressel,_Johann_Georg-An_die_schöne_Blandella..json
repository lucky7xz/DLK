{"dta.poem.127": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An die sch\u00f6ne  Blandella.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Die kalten Sinnen sind von heisser Lust entbrannt/", "tokens": ["Die", "kal\u00b7ten", "Sin\u00b7nen", "sind", "von", "heis\u00b7ser", "Lust", "ent\u00b7brannt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der netten Augen-Pracht hat solche angez\u00fcndet/", "tokens": ["Der", "net\u00b7ten", "Au\u00b7gen\u00b7Pracht", "hat", "sol\u00b7che", "an\u00b7ge\u00b7z\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Wirckung dieses Strahls die Seele mit empfindet/", "tokens": ["Die", "Wir\u00b7ckung", "die\u00b7ses", "Strahls", "die", "See\u00b7le", "mit", "emp\u00b7fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "ART", "NN", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein gold-gemengtes Haar/ der Stirnen Perlen-Band", "tokens": ["Dein", "gold\u00b7ge\u00b7meng\u00b7tes", "Haar", "/", "der", "Stir\u00b7nen", "Per\u00b7len\u00b7Band"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und die Corallen-See der Lippen sind ", "tokens": ["Und", "die", "Coral\u00b7len\u00b7See", "der", "Lip\u00b7pen", "sind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "VAFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Des Busens heisser Schnee sich mit der Gluht verbindet/", "tokens": ["Des", "Bu\u00b7sens", "heis\u00b7ser", "Schnee", "sich", "mit", "der", "Gluht", "ver\u00b7bin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die aus den Augen bricht: dem/ wer sich unterwindet", "tokens": ["Die", "aus", "den", "Au\u00b7gen", "bricht", ":", "dem", "/", "wer", "sich", "un\u00b7ter\u00b7win\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "$(", "PWS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dich dreiste anzusehn/ ists Feur schon zuerkant.", "tokens": ["Dich", "dreis\u00b7te", "an\u00b7zu\u00b7sehn", "/", "ists", "Feur", "schon", "zu\u00b7er\u00b7kant", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVIZU", "$(", "VAFIN", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Glieder lassen sich wie klahre Perlen sehen;", "tokens": ["Die", "Glie\u00b7der", "las\u00b7sen", "sich", "wie", "klah\u00b7re", "Per\u00b7len", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "KOKOM", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Um den Albaster Leib die Anmuths-Liljen siehen/", "tokens": ["Um", "den", "Al\u00b7bas\u00b7ter", "Leib", "die", "An\u00b7muths\u00b7Lil\u00b7jen", "sie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und die ", "tokens": ["Und", "die"], "token_info": ["word", "word"], "pos": ["KON", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.12": {"text": "Des Busens-Paradie\u00df l\u00e4st mich auf Schwanen gehen/", "tokens": ["Des", "Bu\u00b7sens\u00b7Pa\u00b7ra\u00b7die\u00df", "l\u00e4st", "mich", "auf", "Schwa\u00b7nen", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich schau ein Canaan von den Albaster H\u00f6hen/", "tokens": ["Ich", "schau", "ein", "Ca\u00b7naan", "von", "den", "Al\u00b7bas\u00b7ter", "H\u00f6\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Blandella f\u00fchr mich doch in dis Gelobte-Land.", "tokens": ["Blan\u00b7del\u00b7la", "f\u00fchr", "mich", "doch", "in", "dis", "Ge\u00b7lob\u00b7te\u00b7Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPR", "PDS", "NN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}}}}