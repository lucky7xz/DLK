{"textgrid.poem.39215": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kennt Ihr mein Lieb, sein Aug' ist gro\u00df,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kennt Ihr mein Lieb, sein Aug' ist gro\u00df,", "tokens": ["Kennt", "Ihr", "mein", "Lieb", ",", "sein", "Aug'", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kennt Ihr das Aug' und wie es trifft,", "tokens": ["Kennt", "Ihr", "das", "Aug'", "und", "wie", "es", "trifft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwarzdunkel wie der Wolke Scho\u00df", "tokens": ["Schwarz\u00b7dun\u00b7kel", "wie", "der", "Wol\u00b7ke", "Scho\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leuchtend wie des Blitzes Schrift?", "tokens": ["Und", "leuch\u00b7tend", "wie", "des", "Blit\u00b7zes", "Schrift", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sch\u00f6n ist es, wenn es l\u00e4chelnd tagt,", "tokens": ["Sch\u00f6n", "ist", "es", ",", "wenn", "es", "l\u00e4\u00b7chelnd", "tagt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sch\u00f6n, wenn's im Kreis zorndunkel f\u00e4hrt,", "tokens": ["Sch\u00f6n", ",", "wenn's", "im", "Kreis", "zorn\u00b7dun\u00b7kel", "f\u00e4hrt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wie ist es blitzend, wenn's versagt,", "tokens": ["Wie", "ist", "es", "blit\u00b7zend", ",", "wenn's", "ver\u00b7sagt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie ist es tauig, wenn's gew\u00e4hrt!", "tokens": ["Wie", "ist", "es", "tau\u00b7ig", ",", "wenn's", "ge\u00b7w\u00e4hrt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "KOUS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr kennt das Aug' und wie es spr\u00fcht,", "tokens": ["Ihr", "kennt", "das", "Aug'", "und", "wie", "es", "spr\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es hat Euch oft das Hirn versengt,", "tokens": ["Es", "hat", "Euch", "oft", "das", "Hirn", "ver\u00b7sengt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ihr aber seid's, die's \u00fcbersieht,", "tokens": ["Ihr", "a\u00b7ber", "sei\u00b7d's", ",", "die's", "\u00fc\u00b7ber\u00b7sieht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "PDS", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und ich, ich bin's, an dem es h\u00e4ngt.", "tokens": ["Und", "ich", ",", "ich", "bin's", ",", "an", "dem", "es", "h\u00e4ngt", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VAFIN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Kennt Ihr mein Lieb?", "tokens": ["Kennt", "Ihr", "mein", "Lieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Kennt Ihr mein Lieb, sein Mund ist rot,", "tokens": ["Kennt", "Ihr", "mein", "Lieb", ",", "sein", "Mund", "ist", "rot", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kennt Ihr den Mund und wie er spricht?", "tokens": ["Kennt", "Ihr", "den", "Mund", "und", "wie", "er", "spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "KON", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie zuckt er trotzig, wenn er droht,", "tokens": ["Wie", "zuckt", "er", "trot\u00b7zig", ",", "wenn", "er", "droht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch mir alleine droht er nicht.", "tokens": ["Doch", "mir", "al\u00b7lei\u00b7ne", "droht", "er", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vor aller Welt, wie spricht er klug,", "tokens": ["Vor", "al\u00b7ler", "Welt", ",", "wie", "spricht", "er", "klug", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PWAV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie kindisch in verschwieg'ner Stund';", "tokens": ["Wie", "kin\u00b7disch", "in", "ver\u00b7schwieg'\u00b7ner", "Stund'", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gesegnet sei dein Atemzug,", "tokens": ["Ge\u00b7seg\u00b7net", "sei", "dein", "A\u00b7tem\u00b7zug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Granatenbl\u00fcte, s\u00fc\u00dfer Mund!", "tokens": ["Gra\u00b7na\u00b7ten\u00b7bl\u00fc\u00b7te", ",", "s\u00fc\u00b7\u00dfer", "Mund", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr kennt den Mund, wie hold er t\u00f6nt,", "tokens": ["Ihr", "kennt", "den", "Mund", ",", "wie", "hold", "er", "t\u00f6nt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und sterbt vor schmachtendem Gel\u00fcst,", "tokens": ["Und", "sterbt", "vor", "schmach\u00b7ten\u00b7dem", "Ge\u00b7l\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Denn Ihr, Ihr seid es, die er h\u00f6hnt,", "tokens": ["Denn", "Ihr", ",", "Ihr", "seid", "es", ",", "die", "er", "h\u00f6hnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und ich, ich bin es, den er k\u00fc\u00dft!", "tokens": ["Und", "ich", ",", "ich", "bin", "es", ",", "den", "er", "k\u00fc\u00dft", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Kennt Ihr mein Lieb?", "tokens": ["Kennt", "Ihr", "mein", "Lieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Ihr kennt mein wundervolles Lieb,", "tokens": ["Ihr", "kennt", "mein", "wun\u00b7der\u00b7vol\u00b7les", "Lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Quelle meiner Phantasie'n,", "tokens": ["Die", "Quel\u00b7le", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Tropf, wer bei Verstande blieb',", "tokens": ["Ein", "Tropf", ",", "wer", "bei", "Ver\u00b7stan\u00b7de", "blieb'", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihn ein solcher Strahl beschien'!", "tokens": ["Wenn", "ihn", "ein", "sol\u00b7cher", "Strahl", "be\u00b7schien'", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein Lieb ist sch\u00f6n, wie keine mehr", "tokens": ["Mein", "Lieb", "ist", "sch\u00f6n", ",", "wie", "kei\u00b7ne", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PWAV", "PIAT", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hier unterm Pfad des Sonnenballs;", "tokens": ["Hier", "un\u00b7term", "Pfad", "des", "Son\u00b7nen\u00b7balls", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Lieb ist sch\u00f6n, zu seiner Ehr'", "tokens": ["Mein", "Lieb", "ist", "sch\u00f6n", ",", "zu", "sei\u00b7ner", "Ehr'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Br\u00e4ch' ich Euch allen gern den Hals!", "tokens": ["Br\u00e4ch'", "ich", "Euch", "al\u00b7len", "gern", "den", "Hals", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPER", "PIS", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ihr kennt mein Lieb, mein Lieb ist hold;", "tokens": ["Ihr", "kennt", "mein", "Lieb", ",", "mein", "Lieb", "ist", "hold", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nun neidet mir's und seufzt Euch satt,", "tokens": ["Nun", "nei\u00b7det", "mir's", "und", "seufzt", "Euch", "satt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "KON", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Ihr seid es, die Ihr's haben wollt,", "tokens": ["Ihr", "seid", "es", ",", "die", "Ih\u00b7r's", "ha\u00b7ben", "wollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "$,", "PRELS", "PIS", "VAINF", "VMFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Und ich, ich bin es, der es hat.", "tokens": ["Und", "ich", ",", "ich", "bin", "es", ",", "der", "es", "hat", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPER", "VAFIN", "PPER", "$,", "PRELS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Kennt Ihr mein Lieb?", "tokens": ["Kennt", "Ihr", "mein", "Lieb", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Kennt Ihr mein Lieb, sein Aug' ist gro\u00df,", "tokens": ["Kennt", "Ihr", "mein", "Lieb", ",", "sein", "Aug'", "ist", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFI