{"textgrid.poem.65663": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Minister", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So wollen wir einmal beschaun doch sein Gesch\u00e4fts-Register.", "tokens": ["So", "wol\u00b7len", "wir", "ein\u00b7mal", "be\u00b7schaun", "doch", "sein", "Ge\u00b7sch\u00e4fts\u00b7Re\u00b7gis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "KON", "PPOSAT", "NN", "$."], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Ei, gab es denn in diesem Jahr so schrecklich viel zu schaffen?", "tokens": ["Ei", ",", "gab", "es", "denn", "in", "die\u00b7sem", "Jahr", "so", "schreck\u00b7lich", "viel", "zu", "schaf\u00b7fen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "ADV", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Was ist geschaffen und geschafft? \u2013 Wir d\u00fcrfen's auch begaffen.", "tokens": ["Was", "ist", "ge\u00b7schaf\u00b7fen", "und", "ge\u00b7schafft", "?", "\u2013", "Wir", "d\u00fcr\u00b7fen's", "auch", "be\u00b7ga\u00b7ffen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "VVPP", "KON", "VVPP", "$.", "$(", "PPER", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Die Segel auf! Gen Osten hin! Da giebt es was zu sehen.", "tokens": ["Die", "Se\u00b7gel", "auf", "!", "Gen", "Os\u00b7ten", "hin", "!", "Da", "giebt", "es", "was", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "NN", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "Schon leuchten uns von Chios Strand entgegen die Troph\u00e4en,", "tokens": ["Schon", "leuch\u00b7ten", "uns", "von", "Chios", "Strand", "ent\u00b7ge\u00b7gen", "die", "Trop\u00b7h\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+--", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Troph\u00e4en, pr\u00e4chtig aufgeth\u00fcrmt, Troph\u00e4en ohne Gleichen,", "tokens": ["Trop\u00b7h\u00e4\u00b7en", ",", "pr\u00e4ch\u00b7tig", "auf\u00b7ge\u00b7th\u00fcrmt", ",", "Trop\u00b7h\u00e4\u00b7en", "oh\u00b7ne", "Glei\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVPP", "$,", "NN", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.7": {"text": "Troph\u00e4en, wei\u00df und schwarz und roth, von Sch\u00e4deln, Blut und Leichen.", "tokens": ["Trop\u00b7h\u00e4\u00b7en", ",", "wei\u00df", "und", "schwarz", "und", "roth", ",", "von", "Sch\u00e4\u00b7deln", ",", "Blut", "und", "Lei\u00b7chen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "KON", "ADJD", "KON", "ADJD", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.8": {"text": "Und Kreuze liegen oben auf, bespieen und zerschlagen \u2013", "tokens": ["Und", "Kreu\u00b7ze", "li