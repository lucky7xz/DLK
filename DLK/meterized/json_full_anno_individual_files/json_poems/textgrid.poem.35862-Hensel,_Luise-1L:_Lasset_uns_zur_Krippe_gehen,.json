": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Nein, o nein! mein Erdenleben", "tokens": ["Nein", ",", "o", "nein", "!", "mein", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "FM", "PTKANT", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich Dir zum Opfer weihn.", "tokens": ["Will", "ich", "Dir", "zum", "Op\u00b7fer", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Denken, all mein Streben", "tokens": ["All", "mein", "Den\u00b7ken", ",", "all", "mein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll zu Deiner Ehre sein.", "tokens": ["Soll", "zu", "Dei\u00b7ner", "Eh\u00b7re", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Immer will ich mit Dir ziehen,", "tokens": ["Im\u00b7mer", "will", "ich", "mit", "Dir", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will allein auf Dich nur sehn,", "tokens": ["Will", "al\u00b7lein", "auf", "Dich", "nur", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Dir nach Aegypten fliehen,", "tokens": ["Mit", "Dir", "nach", "A\u00b7e\u00b7gyp\u00b7ten", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Dir in den Tempel gehn.", "tokens": ["Mit", "Dir", "in", "den", "Tem\u00b7pel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.17": {"line.1": {"text": "Mit Dir in der Wildni\u00df wandeln,", "tokens": ["Mit", "Dir", "in", "der", "Wild\u00b7ni\u00df", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Willig gehn durch Kies und Sand,", "tokens": ["Wil\u00b7lig", "gehn", "durch", "Kies", "und", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Dir k\u00e4mpfen, leiden, handeln,", "tokens": ["Mit", "Dir", "k\u00e4mp\u00b7fen", ",", "lei\u00b7den", ",", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "$,", "VVINF", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit Dir ziehn durch Stadt und Land.", "tokens": ["Mit", "Dir", "ziehn", "durch", "Stadt", "und", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.18": {"line.1": {"text": "Mit Dir weinen, mit Dir flehen,", "tokens": ["Mit", "Dir",