{"textgrid.poem.35862": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lasset uns zur Krippe gehen,", "genre": "verse", "period": "N.A.", "pub_year": 1818, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lasset uns zur Krippe gehen,", "tokens": ["Las\u00b7set", "uns", "zur", "Krip\u00b7pe", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Drin das Jesuskindlein ruht.", "tokens": ["Drin", "das", "Je\u00b7sus\u00b7kin\u00b7dlein", "ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00dft uns froh das Wunder sehen,", "tokens": ["La\u00dft", "uns", "froh", "das", "Wun\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADJD", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das der Herr uns Menschen thut.", "tokens": ["Das", "der", "Herr", "uns", "Men\u00b7schen", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Lasset uns das Kindlein gr\u00fc\u00dfen:", "tokens": ["Las\u00b7set", "uns", "das", "Kin\u00b7dlein", "gr\u00fc\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zartes Kind! Gewalt'ger Held!", "tokens": ["Zar\u00b7tes", "Kind", "!", "Ge\u00b7walt'\u00b7ger", "Held", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, wie unsre Thr\u00e4nen flie\u00dfen \u2013", "tokens": ["Sieh", ",", "wie", "uns\u00b7re", "Thr\u00e4\u00b7nen", "flie\u00b7\u00dfen", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei willkommen auf der Welt!", "tokens": ["Sei", "will\u00b7kom\u00b7men", "auf", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "O, Du tr\u00e4gst in Deinen H\u00e4nden", "tokens": ["O", ",", "Du", "tr\u00e4gst", "in", "Dei\u00b7nen", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alles, was uns Armen fehlt,", "tokens": ["Al\u00b7les", ",", "was", "uns", "Ar\u00b7men", "fehlt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und Du wirst die Knechtschaft enden,", "tokens": ["Und", "Du", "wirst", "die", "Knecht\u00b7schaft", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die seit Adam uns gequ\u00e4lt.", "tokens": ["Die", "seit", "A\u00b7dam", "uns", "ge\u00b7qu\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ja, Du willst uns ew'ges Leben,", "tokens": ["Ja", ",", "Du", "willst", "uns", "ew'\u00b7ges", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sel'ge F\u00fcll' und Herrlichkeit,", "tokens": ["Sel'\u00b7ge", "F\u00fcll'", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ew'gen Frieden willst Du geben,", "tokens": ["Ew'\u00b7gen", "Frie\u00b7den", "willst", "Du", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Rufst uns All' zur Seligkeit.", "tokens": ["Rufst", "uns", "All'", "zur", "Se\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wir sollten Dich nicht h\u00f6ren,", "tokens": ["Und", "wir", "soll\u00b7ten", "Dich", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich, Der uns so innig liebt?", "tokens": ["Dich", ",", "Der", "uns", "so", "in\u00b7nig", "liebt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollten selbst der Gnade wehren,", "tokens": ["Soll\u00b7ten", "selbst", "der", "Gna\u00b7de", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die uns Dein Erbarmen giebt?", "tokens": ["Die", "uns", "Dein", "Er\u00b7bar\u00b7men", "giebt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nein, o nein! mein Erdenleben", "tokens": ["Nein", ",", "o", "nein", "!", "mein", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$,", "FM", "PTKANT", "$.", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will ich Dir zum Opfer weihn.", "tokens": ["Will", "ich", "Dir", "zum", "Op\u00b7fer", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "All mein Denken, all mein Streben", "tokens": ["All", "mein", "Den\u00b7ken", ",", "all", "mein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Soll zu Deiner Ehre sein.", "tokens": ["Soll", "zu", "Dei\u00b7ner", "Eh\u00b7re", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Immer will ich mit Dir ziehen,", "tokens": ["Im\u00b7mer", "will", "ich", "mit", "Dir", "zie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Will allein auf Dich nur sehn,", "tokens": ["Will", "al\u00b7lein", "auf", "Dich", "nur", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Dir nach Aegypten fliehen,", "tokens": ["Mit", "Dir", "nach", "A\u00b7e\u00b7gyp\u00b7ten", "flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NE", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Dir in den Tempel gehn.", "tokens": ["Mit", "Dir", "in", "den", "Tem\u00b7pel", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Mit Dir in der Wildni\u00df wandeln,", "tokens": ["Mit", "Dir", "in", "der", "Wild\u00b7ni\u00df", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Willig gehn durch Kies und Sand,", "tokens": ["Wil\u00b7lig", "gehn", "durch", "Kies", "und", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Dir k\u00e4mpfen, leiden, handeln,", "tokens": ["Mit", "Dir", "k\u00e4mp\u00b7fen", ",", "lei\u00b7den", ",", "han\u00b7deln", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "wo