unct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' da\u00df man ihn aufgehangen", "tokens": ["Eh'", "da\u00df", "man", "ihn", "auf\u00b7ge\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stach man ihn durch Zung' und Herz,", "tokens": ["Stach", "man", "ihn", "durch", "Zung'", "und", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nagelte ihn hinterw\u00e4rts", "tokens": ["Na\u00b7gel\u00b7te", "ihn", "hin\u00b7ter\u00b7w\u00e4rts"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf sein Corpus Juris.", "tokens": ["Auf", "sein", "Cor\u00b7pus", "Ju\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.22": {"line.1": {"text": "Als die Waldschlacht war zu Ende,", "tokens": ["Als", "die", "Wald\u00b7schlacht", "war", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rieb F\u00fcrst Hermann sich die H\u00e4nde,", "tokens": ["Rieb", "F\u00fcrst", "Her\u00b7mann", "sich", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und um seinen Sieg zu weihn,", "tokens": ["Und", "um", "sei\u00b7nen", "Sieg", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lud er die Cherusker ein", "tokens": ["Lud", "er", "die", "Che\u00b7rus\u00b7ker", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu 'nem gro\u00dfen Fr\u00fchst\u00fcck.", "tokens": ["Zu", "'nem", "gro\u00b7\u00dfen", "Fr\u00fch\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.23": {"line.1": {"text": "Nur in Rom war man nicht heiter,", "tokens": ["Nur", "in", "Rom", "war", "man", "nicht", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VAFIN", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sondern kaufte Trauerkleider.", "tokens": ["Son\u00b7dern", "kauf\u00b7te", "Trau\u00b7er\u00b7klei\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G'rade als beim Mittagmahl", "tokens": ["G'\u00b7ra\u00b7de", "als", "beim", "Mit\u00b7tag\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Augustus sa\u00df im Kaisersaal,", "tokens": ["Au\u00b7gus\u00b7tus", "sa\u00df", "im", "Kai\u00b7ser\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Kam die Trauerbotscha