{"textgrid.poem.40415": {"metadata": {"author": {"name": "Scheffel, Joseph Viktor von", "birth": "N.A.", "death": "N.A."}, "title": "Die Teutoburger Schlacht", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als die R\u00f6mer frech geworden,", "tokens": ["Als", "die", "R\u00f6\u00b7mer", "frech", "ge\u00b7wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zogen sie nach Deutschlands Norden,", "tokens": ["Zo\u00b7gen", "sie", "nach", "Deutschlands", "Nor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NE", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vorne beim Trompetenschall", "tokens": ["Vor\u00b7ne", "beim", "Trom\u00b7pe\u00b7ten\u00b7schall"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ritt der Generalfeldmarschall,", "tokens": ["Ritt", "der", "Ge\u00b7ne\u00b7ral\u00b7feld\u00b7mar\u00b7schall", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Herr Quinctilius Varus.", "tokens": ["Herr", "Quinc\u00b7ti\u00b7lius", "Va\u00b7rus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch im Teutoburger Walde", "tokens": ["Doch", "im", "Teu\u00b7to\u00b7bur\u00b7ger", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "ADJA", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Huh, wie pfiff der Wind so kalte;", "tokens": ["Huh", ",", "wie", "pfiff", "der", "Wind", "so", "kal\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VVFIN", "ART", "NN", "ADV", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Raben flogen durch die Luft", "tokens": ["Ra\u00b7ben", "flo\u00b7gen", "durch", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es war ein Moderduft", "tokens": ["Und", "es", "war", "ein", "Mo\u00b7der\u00b7duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie von Blut und Leichen.", "tokens": ["Wie", "von", "Blut", "und", "Lei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Pl\u00f6tzlich aus des Waldes Duster", "tokens": ["Pl\u00f6tz\u00b7lich", "aus", "des", "Wal\u00b7des", "Dus\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Brachen krampfhaft die Cherusker;", "tokens": ["Bra\u00b7chen", "krampf\u00b7haft", "die", "Che\u00b7rus\u00b7ker", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit Gott f\u00fcr F\u00fcrst und Vaterland", "tokens": ["Mit", "Gott", "f\u00fcr", "F\u00fcrst", "und", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "St\u00fcrmten sie von Wut entbrannt", "tokens": ["St\u00fcrm\u00b7ten", "sie", "von", "Wut", "ent\u00b7brannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gegen die Legionen.", "tokens": ["Ge\u00b7gen", "die", "Le\u00b7gi\u00b7o\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Weh! das ward ein gro\u00dfes Morden.", "tokens": ["Weh", "!", "das", "ward", "ein", "gro\u00b7\u00dfes", "Mor\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PDS", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sie erschlugen die Kohorten;", "tokens": ["Sie", "er\u00b7schlu\u00b7gen", "die", "Ko\u00b7hor\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nur die r\u00f6mische Reiterei", "tokens": ["Nur", "die", "r\u00f6\u00b7mi\u00b7sche", "Rei\u00b7te\u00b7rei"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Rettete sich noch ins Frei',", "tokens": ["Ret\u00b7te\u00b7te", "sich", "noch", "ins", "Frei'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn sie war zu Pferde.", "tokens": ["Denn", "sie", "war", "zu", "Pfer\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "O Quinctili, armer Feldherr!", "tokens": ["O", "Quinc\u00b7ti\u00b7li", ",", "ar\u00b7mer", "Feld\u00b7herr", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Dachtest du, da\u00df so die Welt w\u00e4r'?", "tokens": ["Dach\u00b7test", "du", ",", "da\u00df", "so", "die", "Welt", "w\u00e4r'", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Er geriet in einen Sumpf,", "tokens": ["Er", "ge\u00b7riet", "in", "ei\u00b7nen", "Sumpf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Verlor zwei Stiefel und einen Strumpf", "tokens": ["Ver\u00b7lor", "zwei", "Stie\u00b7fel", "und", "ei\u00b7nen", "Strumpf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "CARD", "NN", "KON", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und blieb elend stecken.", "tokens": ["Und", "blieb", "e\u00b7lend", "ste\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Da sprach er voll \u00c4rgernussen", "tokens": ["Da", "sprach", "er", "voll", "\u00c4r\u00b7ger\u00b7nus\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Zum Centurio Titiussen:", "tokens": ["Zum", "Cen\u00b7tu\u00b7rio", "Ti\u00b7ti\u00b7us\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "\u00bbkamerade, zeuch dein Schwert hervor", "tokens": ["\u00bb", "ka\u00b7me\u00b7ra\u00b7de", ",", "zeuch", "dein", "Schwert", "her\u00b7vor"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "$,", "VVIMP", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und von hinten mich durchbohr',", "tokens": ["Und", "von", "hin\u00b7ten", "mich", "durch\u00b7bohr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da doch alles futsch ist.\u00ab", "tokens": ["Da", "doch", "al\u00b7les", "fut\u00b7sch", "ist", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADV", "PIS", "ADJD", "VAFIN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "In dem armen r\u00f6mischen Heere", "tokens": ["In", "dem", "ar\u00b7men", "r\u00f6\u00b7mi\u00b7schen", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Diente auch als Volont\u00e4re", "tokens": ["Dien\u00b7te", "auch", "als", "Vo\u00b7lon\u00b7t\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sc\u00e4vola, ein Rechtskandidat,", "tokens": ["Sc\u00e4\u00b7vo\u00b7la", ",", "ein", "Rechts\u00b7kan\u00b7di\u00b7dat", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Den man schn\u00f6d gefangen hat,", "tokens": ["Den", "man", "schn\u00f6d", "ge\u00b7fan\u00b7gen", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die andern alle.", "tokens": ["Wie", "die", "an\u00b7dern", "al\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Diesem ist es schlimm ergangen;", "tokens": ["Die\u00b7sem", "ist", "es", "schlimm", "er\u00b7gan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eh' da\u00df man ihn aufgehangen", "tokens": ["Eh'", "da\u00df", "man", "ihn", "auf\u00b7ge\u00b7han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PIS", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Stach man ihn durch Zung' und Herz,", "tokens": ["Stach", "man", "ihn", "durch", "Zung'", "und", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nagelte ihn hinterw\u00e4rts", "tokens": ["Na\u00b7gel\u00b7te", "ihn", "hin\u00b7ter\u00b7w\u00e4rts"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Auf sein Corpus Juris.", "tokens": ["Auf", "sein", "Cor\u00b7pus", "Ju\u00b7ris", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.9": {"line.1": {"text": "Als die Waldschlacht war zu Ende,", "tokens": ["Als", "die", "Wald\u00b7schlacht", "war", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Rieb F\u00fcrst Hermann sich die H\u00e4nde,", "tokens": ["Rieb", "F\u00fcrst", "Her\u00b7mann", "sich", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "PRF", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und um seinen Sieg zu weihn,", "tokens": ["Und", "um", "sei\u00b7nen", "Sieg", "zu", "weihn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Lud er die Cherusker ein", "tokens": ["Lud", "er", "die", "Che\u00b7rus\u00b7ker", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "ART", "NN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Zu 'nem gro\u00dfen Fr\u00fchst\u00fcck.", "tokens": ["Zu", "'nem", "gro\u00b7\u00dfen", "Fr\u00fch\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Nur in Rom war man nicht heiter,", "tokens": ["Nur", "in", "Rom", "war", "man", "nicht", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "VAFIN", "PIS", "PTKNEG", "ADJD", "$,"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Sondern kaufte Trauerkleider.", "tokens": ["Son\u00b7dern", "kauf\u00b7te", "Trau\u00b7er\u00b7klei\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "G'rade als beim Mittagmahl", "tokens": ["G'\u00b7ra\u00b7de", "als", "beim", "Mit\u00b7tag\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Augustus sa\u00df im Kaisersaal,", "tokens": ["Au\u00b7gus\u00b7tus", "sa\u00df", "im", "Kai\u00b7ser\u00b7saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Kam die Trauerbotschaft.", "tokens": ["Kam", "die", "Trau\u00b7er\u00b7bot\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Erst blieb ihm vor j\u00e4hem Schrecken", "tokens": ["Erst", "blieb", "ihm", "vor", "j\u00e4\u00b7hem", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ein St\u00fcck Pfau im Halse stecken,", "tokens": ["Ein", "St\u00fcck", "Pfau", "im", "Hal\u00b7se", "ste\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dann geriet er au\u00dfer sich", "tokens": ["Dann", "ge\u00b7riet", "er", "au\u00b7\u00dfer", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und schrie: \u00bbVarus, Fluch auf dich!", "tokens": ["Und", "schrie", ":", "\u00bb", "Va\u00b7rus", ",", "Fluch", "auf", "dich", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "NE", "$,", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Sein deutscher Sklave, Schmidt gehei\u00dfen,", "tokens": ["Sein", "deut\u00b7scher", "Skla\u00b7ve", ",", "Schmidt", "ge\u00b7hei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dacht': \u00bbIhn soll das M\u00e4usle bei\u00dfen,", "tokens": ["Dacht'", ":", "\u00bb", "Ihn", "soll", "das", "M\u00e4us\u00b7le", "bei\u00b7\u00dfen", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn er sie je wieder kriegt,", "tokens": ["Wenn", "er", "sie", "je", "wie\u00b7der", "kriegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Denn wer einmal tot da liegt,", "tokens": ["Denn", "wer", "ein\u00b7mal", "tot", "da", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wird nicht mehr lebendig.\u00ab", "tokens": ["Wird", "nicht", "mehr", "le\u00b7ben\u00b7dig", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PTKNEG", "ADV", "ADJD", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "s