{"dta.poem.18848": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Gute Nacht !", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Nun so legt euch liebe Lieder", "tokens": ["Nun", "so", "legt", "euch", "lie\u00b7be", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Busen meinem Volke", "tokens": ["An", "den", "Bu\u00b7sen", "mei\u00b7nem", "Vol\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und in einer Moschus-Wolke", "tokens": ["Und", "in", "ei\u00b7ner", "Mo\u00b7schus\u00b7Wol\u00b7ke"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "H\u00fcte Gabriel die Glieder", "tokens": ["H\u00fc\u00b7te", "Ga\u00b7bri\u00b7el", "die", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NE", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Des Erm\u00fcdeten gef\u00e4llig;", "tokens": ["Des", "Er\u00b7m\u00fc\u00b7de\u00b7ten", "ge\u00b7f\u00e4l\u00b7lig", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dass er frisch und wohlerhalten,", "tokens": ["Dass", "er", "frisch", "und", "woh\u00b7ler\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Froh wie immer, gern gesellig,", "tokens": ["Froh", "wie", "im\u00b7mer", ",", "gern", "ge\u00b7sel\u00b7lig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADV", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00f6ge Felsenkl\u00fcfte spalten,", "tokens": ["M\u00f6\u00b7ge", "Fel\u00b7sen\u00b7kl\u00fcf\u00b7te", "spal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Um des Paradieses Weiten,", "tokens": ["Um", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Mit Heroen aller Zeiten,", "tokens": ["Mit", "He\u00b7roen", "al\u00b7ler", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Im Genusse zu durchschreiten;", "tokens": ["Im", "Ge\u00b7nus\u00b7se", "zu", "durch\u00b7schrei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Wo das Sch\u00f6ne, stets das Neue,", "tokens": ["Wo", "das", "Sch\u00f6\u00b7ne", ",", "stets", "das", "Neu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Immer w\u00e4chst nach allen Seiten,", "tokens": ["Im\u00b7mer", "w\u00e4chst", "nach", "al\u00b7len", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Dass die Unzahl sich erfreue.", "tokens": ["Dass", "die", "Un\u00b7zahl", "sich", "er\u00b7freu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Ja, das H\u00fcndlein gar, das treue,", "tokens": ["Ja", ",", "das", "H\u00fcnd\u00b7lein", "gar", ",", "das", "treu\u00b7e", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "ADV", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Darf die Herren hinbegleiten.", "tokens": ["Darf", "die", "Her\u00b7ren", "hin\u00b7be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}