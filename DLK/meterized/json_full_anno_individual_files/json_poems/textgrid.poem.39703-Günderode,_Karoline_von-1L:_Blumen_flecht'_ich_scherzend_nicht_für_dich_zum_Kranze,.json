{"textgrid.poem.39703": {"metadata": {"author": {"name": "G\u00fcnderode, Karoline von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Blumen flecht' ich scherzend nicht f\u00fcr dich zum Kranze,", "genre": "verse", "period": "N.A.", "pub_year": 1801, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Blumen flecht' ich scherzend nicht f\u00fcr dich zum Kranze,", "tokens": ["Blu\u00b7men", "flecht'", "ich", "scher\u00b7zend", "nicht", "f\u00fcr", "dich", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und mein Rhythmus weiht sich nicht zum leichten Tanze,", "tokens": ["Und", "mein", "Rhyth\u00b7mus", "weiht", "sich", "nicht", "zum", "leich\u00b7ten", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Von Bestimmung red' er ernste Worte dir.", "tokens": ["Von", "Be\u00b7stim\u00b7mung", "red'", "er", "erns\u00b7te", "Wor\u00b7te", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Hoffend, w\u00fcnschend, suchst du \u2013 doch vernimm die Lehre,", "tokens": ["Hof\u00b7fend", ",", "w\u00fcn\u00b7schend", ",", "suchst", "du", "\u2013", "doch", "ver\u00b7nimm", "die", "Leh\u00b7re", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "$,", "VVFIN", "PPER", "$(", "ADV", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wenn dem Herzen jeder Wunsch befriedigt w\u00e4re,", "tokens": ["Wenn", "dem", "Her\u00b7zen", "je\u00b7der", "Wunsch", "be\u00b7frie\u00b7digt", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PIAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ungestillet bleibt das Sehnen deiner Brust.", "tokens": ["Un\u00b7ge\u00b7stil\u00b7let", "bleibt", "das", "Seh\u00b7nen", "dei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.3": {"line.1": {"text": "Keins von allen G\u00fctern dieser weiten Erde,", "tokens": ["Keins", "von", "al\u00b7len", "G\u00fc\u00b7tern", "die\u00b7ser", "wei\u00b7ten", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Keines! dem nicht Schmerz und Reue sei Gef\u00e4hrte,", "tokens": ["Kei\u00b7nes", "!", "dem", "nicht", "Schmerz", "und", "Reu\u00b7e", "sei", "Ge\u00b7f\u00e4hr\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$.", "ART", "PTKNEG", "NN", "KON", "NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ueberall verfolgt die Plageg\u00f6ttin dich.", "tokens": ["Ue\u00b7be\u00b7rall", "ver\u00b7folgt", "die", "Pla\u00b7ge\u00b7g\u00f6t\u00b7tin", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.4": {"line.1": {"text": "Freundschaft, Liebe winken freundlich aus der Ferne,", "tokens": ["Freund\u00b7schaft", ",", "Lie\u00b7be", "win\u00b7ken", "freund\u00b7lich", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Wie am Horizonte hell die Br\u00fcder Sterne,", "tokens": ["Wie", "am", "Ho\u00b7ri\u00b7zon\u00b7te", "hell", "die", "Br\u00fc\u00b7der", "Ster\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ADJD", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Doch das eherne Geschick verschont sie nicht.", "tokens": ["Doch", "das", "e\u00b7her\u00b7ne", "Ge\u00b7schick", "ver\u00b7schont", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.5": {"line.1": {"text": "Rei\u00dft dich fremde Schuld nicht von verbund'nen Herzen,", "tokens": ["Rei\u00dft", "dich", "frem\u00b7de", "Schuld", "nicht", "von", "ver\u00b7bun\u00b7d'\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "PTKNEG", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ha! so f\u00fchlst du's sp\u00e4t, durch tief're Schmerzen,", "tokens": ["Ha", "!", "so", "f\u00fchlst", "du's", "sp\u00e4t", ",", "durch", "tie\u00b7f'\u00b7re", "Schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "VVFIN", "PIS", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Eigner Wahn zerri\u00df der Erde sch\u00f6nstes Band.", "tokens": ["Eig\u00b7ner", "Wahn", "zer\u00b7ri\u00df", "der", "Er\u00b7de", "sch\u00f6ns\u00b7tes", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df dein oft get\u00e4uschtes Herz nicht schmerzlich blute.", "tokens": ["Da\u00df", "dein", "oft", "ge\u00b7t\u00e4uschtes", "Herz", "nicht", "schmerz\u00b7lich", "blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ADJA", "NN", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Edlerm Streben spare deines Geistes Kraft.", "tokens": ["Ed\u00b7lerm", "Stre\u00b7ben", "spa\u00b7re", "dei\u00b7nes", "Geis\u00b7tes", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.7": {"line.1": {"text": "Folge nur der Pflicht, ob sie am \u00f6dsten Strande", "tokens": ["Fol\u00b7ge", "nur", "der", "Pflicht", ",", "ob", "sie", "am", "\u00f6ds\u00b7ten", "Stran\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Einsam, ungeliebt und unbeweint dich bannte:", "tokens": ["Ein\u00b7sam", ",", "un\u00b7ge\u00b7liebt", "und", "un\u00b7be\u00b7weint", "dich", "bann\u00b7te", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Deiner G\u00f6tter Abkunft Siegel ist sie dir.", "tokens": ["Dei\u00b7ner", "G\u00f6t\u00b7ter", "Ab\u00b7kunft", "Sie\u00b7gel", "ist", "sie", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "NN", "VAFIN", "PPER", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Tugend ist das Ziel, nach dem die Millionen", "tokens": ["Tu\u00b7gend", "ist", "das", "Ziel", ",", "nach", "dem", "die", "Mil\u00b7lion\u00b7en"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "ART", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Geister, die den ungemess'nen Raum bewohnen,", "tokens": ["Geis\u00b7ter", ",", "die", "den", "un\u00b7ge\u00b7mess'\u00b7nen", "Raum", "be\u00b7woh\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Ringen zur Vollendung und zur G\u00f6ttlichkeit.", "tokens": ["Rin\u00b7gen", "zur", "Vol\u00b7len\u00b7dung", "und", "zur", "G\u00f6tt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "APPRART", "NN", "$."], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Wie Planeten um die Sonn' in ew'gen Kreisen,", "tokens": ["Wie", "Pla\u00b7ne\u00b7ten", "um", "die", "Sonn'", "in", "ew'\u00b7gen", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Eilen sie auf Millionen Weg' und Weisen", "tokens": ["Ei\u00b7len", "sie", "auf", "Mil\u00b7lion\u00b7en", "Weg'", "und", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "NN", "NN", "KON", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hin zum Ideale der Vollkommenheit.", "tokens": ["Hin", "zum", "I\u00b7dea\u00b7le", "der", "Voll\u00b7kom\u00b7men\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.10": {"line.1": {"text": "Blicke stolz hinauf zum herrlich hohen Ziele,", "tokens": ["Bli\u00b7cke", "stolz", "hin\u00b7auf", "zum", "herr\u00b7lich", "ho\u00b7hen", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "ADV", "APPRART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Dr\u00e4ng' ihm zu, und wankst du, irret auch dein Wille,", "tokens": ["Dr\u00e4ng'", "ihm", "zu", ",", "und", "wankst", "du", ",", "ir\u00b7ret", "auch", "dein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "PPER", "$,", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Deiner W\u00fcrd' und Freiheit bleibst du dir bewu\u00dft.", "tokens": ["Dei\u00b7ner", "W\u00fcrd'", "und", "Frei\u00b7heit", "bleibst", "du", "dir", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "PPER", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.11": {"line.1": {"text": "Zwar im Kampfe wird noch deine Kraft erm\u00fcden,", "tokens": ["Zwar", "im", "Kamp\u00b7fe", "wird", "noch", "dei\u00b7ne", "Kraft", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Schwache Erdentugend gibt dem Geist nicht Frieden,", "tokens": ["Schwa\u00b7che", "Er\u00b7den\u00b7tu\u00b7gend", "gibt", "dem", "Geist", "nicht", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ART", "NN", "PTKNEG", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Dennoch deinem Ideale naht sie dich.", "tokens": ["Den\u00b7noch", "dei\u00b7nem", "I\u00b7dea\u00b7le", "naht", "sie", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.12": {"line.1": {"text": "La\u00df denn immerhin die G\u00f6ttin Schicksal walten,", "tokens": ["La\u00df", "denn", "im\u00b7mer\u00b7hin", "die", "G\u00f6t\u00b7tin", "Schick\u00b7sal", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ob sich dunkle Wolken gegen dich auch ballten,", "tokens": ["Ob", "sich", "dunk\u00b7le", "Wol\u00b7ken", "ge\u00b7gen", "dich", "auch", "ball\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ADJA", "NN", "APPR", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Gro\u00df und ruhig siehst du ihrem Gange zu.", "tokens": ["Gro\u00df", "und", "ru\u00b7hig", "siehst", "du", "ih\u00b7rem", "Gan\u00b7ge", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VVFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.13": {"line.1": {"text": "Blumen flecht' ich scherzend nicht f\u00fcr dich zum Kranze,", "tokens": ["Blu\u00b7men", "flecht'", "ich", "scher\u00b7zend", "nicht", "f\u00fcr", "dich", "zum", "Kran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "PTKNEG", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und mein Rhythmus weiht sich nicht zum leichten Tanze,", "tokens": ["Und", "mein", "Rhyth\u00b7mus", "weiht", "sich", "nicht", "zum", "leich\u00b7ten", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PRF", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Von Bestimmung red' er ernste Worte dir.", "tokens": ["Von", "Be\u00b7stim\u00b7mung", "red'", "er", "erns\u00b7te", "Wor\u00b7te", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.14": {"line.1": {"text": "Hoffend, w\u00fcnschend, suchst du \u2013 doch vernimm die Lehre,", "tokens": ["Hof\u00b7fend", ",", "w\u00fcn\u00b7schend", ",", "suchst", "du", "\u2013", "doch", "ver\u00b7nimm", "die", "Leh\u00b7re", ","], "token_info": ["word", "punct",