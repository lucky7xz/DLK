{"textgrid.poem.49946": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Der Frau Generalin v. Varnb\u00fcler", "genre": "verse", "period": "N.A.", "pub_year": 1853, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das edle, das geliebte Angesicht", "tokens": ["Das", "ed\u00b7le", ",", "das", "ge\u00b7lieb\u00b7te", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun wiedersehend, ach, wie fang ich's an,", "tokens": ["Nun", "wie\u00b7der\u00b7se\u00b7hend", ",", "ach", ",", "wie", "fang", "ich's", "an", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In Worte w\u00fcrdig unseren Willkomm", "tokens": ["In", "Wor\u00b7te", "w\u00fcr\u00b7dig", "un\u00b7se\u00b7ren", "Will\u00b7komm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu fassen, bei des Herzens Ungest\u00fcm?", "tokens": ["Zu", "fas\u00b7sen", ",", "bei", "des", "Her\u00b7zens", "Un\u00b7ge\u00b7st\u00fcm", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dieselbige, wie wir dich immer kannten,", "tokens": ["Die\u00b7sel\u00b7bi\u00b7ge", ",", "wie", "wir", "dich", "im\u00b7mer", "kann\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PWAV", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kamst du zur\u00fcck, dein g\u00fctig Auge sagt's,", "tokens": ["Kamst", "du", "zu\u00b7r\u00fcck", ",", "dein", "g\u00fc\u00b7tig", "Au\u00b7ge", "sagt's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Liebe aber ist's, der Ehrfurcht eigen,", "tokens": ["Der", "Lie\u00b7be", "a\u00b7ber", "ist's", ",", "der", "Ehr\u00b7furcht", "ei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df sie, nach kurzem Fernesein, befangen,", "tokens": ["Da\u00df", "sie", ",", "nach", "kur\u00b7zem", "Fer\u00b7ne\u00b7sein", ",", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "APPR", "ADJA", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verwirrt vor ihrem Gegenstande steht,", "tokens": ["Ver\u00b7wirrt", "vor", "ih\u00b7rem", "Ge\u00b7gen\u00b7stan\u00b7de", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gleich als vor einem ungewohnten Gast,", "tokens": ["Gleich", "als", "vor", "ei\u00b7nem", "un\u00b7ge\u00b7wohn\u00b7ten", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn uns sein stiller Blick mit L\u00e4cheln pr\u00fcft.", "tokens": ["Wenn", "uns", "sein", "stil\u00b7ler", "Blick", "mit", "L\u00e4\u00b7cheln", "pr\u00fcft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dieselbe, ja du bist es, teure Mutter!", "tokens": ["Die\u00b7sel\u00b7be", ",", "ja", "du", "bist", "es", ",", "teu\u00b7re", "Mut\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDAT", "$,", "ADV", "PPER", "VAFIN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur tr\u00e4gt dein Antlitz, o wie hell, die Spur", "tokens": ["Nur", "tr\u00e4gt", "dein", "Ant\u00b7litz", ",", "o", "wie", "hell", ",", "die", "Spur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "FM", "KOKOM", "ADJD", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Heiligen, die dich ber\u00fchrt! Umsonst", "tokens": ["Der", "Hei\u00b7li\u00b7gen", ",", "die", "dich", "be\u00b7r\u00fchrt", "!", "Um\u00b7sonst"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$.", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht fleht man ihr; sie wirft dem Wagenden", "tokens": ["Nicht", "fleht", "man", "ihr", ";", "sie", "wirft", "dem", "Wa\u00b7gen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVFIN", "PIS", "PPER", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Aus eisiger Nacht die tauende Rose zu.", "tokens": ["Aus", "ei\u00b7si\u00b7ger", "Nacht", "die", "tau\u00b7en\u00b7de", "Ro\u00b7se", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Wir waren oft bei dir, du glaubst es kaum,", "tokens": ["Wir", "wa\u00b7ren", "oft", "bei", "dir", ",", "du", "glaubst", "es", "kaum", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Leibhaftig eben nicht; doch wenn du pflegtest,", "tokens": ["Leib\u00b7haf\u00b7tig", "e\u00b7ben", "nicht", ";", "doch", "wenn", "du", "pfleg\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PTKNEG", "$.", "ADV", "KOUS", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Im Tannenschatten auf das Moos gebettet,", "tokens": ["Im", "Tan\u00b7nen\u00b7schat\u00b7ten", "auf", "das", "Moos", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Balsamische Luft zu atmen, zweimal t\u00e4glich,", "tokens": ["Bal\u00b7sa\u00b7mi\u00b7sche", "Luft", "zu", "at\u00b7men", ",", "zwei\u00b7mal", "t\u00e4g\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$,", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.5": {"text": "Elise dir zur Seite mit viel andern,", "tokens": ["E\u00b7li\u00b7se", "dir", "zur", "Sei\u00b7te", "mit", "viel", "an\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPRART", "NN", "APPR", "PIAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da kamen wir, zu leichten Traumgestalten", "tokens": ["Da", "ka\u00b7men", "wir", ",", "zu", "leich\u00b7ten", "Traum\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Verkleinert, schl\u00fcpften durch die hohen \u00c4ste", "tokens": ["Ver\u00b7klei\u00b7nert", ",", "schl\u00fcpf\u00b7ten", "durch", "die", "ho\u00b7hen", "\u00c4s\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mit jenen runden Lichtern leis herab,", "tokens": ["Mit", "je\u00b7nen", "run\u00b7den", "Lich\u00b7tern", "leis", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die deines Kleides Saum und Hand und Schultern", "tokens": ["Die", "dei\u00b7nes", "Klei\u00b7des", "Saum", "und", "Hand", "und", "Schul\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Zudringlich k\u00fc\u00dften. Kanntest du sie nicht?", "tokens": ["Zu\u00b7dring\u00b7lich", "k\u00fc\u00df\u00b7ten", ".", "Kann\u00b7test", "du", "sie", "nicht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "VMFIN", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u2013 Wenn nun die ganze Schar in einen Rahmen", "tokens": ["\u2013", "Wenn", "nun", "die", "gan\u00b7ze", "Schar", "in", "ei\u00b7nen", "Rah\u00b7men"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "KOUS", "ADV", "ART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gefangen, eins am andern, dicht gedr\u00e4ngt,", "tokens": ["Ge\u00b7fan\u00b7gen", ",", "eins", "am", "an\u00b7dern", ",", "dicht", "ge\u00b7dr\u00e4ngt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PIS", "APPRART", "ADJA", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Sich wieder zeigte \u2013 ob du sie wohl kennst?", "tokens": ["Sich", "wie\u00b7der", "zeig\u00b7te", "\u2013", "ob", "du", "sie", "wohl", "kennst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVFIN", "$(", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das edle, das geliebte Angesicht", "tokens": ["Das", "ed\u00b7le", ",", "das", "ge\u00b7lieb\u00b7te", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun wiedersehend, ach, wie fang ich's an,", "tokens": ["Nun", "wie\u00b7der\u00b7se\u00b7hend", ",", "ach", ",", "wie", "fang", "ich's", "an", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ITJ", "$,", "PWAV", "VVFIN", "PIS", "PTKVZ", "$,"