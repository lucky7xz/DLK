{"textgrid.poem.28039": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Lentzen glaubt man Brot; Brot hofft man in dem Sommer;", "tokens": ["Im", "Lent\u00b7zen", "glaubt", "man", "Brot", ";", "Brot", "hofft", "man", "in", "dem", "Som\u00b7mer", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "NN", "$.", "NN", "VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Herbste nimmt man Brot; Der Winter stillt den Kummer.", "tokens": ["Im", "Herbs\u00b7te", "nimmt", "man", "Brot", ";", "Der", "Win\u00b7ter", "stillt", "den", "Kum\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PIS", "NN", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Christ lernt glauben erst; nach diesem lernt er hoffen;", "tokens": ["Ein", "Christ", "lernt", "glau\u00b7ben", "erst", ";", "nach", "die\u00b7sem", "lernt", "er", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ADV", "$.", "APPR", "PDAT", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Hoffnung macht ihn starck; im Tod ist alles troffen.", "tokens": ["Die", "Hoff\u00b7nung", "macht", "ihn", "starck", ";", "im", "Tod", "ist", "al\u00b7les", "trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "APPRART", "NN", "VAFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sonst ist es lieblich erst, ein Christ genennt zu werden;", "tokens": ["Sonst", "ist", "es", "lieb\u00b7lich", "erst", ",", "ein", "Christ", "ge\u00b7nennt", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADV", "$,", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wann aber Hitze k\u00fcmmt, M\u00fch, Sorgen und Beschwerden,", "tokens": ["Wann", "a\u00b7ber", "Hit\u00b7ze", "k\u00fcmmt", ",", "M\u00fch", ",", "Sor\u00b7gen", "und", "Be\u00b7schwer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "VVFIN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da geht es schwitzig her; doch folgen drauff viel Fr\u00fcchte,", "tokens": ["Da", "geht", "es", "schwit\u00b7zig", "her", ";", "doch", "fol\u00b7gen", "drauff", "viel", "Fr\u00fcch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "ADV", "VVFIN", "PAV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bi\u00df letzlich uns der Tod die volle Gn\u00fcge richte.", "tokens": ["Bi\u00df", "letz\u00b7lich", "uns", "der", "Tod", "die", "vol\u00b7le", "Gn\u00fc\u00b7ge", "rich\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": [