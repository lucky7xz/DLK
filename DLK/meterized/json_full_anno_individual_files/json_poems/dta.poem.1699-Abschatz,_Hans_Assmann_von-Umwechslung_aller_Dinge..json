{"dta.poem.1699": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Umwechslung aller Dinge.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Armutt f\u00fchrt den Fried/ und dieser Reichthum ein/", "tokens": ["Die", "Ar\u00b7mutt", "f\u00fchrt", "den", "Fried", "/", "und", "die\u00b7ser", "Reicht\u00b7hum", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$(", "KON", "PDAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey Reichthum pfleget man verschwenderisch zu seyn/", "tokens": ["Bey", "Reicht\u00b7hum", "pfle\u00b7get", "man", "ver\u00b7schwen\u00b7de\u00b7risch", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Verschwendung zeucht herbey des Krieges Ungemach/", "tokens": ["Ver\u00b7schwen\u00b7dung", "zeucht", "her\u00b7bey", "des", "Krie\u00b7ges", "Un\u00b7ge\u00b7mach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem Kriege folgen Noth und Jammer stetig nach.", "tokens": ["Dem", "Krie\u00b7ge", "fol\u00b7gen", "Noth", "und", "Jam\u00b7mer", "ste\u00b7tig", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ha\u00df/ Leben/ Ruhe gibt Gunst/ Tod und M\u00fchsamkeit/", "tokens": ["Ha\u00df", "/", "Le\u00b7ben", "/", "Ru\u00b7he", "gibt", "Gunst", "/", "Tod", "und", "M\u00fch\u00b7sam\u00b7keit", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "VVFIN", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es steht und liegt/ was lag und stand vor kurtzer Zeit.", "tokens": ["Es", "steht", "und", "liegt", "/", "was", "lag", "und", "stand", "vor", "kurt\u00b7zer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$(", "PWS", "VVFIN", "KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der vor Gefangne f\u00e4ngt/ der vor Besiegte siegt/", "tokens": ["Der", "vor", "Ge\u00b7fang\u00b7ne", "f\u00e4ngt", "/", "der", "vor", "Be\u00b7sieg\u00b7te", "siegt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$(", "ART", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sieht wie der stoltze Feind zu seinen F\u00fcssen liegt.", "tokens": ["Sieht", "wie", "der", "stolt\u00b7ze", "Feind", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Es mu\u00df das freche Volck den frechen Herren scheuen;", "tokens": ["Es", "mu\u00df", "das", "fre\u00b7che", "Volck", "den", "fre\u00b7chen", "Her\u00b7ren", "scheu\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dem wilden F\u00fcrsten mu\u00df der wilde P\u00f6fel dr\u00e4uen.", "tokens": ["Dem", "wil\u00b7den", "F\u00fcrs\u00b7ten", "mu\u00df", "der", "wil\u00b7de", "P\u00f6\u00b7fel", "dr\u00e4u\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So kehrt das Schicksal um/ was die geraume Welt/", "tokens": ["So", "kehrt", "das", "Schick\u00b7sal", "um", "/", "was", "die", "ge\u00b7rau\u00b7me", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "$(", "PWS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was Mensch/ was Stadt und Land an um und in sich h\u00e4lt.", "tokens": ["Was", "Mensch", "/", "was", "Stadt", "und", "Land", "an", "um", "und", "in", "sich", "h\u00e4lt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$(", "PWS", "NN", "KON", "NN", "APPR", "APPR", "KON", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}