{"textgrid.poem.65192": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Den duft'gen Kr\u00e4nzen wollt' ich gern entsagen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Den duft'gen Kr\u00e4nzen wollt' ich gern entsagen,", "tokens": ["Den", "duft'\u00b7gen", "Kr\u00e4n\u00b7zen", "wollt'", "ich", "gern", "ent\u00b7sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lieb' und Jugend, inniglich verbunden,", "tokens": ["Die", "Lieb'", "und", "Ju\u00b7gend", ",", "in\u00b7nig\u00b7lich", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir einst gereicht in meinen Bl\u00fctestunden,", "tokens": ["Mir", "einst", "ge\u00b7reicht", "in", "mei\u00b7nen", "Bl\u00fc\u00b7tes\u00b7tun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und klag' ich jetzt, nicht ihnen gilt mein Klagen.", "tokens": ["Und", "klag'", "ich", "jetzt", ",", "nicht", "ih\u00b7nen", "gilt", "mein", "Kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PTKNEG", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch, da\u00df, bedr\u00e4ngt von unl\u00f6sbaren Fragen,", "tokens": [