{"dta.poem.2136": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "3. Des Gesichts.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.71", "af:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn in die spiegelnde Crystallen", "tokens": ["Wenn", "in", "die", "spie\u00b7geln\u00b7de", "Crys\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.2": {"text": "Der Augen so viel Wunder fallen,", "tokens": ["Der", "Au\u00b7gen", "so", "viel", "Wun\u00b7der", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wir von allen dem, was sch\u00f6n,", "tokens": ["Und", "wir", "von", "al\u00b7len", "dem", ",", "was", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "PDS", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So herrliche Gestalten sehn,", "tokens": ["So", "herr\u00b7li\u00b7che", "Ge\u00b7stal\u00b7ten", "sehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die uns durch Farb und Licht vergn\u00fcgen,", "tokens": ["Die", "uns", "durch", "Farb", "und", "Licht", "ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Warum will unsre Seele nicht", "tokens": ["Wa\u00b7rum", "will", "uns\u00b7re", "See\u00b7le", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zu solcher Lust bey dem Gesicht", "tokens": ["Zu", "sol\u00b7cher", "Lust", "bey", "dem", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die Kraft der Ueberlegung f\u00fcgen,", "tokens": ["Die", "Kraft", "der", "Ue\u00b7ber\u00b7le\u00b7gung", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und nicht an den, der f\u00fcr die Pracht", "tokens": ["Und", "nicht", "an", "den", ",", "der", "f\u00fcr", "die", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PTKNEG", "APPR", "ART", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-----+-+", "measure": "unknown.measure.di"}, "line.10": {"text": "Der sch\u00f6nen Welt sie sinnlich macht,", "tokens": ["Der", "sch\u00f6\u00b7nen", "Welt", "sie", "sinn\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Jhr dazu Augen wollen schenken,", "tokens": ["Ihr", "da\u00b7zu", "Au\u00b7gen", "wol\u00b7len", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Mit Dank, in ihrer Lust nicht denken?", "tokens": ["Mit", "Dank", ",", "in", "ih\u00b7rer", "Lust", "nicht", "den\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Warum will sie, nach ihrer Pflicht,", "tokens": ["Wa\u00b7rum", "will", "sie", ",", "nach", "ih\u00b7rer", "Pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.14": {"text": "Nicht wenigstens das Sonnenlicht,", "tokens": ["Nicht", "we\u00b7nigs\u00b7tens", "das", "Son\u00b7nen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und in ihr den, der ihre Pracht", "tokens": ["Und", "in", "ihr", "den", ",", "der", "ih\u00b7re", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Zum Nutz der Kreatur gemacht,", "tokens": ["Zum", "Nutz", "der", "Kre\u00b7a\u00b7tur", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Nicht preisen, seiner sich nicht freuen,", "tokens": ["Nicht", "prei\u00b7sen", ",", "sei\u00b7ner", "sich", "nicht", "freu\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "PPOSAT", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Ein froh Bewundern ihm nicht weihen?", "tokens": ["Ein", "froh", "Be\u00b7wun\u00b7dern", "ihm", "nicht", "wei\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}