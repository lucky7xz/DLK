{"textgrid.poem.36291": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Reden \u00fcber die Religion", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es sieht der Musen Freund die offne Pforte", "tokens": ["Es", "sieht", "der", "Mu\u00b7sen", "Freund", "die", "off\u00b7ne", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des gro\u00dfen Tempels sich auf S\u00e4ulen heben,", "tokens": ["Des", "gro\u00b7\u00dfen", "Tem\u00b7pels", "sich", "auf", "S\u00e4u\u00b7len", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wo Pilaster ruhn und Kuppeln streben,", "tokens": ["Und", "wo", "Pi\u00b7las\u00b7ter", "ruhn", "und", "Kup\u00b7peln", "stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "NN", "VVINF", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Naht er getrost dem kunstgeweihten Orte.", "tokens": ["Naht", "er", "ge\u00b7trost", "dem", "kunst\u00b7ge\u00b7weih\u00b7ten", "Or\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drin t\u00f6nt Musik dem Frager Zauberworte,", "tokens": ["Drin", "t\u00f6nt", "Mu\u00b7sik", "dem", "Fra\u00b7ger", "Zau\u00b7ber\u00b7wor\u00b7te", ","], "token_info": ["word", "word", "word",