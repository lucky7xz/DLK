{"textgrid.poem.51072": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "6", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als endlich sie den Sarg hier abgesetzt,", "tokens": ["Als", "end\u00b7lich", "sie", "den", "Sarg", "hier", "ab\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Deckel hoben noch zu guter Letzt,", "tokens": ["Den", "De\u00b7ckel", "ho\u00b7ben", "noch", "zu", "gu\u00b7ter", "Letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In jenem Augenblick hab ich gesehn,", "tokens": ["In", "je\u00b7nem", "Au\u00b7gen\u00b7blick", "hab", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie just die Sonne schied im Untergehn.", "tokens": ["Wie", "just", "die", "Son\u00b7ne", "schied", "im", "Un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Beleuchtet von dem abendroten Strahl", "tokens": ["Be\u00b7leuch\u00b7tet", "von", "dem", "a\u00b7ben\u00b7dro\u00b7ten", "Strahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sah ich all die Gesichter noch einmal,", "tokens": ["Sah", "ich", "all", "die", "Ge\u00b7sich\u00b7ter", "noch", "ein\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "ART", "NN", "ADV", "ADV", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Den Turmknopf oben in der goldnen Ruh \u2013", "tokens": ["Den", "Turm\u00b7knopf", "o\u00b7ben", "in", "der", "gold\u00b7nen", "Ruh", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es war ein Blitz, sie schlossen wieder zu.", "tokens": ["Es", "war", "ein", "Blitz", ",", "sie", "schlos\u00b7sen", "wie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich sah auch zwischen Auf- und Niederschlag,", "tokens": ["Ich", "sah", "auch", "zwi\u00b7schen", "Auf", "und", "Nie\u00b7der\u00b7schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie M\u00e4rzenschnee rings auf den Gr\u00e4bern lag;", "tokens": ["Wie", "M\u00e4r\u00b7zen\u00b7schnee", "rings", "auf", "den", "Gr\u00e4\u00b7bern", "lag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Wetter mu\u00df seither gebrochen sein,", "tokens": ["Das", "Wet\u00b7ter", "mu\u00df", "sei\u00b7ther", "ge\u00b7bro\u00b7chen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADJD", "VVPP", "VAINF", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Denn feucht dringt es in diesen leichten Schrein.", "tokens": ["Denn", "feucht", "dringt", "es", "in", "die\u00b7sen", "leich\u00b7ten", "Schrein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich h\u00f6r ein Knistern, wie wenn sacht und leis", "tokens": ["Ich", "h\u00f6r", "ein", "Knis\u00b7tern", ",", "wie", "wenn", "sacht", "und", "leis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOKOM", "KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sich Schollen l\u00f6sen von des Winters Eis;", "tokens": ["Sich", "Schol\u00b7len", "l\u00f6\u00b7sen", "von", "des", "Win\u00b7ters", "Eis", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich \u00e4rmster Lenzfreund bin ja auch erwacht", "tokens": ["Ich", "\u00e4rms\u00b7ter", "Lenz\u00b7freund", "bin", "ja", "auch", "er\u00b7wacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kann nicht regen mich in dunkler Nacht!", "tokens": ["Und", "kann", "nicht", "re\u00b7gen", "mich", "in", "dunk\u00b7ler", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADJA", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Wie jeglich Samenkorn sich m\u00e4chtig dehnt,", "tokens": ["Wie", "jeg\u00b7lich", "Sa\u00b7men\u00b7korn", "sich", "m\u00e4ch\u00b7tig", "dehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der junge Halm ans warme Licht sich sehnt,", "tokens": ["Der", "jun\u00b7ge", "Halm", "ans", "war\u00b7me", "Licht", "sich", "sehnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So reck ich den gefangnen, meinen Leib,", "tokens": ["So", "reck", "ich", "den", "ge\u00b7fang\u00b7nen", ",", "mei\u00b7nen", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ART", "ADJA", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Doch ist's ein fruchtlos grimmer Zeitvertreib!", "tokens": ["Doch", "ist's", "ein", "frucht\u00b7los", "grim\u00b7mer", "Zeit\u00b7ver\u00b7treib", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "H\u00f6rt man nicht klopfen laut da obenw\u00e4rts", "tokens": ["H\u00f6rt", "man", "nicht", "klop\u00b7fen", "laut", "da", "o\u00b7ben\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PTKNEG", "VVFIN", "ADJD", "ADV", "ADV"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Hier mein zum Bl\u00fchen so bereites Herz?", "tokens": ["Hier", "mein", "zum", "Bl\u00fc\u00b7hen", "so", "be\u00b7rei\u00b7tes", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "APPRART", "NN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie wissen nicht, wie es da unten tut,", "tokens": ["Sie", "wis\u00b7sen", "nicht", ",", "wie", "es", "da", "un\u00b7ten", "tut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und keine W\u00fcnschelrute zeigt dies Blut!", "tokens": ["Und", "kei\u00b7ne", "W\u00fcn\u00b7schel\u00b7ru\u00b7te", "zeigt", "dies", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "PDS", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "K\u00e4m auch geschlichen so von ungef\u00e4hr", "tokens": ["K\u00e4m", "auch", "ge\u00b7schli\u00b7chen", "so", "von", "un\u00b7ge\u00b7f\u00e4hr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VVPP", "ADV", "APPR", "ADJD"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Ein alter Schatz- und Quellengr\u00e4ber her,", "tokens": ["Ein", "al\u00b7ter", "Schatz", "und", "Quel\u00b7len\u00b7gr\u00e4\u00b7ber", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "TRUNC", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein St\u00e4blein, nur auf Geld und Gut gericht,", "tokens": ["Sein", "St\u00e4\u00b7blein", ",", "nur", "auf", "Geld", "und", "Gut", "ge\u00b7richt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "APPR", "NN", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Es sp\u00fcrt' das warme rote Br\u00fcnnlein nicht.", "tokens": ["Es", "sp\u00fcrt'", "das", "war\u00b7me", "ro\u00b7te", "Br\u00fcnn\u00b7lein", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Als endlich sie den Sarg hier abgesetzt,", "tokens": ["Als", "end\u00b7lich", "sie", "den", "Sarg", "hier", "ab\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Deckel hoben noch zu guter Letzt,", "tokens": ["Den", "De\u00b7ckel", "ho\u00b7ben", "noch", "zu", "gu\u00b7ter", "Letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In jenem Augenblick hab ich gesehn,", "tokens": ["In", "je\u00b7nem", "Au\u00b7gen\u00b7blick", "hab", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie just die Sonne schied im Untergehn.", "tokens": ["Wie", "just", "die", "Son\u00b7ne", "schied", "im", "Un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "APPR