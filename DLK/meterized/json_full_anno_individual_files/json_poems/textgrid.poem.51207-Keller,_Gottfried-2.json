{"textgrid.poem.51207": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "2", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aber ein kleiner goldener Stern", "tokens": ["A\u00b7ber", "ein", "klei\u00b7ner", "gol\u00b7de\u00b7ner", "Stern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Sang und klang mir in die Ohren:", "tokens": ["Sang", "und", "klang", "mir", "in", "die", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbtr\u00f6ste dich nur, dein Lied ist fern,", "tokens": ["\u00bb", "tr\u00f6s\u00b7te", "dich", "nur", ",", "dein", "Lied", "ist", "fern", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Fern bei uns und nicht verloren!", "tokens": ["Fern", "bei", "uns", "und", "nicht", "ver\u00b7lo\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Findest du nicht oft einen Klang,", "tokens": ["Fin\u00b7dest", "du", "nicht", "oft", "ei\u00b7nen", "Klang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wie zu fr\u00fch her\u00fcbergeklungen?", "tokens": ["Wie", "zu", "fr\u00fch", "her\u00b7\u00fc\u00b7ber\u00b7ge\u00b7klun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKA", "ADJD", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Also hat sich heut dein Sang", "tokens": ["Al\u00b7so", "hat", "sich", "heut", "dein", "Sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Heimlich zu uns hin\u00fcbergeschwungen!", "tokens": ["Heim\u00b7lich", "zu", "uns", "hin\u00b7\u00fc\u00b7ber\u00b7ge\u00b7schwun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "VVINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Dort, im donnernden Weltgesang,", "tokens": ["Dort", ",", "im", "don\u00b7nern\u00b7den", "Welt\u00b7ge\u00b7sang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Wirst du ein leises Lied erkennen,", "tokens": ["Wirst", "du", "ein", "lei\u00b7ses", "Lied", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das dir, wie fernester Glockenklang,", "tokens": ["Das", "dir", ",", "wie", "fer\u00b7nes\u00b7ter", "Glo\u00b7cken\u00b7klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Diesen Sommertag wird nennen.", "tokens": ["Die\u00b7sen", "Som\u00b7mer\u