{"dta.poem.3831": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Recit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Wie selig ist ein Weid,", "tokens": ["Wie", "se\u00b7lig", "ist", "ein", "Weid", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So Kinder zeuget,", "tokens": ["So", "Kin\u00b7der", "zeu\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wie selig ist ihr Leib,", "tokens": ["Wie", "se\u00b7lig", "ist", "ihr", "Leib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Brust, so selbe s\u00e4uget.", "tokens": ["Die", "Brust", ",", "so", "sel\u00b7be", "s\u00e4u\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sie bl", "tokens": ["Sie", "bl"], "token_info": ["word", "word"], "pos": ["PPER", "NE"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Sie la\u00df die Heiligung sich niemahls rauben.", "tokens": ["Sie", "la\u00df", "die", "Hei\u00b7li\u00b7gung", "sich", "nie\u00b7mahls", "rau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wenn sie die Zucht", "tokens": ["Wenn", "sie", "die", "Zucht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Des Allerh\u00f6chsten Vaters sucht,", "tokens": ["Des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Va\u00b7ters", "sucht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In ihm ergebnen Triebe,", "tokens": ["In", "ihm", "er\u00b7geb\u00b7nen", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Und in der Liebe.", "tokens": ["Und", "in", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Fr\u00f6mmigkeit, die sch\u00f6ne Tugend", "tokens": ["Fr\u00f6m\u00b7mig\u00b7keit", ",", "die", "sch\u00f6\u00b7ne", "Tu\u00b7gend"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist die Zierde zarter Jugend,", "tokens": ["Ist", "die", "Zier\u00b7de", "zar\u00b7ter", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja sie bringt allein Gewinn;", "tokens": ["Ja", "sie", "bringt", "al\u00b7lein", "Ge\u00b7winn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "VVFIN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles heisset sonst vergebens;", "tokens": ["Al\u00b7les", "heis\u00b7set", "sonst", "ver\u00b7ge\u00b7bens", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die Verheissung jenes Lebens", "tokens": ["Die", "Ver\u00b7heis\u00b7sung", "je\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die erg\u00f6tzet meinen Sinn.", "tokens": ["Die", "er\u00b7g\u00f6t\u00b7zet", "mei\u00b7nen", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja wohl,", "tokens": ["Ja", "wohl", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKANT", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Denn da ich scheiden soll,", "tokens": ["Denn", "da", "ich", "schei\u00b7den", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gott Lob! so wei\u00df ich nun", "tokens": ["Gott", "Lob", "!", "so", "wei\u00df", "ich", "nun"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu wem ich mich hinwende,", "tokens": ["Zu", "wem", "ich", "mich", "hin\u00b7wen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So kan ich sanffte ruhn.", "tokens": ["So", "kan", "ich", "sanff\u00b7te", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mein Vater! nimm in deine H\u00e4nde", "tokens": ["Mein", "Va\u00b7ter", "!", "nimm", "in", "dei\u00b7ne", "H\u00e4n\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Seele, so da l\u00e4ngsten dein;", "tokens": ["Die", "See\u00b7le", ",", "so", "da", "l\u00e4ngs\u00b7ten", "dein", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "ADV", "VVFIN", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "La\u00df meinen Leib in seiner Grufft", "tokens": ["La\u00df", "mei\u00b7nen", "Leib", "in", "sei\u00b7ner", "Grufft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "In angenehmer Ruhe seyn,", "tokens": ["In", "an\u00b7ge\u00b7neh\u00b7mer", "Ru\u00b7he", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bis ihn dein Sohn, mein Heiland JEsus rufft.", "tokens": ["Bis", "ihn", "dein", "Sohn", ",", "mein", "Hei\u00b7land", "Je\u00b7sus", "rufft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "In der Stille", "tokens": ["In", "der", "Stil\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Sucht mein Wille,", "tokens": ["Sucht", "mein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Herr! dein frohes Himmels-Zelt.", "tokens": ["Herr", "!", "dein", "fro\u00b7hes", "Him\u00b7mels\u00b7Zelt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Du bist meine/", "tokens": ["Du", "bist", "mei\u00b7ne", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ich die Deine,", "tokens": ["Ich", "die", "Dei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "PPOSAT", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nimm mich, GOtt, wenn dirs gef\u00e4llt.", "tokens": ["Nimm", "mich", ",", "Gott", ",", "wenn", "dirs", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "KOUS", "PIS", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn mein Geist wartt mit Verlangen", "tokens": ["Denn", "mein", "Geist", "wartt", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich, Erl\u00f6ser/ zu umfangen.", "tokens": ["Dich", ",", "Er\u00b7l\u00f6\u00b7ser", "/", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Allein, ", "tokens": ["Al\u00b7lein", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Ach dein Verlust thut uns sehr weh.", "tokens": ["Ach", "dein", "Ver\u00b7lust", "thut", "uns", "sehr", "weh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "PPOSAT", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie wird sich nicht dein Schatz bek\u00fcmmern?", "tokens": ["Wie", "wird", "sich", "nicht", "dein", "Schatz", "be\u00b7k\u00fcm\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wird dein armes Kind nicht wimmern?", "tokens": ["Wie", "wird", "dein", "ar\u00b7mes", "Kind", "nicht", "wim\u00b7mern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So soll es k\u00fcnfftighin allein,", "tokens": ["So", "soll", "es", "k\u00fcnff\u00b7tig\u00b7hin", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ohne seine Mutter seyn.", "tokens": ["Und", "oh\u00b7ne", "sei\u00b7ne", "Mut\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O! kan es dich nicht kr\u00e4ncken?", "tokens": ["O", "!", "kan", "es", "dich", "nicht", "kr\u00e4n\u00b7cken", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wer wird wohl k\u00fcnfftighin das arme Sch\u00e4fgen", "tokens": ["Wer", "wird", "wohl", "k\u00fcnff\u00b7tig\u00b7hin", "das", "ar\u00b7me", "Sch\u00e4f\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "tr\u00e4ncken!", "tokens": ["tr\u00e4n\u00b7cken", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.10": {"text": "Wenn du, als Mutter, solches fliehst,", "tokens": ["Wenn", "du", ",", "als", "Mut\u00b7ter", ",", "sol\u00b7ches", "fliehst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "NN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und deine Milch, als seine Nahrung, ihm entziehst.", "tokens": ["Und", "dei\u00b7ne", "Milch", ",", "als", "sei\u00b7ne", "Nah\u00b7rung", ",", "ihm", "ent\u00b7ziehst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "O! bleibe hie,", "tokens": ["O", "!", "blei\u00b7be", "hie", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.13": {"text": "O! la\u00df dich doch bewegen,", "tokens": ["O", "!", "la\u00df", "dich", "doch", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Was wilt du allzufr\u00fch", "tokens": ["Was", "wilt", "du", "all\u00b7zu\u00b7fr\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Dich in die Grufft, und Sarg und Baare legen.", "tokens": ["Dich", "in", "die", "Grufft", ",", "und", "Sarg", "und", "Baa\u00b7re", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Nein; mein Heiland heist mich sterben,", "tokens": ["Nein", ";", "mein", "Hei\u00b7land", "heist", "mich", "ster\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schatz und Kind zu guter Nacht!", "tokens": ["Schatz", "und", "Kind", "zu", "gu\u00b7ter", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich soll nun den Himmel erben,", "tokens": ["Ich", "soll", "nun", "den", "Him\u00b7mel", "er\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Jesus kommt, es ist vollbracht.", "tokens": ["Je\u00b7sus", "kommt", ",", "es", "ist", "voll\u00b7bracht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lasset euer banges Klagen/", "tokens": ["Las\u00b7set", "eu\u00b7er", "ban\u00b7ges", "Kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lernt das Creutz gedultig tragen,", "tokens": ["Lernt", "das", "Creutz", "ge\u00b7dul\u00b7tig", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Stellet euer Seuffzen ein,", "tokens": ["Stel\u00b7let", "eu\u00b7er", "Seuff\u00b7zen", "ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Gott wird euer Vater seyn.", "tokens": ["Gott", "wird", "eu\u00b7er", "Va\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Dort in Zions frohen H\u00f6hen", "tokens": ["Dort", "in", "Zi\u00b7ons", "fro\u00b7hen", "H\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sollet ihr mich wieder sehen,", "tokens": ["Sol\u00b7let", "ihr", "mich", "wie\u00b7der", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Jesus kommt, es ist vollbracht!", "tokens": ["Je\u00b7sus", "kommt", ",", "es", "ist", "voll\u00b7bracht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Schatz und Kind zu guter Nacht!", "tokens": ["Schatz", "und", "Kind", "zu", "gu\u00b7ter", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Gott Lob! die Stund ist kommen, da ich werd", "tokens": ["Gott", "Lob", "!", "die", "Stund", "ist", "kom\u00b7men", ",", "da", "ich", "werd"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "$.", "ART", "NN", "VAFIN", "VVINF", "$,", "KOUS", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "aufgenommen rc.", "tokens": ["auf\u00b7ge\u00b7nom\u00b7men", "rc", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}