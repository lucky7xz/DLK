{"textgrid.poem.55181": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Die Kr\u00e4nze", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Klopstock will uns vom Pindus entfernen; wir sollen nach Lorbeer", "tokens": ["Klops\u00b7tock", "will", "uns", "vom", "Pin\u00b7dus", "ent\u00b7fer\u00b7nen", ";", "wir", "sol\u00b7len", "nach", "Lor\u00b7beer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$.", "PPER", "VMFIN", "APPR", "NE"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Nicht mehr geizen, uns soll inl\u00e4ndische Eiche gen\u00fcgen;", "tokens": ["Nicht", "mehr", "gei\u00b7zen", ",", "uns", "soll", "in\u00b7l\u00e4n\u00b7di\u00b7sche", "Ei\u00b7che", "ge\u00b7n\u00fc\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "VVINF", "$,", "PPER", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.3": {"text": "Und doch f\u00fchret er selbst den \u00fcberepischen Kreuzzug", "tokens": ["Und", "doch", "f\u00fch\u00b7ret", "er", "selbst", "den", "\u00fc\u00b7be\u00b7re\u00b7pi\u00b7schen", "Kreuz\u00b7zug"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+-+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Hin auf Golgathas Gipfel, ausl\u00e4ndische G\u00f6tter zu ehren!", "tokens": ["Hin", "auf", "Gol\u00b7gat\u00b7has", "Gip\u00b7fel", ",", "aus\u00b7l\u00e4n\u00b7di\u00b7sche", "G\u00f6t\u00b7ter", "zu", "eh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "punct", 