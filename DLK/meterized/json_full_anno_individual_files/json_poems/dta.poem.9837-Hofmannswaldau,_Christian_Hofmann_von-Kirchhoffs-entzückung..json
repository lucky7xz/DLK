{"dta.poem.9837": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Kirchhoffs-entz\u00fcckung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo find' ich mich itzund? auff was f\u00fcr stillem sande", "tokens": ["Wo", "find'", "ich", "mich", "it\u00b7zund", "?", "auff", "was", "f\u00fcr", "stil\u00b7lem", "san\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV", "$.", "APPR", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Steht mein erstarrter fu\u00df? best\u00fcrtzte einsamkeit!", "tokens": ["Steht", "mein", "er\u00b7starr\u00b7ter", "fu\u00df", "?", "be\u00b7st\u00fcrtz\u00b7te", "ein\u00b7sam\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "PTKVZ", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer herrscht und wohnet hier im h\u00fcgel-vollen lande/", "tokens": ["Wer", "herrscht", "und", "woh\u00b7net", "hier", "im", "h\u00fc\u00b7gel\u00b7vol\u00b7len", "lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KON", "VVFIN", "ADV", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo taxen und napel gantz h\u00e4uffig hingestreut?", "tokens": ["Wo", "ta\u00b7xen", "und", "na\u00b7pel", "gantz", "h\u00e4uf\u00b7fig", "hin\u00b7ge\u00b7streut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVINF", "KON", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ist dieses nicht das feld/ wo tausend schedel liegen/", "tokens": ["Ist", "die\u00b7ses", "nicht", "das", "feld", "/", "wo", "tau\u00b7send", "sche\u00b7del", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ART", "NN", "$(", "PWAV", "ADJD", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und unter meinem fu\u00df die blancke todten-bein", "tokens": ["Und", "un\u00b7ter", "mei\u00b7nem", "fu\u00df", "die", "blan\u00b7cke", "tod\u00b7ten\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mit schlangen-blauem rost in der verwesung kriegen/", "tokens": ["Mit", "schlan\u00b7gen\u00b7blau\u00b7em", "rost", "in", "der", "ver\u00b7we\u00b7sung", "krie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo leichen-volle gr\u00fcft\u2019 um mich geh\u00fcgelt seyn?", "tokens": ["Wo", "lei\u00b7chen\u00b7vol\u00b7le", "gr\u00fcft'", "um", "mich", "ge\u00b7h\u00fc\u00b7gelt", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich schaue nichtes mehr/ als creutzer/ pf\u00e4l\u2019 und steine;", "tokens": ["Ich", "schau\u00b7e", "nich\u00b7tes", "mehr", "/", "als", "creut\u00b7zer", "/", "pf\u00e4l'", "und", "stei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "$(", "KOUS", "NN", "$(", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Ein faules schweigen stutzt den halb entraften sinn/", "tokens": ["Ein", "fau\u00b7les", "schwei\u00b7gen", "stutzt", "den", "halb", "ent\u00b7raf\u00b7ten", "sinn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und h\u00e4uffet sich noch mehr durch die bemosten z\u00e4une:", "tokens": ["Und", "h\u00e4uf\u00b7fet", "sich", "noch", "mehr", "durch", "die", "be\u00b7mos\u00b7ten", "z\u00e4u\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein kirchhof ist es ja/ worauf ich itzund bin.", "tokens": ["Ein", "kirch\u00b7hof", "ist", "es", "ja", "/", "wo\u00b7rauf", "ich", "it\u00b7zund", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "$(", "PWAV", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Gl\u00fcck zu/ du todten thal/ und ihr besteckten h\u00fcgel/", "tokens": ["Gl\u00fcck", "zu", "/", "du", "tod\u00b7ten", "thal", "/", "und", "ihr", "be\u00b7steck\u00b7ten", "h\u00fc\u00b7gel", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "$(", "PPER", "ADJA", "NN", "$(", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Umb welche/ wie ich schau/ nur tod und sterben siegt!", "tokens": ["Umb", "wel\u00b7che", "/", "wie", "ich", "schau", "/", "nur", "tod", "und", "ster\u00b7ben", "siegt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$(", "PWAV", "PPER", "PTKVZ", "$(", "ADV", "NN", "KON", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Gl\u00fcck zu/ ihr creutzer/ pf\u00e4l\u2019/ ihr stein\u2019 und todten-siegel/", "tokens": ["Gl\u00fcck", "zu", "/", "ihr", "creut\u00b7zer", "/", "pf\u00e4l'", "/", "ihr", "stein'", "und", "tod\u00b7ten\u00b7sie\u00b7gel", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "$(", "PPOSAT", "NN", "$(", "VVFIN", "$(", "PPER", "VVFIN", "KON", "NN", "$("], "meter": "---+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.16": {"text": "Auch die ihr halb und gantz im sarg vermodert liegt!", "tokens": ["Auch", "die", "ihr", "halb", "und", "gantz", "im", "sarg", "ver\u00b7mo\u00b7dert", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPOSAT", "ADJD", "KON", "ADV", "APPRART", "ADJD", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "O s\u00fcsses schlaf-gemach! hier steh\u2019 ich/ umb zu lernen/", "tokens": ["O", "s\u00fcs\u00b7ses", "schlaf\u00b7ge\u00b7mach", "!", "hier", "steh'", "ich", "/", "umb", "zu", "ler\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "$(", "KOUI", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was wahre wei\u00dfheit hei\u00df\u2019t und endlich unser zweg:", "tokens": ["Was", "wah\u00b7re", "wei\u00df\u00b7heit", "hei\u00df't", "und", "end\u00b7lich", "un\u00b7ser", "zweg", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "NN", "VVFIN", "KON", "ADV", "PPOSAT", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "An diesem anblick mu\u00df sich aller witz entfernen;", "tokens": ["An", "die\u00b7sem", "an\u00b7blick", "mu\u00df", "sich", "al\u00b7ler", "witz", "ent\u00b7fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VMFIN", "PRF", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Denn du bist eintzig nur der gr\u00f6\u00df\u2019sten wei\u00dfheit weg.", "tokens": ["Denn", "du", "bist", "eint\u00b7zig", "nur", "der", "gr\u00f6\u00df'\u00b7sten", "wei\u00df\u00b7heit", "weg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADJD", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Wen aber find\u2019 ich hier/ der mein begehren stillet/", "tokens": ["Wen", "a\u00b7ber", "find'", "ich", "hier", "/", "der", "mein", "be\u00b7geh\u00b7ren", "stil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "$(", "ART", "PPOSAT", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und/ was ich lernen will/ mich deutlich unterrichtt?", "tokens": ["Und", "/", "was", "ich", "ler\u00b7nen", "will", "/", "mich", "deut\u00b7lich", "un\u00b7ter\u00b7richtt", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$(", "PWS", "PPER", "VVINF", "VMFIN", "$(", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Soll wol die einsamkeit/ die diesen ort umbh\u00fcllet/", "tokens": ["Soll", "wol", "die", "ein\u00b7sam\u00b7keit", "/", "die", "die\u00b7sen", "ort", "umb\u00b7h\u00fcl\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "$(", "ART", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Dem werck gewachsen seyn? O nein/ bey weitem nicht!", "tokens": ["Dem", "werck", "ge\u00b7wach\u00b7sen", "seyn", "?", "O", "nein", "/", "bey", "wei\u00b7tem", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAINF", "$.", "NE", "PTKANT", "$(", "APPR", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wie wird mir? schauert nicht die haut und alle glieder?", "tokens": ["Wie", "wird", "mir", "?", "schau\u00b7ert", "nicht", "die", "haut", "und", "al\u00b7le", "glie\u00b7der", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$.", "VVFIN", "PTKNEG", "ART", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Umschleust nicht kaltes ei\u00df das hertz in meiner brust?", "tokens": ["Um\u00b7schleust", "nicht", "kal\u00b7tes", "ei\u00df", "das", "hertz", "in", "mei\u00b7ner", "brust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADJA", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Rei\u00dft\u2019 nicht ein wacklen mich in diesem sande nieder/", "tokens": ["Rei\u00dft'", "nicht", "ein", "wack\u00b7len", "mich", "in", "die\u00b7sem", "san\u00b7de", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PTKVZ", "VVFIN", "PRF", "APPR", "PDAT", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und raffet meinen geist in einen duncklen wust?", "tokens": ["Und", "raf\u00b7fet", "mei\u00b7nen", "geist", "in", "ei\u00b7nen", "dunck\u00b7len", "wust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Erblick ich nicht/ wie mond und sterne gantz erblassen/", "tokens": ["Er\u00b7blick", "ich", "nicht", "/", "wie", "mond", "und", "ster\u00b7ne", "gantz", "er\u00b7blas\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$(", "PWAV", "ADJD", "KON", "VVFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und wie nur finstern\u00fcs mich um und um bedeckt?", "tokens": ["Und", "wie", "nur", "fins\u00b7ter\u00b7n\u00fcs", "mich", "um", "und", "um", "be\u00b7deckt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVFIN", "PPER", "PTKVZ", "KON", "APPR", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich f\u00fchl\u2019/ und wei\u00df nicht wie/ durch was mich umgefassen/", "tokens": ["Ich", "f\u00fchl'", "/", "und", "wei\u00df", "nicht", "wie", "/", "durch", "was", "mich", "um\u00b7ge\u00b7fas\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VVFIN", "PTKNEG", "KOKOM", "$(", "APPR", "PRELS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und nicht durch schlechte furcht bi\u00df auf das blut erschreckt.", "tokens": ["Und", "nicht", "durch", "schlech\u00b7te", "furcht", "bi\u00df", "auf", "das", "blut", "er\u00b7schreckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "ADJA", "NN", "APPR", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wie? augen/ schaut ihr nicht die creutzer sich bewegen/", "tokens": ["Wie", "?", "au\u00b7gen", "/", "schaut", "ihr", "nicht", "die", "creut\u00b7zer", "sich", "be\u00b7we\u00b7gen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "NN", "$(", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und die entweltzten stein\u2019 in voller h\u00f6he stehn/", "tokens": ["Und", "die", "ent\u00b7weltz\u00b7ten", "stein'", "in", "vol\u00b7ler", "h\u00f6\u00b7he", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und alle todten sich in ihren s\u00e4rgern regen/", "tokens": ["Und", "al\u00b7le", "tod\u00b7ten", "sich", "in", "ih\u00b7ren", "s\u00e4r\u00b7gern", "re\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "PRF", "APPR", "PPOSAT", "ADJA", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Und eine d\u00fcrre leich aus iederm h\u00fcgel gehn?", "tokens": ["Und", "ei\u00b7ne", "d\u00fcr\u00b7re", "leich", "aus", "ie\u00b7derm", "h\u00fc\u00b7gel", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJD", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Hilf GOtt! was sind dies doch f\u00fcr scheu\u00dfliche gesichte/", "tokens": ["Hilf", "Gott", "!", "was", "sind", "dies", "doch", "f\u00fcr", "scheu\u00df\u00b7li\u00b7che", "ge\u00b7sich\u00b7te", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "PWS", "VAFIN", "PDS", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+++-+-+-+-", "measure": "unknown.measure.septa"}, "line.38": {"text": "Und he\u00dfliche geripp mit schimmel angefeucht?", "tokens": ["Und", "he\u00df\u00b7li\u00b7che", "ge\u00b7ripp", "mit", "schim\u00b7mel", "an\u00b7ge\u00b7feucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Wenn ich mein angesicht nach threm scheitel richte/", "tokens": ["Wenn", "ich", "mein", "an\u00b7ge\u00b7sicht", "nach", "th\u00b7rem", "schei\u00b7tel", "rich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+