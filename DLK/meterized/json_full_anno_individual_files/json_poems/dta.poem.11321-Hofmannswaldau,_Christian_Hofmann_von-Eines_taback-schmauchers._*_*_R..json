{"dta.poem.11321": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines taback-schmauchers.  \n * *  R.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe mich mit rauch, wie alle welt, gen\u00e4hret,", "tokens": ["Ich", "ha\u00b7be", "mich", "mit", "rauch", ",", "wie", "al\u00b7le", "welt", ",", "ge\u00b7n\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ADJD", "$,", "PWAV", "PIAT", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denn alles, was sie liebt, wird von der zeit verzehret.", "tokens": ["Denn", "al\u00b7les", ",", "was", "sie", "liebt", ",", "wird", "von", "der", "zeit", "ver\u00b7zeh\u00b7ret", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein leser! g\u00f6nnst du mir nun eine sanffte ruh;", "tokens": ["Mein", "le\u00b7ser", "!", "g\u00f6nnst", "du", "mir", "nun", "ei\u00b7ne", "sanff\u00b7te", "ruh", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So setz dich auf mein grab, und rauch taback darzu!", "tokens": ["So", "setz", "dich", "auf", "mein", "grab", ",", "und", "rauch", "ta\u00b7back", "dar\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$,", "KON", "ADJD", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}