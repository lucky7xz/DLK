{"dta.poem.902": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Im WINTER", "genre": "Lyrik", "period": "N.A.", "pub_year": "1913", "urn": "urn:nbn:de:kobv:b4-30357-9", "language": ["de:0.99"], "booktitle": "Trakl, Georg: Gedichte. Leipzig, 1913."}, "poem": {"stanza.1": {"line.1": {"text": "Der Acker leuchtet wei\u00df und kalt.", "tokens": ["Der", "A\u00b7cker", "leuch\u00b7tet", "wei\u00df", "und", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Himmel ist einsam und ungeheuer.", "tokens": ["Der", "Him\u00b7mel", "ist", "ein\u00b7sam", "und", "un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Dohlen kreisen \u00fcber dem Weiher", "tokens": ["Doh\u00b7len", "krei\u00b7sen", "\u00fc\u00b7ber", "dem", "Wei\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und J\u00e4ger steigen nieder vom Wald.", "tokens": ["Und", "J\u00e4\u00b7ger", "stei\u00b7gen", "nie\u00b7der", "vom", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Ein Schweigen in schwarzen Wipfeln wohnt.", "tokens": ["Ein", "Schwei\u00b7gen", "in", "schwar\u00b7zen", "Wip\u00b7feln", "wohnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Feuerschein huscht aus den H\u00fctten.", "tokens": ["Ein", "Feu\u00b7er\u00b7schein", "huscht", "aus", "den", "H\u00fct\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Bisweilen schellt sehr fern ein Schlitten", "tokens": ["Bis\u00b7wei\u00b7len", "schellt", "sehr", "fern", "ein", "Schlit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und langsam steigt der graue Mond.", "tokens": ["Und", "lang\u00b7sam", "steigt", "der", "grau\u00b7e", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ein Wild verblutet sanft am Rain", "tokens": ["Ein", "Wild", "ver\u00b7blu\u00b7tet", "sanft", "am", "Rain"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und Raben pl\u00e4tschern in blutigen Gossen.", "tokens": ["Und", "Ra\u00b7ben", "pl\u00e4t\u00b7schern", "in", "blu\u00b7ti\u00b7gen", "Gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Das Rohr bebt gelb und aufgeschossen.", "tokens": ["Das", "Rohr", "bebt", "gelb", "und", "auf\u00b7ge\u00b7schos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Frost, Rauch, ein Schritt im leeren Hain.", "tokens": ["Frost", ",", "Rauch", ",", "ein", "Schritt", "im", "lee\u00b7ren", "Hain", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}