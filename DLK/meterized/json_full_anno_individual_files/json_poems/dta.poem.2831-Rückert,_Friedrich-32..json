{"dta.poem.2831": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "32.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Schon wieder hat der Baum der Hoffnung fehlgetragen,", "tokens": ["Schon", "wie\u00b7der", "hat", "der", "Baum", "der", "Hoff\u00b7nung", "fehl\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und abermal das Reis des Wunsches fehlgeschlagen.", "tokens": ["Und", "a\u00b7ber\u00b7mal", "das", "Reis", "des", "Wun\u00b7sches", "fehl\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ist zu thun? geschwind, bevor der Tag vergeht,", "tokens": ["Was", "ist", "zu", "thun", "?", "ge\u00b7schwind", ",", "be\u00b7vor", "der", "Tag", "ver\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PTKZU", "VVINF", "$.", "VVPP", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Schlag auf das Tagebuch, worin soviel schon steht.", "tokens": ["Schlag", "auf", "das", "Ta\u00b7ge\u00b7buch", ",", "wo\u00b7rin", "so\u00b7viel", "schon", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PWAV", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Trag ein den Fehlertrag, er fehle nicht darin;", "tokens": ["Trag", "ein", "den", "Feh\u00b7ler\u00b7trag", ",", "er", "feh\u00b7le", "nicht", "da\u00b7rin", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ART", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schlag dir dann das Fehlgeschlagne aus dem Sinn.", "tokens": ["Und", "schlag", "dir", "dann", "das", "Fehl\u00b7ge\u00b7schlag\u00b7ne", "aus", "dem", "Sinn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}