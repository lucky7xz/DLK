{"textgrid.poem.25755": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gestern bedeckte dein Haar ein r\u00f6thlicher Puder und Salben,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestern bedeckte dein Haar ein r\u00f6thlicher Puder und Salben,", "tokens": ["Ge\u00b7stern", "be\u00b7deck\u00b7te", "dein", "Haar", "ein", "r\u00f6th\u00b7li\u00b7cher", "Pu\u00b7der", "und", "Sal\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Federn vom Reiher und Strau\u00df, Blumen und Spitzen und Band.", "tokens": ["Fe\u00b7dern", "vom", "Rei\u00b7her", "und", "Strau\u00df", ",", "Blu\u00b7men", "und", "Spit\u00b7zen", "und", "Band", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "KON", "NE", "$,", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "+--+---+--+--+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Heute seh' ich es schwarz, gleich F\u00e4den von seidenem Atla\u00df,", "tokens": ["Heu\u00b7te", "seh'", "ich", "es", "schwarz", ",", "gleich", "F\u00e4\u00b7den", "von", "sei\u00b7de\u00b7nem", "At\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADJD", "$,", "ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Sehe, nicht minder erstaunt, da\u00df es die H\u00fcfte dir k\u00fc\u00dft.", "tokens": ["Se\u00b7he", ",", "nicht", "min\u00b7der", "er\u00b7staunt", ",", "da\u00df", "es", "die", "H\u00fcf\u00b7te", "dir", "k\u00fc\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PTKNEG", "ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Gestern verh\u00fcllte das Fell von einer cyprischen Katze,", "tokens": ["Ge\u00b7stern", "ver\u00b7h\u00fcll\u00b7te", "das", "Fell", "von", "ei\u00b7ner", "cy\u00b7pri\u00b7schen", "Kat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und ein Segel von Flor, m\u00e4chtig sich bl\u00e4hend, die Brust;", "tokens": ["Und", "ein", "Se\u00b7gel", "von", "Flor", ",", "m\u00e4ch\u00b7tig", "sich", "bl\u00e4\u00b7hend", ",", "die", "Brust", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "$,", "ADJD", "PRF", "VVPP", "$,", "ART", "NN", "$."], "meter": "--+--+---+--+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Heut errath' ich sogleich, das Auge werde so wenig", "tokens": ["Heut", "er\u00b7ra\u00b7th'", "ich", "sog\u00b7leich", ",", "das", "Au\u00b7ge", "wer\u00b7de", "so", "we\u00b7nig"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "VAFIN", "ADV", "PIS"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.8": {"text": "Durch den Busen get\u00e4uscht, als durch die Lippen das Herz.", "tokens": ["Durch", "den", "Bu\u00b7sen", "ge\u00b7t\u00e4uscht", ",", "als", "durch", "die", "Lip\u00b7pen", "das", "Herz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Gestern ber\u00fchrte der Saum, \u2013 der Blumenstickerin Sch\u00f6pfung \u2013", "tokens": ["Ge\u00b7stern", "be\u00b7r\u00fchr\u00b7te", "der", "Saum", ",", "\u2013", "der", "Blu\u00b7men\u00b7sti\u00b7cke\u00b7rin", "Sch\u00f6p\u00b7fung", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "$(", "ART", "NN", "NN", "$("], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Selbst die Sohle des Schuhs, der sich vermuthen nur lie\u00df;", "tokens": ["Selbst", "die", "Soh\u00b7le", "des", "Schuhs", ",", "der", "sich", "ver\u00b7mu\u00b7then", "nur", "lie\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN", "ADV", "VVFIN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Heut \u00fcberrasch' ich dich im Morgenr\u00f6ckchen, du Holde,", "tokens": ["Heut", "\u00fc\u00b7ber\u00b7rasch'", "ich", "dich", "im", "Mor\u00b7gen\u00b7r\u00f6ck\u00b7chen", ",", "du", "Hol\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Und du tanzest sogar mit dem chinesischen Fu\u00df'!", "tokens": ["Und", "du", "tan\u00b7zest", "so\u00b7gar", "mit", "dem", "chi\u00b7ne\u00b7si\u00b7schen", "Fu\u00df'", "!"], "token_info": ["word", "word", "word", "w