{"dta.poem.9879": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Kinder! glaubt es nicht/ was mit gef\u00e4rbtem schein/", "tokens": ["O", "Kin\u00b7der", "!", "glaubt", "es", "nicht", "/", "was", "mit", "ge\u00b7f\u00e4rb\u00b7tem", "schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$(", "PWS", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was mit geschmincktem mund pflegt unser wort zu seyn/", "tokens": ["Was", "mit", "ge\u00b7schminck\u00b7tem", "mund", "pflegt", "un\u00b7ser", "wort", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was offt bey seiner seelen", "tokens": ["Was", "offt", "bey", "sei\u00b7ner", "see\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein courtisan verspricht/", "tokens": ["Ein", "cour\u00b7ti\u00b7san", "ver\u00b7spricht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ich mags euch nicht verhehlen/", "tokens": ["Ich", "mags", "euch", "nicht", "ver\u00b7heh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "O kinder! glaubt es nicht.", "tokens": ["O", "kin\u00b7der", "!", "glaubt", "es", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ist etwas in der welt/ das frevel-l\u00fcgen liebt/", "tokens": ["Ist", "et\u00b7was", "in", "der", "welt", "/", "das", "fre\u00b7vel\u00b7l\u00fc\u00b7gen", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$(", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ists ein courtisan/ der s\u00fcsse worte giebt/", "tokens": ["So", "ists", "ein", "cour\u00b7ti\u00b7san", "/", "der", "s\u00fcs\u00b7se", "wor\u00b7te", "giebt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sich erbeut zu sterben;", "tokens": ["Der", "sich", "er\u00b7beut", "zu", "ster\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Glaubt ja nicht/ seyd ihr klug;", "tokens": ["Glaubt", "ja", "nicht", "/", "seyd", "ihr", "klug", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$(", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was ihr denckt zu ererben/", "tokens": ["Was", "ihr", "denckt", "zu", "er\u00b7er\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Ist thorheit/ ist betrug.", "tokens": ["Ist", "thor\u00b7heit", "/", "ist", "be\u00b7trug", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "VAFIN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der will eur selave seyn/ und mit gebognem knie", "tokens": ["Der", "will", "eur", "se\u00b7la\u00b7ve", "seyn", "/", "und", "mit", "ge\u00b7bog\u00b7nem", "knie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPOSAT", "ADJA", "VAINF", "$(", "KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Euch warten auff den dienst/ so sp\u00e4t/ so wieder fr\u00fch;", "tokens": ["Euch", "war\u00b7ten", "auff", "den", "dienst", "/", "so", "sp\u00e4t", "/", "so", "wie\u00b7der", "fr\u00fch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "ADV", "ADJD", "$(", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der steht mit krancken sitten", "tokens": ["Der", "steht", "mit", "kran\u00b7cken", "sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und betet euch schier an;", "tokens": ["Und", "be\u00b7tet", "euch", "schier", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Meint ihr/ da\u00df solches bitten", "tokens": ["Meint", "ihr", "/", "da\u00df", "sol\u00b7ches", "bit\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$(", "KOUS", "PIS", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von hertzen gehen kan?", "tokens": ["Von", "hert\u00b7zen", "ge\u00b7hen", "kan", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Dort lobt euch ein phantast/ und euer augenschein", "tokens": ["Dort", "lobt", "euch", "ein", "phan\u00b7tast", "/", "und", "eu\u00b7er", "au\u00b7gen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$(", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mu\u00df auch der sonnen glantz weit vorzuziehen seyn;", "tokens": ["Mu\u00df", "auch", "der", "son\u00b7nen", "glantz", "weit", "vor\u00b7zu\u00b7zie\u00b7hen", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein blitz ist euch zu gleichen", "tokens": ["Kein", "blitz", "ist", "euch", "zu", "glei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch eurer lichter pracht;", "tokens": ["Noch", "eu\u00b7rer", "lich\u00b7ter", "pracht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die sternen m\u00fcssen weichen/", "tokens": ["Die", "ster\u00b7nen", "m\u00fcs\u00b7sen", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Weil ihr sie schamroth macht.", "tokens": ["Weil", "ihr", "sie", "scham\u00b7roth", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kommt man bi\u00df an die hand/ da ist nicht maa\u00df noch ziel/", "tokens": ["Kommt", "man", "bi\u00df", "an", "die", "hand", "/", "da", "ist", "nicht", "maa\u00df", "noch", "ziel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "APPR", "ART", "NN", "$(", "ADV", "VAFIN", "PTKNEG", "VVFIN", "ADV", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Trotz der sie nicht dem schnee an weisse vorziehn will!", "tokens": ["Trotz", "der", "sie", "nicht", "dem", "schnee", "an", "weis\u00b7se", "vor\u00b7ziehn", "will", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKNEG", "ART", "NN", "APPR", "ADJA", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was schnee? der mu\u00df verdunckelt", "tokens": ["Was", "schnee", "?", "der", "mu\u00df", "ver\u00b7dun\u00b7ckelt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$.", "ART", "VMFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vor euren h\u00e4nden stehn;", "tokens": ["Vor", "eu\u00b7ren", "h\u00e4n\u00b7den", "stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Was gleich dem demant funckelt/", "tokens": ["Was", "gleich", "dem", "de\u00b7mant", "fun\u00b7ckelt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJD", "VVFIN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Ist blo\u00df an euch zu sehn.", "tokens": ["Ist", "blo\u00df", "an", "euch", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Nicht da\u00df ich euer lob zu g\u00f6nnen nicht vermeint;", "tokens": ["Nicht", "da\u00df", "ich", "eu\u00b7er", "lob", "zu", "g\u00f6n\u00b7nen", "nicht", "ver\u00b7meint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich selber lob euch mit: doch rath ich als ein freund/", "tokens": ["Ich", "sel\u00b7ber", "lob", "euch", "mit", ":", "doch", "rath", "ich", "als", "ein", "freund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wolt ihr seyn unbetrogen/", "tokens": ["Wolt", "ihr", "seyn", "un\u00b7be\u00b7tro\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So mercket ihre list;", "tokens": ["So", "mer\u00b7cket", "ih\u00b7re", "list", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn alles ist erlogen/", "tokens": ["Denn", "al\u00b7les", "ist", "er\u00b7lo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Was so gefedert ist.", "tokens": ["Was", "so", "ge\u00b7fe\u00b7dert", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein kind ist auf der welt/ das mich von hertzen liebt/", "tokens": ["Ein", "kind", "ist", "auf", "der", "welt", "/", "das", "mich", "von", "hert\u00b7zen", "liebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$(", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem sich auch mein gem\u00fcth zum gegendienst ergiebt/", "tokens": ["Dem", "sich", "auch", "mein", "ge\u00b7m\u00fcth", "zum", "ge\u00b7gen\u00b7dienst", "er\u00b7giebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ADV", "PPOSAT", "NN", "APPRART", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dem sey di\u00df nicht geschrieben/", "tokens": ["Dem", "sey", "di\u00df", "nicht", "ge\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PDS", "PTKNEG", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dem sey mein hertz verpflicht/", "tokens": ["Dem", "sey", "mein", "hertz", "ver\u00b7pflicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Die will ich treulich lieben/", "tokens": ["Die", "will", "ich", "treu\u00b7lich", "lie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Jhr andern glaubt es nicht.", "tokens": ["Ihr", "an\u00b7dern", "glaubt", "es", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}