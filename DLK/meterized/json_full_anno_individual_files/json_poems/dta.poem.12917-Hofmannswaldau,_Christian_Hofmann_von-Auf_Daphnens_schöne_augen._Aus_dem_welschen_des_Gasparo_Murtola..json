{"dta.poem.12917": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf Daphnens sch\u00f6ne augen.  \n Aus dem welschen des   Gasparo Murtola.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der augen funckelnder saphir", "tokens": ["Der", "au\u00b7gen", "fun\u00b7ckeln\u00b7der", "sa\u00b7phir"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt mir das ebenbild von zweyen himmeln f\u00fcr.", "tokens": ["Stellt", "mir", "das", "e\u00b7ben\u00b7bild", "von", "zwe\u00b7yen", "him\u00b7meln", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "APPR", "VVFIN", "NN", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der sternen reines licht vertritt der sonnen schein,", "tokens": ["Der", "ster\u00b7nen", "rei\u00b7nes", "licht", "ver\u00b7tritt", "der", "son\u00b7nen", "schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und der engel, dessen h\u00e4nden", "tokens": ["Und", "der", "en\u00b7gel", ",", "des\u00b7sen", "h\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie anvertrauet sind, und der sie pflegt zu wenden,", "tokens": ["Sie", "an\u00b7ver\u00b7trau\u00b7et", "sind", ",", "und", "der", "sie", "pflegt", "zu", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VAFIN", "$,", "KON", "PRELS", "PPER", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan niemand sonst, als Amor seyn.", "tokens": ["Kan", "nie\u00b7mand", "sonst", ",", "als", "A\u00b7mor", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "$,", "KOUS", "NE", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}