{"dta.poem.8951": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "I.  \n  Die Fischer auf Capri.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Schroffes Gestad als Pilger besucht, dann wei\u00dft du, wie", "tokens": ["Schrof\u00b7fes", "Ge\u00b7stad", "als", "Pil\u00b7ger", "be\u00b7sucht", ",", "dann", "wei\u00dft", "du", ",", "wie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "KOUS", "NN", "VVPP", "$,", "ADV", "VVFIN", "PPER", "$,", "PWAV"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Dorten ein Landungsplatz f\u00fcr nahende Schiffe zu sp\u00e4hn ist:", "tokens": ["Dor\u00b7ten", "ein", "Lan\u00b7dungs\u00b7platz", "f\u00fcr", "na\u00b7hen\u00b7de", "Schif\u00b7fe", "zu", "sp\u00e4hn", "ist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "+--+-+-+--+-++-", "measure": "iambic.septa.invert"}, "line.3": {"text": "Nur zwey Stellen erscheinen bequem. Manch m\u00e4chtiges", "tokens": ["Nur", "zwey", "Stel\u00b7len", "er\u00b7schei\u00b7nen", "be\u00b7quem", ".", "Manch", "m\u00e4ch\u00b7ti\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "CARD", "NN", "VVFIN", "ADJD", "$.", "PIAT", "ADJA"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Mag der ger\u00e4umige Haven empfahn, der gegen Neapels", "tokens": ["Mag", "der", "ge\u00b7r\u00e4u\u00b7mi\u00b7ge", "Ha\u00b7ven", "em\u00b7pfahn", ",", "der", "ge\u00b7gen", "Nea\u00b7pels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVPP", "$,", "PRELS", "APPR", "NE"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Lieblichen Golf hindeutet und gegen Salerns Meerbusen.", "tokens": ["Lieb\u00b7li\u00b7chen", "Golf", "hin\u00b7deu\u00b7tet", "und", "ge\u00b7gen", "Sa\u00b7lerns", "Meer\u00b7bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "KON", "APPR", "NE", "NN", "$."], "meter": "+--+-+--+-+-+--", "measure": "iambic.hexa.invert"}, "line.6": {"text": "Aber die andere Stelle (sie nennen den kleineren Strand", "tokens": ["A\u00b7ber", "die", "an\u00b7de\u00b7re", "Stel\u00b7le", "(", "sie", "nen\u00b7nen", "den", "klei\u00b7ne\u00b7ren", "Strand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+-+---+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Kehrt sich gegen das \u00f6dere Meer, in die wogende Wild-", "tokens": ["Kehrt", "sich", "ge\u00b7gen", "das", "\u00f6\u00b7de\u00b7re", "Meer", ",", "in", "die", "wo\u00b7gen\u00b7de", "Wild"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "+-+--+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Wo kein Ufer du siehst, als das, auf welchem du selbst", "tokens": ["Wo", "kein", "U\u00b7fer", "du", "siehst", ",", "als", "das", ",", "auf", "wel\u00b7chem", "du", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "PPER", "VVFIN", "$,", "KOUS", "PDS", "$,", "APPR", "PRELS", "PPER", "ADV"], "meter": "--+--+--++--+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Nur ein geringeres Boot mag hier anlanden, es liegen", "tokens": ["Nur", "ein", "ge\u00b7rin\u00b7ge\u00b7res", "Boot", "mag", "hier", "an\u00b7lan\u00b7den", ",", "es", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$,", "PPER", "VVFIN"], "meter": "-+-+--+--++--+-", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "Felsige Tr\u00fcmmer umher, und es braust die best\u00e4ndige", "tokens": ["Fel\u00b7si\u00b7ge", "Tr\u00fcm\u00b7mer", "um\u00b7her", ",", "und", "es", "braust", "die", "be\u00b7st\u00e4n\u00b7di\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "ART", "ADJA"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.11": {"text": "Auf dem erh\u00f6hteren Felsen erscheint ein zerfallendes Vor-", "tokens": ["Auf", "dem", "er\u00b7h\u00f6h\u00b7te\u00b7ren", "Fel\u00b7sen", "er\u00b7scheint", "ein", "zer\u00b7fal\u00b7len\u00b7des", "Vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+--+--+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Mit Schie\u00dfscharten versehn; sey's, da\u00df hier immer ein", "tokens": ["Mit", "Schie\u00df\u00b7schar\u00b7ten", "ver\u00b7sehn", ";", "sey's", ",", "da\u00df", "hier", "im\u00b7mer", "ein"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVINF", "$.", "VAFIN", "$,", "KOUS", "ADV", "ADV", "ART"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Ragte, den offenen Strand vor Algiers Flagge zu h\u00fcten,", "tokens": ["Rag\u00b7te", ",", "den", "of\u00b7fe\u00b7nen", "Strand", "vor", "Al\u00b7giers", "Flag\u00b7ge", "zu", "h\u00fc\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "ADJA", "NN", "APPR", "NE", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Die von dem Eiland oft Jungfrauen und J\u00fcnglinge weg-", "tokens": ["Die", "von", "dem", "Ei\u00b7land", "oft", "Jung\u00b7frau\u00b7en", "und", "J\u00fcng\u00b7lin\u00b7ge", "weg"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "ADV", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.15": {"text": "Sey's, da\u00df gegen den Stolz Englands und erfahrene See-", "tokens": ["Sey's", ",", "da\u00df", "ge\u00b7gen", "den", "Stolz", "En\u00b7glands", "und", "er\u00b7fah\u00b7re\u00b7ne", "See"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "APPR", "ART", "NN", "NE", "KON", "ADJA", "TRUNC"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.16": {"text": "Erst in der j\u00fcngeren Zeit es erbaut der Napoleonide,", "tokens": ["Erst", "in", "der", "j\u00fcn\u00b7ge\u00b7ren", "Zeit", "es", "er\u00b7baut", "der", "Na\u00b7po\u00b7le\u00b7o\u00b7ni\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "---+--+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Dem Parthenope sonst ausspannte die Pferde des Wagens,", "tokens": ["Dem", "Par\u00b7the\u00b7no\u00b7pe", "sonst", "aus\u00b7spann\u00b7te", "die", "Pfer\u00b7de", "des", "Wa\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Ihn dann aber verjagte, verrieth, ja t\u00f6dtete, da er", "tokens": ["Ihn", "dann", "a\u00b7ber", "ver\u00b7jag\u00b7te", ",", "ver\u00b7rieth", ",", "ja", "t\u00f6d\u00b7te\u00b7te", ",", "da", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "$,", "VVFIN", "$,", "ADV", "VVFIN", "$,", "KOUS", "PPER"], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.19": {"text": "An's treulose Gestad' durch schmeichelnde Briefe gelockt", "tokens": ["An's", "treu\u00b7lo\u00b7se", "Ge\u00b7stad'", "durch", "schmei\u00b7cheln\u00b7de", "Brie\u00b7fe", "ge\u00b7lockt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "+----+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.20": {"text": "Steigst du herab in den sandigen Kies, so gewahrst du", "tokens": ["Steigst", "du", "her\u00b7ab", "in", "den", "san\u00b7di\u00b7gen", "Kies", ",", "so", "ge\u00b7wahrst", "du"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "PPER"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.21": {"text": "Niedrig und platt in die Wogen hinaus Trotz bieten der", "tokens": ["Nied\u00b7rig", "und", "platt", "in", "die", "Wo\u00b7gen", "hin\u00b7aus", "Trotz", "bie\u00b7ten", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "APPR", "ART", "NN", "APZR", "NN", "VVFIN", "ART"], "meter": "+--+--+-+-++-+", "measure": "dactylic.di.plus"}, "line.22": {"text": "Dort anlehnt sich mit rundlichem Dach die bescheidene", "tokens": ["Dort", "an\u00b7lehnt", "sich", "mit", "rund\u00b7li\u00b7chem", "Dach", "die", "be\u00b7schei\u00b7de\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ADJA", "NN", "ART", "ADJA"], "meter": "-+---+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "D\u00fcrftiger Fischer, es ist die entlegenste H\u00fctte der Insel,", "tokens": ["D\u00fcrf\u00b7ti\u00b7ger", "Fi\u00b7scher", ",", "es", "ist", "die", "ent\u00b7le\u00b7gens\u00b7te", "H\u00fct\u00b7te", "der", "In\u00b7sel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+--+-+-+-+-+-+", "measure": "dactylic.di.plus"}, "line.24": {"text": "Blos durch riesige Steine besch\u00fczt vor st\u00fcrmischem An-", "tokens": ["Blos", "durch", "rie\u00b7si\u00b7ge", "Stei\u00b7ne", "be\u00b7sch\u00fczt", "vor", "st\u00fcr\u00b7mi\u00b7schem", "An"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "TRUNC"], "meter": "+-+--+--+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Der oft \u00fcber den Sand wegsp\u00fchlt und die Schwelle be-", "tokens": ["Der", "oft", "\u00fc\u00b7ber", "den", "Sand", "weg\u00b7sp\u00fchlt", "und", "die", "Schwel\u00b7le", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VVPP", "KON", "ART", "NN", "TRUNC"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.26": {"text": "Kaum hegt, irgend umher, einfachere Menschen die", "tokens": ["Kaum", "hegt", ",", "ir\u00b7gend", "um\u00b7her", ",", "ein\u00b7fa\u00b7che\u00b7re", "Men\u00b7schen", "die"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "PTKVZ", "$,", "ADJA", "NN", "ART"], "meter": "-+---+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Ja kaum hegt sie sie noch, es ern\u00e4hrt sie die sch\u00e4umende", "tokens": ["Ja", "kaum", "hegt", "sie", "sie", "noch", ",", "es", "er\u00b7n\u00e4hrt", "sie", "die", "sch\u00e4u\u00b7men\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "PPER", "VVFIN", "PPER", "ART", "ADJA"], "meter": "--+--+--+--+--", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Nicht die Gefilde der Insel bewohnt dies arme Geschlecht,", "tokens": ["Nicht", "die", "Ge\u00b7fil\u00b7de", "der", "In\u00b7sel", "be\u00b7wohnt", "dies", "ar\u00b7me", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "PDS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.29": {"text": "Pfl\u00fcckt es des Oelbaums Frucht, nie schlummert es un-", "tokens": ["Pfl\u00fcckt", "es", "des", "O\u00b7el\u00b7baums", "Frucht", ",", "nie", "schlum\u00b7mert", "es", "un"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$,", "ADV", "VVFIN", "PPER", "TRUNC"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.30": {"text": "Nur die verwilderte Myrte noch bl\u00fcht und der wuchernde", "tokens": ["Nur", "die", "ver\u00b7wil\u00b7der\u00b7te", "Myr\u00b7te", "noch", "bl\u00fcht", "und", "der", "wu\u00b7chern\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "ADV", "VVFIN", "KON", "ART", "NN"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.31": {"text": "Aus unwirthlichem Stein, nur wenige Blumen und", "tokens": ["Aus", "un\u00b7wirth\u00b7li\u00b7chem", "Stein", ",", "nur", "we\u00b7ni\u00b7ge", "Blu\u00b7men", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "PIAT", "NN", "KON"], "meter": "--+--+-+--+-+", "measure": "anapaest.di.plus"}, "line.32": {"text": "Eher verwandt ist hier dem gewaltigen Schaumelemente", "tokens": ["E\u00b7her", "ver\u00b7wandt", "ist", "hier", "dem", "ge\u00b7wal\u00b7ti\u00b7gen", "Schau\u00b7me\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+-+--+--+-+--", "measure": "iambic.hexa.invert"}, "line.33": {"text": "Als der beackerten Scholle der Mensch und dem \u00fcppigen", "tokens": ["Als", "der", "be\u00b7ac\u00b7ker\u00b7ten", "Schol\u00b7le", "der", "Mensch", "und", "dem", "\u00fcp\u00b7pi\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "ADJA"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.34": {"text": "Gleiches Gesch\u00e4ft erbt stets von dem heutigen Tage der", "tokens": ["Glei\u00b7ches", "Ge\u00b7sch\u00e4ft", "erbt", "stets", "von", "dem", "heu\u00b7ti\u00b7gen", "Ta\u00b7ge", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "ART"], "meter": "+--+-+--+--+-+", "measure": "iambic.hexa.invert"}, "line.35": {"text": "Immer das Netz auswerfen, es einziehn; wieder es", "tokens": ["Im\u00b7mer", "das", "Netz", "aus\u00b7wer\u00b7fen", ",", "es", "ein\u00b7ziehn", ";", "wie\u00b7der", "es"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "$,", "PPER", "VVINF", "$.", "ADV", "PPER"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.36": {"text": "Ueber dem sonnigen Kies, dann wieder es werfen und", "tokens": ["Ue\u00b7ber", "dem", "son\u00b7ni\u00b7gen", "Kies", ",", "dann", "wie\u00b7der", "es", "wer\u00b7fen", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADV", "PPER", "VVINF", "KON"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.37": {"text": "Hier hat fr\u00fche der Knabe versucht in der Welle zu pl\u00e4t-", "tokens": ["Hier", "hat", "fr\u00fc\u00b7he", "der", "Kna\u00b7be", "ver\u00b7sucht", "in", "der", "Wel\u00b7le", "zu", "pl\u00e4t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "ART", "NN", "VVPP", "APPR", "ART", "NN", "APPR", "TRUNC"], "meter": "--+--+--+--+--+", "measure": "anapaest.tetra.plus"}, "line.38": {"text": "Fr\u00fche das Steuer zu drehen gelernt und die Ruder zu", "tokens": ["Fr\u00fc\u00b7he", "das", "Steu\u00b7er", "zu", "dre\u00b7hen", "ge\u00b7lernt", "und", "die", "Ru\u00b7der", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "VVPP", "KON", "ART", "NN", "PTKZU"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.39": {"text": "Hat als Kind muthwillig gestreichelt den rollenden Del-", "tokens": ["Hat", "als", "Kind", "mut\u00b7hwil\u00b7lig", "ge\u00b7strei\u00b7chelt", "den", "rol\u00b7len\u00b7den", "Del"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "KOUS", "NN", "ADJD", "VVPP", "ART", "ADJA", "TRUNC"], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.40": {"text": "Der, durch T\u00f6ne gelockt, an die Barke heran sich w\u00e4lzte.", "tokens": ["Der", ",", "durch", "T\u00f6\u00b7ne", "ge\