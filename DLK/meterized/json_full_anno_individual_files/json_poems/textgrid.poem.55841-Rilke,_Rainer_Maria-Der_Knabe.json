{"textgrid.poem.55841": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Der Knabe", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte einer werden so wie die,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "ei\u00b7ner", "wer\u00b7den", "so", "wie", "die", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIS", "VAFIN", "ADV", "KOKOM", "ART", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die durch die Nacht mit wilden Pferden fahren,", "tokens": ["die", "durch", "die", "Nacht", "mit", "wil\u00b7den", "Pfer\u00b7den", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mit Fackeln, die gleich aufgegangnen Haaren", "tokens": ["mit", "Fa\u00b7ckeln", ",", "die", "gleich", "auf\u00b7ge\u00b7gang\u00b7nen", "Haa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "in ihres Jagens gro\u00dfem Winde wehn.", "tokens": ["in", "ih\u00b7res", "Ja\u00b7gens", "gro\u00b7\u00dfem", "Win\u00b7de", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vorn m\u00f6cht ich stehen wie in einem Kahne,", "tokens": ["Vorn", "m\u00f6cht", "ich", "ste\u00b7hen", "wie", "in", "ei\u00b7nem", "Kah\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "wo