{"textgrid.poem.31028": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "142.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Tod, du schwartzer Tod, du Schauer unsrer Sinnen!", "tokens": ["O", "Tod", ",", "du", "schwart\u00b7zer", "Tod", ",", "du", "Schau\u00b7er", "uns\u00b7rer", "Sin\u00b7nen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "NE", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O, thu ich dir zu viel? Ja, ja; du kanst gewinnen", "tokens": ["O", ",", "thu", "ich", "dir", "zu", "viel", "?", "Ja", ",", "ja", ";", "du", "kanst", "ge\u00b7win\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "APPR", "PIS", "$.", "PTKANT", "$,", "ADV", "$.", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein englisches Gesicht. Dann du bists, der erfreut;", "tokens": ["Ein", "eng\u00b7li\u00b7sches", "Ge\u00b7sicht", ".", "Dann", "du", "bists", ",", "der", "er\u00b7freut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "PPER", "ADV", "$,", "PRELS", "ADJD", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Du bists, der uns entzeucht dem Toben toller Zeit;", "tokens": ["Du", "bists", ",", "der", "uns", "ent\u00b7zeucht", "dem", "To\u00b7ben", "tol\u00b7ler", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word",