{"textgrid.poem.62321": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Gebet", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, Gott, der du die Welten lenkest,", "tokens": ["O", ",", "Gott", ",", "der", "du", "die", "Wel\u00b7ten", "len\u00b7kest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der du sie schufst und sie erh\u00e4ltst,", "tokens": ["Der", "du", "sie", "schufst", "und", "sie", "er\u00b7h\u00e4ltst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O, Gott, der du die Menschen und die Tiere schufst,", "tokens": ["O", ",", "Gott", ",", "der", "du", "die", "Men\u00b7schen", "und", "die", "Tie\u00b7re", "schufst", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "PRELS", "PPER", "ART", "NN", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der du sie schufst und erh\u00e4ltst,", "tokens": ["Der", "du", "sie", "schufst", "und", "er\u00b7h\u00e4ltst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Erhalte, errette und sch\u00fctze alle,", "tokens": ["Er\u00b7hal\u00b7te", ",", "er\u00b7ret\u00b7te", "und", "sch\u00fct\u00b7ze", "al\u00b7le", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PIS", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Die mir im Herzen wohnen.", "tokens": ["Die", "mir", "im", "Her\u00b7zen", "woh\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Begl\u00fccke ganz und jeden, errette, erleuchte Alle,", "tokens": ["Be\u00b7gl\u00fc\u00b7cke", "ganz", "und", "je\u00b7den", ",", "er\u00b7ret\u00b7te", ",", "er\u00b7leuch\u00b7te", "Al\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "KON", "PIAT", "$,", "VVFIN", "$,", "VVFIN", "PIS", "$,"], "meter": "-+-+-+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Da\u00df sie von ihren Irrt\u00fcmern geheilt.", "tokens": ["Da\u00df", "sie", "von", "ih\u00b7ren", "Irr\u00b7t\u00fc\u00b7mern", "ge\u00b7heilt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Ja begl\u00fccke, erhebe und vergebe ihnen alles,", "tokens": ["Ja", "be\u00b7gl\u00fc\u00b7cke", ",", "er\u00b7he\u00b7be", "und", "ver\u00b7ge\u00b7be", "ih\u00b7nen", "al\u00b7les", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "PPER", "PIS", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "Was sie aus Irrtum getan.", "tokens": ["Was", "sie", "aus", "Irr\u00b7tum", "ge\u00b7tan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "VVPP", "$."], "meter": "-+-+--+"