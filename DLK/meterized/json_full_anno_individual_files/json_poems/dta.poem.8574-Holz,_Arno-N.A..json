{"dta.poem.8574": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1899", "urn": "urn:nbn:de:kobv:b4-200905192690", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Er kann kein Vogelgezwitscher vertragen.", "tokens": ["Er", "kann", "kein", "Vo\u00b7gel\u00b7ge\u00b7zwit\u00b7scher", "ver\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Die sogenannten Naturlaute der Nachtigallen und Lerchen", "tokens": ["Die", "so\u00b7ge\u00b7nann\u00b7ten", "Na\u00b7tur\u00b7lau\u00b7te", "der", "Nach\u00b7ti\u00b7gal\u00b7len", "und", "Ler\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "KON", "NN"], "meter": "-+-+--++--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "sind ihm zuwider.", "tokens": ["sind", "ihm", "zu\u00b7wi\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Sein Hirn", "tokens": ["Sein", "Hirn"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "ist vollst\u00e4ndig mit Watte tapeziert.", "tokens": ["ist", "voll\u00b7st\u00e4n\u00b7dig", "mit", "Wat\u00b7te", "ta\u00b7pe\u00b7ziert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "In der Mitte", "tokens": ["In", "der", "Mit\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": 