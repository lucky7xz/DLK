{"textgrid.poem.62466": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als wir zum Schlagen r\u00fcckten", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als wir zum Schlagen r\u00fcckten", "tokens": ["Als", "wir", "zum", "Schla\u00b7gen", "r\u00fcck\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und nun die Stadt erblickten,", "tokens": ["Und", "nun", "die", "Stadt", "er\u00b7blick\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bei der man k\u00fcnftig schw\u00f6rt;", "tokens": ["Bei", "der", "man", "k\u00fcnf\u00b7tig", "schw\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da dachten wir im Herzen", "tokens": ["Da", "dach\u00b7ten", "wir", "im", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auch an den zweiten Mai,", "tokens": ["Auch", "an", "den", "zwei\u00b7ten", "Mai", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und deine Wittwenschmerzen", "tokens": ["Und", "dei\u00b7ne", "Witt\u00b7wensc\u00b7hmer\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erwachten in uns neu.", "tokens": ["Er\u00b7wach\u00b7ten", "in", "uns", "neu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wir sahen Wolken schwimmen,", "tokens": ["Wir", "sa\u00b7hen", "Wol\u00b7ken", "schwim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wir h\u00f6rten Geisterstimmen", "tokens": ["Wir", "h\u00f6r\u00b7ten", "Geis\u00b7ters\u00b7tim\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vom nahen L\u00fctzenheer.", "tokens": ["Vom", "na\u00b7hen", "L\u00fct\u00b7zen\u00b7heer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die Donner Gottes klangen", "tokens": ["Die", "Don\u00b7ner", "Got\u00b7tes", "klan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In Ost, S\u00fcd, Nord und West,", "tokens": ["In", "Ost", ",", "S\u00fcd", ",", "Nord", "und", "West", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da haben wir begangen", "tokens": ["Da", "ha\u00b7ben", "wir", "be\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Der Br\u00fcder Leichenfest.", "tokens": ["Der", "Br\u00fc\u00b7der", "Lei\u00b7chen\u00b7fest", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In jenen hehren Tagen", "tokens": ["In", "je\u00b7nen", "heh\u00b7ren", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hat mich, gleich milden Sagen,", "tokens": ["Hat", "mich", ",", "gleich", "mil\u00b7den", "Sa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Dein liebes Bild umschwebt.", "tokens": ["Dein", "lie\u00b7bes", "Bild", "um\u00b7schwebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich sah dein Antlitz scheinen,", "tokens": ["Ich", "sah", "dein", "Ant\u00b7litz", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein leuchtendes Panier,", "tokens": ["Ein", "leuch\u00b7ten\u00b7des", "Pa\u00b7nier", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vernahm dein stilles Weinen", "tokens": ["Ver\u00b7nahm", "dein", "stil\u00b7les", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Um jenes Schlachtrevier.", "tokens": ["Um", "je\u00b7nes", "Schlach\u00b7tre\u00b7vier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich mu\u00df dich immer denken,", "tokens": ["Ich", "mu\u00df", "dich", "im\u00b7mer", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00df immer mich versenken", "tokens": ["Mu\u00df", "im\u00b7mer", "mich", "ver\u00b7sen\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In diesen Zaubersee.", "tokens": ["In", "die\u00b7sen", "Zau\u00b7ber\u00b7see", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich kann dich nicht verlieren,", "tokens": ["Ich", "kann", "dich", "nicht", "ver\u00b7lie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du Bild aus holder Zeit,", "tokens": ["Du", "Bild", "aus", "hol\u00b7der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und Myrthen seh' ich zieren", "tokens": ["Und", "Myr\u00b7then", "seh'", "ich", "zie\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auch noch dein Wittwenkleid.", "tokens": ["Auch", "noch", "dein", "Witt\u00b7wen\u00b7kleid", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So mu\u00df es sich begeben,", "tokens": ["So", "mu\u00df", "es", "sich", "be\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In diesem Erdenleben", "tokens": ["In", "die\u00b7sem", "Er\u00b7den\u00b7le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bl\u00fcht Sch\u00f6nheit aus der Qual.", "tokens": ["Bl\u00fcht", "Sch\u00f6n\u00b7heit", "aus", "der", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So hat es Gott erkoren,", "tokens": ["So", "hat", "es", "Gott", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der ewig treue Hirt,", "tokens": ["Der", "e\u00b7wig", "treu\u00b7e", "Hirt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df nur im Weh geboren", "tokens": ["Da\u00df", "nur", "im", "Weh", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein Himmelsb\u00fcrger wird.", "tokens": ["Ein", "Him\u00b7mels\u00b7b\u00fcr\u00b7ger", "wird", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "In Schauern sich gestalten,", "tokens": ["In", "Schau\u00b7ern", "sich", "ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Schmerzen sich entfalten,", "tokens": ["In", "Schmer\u00b7zen", "sich", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df jedes Lebens Keim.", "tokens": ["Mu\u00df", "je\u00b7des", "Le\u00b7bens", "Keim", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So wurden in den Tagen", "tokens": ["So", "wur\u00b7den", "in", "den", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der ersten Christenheit", "tokens": ["Der", "ers\u00b7ten", "Chris\u00b7ten\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Durch Bu\u00dfe, Schmerz und Plagen", "tokens": ["Durch", "Bu\u00b7\u00dfe", ",", "Schmerz", "und", "Pla\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die M\u00e4rtyrer geweiht.", "tokens": ["Die", "M\u00e4r\u00b7ty\u00b7rer", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Und was wir jetzt erstehen", "tokens": ["Und", "was", "wir", "jetzt", "er\u00b7ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus tiefer Gruft gesehen", "tokens": ["Aus", "tie\u00b7fer", "Gruft", "ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im frischen Jugendglanz \u2013", "tokens": ["Im", "fri\u00b7schen", "Ju\u00b7gend\u00b7glanz", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es war mit Schmach beladen", "tokens": ["Es", "war", "mit", "Schmach", "be\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das werthe Vaterland,", "tokens": ["Das", "wert\u00b7he", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Da winkte Gott in Gnaden", "tokens": ["Da", "wink\u00b7te", "Gott", "in", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und w\u00e4hlte unsre Hand.", "tokens": ["Und", "w\u00e4hl\u00b7te", "uns\u00b7re", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ein Wunder war's zu sehen,", "tokens": ["Ein", "Wun\u00b7der", "wa\u00b7r's", "zu", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie er im Sturmeswehen", "tokens": ["Wie", "er", "im", "Stur\u00b7mes\u00b7we\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Durch unsre L\u00e4nder fuhr,", "tokens": ["Durch", "uns\u00b7re", "L\u00e4n\u00b7der", "fuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und alle Herzen brannten,", "tokens": ["Und", "al\u00b7le", "Her\u00b7zen", "brann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den ersten Pfingsten gleich,", "tokens": ["Den", "ers\u00b7ten", "Pfings\u00b7ten", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht Rast noch Ruhe kannten,", "tokens": ["Nicht", "Rast", "noch", "Ru\u00b7he", "kann\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Zu streiten f\u00fcr sein Reich.", "tokens": ["Zu", "strei\u00b7ten", "f\u00fcr", "sein", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Auch du hast ihm gegeben", "tokens": ["Auch", "du", "hast", "ihm", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dein s\u00fc\u00dfes Bl\u00fctenleben", "tokens": ["Dein", "s\u00fc\u00b7\u00dfes", "Bl\u00fc\u00b7ten\u00b7le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In stiller Opferung;", "tokens": ["In", "stil\u00b7ler", "Op\u00b7fe\u00b7rung", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Auch deinen Namen melden", "tokens": ["Auch", "dei\u00b7nen", "Na\u00b7men", "mel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soll man zu Gottes Ehr',", "tokens": ["Soll", "man", "zu", "Got\u00b7tes", "Ehr'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Von allen seinen Helden", "tokens": ["Von", "al\u00b7len", "sei\u00b7nen", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Gab keiner ihm wol mehr.", "tokens": ["Gab", "kei\u00b7ner", "ihm", "wol", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wie freundlich ist sein Wille,", "tokens": ["Wie", "freund\u00b7lich", "ist", "sein", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Trostes hat er F\u00fclle", "tokens": ["Des", "Tros\u00b7tes", "hat", "er", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr jede Menschenbrust,", "tokens": ["F\u00fcr", "je\u00b7de", "Men\u00b7schen\u00b7brust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und was er hier mu\u00df nehmen,", "tokens": ["Und", "was", "er", "hier", "mu\u00df", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er bringt es reichlich ein,", "tokens": ["Er", "bringt", "es", "reich\u00b7lich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Drum soll der Christen Gr\u00e4men", "tokens": ["Drum", "soll", "der", "Chris\u00b7ten", "Gr\u00e4\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Noch stets voll Freude sein.", "tokens": ["Noch", "stets", "voll", "Freu\u00b7de", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die wei\u00dfe Himmelsrose,", "tokens": ["Die", "wei\u00b7\u00dfe", "Him\u00b7mels\u00b7ro\u00b7se", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Mutter, der im Schoo\u00dfe", "tokens": ["Die", "Mut\u00b7ter", ",", "der", "im", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Erbla\u00dft der Heiland lag,", "tokens": ["Er\u00b7bla\u00dft", "der", "Hei\u00b7land", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Maria schaut mit L\u00e4cheln,", "tokens": ["Ma\u00b7ria", "schaut", "mit", "L\u00e4\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf dich und Wilhelms Kind,", "tokens": ["Auf", "dich", "und", "Wil\u00b7helms", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "NE", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Und ihre Engel f\u00e4cheln", "tokens": ["Und", "ih\u00b7re", "En\u00b7gel", "f\u00e4\u00b7cheln"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Dir Tr\u00f6stung k\u00fchl und lind.", "tokens": ["Dir", "Tr\u00f6s\u00b7tung", "k\u00fchl", "und", "lind", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Blick' auf, du Vielbetr\u00fcbte,", "tokens": ["Blick'", "auf", ",", "du", "Viel\u00b7be\u00b7tr\u00fcb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPER", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sei fr\u00f6hlich, Gottgeliebte,", "tokens": ["Sei", "fr\u00f6h\u00b7lich", ",", "Gott\u00b7ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie strahlt dein Liebesstern!", "tokens": ["Wie", "strahlt", "dein", "Lie\u00b7bess\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.4": {"text": "Mit Hel