{"textgrid.poem.26425": {"metadata": {"author": {"name": "Whitman, Walt", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als ich m\u00fchselig wanderte durch Virginias W\u00e4lder", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ich m\u00fchselig wanderte durch Virginias W\u00e4lder", "tokens": ["Als", "ich", "m\u00fch\u00b7se\u00b7lig", "wan\u00b7der\u00b7te", "durch", "Vir\u00b7gi\u00b7ni\u00b7as", "W\u00e4l\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "APPR", "NE", "NN"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Zur Musik der raschelnden Bl\u00e4tter, die mein Fu\u00df aufstie\u00df, denn es war Herbstzeit,", "tokens": ["Zur", "Mu\u00b7sik", "der", "ra\u00b7scheln\u00b7den", "Bl\u00e4t\u00b7ter", ",", "die", "mein", "Fu\u00df", "auf\u00b7stie\u00df", ",", "denn", "es", "war", "Herbst\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,", "KON", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+--+--+-", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Fand ich am Fu\u00df eines Baumes das Grab eines Soldaten \u2013", "tokens": ["Fand", "ich", "am", "Fu\u00df", "ei\u00b7nes", "Bau\u00b7mes", "das", "Grab", "ei\u00b7nes", "Sol\u00b7da\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+--+--+---+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "T\u00f6dlich getroffen und beim R\u00fcckzug bestattet \u2013 leicht konnt i