_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "$,", "ADJD", "KON", "ADJD", "KON", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Lang, lang blieb ich sinnend, dann ging ich wandernd des Weges.", "tokens": ["Lang", ",", "lang", "blieb", "ich", "sin\u00b7nend", ",", "dann", "ging", "ich", "wan\u00b7dernd", "des", "We\u00b7ges", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Manch wechselnde Jahreszeit folgte und manche Scene des Lebens,", "tokens": ["Manch", "wech\u00b7seln\u00b7de", "Jah\u00b7res\u00b7zeit", "folg\u00b7te", "und", "man\u00b7che", "Sce\u00b7ne", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "KON", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+--+--+--+-+--+-", "measure": "amphibrach.tetra.plus"}, "line.3": {"text": "Aber zuweilen inmitten der wechselnden Zeiten und Bilder,", "tokens": ["A\u00b7ber", "zu\u00b7wei\u00b7len", "in\u00b7mit\u00b7ten", "der", "wech\u00b7seln\u00b7den", "Zei\u00b7ten", "und", "Bil\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Pl\u00f6tzlich, einsam oder im Gedr\u00e4nge der Stra\u00dfen", "tokens": ["Pl\u00f6tz\u00b7lich", ",", "ein\u00b7sam", "o\u00b7der", "im", "Ge\u00b7dr\u00e4n\u00b7ge", "der", "Stra\u00b7\u00dfen"], "token_info": ["word", "punct", "word",