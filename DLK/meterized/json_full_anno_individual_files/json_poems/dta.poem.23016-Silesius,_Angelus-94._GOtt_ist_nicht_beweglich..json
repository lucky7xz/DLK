{"dta.poem.23016": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "94. GOtt ist nicht beweglich.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Wer saget da\u00df sich Gott vom S\u00fcnder abewendt/", "tokens": ["Wer", "sa\u00b7get", "da\u00df", "sich", "Gott", "vom", "S\u00fcn\u00b7der", "a\u00b7be\u00b7wendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "PRF", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der giebet klar an Tag da\u00df er Gott noch nicht kennt.", "tokens": ["Der", "gie\u00b7bet", "klar", "an", "Tag", "da\u00df", "er", "Gott", "noch", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "APPR", "NN", "KOUS", "PPER", "NN", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Merke. Gott wendet sich nicht ab/ sondern der S\u00fcn-", "tokens": ["Mer\u00b7ke", ".", "Gott", "wen\u00b7det", "sich", "nicht", "ab", "/", "son\u00b7dern", "der", "S\u00fcn"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NN", "VVFIN", "PRF", "PTKNEG", "PTKVZ", "$(", "KON", "ART", "TRUNC"], "meter": "+--+----+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "der wendt sich von Gott.", "tokens": ["der", "wendt", "sich", "von", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}