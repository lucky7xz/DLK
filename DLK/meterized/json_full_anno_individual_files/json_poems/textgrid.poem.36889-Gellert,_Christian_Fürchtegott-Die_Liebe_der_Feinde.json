{"textgrid.poem.36889": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe der Feinde", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nie will ich dem zu schaden suchen,", "tokens": ["Nie", "will", "ich", "dem", "zu", "scha\u00b7den", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir zu schaden sucht.", "tokens": ["Der", "mir", "zu", "scha\u00b7den", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie will ich meinem Feinde fluchen,", "tokens": ["Nie", "will", "ich", "mei\u00b7nem", "Fein\u00b7de", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er aus Ha\u00df mir flucht.", "tokens": ["Wenn", "er", "aus", "Ha\u00df", "mir", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit G\u00fcte will ich ihm begegnen,", "tokens": ["Mit", "G\u00fc\u00b7te", "will", "ich", "ihm", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht drohen, wenn er droht.", "tokens": ["Nicht", "dro\u00b7hen", ",", "wenn", "er", "droht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er mich schilt, will ich ihn segnen;", "tokens": ["Wenn", "er", "mich", "schilt", ",", "will", "ich", "ihn", "seg\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies ist des Herrn Gebot.", "tokens": ["Dies", "ist", "des", "Herrn", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Er, der von keiner S\u00fcnde wu\u00dfte,", "tokens": ["Er", ",", "der", "von", "kei\u00b7ner", "S\u00fcn\u00b7de", "wu\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergalt die Schmach mit Huld,", "tokens": ["Ver\u00b7galt", "die", "Schmach", "mit", "Huld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und litt, so viel er leiden mu\u00dfte,", "tokens": ["Und", "litt", ",", "so", "viel", "er", "lei\u00b7den", "mu\u00df\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Sanftmut und Geduld.", "tokens": ["Mit", "Sanft\u00b7mut", "und", "Ge\u00b7duld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Will ich, sein J\u00fcnger, widerschelten,", "tokens": ["Will", "ich", ",", "sein", "J\u00fcn\u00b7ger", ",", "wi\u00b7der\u00b7schel\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da er nicht widerschalt?", "tokens": ["Da", "er", "nicht", "wi\u00b7der\u00b7schalt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit Liebe nicht den Ha\u00df vergelten,", "tokens": ["Mit", "Lie\u00b7be", "nicht", "den", "Ha\u00df", "ver\u00b7gel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie er den Ha\u00df vergalt?", "tokens": ["Wie", "er", "den", "Ha\u00df", "ver\u00b7galt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wahr ist's, Verleumdung dulden m\u00fcssen,", "tokens": ["Wahr", "ist's", ",", "Ver\u00b7leum\u00b7dung", "dul\u00b7den", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist eine schwere Pflicht.", "tokens": ["Ist", "ei\u00b7ne", "schwe\u00b7re", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch selig, wenn ein gut Gewissen", "tokens": ["Doch", "se\u00b7lig", ",", "wenn", "ein", "gut", "Ge\u00b7wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KOUS", "ART", "ADJD", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Zu unsrer Ehre spricht!", "tokens": ["Zu", "uns\u00b7rer", "Eh\u00b7re", "spricht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Dies will ich desto mehr bewahren;", "tokens": ["Dies", "will", "ich", "des\u00b7to", "mehr", "be\u00b7wah\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So bessert mich mein Feind,", "tokens": ["So", "bes\u00b7sert", "mich", "mein", "Feind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lehrt mich, weiser nur verfahren,", "tokens": ["Und", "lehrt", "mich", ",", "wei\u00b7ser", "nur", "ver\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PRELS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Indem er's b\u00f6se meint.", "tokens": ["In\u00b7dem", "er's", "b\u00f6\u00b7se", "meint", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich will mich vor den Fehlern h\u00fcten,", "tokens": ["Ich", "will", "mich", "vor", "den", "Feh\u00b7lern", "h\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die er von mir ersann;", "tokens": ["Die", "er", "von", "mir", "er\u00b7sann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und auch die Fehler mir verbieten,", "tokens": ["Und", "auch", "die", "Feh\u00b7ler", "mir", "ver\u00b7bie\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die er nicht wissen kann.", "tokens": ["Die", "er", "nicht", "wis\u00b7sen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "So will ich mich durch Sanftmut r\u00e4chen,", "tokens": ["So", "will", "ich", "mich", "durch", "Sanft\u00b7mut", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "An ihm das Gute sehn,", "tokens": ["An", "ihm", "das", "Gu\u00b7te", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dieses Gute von ihm sprechen;", "tokens": ["Und", "die\u00b7ses", "Gu\u00b7te", "von", "ihm", "spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie k\u00f6nnt er l\u00e4nger schm\u00e4hn!", "tokens": ["Wie", "k\u00f6nnt", "er", "l\u00e4n\u00b7ger", "schm\u00e4hn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "In seinem Ha\u00df ihn zu erm\u00fcden,", "tokens": ["In", "sei\u00b7nem", "Ha\u00df", "ihn", "zu", "er\u00b7m\u00fc\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Will ich ihm gern verzeihn,", "tokens": ["Will", "ich", "ihm", "gern", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und als ein Christ bereit zum Frieden,", "tokens": ["Und", "als", "ein", "Christ", "be\u00b7reit", "zum", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bereit zu Diensten sein.", "tokens": ["Be\u00b7reit", "zu", "Diens\u00b7ten", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und wird er, mich zu untertreten,", "tokens": ["Und", "wird", "er", ",", "mich", "zu", "un\u00b7ter\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch G\u00fcte mehr erhitzt:", "tokens": ["Durch", "G\u00fc\u00b7te", "mehr", "er\u00b7hitzt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Will ich im stillen f\u00fcr ihn beten,", "tokens": ["Will", "ich", "im", "stil\u00b7len", "f\u00fcr", "ihn", "be\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gott vertraun; Gott sch\u00fctzt.", "tokens": ["Und", "Gott", "ver\u00b7traun", ";", "Gott", "sch\u00fctzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "$.", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Nie will ich dem zu schaden suchen,", "tokens": ["Nie", "will", "ich", "dem", "zu", "scha\u00b7den", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ART", "PTKZU", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der mir zu schaden sucht.", "tokens": ["Der", "mir", "zu", "scha\u00b7den", "sucht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nie will ich meinem Feinde fluchen,", "tokens": ["Nie", "will", "ich", "mei\u00b7nem", "Fein\u00b7de", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er aus Ha\u00df mir flucht.", "tokens": ["Wenn", "er", "aus", "Ha\u00df", "mir", "flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mit G\u00fcte will ich ihm begegnen,", "tokens": ["Mit", "G\u00fc\u00b7te", "will", "ich", "ihm", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht drohen, wenn er droht.", "tokens": ["Nicht", "dro\u00b7hen", ",", "wenn", "er", "droht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn er mich schilt, will ich ihn segnen;", "tokens": ["Wenn", "er", "mich", "schilt", ",", "will", "ich", "ihn", "seg\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVFIN", "$,", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies ist des Herrn Gebot.", "tokens": ["Dies", "ist", "des", "Herrn", "Ge\u00b7bot", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Er, der von keiner S\u00fcnde wu\u00dfte,", "tokens": ["Er", ",", "der", "von", "kei\u00b7ner", "S\u00fcn\u00b7de", "wu\u00df\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vergalt die Schmach mit Huld,", "toke