{"textgrid.poem.49528": {"metadata": {"author": {"name": "G\u00f6tz, Nicolaus", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Tod eines Freundes", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist Lycidas nicht mehr am Leben?", "tokens": ["Ist", "Ly\u00b7ci\u00b7das", "nicht", "mehr", "am", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt ihn der Himmel hin, der mir ihn doch gegeben?", "tokens": ["Nimmt", "ihn", "der", "Him\u00b7mel", "hin", ",", "der", "mir", "ihn", "doch", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ach! holder Gegenstand von meiner Traurigkeit,", "tokens": ["Ach", "!", "hol\u00b7der", "Ge\u00b7gen\u00b7stand", "von", "mei\u00b7ner", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dir flieh ich willig nach! wo bistu, bistu weit?", "tokens": ["Dir", "flieh", "ich", "wil\u00b7lig", "nach", "!", "wo", "bis\u00b7tu", ",", "bis\u00b7tu", "weit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$.", "PWAV", "NE", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dich, den ich t\u00e4glich sah an meiner Seite gehen,", "tokens": ["Dich", ",", "den", "ich", "t\u00e4g\u00b7lich", "sah", "an", "mei\u00b7ner", "Sei\u00b7te", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der meine Lust bey Nacht,", "tokens": ["Der", "mei\u00b7ne", "Lust", "bey", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Gl\u00fcck bey Tag gemacht,", "tokens": ["Mein", "Gl\u00fcck", "bey", "Tag", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Soll ich auf ewig nicht mehr sehen!", "tokens": ["Soll", "ich", "auf", "e\u00b7wig", "nicht", "mehr", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADJD", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Gerechte G\u00f6tter! \u2013 \u2013 Wie? \u2013 \u2013 ich soll nicht bey ihm seyn,", "tokens": ["Ge\u00b7rech\u00b7te", "G\u00f6t\u00b7ter", "!", "\u2013", "\u2013", "Wie", "?", "\u2013", "\u2013", "ich", "soll", "nicht", "bey", "ihm", "seyn", ","], "token_info": ["word", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "$(", "$(", "PWAV", "$.", "$(", "$(", "PPER", "VMFIN", "PTKNEG", "APPR", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Als durch ein traurigs Angedencken?", "tokens": ["Als", "durch", "ein", "trau\u00b7rigs", "An\u00b7ge\u00b7den\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Wi\u00dft, St\u00f6rer meiner Ruh'; ihr k\u00f6nnet ihn versencken,", "tokens": ["Wi\u00dft", ",", "St\u00f6\u00b7rer", "mei\u00b7ner", "Ruh'", ";", "ihr", "k\u00f6n\u00b7net", "ihn", "ver\u00b7sen\u00b7cken", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Doch schlie\u00df' ich mich mit ihm in die Verwesung ein.", "tokens": ["Doch", "schlie\u00df'", "ich", "mich", "mit", "ihm", "in", "die", "Ver\u00b7we\u00b7sung", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist Lycidas nicht mehr am Leben?", "tokens": ["Ist", "Ly\u00b7ci\u00b7das", "nicht", "mehr", "am", "Le\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nimmt ihn der Himmel hin, der mir ihn doch gegeben?", "tokens": ["Nimmt", "ihn", "der", "Him\u00b7mel", "hin", ",", "der", "mir", "ihn", "doch", "ge\u00b7ge\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+