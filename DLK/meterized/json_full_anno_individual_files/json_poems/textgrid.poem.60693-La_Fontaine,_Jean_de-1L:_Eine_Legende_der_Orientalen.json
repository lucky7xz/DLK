{"textgrid.poem.60693": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine Legende der Orientalen", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Legende der Orientalen", "tokens": ["Ei\u00b7ne", "Le\u00b7gen\u00b7de", "der", "O\u00b7rien\u00b7ta\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Erz\u00e4hlt von einer gewissen Ratte,", "tokens": ["Er\u00b7z\u00e4hlt", "von", "ei\u00b7ner", "ge\u00b7wis\u00b7sen", "Rat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die sich vor den irdischen Sorgen und Qualen", "tokens": ["Die", "sich", "vor", "den", "ir\u00b7di\u00b7schen", "Sor\u00b7gen", "und", "Qua\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "ART", "ADJA", "NN", "KON", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "In einen Holl\u00e4nder K\u00e4s zur\u00fcckgezogen hatte.", "tokens": ["In", "ei\u00b7nen", "Hol\u00b7l\u00e4n\u00b7der", "K\u00e4s", "zu\u00b7r\u00fcck\u00b7ge\u00b7zo\u00b7gen", "hat\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "VAFIN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Die Einsamkeit", "tokens": ["Die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "War tief und weit", "tokens": ["War", "tief", "und", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "In diesem Wartesaal zum Paradies,", "tokens": ["In", "die\u00b7sem", "War\u00b7te\u00b7saal", "zum", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Darin der Klausner fromm sich niederlie\u00df.", "tokens": ["Da\u00b7rin", "der", "Klaus\u00b7ner", "fromm", "sich", "nie\u00b7der\u00b7lie\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Er schaffte so t\u00fcchtig mit Z\u00e4hnen und F\u00fc\u00dfen,", "tokens": ["Er", "schaff\u00b7te", "so", "t\u00fcch\u00b7tig", "mit", "Z\u00e4h\u00b7nen", "und", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.10": {"text": "Da\u00df er in wenig Tagen schon", "tokens": ["Da\u00df", "er", "in", "we\u00b7nig", "Ta\u00b7gen", "schon"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Neben guter Nahrung \u2013 als Gottes Lohn \u2013", "tokens": ["Ne\u00b7ben", "gu\u00b7ter", "Nah\u00b7rung", "\u2013", "als", "Got\u00b7tes", "Lohn", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KOUS", "NN", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "Eine Unterkunft hatte zum Beten und B\u00fc\u00dfen.", "tokens": ["Ei\u00b7ne", "Un\u00b7ter\u00b7kunft", "hat\u00b7te", "zum", "Be\u00b7ten", "und", "B\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "KON", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Er wurde dick und fett dabei;", "tokens": ["Er", "wur\u00b7de", "dick", "und", "fett", "da\u00b7bei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Denn Gott verschwendet seine Gaben", "tokens": ["Denn", "Gott", "ver\u00b7schwen\u00b7det", "sei\u00b7ne", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "An die, die ihn zum Vater haben.", "tokens": ["An", "die", ",", "die", "ihn", "zum", "Va\u00b7ter", "ha\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Einst klopfte es an die Einsiedelei.", "tokens": ["Einst", "klopf\u00b7te", "es", "an", "die", "Ein\u00b7sie\u00b7de\u00b7lei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Der Heilige fragte, wer es sei?", "tokens": ["Der", "Hei\u00b7li\u00b7ge", "frag\u00b7te", ",", "wer", "es", "sei", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Da hie\u00df es: Boten von den Ratten,", "tokens": ["Da", "hie\u00df", "es", ":", "Bo\u00b7ten", "von", "den", "Rat\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die lie\u00dfen ihn um ein Almosen bitten;", "tokens": ["Die", "lie\u00b7\u00dfen", "ihn", "um", "ein", "Al\u00b7mo\u00b7sen", "bit\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Rattopolis habe arg gelitten,", "tokens": ["Rat\u00b7to\u00b7po\u00b7lis", "ha\u00b7be", "arg", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.21": {"text": "Da die Katzen es rings umschlossen hatten,", "tokens": ["Da", "die", "Kat\u00b7zen", "es", "rings", "um\u00b7schlos\u00b7sen", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP", "VAFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Und immer noch sei die Stadt bedroht,", "tokens": ["Und", "im\u00b7mer", "noch", "sei", "die", "Stadt", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Und es fehle am allern\u00f6tigsten Brot,", "tokens": ["Und", "es", "feh\u00b7le", "am", "al\u00b7ler\u00b7n\u00f6\u00b7tigs\u00b7ten", "Brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.24": {"text": "Und sie b\u00e4ten um etwas \u2013 um Christi willen \u2013", "tokens": ["Und", "sie", "b\u00e4\u00b7ten", "um", "et\u00b7was", "\u2013", "um", "Chris\u00b7ti", "wil\u00b7len", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIS", "$(", "APPR", "NE", "NN", "$("], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}}, "stanza.2": {"line.1": {"text": "Um den Hunger der Schw\u00e4chsten so lange zu stillen,", "tokens": ["Um", "den", "Hun\u00b7ger", "der", "Schw\u00e4chs\u00b7ten", "so", "lan\u00b7ge", "zu", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Bis da\u00df die Marder k\u00e4men, die Entsatz versprochen;", "tokens": ["Bis", "da\u00df", "die", "Mar\u00b7der", "k\u00e4\u00b7men", ",", "die", "Ent\u00b7satz", "ver\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In wenig Tagen sei die schwere Not gebrochen.", "tokens": ["In", "we\u00b7nig", "Ta\u00b7gen", "sei", "die", "schwe\u00b7re", "Not", "ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u00bbmeine Freunde,\u00ab sprach da aus der K\u00e4sezelle", "tokens": ["\u00bb", "mei\u00b7ne", "Freun\u00b7de", ",", "\u00ab", "sprach", "da", "aus", "der", "K\u00e4\u00b7se\u00b7zel\u00b7le"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "$,", "$(", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Die fromme Ratte, \u00bbderartige F\u00e4lle", "tokens": ["Die", "from\u00b7me", "Rat\u00b7te", ",", "\u00bb", "der\u00b7ar\u00b7ti\u00b7ge", "F\u00e4l\u00b7le"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "$(", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und weltlichen Dinge sind mir verha\u00dft.", "tokens": ["Und", "welt\u00b7li\u00b7chen", "Din\u00b7ge", "sind", "mir", "ver\u00b7ha\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+--+-++-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Ich trage in Armut der Bu\u00dfe Last;", "tokens": ["Ich", "tra\u00b7ge", "in", "Ar\u00b7mut", "der", "Bu\u00b7\u00dfe", "Last", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "NN", "NE", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Nichts kann ich euch geben als nur mein Gebet,", "tokens": ["Nichts", "kann", "ich", "euch", "ge\u00b7ben", "als", "nur", "mein", "Ge\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "PPER", "VVINF", "KOKOM", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+---", "measure": "trochaic.tetra.relaxed"}, "line.7": {"text": "Das gern f\u00fcr die Ratten zum Himmel fleht,", "tokens": ["Das", "gern", "f\u00fcr", "die", "Rat\u00b7ten", "zum", "Him\u00b7mel", "fleht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Und ich hoffe, er wird euch nicht vergessen.", "tokens": ["Und", "ich", "hof\u00b7fe", ",", "er", "wird", "euch", "nicht", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Mehr kann ich beim besten Willen nicht tun.\u00ab", "tokens": ["Mehr", "kann", "ich", "beim", "bes\u00b7ten", "Wil\u00b7len", "nicht", "tun", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VMFIN", "PPER", "APPRART", "ADJA", "NN", "PTKNEG", "VVINF", "$.", "$("], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "Und sie zog sich zur\u00fcck, um sich vollzufressen", "tokens": ["Und", "sie", "zog", "sich", "zu\u00b7r\u00fcck", ",", "um", "sich", "voll\u00b7zu\u00b7fres\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PTKVZ", "$,", "KOUI", "PRF", "VVINF"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.11": {"text": "Und hinterher sich auszuruhn.", "tokens": ["Und", "hin\u00b7ter\u00b7her", "sich", "aus\u00b7zu\u00b7ruhn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PRF", "VVIZU", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wem ist wohl das Herz dieser Ratte eigen?", "tokens": ["Wem", "ist", "wohl", "das", "Herz", "die\u00b7ser", "Rat\u00b7te", "ei\u00b7gen", "?"], "toke