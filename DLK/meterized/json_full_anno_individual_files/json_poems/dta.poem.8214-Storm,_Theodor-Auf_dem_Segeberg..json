{"dta.poem.8214": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Segeberg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Die Wiege einer deutschen Frau;", "tokens": ["Die", "Wie\u00b7ge", "ei\u00b7ner", "deut\u00b7schen", "Frau", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schaut mich an mit Augen blau,", "tokens": ["Die", "schaut", "mich", "an", "mit", "Au\u00b7gen", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und auf dem Felsen, drauf ich liege,", "tokens": ["Und", "auf", "dem", "Fel\u00b7sen", ",", "drauf", "ich", "lie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schlie\u00dft sie mich pl\u00f6tzlich an die Brust.", "tokens": ["Schlie\u00dft", "sie", "mich", "pl\u00f6tz\u00b7lich", "an", "die", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da werd' ich mir des Gl\u00fccks bewu\u00dft;", "tokens": ["Da", "werd'", "ich", "mir", "des", "Gl\u00fccks", "be\u00b7wu\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich seh' die Welt so unverg\u00e4nglich,", "tokens": ["Ich", "seh'", "die", "Welt", "so", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Voll Sch\u00f6nheit mir zu F\u00fc\u00dfen ruhn,", "tokens": ["Voll", "Sch\u00f6n\u00b7heit", "mir", "zu", "F\u00fc\u00b7\u00dfen", "ruhn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und alle Sorgen, die so b\u00e4nglich", "tokens": ["Und", "al\u00b7le", "Sor\u00b7gen", ",", "die", "so", "b\u00e4ng\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Herz bedr\u00e4ngten, schweigen nun.", "tokens": ["Mein", "Herz", "be\u00b7dr\u00e4ng\u00b7ten", ",", "schwei\u00b7gen", "nun", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Musik! Musik! Die Lerchen singen,", "tokens": ["Mu\u00b7sik", "!", "Mu\u00b7sik", "!", "Die", "Ler\u00b7chen", "sin\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Aus Wies' und W\u00e4ldern steigt Gesang,", "tokens": ["Aus", "Wies'", "und", "W\u00e4l\u00b7dern", "steigt", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die M\u00fccken in den L\u00fcften schwingen", "tokens": ["Die", "M\u00fc\u00b7cken", "in", "den", "L\u00fcf\u00b7ten", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Den s\u00fc\u00dfen Sommerharfenklang.", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Som\u00b7mer\u00b7har\u00b7fen\u00b7klang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Und unten auf besonnter Flur", "tokens": ["Und", "un\u00b7ten", "auf", "be\u00b7sonn\u00b7ter", "Flur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Seh' ich des Kornes Wellen treiben,", "tokens": ["Seh'", "ich", "des", "Kor\u00b7nes", "Wel\u00b7len", "trei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "In blauen W\u00f6lkchen dr\u00fcber st\u00e4uben", "tokens": ["In", "blau\u00b7en", "W\u00f6lk\u00b7chen", "dr\u00fc\u00b7ber", "st\u00e4u\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PAV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Ein keusch' Geheimni\u00df der Natur. \u2014", "tokens": ["Ein", "keusch'", "Ge\u00b7heim\u00b7ni\u00df", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "PIAT", "NN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Da tauchen an des Berges Seite", "tokens": ["Da", "tau\u00b7chen", "an", "des", "Ber\u00b7ges", "Sei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Zwei K\u00f6pfchen auf aus dem Gestein,", "tokens": ["Zwei", "K\u00f6pf\u00b7chen", "auf", "aus", "dem", "Ge\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Zwei Knaben steigen durch's Gekr\u00e4ute;", "tokens": ["Zwei", "Kna\u00b7ben", "stei\u00b7gen", "durch's", "Ge\u00b7kr\u00e4u\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Und sie sind unser, mein und dein.", "tokens": ["Und", "sie", "sind", "un\u00b7ser", ",", "mein", "und", "dein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPOSAT", "$,", "PPOSAT", "KON", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Sie jauchzen auf, die Felsen klingen;", "tokens": ["Sie", "jauch\u00b7zen", "auf", ",", "die", "Fel\u00b7sen", "klin\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Mein Bursche schlank, mein Bursche klein!", "tokens": ["Mein", "Bur\u00b7sche", "schlank", ",", "mein", "Bur\u00b7sche", "klein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Schau, wie sie purzeln, wie sie springen,", "tokens": ["Schau", ",", "wie", "sie", "pur\u00b7zeln", ",", "wie", "sie", "sprin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "Und Jeder will der Erste sein.", "tokens": ["Und", "Je\u00b7der", "will", "der", "Ers\u00b7te", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "In Kinderlust die Wangen gl\u00fchen;", "tokens": ["In", "Kin\u00b7der\u00b7lust", "die", "Wan\u00b7gen", "gl\u00fc\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.27": {"text": "Die Welt, die Welt, o wie sie lacht!", "tokens": ["Die", "Welt", ",", "die", "Welt", ",", "o", "wie", "sie", "lacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "FM", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Nun h\u00e4ngen sie an deinen Knieen,", "tokens": ["Nun", "h\u00e4n\u00b7gen", "sie", "an", "dei\u00b7nen", "Kni\u00b7e\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.29": {"text": "Nun an den meinen unbedacht.", "tokens": ["Nun", "an", "den", "mei\u00b7nen", "un\u00b7be\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der Gro\u00dfe hier, und hier der Kleine,", "tokens": ["Der", "Gro\u00b7\u00dfe", "hier", ",", "und", "hier", "der", "Klei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "$,", "KON", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Sie halten mich so eng umfa\u00dft,", "tokens": ["Sie", "hal\u00b7ten", "mich", "so", "eng", "um\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Da\u00df in den Thymian der Steine", "tokens": ["Da\u00df", "in", "den", "Thy\u00b7mi\u00b7an", "der", "Stei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NE", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Mich hinzieht die geliebte Last.", "tokens": ["Mich", "hin\u00b7zieht", "die", "ge\u00b7lieb\u00b7te", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.34": {"text": "Die Schatten, die mein Auge tr\u00fcbten,", "tokens": ["Die", "Schat\u00b7ten", ",", "die", "mein", "Au\u00b7ge", "tr\u00fcb\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Die letzten scheucht der Kindermund;", "tokens": ["Die", "letz\u00b7ten", "scheucht", "der", "Kin\u00b7der\u00b7mund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Ich seh' der Heimath, der geliebten,", "tokens": ["Ich", "seh'", "der", "Hei\u00b7math", ",", "der", "ge\u00b7lieb\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.37": {"text": "Zukunft in dieser Augen Grund.", "tokens": ["Zu\u00b7kunft", "in", "die\u00b7ser", "Au\u00b7gen", "Grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}