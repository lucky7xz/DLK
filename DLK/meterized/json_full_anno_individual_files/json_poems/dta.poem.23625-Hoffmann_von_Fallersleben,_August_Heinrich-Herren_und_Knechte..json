{"dta.poem.23625": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Herren und Knechte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr wolltet euch zu G\u00f6ttern machen,", "tokens": ["Ihr", "woll\u00b7tet", "euch", "zu", "G\u00f6t\u00b7tern", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und siehe, das gelang euch schlecht;", "tokens": ["Und", "sie\u00b7he", ",", "das", "ge\u00b7lang", "euch", "schlecht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "PDS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da machtet ihr das Volk der Schwachen", "tokens": ["Da", "mach\u00b7tet", "ihr", "das", "Volk", "der", "Schwa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zu einem dienenden Geschlecht.", "tokens": ["Zu", "ei\u00b7nem", "die\u00b7nen\u00b7den", "Ge\u00b7schlecht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und dies Geschlecht mu\u00df immer b\u00fc\u00dfen,", "tokens": ["Und", "dies", "Ge\u00b7schlecht", "mu\u00df", "im\u00b7mer", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "NN", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu gro\u00df ist seine eigne Schuld,", "tokens": ["Zu", "gro\u00df", "ist", "sei\u00b7ne", "eig\u00b7ne", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wollt ihr's Leben ihm vers\u00fc\u00dfen,", "tokens": ["Und", "wollt", "ih\u00b7r's", "Le\u00b7ben", "ihm", "ver\u00b7s\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So ist es eure Gnad' und Huld.", "tokens": ["So", "ist", "es", "eu\u00b7re", "Gnad'", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da ist die Rede nicht vom Rechte,", "tokens": ["Da", "ist", "die", "Re\u00b7de", "nicht", "vom", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PTKNEG", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das w\u00e4r' auch nur ein toller Wahn:", "tokens": ["Das", "w\u00e4r'", "auch", "nur", "ein", "tol\u00b7ler", "Wahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr seid die Herrn, sie sind die Knechte,", "tokens": ["Ihr", "seid", "die", "Herrn", ",", "sie", "sind", "die", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was ihr thut ist wohlgethan.", "tokens": ["Und", "was", "ihr", "thut", "ist", "wohl\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}