{"dta.poem.5385": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Nutz und Nothwendigkeit der  \n Ueberlegung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich der Felder Schmuck, wenn ich der G\u00e4rten", "tokens": ["Wenn", "ich", "der", "Fel\u00b7der", "Schmuck", ",", "wenn", "ich", "der", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Pracht,", "tokens": ["Pracht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Zusammt der W\u00e4lder Zier, im Sommer sehe;", "tokens": ["Zu\u00b7sammt", "der", "W\u00e4l\u00b7der", "Zier", ",", "im", "Som\u00b7mer", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Gedenck ich an die Winter-Nacht", "tokens": ["Ge\u00b7denck", "ich", "an", "die", "Win\u00b7ter\u00b7Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wie der Mittags-Schein des Jahrs so schnell vergehe;", "tokens": ["Und", "wie", "der", "Mit\u00b7tags\u00b7Schein", "des", "Jahrs", "so", "schnell", "ver\u00b7ge\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ART", "NN", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch zu dem Endzweck nicht,", "tokens": ["Doch", "zu", "dem", "End\u00b7zweck", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "Durch ein zuk\u00fcnftig Leid", "tokens": ["Durch", "ein", "zu\u00b7k\u00fcnf\u00b7tig", "Leid"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die gegenw\u00e4rt\u2019ge Lust zu st\u00f6hren,", "tokens": ["Die", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7ge", "Lust", "zu", "st\u00f6h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Wol aber, durch die Fl\u00fcchtigkeit,", "tokens": ["Wol", "a\u00b7ber", ",", "durch", "die", "Fl\u00fcch\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Noch meine Freude zu vermehren.", "tokens": ["Noch", "mei\u00b7ne", "Freu\u00b7de", "zu", "ver\u00b7meh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Denn, denck ich: dauret es nur kurtze Zeit;", "tokens": ["Denn", ",", "denck", "ich", ":", "dau\u00b7ret", "es", "nur", "kurt\u00b7ze", "Zeit", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PRELS", "PPER", "$.", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Warum la\u00df ich die kurtze Zeit verschwinden?", "tokens": ["Wa\u00b7rum", "la\u00df", "ich", "die", "kurt\u00b7ze", "Zeit", "ver\u00b7schwin\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Warum bestreb\u2019 ich mich nicht, so viel mehr,", "tokens": ["Wa\u00b7rum", "be\u00b7streb'", "ich", "mich", "nicht", ",", "so", "viel", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKNEG", "$,", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So lang als ich es hab\u2019, es \u00f6fter zu empfinden,", "tokens": ["So", "lang", "als", "ich", "es", "hab'", ",", "es", "\u00f6f\u00b7ter", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "PPER", "PPER", "VAFIN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und, durch ein dergestalt oft wiederholt Ergetzen,", "tokens": ["Und", ",", "durch", "ein", "der\u00b7ge\u00b7stalt", "oft", "wie\u00b7der\u00b7holt", "Er\u00b7get\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "ADV", "ADV", "ADJD", "NN", "$,"], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "In l\u00e4ngeren Besitz des guten mich zu setzen?", "tokens": ["In", "l\u00e4n\u00b7ge\u00b7ren", "Be\u00b7sitz", "des", "gu\u00b7ten", "mich", "zu", "set\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Erwegt man oft die Gegenwart des Guten;", "tokens": ["Er\u00b7wegt", "man", "oft", "die", "Ge\u00b7gen\u00b7wart", "des", "Gu\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "So l\u00e4sst sich dadurch gleichsam binden", "tokens": ["So", "l\u00e4sst", "sich", "da\u00b7durch", "gleich\u00b7sam", "bin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PAV", "ADJD", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die rege Schnelligkeit der fl\u00fcchtigen Minuten.", "tokens": ["Die", "re\u00b7ge", "Schnel\u00b7lig\u00b7keit", "der", "fl\u00fcch\u00b7ti\u00b7gen", "Mi\u00b7nu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Gedancken sind es blos allein,", "tokens": ["Ge\u00b7dan\u00b7cken", "sind", "es", "blos", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Wodurch die G\u00fcter dieser Erden", "tokens": ["Wo\u00b7durch", "die", "G\u00fc\u00b7ter", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PDAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Uns zugeeignet werden.", "tokens": ["Uns", "zu\u00b7ge\u00b7eig\u00b7net", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ach, la\u00dft uns denn besch\u00e4ftigt seyn,", "tokens": ["Ach", ",", "la\u00dft", "uns", "denn", "be\u00b7sch\u00e4f\u00b7tigt", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Durch ein in unsre Macht gesetztes Dencken", "tokens": ["Durch", "ein", "in", "uns\u00b7re", "Macht", "ge\u00b7setz\u00b7tes", "Den\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Uns oft viel Guts uns selbst zu schencken!", "tokens": ["Uns", "oft", "viel", "Guts", "uns", "selbst", "zu", "schen\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PIAT", "NN", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}