{"dta.poem.5444": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Allgegenwart des Sch\u00f6pfers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was mich fast aus mir selber setzt, ist von dem Sch\u00f6pfer", "tokens": ["Was", "mich", "fast", "aus", "mir", "sel\u00b7ber", "setzt", ",", "ist", "von", "dem", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$,", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die Jdee,", "tokens": ["die", "Jdee", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Den ich so kenntlich im Gesch\u00f6pf, und, in der Unerm\u00e4\u00dflichkeit", "tokens": ["Den", "ich", "so", "kennt\u00b7lich", "im", "Ge\u00b7sch\u00f6pf", ",", "und", ",", "in", "der", "Un\u00b7er\u00b7m\u00e4\u00df\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPER", "ADV", "ADJD", "APPRART", "NN", "$,", "KON", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "So wunderbar verborgen, sehe.", "tokens": ["So", "wun\u00b7der\u00b7bar", "ver\u00b7bor\u00b7gen", ",", "se\u00b7he", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist derselbige zu gleicher Zeit", "tokens": ["Es", "ist", "der\u00b7sel\u00b7bi\u00b7ge", "zu", "glei\u00b7cher", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Bekannt und unbekannt, verdeckt und klar,", "tokens": ["Be\u00b7kannt", "und", "un\u00b7be\u00b7kannt", ",", "ver\u00b7deckt", "und", "klar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "$,", "VVPP", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verborgen und auch offenbar", "tokens": ["Ver\u00b7bor\u00b7gen", "und", "auch", "of\u00b7fen\u00b7bar"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Doch \u00fcberall Anbethungs-w\u00fcrdig. Er ist uns allen nah", "tokens": ["Doch", "\u00fc\u00b7be\u00b7rall", "An\u00b7bethungs\u00b7w\u00fcr\u00b7dig", ".", "Er", "ist", "uns", "al\u00b7len", "nah"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "$.", "PPER", "VAFIN", "PPER", "PIAT", "NN"], "meter": "-+--+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "und fern;", "tokens": ["und", "fern", ";"], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "Als HErr und Herrscher aller Herr\u2019n,", "tokens": ["Als", "Herr", "und", "Herr\u00b7scher", "al\u00b7ler", "Herr'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Enth\u00e4lt sich seine Gr\u00f6\u00df\u2019 und Vollenkommenheit", "tokens": ["Ent\u00b7h\u00e4lt", "sich", "sei\u00b7ne", "Gr\u00f6\u00df'", "und", "Vol\u00b7len\u00b7kom\u00b7men\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In seiner Unbegreiflichkeit.", "tokens": ["In", "sei\u00b7ner", "Un\u00b7be\u00b7greif\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Er spricht allein mit uns und zeigt uns seine Spur,", "tokens": ["Er", "spricht", "al\u00b7lein", "mit", "uns", "und", "zeigt", "uns", "sei\u00b7ne", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPER", "KON", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durchs Mittel seiner Creatur,", "tokens": ["Durchs", "Mit\u00b7tel", "sei\u00b7ner", "Crea\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Die allenthalben von ihm spricht, und nie von seinen We-", "tokens": ["Die", "al\u00b7len\u00b7thal\u00b7ben", "von", "ihm", "spricht", ",", "und", "nie", "von", "sei\u00b7nen", "We"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "PPER", "VVFIN", "$,", "KON", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.16": {"text": "sen schweiget,", "tokens": ["sen", "schwei\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Die ist der Spiegel seines Wesens, der aller Arten ihn", "tokens": ["Die", "ist", "der", "Spie\u00b7gel", "sei\u00b7nes", "We\u00b7sens", ",", "der", "al\u00b7ler", "Ar\u00b7ten", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$,", "PRELS", "PIAT", "NN", "PPER"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "uns zeiget.", "tokens": ["uns", "zei\u00b7get", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}