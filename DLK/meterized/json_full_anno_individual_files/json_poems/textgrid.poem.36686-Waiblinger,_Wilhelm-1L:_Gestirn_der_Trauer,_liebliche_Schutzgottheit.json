{"textgrid.poem.36686": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gestirn der Trauer, liebliche Schutzgottheit", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gestirn der Trauer, liebliche Schutzgottheit", "tokens": ["Ge\u00b7stirn", "der", "Trau\u00b7er", ",", "lieb\u00b7li\u00b7che", "Schutz\u00b7got\u00b7theit"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gest\u00fcrzter Tempel, du der Ruinenwelt", "tokens": ["Ge\u00b7st\u00fcrz\u00b7ter", "Tem\u00b7pel", ",", "du", "der", "Ru\u00b7i\u00b7nen\u00b7welt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Schwerm\u00fcth'ge Freundin, wie zur Heimath", "tokens": ["Schwer\u00b7m\u00fcth'\u00b7ge", "Freun\u00b7din", ",", "wie", "zur", "Hei\u00b7math"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du erkoren die stille Roma!", "tokens": ["Hast", "du", "er\u00b7ko\u00b7ren", "die", "stil\u00b7le", "Ro\u00b7ma", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Du selbst ja gleichst ihr: wie du dein heilig Licht", "tokens": ["Du", "selbst", "ja", "gleichst", "ihr", ":", "wie", "du", "dein", "hei\u00b7lig", "Licht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVFIN", "PPER", "$.", "PWAV", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Sonne dankst, der untergegangenen,", "tokens": ["Der", "Son\u00b7ne", "dankst", ",", "der", "un\u00b7ter\u00b7ge\u00b7gan\u00b7ge\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So dankt auch sie die ew'ge Hoheit", "tokens": ["So", "dankt", "auch", "sie", "die", "ew'\u00b7ge", "Ho\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihrer entflohenen Herrschersonne.", "tokens": ["Ih\u00b7rer", "ent\u00b7flo\u00b7he\u00b7nen", "Herr\u00b7scher\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wo auch herab sich senke dein milder Blick,", "tokens": ["Wo", "auch", "her\u00b7ab", "sich", "sen\u00b7ke", "dein", "mil\u00b7der", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "PRF", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ob auf die \u00f6den Mauern, wo einsam sich", "tokens": ["Ob", "auf", "die", "\u00f6\u00b7den", "Mau\u00b7ern", ",", "wo", "ein\u00b7sam", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,", "PWAV", "ADJD", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Stra\u00dfe windet und zuweilen", "tokens": ["Die", "Stra\u00b7\u00dfe", "win\u00b7det", "und", "zu\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Epheubewachsene Gr\u00e4ber d\u00fcstern,", "tokens": ["E\u00b7pheu\u00b7be\u00b7wach\u00b7se\u00b7ne", "Gr\u00e4\u00b7ber", "d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ob auf Kapellen, schweigende Kl\u00f6ster auch,", "tokens": ["Ob", "auf", "Ka\u00b7pel\u00b7len", ",", "schwei\u00b7gen\u00b7de", "Kl\u00f6s\u00b7ter", "auch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NN", "$,", "ADJA", "NN", "ADV", "$,"], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die halb aus vollen B\u00fcschen und G\u00e4rten sich", "tokens": ["Die", "halb", "aus", "vol\u00b7len", "B\u00fc\u00b7schen", "und", "G\u00e4r\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "APPR", "ADJA", "NN", "KON", "NN", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Im Schattendach der Pinie heben,", "tokens": ["Im", "Schat\u00b7ten\u00b7dach", "der", "Pi\u00b7nie", "he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Halb sich im \u00fcpp'gen Gew\u00e4chs verbergen,", "tokens": ["Halb", "sich", "im", "\u00fcpp'\u00b7gen", "Ge\u00b7w\u00e4chs", "ver\u00b7ber\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Ob in des Tibers schicksalgeweihte Fluth,", "tokens": ["Ob", "in", "des", "Ti\u00b7bers", "schick\u00b7sal\u00b7ge\u00b7weih\u00b7te", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo sich des Fischers Netz in die Wasser taucht,", "tokens": ["Wo", "sich", "des", "Fi\u00b7schers", "Netz", "in", "die", "Was\u00b7ser", "taucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und Br\u00fcck' und Insel und der Besta", "tokens": ["Und", "Br\u00fcck", "und", "In\u00b7sel", "und", "der", "Be\u00b7sta"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "KON", "ART", "NN"], "meter": "-+-+-++-+", "measure": "unknown.measure.penta"}, "line.4": {"text": "Trauernder Tempel der Erd' entsteigen;", "tokens": ["Trau\u00b7ern\u00b7der", "Tem\u00b7pel", "der", "Erd'", "ent\u00b7stei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Stets blickst mit gleicher Liebe dein Rom du an,", "tokens": ["Stets", "blickst", "mit", "glei\u00b7cher", "Lie\u00b7be", "dein", "Rom", "du", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "PPOSAT", "NE", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und unaussprechlich finster erhaben ruht's,", "tokens": ["Und", "un\u00b7aus\u00b7sprech\u00b7lich", "fins\u00b7ter", "er\u00b7ha\u00b7ben", "ruht's", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Tr\u00fcmmern und Cypressenh\u00fcgeln", "tokens": ["Mit", "Tr\u00fcm\u00b7mern", "und", "Cyp\u00b7res\u00b7sen\u00b7h\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "D\u00e4mmernd in Mondlicht und Todtenstille.", "tokens": ["D\u00e4m\u00b7mernd", "in", "Mond\u00b7licht", "und", "Tod\u00b7ten\u00b7stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "So oft in tiefen Schauern durchwandl' ich noch", "tokens": ["So", "oft", "in", "tie\u00b7fen", "Schau\u00b7ern", "durch\u00b7wandl'", "ich", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die hohen St\u00e4tten, und die Allee entlang", "tokens": ["Die", "ho\u00b7hen", "St\u00e4t\u00b7ten", ",", "und", "die", "Al\u00b7lee", "ent\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "APPO"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lenk' ich den Tritt, wo einst der heil'ge", "tokens": ["Lenk'", "ich", "den", "Tritt", ",", "wo", "einst", "der", "heil'\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PWAV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weg an den Tempeln vor\u00fcberf\u00fchrte.", "tokens": ["Weg", "an", "den", "Tem\u00b7peln", "vor\u00b7\u00fc\u00b7berf\u00b7\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Dann harr' ich, bis die Glock' auf dem Capitol", "tokens": ["Dann", "harr'", "ich", ",", "bis", "die", "Glock'", "auf", "dem", "Ca\u00b7pi\u00b7tol"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die ernste Stund' ank\u00fcndigt der Mitternacht,", "tokens": ["Die", "erns\u00b7te", "Stund'", "an\u00b7k\u00fcn\u00b7digt", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein dumpfer Klang und pl\u00f6tzlich wieder", "tokens": ["Ein", "dum\u00b7pfer", "Klang", "und", "pl\u00f6tz\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schweiget die Welt und ihr off'nes Grab hier.", "tokens": ["Schwei\u00b7get", "die", "Welt", "und", "ihr", "off'\u00b7nes", "Grab", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Dir dann, du schmachtend Auge der Nacht, o Mond,", "tokens": ["Dir", "dann", ",", "du", "schmach\u00b7tend", "Au\u00b7ge", "der", "Nacht", ",", "o", "Mond", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "ADJD", "NN", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir blick' ich tr\u00e4umend wieder von neuem zu,", "tokens": ["Dir", "blick'", "ich", "tr\u00e4u\u00b7mend", "wie\u00b7der", "von", "neu\u00b7em", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wolken seh' ich um dich wandeln,", "tokens": ["Die", "Wol\u00b7ken", "seh'", "ich", "um", "dich", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All', wie sie kommen, wie sie verschwinden.", "tokens": ["All'", ",", "wie", "sie", "kom\u00b7men", ",", "wie", "sie", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Oft bist du klar, sanft l\u00e4chelnde Freundin Roms!", "tokens": ["Oft", "bist", "du", "klar", ",", "sanft", "l\u00e4\u00b7cheln\u00b7de", "Freun\u00b7din", "Roms", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oft aber gleich den Schatten des Schicksals, gleich", "tokens": ["Oft", "a\u00b7ber", "gleich", "den", "Schat\u00b7ten", "des", "Schick\u00b7sals", ",", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den V\u00f6lkerst\u00fcrmen und den Schrecken,", "tokens": ["Den", "V\u00f6l\u00b7ker\u00b7st\u00fcr\u00b7men", "und", "den", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die einst gew\u00fcthet an Roma's Himmel,", "tokens": ["Die", "einst", "ge\u00b7w\u00fct\u00b7het", "an", "Ro\u00b7ma's", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Bedeckt dein Antlitz fliegend Gew\u00f6lk, und schwarz", "tokens": ["Be\u00b7deckt", "dein", "Ant\u00b7litz", "flie\u00b7gend", "Ge\u00b7w\u00f6lk", ",", "und", "schwarz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entragt der Siegesbogen des Abgrunds Grau'n,", "tokens": ["E\u00b7ntragt", "der", "Sie\u00b7ges\u00b7bo\u00b7gen", "des", "Ab\u00b7grunds", "Grau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und selbst des Donn'rers S\u00e4ulentempel", "tokens": ["Und", "selbst", "des", "Donn'\u00b7rers", "S\u00e4u\u00b7len\u00b7tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwindet in D\u00e4mm'rung am Capitole.", "tokens": ["Schwin\u00b7det", "in", "D\u00e4m\u00b7m'\u00b7rung", "am", "Ca\u00b7pi\u00b7to\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}}, "stanza.12": {"line.1": {"text": "Und stumm seh' ich die m\u00e4chtigen Treppen an,", "tokens": ["Und", "stumm", "seh'", "ich", "die", "m\u00e4ch\u00b7ti\u00b7gen", "Trep\u00b7pen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die nun urpl\u00f6tzlich wieder der Vollmond hellt,", "tokens": ["Die", "nun", "ur\u00b7pl\u00f6tz\u00b7lich", "wie\u00b7der", "der", "Voll\u00b7mond", "hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und starre hin, und lausch' und horche,", "tokens": ["Und", "star\u00b7re", "hin", ",", "und", "lausch'", "und", "hor\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob wohl nicht C\u00e4sar heruntersteige.", "tokens": ["Ob", "wohl", "nicht", "C\u00e4\u00b7sar", "her\u00b7un\u00b7ter\u00b7stei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-", 