 "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "So oft in tiefen Schauern durchwandl' ich noch", "tokens": ["So", "oft", "in", "tie\u00b7fen", "Schau\u00b7ern", "durch\u00b7wandl'", "ich", "noch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "VMFIN", "PPER", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die hohen St\u00e4tten, und die Allee entlang", "tokens": ["Die", "ho\u00b7hen", "St\u00e4t\u00b7ten", ",", "und", "die", "Al\u00b7lee", "ent\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ART", "NN", "APPO"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Lenk' ich den Tritt, wo einst der heil'ge", "tokens": ["Lenk'", "ich", "den", "Tritt", ",", "wo", "einst", "der", "heil'\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PWAV", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weg an den Tempeln vor\u00fcberf\u00fchrte.", "tokens": ["Weg", "an", "den", "Tem\u00b7peln", "vor\u00b7\u00fc\u00b7berf\u00b7\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.21": {"line.1": {"text": "Dann harr' ich, bis die Glock' auf dem Capitol", "tokens": ["Dann", "harr'", "ich", ",", "bis", "die", "Glock'", "auf", "dem", "Ca\u00b7pi\u00b7tol"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die ernste Stund' ank\u00fcndigt der Mitternacht,", "tokens": ["Die", "erns\u00b7te", "Stund'", "an\u00b7k\u00fcn\u00b7digt", "der", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ein dumpfer Klang und pl\u00f6tzlich wieder", "tokens": ["Ein", "dum\u00b7pfer", "Klang", "und", "pl\u00f6tz\u00b7lich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJD", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schweiget die Welt und ihr off'nes Grab hier.", "tokens": ["Schwei\u00b7get", "die", "Welt", "und", "ihr", "off'\u00b7nes", "Grab", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "ADV", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.22": {"line.1": {"text": "Dir dann, du schmachtend Auge der Nacht, o Mond,", "tokens": ["Dir", "dann", ",", "du", "schmach\u00b7tend", "Au\u00b7ge", "der", "Nacht", ",", "o", "Mond", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPER", "ADJD", "NN", "ART", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Dir blick' ich tr\u00e4umend wieder von neuem zu,", "tokens": ["Dir", "blick'", "ich", "tr\u00e4u\u00b7mend", "wie\u00b7der", "von", "neu\u00b7em", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Wolken seh' ich um dich wandeln,", "tokens": ["Die", "Wol\u00b7ken", "seh'", "ich", "um", "dich", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "All', wie sie kommen, wie sie verschwinden.", "tokens": ["All'", ",", "wie", "sie", "kom\u00b7men", ",", "wie", "sie", "ver\u00b7schwin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PWAV", "PPER", "VVINF", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.23": {"line.1": {"text": "Oft bist du klar, sanft l\u00e4chelnde Freundin Roms!", "tokens": ["Oft", "bist", "du", "klar", ",", "sanft", "l\u00e4\u00b7cheln\u00b7de", "Freun\u00b7din", "Roms", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Oft aber gleich den Schatten des Schicksals, gleich", "tokens": ["Oft", "a\u00b7ber", "gleich", "den", "Schat\u00b7ten", "des", "Schick\u00b7sals", ",", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den V\u00f6lkerst\u00fcrmen und den Schrecken,", "tokens": ["Den", "V\u00f6l\u00b7ker\u00b7st\u00fcr\u00b7men", "und", "den", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die einst gew\u00fcthet an Roma's Himmel,", "tokens": ["Die", "einst", "ge\u00b7w\u00fct\u00b7het", "an", "Ro\u00b7ma's", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.24": {"line.1": {"text": "Bedeckt dein Antlitz fliegend Gew\u00f6lk, und schwarz", "tokens": ["Be\u00b7deckt", "dein", "Ant\u00b7litz", "flie\u00b7gend", "Ge\u00b7w\u00f6lk", ",", "und", "schwarz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "ADJD", "NN", "$,", "KON", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Entragt der Siegesbogen des Abgrunds Grau'n,", "tokens": ["E\u00b7ntragt", "der", "Sie\u00b7ges\u00b7bo\u00b7gen", "des", "Ab\u00b7grunds", "Grau'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und selbst des Donn'rers S\u00e4ulentempel", "tokens": ["Und", "selbst", "des", "Donn'\u00b7rers", "S\u00e4u\u00b7len\u00b7tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schwindet in D\u00e4mm'rung am Capitole.", "tokens": ["Schwin\u00b7det", "in", "D\u00e4m\u00b7m'\u00b7rung", "am", "Ca\u00b7pi\u00b7to\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "APPRART", "NN", "$."], "meter": "+--+---+-+-", "measure": "dactylic.di.plus"}}, "stanza.25": {"line.1": {"text": "Und stumm seh' ich die m\u00e4chtigen Treppen an,", "tokens": ["Und", "stumm", "seh'", "ich", "die", "m\u00e4ch\u00b7ti\u00b7gen", "Trep\u00b7pen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die nun urpl\u00f6tzlich wieder der Vollmond hellt,", "tokens": ["Die", "nun", "ur\u00b7pl\u00f6tz\u00b7lich", "wie\u00b7der", "der", "Voll\u00b7mond", "hellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--++-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und starre hin, und lausch' und horche,", "tokens": ["Und", "star\u00b7re", "hin", ",", "und", "lausch'", "und", "hor\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "KON", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ob wohl nicht C\u00e4sar heruntersteige.", "tokens": ["Ob", "wohl", "nicht", "C\u00e4\u00b7sar", "her\u00b7un\u00b7ter\u00b7stei\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PTKNEG", "NE", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.26": {"line.1": {"text": "Und einsmals aus dem buschigen Palatin,", "tokens": ["Und", "eins\u00b7mals", "aus", "dem", "bu\u00b7schi\u00b7gen", "Pa\u00b7la\u00b7tin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem tr\u00fcmmerschwarzen, klagt' eine Nachtigall", "tokens": ["Dem", "tr\u00fcm\u00b7mer\u00b7schwar\u00b7zen", ",", "klagt'", "ei\u00b7ne", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVINF", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "In all' die Nacht, in all' die Stille,", "tokens": ["In", "all'", "die", "Nacht", ",", "in", "all'", "die", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "ART", "NN", "$,", "APPR", "PIS", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Klagte vielleicht von der goldnen Vorzeit.", "tokens": ["Klag\u00b7te", "viel\u00b7leicht", "von", "der", "gold\u00b7nen", "Vor\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}}}}