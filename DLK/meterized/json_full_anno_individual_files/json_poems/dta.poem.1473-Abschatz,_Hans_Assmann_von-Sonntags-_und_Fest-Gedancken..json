{"dta.poem.1473": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonntags- und Fest-Gedancken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Weg bange Grabes-Furcht! mein JEsus liegt im Grabe/", "tokens": ["Weg", "ban\u00b7ge", "Gra\u00b7bes\u00b7Furcht", "!", "mein", "Je\u00b7sus", "liegt", "im", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NN", "$.", "PPOSAT", "NE", "VVFIN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was fehlt mir/ wenn ich den zum Schlaff-Geferten", "tokens": ["Was", "fehlt", "mir", "/", "wenn", "ich", "den", "zum", "Schlaff\u00b7Ge\u00b7fer\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ART", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "habe?", "tokens": ["ha\u00b7be", "?"], "token_info": ["word", "punct"], "pos": ["VAFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Weg zage Todes-Angst/ mein JEsus lag im Grabe/", "tokens": ["Weg", "za\u00b7ge", "To\u00b7des\u00b7Angst", "/", "mein", "Je\u00b7sus", "lag", "im", "Gra\u00b7be", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "PPOSAT", "NE", "VVFIN", "APPRART", "NN", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Was fehlt mir/ wenn ich den zum Aufferwecker habe?", "tokens": ["Was", "fehlt", "mir", "/", "wenn", "ich", "den", "zum", "Auf\u00b7fer\u00b7we\u00