{"dta.poem.387": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "An  Chrysandern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Denck ich/ mein Engels-Kind/ an deine holde Zier/", "tokens": ["Denck", "ich", "/", "mein", "En\u00b7gels\u00b7Kind", "/", "an", "dei\u00b7ne", "hol\u00b7de", "Zier", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$(", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So stell\u2019 ich mir die Pracht der edlen Sch\u00f6nheit f\u00fcr/", "tokens": ["So", "stell'", "ich", "mir", "die", "Pracht", "der", "ed\u00b7len", "Sch\u00f6n\u00b7heit", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ART", "NN", "ART", "ADJA", "NN", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Kein Gold/ kein Diamant dir zu vergleichen ist", "tokens": ["Kein", "Gold", "/", "kein", "Di\u00b7a\u00b7mant", "dir", "zu", "ver\u00b7glei\u00b7chen", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "PIAT", "NN", "PPER", "PTKZU", "VVINF", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein Engel/ du mir mehr als alle Sch\u00e4tze bist.", "tokens": ["Mein", "En\u00b7gel", "/", "du", "mir", "mehr", "als", "al\u00b7le", "Sch\u00e4t\u00b7ze", "bist", "."], "token_info": ["word", "word", "punct", "word", "word",