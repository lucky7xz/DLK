{"textgrid.poem.24120": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fchle nur", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einsam bist du? Sieh, die vielen Sterne", "tokens": ["Ein\u00b7sam", "bist", "du", "?", "Sieh", ",", "die", "vie\u00b7len", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PPER", "$.", "NE", "$,", "PRELS", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stehn, ein Weltenkranz, ob deinem Haupte,", "tokens": ["Stehn", ",", "ein", "Wel\u00b7ten\u00b7kranz", ",", "ob", "dei\u00b7nem", "Haup\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Lindenb\u00e4ume, Kronentr\u00e4ger,", "tokens": ["Und", "die", "Lin\u00b7den\u00b7b\u00e4u\u00b7me", ",", "Kro\u00b7nen\u00b7tr\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schicken ihre D\u00fcfte dir ins Zimmer.", "tokens": ["Schi\u00b7cken", "ih\u00b7re", "D\u00fcf\u00b7te", "dir", "ins", "Zim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "F\u00fchle nur! Saug ein und gieb dich wieder!", "tokens": ["F\u00fch\u00b7le", "nur", "!", "Saug", "ein", "und", "gieb", "dich", "wie\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "NN", "PTKVZ", "KON", "VVIMP", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Schm\u00e4he niemand, schm\u00e4h auch dich nicht selber!", "tokens": ["Schm\u00e4\u00b7he", "nie\u00b7mand", ",", "schm\u00e4h", "auch", "dich", "nicht", "sel\u00b7ber", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "VVFIN", "ADV", "PPER", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Denk: du darfst auf dieser reichen Erde", "tokens": ["Denk", ":", "du", "darfst", "auf", "die\u00b7ser", "rei\u00b7chen", "Er\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$.", "PPER", "VMFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Durch den sonnenvollen Weltraum fliegen,", "tokens": ["Durch", "den", "son\u00b7nen\u00b7vol\u00b7len", "Welt\u00b7raum", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Und dein Herz geh\u00f6rt auch zu den Sternen,", "tokens": ["Und", "dein", "Herz", "ge\u00b7h\u00f6rt", "auch", "zu", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die ein bischen Lust und W\u00e4rme strahlen.", "tokens": ["Die", "ein", "bi\u00b7schen", "Lust", "und", "W\u00e4r\u00b7me", "strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "