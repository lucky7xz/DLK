{"dta.poem.13058": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201enun komm\u2019 ich wiederum zu diesem Tugend-Saal:", "tokens": ["\u201e", "nun", "komm'", "ich", "wie\u00b7de\u00b7rum", "zu", "die\u00b7sem", "Tu\u00b7gen\u00b7dSaal", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "530\u201dErrathet ihr, was ich durch diese Fabel mahl?", "tokens": ["\"", "Er\u00b7ra\u00b7thet", "ihr", ",", "was", "ich", "durch", "die\u00b7se", "Fa\u00b7bel", "mahl", "?"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "PWS", "PPER", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201eder Sternen-Krei\u00df seyd ihr. ", "tokens": ["\u201e", "der", "Ster\u00b7nen\u00b7Krei\u00df", "seyd", "ihr", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "\u201edie F\u00fcrstinn euers Chors, Vergn\u00fcgen, Lust und Wonne.", "tokens": ["\u201e", "die", "F\u00fcrs\u00b7tinn", "eu\u00b7ers", "Chors", ",", "Ver\u00b7gn\u00fc\u00b7gen", ",", "Lust", "und", "Won\u00b7ne", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201emich dunckt ich irre nicht; und selber ihr versteht,", "tokens": ["\u201e", "mich", "dunckt", "ich", "ir\u00b7re", "nicht", ";", "und", "sel\u00b7ber", "ihr", "ver\u00b7steht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "VVFIN", "PTKNEG", "$.", "KON", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eda\u00df die Begebenheit nach meiner Absicht geht.", "tokens": ["\u201e", "da\u00df", "die", "Be\u00b7ge\u00b7ben\u00b7heit", "nach", "mei\u00b7ner", "Ab\u00b7sicht", "geht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "535\u201dDie Sonne reget euch, verherrlichet, belebet,", "tokens": ["\"", "Die", "Son\u00b7ne", "re\u00b7get", "euch", ",", "ver\u00b7herr\u00b7li\u00b7chet", ",", "be\u00b7le\u00b7bet", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ebegeistert eure Macht, wann ihr um selbe schwebet.", "tokens": ["\u201e", "be\u00b7geis\u00b7tert", "eu\u00b7re", "Macht", ",", "wann", "ihr", "um", "sel\u00b7be", "schwe\u00b7bet", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPOSAT", "NN", "$,", "PWAV", "PPER", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ejhr h\u00e4ttet selbst vielleicht des Monds Entschlu\u00df verlacht,", "tokens": ["\u201e", "jhr", "h\u00e4t\u00b7tet", "selbst", "viel\u00b7leicht", "des", "Monds", "Ent\u00b7schlu\u00df", "ver\u00b7lacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eals er der Sonne Glanz den Morgen-Gru\u00df gebracht.", "tokens": ["\u201e", "als", "er", "der", "Son\u00b7ne", "Glanz", "den", "Mor\u00b7gen\u00b7Gru\u00df", "ge\u00b7bracht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "ART", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewie kann es demnach seyn, da\u00df ihr ihr Haupt bekr\u00e4nzet,", "tokens": ["\u201e", "wie", "kann", "es", "dem\u00b7nach", "seyn", ",", "da\u00df", "ihr", "ihr", "Haupt", "be\u00b7kr\u00e4n\u00b7zet", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VMFIN", "PPER", "PAV", "VAINF", "$,", "KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "540\u201dDa selber eure Pracht nicht als durch selbes gl\u00e4nzet?", "tokens": ["\"", "Da", "sel\u00b7ber", "eu\u00b7re", "Pracht", "nicht", "als", "durch", "sel\u00b7bes", "gl\u00e4n\u00b7zet", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "PPOSAT", "NN", "PTKNEG", "KOKOM", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}