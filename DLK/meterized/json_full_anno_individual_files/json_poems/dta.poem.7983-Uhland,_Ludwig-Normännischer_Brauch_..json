{"dta.poem.7983": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Norm\u00e4nnischer Brauch .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl sitzt am Meeresstrande", "tokens": ["Wohl", "sitzt", "am", "Mee\u00b7res\u00b7stran\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein zartes Jungfr\u00e4ulein,", "tokens": ["Ein", "zar\u00b7tes", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie angelt manche Stunde,", "tokens": ["Sie", "an\u00b7gelt", "man\u00b7che", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Fischlein bei\u00dft ihr ein.", "tokens": ["Kein", "Fisc\u00b7hlein", "bei\u00dft", "ihr", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie hat \u2019nen Ring am Finger", "tokens": ["Sie", "hat", "'nen", "Ring", "am", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit rothem Edelstein,", "tokens": ["Mit", "ro\u00b7them", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den bindt sie an die Angel,", "tokens": ["Den", "bindt", "sie", "an", "die", "An\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wirft ihn in\u2019s Meer hinein.", "tokens": ["Wirft", "ihn", "in's", "Meer", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da hebt sich aus der Tiefe", "tokens": ["Da", "hebt", "sich", "aus", "der", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u2019ne Hand, wie Elfenbein,", "tokens": ["'ne", "Hand", ",", "wie", "El\u00b7fen\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die l\u00e4\u00dft am Finger blinken", "tokens": ["Die", "l\u00e4\u00dft", "am", "Fin\u00b7ger", "blin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das goldne Ringelein.", "tokens": ["Das", "gold\u00b7ne", "Rin\u00b7ge\u00b7lein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Da hebt sich aus dem Grunde", "tokens": ["Da", "hebt", "sich", "aus", "dem", "Grun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Ritter, jung und fein,", "tokens": ["Ein", "Rit\u00b7ter", ",", "jung", "und", "fein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er prangt in goldnen Schuppen", "tokens": ["Er", "prangt", "in", "gold\u00b7nen", "Schup\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und spielt im Sonnenschein.", "tokens": ["Und", "spielt", "im", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Das M\u00e4gdlein spricht erschrocken:", "tokens": ["Das", "M\u00e4gd\u00b7lein", "spricht", "er\u00b7schro\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201enein, edler Ritter, nein!", "tokens": ["\u201e", "nein", ",", "ed\u00b7ler", "Rit\u00b7ter", ",", "nein", "!"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PTKANT", "$,", "ADJA", "NN", "$,", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df du mein Ringlein golden!", "tokens": ["La\u00df", "du", "mein", "Rin\u00b7glein", "gol\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gar nicht begehrt\u2019 ich dein.\u201c", "tokens": ["Gar", "nicht", "be\u00b7gehrt'", "ich", "dein", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "PPER", "PPOSAT", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u201eman angelt nicht nach Fischen", "tokens": ["\u201e", "man", "an\u00b7gelt", "nicht", "nach", "Fi\u00b7schen"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PIS", "VVFIN", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit Gold und Edelstein,", "tokens": ["Mit", "Gold", "und", "E\u00b7del\u00b7stein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das Ringlein lass\u2019 ich nimmer,", "tokens": ["Das", "Rin\u00b7glein", "lass'", "ich", "nim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein eigen mu\u00dft du seyn.\u201c", "tokens": ["Mein", "ei\u00b7gen", "mu\u00dft", "du", "seyn", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VAINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}