{"textgrid.poem.43226": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich schm\u00e4hte Dich, weil Du mir Lieb' gelogen,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich schm\u00e4hte Dich, weil Du mir Lieb' gelogen,", "tokens": ["Ich", "schm\u00e4h\u00b7te", "Dich", ",", "weil", "Du", "mir", "Lieb'", "ge\u00b7lo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ich einst arm, unwissend Dich gefunden,", "tokens": ["Als", "ich", "einst", "arm", ",", "un\u00b7wis\u00b7send", "Dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "$,", "ADJD", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weil Du nun stolz und weil Dein Herz gebunden", "tokens": ["Weil", "Du", "nun", "stolz", "und", "weil", "Dein", "Herz", "ge\u00b7bun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "KOUS", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An jenen Mann, der Dich belehrt, erzogen.", "tokens": ["An", "je\u00b7nen", "Mann", ",", "der", "Dich", "be\u00b7lehrt", ",", "er\u00b7zo\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich z\u00fcrnte Dir noch in den j