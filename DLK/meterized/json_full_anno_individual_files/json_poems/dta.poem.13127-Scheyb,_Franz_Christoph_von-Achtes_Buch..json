{"dta.poem.13127": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Kronen h\u00f6chste Zier, des Purpurs Eigenschaften", "tokens": ["\u201e", "der", "Kro\u00b7nen", "h\u00f6chs\u00b7te", "Zier", ",", "des", "Pur\u00b7purs", "Ei\u00b7gen\u00b7schaf\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ART", "NN", "ADJA", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "630\u201dSeynd, da\u00df der L\u00e4nder Ruh und Fried an ihnen hafften.", "tokens": ["\"", "Seynd", ",", "da\u00df", "der", "L\u00e4n\u00b7der", "Ruh", "und", "Fried", "an", "ih\u00b7nen", "haff\u00b7ten", "."], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NE", "$,", "KOUS", "ART", "NN", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewer einen Thron besteigt; wem GOtt den Zepter gibt;", "tokens": ["\u201e", "wer", "ei\u00b7nen", "Thron", "be\u00b7steigt", ";", "wem", "Gott", "den", "Zep\u00b7ter", "gibt", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "ART", "NN", "VVPP", "$.", "PWS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201ewer herrscht, und dieses Amt mit wahrem Eifer \u00fcbt;", "tokens": ["\u201e", "wer", "herrscht", ",", "und", "die\u00b7ses", "Amt", "mit", "wah\u00b7rem", "Ei\u00b7fer", "\u00fcbt", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "$,", "KON", "PDAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewem dieser Tag und Nacht pflegt Sinn und Herz zu regen;", "tokens": ["\u201e", "wem", "die\u00b7ser", "Tag", "und", "Nacht", "pflegt", "Sinn", "und", "Herz", "zu", "re\u00b7gen", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PDAT", "NN", "KON", "NN", "VVFIN", "NN", "KON", "NN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201edem n\u00fczet nichts so sehr, als meines Amts Verm\u00f6gen.", "tokens": ["\u201e", "dem", "n\u00fc\u00b7zet", "nichts", "so", "sehr", ",", "als", "mei\u00b7nes", "Amts", "Ver\u00b7m\u00f6\u00b7gen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "PIS", "ADV", "ADV", "$,", "KOUS", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "635\u201dWer zu des Vaterlands gemeiner Wohlfart wacht,", "tokens": ["\"", "Wer", "zu", "des", "Va\u00b7ter\u00b7lands", "ge\u00b7mei\u00b7ner", "Wohl\u00b7fart", "wacht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201egibt auf den hohen Schaz, gesund zu seyn, nicht acht.", "tokens": ["\u201e", "gibt", "auf", "den", "ho\u00b7hen", "Schaz", ",", "ge\u00b7sund", "zu", "seyn", ",", "nicht", "acht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,", "ADJD", "PTKZU", "VAINF", "$,", "PTKNEG", "CARD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eer ist durch seines Amts Gewalt davon verhindert;", "tokens": ["\u201e", "er", "ist", "durch", "sei\u00b7nes", "Amts", "Ge\u00b7walt", "da\u00b7von", "ver\u00b7hin\u00b7dert", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "APPR", "PPOSAT", "NN", "NN", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201edes Staats Heil wird vermehrt, das eigene gemindert.", "tokens": ["\u201e", "des", "Staats", "Heil", "wird", "ver\u00b7mehrt", ",", "das", "ei\u00b7ge\u00b7ne", "ge\u00b7min\u00b7dert", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VAFIN", "ADJD", "$,", "ART", "ADJA", "VVPP", "$."], "meter": "-+---+-+---+-", "measure": "dactylic.init"}, "line.11": {"text": "\u201edie\u00df liessen wir ..... jedoch ich irre. GOttes Macht", "tokens": ["\u201e", "die\u00df", "lies\u00b7sen", "wir", ".....", "je\u00b7doch", "ich", "ir\u00b7re", ".", "Got\u00b7tes", "Macht"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PDS", "VVFIN", "PPER", "$(", "ADV", "PPER", "VVFIN", "$.", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "640\u201dNicht meinen Kr\u00e4ften sey die Ehre zugedacht.", "tokens": ["\"", "Nicht", "mei\u00b7nen", "Kr\u00e4f\u00b7ten", "sey", "die", "Eh\u00b7re", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201edas liesse GOtt, sag\u2019 ich, an ihr zwar nicht erfahren,", "tokens": ["\u201e", "das", "lies\u00b7se", "Gott", ",", "sag'", "ich", ",", "an", "ihr", "zwar", "nicht", "er\u00b7fah\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "NN", "$,", "VVFIN", "PPER", "$,", "APPR", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201e", "tokens": ["\u201e"], "token_info": ["punct"], "pos": ["$("]}}}}}