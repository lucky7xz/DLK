{"dta.poem.13405": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Der 90 Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "HeRR! zu dem wir jetzo beten!", "tokens": ["HeRR", "!", "zu", "dem", "wir", "jet\u00b7zo", "be\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "APPR", "PRELS", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "GoTT! dein Saame schreyt zu Dir!", "tokens": ["GoTT", "!", "dein", "Saa\u00b7me", "schreyt", "zu", "Dir", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PPOSAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn Du bleibest doch in N\u00f6then", "tokens": ["Denn", "Du", "blei\u00b7best", "doch", "in", "N\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Zuflucht f\u00fcr und f\u00fcr;", "tokens": ["Uns\u00b7re", "Zu\u00b7flucht", "f\u00fcr", "und", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "KON", "APPR", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bevor die Berge sich erhoben,", "tokens": ["Be\u00b7vor", "die", "Ber\u00b7ge", "sich", "er\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Erde ward, die Welt bereit,", "tokens": ["Die", "Er\u00b7de", "ward", ",", "die", "Welt", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da warest Du Erhabner droben", "tokens": ["Da", "wa\u00b7rest", "Du", "Er\u00b7hab\u00b7ner", "dro\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Ewigkeit zu Ewigkeit.", "tokens": ["Von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du befiehlst! die Menschen sterben,", "tokens": ["Du", "be\u00b7fiehlst", "!", "die", "Men\u00b7schen", "ster\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie gehen in den Staub,", "tokens": ["Und", "sie", "ge\u00b7hen", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn sprichst Du: Kehrt wieder, Erben!", "tokens": ["Denn", "sprichst", "Du", ":", "Kehrt", "wie\u00b7der", ",", "Er\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$.", "VVIMP", "ADV", "$,", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Seyd nicht mehr des Todes Raub!", "tokens": ["Seyd", "nicht", "mehr", "des", "To\u00b7des", "Raub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PTKNEG", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn tausend Jahre, die verstrichen,", "tokens": ["Denn", "tau\u00b7send", "Jah\u00b7re", ",", "die", "ver\u00b7stri\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "PRELS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind vor Dir l\u00e4nger nicht verbracht,", "tokens": ["Sind", "vor", "Dir", "l\u00e4n\u00b7ger", "nicht", "ver\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADJD", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als wie der Tag, der abgewichen,", "tokens": ["Als", "wie", "der", "Tag", ",", "der", "ab\u00b7ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und als die Wachzeit in der Nacht.", "tokens": ["Und", "als", "die", "Wach\u00b7zeit", "in", "der", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie in Str\u00f6hmen man Gew\u00e4sser", "tokens": ["Wie", "in", "Str\u00f6h\u00b7men", "man", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "PIS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Siehet schnell vor\u00fcber ziehn,", "tokens": ["Sie\u00b7het", "schnell", "vor\u00b7\u00fc\u00b7ber", "ziehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Eben also und nicht besser", "tokens": ["E\u00b7ben", "al\u00b7so", "und", "nicht", "bes\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "KON", "PTKNEG", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "L\u00e4ssest Du sie auch dahin;", "tokens": ["L\u00e4s\u00b7sest", "Du", "sie", "auch", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie, wenn der Schlaf sich eingefunden,", "tokens": ["Wie", ",", "wenn", "der", "Schlaf", "sich", "ein\u00b7ge\u00b7fun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da ohne sinnlichen Gebrauch", "tokens": ["Da", "oh\u00b7ne", "sinn\u00b7li\u00b7chen", "Ge\u00b7brauch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Man nicht merkt, wie die Zeit verschwunden,", "tokens": ["Man", "nicht", "merkt", ",", "wie", "die", "Zeit", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PTKNEG", "VVFIN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "So ist der Menschen Leben auch.", "tokens": ["So", "ist", "der", "Men\u00b7schen", "Le\u00b7ben", "auch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie das Gras am Morgen pranget,", "tokens": ["Wie", "das", "Gras", "am", "Mor\u00b7gen", "pran\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und voll sch\u00f6ner Blumen steht,", "tokens": ["Und", "voll", "sch\u00f6\u00b7ner", "Blu\u00b7men", "steht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das bald an zu welken fanget,", "tokens": ["Das", "bald", "an", "zu", "wel\u00b7ken", "fan\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "APPR", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ehe noch der Tag vergeht;", "tokens": ["E\u00b7he", "noch", "der", "Tag", "ver\u00b7geht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und bey der Abendd\u00e4mmrung Grauen,", "tokens": ["Und", "bey", "der", "A\u00b7bend\u00b7d\u00e4mm\u00b7rung", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Eh alles seinen Wuchs erreicht,", "tokens": ["Eh", "al\u00b7les", "sei\u00b7nen", "Wuchs", "er\u00b7reicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Schon wiederum wird abgehauen,", "tokens": ["Schon", "wie\u00b7de\u00b7rum", "wird", "ab\u00b7ge\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Da es verdorret und erbleicht.", "tokens": ["Da", "es", "ver\u00b7dor\u00b7ret", "und", "er\u00b7bleicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Doch dein Zorn ist uns entgegen,", "tokens": ["Doch", "dein", "Zorn", "ist", "uns", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieser macht, da\u00df wir so bald", "tokens": ["Die\u00b7ser", "macht", ",", "da\u00df", "wir", "so", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Uns zu Bette m\u00fcssen legen,", "tokens": ["Uns", "zu", "Bet\u00b7te", "m\u00fcs\u00b7sen", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und dein Grimm straft dergestalt,", "tokens": ["Und", "dein", "Grimm", "straft", "der\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NE", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wir so pl\u00f6tzlich und geschwinde", "tokens": ["Da\u00df", "wir", "so", "pl\u00f6tz\u00b7lich", "und", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Verschwinden sehen unsre Zeit,", "tokens": ["Ver\u00b7schwin\u00b7den", "se\u00b7hen", "uns\u00b7re", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, gleich dem ungest\u00fcmen Winde,", "tokens": ["Und", ",", "gleich", "dem", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Win\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Befallen sind mit Sterblichkeit.", "tokens": ["Be\u00b7fal\u00b7len", "sind", "mit", "Sterb\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Unser b\u00f6slich Unterwinden", "tokens": ["Un\u00b7ser", "b\u00f6s\u00b7lich", "Un\u00b7ter\u00b7win\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stellest Du vor Dir ins Licht,", "tokens": ["Stel\u00b7lest", "Du", "vor", "Dir", "ins", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ja die unerkannten S\u00fcnden", "tokens": ["Ja", "die", "un\u00b7er\u00b7kann\u00b7ten", "S\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stehn vor deinem Angesicht;", "tokens": ["Stehn", "vor", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Drum fahren auch all unsre Tage", "tokens": ["Drum", "fah\u00b7ren", "auch", "all", "uns\u00b7re", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADV", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Durch deinen Zorn dahin im Nu,", "tokens": ["Durch", "dei\u00b7nen", "Zorn", "da\u00b7hin", "im", "Nu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PAV", "APPRART", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir bringen, unter Noth und Plage,", "tokens": ["Wir", "brin\u00b7gen", ",", "un\u00b7ter", "Noth", "und", "Pla\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als ein Geschw\u00e4tz die Jahre zu.", "tokens": ["Als", "ein", "Ge\u00b7schw\u00e4tz", "die", "Jah\u00b7re", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie, die Lebensjahre, w\u00e4hren,", "tokens": ["Sie", ",", "die", "Le\u00b7bens\u00b7jah\u00b7re", ",", "w\u00e4h\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Das man kaum z\u00e4hlt siebzig mahl,", "tokens": ["Das", "man", "kaum", "z\u00e4hlt", "sieb\u00b7zig", "mahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "ADV", "VVFIN", "CARD", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn es hoch k\u00f6mmt, wird man h\u00f6ren,", "tokens": ["Wenn", "es", "hoch", "k\u00f6mmt", ",", "wird", "man", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVFIN", "$,", "VAFIN", "PIS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Achtzig sind ihr nach der Zahl!", "tokens": ["Acht\u00b7zig", "sind", "ihr", "nach", "der", "Zahl", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sind sie noch k\u00f6stlich, auserlesen,", "tokens": ["Sind", "sie", "noch", "k\u00f6st\u00b7lich", ",", "au\u00b7ser\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So ist Verdru\u00df und M\u00fch der Lohn;", "tokens": ["So", "ist", "Ver\u00b7dru\u00df", "und", "M\u00fch", "der", "Lohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es f\u00e4hrt dahin, wenn es gewesen,", "tokens": ["Es", "f\u00e4hrt", "da\u00b7hin", ",", "wenn", "es", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "$,", "KOUS", "PPER", "VAPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So schnell, als fl\u00f6hen wir davon.", "tokens": ["So", "schnell", ",", "als", "fl\u00f6\u00b7hen", "wir", "da\u00b7von", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "VVFIN", "PPER", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch wer glaubt der Warnungsstimme,", "tokens": ["Doch", "wer", "glaubt", "der", "War\u00b7nungs\u00b7stim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df dein Zorn so sehr entr\u00fcst,", "tokens": ["Da\u00df", "dein", "Zorn", "so", "sehr", "ent\u00b7r\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer hat Furcht vor solchem Grimme,", "tokens": ["Wer", "hat", "Furcht", "vor", "sol\u00b7chem", "Grim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welcher Menschen t\u00f6dtlich ist?", "tokens": ["Wel\u00b7cher", "Men\u00b7schen", "t\u00f6dt\u00b7lich", "ist", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "So lehr uns denn, da\u00df wir bedenken,", "tokens": ["So", "lehr", "uns", "denn", ",", "da\u00df", "wir", "be\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "$,", "KOUS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ohnumst\u00f6\u00dflich fest der Schlu\u00df,", "tokens": ["Wie", "oh\u00b7num\u00b7st\u00f6\u00df\u00b7lich", "fest", "der", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Tod wird uns ins Grab versenken,", "tokens": ["Der", "Tod", "wird", "uns", "ins", "Grab", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Damit man kl\u00fcger werden mu\u00df.", "tokens": ["Da\u00b7mit", "man", "kl\u00fc\u00b7ger", "wer\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Kehre Dich, o HErr! doch wieder", "tokens": ["Keh\u00b7re", "Dich", ",", "o", "Herr", "!", "doch", "wie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$.", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu uns, welche Demuth beugt!", "tokens": ["Zu", "uns", ",", "wel\u00b7che", "De\u00b7muth", "beugt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PWAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Deine Knechte fallen nieder;", "tokens": ["Dei\u00b7ne", "Knech\u00b7te", "fal\u00b7len", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werde gn\u00e4dig und geneigt!", "tokens": ["Wer\u00b7de", "gn\u00e4\u00b7dig", "und", "ge\u00b7neigt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Erf\u00fcll uns fr\u00fch mit deiner Gnade,", "tokens": ["Er\u00b7f\u00fcll", "uns", "fr\u00fch", "mit", "dei\u00b7ner", "Gna\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die r\u00fchmet unser Lustgesang,", "tokens": ["Die", "r\u00fch\u00b7met", "un\u00b7ser", "Lust\u00b7ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Denn sind wir frey von allem Schade,", "tokens": ["Denn", "sind", "wir", "frey", "von", "al\u00b7lem", "Scha\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Auch fr\u00f6lich unser Lebelang.", "tokens": ["Auch", "fr\u00f6\u00b7lich", "un\u00b7ser", "Le\u00b7be\u00b7lang", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da Du uns so lange plagest,", "tokens": ["Da", "Du", "uns", "so", "lan\u00b7ge", "pla\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch verz\u00f6gernd Ungemach,", "tokens": ["Durch", "ver\u00b7z\u00f6\u00b7gernd", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "VVPP", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht nach deinem Volke fragest,", "tokens": ["Nicht", "nach", "dei\u00b7nem", "Vol\u00b7ke", "fra\u00b7gest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auch nicht merkest auf ihr Ach!", "tokens": ["Auch", "nicht", "mer\u00b7kest", "auf", "ihr", "Ach", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "So la\u00df uns wiederum erfreuen,", "tokens": ["So", "la\u00df", "uns", "wie\u00b7de\u00b7rum", "er\u00b7freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Knechte sehn, was Du vollbracht;", "tokens": ["Die", "Knech\u00b7te", "sehn", ",", "was", "Du", "voll\u00b7bracht", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Was Dir zur Ehre kan gedeyen,", "tokens": ["Was", "Dir", "zur", "Eh\u00b7re", "kan", "ge\u00b7de\u00b7yen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Sey ihren Kindern kund gemacht.", "tokens": ["Sey", "ih\u00b7ren", "Kin\u00b7dern", "kund", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKVZ", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Herr! Du GOtt! Dein Angesichte", "tokens": ["Herr", "!", "Du", "Gott", "!", "Dein", "An\u00b7ge\u00b7sich\u00b7te"], "token_info": ["word", "punct", "word