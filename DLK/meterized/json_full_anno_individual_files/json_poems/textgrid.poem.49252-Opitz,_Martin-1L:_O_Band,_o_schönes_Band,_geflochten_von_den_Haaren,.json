{"textgrid.poem.49252": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1L: O Band, o sch\u00f6nes Band, geflochten von den Haaren,", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O Band, o sch\u00f6nes Band, geflochten von den Haaren,", "tokens": ["O", "Band", ",", "o", "sch\u00f6\u00b7nes", "Band", ",", "ge\u00b7floch\u00b7ten", "von", "den", "Haa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "FM", "ADJA", "NN", "$,", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die auf der Liebsten Haupt' hievor gestanden waren,", "tokens": ["Die", "auf", "der", "Liebs\u00b7ten", "Haupt'", "hie\u00b7vor", "ge\u00b7stan\u00b7den", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PAV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "An Gold und Perlen reich, umbunden meiner Hand,", "tokens": ["An", "Gold", "und", "Per\u00b7len", "reich", ",", "um\u00b7bun\u00b7den", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$,", "KOUI", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Zeichen ihrer Treu und ihrer Liebe Pfand,", "tokens": ["Ein", "Zei\u00b7chen", "ih\u00b7rer", "Treu", "und", "ih\u00b7rer", "Lie\u00b7be", "Pfand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du hast mir nicht allein die schwache Faust umgeben,", "tokens": ["Du", "hast", "mir", "nicht", "al\u00b7lein", "die", "schwa\u00b7che", "Faust", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Durch dich ist auch bestrickt mein Sinn, mein Herz und Leben;", "tokens": ["Durch", "dich", "ist", "auch", "be\u00b7strickt", "mein", "Sinn", ",", "mein", "Herz", "und", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "O werthes, edles Pfand, o B\u00fcrgin ihrer Hold,", "tokens": ["O", "wert\u00b7hes", ",", "ed\u00b7les", "Pfand", ",", "o", "B\u00fcr\u00b7gin", "ih\u00b7rer", "Hold", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,", "FM", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An dir ist um und um Geringers nichts als Gold.", "tokens": ["An", "dir", "ist", "um", "und", "um", "Ge\u00b7rin\u00b7gers", "nichts", "als", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "APPR", "KON", "APPR", "NN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "O Band, o sch\u00f6nes Band, geflochten von den Haaren,", "tokens":