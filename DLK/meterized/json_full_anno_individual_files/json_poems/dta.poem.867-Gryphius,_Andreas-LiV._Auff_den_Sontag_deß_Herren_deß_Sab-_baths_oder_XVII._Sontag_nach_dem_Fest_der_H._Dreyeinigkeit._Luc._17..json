{"dta.poem.867": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "LiV.  Auff den Sontag de\u00df Herren de\u00df Sab-  \n baths/ oder  XVII.  Sontag/ nach dem Fest der  \n H. Dreyeinigkeit. Luc. 17.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Heyland welchem nichts verborgen! GOTT der Hertzvnd ", "tokens": ["Hey\u00b7land", "wel\u00b7chem", "nichts", "ver\u00b7bor\u00b7gen", "!", "GoTT", "der", "Hertzvnd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PRELS", "PIS", "VVPP", "$.", "NE", "ART", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Nieren kennet/", "tokens": ["Nie\u00b7ren", "ken\u00b7net", "/"], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schawe wie viel falsche ", "tokens": ["Scha\u00b7we", "wie", "viel", "fal\u00b7sche"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KOKOM", "PIAT", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "nacht", "tokens": ["nacht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.2": {"line.1": {"text": "Mich den Jedermann zu f\u00e4llen vnter freundtschafft Nah-", "tokens": ["Mich", "den", "Je\u00b7der\u00b7mann", "zu", "f\u00e4l\u00b7len", "vn\u00b7ter", "freundt\u00b7schafft", "Nah"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PIS", "PTKZU", "VVINF", "APPR", "NE", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "men tracht.", "tokens": ["men", "tracht", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Mich den man so h\u00fcndisch neydet/ weil du mich dein Kindt", "tokens": ["Mich", "den", "man", "so", "h\u00fcn\u00b7disch", "ney\u00b7det", "/", "weil", "du", "mich", "dein", "Kindt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "PIS", "ADV", "ADJD", "VVFIN", "$(", "KOUS", "PPER", "PRF", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "genennet.", "tokens": ["ge\u00b7nen\u00b7net", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Mir wird durch vergiffte Zungen mein stets blutend ", "tokens": ["Mir", "wird", "durch", "ver\u00b7giff\u00b7te", "Zun\u00b7gen", "mein", "stets", "blu\u00b7tend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ADJA", "NN", "PPOSAT", "ADV", "VVPP"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "zutrennet.", "tokens": ["zu\u00b7tren\u00b7net", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Dein/ vnd meine Feinde jauchzen! Jhrer stoltzen geister ", "tokens": ["Dein", "/", "vnd", "mei\u00b7ne", "Fein\u00b7de", "jauch\u00b7zen", "!", "Ih\u00b7rer", "stolt\u00b7zen", "geis\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "$(", "KON", "PPOSAT", "NN", "VVINF", "$.", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "pracht ", "tokens": ["pracht"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wird die grosse welt zu enge/ weil mich jhrer hoffart", "tokens": ["Wird", "die", "gros\u00b7se", "welt", "zu", "en\u00b7ge", "/", "weil", "mich", "jhrer", "hof\u00b7fart"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "$(", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "Macht ", "tokens": ["Macht"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.7": {"text": "Au\u00df der letzten vnterstelle in den staub zu boden rennet.", "tokens": ["Au\u00df", "der", "letz\u00b7ten", "vn\u00b7ter\u00b7stel\u00b7le", "in", "den", "staub", "zu", "bo\u00b7den", "ren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.4": {"line.1": {"text": "Doch ich wei\u00df! der wird err\u00f6then/ der wird schanden voll", "tokens": ["Doch", "ich", "wei\u00df", "!", "der", "wird", "er\u00b7r\u00f6\u00b7then", "/", "der", "wird", "schan\u00b7den", "voll"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "$.", "ART", "VAFIN", "VVINF", "$(", "ART", "VAFIN", "ADJD", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "noch stehn! ", "tokens": ["noch", "stehn", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Der in hochmuth jtzt ersoffen/ will auf aller K\u00f6pffen gehn.", "tokens": ["Der", "in", "hoch\u00b7muth", "jtzt", "er\u00b7sof\u00b7fen", "/", "will", "auf", "al\u00b7ler", "K\u00f6pf\u00b7fen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADV", "VVPP", "$(", "VMFIN", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wenn du wirst/ was klein/ erh\u00f6hen/ vnd was hoch zu grunde ", "tokens": ["Wenn", "du", "wirst", "/", "was", "klein", "/", "er\u00b7h\u00f6\u00b7hen", "/", "vnd", "was", "hoch", "zu", "grun\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VAFIN", "$(", "PWS", "ADJD", "$(", "VVINF", "$(", "KON", "PWS", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "st\u00fcrtzen. ", "tokens": ["st\u00fcrt\u00b7zen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.5": {"line.1": {"text": "G\u00f6nnt der welt die kurtze Wonne/ die ein Augenblick ver-", "tokens": ["G\u00f6nnt", "der", "welt", "die", "kurt\u00b7ze", "Won\u00b7ne", "/", "die", "ein", "Au\u00b7gen\u00b7blick", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$(", "PRELS", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "kehrt ", "tokens": ["kehrt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Vnser Sabbath wird anbrech\u1ebd! in dem jhre lust hin f\u00e4hrt/", "tokens": ["Vn\u00b7ser", "Sab\u00b7ba\u00b7th", "wird", "an\u00b7brech\u1ebd", "!", "in", "dem", "jhre", "lust", "hin", "f\u00e4hrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "APPR", "ART", "PPOSAT", "NN", "ADV", "VVFIN", "$("], "meter": "+-+-+-+---+--+", "measure": "iambic.hexa.chol"}, "line.4": {"text": "Wird der keuschen ", "tokens": ["Wird", "der", "keu\u00b7schen"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}}}}}