{"dta.poem.3360": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Freie Presse .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1846", "urn": "urn:nbn:de:kobv:b4-200905191464", "language": ["de:0.99"], "booktitle": "Freiligrath, Ferdinand: \u00c7a ira! Herisau, 1846."}, "poem": {"stanza.1": {"line.1": {"text": "Festen Tons zu seinen Leuten spricht der Herr der", "tokens": ["Fes\u00b7ten", "Tons", "zu", "sei\u00b7nen", "Leu\u00b7ten", "spricht", "der", "Herr", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Druckerei:", "tokens": ["Dru\u00b7cke\u00b7rei", ":"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "\u201emorgen, wi\u00dft ihr, soll es losgeh\u2019n, und zum", "tokens": ["\u201e", "mor\u00b7gen", ",", "wi\u00dft", "ihr", ",", "soll", "es", "los\u00b7geh'n", ",", "und", "zum"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "$,", "VVFIN", "PPER", "$,", "VMFIN", "PPER", "VVINF", "$,", "KON", "APPRART"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Schie\u00dfen braucht man Blei!", "tokens": ["Schie\u00b7\u00dfen", "braucht", "man", "Blei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PIS", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Wohl, wir haben unsre Schriften: \u2014 Morgen in", "tokens": ["Wohl", ",", "wir", "ha\u00b7ben", "uns\u00b7re", "Schrif\u00b7ten", ":", "Mor\u00b7gen", "in"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$.", "$(", "NN", "APPR"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "die Reih\u2019n getreten!", "tokens": ["die", "Reih'n", "ge\u00b7tre\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Heute Munition gegossen aus metall\u2019nen Alpha-", "tokens": ["Heu\u00b7te", "Mu\u00b7ni\u00b7tion", "ge\u00b7gos\u00b7sen", "aus", "me\u00b7tall'\u00b7nen", "Al\u00b7pha"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVPP", "APPR", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "beten!", "tokens": ["be\u00b7ten", "!"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "\u201ehier die Formen, hier die Tiegel! auch die Kohlen", "tokens": ["\u201e", "hier", "die", "For\u00b7men", ",", "hier", "die", "Tie\u00b7gel", "!", "auch", "die", "Koh\u00b7len"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "$.", "ADV", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "facht\u2019 ich an!", "tokens": ["facht'", "ich", "an", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Und die Pforten sind verrammelt, da\u00df uns Nie-", "tokens": ["Und", "die", "Pfor\u00b7ten", "sind", "ver\u00b7ram\u00b7melt", ",", "da\u00df", "uns", "Nie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "TRUNC"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "mand st\u00f6ren kann!", "tokens": ["mand", "st\u00f6\u00b7ren", "kann", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "An die Arbeit denn, ihr Herren! Alle, die ihr", "tokens": ["An", "die", "Ar\u00b7beit", "denn", ",", "ihr", "Her\u00b7ren", "!", "Al\u00b7le", ",", "die", "ihr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "PPOSAT", "NN", "$.", "PIS", "$,", "PRELS", "PPER"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.6": {"text": "setzt und pre\u00dft!", "tokens": ["setzt", "und", "pre\u00dft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Helft mir auf die Beine bringen dieses Freiheits-", "tokens": ["Helft", "mir", "auf", "die", "Bei\u00b7ne", "brin\u00b7gen", "die\u00b7ses", "Frei\u00b7heits"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVINF", "PDAT", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "manifest!\u201c", "tokens": ["ma\u00b7ni\u00b7fest", "!", "\u201c"], "token_info": ["word", "punct", "punct"], "pos": ["VVFIN", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.3": {"line.1": {"text": "Spricht\u2019s, und wirft die ersten Lettern in den", "tokens": ["Spricht's", ",", "und", "wirft", "die", "ers\u00b7ten", "Let\u00b7tern", "in", "den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "APPR", "ART"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Tiegel frischer Hand.", "tokens": ["Tie\u00b7gel", "fri\u00b7scher", "Hand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Von der Hitze bald geschmolzen, brodeln Perl und", "tokens": ["Von", "der", "Hit\u00b7ze", "bald", "ge\u00b7schmol\u00b7zen", ",", "bro\u00b7deln", "Perl", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$,", "VVFIN", "NE", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Diamant;", "tokens": ["Di\u00b7a\u00b7mant", ";"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Brodeln Colonel und Corpus; hier Antiqua, dort", "tokens": ["Bro\u00b7deln", "Co\u00b7lo\u00b7nel", "und", "Cor\u00b7pus", ";", "hier", "An\u00b7ti\u00b7qua", ",", "dort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "NE", "KON", "NE", "$.", "ADV", "NE", "$,", "ADV"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Fraktur", "tokens": ["Frak\u00b7tur"], "token_info": ["word"], "pos": ["NN"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Werfen radikale Blasen, dreist umgehend die", "tokens": ["Wer\u00b7fen", "ra\u00b7di\u00b7ka\u00b7le", "Bla\u00b7sen", ",", "dreist", "um\u00b7ge\u00b7hend", "die"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADJA", "NN", "$,", "ADJD", "VVPP", "ART"], "meter": "----+-+-+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Censur.", "tokens": ["Cen\u00b7sur", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.4": {"line.1": {"text": "Dampfend in die Kugelformen zischt die gl\u00fch\u2019nde", "tokens": ["Damp\u00b7fend", "in", "die", "Ku\u00b7gel\u00b7for\u00b7men", "zischt", "die", "gl\u00fch'n\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Masse dann: \u2014", "tokens": ["Mas\u00b7se", "dann", ":"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "ADV", "$.", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So die ganze lange Herbstnacht schaffen diese", "tokens": ["So", "die", "gan\u00b7ze", "lan\u00b7ge", "Herbst\u00b7nacht", "schaf\u00b7fen", "die\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "ADJA", "NN", "VVFIN", "PDAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "zwanzig Mann;", "tokens": ["zwan\u00b7zig", "Mann", ";"], "token_info": ["word", "word", "punct"], "pos": ["CARD", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Athmen r\u00fcstig in die Kohlen; sch\u00fcren, schmelzen", "tokens": ["Ath\u00b7men", "r\u00fcs\u00b7tig", "in", "die", "Koh\u00b7len", ";", "sch\u00fc\u00b7ren", ",", "schmel\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$.", "VVFIN", "$,", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "unverdrossen,", "tokens": ["un\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Bis in runde, blanke Kugeln Schrift und Zeug", "tokens": ["Bis", "in", "run\u00b7de", ",", "blan\u00b7ke", "Ku\u00b7geln", "Schrift", "und", "Zeug"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ADJA", "$,", "ADJA", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "sie umgegossen!", "tokens": ["sie", "um\u00b7ge\u00b7gos\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVPP", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wohl verpackt in grauen Beuteln liegt der Vorrath", "tokens": ["Wohl", "ver\u00b7packt", "in", "grau\u00b7en", "Beu\u00b7teln", "liegt", "der", "Vor\u00b7rath"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "an der Erde,", "tokens": ["an", "der", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Fertig, da\u00df er mit der Fr\u00fche br\u00fchwarm ausge-", "tokens": ["Fer\u00b7tig", ",", "da\u00df", "er", "mit", "der", "Fr\u00fc\u00b7he", "br\u00fch\u00b7warm", "aus\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "APPR", "ART", "NN", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "geben werde!", "tokens": ["ge\u00b7ben", "wer\u00b7de", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVINF", "VAFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Eine dreiste Morgenzeitung! Wahrlich, gleich beherzt", "tokens": ["Ei\u00b7ne", "dreis\u00b7te", "Mor\u00b7gen\u00b7zei\u00b7tung", "!", "Wahr\u00b7lich", ",", "gleich", "be\u00b7herzt"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ADV", "$,", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.6": {"text": "und k\u00fchn", "tokens": ["und", "k\u00fchn"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Sah man keine noch entschwirren dieser alten", "tokens": ["Sah", "man", "kei\u00b7ne", "noch", "ent\u00b7schwir\u00b7ren", "die\u00b7ser", "al\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PIAT", "ADV", "VVFIN", "PDAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Offizin!", "tokens": ["Of\u00b7fi\u00b7zin", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}