{"textgrid.poem.43009": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Zeppelin fliegt \u00fcbers Meer.", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Zeppelin fliegt \u00fcbers Meer.", "tokens": ["Ein", "Zep\u00b7pe\u00b7lin", "fliegt", "\u00fc\u00b7bers", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aber es gibt schon heute", "tokens": ["A\u00b7ber", "es", "gibt", "schon", "heu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADV"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ganz gut gescheite Leute,", "tokens": ["Ganz", "gut", "ge\u00b7schei\u00b7te", "Leu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die interessiert das gar nicht sehr.", "tokens": ["Die", "in\u00b7ter\u00b7es\u00b7siert", "das", "gar", "nicht", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "ADV", "PTKNEG", "ADV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Weltenraumverkehr floriert", "tokens": ["Der", "Wel\u00b7ten\u00b7raum\u00b7ver\u00b7kehr", "flo\u00b7riert"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seit Urzeit, niemals minder.", "tokens": ["Seit", "Ur\u00b7zeit", ",", "nie\u00b7mals", "min\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,",