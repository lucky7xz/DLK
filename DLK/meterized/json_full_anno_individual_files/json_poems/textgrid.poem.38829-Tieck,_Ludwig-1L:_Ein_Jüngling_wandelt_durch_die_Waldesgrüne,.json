{"textgrid.poem.38829": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein J\u00fcngling wandelt durch die Waldesgr\u00fcne,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00fcngling wandelt durch die Waldesgr\u00fcne,", "tokens": ["Ein", "J\u00fcng\u00b7ling", "wan\u00b7delt", "durch", "die", "Wal\u00b7des\u00b7gr\u00fc\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Einsam, verlassen, seufzend und in Thr\u00e4nen;", "tokens": ["Ein\u00b7sam", ",", "ver\u00b7las\u00b7sen", ",", "seuf\u00b7zend", "und", "in", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "VVPP", "KON", "APPR", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Was will sein H\u00e4nderingen doch ersehnen!", "tokens": ["Was", "will", "sein", "H\u00e4n\u00b7de\u00b7rin\u00b7gen", "doch", "er\u00b7seh\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was sagt die tr\u00fcbe, liebe Leidensmiene?", "tokens": ["Was", "sagt", "die", "tr\u00fc\u00b7be", ",", "lie\u00b7be", "Lei\u00b7dens\u00b7mie\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bald ists, als ob ein Engel ihm erschiene,", "tokens": ["Bald", "ists", ",", "als", "ob", "ein", "En\u00b7gel", "ihm", "er\u00b7schie\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "KOKOM", "KOUS", "ART", "NN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So schaut er in das Gr\u00fcn mit hohem Sehnen,", "tokens": ["So", "schaut", "er", "in", "das", "Gr\u00fcn", "mit", "ho\u00b7hem", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er spricht mit V\u00f6geln, mit der Luft im W\u00e4hnen,", "tokens": ["Er", "spricht", "mit", "V\u00f6\u00b7geln", ",", "mit", "der", "Luft", "im", "W\u00e4h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "APPR", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Zweigen neigen Arme sich zur S\u00fchne.", "tokens": ["In", "Zwei\u00b7gen", "nei\u00b7gen", "Ar\u00b7me", "sich", "zur", "S\u00fch\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "NN", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da l\u00e4chelt er in Andacht und in Liebe,", "tokens": ["Da", "l\u00e4\u00b7chelt", "er", "in", "An\u00b7dacht", "und", "in", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Sonne scheint auf ihn mit rothen Lichtern,", "tokens": ["Die", "Son\u00b7ne", "scheint", "auf", "ihn", "mit", "ro\u00b7then", "Lich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In Glorien wallt der Tag und k\u00fc\u00dft ihn scheidend.", "tokens": ["In", "Glo\u00b7ri\u00b7en", "wallt", "der", "Tag", "und", "k\u00fc\u00dft", "ihn", "schei\u00b7dend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Ach, da\u00df der goldne Glanz zugegen bliebe!", "tokens": ["Ach", ",", "da\u00df", "der", "gold\u00b7ne", "Glanz", "zu\u00b7ge\u00b7gen", "blie\u00b7be", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nacht steigt auf mit Wolkenangesichtern,", "tokens": ["Die", "Nacht", "steigt", "auf", "mit", "Wol\u00b7ken\u00b7an\u00b7ge\u00b7sich\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Dunkel fa\u00dft ihn und er spricht s\u00fc\u00df leidend:", "tokens": ["Das", "Dun\u00b7kel", "fa\u00dft", "ihn", "und", "er", "spricht", "s\u00fc\u00df", "lei\u00b7dend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein J\u00fcngling wandelt