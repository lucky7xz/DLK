{"dta.poem.9854": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sechstinne.  \n Wett-streit der haare/ augen/ wangen/ lippen/  \n hal\u00df und br\u00fcste.  \n H. M.  \n Haare.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer sagt/ da\u00df unser ruhm nicht g\u00f6ldne fessel schencket/", "tokens": ["Wer", "sagt", "/", "da\u00df", "un\u00b7ser", "ruhm", "nicht", "g\u00f6ld\u00b7ne", "fes\u00b7sel", "schen\u00b7cket", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "KOUS", "PPOSAT", "NN", "PTKNEG", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn sie ein linder west um beyde br\u00fcste schwencket/", "tokens": ["Wenn", "sie", "ein", "lin\u00b7der", "west", "um", "bey\u00b7de", "br\u00fcs\u00b7te", "schwen\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "VVFIN", "APPR", "PIAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Entkerckert frey und lo\u00df? hier wird ein geist umschrencket", "tokens": ["Ent\u00b7ker\u00b7ckert", "frey", "und", "lo\u00df", "?", "hier", "wird", "ein", "geist", "um\u00b7schren\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "KON", "PTKVZ", "$.", "ADV", "VAFIN", "ART", "NN", "VVFIN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Mit steter dienstbarkeit/ der vor sich weggelencket", "tokens": ["Mit", "ste\u00b7ter", "dienst\u00b7bar\u00b7keit", "/", "der", "vor", "sich", "weg\u00b7ge\u00b7len\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$(", "ART", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Von band und ketten hat. Ein ewig nectar tr\u00e4ncket", "tokens": ["Von", "band", "und", "ket\u00b7ten", "hat", ".", "Ein", "e\u00b7wig", "nec\u00b7tar", "tr\u00e4n\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "KON", "VVFIN", "VAFIN", "$.", "ART", "ADJD", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der haare liebes", "tokens": ["Der", "haa\u00b7re", "lie\u00b7bes"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}}}}}