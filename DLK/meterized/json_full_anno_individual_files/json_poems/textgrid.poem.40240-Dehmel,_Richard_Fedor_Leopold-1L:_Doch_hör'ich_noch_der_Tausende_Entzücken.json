{"textgrid.poem.40240": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "1L: Doch h\u00f6r'ich noch der Tausende Entz\u00fccken", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Doch h\u00f6r'ich noch der Tausende Entz\u00fccken", "tokens": ["Doch", "h\u00f6r'\u00b7ich", "noch", "der", "Tau\u00b7sen\u00b7de", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "und Ihn von seinen goldnen Sternen sprechen", "tokens": ["und", "Ihn", "von", "sei\u00b7nen", "gold\u00b7nen", "Ster\u00b7nen", "spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und sehe noch ihn seine Rosen brechen", "tokens": ["und", "se\u00b7he", "noch", "ihn", "sei\u00b7ne", "Ro\u00b7sen", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und noch den Kranz das Haupt ihm blutig dr\u00fccken.", "tokens": ["und", "noch", "den", "Kranz", "das", "Haupt", "ihm", "blu\u00b7tig", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie lagen jubelnd an den Silberb\u00e4chen", "tokens": ["Sie", "la\u00b7gen", "ju\u00b7belnd", "an", "den", "Sil\u00b7ber\u00b7b\u00e4\u00b7chen"], "token_inf