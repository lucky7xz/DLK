{"textgrid.poem.63126": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "20", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin gef\u00fcllt mit giftigen Getr\u00e4nken,", "tokens": ["Ich", "bin", "ge\u00b7f\u00fcllt", "mit", "gif\u00b7ti\u00b7gen", "Ge\u00b7tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich speie Eiter, wenn ich wen besah;", "tokens": ["Ich", "spei\u00b7e", "Ei\u00b7ter", ",", "wenn", "ich", "wen", "be\u00b7sah", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,", "KOUS", "PPER", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich fluche jedem heiligen Hallelujah", "tokens": ["Ich", "flu\u00b7che", "je\u00b7dem", "hei\u00b7li\u00b7gen", "Hal\u00b7le\u00b7lu\u00b7jah"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und will ein Pestgewand als frohe Fahne schwenken.", "tokens": ["Und", "will", "ein", "Pest\u00b7ge\u00b7wand", "als", "fro\u00b7he", "Fah\u00b7ne", "schwen\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ART", "NN", "KOUS", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich stehle Geld wie Sand \u2013", "tokens": ["Ich", "steh\u00b7le", "Geld", "wie", "Sand", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KOKOM", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich werfe Brand ins Land,", "tokens": ["Ich", "wer\u00b7fe", "Brand", "ins", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und dennoch, Wolke, wagst du dich zu schenken?", "tokens": ["Und", "den\u00b7noch", ",", "Wol\u00b7ke", ",", "wagst", "du", "dich", "zu", "schen\u00b7ken", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich bin verbittert und mit Gram verschlossen,", "tokens": ["Ich", "bin", "ver\u00b7bit\u00b7tert", "und", "mit", "Gram", "ver\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nur ein Messer \u00f6ffnete mein Herz.", "tokens": ["Und", "nur", "ein", "Mes\u00b7ser", "\u00f6ff\u00b7ne\u00b7te", "mein", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Faul stinkt mein Atem, meine Faust ist Erz,", "tokens": ["Faul", "stinkt", "mein", "A\u00b7tem", ",", "mei\u00b7ne", "Faust", "ist", "Erz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich schlafe selig in verdreckten Gossen,", "tokens": ["Ich", "schla\u00b7fe", "se\u00b7lig", "in", "ver\u00b7dreck\u00b7ten", "Gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich reite nackt auf ungez\u00e4hmten Rossen,", "tokens": ["Ich", "rei\u00b7te", "nackt", "auf", "un\u00b7ge\u00b7z\u00e4hm\u00b7ten", "Ros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin bei Spiel und Wein", "tokens": ["Ich", "bin", "bei", "Spiel", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Allein und ganz allein", "tokens": ["Al\u00b7lein", "und", "ganz", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KON", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Und von den Tr\u00e4nen fremder Fraun umflossen.", "tokens": ["Und", "von", "den", "Tr\u00e4\u00b7nen", "frem\u00b7der", "Fraun", "um\u00b7flos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O m\u00f6cht ich einmal nicht als Licht mehr scheinen!", "tokens": ["O", "m\u00f6cht", "ich", "ein\u00b7mal", "nicht", "als", "Licht", "mehr", "schei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "PPER", "ADV", "PTKNEG", "KOUS", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und nicht mehr Stunde sein und Zeit der Nacht!", "tokens": ["Und", "nicht", "mehr", "Stun\u00b7de", "sein", "und", "Zeit", "der", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIAT", "NN", "VAINF", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich habe meinen Sohn zu Tod gebracht;", "tokens": ["Ich", "ha\u00b7be", "mei\u00b7nen", "Sohn", "zu", "Tod", "ge\u00b7bracht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich h\u00fcllte seine Gliederchen in Hemdenleinen,", "tokens": ["Ich", "h\u00fcll\u00b7te", "sei\u00b7ne", "Glie\u00b7der\u00b7chen", "in", "Hem\u00b7den\u00b7lei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich grub ein Grab ihm unter Pflastersteinen \u2013", "tokens": ["Ich", "grub", "ein", "Grab", "ihm", "un\u00b7ter", "Pflas\u00b7ter\u00b7stei\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O Wolke, wer du seist,", "tokens": ["O", "Wol\u00b7ke", ",", "wer", "du", "seist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ich gr\u00fc\u00dfe deinen Geist,", "tokens": ["Ich", "gr\u00fc\u00b7\u00dfe", "dei\u00b7nen", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "m