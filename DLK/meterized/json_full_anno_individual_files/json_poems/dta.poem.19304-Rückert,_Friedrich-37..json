{"dta.poem.19304": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn sein Gott\u00e4hnliches du willst dem Menschen zeigen,", "tokens": ["Wenn", "sein", "Got\u00b7t\u00e4hn\u00b7li\u00b7ches", "du", "willst", "dem", "Men\u00b7schen", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So darfst du ihm auch nicht sein Thierisches verschweigen.", "tokens": ["So", "darfst", "du", "ihm", "auch", "nicht", "sein", "Thie\u00b7ri\u00b7sches", "ver\u00b7schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Gef\u00e4hrlich ist es, ihn bewundern sich zu lassen;", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "ist", "es", ",", "ihn", "be\u00b7wun\u00b7dern", "sich", "zu", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "PPER", "VVFIN", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gef\u00e4hrlich auch, ihn nur zu zwingen, sich zu hassen.", "tokens": ["Ge\u00b7f\u00e4hr\u00b7lich", "auch", ",", "ihn", "nur", "zu", "zwin\u00b7gen", ",", "sich", "zu", "has\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Auffordern mu\u00dft du ihn, sich selber zu bekriegen,", "tokens": ["Auf\u00b7for\u00b7dern", "mu\u00dft", "du", "ihn", ",", "sich", "sel\u00b7ber", "zu", "be\u00b7krie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um durch sein Besseres sein Schlechteres zu besiegen.", "tokens": ["Um", "durch", "sein", "Bes\u00b7se\u00b7res", "sein", "Schlech\u00b7te\u00b7res", "zu", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}}}}}