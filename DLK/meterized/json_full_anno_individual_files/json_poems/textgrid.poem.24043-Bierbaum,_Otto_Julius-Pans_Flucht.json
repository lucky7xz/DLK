{"textgrid.poem.24043": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Pans Flucht", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gr\u00fcn umbuscht und bunt umbl\u00fcht,", "tokens": ["Gr\u00fcn", "um\u00b7buscht", "und", "bunt", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mittagssonnen\u00fcbergl\u00fcht,", "tokens": ["Mit\u00b7tags\u00b7son\u00b7nen\u00b7\u00fc\u00b7berg\u00b7l\u00fcht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Inselheckensicher sitzt", "tokens": ["In\u00b7sel\u00b7he\u00b7cken\u00b7si\u00b7cher", "sitzt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pan und schnitzt.", "tokens": ["Pan", "und", "schnitzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Schnitzt aus Fliederholze sich", "tokens": ["Schnitzt", "aus", "Flie\u00b7der\u00b7hol\u00b7ze", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Fl\u00f6te meisterlich;", "tokens": ["Ei\u00b7ne", "Fl\u00f6\u00b7te", "meis\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er setzt sie an den Bart", "tokens": ["Und", "er", "setzt", "sie", "an", "den", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliederzart.", "tokens": ["Flie\u00b7derz\u00b7art", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Zierlich, sacht,", "tokens": ["Zier\u00b7lich", ",", "sacht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und er lacht:", "tokens": ["Und", "er", "lacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Blas ich damit auf dem See", "tokens": ["Blas", "ich", "da\u00b7mit", "auf", "dem", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Nacht,", "tokens": ["In", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Wird den wackern Dichtern weh", "tokens": ["Wird", "den", "wa\u00b7ckern", "Dich\u00b7tern", "weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Nacht.", "tokens": ["In", "der", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Blas ich damit s\u00fc\u00df am Tage,", "tokens": ["Blas", "ich", "da\u00b7mit", "s\u00fc\u00df", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach!", "tokens": ["Ach", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Weck ich ihnen Dichterklage,", "tokens": ["Weck", "ich", "ih\u00b7nen", "Dich\u00b7ter\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach!", "tokens": ["Ach", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wehe, weh mir armem Pan,", "tokens": ["We\u00b7he", ",", "weh", "mir", "ar\u00b7mem", "Pan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich thu ist mi\u00dfgethan,", "tokens": ["Was", "ich", "thu", "ist", "mi\u00df\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn, dieweil ich schlief, indessen", "tokens": ["Denn", ",", "die\u00b7weil", "ich", "schlief", ",", "in\u00b7des\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Haben sie es ganz vergessen,", "tokens": ["Ha\u00b7ben", "sie", "es", "ganz", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie sichs lacht.", "tokens": ["Wie", "sichs", "lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Leise fl\u00f6tet er. Das klingt,", "tokens": ["Lei\u00b7se", "fl\u00f6\u00b7tet", "er", ".", "Das", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie wenn zwischen frischem Moose", "tokens": ["Wie", "wenn", "zwi\u00b7schen", "fri\u00b7schem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ueber Kiesel, glatte gro\u00dfe,", "tokens": ["Ue\u00b7ber", "Kie\u00b7sel", ",", "glat\u00b7te", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine helle Quelle springt.", "tokens": ["Ei\u00b7ne", "hel\u00b7le", "Quel\u00b7le", "springt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie des blauen Flieders Duft", "tokens": ["Wie", "des", "blau\u00b7en", "Flie\u00b7ders", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwebt dies T\u00f6nen durch die Luft,", "tokens": ["Schwebt", "dies", "T\u00f6\u00b7nen", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Voll und lind.", "tokens": ["Voll", "und", "lind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Und die Fl\u00f6te h\u00f6rt ein Kind,", "tokens": ["Und", "die", "Fl\u00f6\u00b7te", "h\u00f6rt", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Das im Busche Blumen brach.", "tokens": ["Das", "im", "Bu\u00b7sche", "Blu\u00b7men", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es geht dem Klange nach,", "tokens": ["Und", "es", "geht", "dem", "Klan\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herzgeschwind.", "tokens": ["Herz\u00b7ge\u00b7schwind", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dachte hier sich ganz allein,", "tokens": ["Dach\u00b7te", "hier", "sich", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nun fl\u00f6tet Einer,", "tokens": ["Und", "nun", "fl\u00f6\u00b7tet", "Ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wer mag dieser Fl\u00f6ter sein?", "tokens": ["Wer", "mag", "die\u00b7ser", "Fl\u00f6\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So wie der kanns Keiner,", "tokens": ["So", "wie", "der", "kanns", "Kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "VMFIN", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Keiner, den sie je geh\u00f6rt;", "tokens": ["Kei\u00b7ner", ",", "den", "sie", "je", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ach, sie ist ganz tonbeth\u00f6rt,", "tokens": ["Ach", ",", "sie", "ist", "ganz", "ton\u00b7be\u00b7th\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und ihr Herz schl\u00e4gt schnelle.", "tokens": ["Und", "ihr", "Herz", "schl\u00e4gt", "schnel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Sicher, gar ein sch\u00f6ner Mann", "tokens": ["Si\u00b7cher", ",", "gar", "ein", "sch\u00f6\u00b7ner", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ist, der also fl\u00f6ten kann,", "tokens": ["Ist", ",", "der", "al\u00b7so", "fl\u00f6\u00b7ten", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und ein junger Geselle.", "tokens": ["Und", "ein", "jun\u00b7ger", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.7": {"line.1": {"text": "Und sie sch\u00fcrzt sich hoch den Rock,", "tokens": ["Und", "sie", "sch\u00fcrzt", "sich", "hoch", "den", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgt dem Klange immer zu,", "tokens": ["Folgt", "dem", "Klan\u00b7ge", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Busch durch \u00fcber Stein und Stock;", "tokens": ["Busch", "durch", "\u00fc\u00b7ber", "Stein", "und", "Stock", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein doch, hu!:", "tokens": ["Nein", "doch", ",", "hu", "!", ":"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKANT", "ADV", "$,", "ITJ", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der da fl\u00f6tet ist ein Bock!", "tokens": ["Der", "da", "fl\u00f6\u00b7tet", "ist", "ein", "Bock", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "VAFIN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Himmel, ach, wie sieht der aus!", "tokens": ["Him\u00b7mel", ",", "ach", ",", "wie", "sieht", "der", "aus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PWAV", "VVFIN", "ART", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Braune Haare, dick und kraus,", "tokens": ["Brau\u00b7ne", "Haa\u00b7re", ",", "dick", "und", "kraus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um und um;", "tokens": ["Um", "und", "um", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "KON", "KOUI", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und die Nase, und die Beine,", "tokens": ["Und", "die", "Na\u00b7se", ",", "und", "die", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sind krumm!", "tokens": ["Die", "sind", "krumm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Hat ein Wackelschw\u00e4nzchen gar", "tokens": ["Hat", "ein", "Wa\u00b7ckel\u00b7schw\u00e4nz\u00b7chen", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und zwei H\u00f6rner, wunderbar!", "tokens": ["Und", "zwei", "H\u00f6r\u00b7ner", ",", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber Kleider keine.", "tokens": ["A\u00b7ber", "Klei\u00b7der", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Und sie lacht und lacht und lacht,", "tokens": ["Und", "sie", "lacht", "und", "lacht", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis ihr Thr\u00e4nen rinnen.", "tokens": ["Bis", "ihr", "Thr\u00e4\u00b7nen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Pan ist aus dem Lied erwacht,", "tokens": ["Pan", "ist", "aus", "dem", "Lied", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er flieht von hinnen.", "tokens": ["Und", "er", "flieht", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Flieht in tiefste Einsamkeit,", "tokens": ["Flieht", "in", "tiefs\u00b7te", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschensicher, menschenweit.", "tokens": ["Men\u00b7schen\u00b7si\u00b7cher", ",", "men\u00b7schen\u00b7weit", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Gr\u00fcn umbuscht und bunt umbl\u00fcht,", "tokens": ["Gr\u00fcn", "um\u00b7buscht", "und", "bunt", "um\u00b7bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mittagssonnen\u00fcbergl\u00fcht,", "tokens": ["Mit\u00b7tags\u00b7son\u00b7nen\u00b7\u00fc\u00b7berg\u00b7l\u00fcht", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Inselheckensicher sitzt", "tokens": ["In\u00b7sel\u00b7he\u00b7cken\u00b7si\u00b7cher", "sitzt"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Pan und schnitzt.", "tokens": ["Pan", "und", "schnitzt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.12": {"line.1": {"text": "Schnitzt aus Fliederholze sich", "tokens": ["Schnitzt", "aus", "Flie\u00b7der\u00b7hol\u00b7ze", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NN", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Eine Fl\u00f6te meisterlich;", "tokens": ["Ei\u00b7ne", "Fl\u00f6\u00b7te", "meis\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und er setzt sie an den Bart", "tokens": ["Und", "er", "setzt", "sie", "an", "den", "Bart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliederzart.", "tokens": ["Flie\u00b7derz\u00b7art", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.13": {"line.1": {"text": "Zierlich, sacht,", "tokens": ["Zier\u00b7lich", ",", "sacht", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Und er lacht:", "tokens": ["Und", "er", "lacht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Blas ich damit auf dem See", "tokens": ["Blas", "ich", "da\u00b7mit", "auf", "dem", "See"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PAV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "In der Nacht,", "tokens": ["In", "der", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+", "measure": "anapaest.init"}, "line.5": {"text": "Wird den wackern Dichtern weh", "tokens": ["Wird", "den", "wa\u00b7ckern", "Dich\u00b7tern", "weh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "PTKVZ"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Nacht.", "tokens": ["In", "der", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}}, "stanza.14": {"line.1": {"text": "Blas ich damit s\u00fc\u00df am Tage,", "tokens": ["Blas", "ich", "da\u00b7mit", "s\u00fc\u00df", "am", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach!", "tokens": ["Ach", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Weck ich ihnen Dichterklage,", "tokens": ["Weck", "ich", "ih\u00b7nen", "Dich\u00b7ter\u00b7kla\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach!", "tokens": ["Ach", "!"], "token_info": ["word", "punct"], "pos": ["ITJ", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Wehe, weh mir armem Pan,", "tokens": ["We\u00b7he", ",", "weh", "mir", "ar\u00b7mem", "Pan", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Was ich thu ist mi\u00dfgethan,", "tokens": ["Was", "ich", "thu", "ist", "mi\u00df\u00b7ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn, dieweil ich schlief, indessen", "tokens": ["Denn", ",", "die\u00b7weil", "ich", "schlief", ",", "in\u00b7des\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Haben sie es ganz vergessen,", "tokens": ["Ha\u00b7ben", "sie", "es", "ganz", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie sichs lacht.", "tokens": ["Wie", "sichs", "lacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.15": {"line.1": {"text": "Leise fl\u00f6tet er. Das klingt,", "tokens": ["Lei\u00b7se", "fl\u00f6\u00b7tet", "er", ".", "Das", "klingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$.", "PDS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie wenn zwischen frischem Moose", "tokens": ["Wie", "wenn", "zwi\u00b7schen", "fri\u00b7schem", "Moo\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ueber Kiesel, glatte gro\u00dfe,", "tokens": ["Ue\u00b7ber", "Kie\u00b7sel", ",", "glat\u00b7te", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "VVFIN", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine helle Quelle springt.", "tokens": ["Ei\u00b7ne", "hel\u00b7le", "Quel\u00b7le", "springt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie des blauen Flieders Duft", "tokens": ["Wie", "des", "blau\u00b7en", "Flie\u00b7ders", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Schwebt dies T\u00f6nen durch die Luft,", "tokens": ["Schwebt", "dies", "T\u00f6\u00b7nen", "durch", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Voll und lind.", "tokens": ["Voll", "und", "lind", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.16": {"line.1": {"text": "Und die Fl\u00f6te h\u00f6rt ein Kind,", "tokens": ["Und", "die", "Fl\u00f6\u00b7te", "h\u00f6rt", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Das im Busche Blumen brach.", "tokens": ["Das", "im", "Bu\u00b7sche", "Blu\u00b7men", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es geht dem Klange nach,", "tokens": ["Und", "es", "geht", "dem", "Klan\u00b7ge", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Herzgeschwind.", "tokens": ["Herz\u00b7ge\u00b7schwind", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Dachte hier sich ganz allein,", "tokens": ["Dach\u00b7te", "hier", "sich", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PRF", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und nun fl\u00f6tet Einer,", "tokens": ["Und", "nun", "fl\u00f6\u00b7tet", "Ei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIS", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wer mag dieser Fl\u00f6ter sein?", "tokens": ["Wer", "mag", "die\u00b7ser", "Fl\u00f6\u00b7ter", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PDAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So wie der kanns Keiner,", "tokens": ["So", "wie", "der", "kanns", "Kei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "VMFIN", "PIS", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Keiner, den sie je geh\u00f6rt;", "tokens": ["Kei\u00b7ner", ",", "den", "sie", "je", "ge\u00b7h\u00f6rt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Ach, sie ist ganz tonbeth\u00f6rt,", "tokens": ["Ach", ",", "sie", "ist", "ganz", "ton\u00b7be\u00b7th\u00f6rt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Und ihr Herz schl\u00e4gt schnelle.", "tokens": ["Und", "ihr", "Herz", "schl\u00e4gt", "schnel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Sicher, gar ein sch\u00f6ner Mann", "tokens": ["Si\u00b7cher", ",", "gar", "ein", "sch\u00f6\u00b7ner", "Mann"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Ist, der also fl\u00f6ten kann,", "tokens": ["Ist", ",", "der", "al\u00b7so", "fl\u00f6\u00b7ten", "kann", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Und ein junger Geselle.", "tokens": ["Und", "ein", "jun\u00b7ger", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.17": {"line.1": {"text": "Und sie sch\u00fcrzt sich hoch den Rock,", "tokens": ["Und", "sie", "sch\u00fcrzt", "sich", "hoch", "den", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Folgt dem Klange immer zu,", "tokens": ["Folgt", "dem", "Klan\u00b7ge", "im\u00b7mer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Busch durch \u00fcber Stein und Stock;", "tokens": ["Busch", "durch", "\u00fc\u00b7ber", "Stein", "und", "Stock", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Nein doch, hu!:", "tokens": ["Nein", "doch", ",", "hu", "!", ":"], "token_info": ["word", "word", "punct", "word", "punct", "punct"], "pos": ["PTKANT", "ADV", "$,", "ITJ", "$.", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der da fl\u00f6tet ist ein Bock!", "tokens": ["Der", "da", "fl\u00f6\u00b7tet", "ist", "ein", "Bock", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "VAFIN", "ART", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Himmel, ach, wie sieht der aus!", "tokens": ["Him\u00b7mel", ",", "ach", ",", "wie", "sieht", "der", "aus", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ITJ", "$,", "PWAV", "VVFIN", "ART", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Braune Haare, dick und kraus,", "tokens": ["Brau\u00b7ne", "Haa\u00b7re", ",", "dick", "und", "kraus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJD", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Um und um;", "tokens": ["Um", "und", "um", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "KON", "KOUI", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und die Nase, und die Beine,", "tokens": ["Und", "die", "Na\u00b7se", ",", "und", "die", "Bei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die sind krumm!", "tokens": ["Die", "sind", "krumm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Hat ein Wackelschw\u00e4nzchen gar", "tokens": ["Hat", "ein", "Wa\u00b7ckel\u00b7schw\u00e4nz\u00b7chen", "gar"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und zwei H\u00f6rner, wunderbar!", "tokens": ["Und", "zwei", "H\u00f6r\u00b7ner", ",", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "CARD", "NN", "$,", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Aber Kleider keine.", "tokens": ["A\u00b7ber", "Klei\u00b7der", "kei\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NN", "PIAT", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.19": {"line.1": {"text": "Und sie lacht und lacht und lacht,", "tokens": ["Und", "sie", "lacht", "und", "lacht", "und", "lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis ihr Thr\u00e4nen rinnen.", "tokens": ["Bis", "ihr", "Thr\u00e4\u00b7nen", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.20": {"line.1": {"text": "Pan ist aus dem Lied erwacht,", "tokens": ["Pan", "ist", "aus", "dem", "Lied", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und er flieht von hinnen.", "tokens": ["Und", "er", "flieht", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADV", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Flieht in tiefste Einsamkeit,", "tokens": ["Flieht", "in", "tiefs\u00b7te", "Ein\u00b7sam\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Menschensicher, menschenweit.", "tokens": ["Men\u00b7schen\u00b7si\u00b7cher", ",", "men\u00b7schen\u00b7weit", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}