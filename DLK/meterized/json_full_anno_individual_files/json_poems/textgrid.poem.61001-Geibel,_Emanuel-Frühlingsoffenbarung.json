{"textgrid.poem.61001": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsoffenbarung", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kommt her zum Fr\u00fchlingswald, ihr Glaubenslosen!", "tokens": ["Kommt", "her", "zum", "Fr\u00fch\u00b7lings\u00b7wald", ",", "ihr", "Glau\u00b7bens\u00b7lo\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPRART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ist ein Dom, drin pred'gen tausend Zungen;", "tokens": ["Das", "ist", "ein", "Dom", ",", "drin", "pre\u00b7d'\u00b7gen", "tau\u00b7send", "Zun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "CARD", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Seht diese bl\u00fchnden S\u00e4ulen, diese Rosen,", "tokens": ["Seht", "die\u00b7se", "bl\u00fchn\u00b7den", "S\u00e4u\u00b7len", ",", "die\u00b7se", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die lichte W\u00f6lbung, Gr\u00fcn in Gr\u00fcn verschlungen!", "tokens": ["Die", "lich\u00b7te", "W\u00f6l\u00b7bung", ",", "Gr\u00fcn", "in", "Gr\u00fcn", "ver\u00b7schlun\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie Weihrauchswolken steigt der Blumen D\u00fcften,", "tokens": ["Wie", "Weih\u00b7rauchs\u00b7wol\u00b7ken", "steigt", "der", "Blu\u00b7men", "D\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich goldnen Kerzen flammt das Licht der Sonnen,", "tokens": ["Gleich", "gold\u00b7nen", "Ker\u00b7zen", "flammt", "das", "Licht", "der", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Jubelhymnen fluten in den L\u00fcften", "tokens": ["Als", "Ju\u00b7bel\u00b7hym\u00b7nen", "flu\u00b7ten", "in", "den", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Stimmen all von V\u00f6glein, Laub und Bronnen.", "tokens": ["Die", "Stim\u00b7men", "all", "von", "V\u00f6\u00b7glein", ",", "Laub", "und", "Bron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "APPR", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Himmel selbst ist tief herabgesunken,", "tokens": ["Der", "Him\u00b7mel", "selbst", "ist", "tief", "her\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df liebend er der Erde sich verm\u00e4hle;", "tokens": ["Da\u00df", "lie\u00b7bend", "er", "der", "Er\u00b7de", "sich", "ver\u00b7m\u00e4h\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es schauern alle Wesen gottestrunken,", "tokens": ["Es", "schau\u00b7ern", "al\u00b7le", "We\u00b7sen", "got\u00b7tes\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, wie verstockt auch, schauert eure Seele.", "tokens": ["Und", ",", "wie", "ver\u00b7stockt", "auch", ",", "schau\u00b7ert", "eu\u00b7re", "See\u00b7le", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "VVFIN", "ADV", "$,", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und dann sprecht: Nein! Es ist ein hohl Getriebe,", "tokens": ["Und", "dann", "sprecht", ":", "Nein", "!", "Es", "ist", "ein", "hohl", "Ge\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$.", "PTKANT", "$.", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Uhrwerk ist's, wir kennen jeden Faden!", "tokens": ["Ein", "Uhr\u00b7werk", "ist's", ",", "wir", "ken\u00b7nen", "je\u00b7den", "Fa\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PPER", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sprecht: Nein! zu diesem \u00dcberma\u00df der Liebe,", "tokens": ["Sprecht", ":", "Nein", "!", "zu", "die\u00b7sem", "\u00dc\u00b7berm\u00b7a\u00df", "d