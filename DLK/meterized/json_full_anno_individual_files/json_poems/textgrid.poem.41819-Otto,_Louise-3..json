{"textgrid.poem.41819": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O sch\u00f6nes Leben, das der Liebe Bande", "tokens": ["O", "sch\u00f6\u00b7nes", "Le\u00b7ben", ",", "das", "der", "Lie\u00b7be", "Ban\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um mich mit allen ihren Zaubern wob!", "tokens": ["Um", "mich", "mit", "al\u00b7len", "ih\u00b7ren", "Zau\u00b7bern", "wob", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein trauter Arm mich in den Himmel hob", "tokens": ["Ein", "trau\u00b7ter", "Arm", "mich", "in", "den", "Him\u00b7mel", "hob"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und Herz an Herz im s\u00fc\u00dfen Feuer brannte.", "tokens": ["Und", "Herz", "an", "Herz", "im", "s\u00fc\u00b7\u00dfen", "Feu\u00b7er", "brann\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ja! Liebe wird zum Himmels Unterpfande!", "tokens": ["Ja", "!", "Lie\u00b7be", "wird", "zum", "Him\u00b7mels", "Un\u00b7ter\u00b7pfan\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct