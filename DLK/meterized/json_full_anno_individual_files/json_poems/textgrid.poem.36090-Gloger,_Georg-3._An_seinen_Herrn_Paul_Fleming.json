{"textgrid.poem.36090": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "3. An seinen Herrn Paul Fleming", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie ohne Mutter Blut Minerva sey empfangen", "tokens": ["Wie", "oh\u00b7ne", "Mut\u00b7ter", "Blut", "Mi\u00b7ner\u00b7va", "sey", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NN", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "In Jupiters Gehirn' \u2013 vnd wie es zugegangen", "tokens": ["In", "Ju\u00b7pi\u00b7ters", "Ge\u00b7hirn'", "\u2013", "vnd", "wie", "es", "zu\u00b7ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "$(", "KON", "PWAV", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als Jungfraw Danae die g\u00fcldnen Tropfen fing'", "tokens": ["Als", "Jung\u00b7fraw", "Da\u00b7nae", "die", "g\u00fcld\u00b7nen", "Trop\u00b7fen", "fing'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "NE", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "In ihre zarte Scho\u00df, davon sie schwanger ging,", "tokens": ["In", "ih\u00b7re", "zar\u00b7te", "Scho\u00df", ",", "da\u00b7von", "sie", "schwan\u00b7ger", "ging", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "PAV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und ihren Perseus bracht, \u2013 auch wie zum Lorberbaume", "tokens": ["Und", "ih\u00b7ren", "Per\u00b7seus", "bracht", ",", "\u2013", "auch", "wie", "zum", "Lor\u00b7ber\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "$(", "ADV", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Dafne worden sey, vnd wie aus einem Schaume", "tokens": ["Die", "Daf\u00b7ne", "wor\u00b7den", "sey", ",", "vnd", "wie", "aus", "ei\u00b7nem", "Schau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NE", "VAPP", "VAFIN", "$,", "KON", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das geile Venus-Weib in kalter Meeresscho\u00df", "tokens": ["Das", "gei\u00b7le", "Ve\u00b7nus\u00b7Weib", "in", "kal\u00b7ter", "Mee\u00b7res\u00b7scho\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Empfangen vnd geborn, ja wie sie Segel blo\u00df", "tokens": ["Emp\u00b7fan\u00b7gen", "vnd", "ge\u00b7born", ",", "ja", "wie", "sie", "Se\u00b7gel", "blo\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "$,", "ADV", "KOKOM", "PPER", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd ohne Schiff darzu vf eine Muschel kommen", "tokens": ["Vnd", "oh\u00b7ne", "Schiff", "dar\u00b7zu", "vf", "ei\u00b7ne", "Mu\u00b7schel", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "PAV", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vnd ihre Reyse hab' vf Cypern zugenommen, \u2013", "tokens": ["Vnd", "ih\u00b7re", "Rey\u00b7se", "hab'", "vf", "Cy\u00b7pern", "zu\u00b7ge\u00b7nom\u00b7men", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "VVPP", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Auch wie das Bacchusva\u00df, der Gott der Schlemmerey", "tokens": ["Auch", "wie", "das", "Bac\u00b7chus\u00b7va\u00df", ",", "der", "Gott", "der", "Schlem\u00b7me\u00b7rey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "$,", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von Mans- vnd Weibsperson zweymal geboren sey, \u2013", "tokens": ["Von", "Mans", "vnd", "Weibs\u00b7per\u00b7son", "zwey\u00b7mal", "ge\u00b7bo\u00b7ren", "sey", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "ADV", "VVPP", "VAFIN", "$,", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Vnd wie der Jupiter sey aus der G\u00f6tter Orden", "tokens": ["Vnd", "wie", "der", "Ju\u00b7pi\u00b7ter", "sey", "aus", "der", "G\u00f6t\u00b7ter", "Or\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Zu Nie\u00dfung seiner Lust zu einem Stiere worden, \u2013", "tokens": ["Zu", "Nie\u00b7\u00dfung", "sei\u00b7ner", "Lust", "zu", "ei\u00b7nem", "Stie\u00b7re", "wor\u00b7den", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "APPR", "ART", "NN", "VAPP", "$,", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Das findet man hier nicht: von solcher Eitelkeit", "tokens": ["Das", "fin\u00b7det", "man", "hier", "nicht", ":", "von", "sol\u00b7cher", "Ei\u00b7tel\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIS", "ADV", "PTKNEG", "$.", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd blindem Fabelwerk' ist deine Muse weit.", "tokens": ["Vnd", "blin\u00b7dem", "Fa\u00b7bel\u00b7werk'", "ist", "dei\u00b7ne", "Mu\u00b7se", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Denn was gleich Pindus selbst vnd Ph\u0153bus Leyer klinget,", "tokens": ["Denn", "was", "gleich", "Pin\u00b7dus", "selbst", "vnd", "Ph\u0153\u00b7bus", "Le\u00b7yer", "klin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "NE", "ADV", "KON", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Vnd aller Musen Schaar vfs lieblichste drein singet,", "tokens": ["Vnd", "al\u00b7ler", "Mu\u00b7sen", "Schaar", "vfs", "lieb\u00b7lichs\u00b7te", "drein", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Wie s\u00fc\u00df es immer t\u00f6nt, jedoch es wenig haft,", "tokens": ["Wie", "s\u00fc\u00df", "es", "im\u00b7mer", "t\u00f6nt", ",", "je\u00b7doch", "es", "we\u00b7nig", "haft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ADV", "VVFIN", "$,", "ADV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wenns nicht belebet wird von einer h\u00f6hern Kraft.", "tokens": ["Wenns", "nicht", "be\u00b7le\u00b7bet", "wird", "von", "ei\u00b7ner", "h\u00f6\u00b7hern", "Kraft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVFIN", "VAFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der Naso, die Syren' vnd Wunder der Poeten,", "tokens": ["Der", "Na\u00b7so", ",", "die", "Sy\u00b7ren'", "vnd", "Wun\u00b7der", "der", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Singt sch\u00f6n' vnd meisterlich von Lieb' vnd Liebesn\u00f6ten,", "tokens": ["Singt", "sch\u00f6n'", "vnd", "meis\u00b7ter\u00b7lich", "von", "Lieb'", "vnd", "Lie\u00b7bes\u00b7n\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Doch ist es nur ein Schall, der nur den l\u00fcstern Sinn", "tokens": ["Doch", "ist", "es", "nur", "ein", "Schall", ",", "der", "nur", "den", "l\u00fcs\u00b7tern", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Vnd schn\u00f6den Vorwitz speist, vnd sonst f\u00e4hrt vberhin,", "tokens": ["Vnd", "schn\u00f6\u00b7den", "Vor\u00b7witz", "speist", ",", "vnd", "sonst", "f\u00e4hrt", "vberh\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,", "KON", "ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "Ja eine Zauberey, die vnsern Sinn bek\u00e4mpfet", "tokens": ["Ja", "ei\u00b7ne", "Zau\u00b7be\u00b7rey", ",", "die", "vn\u00b7sern", "Sinn", "be\u00b7k\u00e4mp\u00b7fet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Vnd wie ein schwarzer Rauch vnd dicker Nebel d\u00e4mpfet,", "tokens": ["Vnd", "wie", "ein", "schwar\u00b7zer", "Rauch", "vnd", "di\u00b7cker", "Ne\u00b7bel", "d\u00e4mp\u00b7fet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Da\u00df er nicht \u00fcber sich zu Gott vnd Himmel an", "tokens": ["Da\u00df", "er", "nicht", "\u00fc\u00b7ber", "sich", "zu", "Gott", "vnd", "Him\u00b7mel", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR", "PRF", "APPR", "NN", "KON", "NN", "PTKVZ"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Vor ihrer schweren Dunst im Geiste kommen kan.", "tokens": ["Vor", "ih\u00b7rer", "schwe\u00b7ren", "Dunst", "im", "Geis\u00b7te", "kom\u00b7men", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Drumb lestu billich stehn solch' vnbeseelte Lieder", "tokens": ["Drumb", "le\u00b7stu", "bil\u00b7lich", "stehn", "solch'", "vn\u00b7be\u00b7seel\u00b7te", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ADJD", "VVFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vnd setzest dich dort hin in Stall zur Krippen nieder,", "tokens": ["Vnd", "set\u00b7zest", "dich", "dort", "hin", "in", "Stall", "zur", "Krip\u00b7pen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPR", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die zwar von Ansehn schlecht, doch mehr am Werthe h\u00e4lt,", "tokens": ["Die", "zwar", "von", "An\u00b7sehn", "schlecht", ",", "doch", "mehr", "am", "Wert\u00b7he", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$,", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Als nicht erkaufen kan die ganze weite Welt.", "tokens": ["Als", "nicht", "er\u00b7kau\u00b7fen", "kan", "die", "gan\u00b7ze", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "VVINF", "VMFIN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Darumb du Lorbeerlaub vnd frischen Eppich f\u00fchrest", "tokens": ["Da\u00b7rumb", "du", "Lor\u00b7beer\u00b7laub", "vnd", "fri\u00b7schen", "Ep\u00b7pich", "f\u00fch\u00b7rest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "NN", "KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vnd sie mit allerley Gebl\u00fcm' vnd Gr\u00fcnem zierest,", "tokens": ["Vnd", "sie", "mit", "al\u00b7ler\u00b7ley", "Ge\u00b7bl\u00fcm'", "vnd", "Gr\u00fc\u00b7nem", "zie\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PIAT", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Daraus das liebe Kind bey rawer kalter Nacht", "tokens": ["Da\u00b7raus", "das", "lie\u00b7be", "Kind", "bey", "ra\u00b7wer", "kal\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "So lieblich blicken thut, da\u00df alls f\u00fcr Frewde lacht.", "tokens": ["So", "lieb\u00b7lich", "bli\u00b7cken", "thut", ",", "da\u00df", "alls", "f\u00fcr", "Frew\u00b7de", "lacht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VVFIN", "$,", "KOUS", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nun wol, du machst es gut. Was deine Musa singet,", "tokens": ["Nun", "wol", ",", "du", "machst", "es", "gut", ".", "Was", "dei\u00b7ne", "Mu\u00b7sa", "sin\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$.", "PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nicht nur allein im Stall' vnd vmb die Krippe klinget:", "tokens": ["Nicht", "nur", "al\u00b7lein", "im", "Stall'", "vnd", "vmb", "die", "Krip\u00b7pe", "klin\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "APPRART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Der Schall bricht h\u00f6her sich, f\u00e4hrt vber allen Neid,", "tokens": ["Der", "Schall", "bricht", "h\u00f6\u00b7her", "sich", ",", "f\u00e4hrt", "vber", "al\u00b7len", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PRF", "$,", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.40": {"text": "Vnd machet dich bekant der grawen Ewigkeit.", "tokens": ["Vnd", "ma\u00b7chet", "dich", "be\u00b7kant", "der", "gra\u00b7wen", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie ohne Mutter Blut Minerva sey empfangen", "tokens": ["Wie", "oh\u00b7ne", "Mut\u00b7ter", "Blut", "Mi\u00b7ner\u00b7va", "sey", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "NN", "NE", "VAFIN", "VVPP"], "meter": "-+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "In Jupiters Gehirn' \u2013 vnd wie es zugegangen", "tokens": ["In", "Ju\u00b7pi\u00b7ters", "Ge\u00b7hirn'", "\u2013", "vnd", "wie", "es", "zu\u00b7ge\u00b7gan\u00b7gen"], "tok