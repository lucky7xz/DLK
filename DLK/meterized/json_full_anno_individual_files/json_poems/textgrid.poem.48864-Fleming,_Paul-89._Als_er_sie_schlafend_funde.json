{"textgrid.poem.48864": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "89. Als er sie schlafend funde", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt das sch\u00f6ne Kind in ihrer s\u00fc\u00dfen Ruh,", "tokens": ["Hier", "liegt", "das", "sch\u00f6\u00b7ne", "Kind", "in", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sie bl\u00e4st die sch\u00f6ne Luft, von welcher ich mich qu\u00e4le,", "tokens": ["sie", "bl\u00e4st", "die", "sch\u00f6\u00b7ne", "Luft", ",", "von", "wel\u00b7cher", "ich", "mich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bis an die Seele selbst durch ihre s\u00fc\u00dfe Kehle,", "tokens": ["bis", "an", "die", "See\u00b7le", "selbst", "durch", "ih\u00b7re", "s\u00fc\u00b7\u00dfe", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "hier liegt das sch\u00f6ne Kind und hat die Augen zu.", "tokens": ["hier", "liegt", "das", "sch\u00f6\u00b7ne", "Kind", "und", "hat", "die", "Au\u00b7gen", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "KON", "VAFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Streu Rosen um sie her, du sanfter Zephyr, du,", "tokens": ["Streu", "Ro\u00b7sen", "um", "sie", "her", ",", "du", "sanf\u00b7ter", "Ze\u00b7phyr", ",", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPER", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "mit Nelken untermengt, da\u00df ihr Geruch verm\u00e4le", "tokens": ["mit", "Nel\u00b7ken", "un\u00b7ter\u00b7mengt", ",", "da\u00df", "ihr", "Ge\u00b7ruch", "ver\u00b7m\u00e4\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "mit ihrem Atem sich, dieweil ich leise stehle", "tokens": ["mit", "ih\u00b7rem", "A\u00b7tem", "sich", ",", "die\u00b7weil", "ich", "lei\u00b7se", "steh\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so manchen Ku\u00df von ihr. Silenus sprich kein Muh!", "tokens": ["so", "man\u00b7chen", "Ku\u00df", "von", "ihr", ".", "Si\u00b7le\u00b7nus", "sprich", "kein", "Muh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPOSAT", "$.", "NE", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "St! Satyr, weg, Sylvan! Geht weit von diesem Bache,", "tokens": ["St", "!", "Sa\u00b7tyr", ",", "weg", ",", "Syl\u00b7van", "!", "Geht", "weit", "von", "die\u00b7sem", "Ba\u00b7che", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "NE", "$,", "PTKVZ", "$,", "NE", "$.", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da\u00df meine Seele nicht von eurer Stimm' erwache.", "tokens": ["da\u00df", "mei\u00b7ne", "See\u00b7le", "nicht", "von", "eu\u00b7rer", "Stimm'", "er\u00b7wa\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Klitscht in die H\u00e4nde nicht, ihr schlipfrigen Napeen.", "tokens": ["Klitscht", "in", "die", "H\u00e4n\u00b7de", "nicht", ",", "ihr", "schlipf\u00b7ri\u00b7gen", "Na\u00b7peen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PTKNEG", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Schlaf, Schatz, ich h\u00fcte dein Schlaf, bis du selbst erwachest,", "tokens": ["Schlaf", ",", "Schatz", ",", "ich", "h\u00fc\u00b7te", "dein", "Schlaf", ",", "bis", "du", "selbst", "er\u00b7wa\u00b7chest", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "so wirst du wachend tun, was du im Schlafe machest.", "tokens": ["so", "wirst", "du", "wa\u00b7chend", "tun", ",", "was", "du", "im", "Schla\u00b7fe", "ma\u00b7chest", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir auch tr\u00e4umt itzt mit dir, als solt ich vor dir stehn.", "tokens": ["Mir", "auch", "tr\u00e4umt", "itzt", "mit", "dir", ",", "als", "solt", "ich", "vor", "dir", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPR", "PPER", "$,", "KOUS", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hier liegt das sch\u00f6ne Kind in ihrer s\u00fc\u00dfen Ruh,", "tokens": ["Hier", "liegt", "das", "sch\u00f6\u00b7ne", "Kind", "in", "ih\u00b7rer", "s\u00fc\u00b7\u00dfen", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "sie bl\u00e4st die sch\u00f6ne Luft, von welcher ich mich qu\u00e4le,", "tokens": ["sie", "bl\u00e4st", "die", "sch\u00f6\u00b7ne", "Luft", ",", "von", "wel\u00b7cher", "ich", "mich", "qu\u00e4\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bis an die Seele selbst durch ihre s\u00fc\u00dfe Kehle,", "tokens": ["bis", "an", "die", "See\u00b7le", "selbst", "durch", "