{"dta.poem.13587": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Dichterfr\u00fchling .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die B\u00e4ume lieblich rauschen,", "tokens": ["Wenn", "die", "B\u00e4u\u00b7me", "lieb\u00b7lich", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An den Bergen, an den Seen,", "tokens": ["An", "den", "Ber\u00b7gen", ",", "an", "den", "Seen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die im Sonnenscheine stehen,", "tokens": ["Die", "im", "Son\u00b7nen\u00b7schei\u00b7ne", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Warme Regen niederrauschen,", "tokens": ["War\u00b7me", "Re\u00b7gen", "nie\u00b7der\u00b7rau\u00b7schen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Mag ich gern begeistert lauschen.", "tokens": ["Mag", "ich", "gern", "be\u00b7geis\u00b7tert", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVPP", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Denn um die erfrischten H\u00fcgel", "tokens": ["Denn", "um", "die", "er\u00b7frischten", "H\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf und nieder sich bewegen", "tokens": ["Auf", "und", "nie\u00b7der", "sich", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "PTKVZ", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "F\u00fchl' ich Winde, Gottes Fl\u00fcgel,", "tokens": ["F\u00fchl'", "ich", "Win\u00b7de", ",", "Got\u00b7tes", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$,", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und mir selber wachsen Fl\u00fcgel,", "tokens": ["Und", "mir", "sel\u00b7ber", "wach\u00b7sen", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Athm' ich still den neuen Segen.", "tokens": ["Athm'", "ich", "still", "den", "neu\u00b7en", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie der Kranke von der Schwelle", "tokens": ["Wie", "der", "Kran\u00b7ke", "von", "der", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Endlich wieder in die warme", "tokens": ["End\u00b7lich", "wie\u00b7der", "in", "die", "war\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Luft hinausstreckt Brust und Arme,", "tokens": ["Luft", "hin\u00b7aus\u00b7streckt", "Brust", "und", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es sp\u00fclt des Lebens Welle", "tokens": ["Und", "es", "sp\u00fclt", "des", "Le\u00b7bens", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Fort die Glieder in das Helle:", "tokens": ["Fort", "die", "Glie\u00b7der", "in", "das", "Hel\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Also kommt ein neues Leben", "tokens": ["Al\u00b7so", "kommt", "ein", "neu\u00b7es", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oft auf mich herab vom Himmel,", "tokens": ["Oft", "auf", "mich", "her\u00b7ab", "vom", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und ich seh' vor mir mein Streben", "tokens": ["Und", "ich", "seh'", "vor", "mir", "mein", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Licht und unverg\u00e4nglich schweben", "tokens": ["Licht", "und", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "schwe\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Durch des Lebens bunt Gewimmel.", "tokens": ["Durch", "des", "Le\u00b7bens", "bunt", "Ge\u00b7wim\u00b7mel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will erquickt nun alles prangen,", "tokens": ["Will", "er\u00b7quickt", "nun", "al\u00b7les", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVPP", "ADV", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Irrt der Dichter durch die Schatten,", "tokens": ["Irrt", "der", "Dich\u00b7ter", "durch", "die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch die blumenreichen Matten,", "tokens": ["Durch", "die", "blu\u00b7men\u00b7rei\u00b7chen", "Mat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Denkt der Zeiten, die vergangen,", "tokens": ["Denkt", "der", "Zei\u00b7ten", ",", "die", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ferner Freunde voll Verlangen,", "tokens": ["Fer\u00b7ner", "Freun\u00b7de", "voll", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Und es weben sich die Tr\u00e4ume", "tokens": ["Und", "es", "we\u00b7ben", "sich", "die", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie von selbst zum Werk der Musen,", "tokens": ["Wie", "von", "selbst", "zum", "Werk", "der", "Mu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und rings Berge, Blumen, B\u00e4ume", "tokens": ["Und", "rings", "Ber\u00b7ge", ",", "Blu\u00b7men", ",", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "ADV", "NN", "$,", "NN", "$,", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Wachsen in die heitern R\u00e4ume", "tokens": ["Wach\u00b7sen", "in", "die", "hei\u00b7tern", "R\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nach der Melodie im Busen.", "tokens": ["Nach", "der", "Me\u00b7lo\u00b7die", "im", "Bu\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}