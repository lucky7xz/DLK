{"textgrid.poem.43897": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Geliebtes Kind, der sch\u00f6ne T\u00e4uber,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geliebtes Kind, der sch\u00f6ne T\u00e4uber,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", ",", "der", "sch\u00f6\u00b7ne", "T\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nun mein Nebenbuhler ist,", "tokens": ["Der", "nun", "mein", "Ne\u00b7ben\u00b7buh\u00b7ler", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht, da\u00df der Argwohn mir so Marck als Herze frist.", "tokens": ["Macht", ",", "da\u00df", "der", "Arg\u00b7wohn", "mir", "so", "Marck", "als", "Her\u00b7ze", "frist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "NN", "KOKOM", "VVFIN", "ADJD", "$."], "meter": "+--++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Warum? Ich kenne schon den Unbestand der Weiber,", "tokens": ["Wa\u00b7rum", "?", "Ich", "ken\u00b7ne", "schon", "den", "Un\u00b7be\u00b7stand", "der", "Wei\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die nicht so wohl den Mensch als nur das Wams beschaun", "tokens": ["Die", "nicht", "so", "wohl", "den", "Mensch", "als", "nur", "das", "Wams", "be\u00b7schaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "ART", "NN", "KOKOM", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihres Gl\u00fcckes Grund auf Gold und Silber baun.", "tokens": ["Und", "ih\u00b7res", "Gl\u00fc\u00b7ckes", "Grund", "auf", "Gold", "und", "Sil\u00b7ber", "baun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihr leichter Sinn verschm\u00e4ht die guten K\u00fcnste", "tokens": ["Ihr", "leich\u00b7ter", "Sinn", "ver\u00b7schm\u00e4ht", "die", "gu\u00b7ten", "K\u00fcns\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und stellt die Klugheit und Verstand", "tokens": ["Und", "stellt", "die", "Klug\u00b7heit", "und", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dem Reichthum an die lincke Hand.", "tokens": ["Dem", "Reicht\u00b7hum", "an", "die", "lin\u00b7cke", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Gl\u00e4nzt nur die Brust von einem Wurmgespinste,", "tokens": ["Gl\u00e4nzt", "nur", "die", "Brust", "von", "ei\u00b7nem", "Wurm\u00b7ge\u00b7spins\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Das Farb und Arbeit theuer macht,", "tokens": ["Das", "Farb", "und", "Ar\u00b7beit", "theu\u00b7er", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So wird das Herze nicht bedacht,", "tokens": ["So", "wird", "das", "Her\u00b7ze", "nicht", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PDS", "VVFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Das unter Seid und Sammt oft einen Schalck verheelet;", "tokens": ["Das", "un\u00b7ter", "Seid", "und", "Sammt", "oft", "ei\u00b7nen", "Schalck", "ver\u00b7hee\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ja wiegt der Beuthel nur fein schwer,", "tokens": ["Ja", "wiegt", "der", "Beut\u00b7hel", "nur", "fein", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "ART", "NN", "ADV", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "So wird der Br\u00e4utgam flugs erwehlet,", "tokens": ["So", "wird", "der", "Br\u00e4ut\u00b7gam", "flugs", "er\u00b7weh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und wenn er auch ein Jude w\u00e4r.", "tokens": ["Und", "wenn", "er", "auch", "ein", "Ju\u00b7de", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ach, Olorene, tr\u00f6ste mich!", "tokens": ["Ach", ",", "O\u00b7lo\u00b7re\u00b7ne", ",", "tr\u00f6s\u00b7te", "mich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Du siehst die Furcht, so mich betr\u00fcbet.", "tokens": ["Du", "siehst", "die", "Furcht", ",", "so", "mich", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ich sichre dich,", "tokens": ["Ich", "sich\u00b7re", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Da\u00df meine Redligkeit dir einen Mahlschaz giebet,", "tokens": ["Da\u00df", "mei\u00b7ne", "Red\u00b7lig\u00b7keit", "dir", "ei\u00b7nen", "Mahl\u00b7schaz", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den dieser nie bezahlen kan,", "tokens": ["Den", "die\u00b7ser", "nie", "be\u00b7zah\u00b7len", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der deinen Vater jezt um deine Gunst gesprochen.", "tokens": ["Der", "dei\u00b7nen", "Va\u00b7ter", "jezt", "um", "dei\u00b7ne", "Gunst", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bedencke stets den Eid, den mir dein Mund gethan!", "tokens": ["Be\u00b7den\u00b7cke", "stets", "den", "Eid", ",", "den", "mir", "dein", "Mund", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Liebe falscher Schwur wird warlich auch gerochen.", "tokens": ["Der", "Lie\u00b7be", "fal\u00b7scher", "Schwur", "wird", "war\u00b7lich", "auch", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bl\u00fcht mir das Gl\u00fccke nicht,", "tokens": ["Bl\u00fcht", "mir", "das", "Gl\u00fc\u00b7cke", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Bin ich gedr\u00fcckt und arm und \u00fcberall verla\u00dfen,", "tokens": ["Bin", "ich", "ge\u00b7dr\u00fcckt", "und", "arm", "und", "\u00fc\u00b7be\u00b7rall", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sind jezt der Feinde viel, die meine Warheit ha\u00dfen,", "tokens": ["Sind", "jezt", "der", "Fein\u00b7de", "viel", ",", "die", "mei\u00b7ne", "War\u00b7heit", "ha\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So glaube doch,", "tokens": ["So", "glau\u00b7be", "doch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Der Himmel kan das Joch", "tokens": ["Der", "Him\u00b7mel", "kan", "das", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Dereinst von meinem Halse nehmen", "tokens": ["De\u00b7reinst", "von", "mei\u00b7nem", "Hal\u00b7se", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und meine Wi\u00dfenschaft noch aus dem Staube ziehn.", "tokens": ["Und", "mei\u00b7ne", "Wi\u00b7\u00dfen\u00b7schaft", "noch", "aus", "dem", "Stau\u00b7be", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn m\u00fcste, soltestu mir jezo gleich entfliehn,", "tokens": ["Denn", "m\u00fcs\u00b7te", ",", "sol\u00b7tes\u00b7tu", "mir", "je\u00b7zo", "gleich", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein blinder Selbstbetrug sich seiner Thorheit sch\u00e4men.", "tokens": ["Dein", "blin\u00b7der", "Selbst\u00b7be\u00b7trug", "sich", "sei\u00b7ner", "Thor\u00b7heit", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Geliebtes Kind, der sch\u00f6ne T\u00e4uber,", "tokens": ["Ge\u00b7lieb\u00b7tes", "Kind", ",", "der", "sch\u00f6\u00b7ne", "T\u00e4u\u00b7ber", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der nun mein Nebenbuhler ist,", "tokens": ["Der", "nun", "mein", "Ne\u00b7ben\u00b7buh\u00b7ler", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Macht, da\u00df der Argwohn mir so Marck als Herze frist.", "tokens": ["Macht", ",", "da\u00df", "der", "Arg\u00b7wohn", "mir", "so", "Marck", "als", "Her\u00b7ze", "frist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "PPER", "ADV", "NN", "KOKOM", "VVFIN", "ADJD", "$."], "meter": "+--++--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Warum? Ich kenne schon den Unbestand der Weiber,", "tokens": ["Wa\u00b7rum", "?", "Ich", "ken\u00b7ne", "schon", "den", "Un\u00b7be\u00b7stand", "der", "Wei\u00b7ber", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "a