"-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Und wenn er auch ein Jude w\u00e4r.", "tokens": ["Und", "wenn", "er", "auch", "ein", "Ju\u00b7de", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Ach, Olorene, tr\u00f6ste mich!", "tokens": ["Ach", ",", "O\u00b7lo\u00b7re\u00b7ne", ",", "tr\u00f6s\u00b7te", "mich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "NE", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.18": {"text": "Du siehst die Furcht, so mich betr\u00fcbet.", "tokens": ["Du", "siehst", "die", "Furcht", ",", "so", "mich", "be\u00b7tr\u00fc\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Ich sichre dich,", "tokens": ["Ich", "sich\u00b7re", "dich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Da\u00df meine Redligkeit dir einen Mahlschaz giebet,", "tokens": ["Da\u00df", "mei\u00b7ne", "Red\u00b7lig\u00b7keit", "dir", "ei\u00b7nen", "Mahl\u00b7schaz", "gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Den dieser nie bezahlen kan,", "tokens": ["Den", "die\u00b7ser", "nie", "be\u00b7zah\u00b7len", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der deinen Vater jezt um deine Gunst gesprochen.", "tokens": ["Der", "dei\u00b7nen", "Va\u00b7ter", "jezt", "um", "dei\u00b7ne", "Gunst", "ge\u00b7spro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Bedencke stets den Eid, den mir dein Mund gethan!", "tokens": ["Be\u00b7den\u00b7cke", "stets", "den", "Eid", ",", "den", "mir", "dein", "Mund", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der Liebe falscher Schwur wird warlich auch gerochen.", "tokens": ["Der", "Lie\u00b7be", "fal\u00b7scher", "Schwur", "wird", "war\u00b7lich", "auch", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bl\u00fcht mir das Gl\u00fccke nicht,", "tokens": ["Bl\u00fcht", "mir", "das", "Gl\u00fc\u00b7cke", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Bin ich gedr\u00fcckt und arm und \u00fcberall verla\u00dfen,", "tokens": ["Bin", "ich", "ge\u00b7dr\u00fcckt", "und", "arm", "und", "\u00fc\u00b7be\u00b7rall", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "KON", "ADJD", "KON", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Sind jezt der Feinde viel, die meine Warheit ha\u00dfen,", "tokens": ["Sind", "jezt", "der", "Fein\u00b7de", "viel", ",", "die", "mei\u00b7ne", "War\u00b7heit", "ha\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So glaube doch,", "tokens": ["So", "glau\u00b7be", "doch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Der Himmel kan das Joch", "tokens": ["Der", "Him\u00b7mel", "kan", "das", "Joch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Dereinst von meinem Halse nehmen", "tokens": ["De\u00b7reinst", "von", "mei\u00b7nem", "Hal\u00b7se", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Und meine Wi\u00dfenschaft noch aus dem Staube ziehn.", "tokens": ["Und", "mei\u00b7ne", "Wi\u00b7\u00dfen\u00b7schaft", "noch", "aus", "dem", "Stau\u00b7be", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Denn m\u00fcste, soltestu mir jezo gleich entfliehn,", "tokens": ["Denn", "m\u00fcs\u00b7te", ",", "sol\u00b7tes\u00b7tu", "mir", "je\u00b7zo", "gleich", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "$,", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Dein blinder Selbstbetrug sich seiner Thorheit sch\u00e4men.", "tokens": ["Dein", "blin\u00b7der", "Selbst\u00b7be\u00b7trug", "sich", "sei\u00b7ner", "Thor\u00b7heit", "sch\u00e4\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PRF", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}