{"textgrid.poem.52295": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbden ganzen Dag bit D\u00f6schen stahn", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbden ganzen Dag bit D\u00f6schen stahn", "tokens": ["\u00bb", "den", "gan\u00b7zen", "Dag", "bit", "D\u00f6\u00b7schen", "stahn"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Un \u00fcmmer achtern Haken gahn", "tokens": ["Un", "\u00fcm\u00b7mer", "ach\u00b7tern", "Ha\u00b7ken", "gahn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un up den Acker Kluten pedden", "tokens": ["Un", "up", "den", "A\u00b7cker", "Klu\u00b7ten", "ped\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Un denn mal wedder Ossen ledden", "tokens": ["Un", "denn", "mal", "wed\u00b7der", "Os\u00b7sen", "led\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "ADV", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Un denn mal dr\u00f6g un denn mal natt,", "tokens": ["Un", "denn", "mal", "dr\u00f6g", "un", "denn", "mal", "natt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADV", "VVFIN", "FM", "KON", "ADV", "PTKVZ", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Wo, mine Herrn, gef\u00f6llt Sei dat?", "tokens": ["Wo", ",", "mi\u00b7ne", "Herrn", ",", "ge\u00b7f\u00f6llt", "Sei", "dat", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJA", "NN", "$,", "VVPP", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Un denn? Wat krig ick denn f\u00f6r Eten?", "tokens": ["Un", "denn", "?", "Wat", "krig", "ick", "denn", "f\u00f6r", "E\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "$.", "NN", "ADJD", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.8": {"text": "So slicht un man so'n l\u00fcttes beten!", "tokens": ["So", "slicht", "un", "man", "so'n", "l\u00fct\u00b7tes", "be\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "FM", "FM", "FM", "FM", "VVINF", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}, "line.9": {"text": "Un \u00fcmmer T\u00fcften, \u00fcmmer R\u00e4uwen,", "tokens": ["Un", "\u00fcm\u00b7mer", "T\u00fcf\u00b7ten", ",", "\u00fcm\u00b7mer", "R\u00e4u\u00b7wen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dor mag de Deuwel l\u00e4nger t\u00e4uwen!", "tokens": ["Dor", "mag", "de", "Deu\u00b7wel", "l\u00e4n\u00b7ger", "t\u00e4u\u00b7wen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "NE", "NE", "ADJD", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Ne, Herrn! Denn hett 'ne Ul dor seten,", "tokens": ["Ne", ",", "Herrn", "!", "Denn", "hett", "'ne", "Ul", "dor", "se\u00b7ten", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$.", "KON", "VAFIN", "ART", "NE", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Bi dr\u00f6ge T\u00fcften kann ick nich bestahn.\u00ab", "tokens": ["Bi", "dr\u00f6\u00b7ge", "T\u00fcf\u00b7ten", "kann", "ick", "nich", "be\u00b7stahn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "\u00bbna gaud, min S\u00e4hn, du kannst nu gahn\u00ab,", "tokens": ["\u00bb", "na", "gaud", ",", "min", "S\u00e4hn", ",", "du", "kannst", "nu", "gahn", "\u00ab", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ITJ", "VVFIN", "$,", "PPOSAT", "NN", "$,", "PPER", "VMFIN", "ADV", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Seggt de Burmeister. \u2013 As hei 'rut,", "tokens": ["Seggt", "de", "Burm\u00b7eis\u00b7ter", ".", "\u2013", "As", "hei", "'rut", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$.", "$(", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.15": {"text": "Seggt hei tau'n Ratsherrn Wohlgemuth:", "tokens": ["Seggt", "hei", "tau'n", "Rats\u00b7herrn", "Wohl\u00b7ge\u00b7muth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "\u00bbich glaube fast, und mir will es bed\u00fcnken:", "tokens": ["\u00bb", "ich", "glau\u00b7be", "fast", ",", "und", "mir", "will", "es", "be\u00b7d\u00fcn\u00b7ken", ":"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ADV", "$,", "KON", "PPER", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Der Mensch hat recht;", "tokens": ["Der", "Mensch", "hat", "recht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Denn solches Essen, solches Trinken", "tokens": ["Denn", "sol\u00b7ches", "Es\u00b7sen", ",", "sol\u00b7ches", "Trin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Pa\u00dft sich nicht f\u00fcr en t\u00fccht'gen Knecht.", "tokens": ["Pa\u00dft", "sich", "nicht", "f\u00fcr", "en", "t\u00fccht'\u00b7gen", "Knecht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PTKNEG", "APPR", "NE", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.20": {"text": "Indessen woll'n den Herrn doch auch mal fragen.", "tokens": ["In\u00b7des\u00b7sen", "woll'n", "den", "Herrn", "doch", "auch", "mal", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Heda! raupt mal den B\u00e4cker Hagen.\u00ab", "tokens": ["He\u00b7da", "!", "raupt", "mal", "den", "B\u00e4\u00b7cker", "Ha\u00b7gen", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$.", "VVFIN", "ADV", "ART", "NN", "NE", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.22": {"text": "De B\u00e4cker k\u00fcmmt. \u00bbMein lieber Meister Hagen,", "tokens": ["De", "B\u00e4\u00b7cker", "k\u00fcmmt", ".", "\u00bb", "Mein", "lie\u00b7ber", "Meis\u00b7ter", "Ha\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Ihr Knecht war hier und t\u00e4t sich sehr beklagen:", "tokens": ["Ihr", "Knecht", "war", "hier", "und", "t\u00e4t", "sich", "sehr", "be\u00b7kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "KON", "VVFIN", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Sein Essen sei so schlecht.", "tokens": ["Sein", "Es\u00b7sen", "sei", "so", "schlecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Wir fragen nun: Wie halten Sie den Knecht?\u00ab", "tokens": ["Wir", "fra\u00b7gen", "nun", ":", "Wie", "hal\u00b7ten", "Sie", "den", "Knecht", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PWAV", "VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "\u00bbna, dat m\u00f6t ick gestahn!", "tokens": ["\u00bb", "na", ",", "dat", "m\u00f6t", "ick", "ge\u00b7stahn", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Dor m\u00f6t doch glik dat Wetter 'rinne slahn!", "tokens": ["Dor", "m\u00f6t", "doch", "glik", "dat", "Wet\u00b7ter", "'rin\u00b7ne", "slahn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "ART", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Dat h\u00fcrt ick niemals all min Dag!", "tokens": ["Dat", "h\u00fcrt", "ick", "nie\u00b7mals", "all", "min", "Dag", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Ick holl min L\u00fcd so slicht?", "tokens": ["Ick", "holl", "min", "L\u00fcd", "so", "slicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.30": {"text": "Antwurten S' blot up dese Frag':", "tokens": ["Ant\u00b7wur\u00b7ten", "S'", "blot", "up", "de\u00b7se", "Frag'", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.31": {"text": "Rindfleisch un Plummen, is't en slicht Gericht?\u00ab", "tokens": ["Rind\u00b7fleisch", "un", "Plum\u00b7men", ",", "is't", "en", "slicht", "Ge\u00b7richt", "?", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "FM", "NN", "$,", "FM", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "\u00bbden Dunner ok! Dat is en pr\u00e4chtig Eten!", "tokens": ["\u00bb", "den", "Dun\u00b7ner", "ok", "!", "Dat", "is", "en", "pr\u00e4ch\u00b7tig", "E\u00b7ten", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$.", "FM", "FM", "FM", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Rindfleisch un Plummen? Is't nich wohr?", "tokens": ["Rind\u00b7fleisch", "un", "Plum\u00b7men", "?", "Is't", "nich", "wohr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "FM", "NN", "$.", "NE", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Herr Ratsherr, ne! Wo wull'n wi dorin freten!\u00ab", "tokens": ["Herr", "Rats\u00b7herr", ",", "ne", "!", "Wo", "wull'n", "wi", "do\u00b7rin", "fre\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "NE", "$.", "PWAV", "VVFIN", "KOKOM", "ADV", "VVFIN", "$.", "$("], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.35": {"text": "\u00bbja\u00ab, seggt de Ratsherr, \u00bbdat's gewi\u00df!", "tokens": ["\u00bb", "ja", "\u00ab", ",", "seggt", "de", "Rats\u00b7herr", ",", "\u00bb", "dat's", "ge\u00b7wi\u00df", "!"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PTKANT", "$(", "$,", "VVFIN", "NE", "NN", "$,", "$(", "NE", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Wenn't Rindfleisch m\u00f6r un wenn de Plummen gor,", "tokens": ["Wenn't", "Rind\u00b7fleisch", "m\u00f6r", "un", "wenn", "de", "Plum\u00b7men", "gor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "FM", "FM", "KOUS", "NE", "NE", "NE", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.37": {"text": "Denn wull ick meinen, dat't wat Sch\u00f6nes is.\u00ab", "tokens": ["Denn", "wull", "ick", "mei\u00b7nen", ",", "dat't", "wat", "Sch\u00f6\u00b7nes", "is", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VMFIN", "PPER", "VVFIN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "\u00bbdat is en Eten, as in'n Himmel\u00ab,", "tokens": ["\u00bb", "dat", "is", "en", "E\u00b7ten", ",", "as", "in'n", "Him\u00b7mel", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "$,", "FM", "FM", "NN", "$(", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Seggt de Burmeister, \u00bbt\u00e4uw, du L\u00fcmmel!", "tokens": ["Seggt", "de", "Burm\u00b7eis\u00b7ter", ",", "\u00bb", "t\u00e4uw", ",", "du", "L\u00fcm\u00b7mel", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,", "$(", "NE", "$,", "PPER", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.40": {"text": "Ick ward en W\u00fcrdken mit di spreken", "tokens": ["Ick", "ward", "en", "W\u00fcrd\u00b7ken", "mit", "di", "spre\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "APPR", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Un di de Leckert\u00e4hn utbreken.", "tokens": ["Un", "di", "de", "Le\u00b7cker\u00b7t\u00e4hn", "ut\u00b7bre\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "NE", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.42": {"text": "T\u00e4uw, du Karnalli, ick will di betalen!", "tokens": ["T\u00e4uw", ",", "du", "Kar\u00b7nal\u00b7li", ",", "ick", "will", "di", "be\u00b7ta\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NE", "$,", "PPER", "VMFIN", "NE", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.43": {"text": "Oh, lat't mal glik den Sl\u00fcter halen,", "tokens": ["Oh", ",", "lat't", "mal", "glik", "den", "Sl\u00fc\u00b7ter", "ha\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "De sall em doch up allen F\u00e4llen", "tokens": ["De", "sall", "em", "doch", "up", "al\u00b7len", "F\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "En richtig Dutzend rinne tellen. \u2013", "tokens": ["En", "rich\u00b7tig", "Dut\u00b7zend", "rin\u00b7ne", "tel\u00b7len", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "ADJD", "NN", "VVFIN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "So, mine Herrn, man Platz genamen,", "tokens": ["So", ",", "mi\u00b7ne", "Herrn", ",", "man", "Platz", "ge\u00b7na\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,", "PIS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.47": {"text": "Un lat't den Kirl mal wedder 'rinne kamen! \u2013", "tokens": ["Un", "lat't", "den", "Kirl", "mal", "wed\u00b7der", "'rin\u00b7ne", "ka\u00b7men", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "KON", "NE", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.48": {"text": "Du Sl\u00fcngel! Du entfamtigte Hallunk,", "tokens": ["Du", "Sl\u00fcn\u00b7gel", "!", "Du", "ent\u00b7fam\u00b7tig\u00b7te", "Hal\u00b7lunk", ","], "token_info": ["word", "word", "punct", "