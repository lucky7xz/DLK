{"dta.poem.20742": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Begebenheit zu Wien in der Kaiserlichen  \n Burg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Vom hohen v\u00e4terlichen Throne                 ", "tokens": ["Vom", "ho\u00b7hen", "v\u00e4\u00b7ter\u00b7li\u00b7chen", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kam j\u00fcngst Theresia, warf Brustschmuck hin und Krone", "tokens": ["Kam", "j\u00fcngst", "The\u00b7re\u00b7sia", ",", "warf", "Brust\u00b7schmuck", "hin", "und", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "NE", "$,", "VVFIN", "NN", "PTKVZ", "KON", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Und hing, mit aufgebundnem Zopf,", "tokens": ["Und", "hing", ",", "mit", "auf\u00b7ge\u00b7bund\u00b7nem", "Zopf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bis auf die sch\u00f6ne Brust den gramerf\u00fcllten Kopf,", "tokens": ["Bis", "auf", "die", "sch\u00f6\u00b7ne", "Brust", "den", "gra\u00b7mer\u00b7f\u00fcll\u00b7ten", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und rief: Maria! ach, nun k\u00fcssen wilde Reu\u00dfen", "tokens": ["Und", "rief", ":", "Ma\u00b7ria", "!", "ach", ",", "nun", "k\u00fcs\u00b7sen", "wil\u00b7de", "Reu\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "NE", "$.", "XY", "$,", "ADV", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.6": {"text": "Sich mit den kezrischen mir ganz verha\u00dften Preu\u00dfen!", "tokens": ["Sich", "mit", "den", "kez\u00b7ri\u00b7schen", "mir", "ganz", "ver\u00b7ha\u00df\u00b7ten", "Preu\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Verlohren ist der Krieg, zu meiner Enkel Spott,", "tokens": ["Ver\u00b7loh\u00b7ren", "ist", "der", "Krieg", ",", "zu", "mei\u00b7ner", "En\u00b7kel", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Viel M\u00e4chte halfen mir, und Friedrichen half Gott!", "tokens": ["Viel", "M\u00e4ch\u00b7te", "hal\u00b7fen", "mir", ",", "und", "Fried\u00b7ri\u00b7chen", "half", "Gott", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "$,", "KON", "NN", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}