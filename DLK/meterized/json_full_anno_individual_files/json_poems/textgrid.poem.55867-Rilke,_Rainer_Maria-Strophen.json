{"textgrid.poem.55867": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Strophen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist einer, der nimmt alle in die Hand,", "tokens": ["Ist", "ei\u00b7ner", ",", "der", "nimmt", "al\u00b7le", "in", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df sie wie Sand durch seine Finger rinnen.", "tokens": ["da\u00df", "sie", "wie", "Sand", "durch", "sei\u00b7ne", "Fin\u00b7ger", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er w\u00e4hlt die sch\u00f6nsten aus den K\u00f6niginnen", "tokens": ["Er", "w\u00e4hlt", "die", "sch\u00f6ns\u00b7ten", "aus", "den", "K\u00f6\u00b7ni\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und l\u00e4\u00dft sie sich in wei\u00dfen Marmor hauen,", "tokens": ["und", "l\u00e4\u00dft", "sie", "sich", "in", "wei\u00b7\u00dfen", "Mar\u00b7mor", "hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "still liegend in des Mantels Melodie;", "tokens": ["still", "lie\u00b7gend", "in", "des", "Man\u00b7tels", "Me\u00b7lo\u00b7die", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "und legt die K\u00f6nige zu ihren Frauen,", "tokens": ["und", "legt", "die", "K\u00f6\u00b7ni\u00b7ge", "zu", "ih\u00b7ren", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "gebildet aus dem gleichen Stein wie sie.", "tokens": ["ge\u00b7bil\u00b7det", "aus", "dem", "glei\u00b7chen", "Stein", "wie", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ist einer, der nimmt alle in die Hand,", "tokens": ["Ist", "ei\u00b7ner", ",", "der", "nimmt", "al\u00b7le", "in", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df sie wie schlechte Klingen sind und brechen.", "tokens": ["da\u00df", "sie", "wie", "schlech\u00b7te", "Klin\u00b7gen", "sind", "und", "bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ADJA", "NN", "VAFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er ist kein Fremder, denn er wohnt im Blut,", "tokens": ["Er", "ist", "kein", "Frem\u00b7der", ",", "denn", "er", "wohnt", "im", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "KON", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "das unser Leben ist und rauscht und ruht.", "tokens": ["das", "un\u00b7ser", "Le\u00b7ben", "ist", "und", "rauscht", "und", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "KON", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich kann nicht glauben, da\u00df er Unrecht tut;", "tokens": ["Ich", "kann", "nicht", "glau\u00b7ben", ",", "da\u00df", "er", "Un\u00b7recht", "tut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "doch h\u00f6r ich viele B\u00f6ses von ihm sprechen.", "tokens": ["doch", "h\u00f6r", "ich", "vie\u00b7le", "B\u00f6\u00b7ses", "von", "ihm", "spre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ist einer, der nimmt alle in die Hand,", "tokens": ["Ist", "ei\u00b7ner", ",", "der", "nimmt", "al\u00b7le", "in", "die", "Hand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "$,", "PRELS", "VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "da\u00df sie wie Sand durch seine Finger rinnen.", "tokens": ["da\u00df", "sie", "wie", "Sand", "durch", "sei\u00b7ne", "Fin\u00b7ger", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "w