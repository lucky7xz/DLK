{"textgrid.poem.45958": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Theodor K\u00f6rner an Viktoria", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich wei\u00df es wohl, du hast um mich geweint,", "tokens": ["\u00bb", "ich", "wei\u00df", "es", "wohl", ",", "du", "hast", "um", "mich", "ge\u00b7weint", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es geht die Welt nichts an, du kennst mich gut,", "tokens": ["Es", "geht", "die", "Welt", "nichts", "an", ",", "du", "kennst", "mich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie du mich kennst, so hab' ich es gemeint,", "tokens": ["Wie", "du", "mich", "kennst", ",", "so", "hab'", "ich", "es", "ge\u00b7meint", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit dir, dem Vaterland und meinem Blut,", "tokens": ["Mit", "dir", ",", "dem", "Va\u00b7ter\u00b7land", "und", "mei\u00b7nem", "Blut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Lebenslust hab' ich zur Kunst gestrebt,", "tokens": ["In", "Le\u00b7bens\u00b7lust", "hab'", "ich", "zur", "Kunst", "ge\u00b7strebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der kann nicht dichten, der nicht gerne lebt.", "tokens": ["Der", "kann", "nicht", "dich\u00b7ten", ",", "der", "nicht", "ger\u00b7ne", "lebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ADJA", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du wei\u00dft es wohl, ich habe gern gelebt,", "tokens": ["Du", "wei\u00dft", "es", "wohl", ",", "ich", "ha\u00b7be", "gern", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich war so jung, so fr\u00f6hlich, so gesund,", "tokens": ["Ich", "war", "so", "jung", ",", "so", "fr\u00f6h\u00b7lich", ",", "so", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das Lied, das meiner Lyra kaum entschwebt,", "tokens": ["Das", "Lied", ",", "das", "mei\u00b7ner", "Ly\u00b7ra", "kaum", "ent\u00b7schwebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Trug an der Menschen Herz dein sch\u00f6ner Mund.", "tokens": ["Trug", "an", "der", "Men\u00b7schen", "Herz", "dein", "sch\u00f6\u00b7ner", "Mund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O selig Lied! dem Huld die Seele giebt,", "tokens": ["O", "se\u00b7lig", "Lied", "!", "dem", "Huld", "die", "See\u00b7le", "giebt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$.", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.6": {"text": "Der kann nicht leben, der nicht gerne liebt.", "tokens": ["Der", "kann", "nicht", "le\u00b7ben", ",", "der", "nicht", "ger\u00b7ne", "liebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVINF", "$,", "PRELS", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du wei\u00dft es wohl, ich habe dich geliebt,", "tokens": ["Du", "wei\u00dft", "es", "wohl", ",", "ich", "ha\u00b7be", "dich", "ge\u00b7liebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verzeih, o Liebe, die den Kranz mir wand,", "tokens": ["Ver\u00b7zeih", ",", "o", "Lie\u00b7be", ",", "die", "den", "Kranz", "mir", "wand", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df andre Feier mir den Kranz auch giebt,", "tokens": ["Da\u00df", "and\u00b7re", "Fei\u00b7er", "mir", "den", "Kranz", "auch", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den Eichenkranz das deutsche Vaterland.", "tokens": ["Den", "Ei\u00b7chen\u00b7kranz", "das", "deut\u00b7sche", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Bei einer Eiche senkten sie mich ein,", "tokens": ["Bei", "ei\u00b7ner", "Ei\u00b7che", "senk\u00b7ten", "sie", "mich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der kann nicht lieben, der nicht frei will sein.", "tokens": ["Der", "kann", "nicht", "lie\u00b7ben", ",", "der", "nicht", "frei", "will", "sein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVINF", "$,", "PRELS", "PTKNEG", "ADJD", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Du wei\u00dft es wohl, ich konnte frei nur sein", "tokens": ["Du", "wei\u00dft", "es", "wohl", ",", "ich", "konn\u00b7te", "frei", "nur", "sein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VMFIN", "ADJD", "ADV", "PPOSAT"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mit meines Deutschlands deutscher Kunst und Art,", "tokens": ["Mit", "mei\u00b7nes", "Deutschlands", "deut\u00b7scher", "Kunst", "und", "Art", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und setzte deutsch mein deutsches Leben ein,", "tokens": ["Und", "setz\u00b7te", "deutsch", "mein", "deut\u00b7sches", "Le\u00b7ben", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gleich deutschen Dichtern auf der Ritterfahrt.", "tokens": ["Gleich", "deut\u00b7schen", "Dich\u00b7tern", "auf", "der", "Rit\u00b7ter\u00b7fahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der hat gedichtet nicht, geliebt, gelebt,", "tokens": ["Der", "hat", "ge\u00b7dich\u00b7tet", "nicht", ",", "ge\u00b7liebt", ",", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "PTKNEG", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der kann nicht frei sein, der dem Tod erbebt.", "tokens": ["Der", "kann", "nicht", "frei", "sein", ",", "der", "dem", "Tod", "er\u00b7bebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "ADJD", "VAINF", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du wei\u00dft es wohl, da\u00df gern den Tod ich starb.", "tokens": ["Du", "wei\u00dft", "es", "wohl", ",", "da\u00df", "gern", "den", "Tod", "ich", "starb", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich sah Viktoria dich, und stieg hinab,", "tokens": ["Ich", "sah", "Vik\u00b7to\u00b7ria", "dich", ",", "und", "stieg", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "PPER", "$,", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Leg nun die Kr\u00e4nze all, die ich erwarb,", "tokens": ["Leg", "nun", "die", "Kr\u00e4n\u00b7ze", "all", ",", "die", "ich", "er\u00b7warb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "PIAT", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kunst, Liebe, Leben, Freiheit, auf mein Grab,", "tokens": ["Kunst", ",", "Lie\u00b7be", ",", "Le\u00b7ben", ",", "Frei\u00b7heit", ",", "auf", "mein", "Grab", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "O Epheu, Lorbeer, Myrte, deutsche Eiche,", "tokens": ["O", "E\u00b7pheu", ",", "Lor\u00b7beer", ",", "Myr\u00b7te", ",", "deut\u00b7sche", "Ei\u00b7che", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "NE", "$,", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Singt der Viktoria, was ich verschweige.\u00ab", "tokens": ["Singt", "der", "Vik\u00b7to\u00b7ria", ",", "was", "ich", "ver\u00b7schwei\u00b7ge", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NE", "$,", "PWS", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Sch\u00f6n war sein Tod, ich traure nicht um ihn.", "tokens": ["Sch\u00f6n", "war", "sein", "Tod", ",", "ich", "trau\u00b7re", "nicht", "um", "ihn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Fr\u00fchling k\u00f6mmt, und macht die B\u00e4ume gr\u00fcn,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "k\u00f6mmt", ",", "und", "macht", "die", "B\u00e4u\u00b7me", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Vogel singt, die gr\u00fcnen R\u00e4ume bl\u00fchn,", "tokens": ["Der", "Vo\u00b7gel", "singt", ",", "die", "gr\u00fc\u00b7nen", "R\u00e4u\u00b7me", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Bl\u00fcte f\u00e4llt, die reifen Fr\u00fcchte gl\u00fchn,", "tokens": ["Die", "Bl\u00fc\u00b7te", "f\u00e4llt", ",", "die", "rei\u00b7fen", "Fr\u00fcch\u00b7te", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie bricht der Herbst, die S\u00e4nger weiter ziehn.", "tokens": ["Sie", "bricht", "der", "Herbst", ",", "die", "S\u00e4n\u00b7ger", "wei\u00b7ter", "ziehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Still wird die Welt, es neiget sich der Winter,", "tokens": ["Still", "wird", "die", "Welt", ",", "es", "nei\u00b7get", "sich", "der", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PPER", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Und zu des ew'gen Feuers Licht f\u00fchrt Gott die Kinder!", "tokens": ["Und", "zu", "des", "ew'\u00b7gen", "Feu\u00b7ers", "Licht", "f\u00fchrt", "Gott", "die", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "VVFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "\u00bbich wei\u00df es wohl, du hast um mich geweint,", "tokens": ["\u00bb", "ich", "wei\u00df", "es", "wohl", ",", "du", "hast", "um", "mich", "ge\u00b7weint", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADV", "$,", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es geht die Welt nichts an, du kennst mich gut,", "tokens": ["Es", "geht", "die", "Welt", "nichts", "an", ",", "du", "kennst", "mich", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct",