{"textgrid.poem.50894": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "14", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mich tadelt der Fanatiker, in deinen Armen weich zu ruhn,", "tokens": ["Mich", "ta\u00b7delt", "der", "Fa\u00b7na\u00b7ti\u00b7ker", ",", "in", "dei\u00b7nen", "Ar\u00b7men", "weich", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und heischt, indem zum Streit er eilt, zu l\u00e4rmen und ihm gleichzutun.", "tokens": ["Und", "heischt", ",", "in\u00b7dem", "zum", "Streit", "er", "eilt", ",", "zu", "l\u00e4r\u00b7men", "und", "ihm", "gleich\u00b7zu\u00b7tun", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPRART", "NN", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "KON", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.3": {"text": "In tollen S\u00e4tzen eilt er fort und peitscht die Luft mit seinem Stahl", "tokens": ["In", "tol\u00b7len", "S\u00e4t\u00b7zen", "eilt", "er", "fort", "und", "peitscht", "die", "Luft", "mit", "sei\u00b7nem", "Stahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Und schw\u00f6rt: es g\u00e4b kein gr\u00f6\u00dfer Heil, als auf dem Schlachtfeld bleich zu ruhn!", "tokens": ["Und", "schw\u00f6rt", ":", "es", "g\u00e4b", "kein", "gr\u00f6\u00b7\u00dfer", "Heil", ",", "als", "auf", "dem", "Schlacht\u00b7feld", "bleich", "zu", "ruhn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "PIAT", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.5": {"text": "La\u00df laufen ihn, den N\u00e4rrischen, und k\u00fcsse mich noch hundertmal!", "tokens": ["La\u00df", "lau\u00b7fen", "ihn", ",", "den", "N\u00e4r\u00b7ri\u00b7schen", ",", "und", "k\u00fcs\u00b7se", "mich", "noch", "hun\u00b7dert\u00b7mal", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "VVFIN", "PPER", "$,", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Ich denke doch beizeiten noch ", "tokens": ["Ich", "den\u00b7ke", "doch", "bei\u00b7zei\u00b7ten", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mich tadelt der Fanatiker, in deinen Armen weich zu ruhn,", "tokens": ["Mich", "ta\u00b7delt", "der", "Fa\u00b7na\u00b7ti\u00b7ker", ",", "in", "dei\u00b7nen", "Ar\u00b7men", "weich", "zu", "ruhn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und heischt, indem zum Streit er eilt, zu l\u00e4rmen und ihm gleichzutun.", "toke