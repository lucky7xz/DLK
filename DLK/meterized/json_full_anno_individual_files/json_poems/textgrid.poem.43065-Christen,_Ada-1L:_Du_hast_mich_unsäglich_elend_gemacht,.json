{"textgrid.poem.43065": {"metadata": {"author": {"name": "Christen, Ada", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du hast mich uns\u00e4glich elend gemacht,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du hast mich uns\u00e4glich elend gemacht,", "tokens": ["Du", "hast", "mich", "un\u00b7s\u00e4g\u00b7lich", "e\u00b7lend", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PRF", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und doch, ich kann dich nicht lassen;", "tokens": ["Und", "doch", ",", "ich", "kann", "dich", "nicht", "las\u00b7sen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich liebe dich stets mehr und mehr \u2013", "tokens": ["Ich", "lie\u00b7be", "dich", "stets", "mehr", "und", "mehr", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "KON", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sollte dich endlos hassen.", "tokens": ["Und", "soll\u00b7te", "dich", "end\u00b7los", "has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Mein letzter Stern ging unter,", "tokens": ["Mein", "letz\u00b7ter", "Stern", "ging", "un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "$,"], "meter": "-+-+-+-", "measure": "iambic