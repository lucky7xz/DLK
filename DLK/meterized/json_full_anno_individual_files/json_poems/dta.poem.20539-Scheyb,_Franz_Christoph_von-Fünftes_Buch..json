{"dta.poem.20539": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "F\u00fcnftes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20536-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So bald die Gnade schwieg, so bald war ich erfreut,", "tokens": ["So", "bald", "die", "Gna\u00b7de", "schwieg", ",", "so", "bald", "war", "ich", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dachte: schlie\u00dft man jezt vielleicht den Tu-", "tokens": ["Und", "dach\u00b7te", ":", "schlie\u00dft", "man", "jezt", "viel\u00b7leicht", "den", "Tu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "PIS", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich konnte nicht verstehn, wer \u00fcberwinden solle;", "tokens": ["Ich", "konn\u00b7te", "nicht", "ver\u00b7stehn", ",", "wer", "\u00fc\u00b7berw\u00b7in\u00b7den", "sol\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PWS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wem der erleuchte Krei\u00df das Friese widmen wolle.", "tokens": ["Wem", "der", "er\u00b7leuch\u00b7te", "Krei\u00df", "das", "Frie\u00b7se", "wid\u00b7men", "wol\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So murrt ich mit mir selbst; die Zeit ward mir zu lang,", "tokens": ["So", "murrt", "ich", "mit", "mir", "selbst", ";", "die", "Zeit", "ward", "mir", "zu", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER", "ADV", "$.", "ART", "NN", "VAFIN", "PPER", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich k\u00fcmmerte mich auch nicht viel um dieseu Rang:", "tokens": ["Ich", "k\u00fcm\u00b7mer\u00b7te", "mich", "auch", "nicht", "viel", "um", "die\u00b7seu", "Rang", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKNEG", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Besonders da ich sah, da\u00df alle sich bestreben,", "tokens": ["Be\u00b7son\u00b7ders", "da", "ich", "sah", ",", "da\u00df", "al\u00b7le", "sich", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch allerhand Beweis den Nuzen zu erheben,", "tokens": ["Durch", "al\u00b7ler\u00b7hand", "Be\u00b7weis", "den", "Nu\u00b7zen", "zu", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den ihre Tugend bracht. Die Anzahl war so gro\u00df,", "tokens": ["Den", "ih\u00b7re", "Tu\u00b7gend", "bracht", ".", "Die", "An\u00b7zahl", "war", "so", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$.", "ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So mehr als ich den Schlaf so sehr im Auge f\u00fchlte;", "tokens": ["So", "mehr", "als", "ich", "den", "Schlaf", "so", "sehr", "im", "Au\u00b7ge", "f\u00fchl\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOUS", "PPER", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da\u00df ich den ganzen Saal vor eingebildet hielte.", "tokens": ["Da\u00df", "ich", "den", "gan\u00b7zen", "Saal", "vor", "ein\u00b7ge\u00b7bil\u00b7det", "hiel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Thalia nahm es war; so sagt\u2019 ich: es ist sp\u00e4t,", "tokens": ["Tha\u00b7lia", "nahm", "es", "war", ";", "so", "sagt'", "ich", ":", "es", "ist", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "$.", "PPER", "VAFIN", "ADJD", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.13": {"text": "Wer wei\u00df, wann dieser Streit und Rath zum Ende geht;", "tokens": ["Wer", "wei\u00df", ",", "wann", "die\u00b7ser", "Streit", "und", "Rath", "zum", "En\u00b7de", "geht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "PWAV", "PDAT", "NN", "KON", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ehab noch Geduld und sieh dort jene Tugend an,", "tokens": ["\u201e", "hab", "noch", "Ge\u00b7duld", "und", "sieh", "dort", "je\u00b7ne", "Tu\u00b7gend", "an", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "NN", "KON", "VVIMP", "ADV", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.15": {"text": "\u201evielleicht ists ihr Verdienst, der alles enden kann.", "tokens": ["\u201e", "viel\u00b7leicht", "ists", "ihr", "Ver\u00b7dienst", ",", "der", "al\u00b7les", "en\u00b7den", "kann", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}