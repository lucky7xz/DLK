 "mir", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sonst aber sind sie still wie du.", "tokens": ["Sonst", "a\u00b7ber", "sind", "sie", "still", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und auch wie du vom Lilienkleide", "tokens": ["Und", "auch", "wie", "du", "vom", "Li\u00b7li\u00b7en\u00b7klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOKOM", "PPER", "APPRART", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In tiefem Schlummer zugedeckt,", "tokens": ["In", "tie\u00b7fem", "Schlum\u00b7mer", "zu\u00b7ge\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fern der Welt und allem Leide,", "tokens": ["So", "fern", "der", "Welt", "und", "al\u00b7lem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "KON", "PIS", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von keinem Lebenshauch geweckt.", "tokens": ["Von", "kei\u00b7nem", "Le\u00b7bens\u00b7hauch", "ge\u00b7weckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur etwas schwebt wie sanfte Klage", "tokens": ["Nur", "et\u00b7was", "schwebt", "wie", "sanf\u00b7te", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "w