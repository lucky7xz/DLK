{"textgrid.poem.33621": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Winter im Gebirg", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Verklungen sind die holden Schw\u00fcre,", "tokens": ["Ver\u00b7klun\u00b7gen", "sind", "die", "hol\u00b7den", "Schw\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die hier gar oft der Mond belauscht.", "tokens": ["Die", "hier", "gar", "oft", "der", "Mond", "be\u00b7lauscht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Statt Fl\u00fcstern vor der Kammert\u00fcre,", "tokens": ["Statt", "Fl\u00fcs\u00b7tern", "vor", "der", "Kam\u00b7mer\u00b7t\u00fc\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ist's nur der Brunnen, der da rauscht.", "tokens": ["Ist's", "nur", "der", "Brun\u00b7nen", ",", "der", "da", "rauscht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo keine Sch\u00f6ne kalt geblieben,", "tokens": ["Wo", "kei\u00b7ne", "Sch\u00f6\u00b7ne", "kalt", "ge\u00b7blie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ward ihr gebracht ein Edelwei\u00df,", "tokens": ["Ward", "ihr", "ge\u00b7bracht", "ein", "E\u00b7del\u00b7wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo wir den Kahn ans Land getrieben,", "tokens": ["Wo", "wir", "den", "Kahn", "ans", "Land", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Knarrt n\u00e4chtlich aufgeschreckt das Eis.", "tokens": ["Knarrt", "n\u00e4cht\u00b7lich", "auf\u00b7ge\u00b7schreckt", "das", "Eis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und auch die Felder sind gefroren,", "tokens": ["Und", "auch", "die", "Fel\u00b7der", "sind", "ge\u00b7fro\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Wald, in dem man sich erging,", "tokens": ["Der", "Wald", ",", "in", "dem", "man", "sich", "er\u00b7ging", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRELS", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo man im Pf\u00e4nderspiel verloren", "tokens": ["Wo", "man", "im", "Pf\u00e4n\u00b7der\u00b7spiel", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und einen Ku\u00df daf\u00fcr empfing.", "tokens": ["Und", "ei\u00b7nen", "Ku\u00df", "da\u00b7f\u00fcr", "emp\u00b7fing", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Schnee bedeckt die Spur der Kohlen,", "tokens": ["Der", "Schnee", "be\u00b7deckt", "die", "Spur", "der", "Koh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo Freudenfeuer hell gegl\u00fcht,", "tokens": ["Wo", "Freu\u00b7den\u00b7feu\u00b7er", "hell", "ge\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo Primeln und wo Bergviolen", "tokens": ["Wo", "Pri\u00b7meln", "und", "wo", "Berg\u00b7vi\u00b7o\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "PWAV", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Am sch\u00f6nsten Busen einst gebl\u00fcht.", "tokens": ["Am", "sch\u00f6ns\u00b7ten", "Bu\u00b7sen", "einst", "ge\u00b7bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Fr\u00fchling wird sie wieder bringen,", "tokens": ["Der", "Fr\u00fch\u00b7ling", "wird", "sie", "wie\u00b7der", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bald tost der F\u00f6hn und l\u00f6st den Schnee;", "tokens": ["Bald", "tost", "der", "F\u00f6hn", "und", "l\u00f6st", "den", "Schnee", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur dich h\u00f6rt niemals wieder singen", "tokens": ["Nur", "dich", "h\u00f6rt", "nie\u00b7mals", "wie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das Felstal und der gr\u00fcne See.", "tokens": ["Das", "Fel\u00b7stal", "und", "der", "gr\u00fc\u00b7ne", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "In dieser Berge dunklem Rahmen", "tokens": ["In", "die\u00b7ser", "Ber\u00b7ge", "dunk\u00b7lem", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie schienst du hell das Bild dazu!", "tokens": ["Wie", "schienst", "du", "hell", "das", "Bild", "da\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Echo ruft mir deinen Namen,", "tokens": ["Ihr", "E\u00b7cho", "ruft", "mir", "dei\u00b7nen", "Na\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sonst aber sind sie still wie du.", "tokens": ["Sonst", "a\u00b7ber", "sind", "sie", "still", "wie", "du", "."], "token_info": ["word", "word", "word", "word", "word"