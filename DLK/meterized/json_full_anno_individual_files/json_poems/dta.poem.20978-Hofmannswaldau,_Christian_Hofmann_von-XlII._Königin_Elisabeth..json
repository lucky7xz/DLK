{"dta.poem.20978": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XlII.  \n K\u00f6nigin Elisabeth.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Ich habe Cron und Schwerd doch keinen Mann", "tokens": ["Ich", "ha\u00b7be", "Cron", "und", "Schwerd", "doch", "kei\u00b7nen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "getragen/", "tokens": ["ge\u00b7tra\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Es mag mein K\u00f6nigreich von meinen Thaten sagen.", "tokens": ["Es", "mag", "mein", "K\u00f6\u00b7nig\u00b7reich", "von", "mei\u00b7nen", "Tha\u00b7ten", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Todten reden nicht/ wer h\u00f6rt den faulen Leib?", "tokens": ["Die", "Tod\u00b7ten", "re\u00b7den", "nicht", "/", "wer", "h\u00f6rt", "den", "fau\u00b7len", "Leib", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich sage nichts als di\u00df: Hier ruht ein Englisch", "tokens": ["Ich", "sa\u00b7ge", "nichts", "als", "di\u00df", ":", "Hier", "ruht", "ein", "Eng\u00b7lisch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "PDS", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "(weib.", "tokens": ["(", "weib", "."], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVFIN", "$."], "meter": "+", "measure": "single.up"}}}}}