{"textgrid.poem.47768": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[wenn auf ein Haupt im Staube]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn auf ein Haupt im Staube", "tokens": ["Wenn", "auf", "ein", "Haupt", "im", "Stau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Schwelle der Taberne", "tokens": ["Der", "Schwel\u00b7le", "der", "Ta\u00b7ber\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Dein Auge f\u00e4llt, das stolze,", "tokens": ["Dein", "Au\u00b7ge", "f\u00e4llt", ",", "das", "stol\u00b7ze", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Tritt nicht darauf, ich flehe;", "tokens": ["Tritt", "nicht", "da\u00b7rauf", ",", "ich", "fle\u00b7he", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "PAV", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es ist das Haupt Hafisens,", "tokens": ["Es", "ist", "das", "Haupt", "Ha\u00b7fi\u00b7sens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der seiner selbst nicht Meister;", "tokens": ["Der", "sei\u00b7ner", "selbst", "nicht", "Meis\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADV", "PTKNEG", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ach, m\u00e4\u00dfige der Reden,", "tokens": ["Ach", ",", "m\u00e4\u00b7\u00dfi\u00b7ge", "der", "Re\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "ART", "NN", "$,"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.8": {"text": "Ach, z\u00fcgele der Thaten", "tokens": ["Ach", ",", "z\u00fc\u00b7ge\u00b7le", "der", "Tha\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Entehrende Gewalt!", "tokens": ["E\u00b7nteh\u00b7ren\u00b7de", "Ge\u00b7walt", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+---+", "measure": "dactylic.init"}, "line.10": {"text": "Es bildete, so scheint es,", "tokens": ["Es", "bil\u00b7de\u00b7te", ",", "so", "scheint", "es", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Der Hochgebenedeite,", "tokens": ["Der", "Hoch\u00b7ge\u00b7be\u00b7ne\u00b7dei\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der thut, was ihm gef\u00e4llig,", "tokens": ["Der", "thut", ",", "was", "ihm", "ge\u00b7f\u00e4l\u00b7lig", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Ganz aus Tabernenstaube", "tokens": ["Ganz", "aus", "Ta\u00b7ber\u00b7nen\u00b7stau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.14": {"text": "Dies Wesen ohne Halt.", "tokens": ["Di