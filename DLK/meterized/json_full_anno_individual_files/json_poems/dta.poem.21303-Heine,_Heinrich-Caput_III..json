{"dta.poem.21303": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Caput III.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-30602-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin in diesem langweilgen Nest", "tokens": ["Ich", "bin", "in", "die\u00b7sem", "lang\u00b7weil\u00b7gen", "Nest"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein St\u00fcndchen herumgeschlendert.", "tokens": ["Ein", "St\u00fcnd\u00b7chen", "her\u00b7um\u00b7ge\u00b7schlen\u00b7dert", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Sah wieder preu\u00dfisches Milit\u00e4r,", "tokens": ["Sah", "wie\u00b7der", "preu\u00b7\u00dfi\u00b7sches", "Mi\u00b7li\u00b7t\u00e4r", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat sich nicht sehr ver\u00e4ndert.", "tokens": ["Hat", "sich", "nicht", "sehr", "ver\u00b7\u00e4n\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es sind die grauen M\u00e4ntel noch", "tokens": ["Es", "sind", "die", "grau\u00b7en", "M\u00e4n\u00b7tel", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit dem hohen, rothen Kragen \u2013", "tokens": ["Mit", "dem", "ho\u00b7hen", ",", "ro\u00b7then", "Kra\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "(das Roth bedeutet Franzosenblut,", "tokens": ["(", "das", "Roth", "be\u00b7deu\u00b7tet", "Fran\u00b7zo\u00b7sen\u00b7blut", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sang K\u00f6rner in fr\u00fcheren Tagen.)", "tokens": ["Sang", "K\u00f6r\u00b7ner", "in", "fr\u00fc\u00b7he\u00b7ren", "Ta\u00b7gen", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "NN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.3": {"line.1": {"text": "Noch immer das h\u00f6lzern pedantische Volk,", "tokens": ["Noch", "im\u00b7mer", "das", "h\u00f6l\u00b7zern", "pe\u00b7dan\u00b7ti\u00b7sche", "Volk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Noch immer ein rechter Winkel", "tokens": ["Noch", "im\u00b7mer", "ein", "rech\u00b7ter", "Win\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "In jeder Bewegung, und im Gesicht", "tokens": ["In", "je\u00b7der", "Be\u00b7we\u00b7gung", ",", "und", "im", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "KON", "APPRART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Der eingefrorene D\u00fcnkel.", "tokens": ["Der", "ein\u00b7ge\u00b7fro\u00b7re\u00b7ne", "D\u00fcn\u00b7kel", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Sie stelzen noch immer so steif herum,", "tokens": ["Sie", "stel\u00b7zen", "noch", "im\u00b7mer", "so", "steif", "he\u00b7rum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So kerzengrade geschniegelt,", "tokens": ["So", "ker\u00b7zen\u00b7gra\u00b7de", "ge\u00b7schnie\u00b7gelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Als h\u00e4tten sie verschluckt den Stock", "tokens": ["Als", "h\u00e4t\u00b7ten", "sie", "ver\u00b7schluckt", "den", "Stock"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Womit man sie einst gepr\u00fcgelt.", "tokens": ["Wo\u00b7mit", "man", "sie", "einst", "ge\u00b7pr\u00fc\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}}}}