{"dta.poem.9196": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "3.  Die erste Liebe.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt ich das zuvor bedacht/", "tokens": ["H\u00e4tt", "ich", "das", "zu\u00b7vor", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df ein blickgen solche macht", "tokens": ["Da\u00df", "ein", "blick\u00b7gen", "sol\u00b7che", "macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gegen unsre seelen h\u00e4tte/", "tokens": ["Ge\u00b7gen", "uns\u00b7re", "see\u00b7len", "h\u00e4t\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach/ so gieng ich auffgericht/", "tokens": ["Ach", "/", "so", "gieng", "ich", "auff\u00b7ge\u00b7richt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "ADV", "VVFIN", "PPER", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mein hertze l\u00e4ge nicht/", "tokens": ["Und", "mein", "hert\u00b7ze", "l\u00e4\u00b7ge", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PTKNEG", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "An der strengen liebes-kette.", "tokens": ["An", "der", "stren\u00b7gen", "lie\u00b7bes\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun ist meine freyheit hin/", "tokens": ["Nun", "ist", "mei\u00b7ne", "frey\u00b7heit", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich mu\u00df den armen sinn", "tokens": ["Und", "ich", "mu\u00df", "den", "ar\u00b7men", "sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die eitle sch\u00f6nheit binden:", "tokens": ["An", "die", "eit\u00b7le", "sch\u00f6n\u00b7heit", "bin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meine freude liegt daran/", "tokens": ["Mei\u00b7ne", "freu\u00b7de", "liegt", "da\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PAV", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ob ich bey der liebsten kan", "tokens": ["Ob", "ich", "bey", "der", "liebs\u00b7ten", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe/ gunst und gnade finden.", "tokens": ["Lie\u00b7be", "/", "gunst", "und", "gna\u00b7de", "fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}