}}, "stanza.8": {"line.1": {"text": "Zwar ist manches fein und zierlich,", "tokens": ["Zwar", "ist", "man\u00b7ches", "fein", "und", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Geht in bunten Kleidern gern,", "tokens": ["Geht", "in", "bun\u00b7ten", "Klei\u00b7dern", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dreht und wendet sich manierlich,", "tokens": ["Dreht", "und", "wen\u00b7det", "sich", "ma\u00b7nier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00dft und bittet nur von fern:", "tokens": ["Gr\u00fc\u00dft", "und", "bit\u00b7tet", "nur", "von", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch sind's meistens wilde Knaben,", "tokens": ["Doch", "sin\u00b7d's", "meis\u00b7tens", "wil\u00b7de", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Laufen immer gradezu,", "tokens": ["Lau\u00b7fen", "im\u00b7mer", "gra\u00b7de\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollen Alles sehn und haben,", "tokens": ["Wol\u00b7len", "Al\u00b7les", "sehn", "und", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVINF", "KON", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lassen mir nicht Rast noch Ruh.", "tokens": ["Las\u00b7sen", "mir", "nicht", "Rast", "noch", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Wohl erkenn' ich ihn, den Einen,", "tokens": ["Wohl", "er\u00b7kenn'", "ich", "ihn", ",", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sie Alle mir verf\u00fchrt.", "tokens": ["Der", "sie", "Al\u00b7le", "mir", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromm und artig m\u00f6cht' er scheinen;", "tokens": ["Fromm", "und", "ar\u00b7tig", "m\u00f6cht'", "er", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ich hab' ihn ausgesp\u00fcrt.", "tokens": ["Doch", "ich", "hab'", "ihn", "aus\u00b7ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so voll von b\u00f6sen R\u00e4nken,", "tokens": ["Ach", ",", "so", "voll", "von", "b\u00f6\u00b7sen", "R\u00e4n\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So voll Trug und Lug und List", "tokens": ["So", "voll", "Trug", "und", "Lug", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "KON", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann man keinen sehn noch denken,", "tokens": ["Kann", "man", "kei\u00b7nen", "sehn", "noch", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIAT", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als der Schelm, der Amor, ist.", "tokens": ["Als", "der", "Schelm", ",", "der", "A\u00b7mor", ",", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NE", "$,", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hab' ich doch an manchen Tagen", "tokens": ["Hab'", "ich", "doch", "an", "man\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Liebsten ihn geschickt,", "tokens": ["Zu", "der", "Liebs\u00b7ten", "ihn", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies und jenes ihr zu sagen,", "tokens": ["Dies", "und", "je\u00b7nes", "ihr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mir lang