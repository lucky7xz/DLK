{"textgrid.poem.41888": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Kleine Lieder, geht nur immer,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kleine Lieder, geht nur immer,", "tokens": ["Klei\u00b7ne", "Lie\u00b7der", ",", "geht", "nur", "im\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft die Liebste sch\u00f6n von mir;", "tokens": ["Gr\u00fc\u00dft", "die", "Liebs\u00b7te", "sch\u00f6n", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubt mir, sie verst\u00f6\u00dft euch nimmer,", "tokens": ["Glaubt", "mir", ",", "sie", "ver\u00b7st\u00f6\u00dft", "euch", "nim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt ihr t\u00e4glich auch zu ihr.", "tokens": ["Kommt", "ihr", "t\u00e4g\u00b7lich", "auch", "zu", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn bey mir k\u00f6nnt ihr nicht bleiben,", "tokens": ["Denn", "bey", "mir", "k\u00f6nnt", "ihr", "nicht", "blei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Voll ist schon das ganze Haus,", "tokens": ["Voll", "ist", "schon", "das", "gan\u00b7ze", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die losen Buben treiben", "tokens": ["Und", "die", "lo\u00b7sen", "Bu\u00b7ben", "trei\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Fast mich selbst zur Th\u00fcr hinaus.", "tokens": ["Fast", "mich", "selbst", "zur", "Th\u00fcr", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf den B\u00fcschen, auf den B\u00e4umen", "tokens": ["Auf", "den", "B\u00fc\u00b7schen", ",", "auf", "den", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wachsen sie wie Laub empor,", "tokens": ["Wach\u00b7sen", "sie", "wie", "Laub", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schaun aus allen Bl\u00fcthenkeimen,", "tokens": ["Schaun", "aus", "al\u00b7len", "Bl\u00fc\u00b7then\u00b7kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Fr\u00fchling, bunt hervor;", "tokens": ["Wie", "der", "Fr\u00fch\u00b7ling", ",", "bunt", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ich steh' und gehe, schw\u00e4rmen", "tokens": ["Wo", "ich", "steh'", "und", "ge\u00b7he", ",", "schw\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie in Schaaren hinterdrein.", "tokens": ["Sie", "in", "Schaa\u00b7ren", "hin\u00b7ter\u00b7drein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann bey solchem Kinderl\u00e4rmen", "tokens": ["Kann", "bey", "sol\u00b7chem", "Kin\u00b7der\u00b7l\u00e4r\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wohl ein Mensch vern\u00fcnftig seyn?", "tokens": ["Wohl", "ein", "Mensch", "ver\u00b7n\u00fcnf\u00b7tig", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Zwar ist manches fein und zierlich,", "tokens": ["Zwar", "ist", "man\u00b7ches", "fein", "und", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.2": {"text": "Geht in bunten Kleidern gern,", "tokens": ["Geht", "in", "bun\u00b7ten", "Klei\u00b7dern", "gern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dreht und wendet sich manierlich,", "tokens": ["Dreht", "und", "wen\u00b7det", "sich", "ma\u00b7nier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gr\u00fc\u00dft und bittet nur von fern:", "tokens": ["Gr\u00fc\u00dft", "und", "bit\u00b7tet", "nur", "von", "fern", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ADV", "APPR", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch sind's meistens wilde Knaben,", "tokens": ["Doch", "sin\u00b7d's", "meis\u00b7tens", "wil\u00b7de", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Laufen immer gradezu,", "tokens": ["Lau\u00b7fen", "im\u00b7mer", "gra\u00b7de\u00b7zu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollen Alles sehn und haben,", "tokens": ["Wol\u00b7len", "Al\u00b7les", "sehn", "und", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVINF", "KON", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Lassen mir nicht Rast noch Ruh.", "tokens": ["Las\u00b7sen", "mir", "nicht", "Rast", "noch", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "ADV", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wohl erkenn' ich ihn, den Einen,", "tokens": ["Wohl", "er\u00b7kenn'", "ich", "ihn", ",", "den", "Ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PRELS", "ART", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der sie Alle mir verf\u00fchrt.", "tokens": ["Der", "sie", "Al\u00b7le", "mir", "ver\u00b7f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Fromm und artig m\u00f6cht' er scheinen;", "tokens": ["Fromm", "und", "ar\u00b7tig", "m\u00f6cht'", "er", "schei\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VMFIN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch ich hab' ihn ausgesp\u00fcrt.", "tokens": ["Doch", "ich", "hab'", "ihn", "aus\u00b7ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ach, so voll von b\u00f6sen R\u00e4nken,", "tokens": ["Ach", ",", "so", "voll", "von", "b\u00f6\u00b7sen", "R\u00e4n\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "So voll Trug und Lug und List", "tokens": ["So", "voll", "Trug", "und", "Lug", "und", "List"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "KON", "NE", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kann man keinen sehn noch denken,", "tokens": ["Kann", "man", "kei\u00b7nen", "sehn", "noch", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PIAT", "VVFIN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als der Schelm, der Amor, ist.", "tokens": ["Als", "der", "Schelm", ",", "der", "A\u00b7mor", ",", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NE", "$,", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Hab' ich doch an manchen Tagen", "tokens": ["Hab'", "ich", "doch", "an", "man\u00b7chen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Liebsten ihn geschickt,", "tokens": ["Zu", "der", "Liebs\u00b7ten", "ihn", "ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dies und jenes ihr zu sagen,", "tokens": ["Dies", "und", "je\u00b7nes", "ihr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "PDS", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Was mir lang das Herz gedr\u00fcckt.", "tokens": ["Was", "mir", "lang", "das", "Herz", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gr\u00fc\u00dfend kam er heimgeflogen:", "tokens": ["Gr\u00fc\u00b7\u00dfend", "kam", "er", "heim\u00b7ge\u00b7flo\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Doch zu bald nur sah ich klar,", "tokens": ["Doch", "zu", "bald", "nur", "sah", "ich", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "ADV", "VVFIN", "PPER", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df der Schelm mich doch betrogen", "tokens": ["Da\u00df", "der", "Schelm", "mich", "doch", "be\u00b7tro\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PPER", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und nicht einmal dorten war.", "tokens": ["Und", "nicht", "ein\u00b7mal", "dor\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ADV", "VAFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.6": {"line.1": {"text": "Kleine Lieder, geht nur immer,", "tokens": ["Klei\u00b7ne", "Lie\u00b7der", ",", "geht", "nur", "im\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "ADV", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00fc\u00dft die Liebste sch\u00f6n von mir;", "tokens": ["Gr\u00fc\u00dft", "die", "Liebs\u00b7te", "sch\u00f6n", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Glaubt mir, sie verst\u00f6\u00dft euch nimmer,", "tokens": ["Glaubt", "mir", ",", "sie", "ver\u00b7st\u00f6\u00dft", "euch", "nim\u00b7mer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kommt ihr t\u00e4glich auch