{"dta.poem.2635": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Indianische Tristis oder Trauer-Blume/  \n Bey f. \u00fchzeitigen Erblassen Fr. S. C. P. g. H.  \n entworffen/ den 8. May 1672.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durchaus betr&#252;bter Freund! in dem de\u00df Lentzen\nBilder/", "tokens": ["Durc\u00b7haus", "be\u00b7tr", "&#252;", "bter", "Freund", "!", "in", "dem", "de\u00df", "Lent\u00b7zen", "Bil\u00b7der", "/"], "token_info": ["word", "word", "XML_entity", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$(", "ADJA", "NN", "$.", "APPR", "ART", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Blumen wir erfreut im sch\u00f6nsten Wachsthum", "tokens": ["Die", "Blu\u00b7men", "wir", "er\u00b7freut", "im", "sch\u00f6ns\u00b7ten", "Wach\u00b7sthum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und Florens Eh-", "tokens": ["Und", "Flo\u00b7rens", "Eh"], "token_info": ["word", "word", "word"], "pos": ["KON", "NE", "TRUNC"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Durch einen s\u00fcssen Hauch pflegt freundlich anzuwehn.", "tokens": ["Durch", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Hauch", "pflegt", "freund\u00b7lich", "an\u00b7zu\u00b7wehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da Tulpen und Narci\u00df in glattem Atlas prangen/", "tokens": ["Da", "Tul\u00b7pen", "und", "Nar\u00b7ci\u00df", "in", "glat\u00b7tem", "At\u00b7las", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und sich dem Himmel gleich der Erde Brust besternt/", "tokens": ["Und", "sich", "dem", "Him\u00b7mel", "gleich", "der", "Er\u00b7de", "Brust", "bes\u00b7ternt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "ADV", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ist euer gantzes Haus mit Ach! und Weh! umfangen/", "tokens": ["Ist", "eu\u00b7er", "gant\u00b7zes", "Haus", "mit", "Ach", "!", "und", "Weh", "!", "um\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "ITJ", "$.", "KON", "NN", "$.", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil deine Blume sich den Augen hat entfernt.", "tokens": ["Weil", "dei\u00b7ne", "Blu\u00b7me", "sich", "den", "Au\u00b7gen", "hat", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und sie nicht nur allein. In so gar kurtzen Zeiten/", "tokens": ["Und", "sie", "nicht", "nur", "al\u00b7lein", ".", "In", "so", "gar", "kurt\u00b7zen", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "ADV", "$.", "APPR", "ADV", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Eh\u2019 noch Dianens Licht zum vierdten sich verkehrt/", "tokens": ["Eh'", "noch", "Di\u00b7a\u00b7nens", "Licht", "zum", "vierd\u00b7ten", "sich", "ver\u00b7kehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "NE", "NN", "APPRART", "ADJA", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Must du dem Fleisch und Blut zwey Kinder auch begleiten.", "tokens": ["Must", "du", "dem", "Fleisch", "und", "Blut", "zwey", "Kin\u00b7der", "auch", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "KON", "NN", "CARD", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Fall! der gleich einem Blitz durch Marck und Beine f\u00e4hrt.", "tokens": ["Fall", "!", "der", "gleich", "ei\u00b7nem", "Blitz", "durch", "Marck", "und", "Bei\u00b7ne", "f\u00e4hrt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "ADV", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Gleicht nun dein Zustand jetzt nicht wilden W\u00fcsteneyen/", "tokens": ["Gleicht", "nun", "dein", "Zu\u00b7stand", "jetzt", "nicht", "wil\u00b7den", "W\u00fcs\u00b7te\u00b7ne\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJA", "NN", "$("], "meter": "----+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Wo Angst und Einsamkeit dir eine Wohnung baut/", "tokens": ["Wo", "Angst", "und", "Ein\u00b7sam\u00b7keit", "dir", "ei\u00b7ne", "Woh\u00b7nung", "baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wo alles dich erschreckt/ was vor pflag zu erfreuen/", "tokens": ["Wo", "al\u00b7les", "dich", "er\u00b7schreckt", "/", "was", "vor", "pflag", "zu", "er\u00b7freu\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VVFIN", "$(", "PWS", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "In dem dein Auge nichts als S\u00e4rch und Leichen schaut?", "tokens": ["In", "dem", "dein", "Au\u00b7ge", "nichts", "als", "S\u00e4rch", "und", "Lei\u00b7chen", "schaut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "PIS", "KOKOM", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So gar heist Gottes Spruch dich in der Aschen sitzen/", "tokens": ["So", "gar", "heist", "Got\u00b7tes", "Spruch", "dich", "in", "der", "A\u00b7schen", "sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "NN", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und schenckt des Creutzes Kelch dir wolgemessen ein.", "tokens": ["Und", "schenckt", "des", "Creut\u00b7zes", "Kelch", "dir", "wol\u00b7ge\u00b7mes\u00b7sen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich seh umb deinen Kopff nichts als Cometen blitzen/", "tokens": ["Ich", "seh", "umb", "dei\u00b7nen", "Kopff", "nichts", "als", "Co\u00b7me\u00b7ten", "blit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PIS", "KOKOM", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wen dein Leid nicht r\u00fchrt/ der ist ein Marmelstein.", "tokens": ["Und", "wen", "dein", "Leid", "nicht", "r\u00fchrt", "/", "der", "ist", "ein", "Mar\u00b7mel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$(", "ART", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Der G\u00e4rtner wird betr\u00fcbt wenn sein vertraulich hoffen/", "tokens": ["Der", "G\u00e4rt\u00b7ner", "wird", "be\u00b7tr\u00fcbt", "wenn", "sein", "ver\u00b7trau\u00b7lich", "hof\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "KOUS", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und vieler Stunden Flei\u00df/ ein Blumenst\u00fcck/ verdirbt.", "tokens": ["Und", "vie\u00b7ler", "Stun\u00b7den", "Flei\u00df", "/", "ein", "Blu\u00b7men\u00b7st\u00fcck", "/", "ver\u00b7dirbt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$(", "ART", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dich hat/ ", "tokens": ["Dich", "hat", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VAFIN", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.24": {"text": "Indem dir Stock und Blum an deiner Seite stirbt.", "tokens": ["In\u00b7dem", "dir", "Stock", "und", "Blum", "an", "dei\u00b7ner", "Sei\u00b7te", "stirbt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wo such ich etwas aus/ das solche Wunden heile?", "tokens": ["Wo", "such", "ich", "et\u00b7was", "aus", "/", "das", "sol\u00b7che", "Wun\u00b7den", "hei\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das diesen Seelen-Ri\u00df mit Trost verbinden kan?", "tokens": ["Das", "die\u00b7sen", "See\u00b7len\u00b7Ri\u00df", "mit", "Trost", "ver\u00b7bin\u00b7den", "kan", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PDAT", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Achilles heilte zwar die Sch\u00e4den seiner Pfeile/", "tokens": ["A\u00b7chil\u00b7les", "heil\u00b7te", "zwar", "die", "Sch\u00e4\u00b7den", "sei\u00b7ner", "Pfei\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Hier geht die H\u00fclffe nur des H\u00f6chsten Allmacht an.", "tokens": ["Hier", "geht", "die", "H\u00fclf\u00b7fe", "nur", "des", "H\u00f6chs\u00b7ten", "All\u00b7macht", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Stell alles dem anheim/ der von der ersten Wiegen/", "tokens": ["Stell", "al\u00b7les", "dem", "an\u00b7heim", "/", "der", "von", "der", "ers\u00b7ten", "Wie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "ART", "NE", "$(", "ART", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Bi\u00df auff das kalte Grab uns wunder-heilsam f\u00fchrt.", "tokens": ["Bi\u00df", "auff", "das", "kal\u00b7te", "Grab", "uns", "wun\u00b7der\u00b7heil\u00b7sam", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich wei\u00df/ und will sichs gleich mit der Vernunfft nicht f\u00fcgen/", "tokens": ["Ich", "wei\u00df", "/", "und", "will", "sichs", "gleich", "mit", "der", "Ver\u00b7nunfft", "nicht", "f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "VMFIN", "PIS", "ADV", "APPR", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df doch den sch\u00e4rffsten Kampff die beste Krone ziert.", "tokens": ["Da\u00df", "doch", "den", "sch\u00e4rffs\u00b7ten", "Kampff", "die", "bes\u00b7te", "Kro\u00b7ne", "ziert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die Hand/ so dich jetzt dr\u00fcckt/ die wird dich bald umbarmen/", "tokens": ["Die", "Hand", "/", "so", "dich", "jetzt", "dr\u00fcckt", "/", "die", "wird", "dich", "bald", "um\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "ADV", "VVFIN", "$(", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Gott greiffet/ die ihm lieb/ mit harten Proben an/", "tokens": ["Gott", "greif\u00b7fet", "/", "die", "ihm", "lieb", "/", "mit", "har\u00b7ten", "Pro\u00b7ben", "an", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PRELS", "PPER", "ADJD", "$(", "APPR", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Und in der h\u00f6chsten Noth ist sein geneigt erbarmen/", "tokens": ["Und", "in", "der", "h\u00f6chs\u00b7ten", "Noth", "ist", "sein", "ge\u00b7neigt", "er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ein unbeweglich Fels/ auf den man gr\u00fcnden kan.", "tokens": ["Ein", "un\u00b7be\u00b7weg\u00b7lich", "Fels", "/", "auf", "den", "man", "gr\u00fcn\u00b7den", "kan", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$(", "APPR", "ART", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Mensch beliebe nur was GOtt hat wolgefallen/", "tokens": ["Der", "Mensch", "be\u00b7lie\u00b7be", "nur", "was", "Gott", "hat", "wol\u00b7ge\u00b7fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PWS", "NN", "VAFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Gehorsam ist allhier ein Opffer voller Ruhm/", "tokens": ["Ge\u00b7hor\u00b7sam", "ist", "all\u00b7hier", "ein", "Opf\u00b7fer", "vol\u00b7ler", "Ruhm", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und schmeckt es unserm Mund als bitter Gifft und Gallen/", "tokens": ["Und", "schmeckt", "es", "un\u00b7serm", "Mund", "als", "bit\u00b7ter", "Gifft", "und", "Gal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "KOUS", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So bleibt das Creutze doch der Christen Eigenthum.", "tokens": ["So", "bleibt", "das", "Creut\u00b7ze", "doch", "der", "Chris\u00b7ten", "Ei\u00b7gen\u00b7thum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Jedoch was m\u00fch ich mich den Mittag zuerleuchten?", "tokens": ["Je\u00b7doch", "was", "m\u00fch", "ich", "mich", "den", "Mit\u00b7tag", "zu\u00b7er\u00b7leuch\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "w