{"textgrid.poem.63824": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Johannes Brahms \u2020", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und noch ein glorreich hochgef\u00fcrstet Haupt", "tokens": ["Und", "noch", "ein", "glor\u00b7reich", "hoch\u00b7ge\u00b7f\u00fcrs\u00b7tet", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJD", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat dieser wilde Lenz der Welt geraubt.", "tokens": ["Hat", "die\u00b7ser", "wil\u00b7de", "Lenz", "der", "Welt", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Haupt, das \u00e4u\u00dfrer W\u00fcrden sich entschlug,", "tokens": ["Ein", "Haupt", ",", "das", "\u00e4u\u00df\u00b7rer", "W\u00fcr\u00b7den", "sich", "ent\u00b7schlug", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch in dem Reich der Sch\u00f6nheit Krone trug!", "tokens": ["Doch", "in", "dem", "Reich", "der", "Sch\u00f6n\u00b7heit", "Kro\u00b7ne", "trug", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihm ward in T\u00f6nen wundersamer Art", "tokens": ["Ihm", "ward", "in", "T\u00f6\u00b7nen", "wun\u00b7der\u00b7sa\u00b7mer", "Art"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das tiefste Weltgeheimnis offenbart,", "tokens": ["Das", "tiefs\u00b7te", "Welt\u00b7ge\u00b7heim\u00b7nis", "of\u00b7fen\u00b7bart", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und was zu seiner lauschenden Seele drang,", "tokens": ["Und", "was", "zu", "sei\u00b7ner", "lau\u00b7schen\u00b7den", "See\u00b7le", "drang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beseelt ausstr\u00f6men mu\u00dft' es sein Gesang,", "tokens": ["Be\u00b7seelt", "aus\u00b7str\u00f6\u00b7men", "mu\u00dft'", "es", "sein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Voll heil'gen Tiefsinns, der am stumpfen Ohr", "tokens": ["Voll", "heil'\u00b7gen", "Tief\u00b7sinns", ",", "der", "am", "stump\u00b7fen", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "NN", "$,", "PRELS", "APPRART", "ADJA", "NN"], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der flachen Menge freilich sich verlor,", "tokens": ["Der", "fla\u00b7chen", "Men\u00b7ge", "frei\u00b7lich", "sich", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Da leichtre Kunst am lauten Markt sich bl\u00e4ht", "tokens": ["Da", "leicht\u00b7re", "Kunst", "am", "lau\u00b7ten", "Markt", "sich", "bl\u00e4ht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPRART", "ADJA", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und Z\u00fcgelloses hoch im Preise steht.", "tokens": ["Und", "Z\u00fc\u00b7gel\u00b7lo\u00b7ses", "hoch", "im", "Prei\u00b7se", "steht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Er aber schritt, nicht buhlend um Gewinn", "tokens": ["Er", "a\u00b7ber", "schritt", ",", "nicht", "buh\u00b7lend", "um", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "$,", "PTKNEG", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Gemeinen Ruhms, mit heitrem Mut dahin,", "tokens": ["Ge\u00b7mei\u00b7nen", "Ruhms", ",", "mit", "heit\u00b7rem", "Mut", "da\u00b7hin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Gest\u00e4rkt, beseligt durch der Muse Ku\u00df,", "tokens": ["Ge\u00b7st\u00e4rkt", ",", "be\u00b7se\u00b7ligt", "durch", "der", "Mu\u00b7se", "Ku\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die immer treu blieb seinem Genius,", "tokens": ["Die", "im\u00b7mer", "treu", "blieb", "sei\u00b7nem", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.9": {"line.1": {"text": "Bis dann in S\u00fc\u00dfigkeit und herber Kraft", "tokens": ["Bis", "dann", "in", "S\u00fc\u00b7\u00dfig\u00b7keit", "und", "her\u00b7ber", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Neid besch\u00e4mte seine Meisterschaft", "tokens": ["Den", "Neid", "be\u00b7sch\u00e4m\u00b7te", "sei\u00b7ne", "Meis\u00b7ter\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "Und zu den Treuen, die ihn fr\u00fch erkannt,", "tokens": ["Und", "zu", "den", "Treu\u00b7en", ",", "die", "ihn", "fr\u00fch", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein endlos Volk sein Zauber \u00fcberwand.", "tokens": ["Ein", "end\u00b7los", "Volk", "sein", "Zau\u00b7ber", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So, da die Welt vernahm: nach schwerer Pein", "tokens": ["So", ",", "da", "die", "Welt", "ver\u00b7nahm", ":", "nach", "schwe\u00b7rer", "Pein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging dieser Herrliche zum Frieden ein,", "tokens": ["Ging", "die\u00b7ser", "Herr\u00b7li\u00b7che", "zum", "Frie\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Erscholl nur eine Klage weit ringsum,", "tokens": ["Er\u00b7scholl", "nur", "ei\u00b7ne", "Kla\u00b7ge", "weit", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df nun die goldne S\u00e4ngerlippe stumm.", "tokens": ["Da\u00df", "nun", "die", "gold\u00b7ne", "S\u00e4n\u00b7ger\u00b7lip\u00b7pe", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "O wohl! Der M\u00fcde schl\u00e4ft am stillsten Ort,", "tokens": ["O", "wohl", "!", "Der", "M\u00fc\u00b7de", "schl\u00e4ft", "am", "stills\u00b7ten", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch sein Gesang t\u00f6nt unverg\u00e4nglich fort,", "tokens": ["Doch", "sein", "Ge\u00b7sang", "t\u00f6nt", "un\u00b7ver\u00b7g\u00e4ng\u00b7lich", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "So wie im Weltall, wenn ein Stern versinkt,", "tokens": ["So", "wie", "im", "Wel\u00b7tall", ",", "wenn", "ein", "Stern", "ver\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Licht noch lang sich durch den \u00c4ther schwingt", "tokens": ["Sein", "Licht", "noch", "lang", "sich", "durch", "den", "\u00c4\u00b7ther", "schwingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "PRF", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Und in das Auge, das gen Himmel blickt,", "tokens": ["Und", "in", "das", "Au\u00b7ge", ",", "das", "gen", "Him\u00b7mel", "blickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Noch einen Strahl der Freude niederschickt.", "tokens": ["Noch", "ei\u00b7nen", "Strahl", "der", "Freu\u00b7de", "nie\u00b7der\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Und noch ein glorreich hochgef\u00fcrstet Haupt", "tokens": ["Und", "noch", "ein", "glor\u00b7reich", "hoch\u00b7ge\u00b7f\u00fcrs\u00b7tet", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJD", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Hat dieser wilde Lenz der Welt geraubt.", "tokens": ["Hat", "die\u00b7ser", "wil\u00b7de", "Lenz", "der", "Welt", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "wor