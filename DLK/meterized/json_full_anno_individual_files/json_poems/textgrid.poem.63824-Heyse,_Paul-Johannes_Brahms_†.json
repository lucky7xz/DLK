25": {"line.1": {"text": "Und zu den Treuen, die ihn fr\u00fch erkannt,", "tokens": ["Und", "zu", "den", "Treu\u00b7en", ",", "die", "ihn", "fr\u00fch", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein endlos Volk sein Zauber \u00fcberwand.", "tokens": ["Ein", "end\u00b7los", "Volk", "sein", "Zau\u00b7ber", "\u00fc\u00b7ber\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.26": {"line.1": {"text": "So, da die Welt vernahm: nach schwerer Pein", "tokens": ["So", ",", "da", "die", "Welt", "ver\u00b7nahm", ":", "nach", "schwe\u00b7rer", "Pein"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ging dieser Herrliche zum Frieden ein,", "tokens": ["Ging", "die\u00b7ser", "Herr\u00b7li\u00b7che", "zum", "Frie\u00b7den", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.27": {"line.1": {"text": "Erscholl nur eine Klage weit ringsum,", "tokens": ["Er\u00b7scholl", "nur", "ei\u00b7ne", "Kla\u00b7ge", "weit", "ring\u00b7sum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df nun die goldne S\u00e4ngerlippe stumm.", "tokens": ["Da\u00df", "nun", "die", "gold\u00b7ne", "S\u00e4n\