{"dta.poem.10116": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fcchte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bishero haben wir die Erde blo\u00df allein", "tokens": ["Bis\u00b7he\u00b7ro", "ha\u00b7ben", "wir", "die", "Er\u00b7de", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als eine Wies\u2019 und Bluhmen-Garten,", "tokens": ["Als", "ei\u00b7ne", "Wies'", "und", "Bluh\u00b7men\u00b7Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit vieler Anmuth, angesehn.", "tokens": ["Mit", "vie\u00b7ler", "An\u00b7muth", ",", "an\u00b7ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Runmehro zeigt sie sich nicht minder sch\u00f6n,", "tokens": ["Run\u00b7meh\u00b7ro", "zeigt", "sie", "sich", "nicht", "min\u00b7der", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PRF", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als einen Hof voll B\u00e4um\u2019 und Fr\u00fcchte mancher Arten,", "tokens": ["Als", "ei\u00b7nen", "Hof", "voll", "B\u00e4um'", "und", "Fr\u00fcch\u00b7te", "man\u00b7cher", "Ar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "NN", "KON", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die nicht so gleich (o neues Wunder-Werck,", "tokens": ["Die", "nicht", "so", "gleich", "(", "o", "neu\u00b7es", "Wun\u00b7der\u00b7\u00b7Werck", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ADV", "$(", "FM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So w\u00fcrdig, da\u00df ich es mit Ehr-Furcht merck!)", "tokens": ["So", "w\u00fcr\u00b7dig", ",", "da\u00df", "ich", "es", "mit", "Ehr\u00b7Furcht", "merck", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "PPER", "APPR", "NN", "NE", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Nein, allgemach und Wechsels-weis\u2019 entstehn.", "tokens": ["Nein", ",", "all\u00b7ge\u00b7mach", "und", "Wech\u00b7sels\u00b7weis'", "ent\u00b7stehn", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich sehe solchen Baum, von welchem ieder Ast", "tokens": ["Ich", "se\u00b7he", "sol\u00b7chen", "Baum", ",", "von", "wel\u00b7chem", "ie\u00b7der", "Ast"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "APPR", "PRELS", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gebogen und gekr\u00fcmmt, durch seiner Fr\u00fcchte Last.", "tokens": ["Ge\u00b7bo\u00b7gen", "und", "ge\u00b7kr\u00fcmmt", ",", "durch", "sei\u00b7ner", "Fr\u00fcch\u00b7te", "Last", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,", "APPR", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und deren liebliches Gepr\u00e4nge,", "tokens": ["Und", "de\u00b7ren", "lieb\u00b7li\u00b7ches", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit frohen Blicken, durch Geruch und Farb\u2019, entdecken,", "tokens": ["Mit", "fro\u00b7hen", "Bli\u00b7cken", ",", "durch", "Ge\u00b7ruch", "und", "Fa\u00b7rb'", ",", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wie niedlich s\u00e4urlich s\u00fc\u00df die Fr\u00fcchte, deren Menge", "tokens": ["Wie", "nied\u00b7lich", "s\u00e4ur\u00b7lich", "s\u00fc\u00df", "die", "Fr\u00fcch\u00b7te", ",", "de\u00b7ren", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "ADJD", "ADJD", "ADJD", "ART", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nicht zehlbar, werden schmecken.", "tokens": ["Nicht", "zehl\u00b7bar", ",", "wer\u00b7den", "schme\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mich deucht, es sprech durch seiner Fr\u00fcchte Zier", "tokens": ["Mich", "deucht", ",", "es", "sprech", "durch", "sei\u00b7ner", "Fr\u00fcch\u00b7te", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein solcher Baum zu mir:", "tokens": ["Ein", "sol\u00b7cher", "Baum", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erkenne doch in meiner Pracht,", "tokens": ["Er\u00b7ken\u00b7ne", "doch", "in", "mei\u00b7ner", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gro\u00df die G\u00fcte, Macht und Majest\u00e4t", "tokens": ["Wie", "gro\u00df", "die", "G\u00fc\u00b7te", ",", "Macht", "und", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Desjenigen, der mich f\u00fcr dich gemacht.", "tokens": ["Des\u00b7je\u00b7ni\u00b7gen", ",", "der", "mich", "f\u00fcr", "dich", "ge\u00b7macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "PRELS", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin ja nicht f\u00fcr Jhn so reich, auch nicht f\u00fcr mich,", "tokens": ["Ich", "bin", "ja", "nicht", "f\u00fcr", "Jhn", "so", "reich", ",", "auch", "nicht", "f\u00fcr", "mich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "APPR", "PPER", "ADV", "ADJD", "$,", "ADV", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Jhm fehlet nichts, und ich", "tokens": ["Jhm", "feh\u00b7let", "nichts", ",", "und", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KON", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Kann das, was Er mir gab, nicht brauchen.", "tokens": ["Kann", "das", ",", "was", "Er", "mir", "gab", ",", "nicht", "brau\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "$,", "PRELS", "PPER", "PPER", "VVFIN", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "La\u00df Jhm zu Ehren denn dein Andachts-Opfer rauchen.", "tokens": ["La\u00df", "Jhm", "zu", "Eh\u00b7ren", "denn", "dein", "An\u00b7dachts\u00b7Op\u00b7fer", "rau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und pfl\u00fccke meine Last: danck\u2019 Jhm mit froher Brust,", "tokens": ["Und", "pfl\u00fc\u00b7cke", "mei\u00b7ne", "Last", ":", "danck", "Jhm", "mit", "fro\u00b7her", "Brust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "VVIMP", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und, weil Er mich gemacht zum Werck-Zeug deiner Lust;", "tokens": ["Und", ",", "weil", "Er", "mich", "ge\u00b7macht", "zum", "Wer\u00b7ck\u00b7Zeug", "dei\u00b7ner", "Lust", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "VVPP", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.12": {"text": "So werde du f\u00fcr mich, zu dieser frohen Zeit,", "tokens": ["So", "wer\u00b7de", "du", "f\u00fcr", "mich", ",", "zu", "die\u00b7ser", "fro\u00b7hen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "PPER", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein Werck-Zeug meiner Danckbarkeit.", "tokens": ["Ein", "Wer\u00b7ck\u00b7Zeug", "mei\u00b7ner", "Dan\u00b7ck\u00b7bar\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Es kommt mir vor,", "tokens": ["Es", "kommt", "mir", "vor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Ob h\u00f6re mein vergn\u00fcgtes Ohr,", "tokens": ["Ob", "h\u00f6\u00b7re", "mein", "ver\u00b7gn\u00fcg\u00b7tes", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "So gar von einem ieden Orte,", "tokens": ["So", "gar", "von", "ei\u00b7nem", "ie\u00b7den", "Or\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Dergleichen Lehr-und Anmuth-reiche Worte.", "tokens": ["Derg\u00b7lei\u00b7chen", "Lehr\u00b7\u00b7und", "An\u00b7muth\u00b7rei\u00b7che", "Wor\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Je weiter ich nun ferner gehe,", "tokens": ["Je", "wei\u00b7ter", "ich", "nun", "fer\u00b7ner", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Je mehr entdeck\u2019 ich, h\u00f6re, sehe,", "tokens": ["Je", "mehr", "ent\u00b7deck'", "ich", ",", "h\u00f6\u00b7re", ",", "se\u00b7he", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und treff\u2019 ich neue Vorw\u00fcrff\u2019 an,", "tokens": ["Und", "treff'", "ich", "neu\u00b7e", "Vor\u00b7w\u00fcrff'", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die mich mit neuer Freude r\u00fchren,", "tokens": ["Die", "mich", "mit", "neu\u00b7er", "Freu\u00b7de", "r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die mich im Danck zum Sch\u00f6pfer f\u00fchren,", "tokens": ["Die", "mich", "im", "Danck", "zum", "Sch\u00f6p\u00b7fer", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und die ich nie genug bewundern kann.", "tokens": ["Und", "die", "ich", "nie", "ge\u00b7nug", "be\u00b7wun\u00b7dern", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Bey einem ieden Schritt werd\u2019 ich aufs neue", "tokens": ["Bey", "ei\u00b7nem", "ie\u00b7den", "Schritt", "werd'", "ich", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PIAT", "NN", "VAFIN", "PPER", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ver\u00e4ndrungen gewahr, wor\u00fcber ich mich freue.", "tokens": ["Ver\u00b7\u00e4n\u00b7drun\u00b7gen", "ge\u00b7wahr", ",", "wo\u00b7r\u00fc\u00b7ber", "ich", "mich", "freu\u00b7e", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$,", "PWAV", "PPER", "PRF", "VVFIN", "$."], "meter": "-++--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Hier seh ich, wie die Frucht in einer Rinde steckt:", "tokens": ["Hier", "seh", "ich", ",", "wie", "die", "Frucht", "in", "ei\u00b7ner", "Rin\u00b7de", "steckt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da h\u00e4lt ein sch\u00f6nes Fleisch den Kern verdeckt:", "tokens": ["Da", "h\u00e4lt", "ein", "sch\u00f6\u00b7nes", "Fleisch", "den", "Kern", "ver\u00b7deckt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die dort, ist in und aussen weich:", "tokens": ["Die", "dort", ",", "ist", "in", "und", "aus\u00b7sen", "weich", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "VAFIN", "APPR", "KON", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Frucht ist, vielen andern gleich,", "tokens": ["Die", "Frucht", "ist", ",", "vie\u00b7len", "an\u00b7dern", "gleich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "PIAT", "PIS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus einer Bluhm\u2019 entsprossen: Jene hier", "tokens": ["Aus", "ei\u00b7ner", "Bluhm'", "ent\u00b7spros\u00b7sen", ":", "Je\u00b7ne", "hier"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "PDS", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Kam, sonder Bluhm\u2019 und Bl\u00fcht\u2019, aus hartem Holtz\u2019 herf\u00fcr:", "tokens": ["Kam", ",", "son\u00b7der", "Bluhm'", "und", "Bl\u00fcht'", ",", "aus", "har\u00b7tem", "Holtz'", "her\u00b7f\u00fcr", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KON", "NN", "KON", "NN", "$,", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die kommt, wann sich der Sommer zu uns wendet,", "tokens": ["Die", "kommt", ",", "wann", "sich", "der", "Som\u00b7mer", "zu", "uns", "wen\u00b7det", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und jene, wann er sich geendet.", "tokens": ["Und", "je\u00b7ne", ",", "wann", "er", "sich", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", 