{"dta.poem.6639": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von denen Sinnlichkeiten  \n insgemein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als eine Pflantze nehrt er sich,", "tokens": ["Als", "ei\u00b7ne", "Pflant\u00b7ze", "nehrt", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Zeit die solchen Leib formiret und verbessert,", "tokens": ["Die", "Zeit", "die", "sol\u00b7chen", "Leib", "for\u00b7mi\u00b7ret", "und", "ver\u00b7bes\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIAT", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der durch die Nahrung sich vergr\u00f6ssert,", "tokens": ["Der", "durch", "die", "Nah\u00b7rung", "sich", "ver\u00b7gr\u00f6s\u00b7sert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Adern, Muskeln, Hirn und Hertz, fast sichtbarlich", "tokens": ["Die", "A\u00b7dern", ",", "Mus\u00b7keln", ",", "Hirn", "und", "Hertz", ",", "fast", "sicht\u00b7bar\u00b7lich"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Entwickelt und entdeckt; bem\u00fchen sich zu leben,", "tokens": ["Ent\u00b7wi\u00b7ckelt", "und", "ent\u00b7deckt", ";", "be\u00b7m\u00fc\u00b7hen", "sich", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVPP", "$.", "VVFIN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, nebst der Geistigkeit, dem Blut\u2019 den Lauf zu geben.", "tokens": ["Und", ",", "nebst", "der", "Geis\u00b7tig\u00b7keit", ",", "dem", "Blut'", "den", "Lauf", "zu", "ge\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Es stellt die Frucht forthin kein blo\u00df Gew\u00e4chs mehr f\u00fcr,", "tokens": ["Es", "stellt", "die", "Frucht", "for\u00b7thin", "kein", "blo\u00df", "Ge\u00b7w\u00e4chs", "mehr", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "PIAT", "ADV", "NN", "ADV", "APPR", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie athmet, reget sich, und ist ein Thier.", "tokens": ["Sie", "ath\u00b7met", ",", "re\u00b7get", "sich", ",", "und", "ist", "ein", "Thier", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PRF", "$,", "KON", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "So unterschiedner Ding, verschiedne Eigenschafften", "tokens": ["So", "un\u00b7ter\u00b7schied\u00b7ner", "Ding", ",", "ver\u00b7schied\u00b7ne", "Ei\u00b7gen\u00b7schaff\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Vermengen sich, verbinden sich und hafften", "tokens": ["Ver\u00b7men\u00b7gen", "sich", ",", "ver\u00b7bin\u00b7den", "sich", "und", "haff\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "$,", "VVFIN", "PRF", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "In-und an seinem edlen Wesen.", "tokens": ["In\u00b7\u00b7und", "an", "sei\u00b7nem", "ed\u00b7len", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Jedoch ist es ein Leib aus Elementen nur", "tokens": ["Je\u00b7doch", "ist", "es", "ein", "Leib", "aus", "E\u00b7le\u00b7men\u00b7ten", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "APPR", "NN", "ADV"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Der blo\u00df die Ordnung hat und die Figur,", "tokens": ["Der", "blo\u00df", "die", "Ord\u00b7nung", "hat", "und", "die", "Fi\u00b7gur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VAFIN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Bis ihn, da er zum h\u00f6hern Grad erlesen,", "tokens": ["Bis", "ihn", ",", "da", "er", "zum", "h\u00f6\u00b7hern", "Grad", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Ein Strahl belebt, der aus der GOTTHEJT fleusst.", "tokens": ["Ein", "Strahl", "be\u00b7lebt", ",", "der", "aus", "der", "GoTTHEJT", "fleusst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Mit den Bewegungen verbindet sich der Geist:", "tokens": ["Mit", "den", "Be\u00b7we\u00b7gun\u00b7gen", "ver\u00b7bin\u00b7det", "sich", "der", "Geist", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Damit er noch vollkommner sey; so schencket", "tokens": ["Da\u00b7mit", "er", "noch", "voll\u00b7komm\u00b7ner", "sey", ";", "so", "schen\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$.", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sein SCHOEPFFER ihm den ", "tokens": ["Sein", "ScHOE\u00b7PF\u00b7FER", "ihm", "den"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PPER", "ART"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.19": {"text": "gedencket.", "tokens": ["ge\u00b7den\u00b7cket", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}