{"textgrid.poem.41846": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein leichter Sinn mag oft in neuen Weisen", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein leichter Sinn mag oft in neuen Weisen", "tokens": ["Ein", "leich\u00b7ter", "Sinn", "mag", "oft", "in", "neu\u00b7en", "Wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Lust erh\u00f6hn, die wechselnd ihn begl\u00fcckt;", "tokens": ["Die", "Lust", "er\u00b7h\u00f6hn", ",", "die", "wech\u00b7selnd", "ihn", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "PRELS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Singen soll nur eine Herrin preisen,", "tokens": ["Mein", "Sin\u00b7gen", "soll", "nur", "ei\u00b7ne", "Her\u00b7rin", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die doppelt stets mein zweifelnd Aug' erblickt.", "tokens": ["Die", "dop\u00b7pelt", "stets", "mein", "zwei\u00b7felnd", "Aug'", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "PPOSAT", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dort in des Grabes ewig stummen Kreisen,", "tokens": ["Dort", "in", "des", "Gra\u00b7bes", "e\u00b7wig", "stum\u00b7men", "Krei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hier mit des Lebens frischem Reiz geschm\u00fcckt;", "tokens": ["Hier", "mit", "des", "Le\u00b7bens", "fri\u00b7schem", "Reiz", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ADJA", "NN", "VVPP", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "Und wenn auch hier zwey Namen sie benennen,", "tokens": ["Und", "wenn", "auch", "hier", "zwey", "Na\u00b7men", "sie", "be\u00b7nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ADV", "CARD", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nie kann mein Herz die holden Bilder trennen.", "tokens": ["Nie", "kann", "mein", "Herz", "die", "hol\u00b7den", "Bil\u00b7der", "tren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Denn wie sich Tr\u00e4um' im Leben oft entfalten,", "tokens": ["Denn", "wie", "sich", "Tr\u00e4um'", "im", "Le\u00b7ben", "oft", "ent\u00b7fal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PRF", "NN", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Leben oft in luft'gen Tr\u00e4umen bl\u00fcht,", "tokens": ["Und", "Le\u00b7ben", "oft", "in", "luft'\u00b7gen", "Tr\u00e4u\u00b7men", "bl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So gatten sich die minnigen Gestalten", "tokens": ["So", "gat\u00b7ten", "sich", "die", "min\u00b7ni\u00b7gen", "Ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu ", "tokens": ["Zu"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "In dieser streb' ich Jene festzuhalten,", "tokens": ["In", "die\u00b7ser", "streb'", "ich", "Je\u00b7ne", "fest\u00b7zu\u00b7hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PDAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und w\u00e4hne, da\u00df mit dieser Jene flieht.", "tokens": ["Und", "w\u00e4h\u00b7ne", ",", "da\u00df", "mit", "die\u00b7ser", "Je\u00b7ne", "flieht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "PDAT", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch weil die Eine l\u00e4ngst sich mir entrissen,", "tokens": ["Doch", "weil", "die", "Ei\u00b7ne", "l\u00e4ngst", "sich", "mir", "ent\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "PRF", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mu\u00dft' ich auch stets der Andern Liebe missen.", "tokens": ["Mu\u00dft'", "ich", "auch", "stets", "der", "An\u00b7dern", "Lie\u00b7be", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt hab' ich lange Fahrt f\u00fcr sie begonnen", "tokens": ["Jetzt", "hab'", "ich", "lan\u00b7ge", "Fahrt", "f\u00fcr", "sie", "be\u00b7gon\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "APPR", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um ihren Preis auch ferne zu erh\u00f6hn;", "tokens": ["Um", "ih\u00b7ren", "Preis", "auch", "fer\u00b7ne", "zu", "er\u00b7h\u00f6hn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Zwey Jahre schon sind fl\u00fcchtig mir verronnen,", "tokens": ["Zwey", "Jah\u00b7re", "schon", "sind", "fl\u00fcch\u00b7tig", "mir", "ver\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VAFIN", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Seit ich zuletzt den Heimathsstrand gesehn.", "tokens": ["Seit", "ich", "zu\u00b7letzt", "den", "Hei\u00b7maths\u00b7strand", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wird auch das Ziel mit M\u00fche nur gewonnen,", "tokens": ["Wird", "auch", "das", "Ziel", "mit", "M\u00fc\u00b7he", "nur", "ge\u00b7won\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch scheint die M\u00fch' um sch\u00f6nes Ziel mir sch\u00f6n,", "tokens": ["Doch", "scheint", "die", "M\u00fch'", "um", "sch\u00f6\u00b7nes", "Ziel", "mir", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wenn ", "tokens": ["Wenn"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Mit s\u00fc\u00dfer Huld und mildem Schimmer leiten.", "tokens": ["Mit", "s\u00fc\u00b7\u00dfer", "Huld", "und", "mil\u00b7dem", "Schim\u00b7mer", "lei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wohl seh' ich oft, wie hell vom goldnen Throne", "tokens": ["Wohl", "seh'", "ich", "oft", ",", "wie", "hell", "vom", "gold\u00b7nen", "Thro\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "ADJD", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Einen Bild zu mir herniedersinkt,", "tokens": ["Der", "Ei\u00b7nen", "Bild", "zu", "mir", "her\u00b7nie\u00b7der\u00b7sinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und freundlich mir zum wundersel'gen Lohne,", "tokens": ["Und", "freund\u00b7lich", "mir", "zum", "wun\u00b7der\u00b7sel'\u00b7gen", "Loh\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df nur f\u00fcr sie mein treues Lied erklingt,", "tokens": ["Da\u00df", "nur", "f\u00fcr", "sie", "mein", "treu\u00b7es", "Lied", "er\u00b7klingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit leiser Hand die heil'ge S\u00e4ngerkrone,", "tokens": ["Mit", "lei\u00b7ser", "Hand", "die", "heil'\u00b7ge", "S\u00e4n\u00b7ger\u00b7kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Den Lorbeerzweig, in meine Locken schlingt,", "tokens": ["Den", "Lor\u00b7beer\u00b7zweig", ",", "in", "mei\u00b7ne", "Lo\u00b7cken", "schlingt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und k\u00fchner l\u00e4\u00dft dann auf Gesanges Wellen", "tokens": ["Und", "k\u00fch\u00b7ner", "l\u00e4\u00dft", "dann", "auf", "Ge\u00b7san\u00b7ges", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mein trunkner Geist des Liedes Segel schwellen.", "tokens": ["Mein", "trunk\u00b7ner", "Geist", "des", "Lie\u00b7des", "Se\u00b7gel", "schwel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch l\u00e4\u00dft sich dann so kalt die Andre schauen", "tokens": ["Doch", "l\u00e4\u00dft", "sich", "dann", "so", "kalt", "die", "And\u00b7re", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ADV", "ADJD", "ART", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit strengem Blick und stolzem Angesicht,", "tokens": ["Mit", "stren\u00b7gem", "Blick", "und", "stol\u00b7zem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dann schwindet bald mein freudiges Vertrauen;", "tokens": ["Dann", "schwin\u00b7det", "bald", "mein", "freu\u00b7di\u00b7ges", "Ver\u00b7trau\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Nacht versinkt der Liebe leitend Licht;", "tokens": ["In", "Nacht", "ver\u00b7sinkt", "der", "Lie\u00b7be", "lei\u00b7tend", "Licht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auf weitem Meer ergreift mich stilles Grauen,", "tokens": ["Auf", "wei\u00b7tem", "Meer", "er\u00b7greift", "mich", "stil\u00b7les", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich wei\u00df den Pfad, das Ziel, den Hafen nicht.", "tokens": ["Ich", "wei\u00df", "den", "Pfad", ",", "das", "Ziel", ",", "den", "Ha\u00b7fen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wohl folgst du, denk' ich, tr\u00fcgerischen Sternen,", "tokens": ["Wohl", "folgst", "du", ",", "denk'", "ich", ",", "tr\u00fc\u00b7ge\u00b7ri\u00b7schen", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was kann dich sonst so weit von ihr entfernen?", "tokens": ["Was", "kann", "dich", "sonst", "so", "weit", "von", "ihr", "ent\u00b7fer\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "O ihr, die treu vereint in Leid und Freuden", "tokens": ["O", "ihr", ",", "die", "treu", "ver\u00b7eint", "in", "Leid", "und", "Freu\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "$,", "PRELS", "ADJD", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur ", "tokens": ["Nur"], "token_info": ["word"], "pos": ["ADV"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Was konnte so im Tode jetzt euch scheiden,", "tokens": ["Was", "konn\u00b7te", "so", "im", "To\u00b7de", "jetzt", "euch", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "APPRART", "NN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df diese flieht, was Jene sch\u00fctzt und pflegt?", "tokens": ["Da\u00df", "die\u00b7se", "flieht", ",", "was", "Je\u00b7ne", "sch\u00fctzt", "und", "pflegt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "$,", "PWS", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du banges Wechselspiel von Lust und Leiden,", "tokens": ["Du", "ban\u00b7ges", "Wech\u00b7sel\u00b7spiel", "von", "Lust", "und", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie hast du oft mein Herz so wild bewegt!", "tokens": ["Wie", "hast", "du", "oft", "mein", "Herz", "so", "wild", "be\u00b7wegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Doch kenn' ich leicht die F\u00e4den, die dich weben:", "tokens": ["Doch", "kenn'", "ich", "leicht", "die", "F\u00e4\u00b7den", ",", "die", "dich", "we\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "S\u00fc\u00df ist der Traum, doch hart und kalt das Leben.", "tokens": ["S\u00fc\u00df", "ist", "der", "Traum", ",", "doch", "hart", "und", "kalt", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART