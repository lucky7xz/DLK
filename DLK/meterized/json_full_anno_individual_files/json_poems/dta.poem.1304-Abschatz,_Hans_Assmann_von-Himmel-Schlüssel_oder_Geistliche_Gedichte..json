{"dta.poem.1304": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die schnellen Jahre gehn und wir mit ihnen hin/", "tokens": ["Die", "schnel\u00b7len", "Jah\u00b7re", "gehn", "und", "wir", "mit", "ih\u00b7nen", "hin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "KON", "PPER", "APPR", "PPER", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Eh man sich richtet ein/ das Jahr wohl anzuwenden/", "tokens": ["Eh", "man", "sich", "rich\u00b7tet", "ein", "/", "das", "Jahr", "wohl", "an\u00b7zu\u00b7wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "VVFIN", "ART", "$(", "ART", "NN", "ADV", "VVIZU", "$("], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Geht uns das meiste Theil desselben aus den H\u00e4nden/", "tokens": ["Geht", "uns", "das", "meis\u00b7te", "Theil", "des\u00b7sel\u00b7ben", "aus", "den", "H\u00e4n\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "PDS", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bleibt Reue/ Qual und Angst sein leidiger Gewinn/", "tokens": ["Bleibt", "Reu\u00b7e", "/", "Qual", "und", "Angst", "sein", "lei\u00b7di\u00b7ger", "Ge\u00b7winn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$(", "NN", "KON", "NN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein ieder hat genung sein Beyspiel anzuziehn.", "tokens": ["Ein", "ie\u00b7der", "hat", "ge\u00b7nung", "sein", "Bey\u00b7spiel", "an\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ADV", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Es hat mich GOttes Gunst ein Jahr nun lassen enden/", "tokens": ["Es", "hat", "mich", "Got\u00b7tes", "Gunst", "ein", "Jahr", "nun", "las\u00b7sen", "en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "NN", "ART", "NN", "ADV", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das neue Schuld und Last gelegt auff meine Lenden/", "tokens": ["Das", "neu\u00b7e", "Schuld", "und", "Last", "ge\u00b7legt", "auff", "mei\u00b7ne", "Len\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mein Leben will/ noch eh es Knospen tr\u00e4gt/ verbl\u00fchn.", "tokens": ["Mein", "Le\u00b7ben", "will", "/", "noch", "eh", "es", "Knos\u00b7pen", "tr\u00e4gt", "/", "ver\u00b7bl\u00fchn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$(", "ADV", "KOUS", "PPER", "NN", "VVFIN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "F\u00fcrst aller Zeit/ durch den ich diese Zeit erlebt/", "tokens": ["F\u00fcrst", "al\u00b7ler", "Zeit", "/", "durch", "den", "ich", "die\u00b7se", "Zeit", "er\u00b7lebt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$(", "APPR", "PRELS", "PPER", "PDAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gieb/ da\u00df die alte Zeit mir nie vor Augen schwebt", "tokens": ["Gieb", "/", "da\u00df", "die", "al\u00b7te", "Zeit", "mir", "nie", "vor", "Au\u00b7gen", "schwebt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "$(", "KOUS", "ART", "ADJA", "NN", "PPER", "ADV", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Samt ihrer alten Schuld/ regiere meine Sinnen/", "tokens": ["Samt", "ih\u00b7rer", "al\u00b7ten", "Schuld", "/", "re\u00b7gie\u00b7re", "mei\u00b7ne", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$(", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df sie der schn\u00f6den Zeit sich recht gebrauchen k\u00fcnnen/", "tokens": ["Da\u00df", "sie", "der", "schn\u00f6\u00b7den", "Zeit", "sich", "recht", "ge\u00b7brau\u00b7chen", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "PRF", "ADJD", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und wenn ich schliessen soll nach deiner meine Zeit/", "tokens": ["Und", "wenn", "ich", "schlies\u00b7sen", "soll", "nach", "dei\u00b7ner", "mei\u00b7ne", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVINF", "VMFIN", "APPR", "PPOSAT", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Versetze mich zu dir ins Reich der Ewigkeit.", "tokens": ["Ver\u00b7set\u00b7ze", "mich", "zu", "dir", "ins", "Reich", "der", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}