{"dta.poem.1186": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "AnEMONS  \n und  \n  ADONIS  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nte man f\u00fcr Liebe sterben/ w\u00e4r ich l\u00e4ngstens kalt und", "tokens": ["K\u00f6n\u00b7te", "man", "f\u00fcr", "Lie\u00b7be", "ster\u00b7ben", "/", "w\u00e4r", "ich", "l\u00e4ngs\u00b7tens", "kalt", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "APPR", "NN", "VVINF", "$(", "VAFIN", "PPER", "ADV", "ADJD", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "todt/", "tokens": ["todt", "/"], "token_info": ["word", "punct"], "pos": ["ADJD", "$("], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Solte sie ein Feuer heissen/ w\u00e4r ich l\u00e4ngstens Asch und Koth:", "tokens": ["Sol\u00b7te", "sie", "ein", "Feu\u00b7er", "heis\u00b7sen", "/", "w\u00e4r", "ich", "l\u00e4ngs\u00b7tens", "Asch", "und", "Koth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$(", "VAFIN", "PPER", "ADV", "ADJD", "KON", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Doch ist sie kein Tod zu nennen/ woher f\u00fchl ich solche Schmer-", "tokens": ["Doch", "ist", "sie", "kein", "Tod", "zu", "nen\u00b7nen", "/", "wo\u00b7her", "f\u00fchl", "ich", "sol\u00b7che", "Schmer"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "NN", "PTKZU", "VVINF", "$(", "PWAV", "VVFIN", "PPER", "PIAT", "TRUNC"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.5": {"text": "tzen?", "tokens": ["tzen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Und ist sie kein brennend Feuer/ was kocht so in meinem Her-", "tokens": ["Und", "ist", "sie", "kein", "bren\u00b7nend", "Feu\u00b7er", "/", "was", "kocht", "so", "in", "mei\u00b7nem", "Her"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "PIAT", "ADJD", "NN", "$(", "PWS", "VVFIN", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "-+--+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.7": {"text": "tzen?", "tokens": ["tzen", "?"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-", "measure": "single.down"}}}}}