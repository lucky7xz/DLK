{"textgrid.poem.64373": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "96. Abendlied", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gro\u00df und rotentflammet schwebet", "tokens": ["Gro\u00df", "und", "ro\u00b7tent\u00b7flam\u00b7met", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch die Sonn' am Himmelsrand,", "tokens": ["Noch", "die", "Sonn'", "am", "Him\u00b7mels\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf blauen Wogen bebet", "tokens": ["Und", "auf", "blau\u00b7en", "Wo\u00b7gen", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ihr Abglanz bis zum Strand;", "tokens": ["Noch", "ihr", "Ab\u00b7glanz", "bis", "zum", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Aus dem Buchenwalde hebet", "tokens": ["Aus", "dem", "Bu\u00b7chen\u00b7wal\u00b7de", "he\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich der Mond, und winket Ruh", "tokens": ["Sich", "der", "Mond", ",", "und", "win\u00b7ket", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "$,", "KON", "VVFIN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seiner Schwester Erde zu.", "tokens": ["Sei\u00b7ner", "Schwes\u00b7ter", "Er\u00b7de", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "In geschwollnen Wolken ballet", "tokens": ["In", "ge\u00b7schwoll\u00b7nen", "Wol\u00b7ken", "bal\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dunkler sich die rote Glut,", "tokens": ["Dunk\u00b7ler", "sich", "die", "ro\u00b7te", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zarter Farbenwechsel wallet", "tokens": ["Zar\u00b7ter", "Far\u00b7ben\u00b7wech\u00b7sel", "wal\u00b7let"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf der Rockenbl\u00fcte Flut;", "tokens": ["Auf", "der", "Ro\u00b7ck\u00b7en\u00b7bl\u00fc\u00b7te", "Flut", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Zwischen schwanken Halmen schallet", "tokens": ["Zwi\u00b7schen", "schwan\u00b7ken", "Hal\u00b7men", "schal\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Reger Wachteln heller Schlag,", "tokens": ["Re\u00b7ger", "Wach\u00b7teln", "hel\u00b7ler", "Schlag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und der Hirte pfeift ihm nach.", "tokens": ["Und", "der", "Hir\u00b7te", "pfeift", "ihm", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.3": {"line.1": {"text": "Wohlgeruch entsteigt den Auen", "tokens": ["Wohl\u00b7ge\u00b7ruch", "ent\u00b7steigt", "den", "Au\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dort in zartgewundnem Duft,", "tokens": ["Dort", "in", "zart\u00b7ge\u00b7wund\u00b7nem", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die j\u00fcngsten Stauden tauen", "tokens": ["Und", "die", "j\u00fcng\u00b7sten", "Stau\u00b7den", "tau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00fchles Labsal durch die Luft;", "tokens": ["K\u00fch\u00b7les", "Lab\u00b7sal", "durch", "die", "Luft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Jedes Bl\u00fcmchen saugt mit lauen", "tokens": ["Je\u00b7des", "Bl\u00fcm\u00b7chen", "saugt", "mit", "lau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lippen, und das Gr\u00e4schen sinkt", "tokens": ["Lip\u00b7pen", ",", "und", "das", "Gr\u00e4sc\u00b7hen", "sinkt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Unter Perlen, die es trinkt.", "tokens": ["Un\u00b7ter", "Per\u00b7len", ",", "die", "es", "trinkt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihre Ringeltauben girren", "tokens": ["Ih\u00b7re", "Rin\u00b7gel\u00b7tau\u00b7ben", "gir\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch die T\u00e4uber sanft in Ruh,", "tokens": ["Noch", "die", "T\u00e4u\u00b7ber", "sanft", "in", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcstre Flederm\u00e4use schwirren", "tokens": ["D\u00fcst\u00b7re", "Fle\u00b7der\u00b7m\u00e4u\u00b7se", "schwir\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun dem glatten Teiche zu,", "tokens": ["Nun", "dem", "glat\u00b7ten", "Tei\u00b7che", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der K\u00e4fer Scharen irren,", "tokens": ["Und", "der", "K\u00e4\u00b7fer", "Scha\u00b7ren", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Uhu, nun erwacht,", "tokens": ["Und", "der", "U\u00b7hu", ",", "nun", "er\u00b7wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Ziehet heulend auf die Wacht.", "tokens": ["Zie\u00b7het", "heu\u00b7lend", "auf", "die", "Wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit dem K\u00f6pfchen im Gefieder", "tokens": ["Mit", "dem", "K\u00f6pf\u00b7chen", "im", "Ge\u00b7fie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlummern unsre S\u00e4nger nun,", "tokens": ["Schlum\u00b7mern", "uns\u00b7re", "S\u00e4n\u00b7ger", "nun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Es verstummen ihre Lieder,", "tokens": ["Es", "ver\u00b7stum\u00b7men", "ih\u00b7re", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst die lauten Stare ruhn", "tokens": ["Selbst", "die", "lau\u00b7ten", "Sta\u00b7re", "ruhn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den schwanken Binsen wieder,", "tokens": ["Auf", "den", "schwan\u00b7ken", "Bin\u00b7sen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nur die Nachtigall allein", "tokens": ["Nur", "die", "Nach\u00b7ti\u00b7gall", "al\u00b7lein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Freut sich noch im Mondenschein.", "tokens": ["Freut", "sich", "noch", "im", "Mon\u00b7den\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie, auch in der Stille rege", "tokens": ["Wie", ",", "auch", "in", "der", "Stil\u00b7le", "re\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ADV", "APPR", "ART", "NN", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mit dem Anbeginn der Nacht,", "tokens": ["Mit", "dem", "An\u00b7be\u00b7ginn", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nach der mannigfalt'gen Pflege,", "tokens": ["Nach", "der", "man\u00b7nig\u00b7falt'\u00b7gen", "Pfle\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nun die Mutter ist bedacht,", "tokens": ["Nun", "die", "Mut\u00b7ter", "ist", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie ihre Kindlein lege;", "tokens": ["Da\u00df", "sie", "ih\u00b7re", "Kin\u00b7dlein", "le\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Wie sie jedes letzten Gru\u00df", "tokens": ["Wie", "sie", "je\u00b7des", "letz\u00b7ten", "Gru\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Noch belohnt mit weichem Ku\u00df;", "tokens": ["Noch", "be\u00b7lohnt", "mit", "wei\u00b7chem", "Ku\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Also, nach des Tags Get\u00fcmmel,", "tokens": ["Al\u00b7so", ",", "nach", "des", "Tags", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schaut der ew'gen Liebe Blick", "tokens": ["Schaut", "der", "ew'\u00b7gen", "Lie\u00b7be", "Blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Durch den sternenvollen Himmel", "tokens": ["Durch", "den", "ster\u00b7nen\u00b7vol\u00b7len", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf die Erde noch zur\u00fcck;", "tokens": ["Auf", "die", "Er\u00b7de", "noch", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fr\u00fch vernimmt sie das Gewimmel", "tokens": ["Fr\u00fch", "ver\u00b7nimmt", "sie", "das", "Ge\u00b7wim\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der erwachten Erd', und sp\u00e4t", "tokens": ["Der", "er\u00b7wach\u00b7ten", "Erd'", ",", "und", "sp\u00e4t"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "H\u00f6rt sie den, der einsam fleht.", "tokens": ["H\u00f6rt", "sie", "den", ",", "der", "ein\u00b7sam", "fleht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wenn die Nachtigallen fl\u00f6ten,", "tokens": ["Wenn", "die", "Nach\u00b7ti\u00b7gal\u00b7len", "fl\u00f6\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hebe dich, mein Geist, empor!", "tokens": ["He\u00b7be", "dich", ",", "mein", "Geist", ",", "em\u00b7por", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PPOSAT", "NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bei des jungen Tags Err\u00f6ten", "tokens": ["Bei", "des", "jun\u00b7gen", "Tags", "Er\u00b7r\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neig', o Vater, mir dein Ohr!", "tokens": ["Neig'", ",", "o", "Va\u00b7ter", ",", "mir", "dein", "Ohr", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Von der Erde Freud' und N\u00f6ten", "tokens": ["Von", "der", "Er\u00b7de", "Freud'", "und", "N\u00f6\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steig', o Geist, im Duft der Au!", "tokens": ["Steig'", ",", "o", "Geist", ",", "im", "Duft", "der", "Au", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM", "NN", "$,", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Send', o Vater, deinen Tau!", "tokens": ["Send'", ",", "o", "Va\u00b7ter", ",", "dei\u00b7nen", "Tau", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Gro\u00df und rotentflammet schwebet", "tokens": ["Gro\u00df", "und", "ro\u00b7tent\u00b7flam\u00b7met", "schwe\u00b7bet"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Noch die Sonn' am Himmelsrand,", "tokens": ["Noch", "die", "Sonn'", "am", "Him\u00b7mels\u00b7rand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf blauen Wogen bebet", "tokens": ["Und", "auf", "blau\u00b7en", "Wo\u00b7gen", "be\u00b7bet"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Noch ihr Abglanz bis zum Strand;", "tokens": ["Noch", "ihr", "Ab\u00b7glanz", "bis", "zum", "Strand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line