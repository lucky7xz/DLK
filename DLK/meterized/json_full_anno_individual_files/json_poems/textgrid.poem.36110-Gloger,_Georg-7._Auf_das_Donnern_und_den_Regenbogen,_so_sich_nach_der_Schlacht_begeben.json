{"textgrid.poem.36110": {"metadata": {"author": {"name": "Gloger, Georg", "birth": "N.A.", "death": "N.A."}, "title": "7. Auf das Donnern und den Regenbogen, so sich nach der Schlacht begeben", "genre": "verse", "period": "N.A.", "pub_year": 1617, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie? sehen wir denn selbst die G\u00f6tter vns zugegen?", "tokens": ["Wie", "?", "se\u00b7hen", "wir", "denn", "selbst", "die", "G\u00f6t\u00b7ter", "vns", "zu\u00b7ge\u00b7gen", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie sinds, vnd ihre H\u00fclf' vns weiter wird geschehn.", "tokens": ["Sie", "sinds", ",", "vnd", "ih\u00b7re", "H\u00fcl\u00b7f'", "vns", "wei\u00b7ter", "wird", "ge\u00b7schehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KON", "PPOSAT", "NN", "PPER", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Die Zeichen, da\u00df wir viel noch werden Feind' erlegen,", "tokens": ["Die", "Zei\u00b7chen", ",", "da\u00df", "wir", "viel", "noch", "wer\u00b7den", "Feind'", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "ADV", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sind gut, vnd lassen sich durch gute Deutung sehn.", "tokens": ["Sind", "gut", ",", "vnd", "las\u00b7sen", "sich", "durch", "gu\u00b7te", "Deu\u00b7tung", "sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KON", "VVFIN", "PRF", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn als die Schlacht ward aus, vnd nun die Feind' erlagen,", "tokens": ["Denn", "als", "die", "Schlacht", "ward", "aus", ",", "vnd", "nun", "die", "Feind'", "er\u00b7la\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VAFIN", "PTKVZ", "$,", "KON", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da gabe Gott fast selbst sein Ja vnd Willen drein,", "tokens": ["Da", "ga\u00b7be", "Gott", "fast", "selbst", "sein", "Ja", "vnd", "Wil\u00b7len", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als sich bey heller Luft dreymal der Donnerwagen", "tokens": ["Als", "sich", "bey", "hel\u00b7ler", "Luft", "drey\u00b7mal", "der", "Don\u00b7ner\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So sanfte h\u00f6ren lie\u00df, als soltens Worte seyn.", "tokens": ["So", "sanf\u00b7te", "h\u00f6\u00b7ren", "lie\u00df", ",", "als", "sol\u00b7tens", "Wor\u00b7te", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVINF", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dreymal vnd weiter nicht. Hierauf wurd Alles stille,", "tokens": ["Drey\u00b7mal", "vnd", "wei\u00b7ter", "nicht", ".", "Hier\u00b7auf", "wurd", "Al\u00b7les", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KON", "ADV", "PTKNEG", "$.", "PAV", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.10": {"text": "Vnd sah am Himmel man den Regenbogen gehn.", "tokens": ["Vnd", "sah", "am", "Him\u00b7mel", "man", "den", "Re\u00b7gen\u00b7bo\u00b7gen", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PIS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie solte nun der Sieg nicht seyn des Herren Wille,", "tokens": ["Wie", "sol\u00b7te", "nun", "der", "Sieg", "nicht", "seyn", "des", "Her\u00b7ren", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ART", "NN", "PTKNEG", "VAINF", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil vmb vnd \u00fcber vns so klare Zeichen stehn?", "tokens": ["Weil", "vmb", "vnd", "\u00fc\u00b7ber", "vns", "so", "kla\u00b7re", "Zei\u00b7chen", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "KON", "APPR", "PPER", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wir gl\u00e4uben festiglich, da\u00df Gott sey selbst zugegen,", "tokens": ["Wir", "gl\u00e4u\u00b7ben", "fes\u00b7tig\u00b7lich", ",", "da\u00df", "Gott", "sey", "selbst", "zu\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "w