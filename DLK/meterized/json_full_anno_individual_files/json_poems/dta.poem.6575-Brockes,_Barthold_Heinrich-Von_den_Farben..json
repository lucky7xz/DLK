{"dta.poem.6575": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Farben.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So haben Farben auch, so wie Vereinigungen;", "tokens": ["So", "ha\u00b7ben", "Far\u00b7ben", "auch", ",", "so", "wie", "Ver\u00b7ei\u00b7ni\u00b7gun\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "$,", "ADV", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht minder Gegens\u00e4tz\u2019 und Hinderungen,", "tokens": ["Nicht", "min\u00b7der", "Ge\u00b7gen\u00b7s\u00e4tz'", "und", "Hin\u00b7de\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wodurch sie sich vertreiben, sich vermindern,", "tokens": ["Wo\u00b7durch", "sie", "sich", "ver\u00b7trei\u00b7ben", ",", "sich", "ver\u00b7min\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "VVINF", "$,", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich schaden, und sich unterbrechen,", "tokens": ["Sich", "scha\u00b7den", ",", "und", "sich", "un\u00b7ter\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "$,", "KON", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Offt sich vertilgen, offt sich schw\u00e4chen.", "tokens": ["Offt", "sich", "ver\u00b7til\u00b7gen", ",", "offt", "sich", "schw\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "VVINF", "$,", "ADV", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}