{"textgrid.poem.33307": {"metadata": {"author": {"name": "Blumauer, Aloys", "birth": "N.A.", "death": "N.A."}, "title": "Brief eines strengen Vaters an seinen Sohn", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein strenger Vater schrieb an seinen Sohn:", "tokens": ["Ein", "stren\u00b7ger", "Va\u00b7ter", "schrieb", "an", "sei\u00b7nen", "Sohn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "\u00bbdurch gegenw\u00e4rt'gen Postillon", "tokens": ["\u00bb", "durch", "ge\u00b7gen\u00b7w\u00e4rt'\u00b7gen", "Pos\u00b7til\u00b7lon"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Erh\u00e4ltst du einen Beutel \u2013 wohlbespicket", "tokens": ["Er\u00b7h\u00e4ltst", "du", "ei\u00b7nen", "Beu\u00b7tel", "\u2013", "wohl\u00b7be\u00b7spi\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$(", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit Thalern, den dir \u2013 ohne da\u00df ich's wei\u00df \u2013", "tokens": ["Mit", "Tha\u00b7lern", ",", "den", "dir", "\u2013", "oh\u00b7ne", "da\u00df", "ich's", "wei\u00df", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PPER", "$(", "APPR", "KOUS", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Hier deine liebe Mutter schicket.", "tokens": ["Hier", "dei\u00b7ne", "lie\u00b7be", "Mut\u00b7ter", "schi\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nach einem Monat holt, wenn du mit Flei\u00df", "tokens": ["Nach", "ei\u00b7nem", "Mo\u00b7nat", "holt", ",", "wenn", "du", "mit", "Flei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mit mehr Emsigkeit studierest,", "tokens": ["Und", "mit", "mehr", "Em\u00b7sig\u00b7keit", "stu\u00b7die\u00b7rest", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit meiner Stutte uns're Magd dich ab.", "tokens": ["Mit", "mei\u00b7ner", "Stut\u00b7te", "un\u00b7s'\u00b7re", "Magd", "dich", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Besteige sie! sie geht den besten Trab:", "tokens": ["Be\u00b7stei\u00b7ge", "sie", "!", "sie", "geht", "den", "bes\u00b7ten", "Trab", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Doch h\u00fcte dich, da\u00df du sie nicht for\u00e7irest!", "tokens": ["Doch", "h\u00fc\u00b7te", "dich", ",", "da\u00df", "du", "sie", "nicht", "for\u00e7i\u00b7rest", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Von dir ist \u00fcbrigens die Sage allgemein,", "tokens": ["Von", "dir", "ist", "\u00fcb\u00b7ri\u00b7gens", "die", "Sa\u00b7ge", "all\u00b7ge\u00b7mein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Du k\u00f6nntest nicht ein Wort Latein", "tokens": ["Du", "k\u00f6nn\u00b7test", "nicht", "ein", "Wort", "La\u00b7tein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Bis dato sprechen, noch auch schreiben.", "tokens": ["Bis", "da\u00b7to", "spre\u00b7chen", ",", "noch", "auch", "schrei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVINF", "$,", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Ich sagt' es dir ja immerhin:", "tokens": ["Ich", "sagt'", "es", "dir", "ja", "im\u00b7mer\u00b7hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du bist und bleibst ein Eselskopf! Ich bin", "tokens": ["Du", "bist", "und", "bleibst", "ein", "E\u00b7sels\u00b7kopf", "!", "Ich", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Dein treuer Vater: Hans von Eiben.\u00ab", "tokens": ["Dein", "treu\u00b7er", "Va\u00b7ter", ":", "Hans", "von", "Ei\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "