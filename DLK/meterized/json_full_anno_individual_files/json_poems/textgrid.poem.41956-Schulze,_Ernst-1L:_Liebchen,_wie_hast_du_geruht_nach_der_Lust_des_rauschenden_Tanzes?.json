n_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Und \u00e4therisches Licht folget dir, Zauberin, gern.", "tokens": ["Und", "\u00e4t\u00b7he\u00b7ri\u00b7sches", "Licht", "fol\u00b7get", "dir", ",", "Zau\u00b7be\u00b7rin", ",", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bin ich dir fern, ist schwarz mir die Sonn' und dunkel der Erdkreis,", "tokens": ["Bin", "ich", "dir", "fern", ",", "ist", "schwarz", "mir", "die", "Sonn'", "und", "dun\u00b7kel", "der", "Erd\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKVZ", "$,", "VAFIN", "ADJD", "PPER", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Doch dein l\u00e4chelnder Blick f\u00fcllet mit Strahlen die Nacht.", "tokens": ["Doch", "dein", "l\u00e4\u00b7cheln\u00b7der", "Blick", "f\u00fcl\u00b7let", "mit", "Strah\u00b7len", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Aber du winkst mir, du l\u00e4chelst mir zu! Schnell flieg' ich hin\u00fcber!", "tokens": ["A\u00b7ber", "du", "winkst", "mir", ",", "du", "l\u00e4\u00b7chelst", "mir", "zu", "!", "Schnell", "flieg'", "ich", "hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.34": {"text": "K\u00fcssest du heute mich nicht, Frevlerin, nimm dich in Acht!", "tokens": ["K\u00fcs\u00b7sest", "du", "heu\u00b7te", "mich", "nicht", ",", "Frev\u00b7le\u00b7rin", ",", "nimm", "dich", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "$,", "NN", "$,", "VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.35": {"text": "Kennst du den magischen Quell, der einst Rinaldo verwandelt,", "tokens": ["Kennst", "du", "den", "ma\u00b7gi\u00b7schen", "Quell", ",", "der", "einst", "Ri\u00b7nal\u00b7do", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "NE", "VVPP", "$,"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.36": {"text": "Als Angelika ihm folgte mit sehnendem Blick?", "tokens": ["Als", "An\u00b7ge\u00b7li\u00b7ka", "ihm", "folg\u00b7te", "mit", "seh\u00b7nen\u00b7dem", "Blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Immer noch rieselt der Quell, und stets noch w\u00e4hrt die Bezaubrung;", "tokens": ["Im\u00b7mer", "noch", "rie\u00b7selt", "der", "Quell", ",", "und", "stets", "noch", "w\u00e4hrt", "die", "Be\u00b7zaub\u00b7rung", ";"], "token_info": ["word", "word", "word", "word