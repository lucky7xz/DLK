{"textgrid.poem.41956": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebchen, wie hast du geruht nach der Lust des rauschenden Tanzes?", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebchen, wie hast du geruht nach der Lust des rauschenden Tanzes?", "tokens": ["Lieb\u00b7chen", ",", "wie", "hast", "du", "ge\u00b7ruht", "nach", "der", "Lust", "des", "rau\u00b7schen\u00b7den", "Tan\u00b7zes", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "VAFIN", "PPER", "VVPP", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Ist dir das K\u00f6pfchen nicht noch schwer von dem w\u00fcsten Gel\u00e4rm?", "tokens": ["Ist", "dir", "das", "K\u00f6pf\u00b7chen", "nicht", "noch", "schwer", "von", "dem", "w\u00fcs\u00b7ten", "Ge\u00b7l\u00e4rm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Tobte nicht lang' in der Nacht der Musik nacht\u00f6nender Aufruhr", "tokens": ["Tob\u00b7te", "nicht", "lang'", "in", "der", "Nacht", "der", "Mu\u00b7sik", "nach\u00b7t\u00f6\u00b7nen\u00b7der", "Auf\u00b7ruhr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ADV", "APPR", "ART", "NN", "ART", "NN", "APPR", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Disharmonisch und wild rings dir um's h\u00fcpfende Bett?", "tokens": ["Dis\u00b7har\u00b7mo\u00b7nisch", "und", "wild", "rings", "dir", "um's", "h\u00fcp\u00b7fen\u00b7de", "Bett", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "ADV", "PPER", "NE", "ADJA", "NN", "$."], "meter": "+--+-+--++--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Oder belastete nicht dir die Brust, als qu\u00e4lender Rachgeist,", "tokens": ["O\u00b7der", "be\u00b7las\u00b7te\u00b7te", "nicht", "dir", "die", "Brust", ",", "als", "qu\u00e4\u00b7len\u00b7der", "Rach\u00b7geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "PPER", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.6": {"text": "Welcher die Schw\u00e4rmer der Nacht \u00e4ngstet, ein feindlicher Alp?", "tokens": ["Wel\u00b7cher", "die", "Schw\u00e4r\u00b7mer", "der", "Nacht", "\u00e4ngs\u00b7tet", ",", "ein", "feind\u00b7li\u00b7cher", "Alp", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.7": {"text": "Aber verzeih, holdseliges Weib, dem t\u00fcckischen Sp\u00f6tter;", "tokens": ["A\u00b7ber", "ver\u00b7zeih", ",", "hold\u00b7se\u00b7li\u00b7ges", "Weib", ",", "dem", "t\u00fc\u00b7cki\u00b7schen", "Sp\u00f6t\u00b7ter", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "+--+-+--+--+-+-", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Gern an fremdem Genu\u00df r\u00e4cht man den eigenen Schmerz.", "tokens": ["Gern", "an", "frem\u00b7dem", "Ge\u00b7nu\u00df", "r\u00e4cht", "man", "den", "ei\u00b7ge\u00b7nen", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVFIN", "PIS", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Nein, ein reizender Traum umgaukelte sicher die Stirn dir,", "tokens": ["Nein", ",", "ein", "rei\u00b7zen\u00b7der", "Traum", "um\u00b7gau\u00b7kel\u00b7te", "si\u00b7cher", "die", "Stirn", "dir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "ADJA", "NN", "VVFIN", "ADJD", "ART", "NN", "PPER", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.10": {"text": "F\u00fchrt' in ein Feengefild deinen entfesselten Geist,", "tokens": ["F\u00fchrt'", "in", "ein", "Fe\u00b7en\u00b7ge\u00b7fild", "dei\u00b7nen", "ent\u00b7fes\u00b7sel\u00b7ten", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.11": {"text": "Kr\u00e4nzte dein Haupt mit den Bl\u00fcthen des Mays und wiegte behende", "tokens": ["Kr\u00e4nz\u00b7te", "dein", "Haupt", "mit", "den", "Bl\u00fc\u00b7then", "des", "Mays", "und", "wieg\u00b7te", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "ART", "NE", "KON", "VVFIN", "ADJA"], "meter": "+--+--+--+-+----", "measure": "dactylic.tri.plus"}, "line.12": {"text": "Auf hellblauem Gew\u00f6lk \u00fcber die Erde dich hin.", "tokens": ["Auf", "hell\u00b7blau\u00b7em", "Ge\u00b7w\u00f6lk", "\u00fc\u00b7ber", "die", "Er\u00b7de", "dich", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "PPER", "PTKVZ", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.13": {"text": "Fr\u00fch schon sitzest du dort mir gegen\u00fcber am Fenster;", "tokens": ["Fr\u00fch", "schon", "sit\u00b7zest", "du", "dort", "mir", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "am", "Fens\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VVFIN", "PPER", "ADV", "PPER", "APPR", "APPRART", "NN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.14": {"text": "Zwar ist schmachtend dein Blick, aber doch heiter und mild;", "tokens": ["Zwar", "ist", "schmach\u00b7tend", "dein", "Blick", ",", "a\u00b7ber", "doch", "hei\u00b7ter", "und", "mild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADJD", "KON", "ADJD", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Sinnend lehnst auf die zierliche Hand du das lockige K\u00f6pfchen;", "tokens": ["Sin\u00b7nend", "lehnst", "auf", "die", "zier\u00b7li\u00b7che", "Hand", "du", "das", "lo\u00b7cki\u00b7ge", "K\u00f6pf\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.16": {"text": "Z\u00e4hlst du, Schelmin, vielleicht deine Gefangenen nach,", "tokens": ["Z\u00e4hlst", "du", ",", "Schel\u00b7min", ",", "viel\u00b7leicht", "dei\u00b7ne", "Ge\u00b7fan\u00b7ge\u00b7nen", "nach", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Die du bei'm fr\u00f6hlichen Fest mit dem siegenden Zauber der Blicke", "tokens": ["Die", "du", "bei'm", "fr\u00f6h\u00b7li\u00b7chen", "Fest", "mit", "dem", "sie\u00b7gen\u00b7den", "Zau\u00b7ber", "der", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und mit des Geistes Gewalt dir in die Netze gescheucht?", "tokens": ["Und", "mit", "des", "Geis\u00b7tes", "Ge\u00b7walt", "dir", "in", "die", "Net\u00b7ze", "ge\u00b7scheucht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.19": {"text": "Ach, ich war dir ein Thor dem frevelnden Wahne zu folgen,", "tokens": ["Ach", ",", "ich", "war", "dir", "ein", "Thor", "dem", "fre\u00b7veln\u00b7den", "Wah\u00b7ne", "zu", "fol\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.20": {"text": "Welcher die Ruhe der Nacht, welcher die Tr\u00e4ume mir nahm.", "tokens": ["Wel\u00b7cher", "die", "Ru\u00b7he", "der", "Nacht", ",", "wel\u00b7cher", "die", "Tr\u00e4u\u00b7me", "mir", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.21": {"text": "Untreu w\u00e4hnet' ich dich, und ich z\u00fcrnt' aufgrollend dem Herzen,", "tokens": ["Un\u00b7treu", "w\u00e4h\u00b7net'", "ich", "dich", ",", "und", "ich", "z\u00fcrnt'", "auf\u00b7grol\u00b7lend", "dem", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "$,", "KON", "PPER", "VVFIN", "VVPP", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.22": {"text": "Da\u00df es aus deiner Gewalt nimmer zu fliehen vermag.", "tokens": ["Da\u00df", "es", "aus", "dei\u00b7ner", "Ge\u00b7walt", "nim\u00b7mer", "zu", "flie\u00b7hen", "ver\u00b7mag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.23": {"text": "Ungl\u00fcckseliger, konntest du so dem eigenen Gl\u00fccke", "tokens": ["Un\u00b7gl\u00fcck\u00b7se\u00b7li\u00b7ger", ",", "konn\u00b7test", "du", "so", "dem", "ei\u00b7ge\u00b7nen", "Gl\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.24": {"text": "Gram seyn? konntest du so w\u00fcnschen den eigenen Tod?", "tokens": ["Gram", "seyn", "?", "konn\u00b7test", "du", "so", "w\u00fcn\u00b7schen", "den", "ei\u00b7ge\u00b7nen", "Tod", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$.", "VMFIN", "PPER", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.25": {"text": "Stets war grauendes Dunkel der Freund tiefbr\u00fctenden Tr\u00fcbsinns;", "tokens": ["Stets", "war", "grau\u00b7en\u00b7des", "Dun\u00b7kel", "der", "Freund", "tief\u00b7br\u00fc\u00b7ten\u00b7den", "Tr\u00fcb\u00b7sinns", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.26": {"text": "Auf dem Gew\u00f6lke der Nacht wiegte die Trauer sich stets:", "tokens": ["Auf", "dem", "Ge\u00b7w\u00f6l\u00b7ke", "der", "Nacht", "wieg\u00b7te", "die", "Trau\u00b7er", "sich", "stets", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "PRF", "ADV", "$."], "meter": "+--+--++--+--+", "measure": "dactylic.di.plus"}, "line.27": {"text": "Doch der ergl\u00fchende Tag verbannt die verha\u00dfte zum Orkus,", "tokens": ["Doch", "der", "er\u00b7gl\u00fc\u00b7hen\u00b7de", "Tag", "ver\u00b7bannt", "die", "ver\u00b7ha\u00df\u00b7te", "zum", "Or\u00b7kus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "ART", "ADJA", "APPRART", "NN", "$,"], "meter": "-+-+--+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.28": {"text": "Und Aurorens Gespann leitet ein fr\u00f6hlicher Geist.", "tokens": ["Und", "Au\u00b7ro\u00b7rens", "Ge\u00b7spann", "lei\u00b7tet", "ein", "fr\u00f6h\u00b7li\u00b7cher", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-++--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.29": {"text": "Doch nicht blos die Natur, auch du gebietest der D\u00e4mmrung,", "tokens": ["Doch", "nicht", "blos", "die", "Na\u00b7tur", ",", "auch", "du", "ge\u00b7bie\u00b7test", "der", "D\u00e4mm\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "$,", "ADV", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.30": {"text": "Und \u00e4therisches Licht folget dir, Zauberin, gern.", "tokens": ["Und", "\u00e4t\u00b7he\u00b7ri\u00b7sches", "Licht", "fol\u00b7get", "dir", ",", "Zau\u00b7be\u00b7rin", ",", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER", "$,", "NN", "$,", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Bin ich dir fern, ist schwarz mir die Sonn' und dunkel der Erdkreis,", "tokens": ["Bin", "ich", "dir", "fern", ",", "ist", "schwarz", "mir", "die", "Sonn'", "und", "dun\u00b7kel", "der", "Erd\u00b7kreis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "PTKVZ", "$,", "VAFIN", "ADJD", "PPER", "ART", "NN", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.32": {"text": "Doch dein l\u00e4chelnder Blick f\u00fcllet mit Strahlen die Nacht.", "tokens": ["Doch", "dein", "l\u00e4\u00b7cheln\u00b7der", "Blick", "f\u00fcl\u00b7let", "mit", "Strah\u00b7len", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "NN", "ART", "NN", "$."], "meter": "--+--++--+--+", "measure": "anapaest.di.plus"}, "line.33": {"text": "Aber du winkst mir, du l\u00e4chelst mir zu! Schnell flieg' ich hin\u00fcber!", "tokens": ["A\u00b7ber", "du", "winkst", "mir", ",", "du", "l\u00e4\u00b7chelst", "mir", "zu", "!", "Schnell", "flieg'", "ich", "hin\u00b7\u00fc\u00b7ber", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ADJD", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.34": {"text": "K\u00fcssest du heute mich nicht, Frevlerin, nimm dich in Acht!", "tokens": ["K\u00fcs\u00b7sest", "du", "heu\u00b7te", "mich", "nicht", ",", "Frev\u00b7le\u00b7rin", ",", "nimm", "dich", "in", "Acht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "$,", "NN", "$,", "VVIMP", "PPER", "APPR", "CARD", "$."], "meter": "+--+-+-+-+-+-+", "measure": "iambic.septa.invert"}, "line.35": {"text": "Kennst du den magischen Quell, der einst Rinaldo verwandelt,", "tokens": ["Kennst", "du", "den", "ma\u00b7gi\u00b7schen", "Quell", ",", "der", "einst", "Ri\u00b7nal\u00b7do", "ver\u00b7wan\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "NE", "VVPP", "$,"], "meter": "+--+--+--+-+-+-", "measure": "elegiambus"}, "line.36": {"text": "Als Angelika ihm folgte mit sehnendem Blick?", "tokens": ["Als", "An\u00b7ge\u00b7li\u00b7ka", "ihm", "folg\u00b7te", "mit", "seh\u00b7nen\u00b7dem", "Blick", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.37": {"text": "Immer noch rieselt der Quell, und stets noch w\u00e4hrt die Bezaubrung;", "tokens": ["Im\u00b7mer", "noch", "rie\u00b7selt", "der", "Quell", ",", "und", "stets", "noch", "w\u00e4hrt", "die", "Be\u00b7zaub\u00b7rung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "$,", "KON", "ADV", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+--