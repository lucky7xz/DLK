{"textgrid.poem.39302": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein J\u00e4ger ritt zum Buchenwald,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein J\u00e4ger ritt zum Buchenwald,", "tokens": ["Ein", "J\u00e4\u00b7ger", "ritt", "zum", "Bu\u00b7chen\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Morgensonne schien,", "tokens": ["Die", "Mor\u00b7gen\u00b7son\u00b7ne", "schien", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ihm \u00fcber'm Haupte w\u00f6lbten sich", "tokens": ["Ihm", "\u00fc\u00b7ber'm", "Haup\u00b7te", "w\u00f6lb\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPRART", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Bl\u00e4tter goldig-gr\u00fcn.", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "gol\u00b7dig\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und als er kam mit freud'gem Mut", "tokens": ["Und", "als", "er", "kam", "mit", "freu\u00b7d'\u00b7gem", "Mut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "An den heimlich stillen See,", "tokens": ["An", "den", "heim\u00b7lich", "stil\u00b7len", "See", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Da sprang aus dunkelgr\u00fcner Flut", "tokens": ["Da", "sprang", "aus", "dun\u00b7kel\u00b7gr\u00fc\u00b7ner", "Flut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein wunderschlankes Reh!", "tokens": ["Ein", "wun\u00b7der\u00b7schlan\u00b7kes", "Reh", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Ritter warf den Speer von sich:", "tokens": ["Der", "Rit\u00b7ter", "warf", "den", "Speer", "von", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbwer t\u00e4te Dir ein Weh?!", "tokens": ["\u00bb", "wer", "t\u00e4\u00b7te", "Dir", "ein", "Weh", "?!"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wohl aber fangen m\u00f6cht' ich Dich,", "tokens": ["Wohl", "a\u00b7ber", "fan\u00b7gen", "m\u00f6cht'", "ich", "Dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVINF", "VMFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du dunkel\u00e4ugig Reh!\u00ab", "tokens": ["Du", "dun\u00b7ke\u00b7l\u00e4u\u00b7gig", "Reh", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADJD", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Durchs Dickicht brach sein Ro\u00df mit Macht,", "tokens": ["Durchs", "Di\u00b7ckicht", "brach", "sein", "Ro\u00df", "mit", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Voran das sch\u00f6ne Tier,", "tokens": ["Vo\u00b7ran", "das", "sch\u00f6\u00b7ne", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Er ritt bis an der Waldesnacht", "tokens": ["Er", "ritt", "bis", "an", "der", "Wal\u00b7des\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Verborgenstes Revier.", "tokens": ["Ver\u00b7bor\u00b7gens\u00b7tes", "Re\u00b7vier", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Im Eichenringe war ein Plan,", "tokens": ["Im", "Ei\u00b7chen\u00b7rin\u00b7ge", "war", "ein", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Tiefgr\u00fcn, wie ein Smaragd,", "tokens": ["Tief\u00b7gr\u00fcn", ",", "wie", "ein", "Sma\u00b7ragd", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Die Z\u00fcgel zog der Ritter an", "tokens": ["Die", "Z\u00fc\u00b7gel", "zog", "der", "Rit\u00b7ter", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von freud'gem Schreck gepackt.", "tokens": ["Von", "freu\u00b7d'\u00b7gem", "Schreck", "ge\u00b7packt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Verschwunden war das scheue Reh,", "tokens": ["Ver\u00b7schwun\u00b7den", "war", "das", "scheu\u00b7e", "Reh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl \u00fcber Stein und Stock,", "tokens": ["Wohl", "\u00fc\u00b7ber", "Stein", "und", "Stock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und vor ihm stand die Waldesfee", "tokens": ["Und", "vor", "ihm", "stand", "die", "Wal\u00b7des\u00b7fee"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit flutendem Gelock.", "tokens": ["Mit", "flu\u00b7ten\u00b7dem", "Ge\u00b7lock", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "In tiefen Augen zitterte", "tokens": ["In", "tie\u00b7fen", "Au\u00b7gen", "zit\u00b7ter\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Meer von Lust und Weh,", "tokens": ["Ein", "Meer", "von", "Lust", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die lange Wimper schattete,", "tokens": ["Die", "lan\u00b7ge", "Wim\u00b7per", "schat\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie Zweige \u00fcber'm See.", "tokens": ["Wie", "Zwei\u00b7ge", "\u00fc\u00b7ber'm", "See", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie schwebte leicht und zauberisch,", "tokens": ["Sie", "schweb\u00b7te", "leicht", "und", "zau\u00b7be\u00b7risch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Wind auf Wellen tanzt;", "tokens": ["Wie", "Wind", "auf", "Wel\u00b7len", "tanzt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "\u00bbich bin das Reh, nun schie\u00dfe frisch,", "tokens": ["\u00bb", "ich", "bin", "das", "Reh", ",", "nun", "schie\u00b7\u00dfe", "frisch", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ART", "NN", "$,", "ADV", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und triff mich, wenn Du kannst!\u00ab", "tokens": ["Und", "triff", "mich", ",", "wenn", "Du", "kannst", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VMFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es ist ein altes M\u00e4rchenlied,", "tokens": ["Es", "ist", "ein", "al\u00b7tes", "M\u00e4r\u00b7chen\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", 