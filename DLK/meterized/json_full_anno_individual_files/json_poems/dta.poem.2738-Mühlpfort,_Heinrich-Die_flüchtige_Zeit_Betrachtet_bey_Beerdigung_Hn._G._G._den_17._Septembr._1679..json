{"dta.poem.2738": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die fl\u00fcchtige Zeit/  \n Betrachtet bey Beerdigung Hn. G. G.  \n den 17. Septembr. 1679.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Zeit will durch Minuten sterben/", "tokens": ["Die", "Zeit", "will", "durch", "Mi\u00b7nu\u00b7ten", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fleucht und folgt ihr selber nach.", "tokens": ["Sie", "fleucht", "und", "folgt", "ihr", "sel\u00b7ber", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie eine Fackel zu verderben", "tokens": ["Wie", "ei\u00b7ne", "Fa\u00b7ckel", "zu", "ver\u00b7der\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PTKZU", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df durch ihr Scheinen allgemach:", "tokens": ["Mu\u00df", "durch", "ihr", "Schei\u00b7nen", "all\u00b7ge\u00b7mach", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So laufft der Zirkel aller Zeiten/", "tokens": ["So", "laufft", "der", "Zir\u00b7kel", "al\u00b7ler", "Zei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der gar nicht zu ver\u00e4ndern ist.", "tokens": ["Der", "gar", "nicht", "zu", "ver\u00b7\u00e4n\u00b7dern", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die Alten wolten di\u00df ausdeuten/", "tokens": ["Die", "Al\u00b7ten", "wol\u00b7ten", "di\u00df", "aus\u00b7deu\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PDS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn der Saturn die Kinder frisst.", "tokens": ["Wenn", "der", "Sa\u00b7turn", "die", "Kin\u00b7der", "frisst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.2": {"line.1": {"text": "Jhr Schlund verzehret Tag und Stunden/", "tokens": ["Ihr", "Schlund", "ver\u00b7zeh\u00b7ret", "Tag", "und", "Stun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie w\u00e4chst und stirbet in der Flucht.", "tokens": ["Sie", "w\u00e4chst", "und", "stir\u00b7bet", "in", "der", "Flucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Tag hat kaum den Monat funden/", "tokens": ["Der", "Tag", "hat", "kaum", "den", "Mo\u00b7nat", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Monat ein Jahr aufgesucht/", "tokens": ["Der", "Mo\u00b7nat", "ein", "Jahr", "auf\u00b7ge\u00b7sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schleust sich kaum der Ring vom Jahre/", "tokens": ["Es", "schleust", "sich", "kaum", "der", "Ring", "vom", "Jah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ART", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So f\u00e4ngt sich schon ein neues an/", "tokens": ["So", "f\u00e4ngt", "sich", "schon", "ein", "neu\u00b7es", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ADV", "ART", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Di\u00df st\u00fcrtzt ein anders auff die Bahre/", "tokens": ["Di\u00df", "st\u00fcrtzt", "ein", "an\u00b7ders", "auff", "die", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wird auch wieder abgethan.", "tokens": ["Und", "wird", "auch", "wie\u00b7der", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Jahr/ Monat/ Tag und Stunden fliehen/", "tokens": ["Jahr", "/", "Mo\u00b7nat", "/", "Tag", "und", "Stun\u00b7den", "flie\u00b7hen", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was weg/ bleibt ewig weggeschwemmt.", "tokens": ["Was", "weg", "/", "bleibt", "e\u00b7wig", "weg\u00b7ge\u00b7schwemmt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "$(", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Kein Rad kan was zur\u00fccke ziehen/", "tokens": ["Kein", "Rad", "kan", "was", "zu\u00b7r\u00fc\u00b7cke", "zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "PIS", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ketten wird hier nichts gehemmt/", "tokens": ["Mit", "Ket\u00b7ten", "wird", "hier", "nichts", "ge\u00b7hemmt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ADV", "PIS", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Adler mag sich nicht so schwingen", "tokens": ["Der", "Ad\u00b7ler", "mag", "sich", "nicht", "so", "schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Pfeil-geschwinder Hurtigkeit/", "tokens": ["Mit", "Pfeil\u00b7ge\u00b7schwin\u00b7der", "Hur\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als unsre Tage sich verdringen/", "tokens": ["Als", "uns\u00b7re", "Ta\u00b7ge", "sich", "ver\u00b7drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PRF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und sich verlaufft der Krei\u00df der Zeit.", "tokens": ["Und", "sich", "ver\u00b7laufft", "der", "Krei\u00df", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Egypten indenck es zu machen/", "tokens": ["E\u00b7gyp\u00b7ten", "in\u00b7denck", "es", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mahlt einen tieff- und finstren Grund/", "tokens": ["Mahlt", "ei\u00b7nen", "tief\u00b7f", "und", "finst\u00b7ren", "Grund", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "TRUNC", "KON", "ADJA", "NN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Der stets bewahrt von einem Drachen/", "tokens": ["Der", "stets", "be\u00b7wahrt", "von", "ei\u00b7nem", "Dra\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und der mit aufgesperrtem Schlund/", "tokens": ["Und", "der", "mit", "auf\u00b7ge\u00b7sperr\u00b7tem", "Schlund", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den eignen Schwantz ihm abgefressen/", "tokens": ["Den", "eig\u00b7nen", "Schwantz", "ihm", "ab\u00b7ge\u00b7fres\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und uners\u00e4ttlich dran genagt.", "tokens": ["Und", "un\u00b7er\u00b7s\u00e4tt\u00b7lich", "dran", "ge\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer wolte nicht hieraus ermessen/", "tokens": ["Wer", "wol\u00b7te", "nicht", "hier\u00b7aus", "er\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "PAV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Es sey uns von der Zeit gesagt?", "tokens": ["Es", "sey", "uns", "von", "der", "Zeit", "ge\u00b7sagt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So ists demnach so hoch zu sch\u00e4tzen?", "tokens": ["So", "ists", "dem\u00b7nach", "so", "hoch", "zu", "sch\u00e4t\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PAV", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sich die Lebens-Frist verl\u00e4ngt/", "tokens": ["Wenn", "sich", "die", "Le\u00b7bens\u00b7Frist", "ver\u00b7l\u00e4ngt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer was den Jahren bey kan setzen/", "tokens": ["Wer", "was", "den", "Jah\u00b7ren", "bey", "kan", "set\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PWS", "ART", "NN", "APPR", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df der so grosses Gut empf\u00e4ngt?", "tokens": ["Da\u00df", "der", "so", "gros\u00b7ses", "Gut", "emp\u00b7f\u00e4ngt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was hilfft\u2019s mit fl\u00fcchtigem Gewebe", "tokens": ["Was", "hilfft's", "mit", "fl\u00fcch\u00b7ti\u00b7gem", "Ge\u00b7we\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das kurtze Garn zu unterziehn?", "tokens": ["Das", "kurt\u00b7ze", "Garn", "zu", "un\u00b7ter\u00b7ziehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df man wol beschneyet lebe/", "tokens": ["Und", "da\u00df", "man", "wol", "be\u00b7schne\u00b7yet", "le\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich mit viel Seuffzern zu bem\u00fchn?", "tokens": ["Sich", "mit", "viel", "Seuff\u00b7zern", "zu", "be\u00b7m\u00fchn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Kind das in der Wieg\u2019 erbleichet/", "tokens": ["Ein", "Kind", "das", "in", "der", "Wieg'", "er\u00b7blei\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und der geb\u00fcckt von hinnen f\u00e4hrt/", "tokens": ["Und", "der", "ge\u00b7b\u00fcckt", "von", "hin\u00b7nen", "f\u00e