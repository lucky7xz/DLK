ra"}}, "stanza.7": {"line.1": {"text": "Der Mensch bleibt nur der Zeiten Beuthe/", "tokens": ["Der", "Mensch", "bleibt", "nur", "der", "Zei\u00b7ten", "Beu\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch Elemente tauren nicht.", "tokens": ["Auch", "E\u00b7le\u00b7men\u00b7te", "tau\u00b7ren", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Wer wolte nun nicht lieber heute/", "tokens": ["Wer", "wol\u00b7te", "nun", "nicht", "lie\u00b7ber", "heu\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "PTKNEG", "ADV", "ADV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gesegnen dieser Sonnen Licht/", "tokens": ["Ge\u00b7seg\u00b7nen", "die\u00b7ser", "Son\u00b7nen", "Licht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als da\u00df bey Martern und bey Qu\u00e4len/", "tokens": ["Als", "da\u00df", "bey", "Mar\u00b7tern", "und", "bey", "Qu\u00e4\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und aller Schmertzen Uberflu\u00df/", "tokens": ["Und", "al\u00b7ler", "Schmert\u00b7zen", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er m\u00f6g ein tieffes Alter zehlen/", "tokens": ["Er", "m\u00f6g", "e