word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKNEG", "VVINF", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Verj\u00fcngten Glanzes la\u00df sie funkeln", "tokens": ["Ver\u00b7j\u00fcng\u00b7ten", "Glan\u00b7zes", "la\u00df", "sie", "fun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVIMP", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ins Fr\u00fchrot deiner Osterzeit!", "tokens": ["Ins", "Fr\u00fch\u00b7rot", "dei\u00b7ner", "Os\u00b7ter\u00b7zeit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn horch, schon brausen Jubellieder,", "tokens": ["Denn", "horch", ",", "schon", "brau\u00b7sen", "Ju\u00b7bel\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und \u00fcber deinem Haupte wieder", "tokens": ["Und", "\u00fc\u00b7ber", "dei\u00b7nem", "Haup\u00b7te", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geht auf des Reiches Herrlichkeit.", "tokens": ["Geht", "auf", "des", "Rei\u00b7ches", "Herr\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Durch Orgelton und Schall der Glocken", "tokens": ["Durch", "Or\u00b7gel\u00b7ton", "und", "Schall", "der", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN"], 