{"textgrid.poem.61766": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "An Deutschland", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun wirf hinweg den Witwenschleier,", "tokens": ["Nun", "wirf", "hin\u00b7weg", "den", "Wit\u00b7wensc\u00b7hlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun g\u00fcrte dich zur Hochzeitsfeier,", "tokens": ["Nun", "g\u00fcr\u00b7te", "dich", "zur", "Hoch\u00b7zeits\u00b7fei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "O Deutschland, hohe Siegerin!", "tokens": ["O", "Deutschland", ",", "ho\u00b7he", "Sie\u00b7ge\u00b7rin", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du mit Klagen und Entsagen", "tokens": ["Die", "du", "mit", "Kla\u00b7gen", "und", "Ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Durch vierundsechzig Jahr' getragen,", "tokens": ["Durch", "vie\u00b7rund\u00b7sech\u00b7zig", "Jahr'", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Zeit der Trauer ist dahin;", "tokens": ["Die", "Zeit", "der", "Trau\u00b7er", "ist", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die Zeit der Zwietracht und Beschwerde,", "tokens": ["Die", "Zeit", "der", "Zwiet\u00b7racht", "und", "Be\u00b7schwer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da du am durchgeborstnen Herde", "tokens": ["Da", "du", "am", "durch\u00b7ge\u00b7borst\u00b7nen", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Staube sa\u00dfest tiefgeb\u00fcckt,", "tokens": ["Im", "Stau\u00b7be", "sa\u00b7\u00dfest", "tief\u00b7ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und kaum dein Lied mit leisem Weinen", "tokens": ["Und", "kaum", "dein", "Lied", "mit", "lei\u00b7sem", "Wei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mehr fragte nach den Edelsteinen,", "tokens": ["Mehr", "frag\u00b7te", "nach", "den", "E\u00b7del\u00b7stei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die einst dein Diadem geschm\u00fcckt.", "tokens": ["Die", "einst", "dein", "Di\u00b7a\u00b7dem", "ge\u00b7schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wohl glaubten sie dein Schwert zerbrochen,", "tokens": ["Wohl", "glaub\u00b7ten", "sie", "dein", "Schwert", "zer\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl zuckten sie, wenn du gesprochen,", "tokens": ["Wohl", "zuck\u00b7ten", "sie", ",", "wenn", "du", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Achsel k\u00fchl im V\u00f6lkerrat,", "tokens": ["Die", "Ach\u00b7sel", "k\u00fchl", "im", "V\u00f6l\u00b7ker\u00b7rat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch unter Tr\u00e4nen wuchs im stillen", "tokens": ["Doch", "un\u00b7ter", "Tr\u00e4\u00b7nen", "wuchs", "im", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "VVFIN", "APPRART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Sehnsucht dir zum heil'gen Willen,", "tokens": ["Die", "Sehn\u00b7sucht", "dir", "zum", "heil'\u00b7gen", "Wil\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Wille dir zur Kraft der Tat.", "tokens": ["Der", "Wil\u00b7le", "dir", "zur", "Kraft", "der", "Tat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und endlich satt, die Schmach zu tragen,", "tokens": ["Und", "end\u00b7lich", "satt", ",", "die", "Schmach", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zerrissest du in sieben Tagen", "tokens": ["Zer\u00b7ris\u00b7sest", "du", "in", "sie\u00b7ben", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Netz, das t\u00f6dlich dich umschn\u00fcrt,", "tokens": ["Das", "Netz", ",", "das", "t\u00f6d\u00b7lich", "dich", "um\u00b7schn\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und heischtest, mit beerztem Schritte", "tokens": ["Und", "heischtest", ",", "mit", "be\u00b7erz\u00b7tem", "Schrit\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "APPR", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Hintretend in Europas Mitte,", "tokens": ["Hin\u00b7tre\u00b7tend", "in", "Eu\u00b7ro\u00b7pas", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den Platz zur\u00fcck, der dir geb\u00fchrt.", "tokens": ["Den", "Platz", "zu\u00b7r\u00fcck", ",", "der", "dir", "ge\u00b7b\u00fchrt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und als der Erbfeind dann, der Franze,", "tokens": ["Und", "als", "der", "Erb\u00b7feind", "dann", ",", "der", "Fran\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nach deiner Ehren jungem Kranze", "tokens": ["Nach", "dei\u00b7ner", "Eh\u00b7ren", "jun\u00b7gem", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Hand erhub von Neid verzehrt,", "tokens": ["Die", "Hand", "er\u00b7hub", "von", "Neid", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zur Riesin pl\u00f6tzlich umgeschaffen", "tokens": ["Zur", "Rie\u00b7sin", "pl\u00f6tz\u00b7lich", "um\u00b7ge\u00b7schaf\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie st\u00fcrmtest du ins Feld der Waffen,", "tokens": ["Wie", "st\u00fcrm\u00b7test", "du", "ins", "Feld", "der", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Behelmte, mit dem Flammenschwert!", "tokens": ["Be\u00b7helm\u00b7te", ",", "mit", "dem", "Flam\u00b7men\u00b7schwert", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O gro\u00dfe, gottgesandte Stunde,", "tokens": ["O", "gro\u00b7\u00dfe", ",", "gott\u00b7ge\u00b7sand\u00b7te", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da deines Haders alte Wunde", "tokens": ["Da", "dei\u00b7nes", "Ha\u00b7ders", "al\u00b7te", "Wun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die heil'ge Not auf ewig schlo\u00df,", "tokens": ["Die", "heil'\u00b7ge", "Not", "auf", "e\u00b7wig", "schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wunderkr\u00e4ftig dir im Innern", "tokens": ["Und", "wun\u00b7der\u00b7kr\u00e4f\u00b7tig", "dir", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus alter Zeit ein stolz Erinnern,", "tokens": ["Aus", "al\u00b7ter", "Zeit", "ein", "stolz", "E\u00b7rin\u00b7nern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Bild zuk\u00fcnft'ger Gr\u00f6\u00dfe spro\u00df!", "tokens": ["Ein", "Bild", "zu\u00b7k\u00fcnft'\u00b7ger", "Gr\u00f6\u00b7\u00dfe", "spro\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wie Erz durchstr\u00f6mte deine Glieder", "tokens": ["Wie", "Erz", "durch\u00b7str\u00f6m\u00b7te", "dei\u00b7ne", "Glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Mark der Nibelungen wieder,", "tokens": ["Das", "Mark", "der", "Ni\u00b7be\u00b7lun\u00b7gen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der Geist des Herrn war \u00fcber dir,", "tokens": ["Der", "Geist", "des", "Herrn", "war", "\u00fc\u00b7ber", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und unterm Schall der Kriegsposaunen", "tokens": ["Und", "un\u00b7term", "Schall", "der", "Kriegs\u00b7po\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aufpflanztest du, der Welt zum Staunen,", "tokens": ["Auf\u00b7pflanz\u00b7test", "du", ",", "der", "Welt", "zum", "Stau\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In Frankreichs Herz dein Siegspanier.", "tokens": ["In", "Fran\u00b7kreichs", "Herz", "dein", "Siegs\u00b7pa\u00b7nier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Da war dir bald, mit Blut beronnen,", "tokens": ["Da", "war", "dir", "bald", ",", "mit", "Blut", "be\u00b7ron\u