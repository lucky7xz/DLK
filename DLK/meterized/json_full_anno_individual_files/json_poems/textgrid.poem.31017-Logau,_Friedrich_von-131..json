{"textgrid.poem.31017": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "131.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kan es noch nicht thun, da\u00df ich mich solte stellen", "tokens": ["Ich", "kan", "es", "noch", "nicht", "thun", ",", "da\u00df", "ich", "mich", "sol\u00b7te", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hin zur Poeten-Rey; ein Urthel mag vor fellen,", "tokens": ["Hin", "zur", "Po\u00b7e\u00b7ten\u00b7Rey", ";", "ein", "Ur\u00b7thel", "mag", "vor", "fel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$.", "ART", "NN", "VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der selbst ist ein Poet mit recht und durch die Kunst;", "tokens": ["Der", "selbst", "ist", "ein", "Po\u00b7et", "mit", "recht", "und", "durch", "die", "Kunst", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VAFIN", "ART", "NN", "APPR", "ADJD", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fellt dieses nun f\u00fcr mich, so ist mirs sondre Gunst;", "tokens": ["Fellt", "die\u00b7ses", "nun", "f\u00fcr", "mich", ",", "so", "ist", "mirs", "sond\u00b7re", "Gunst", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADV", "APPR", "PPER", "$,", "ADV", "VAFIN", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo nicht, so stets dahin. Zu \u00dcbung meiner Sinnen", "tokens": ["Wo", "nicht", ",", "so", "stets", "da\u00b7hin", ".", "Zu", "\u00dc\u00b7bung", "mei\u00b7ner", "Sin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PTKNEG", "$,", "ADV", "ADV", "PAV", "$.", "APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist alles angesehn, verfehlet gleich das k\u00fcnnen,", "tokens": ["Ist", "al\u00b7les", "an\u00b7ge\u00b7sehn", ",", "ver\u00b7feh\u00b7let", "gleich", "das", "k\u00fcn\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$,", "VVFIN", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zu mal mich sonst noch ehrt ein anderes Beginnen.", "tokens": ["Zu", "mal", "mich", "sonst", "noch", "ehrt", "ein", "an\u00b7de\u00b7res", "Be\u00b7gin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PPER", "ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich kan es noch nicht thun, da\u00df ich mich solte stellen", "tokens": ["Ich", "kan", "es", "noch", "nicht", "thun", ",", "da\u00df", "ich", "mich", "sol\u00b7te", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hin 