{"dta.poem.9603": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ljsette hat mein hertz au\u00df meiner brust geri\u00dfen/", "tokens": ["Ljset\u00b7te", "hat", "mein", "hertz", "au\u00df", "mei\u00b7ner", "brust", "ge\u00b7ri\u00b7\u00dfen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und l\u00e4ugnet den ver\u00fcbten raub/", "tokens": ["Und", "l\u00e4ug\u00b7net", "den", "ver\u00b7\u00fcb\u00b7ten", "raub", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie ist vor meinen bitten taub", "tokens": ["Sie", "ist", "vor", "mei\u00b7nen", "bit\u00b7ten", "taub"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und will nicht vom verlust/ noch von erstattung wissen.", "tokens": ["Und", "will", "nicht", "vom", "ver\u00b7lust", "/", "noch", "von", "er\u00b7stat\u00b7tung", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "APPRART", "VVFIN", "$(", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Mein hertze selbst bekennt/ es leb allein in ihr;", "tokens": ["Mein", "hert\u00b7ze", "selbst", "be\u00b7kennt", "/", "es", "leb", "al\u00b7lein", "in", "ihr", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVFIN", "$(", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie aber spricht darzu: sie habe nichts von mir.", "tokens": ["Sie", "a\u00b7ber", "spricht", "dar\u00b7zu", ":", "sie", "ha\u00b7be", "nichts", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PAV", "$.", "PPER", "VAFIN", "PIS", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Mein hertze daurt mich nicht/ Lisette mag es haben/", "tokens": ["Mein", "hert\u00b7ze", "daurt", "mich", "nicht", "/", "Li\u00b7set\u00b7te", "mag", "es", "ha\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "NE", "VMFIN", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich liefr es selbst ihr willig ein/", "tokens": ["Ich", "liefr", "es", "selbst", "ihr", "wil\u00b7lig", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo kan wol selbes besser seyn?", "tokens": ["Wo", "kan", "wol", "sel\u00b7bes", "bes\u00b7ser", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nur sie verl\u00e4ugne nicht/ da\u00df es in ihr begraben.", "tokens": ["Nur", "sie", "ver\u00b7l\u00e4ug\u00b7ne", "nicht", "/", "da\u00df", "es", "in", "ihr", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein sclav erschrickt so sehr vor seinen fesseln nicht/", "tokens": ["Ein", "sclav", "er\u00b7schrickt", "so", "sehr", "vor", "sei\u00b7nen", "fes\u00b7seln", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn er nur wissen soll/ wer selbe zugericht.", "tokens": ["Wenn", "er", "nur", "wis\u00b7sen", "soll", "/", "wer", "sel\u00b7be", "zu\u00b7ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$(", "PWS", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Gestehs/ Lisette/ nur/ du hast es doch genommen/", "tokens": ["Ge\u00b7stehs", "/", "Li\u00b7set\u00b7te", "/", "nur", "/", "du", "hast", "es", "doch", "ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "ADV", "$(", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der himmel weiset selbst auff dich/", "tokens": ["Der", "him\u00b7mel", "wei\u00b7set", "selbst", "auff", "dich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich suche auch sonst nirgends mich:", "tokens": ["Ich", "su\u00b7che", "auch", "sonst", "nir\u00b7gends", "mich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich wei\u00df ja/ wer da hat mein bestes theil bekommen/", "tokens": ["Ich", "wei\u00df", "ja", "/", "wer", "da", "hat", "mein", "bes\u00b7tes", "theil", "be\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$(", "PWS", "ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dein atla\u00df-weiche hand hats ja aus mir ger\u00fcckt/", "tokens": ["Dein", "at\u00b7la\u00df\u00b7wei\u00b7che", "hand", "hats", "ja", "aus", "mir", "ge\u00b7r\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Als ich sie unverhofft an meine brust gebr\u00fcckt.", "tokens": ["Als", "ich", "sie", "un\u00b7ver\u00b7hofft", "an", "mei\u00b7ne", "brust", "ge\u00b7br\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Behalts/ Lisette/ denn/ es soll dir zugeh\u00f6ren/", "tokens": ["Be\u00b7halts", "/", "Li\u00b7set\u00b7te", "/", "denn", "/", "es", "soll", "dir", "zu\u00b7ge\u00b7h\u00f6\u00b7ren", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "NE", "$(", "KON", "$(", "PPER", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht sch\u00e4me dich des schlechten guts:", "tokens": ["Nicht", "sch\u00e4\u00b7me", "dich", "des", "schlech\u00b7ten", "guts", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kleinste tr\u00f6pfchen seines bluts", "tokens": ["Das", "kleins\u00b7te", "tr\u00f6pf\u00b7chen", "sei\u00b7nes", "bluts"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Soll mit besondrer art dich/ meine g\u00f6ttin/ ehren.", "tokens": ["Soll", "mit", "be\u00b7sond\u00b7rer", "art", "dich", "/", "mei\u00b7ne", "g\u00f6t\u00b7tin", "/", "eh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "APPR", "ADJA", "NN", "PPER", "$(", "PPOSAT", "NN", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nur g\u00f6nne/ da\u00df mir di\u00df zum ruhme werd\u2019 gesetzt:", "tokens": ["Nur", "g\u00f6n\u00b7ne", "/", "da\u00df", "mir", "di\u00df", "zum", "ruh\u00b7me", "werd'", "ge\u00b7setzt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$(", "KOUS", "PPER", "PDS", "APPRART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des sch\u00f6nsten leibes ist mein hertze werth gesch\u00e4tzt.", "tokens": ["Des", "sch\u00f6ns\u00b7ten", "lei\u00b7bes", "ist", "mein", "hert\u00b7ze", "werth", "ge\u00b7sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}