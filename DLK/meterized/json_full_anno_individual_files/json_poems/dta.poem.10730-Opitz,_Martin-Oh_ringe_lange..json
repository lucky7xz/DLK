{"dta.poem.10730": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Oh ringe lange.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Weil das Gl\u00fcck vnter dir du heltest in dem zwange/", "tokens": ["Weil", "das", "Gl\u00fcck", "vn\u00b7ter", "dir", "du", "hel\u00b7test", "in", "dem", "zwan\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PPER", "PPER", "VVFIN", "APPR", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd dir sein lachen nit erschwellen mag das Hertz/", "tokens": ["Vnd", "dir", "sein", "la\u00b7chen", "nit", "er\u00b7schwel\u00b7len", "mag", "das", "Hertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPOSAT", "ADJA", "PTKNEG", "VVINF", "VMFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil auch sein trutz vnd macht dir ist ein blosser Schertz/", "tokens": ["Weil", "auch", "sein", "trutz", "vnd", "macht", "dir", "ist", "ein", "blos\u00b7ser", "Schertz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Spricht jedes dapffers Hertz: Heldin/ oh ringe lange!", "tokens": ["Spricht", "je\u00b7des", "dapf\u00b7fers", "Hertz", ":", "Hel\u00b7din", "/", "oh", "rin\u00b7ge", "lan\u00b7ge", "!"], "token_info":