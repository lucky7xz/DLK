{"dta.poem.8116": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "Langsame, steigende Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O der Angst Stimme, die herrufend vom Abgrunde", "tokens": ["O", "der", "Angst", "Stim\u00b7me", ",", "die", "her\u00b7ru\u00b7fend", "vom", "Ab\u00b7grun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "NN", "$,", "PRELS", "VVPP", "APPRART", "NN"], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.2": {"text": "Dumpf t\u00f6net\u2019, aus Staubwolken zu Licht aufklagte!", "tokens": ["Dumpf", "t\u00f6\u00b7net'", ",", "aus", "Staub\u00b7wol\u00b7ken", "zu", "Licht", "auf\u00b7klag\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+---+-+-", "measure": "unknown.measure.penta"}, "line.3": {"text": "Und nunmehr sterbend noch graunvoller schwieg, furchtbarer,", "tokens": ["Und", "nun\u00b7mehr", "ster\u00b7bend", "noch", "graun\u00b7vol\u00b7ler", "schwieg", ",", "furcht\u00b7ba\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADV", "ADJD", "VVFIN", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verstummt, schrecket\u2019, als hinsinkend sie Wehklag\u2019 ausrief!", "tokens": ["Ver\u00b7stummt", ",", "schre\u00b7cket'", ",", "als", "hin\u00b7sin\u00b7kend", "sie", "Weh\u00b7klag'", "aus\u00b7rief", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KOUS", "VV