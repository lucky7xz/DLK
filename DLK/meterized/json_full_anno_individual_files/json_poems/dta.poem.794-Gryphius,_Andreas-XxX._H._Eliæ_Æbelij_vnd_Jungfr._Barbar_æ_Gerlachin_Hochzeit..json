{"dta.poem.794": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "XxX.  \n H.  Eli\u00e6 \u00c6belij  vnd Jungfr. Barbar \u00e6  \n Gerlachin Hochzeit.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Bj\u00dfher/ h\u00f6rt ich allein Mein werther Freund euch sing&#7869;: ", "tokens": ["Bj\u00df\u00b7her", "/", "h\u00f6rt", "ich", "al\u00b7lein", "Mein", "wert\u00b7her", "Freund", "euch", "sing", "&#7869;", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "punct"], "pos": ["NE", "$(", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "PPER", "VVFIN", "$(", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wofern es singen hei\u00dft/ wenn nicht geferten sind/", "tokens": ["Wo\u00b7fern", "es", "sin\u00b7gen", "hei\u00dft", "/", "wenn", "nicht", "ge\u00b7fer\u00b7ten", "sind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VVFIN", "$(", "KOUS", "PTKNEG", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schawt wie der Himmel Euch zu newen danck verbind/", "tokens": ["Schawt", "wie", "der", "Him\u00b7mel", "Euch", "zu", "ne\u00b7wen", "danck", "ver\u00b7bind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PPER", "PTKZU", "VVINF", "APPR", "NE", "$("], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Der zu Violl vnd Laut/ die liebe Brautt mu\u00df bringen/", "tokens": ["Der", "zu", "Vi\u00b7oll", "vnd", "Laut", "/", "die", "lie\u00b7be", "Brautt", "mu\u00df", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "KON", "APPR", "$(", "ART", "ADJA", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Woll. Last die b\u00e4lge gehn! nun wird die Orgel klingen!", "tokens": ["Woll", ".", "Last", "die", "b\u00e4l\u00b7ge", "gehn", "!", "nun", "wird", "die", "Or\u00b7gel", "klin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "$.", "NN", "ART", "NN", "VVINF", "$.", "ADV", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stell\u2019t lange pausen eyn/ singt hurtig nicht zu lind", "tokens": ["Stell't", "lan\u00b7ge", "pau\u00b7sen", "eyn", "/", "singt", "hur\u00b7tig", "nicht", "zu", "lind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "VVPP", "VAINF", "$(", "VVFIN", "ADJD", "PTKNEG", "PTKA", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den Euch bequemen Ba\u00df. Wenn jhr Tenor sich find", "tokens": ["Den", "Euch", "be\u00b7que\u00b7men", "Ba\u00df", ".", "Wenn", "jhr", "Te\u00b7nor", "sich", "find"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJA", "NN", "$.", "KOUS", "PPOSAT", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird letzlich der Discant sich in die tripel schwingen.", "tokens": ["Wird", "letz\u00b7lich", "der", "Dis\u00b7cant", "sich", "in", "die", "tri\u00b7pel", "schwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+--++-+-+-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Alt/ so jtzt noch ruht/ vnd was die kluge ", "tokens": ["Der", "Alt", "/", "so", "jtzt", "noch", "ruht", "/", "vnd", "was", "die", "klu\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "ADV", "VVFIN", "$(", "KON", "PWS", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vor St\u00fccklin mehr erdacht/ drauff man so trefflich h\u00e4lt", "tokens": ["Vor", "St\u00fcck\u00b7lin", "mehr", "er\u00b7dacht", "/", "drauff", "man", "so", "treff\u00b7lich", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "ADV", "VVPP", "$(", "PAV", "PIS", "ADV", "ADJD", "VVFIN"], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wird schon zu rechter zeit sich ins Concert auffmachen. ", "tokens": ["Wird", "schon", "zu", "rech\u00b7ter", "zeit", "sich", "ins", "Con\u00b7cert", "auff\u00b7ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ADJA", "NN", "PRF", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wol dem/ der also singt! wie viel gew\u00fcndschter Lust/", "tokens": ["Wol", "dem", "/", "der", "al\u00b7so", "singt", "!", "wie", "viel", "ge\u00b7w\u00fcnd\u00b7schter", "Lust", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$(", "ART", "ADV", "VVFIN", "$.", "PWAV", "PIAT", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Ist/ dunckt mich ewer Hertz Herr Aebel jhm bewust:", "tokens": ["Ist", "/", "dunckt", "mich", "e\u00b7wer", "Hertz", "Herr", "A\u00b7e\u00b7bel", "jhm", "be\u00b7wust", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Wie wird die Jungfer Brautt doch den so Gerne lachen.", "tokens": ["Wie", "wird", "die", "Jung\u00b7fer", "Brautt", "doch", "den", "so", "Ger\u00b7ne", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NE", "ADV", "ART", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}