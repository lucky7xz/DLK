{"textgrid.poem.36428": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Viertes Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von allen M\u00e4nnern, die dich je bedrohten", "tokens": ["Von", "al\u00b7len", "M\u00e4n\u00b7nern", ",", "die", "dich", "je", "be\u00b7droh\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Bin ich der geilste: sieh' mich zitternd an ... !", "tokens": ["Bin", "ich", "der", "geils\u00b7te", ":", "sieh'", "mich", "zit\u00b7ternd", "an", "...", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "$.", "VVIMP", "PPER", "ADJD", "PTKVZ", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich zerre deine Br\u00fcste Spann f\u00fcr Spann", "tokens": ["Ich", "zer\u00b7re", "dei\u00b7ne", "Br\u00fcs\u00b7te", "Spann", "f\u00fcr", "Spann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und werde sie auf deinem R\u00fccken knoten.", "tokens": ["Und", "wer\u00b7de", "sie", "auf", "dei\u00b7nem", "R\u00fc\u00b7cken", "kno\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch deine F\u00fc\u00dfe kn\u00fcpfe ich daran,", "tokens": ["Auch", "dei\u00b7ne", "F\u00fc\u00b7\u00dfe", "kn\u00fcp\u00b7fe", "ich", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "PPER", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und binde deine kleinen wei\u00dfen Pfoten.", "tokens": ["Und", "bin\u00b7de", "dei\u00b7ne", "klei\u00b7nen", "wei\u00b7\u00dfen", "Pfo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn den Leib du r\u00f6chelnd mir geboten", "tokens": ["Und", "wenn", "den", "Leib", "du", "r\u00f6\u00b7chelnd", "mir", "ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADJD", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewunderst du in mir den starken Mann.", "tokens": ["Be\u00b7wun\u00b7derst", "du", "in", "mir", "den", "star\u00b7ken", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wenn du schreist, so schlitz' ich deinen runden", "tokens": ["Und", "wenn", "du", "schreist", ",", "so", "schlitz'", "ich", "dei\u00b7nen", "run\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und weichen Leib mir auf mit kaltem Streiche.", "tokens": ["Und", "wei\u00b7chen", "Leib", "mir", "auf", "mit", "kal\u00b7tem", "Strei\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], 