: ["Weil", "es"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Vergib der k\u00fchnen Hand, wenn sie mit recht gethan.", "tokens": ["Ver\u00b7gib", "der", "k\u00fch\u00b7nen", "Hand", ",", "wenn", "sie", "mit", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Pflicht befahl es mir; die Ehrfurcht trieb mich an.", "tokens": ["Die", "Pflicht", "be\u00b7fahl", "es", "mir", ";", "die", "Ehr\u00b7furcht", "trieb", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Du lie\u00dft mir ja zur ", "tokens": ["Du", "lie\u00dft", "mir", "ja", "zur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.43": {"text": "Drum such ich, ", "tokens": ["Drum", "such", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "In Unterth\u00e4nigkeit reich ich ", "tokens": ["In", "Un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", "reich", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Mein Herzog! schenke ihm nur einen holden Blick,", "tokens": ["Mein", "Her\u00b7zog", "!", "schen\u00b7ke", "ihm", "nur", "ei\u00b7nen", "hol\u00b7den", "Blick", ","], "token_info"