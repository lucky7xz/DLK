{"textgrid.poem.62717": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Zuschrifft bey der ersten Auflage", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "so hoch dein Purpur prangt;", "tokens": ["so", "hoch", "dein", "Pur\u00b7pur", "prangt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So viel du Ehr und Macht durch die Geburt erlangt!", "tokens": ["So", "viel", "du", "Ehr", "und", "Macht", "durch", "die", "Ge\u00b7burt", "er\u00b7langt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "KON", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So scharf dein Angesicht und Auge F\u00fcrstlich strahlet;", "tokens": ["So", "scharf", "dein", "An\u00b7ge\u00b7sicht", "und", "Au\u00b7ge", "F\u00fcrst\u00b7lich", "strah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "KON", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So viel man dir an Furcht, an Ehr und Liebe zahlet;", "tokens": ["So", "viel", "man", "dir", "an", "Furcht", ",", "an", "Ehr", "und", "Lie\u00b7be", "zah\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "PPER", "APPR", "NN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So gro\u00df ist auch die Huld und Gro\u00dfmuth die dich schm\u00fcckt.", "tokens": ["So", "gro\u00df", "ist", "auch", "die", "Huld", "und", "Gro\u00df\u00b7muth", "die", "dich", "schm\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "NN", "ART", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer ", "tokens": ["Wer"], "token_info": ["word"], "pos": ["PWS"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "F\u00e4llt meinen Worten bey, und mu\u00df mit mir bekennen,", "tokens": ["F\u00e4llt", "mei\u00b7nen", "Wor\u00b7ten", "bey", ",", "und", "mu\u00df", "mit", "mir", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du seyst Trajan, August, ja Titus selbst zu nennen.", "tokens": ["Du", "seyst", "Tra\u00b7jan", ",", "Au\u00b7gust", ",", "ja", "Ti\u00b7tus", "selbst", "zu", "nen\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NE", "$,", "NN", "$,", "ADV", "NE", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was aber sag ich viel von deinem hohen Geist,", "tokens": ["Was", "a\u00b7ber", "sag", "ich", "viel", "von", "dei\u00b7nem", "ho\u00b7hen", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Den ", "tokens": ["Den"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Du bist ein kluger F\u00fcrst/ und ein gelehrtes Wissen,", "tokens": ["Du", "bist", "ein", "klu\u00b7ger", "F\u00fcrst", "/", "und", "ein", "ge\u00b7lehr\u00b7tes", "Wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Hat deine F\u00fcrsten-Brust ganz zu sich hingerissen.", "tokens": ["Hat", "dei\u00b7ne", "F\u00fcrs\u00b7ten\u00b7Brust", "ganz", "zu", "sich", "hin\u00b7ge\u00b7ris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Du kennst der Weisheit Schmuck; du schliessest mit Vernunft;", "tokens": ["Du", "kennst", "der", "Weis\u00b7heit", "Schmuck", ";", "du", "schlies\u00b7sest", "mit", "Ver\u00b7nunft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "$.", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Siehst alles gr\u00fcndlich ein, und folgest nie der Zunft,", "tokens": ["Siehst", "al\u00b7les", "gr\u00fcnd\u00b7lich", "ein", ",", "und", "fol\u00b7gest", "nie", "der", "Zunft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die sich an Thorheit labt, und falsche Gr\u00fcnde lehret:", "tokens": ["Die", "sich", "an", "Thor\u00b7heit", "labt", ",", "und", "fal\u00b7sche", "Gr\u00fcn\u00b7de", "leh\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VVFIN", "$,", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Indem dein Ohr nur das, was rein und gr\u00fcndlich, h\u00f6ret.", "tokens": ["In\u00b7dem", "dein", "Ohr", "nur", "das", ",", "was", "rein", "und", "gr\u00fcnd\u00b7lich", ",", "h\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "PDS", "$,", "PRELS", "ADJD", "KON", "ADJD", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Du fragst, ", "tokens": ["Du", "fragst", ","], "token_info": ["word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.18": {"text": "Wie richtig jener denkt, wie weit der andre geht.", "tokens": ["Wie", "rich\u00b7tig", "je\u00b7ner", "denkt", ",", "wie", "weit", "der", "and\u00b7re", "geht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDS", "VVFIN", "$,", "PWAV", "ADJD", "ART", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Du bist gelehrt und klug, und von besondern Gaben:", "tokens": ["Du", "bist", "ge\u00b7lehrt", "und", "klug", ",", "und", "von", "be\u00b7son\u00b7dern", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "KON", "ADJD", "$,", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "O! m\u00f6chte doch die Welt viel solche F\u00fcrsten haben!", "tokens": ["O", "!", "m\u00f6ch\u00b7te", "doch", "die", "Welt", "viel", "sol\u00b7che", "F\u00fcrs\u00b7ten", "ha\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "ADV", "ART", "NN", "ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es sieht dein hoher Geist in tiefe Sachen ein;", "tokens": ["Es", "sieht", "dein", "ho\u00b7her", "Geist", "in", "tie\u00b7fe", "Sa\u00b7chen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Du l\u00f6\u00dfest W\u00f6rter auf, die andern R\u00e4tzel seyn.", "tokens": ["Du", "l\u00f6\u00b7\u00dfest", "W\u00f6r\u00b7ter", "auf", ",", "die", "an\u00b7dern", "R\u00e4t\u00b7zel", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$,", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Du sinnest selbst was aus; giebst Kennern aufzurathen;", "tokens": ["Du", "sin\u00b7nest", "selbst", "was", "aus", ";", "giebst", "Ken\u00b7nern", "auf\u00b7zu\u00b7ra\u00b7then", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PTKVZ", "$.", "VVFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Kurz: Deinen hohen Geist sieht man aus deinen Thaten.", "tokens": ["Kurz", ":", "Dei\u00b7nen", "ho\u00b7hen", "Geist", "sieht", "man", "aus", "dei\u00b7nen", "Tha\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPOSAT", "ADJA", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.26": {"text": "Was Wunder, wenn du auch gelehrte M\u00e4nner liebst?", "tokens": ["Was", "Wun\u00b7der", ",", "wenn", "du", "auch", "ge\u00b7lehr\u00b7te", "M\u00e4n\u00b7ner", "liebst", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du r\u00fchmest ihren Flei\u00df, und weist sie hoch zu sch\u00e4tzen", "tokens": ["Du", "r\u00fch\u00b7mest", "ih\u00b7ren", "Flei\u00df", ",", "und", "weist", "sie", "hoch", "zu", "sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn sie sich nur bem\u00fchn, was kluges aufzusetzen.", "tokens": ["Wenn", "sie", "sich", "nur", "be\u00b7m\u00fchn", ",", "was", "klu\u00b7ges", "auf\u00b7zu\u00b7set\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VVINF", "$,", "PWS", "ADJA", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die\u00df weis die ganze Welt; ich mu\u00df es auch gestehn;", "tokens": ["Die\u00df", "weis", "die", "gan\u00b7ze", "Welt", ";", "ich", "mu\u00df", "es", "auch", "ge\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKVZ", "ART", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Ich habe dieses Gl\u00fcck auch oft von ", "tokens": ["Ich", "ha\u00b7be", "die\u00b7ses", "Gl\u00fcck", "auch", "oft", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "NN", "ADV", "ADV", "APPR"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Du hast ein Blat von mir sehr gn\u00e4dig durchgelesen,", "tokens": ["Du", "hast", "ein", "Blat", "von", "mir", "sehr", "gn\u00e4\u00b7dig", "durch\u00b7ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ob ichs gleich nicht verdient, und auch nicht werth gewesen.", "tokens": ["Ob", "ichs", "gleich", "nicht", "ver\u00b7dient", ",", "und", "auch", "nicht", "werth", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PTKNEG", "VVPP", "$,", "KON", "ADV", "PTKNEG", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Da\u00df ich mein Bergwerks St\u00fcck, und neues Saitenspiel,", "tokens": ["Da\u00df", "ich", "mein", "Berg\u00b7werks", "St\u00fcck", ",", "und", "neu\u00b7es", "Sai\u00b7ten\u00b7spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Auch wiederum vor ", "tokens": ["Auch", "wie\u00b7de\u00b7rum", "vor"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "APPR"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.35": {"text": "Und abermahls so frey vor deinen Ohren singe.", "tokens": ["Und", "a\u00b7ber\u00b7mahls", "so", "frey", "vor", "dei\u00b7nen", "Oh\u00b7ren", "sin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich widme ", "tokens": ["Ich", "wid\u00b7me"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.37": {"text": "Obs gleich sehr wenig Kunst und Zierde in sich hat.", "tokens": ["Obs", "gleich", "sehr", "we\u00b7nig", "Kunst", "und", "Zier\u00b7de", "in", "sich", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADV", "PIAT", "NN", "KON", "NN", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Jedoch den gr\u00f6\u00dften Schmuck durch ", "tokens": ["Je\u00b7doch", "den", "gr\u00f6\u00df\u00b7ten", "Schmuck", "durch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR"], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.39": {"text": "Weil es ", "tokens": ["Weil", "es"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Vergib der k\u00fchnen Hand, wenn sie mit recht gethan.", "tokens": ["Ver\u00b7gib", "der", "k\u00fch\u00b7nen", "Hand", ",", "wenn", "sie", "mit", "recht", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Die Pflicht befahl es mir; die Ehrfurcht trieb mich an.", "tokens": ["Die", "Pflicht", "be\u00b7fahl", "es", "mir", ";", "die", "Ehr\u00b7furcht", "trieb", "mich", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PPER", "$.", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Du lie\u00dft mir ja zur ", "tokens": ["Du", "lie\u00dft", "mir", "ja", "zur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPRART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.43": {"text": "Drum such ich, ", "tokens": ["Drum", "such", "ich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.44": {"text": "In Unterth\u00e4nigkeit reich ich ", "tokens": ["In", "Un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", "reich", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Mein Herzog! schenke ihm nur einen holden Blick,", "tokens": ["Mein", "Her\u00b7zog", "!", "schen\u00b7ke", "ihm", "nur", "ei\u00b7nen", "hol\u00b7den", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Nimm es so liebreich an; wie allezeit geschehen,", "tokens": ["Nimm", "es", "so", "lieb\u00b7reich", "an", ";", "wie", "al\u00b7le\u00b7zeit", "ge\u00