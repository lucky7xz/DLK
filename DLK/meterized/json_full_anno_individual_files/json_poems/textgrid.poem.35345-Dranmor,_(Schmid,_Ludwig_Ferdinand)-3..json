{"textgrid.poem.35345": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sterben \u2013 gestorben sein \u2013 und doch kein Ende?", "tokens": ["Ster\u00b7ben", "\u2013", "ge\u00b7stor\u00b7ben", "sein", "\u2013", "und", "doch", "kein", "En\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVPP", "VAINF", "$(", "KON", "ADV", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und doch des Denkens Leuchte nicht verglommen,", "tokens": ["Und", "doch", "des", "Den\u00b7kens", "Leuch\u00b7te", "nicht", "ver\u00b7glom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht jede K\u00fcmmernis von uns genommen", "tokens": ["Nicht", "je\u00b7de", "K\u00fcm\u00b7mer\u00b7nis", "von", "uns", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PIAT", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und jeder Zweifel, jede Augenblende?", "tokens": ["Und", "je\u00b7der", "Zwei\u00b7fel", ",", "je\u00b7de", "Au\u00b7gen\u00b7blen\u00b7de", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Tod ist mehr als eine Sonnenwende \u2013", "tokens": ["Der", "Tod", "ist", "mehr", "als", "ei\u00b7ne", "Son\u00b7nen\u00b7wen\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie selten hei\u00dft das Alter ihn willkommen!", "tokens": ["Wie", "sel\u00b7ten", "hei\u00dft", "das", "Al\u00b7ter", "ihn", "will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und selbst der Jugend kann die Lehre frommen:", "tokens": ["Und", "selbst", "der", "Ju\u00b7gend", "kann", "die", "Leh\u00b7re", "from\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VMFIN", "ART", "NN", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Tod ist unsres Lebens beste Spende!", "tokens": ["Der", "Tod", "ist", "uns\u00b7res", "Le\u00b7bens", "bes\u00b7te", "Spen\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn ihn erhellt kein Tag und keine Zeit.", "tokens": ["Denn", "ihn", "er\u00b7hellt", "kein", "Tag", "und", "kei\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PIAT", "NN", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Feuerst\u00e4tten und im feuchten Grabe", "tokens": ["Auf", "Feu\u00b7er\u00b7st\u00e4t\u00b7ten", "und", "im", "feuch\u00b7ten", "Gra\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von tiefster Nacht umschattet trotzt gefeit", "tokens": ["Von", "tiefs\u00b7ter", "Nacht", "um\u00b7schat\u00b7tet", "trotzt", "ge\u00b7feit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "VVFIN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er der Beschw\u00f6rer morschem Zauberstabe", "tokens": ["Er", "der", "Be\u00b7schw\u00f6\u00b7rer", "mor\u00b7schem", "Zau\u00b7ber\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und schenkt uns seine sch\u00f6nste Liebesgabe:", "tokens": ["Und", "schenkt", "uns", "sei\u00b7ne", "sch\u00f6ns\u00b7te", "Lie\u00b7bes\u00b7ga\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ruhe von Ewigkeit zu Ewigkeit.", "tokens": ["Ru\u00b7he", "von", "E\u00b7wig\u00b7keit", "zu", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.5": {"line.1": {"text": "Sterben \u2013 gestorben sein \u2013 und doch kein Ende?", "tokens": ["Ster\u00b7ben", "\u2013", "ge\u00b7stor\u00b7ben", "sein", "\u2013", "und", "doch", "kein", "En\u00b7de", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVPP", "VAINF", "$(", "KON", "ADV", "PIAT", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Und doch des Denkens Leuchte nicht verglommen,", "tokens": ["Und", "doch", "des", "Den\u00b7kens", "Leuch\u00b7te", "nicht"