{"dta.poem.2498": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Keyser Karl der f\u00fcnffte zu einem Spa-  \n nier:  Democrit. Rid. pag. 8.  \n  PAR PARI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "Jhr Spanier plagt mich wol mit eurem Uberlauffen/", "tokens": ["Ihr", "Spa\u00b7nier", "plagt", "mich", "wol", "mit", "eu\u00b7rem", "U\u00b7berl\u00b7auf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und saget mir sehr viel/ da\u00df ich das grosse Sauffen", "tokens": ["Und", "sa\u00b7get", "mir", "sehr", "viel", "/", "da\u00df", "ich", "das", "gros\u00b7se", "Sauf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$(", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Teutschen d\u00e4mpffen soll/ ja ja es sol geschehn", "tokens": ["Der", "Teut\u00b7schen", "d\u00e4mpf\u00b7fen", "soll", "/", "ja", "ja", "es", "sol", "ge\u00b7schehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$(", "ADV", "ADV", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr werdet aber drauff gleich den Gehorsam sehn", "tokens": ["Ihr", "wer\u00b7det", "a\u00b7ber", "drauff", "gleich", "den", "Ge\u00b7hor\u00b7sam", "sehn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "ADV", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Den ihr selbst abgetegt. Jhr seyd mir solche F\u00fcchse/", "tokens": ["Den", "ihr", "selbst", "ab\u00b7ge\u00b7tegt", ".", "Ihr", "seyd", "mir", "sol\u00b7che", "F\u00fcch\u00b7se", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVPP", "$.", "PPER", "VAFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Maulwurff bey euch selbst bey andern aber L\u00fcchse.", "tokens": ["Ein", "Maul\u00b7wurff", "bey", "euch", "selbst", "bey", "an\u00b7dern", "a\u00b7ber", "L\u00fcch\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "APPR", "PIS", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Stellt erst die Hurerey und euren Diebstal ein/", "tokens": ["Stellt", "erst", "die", "Hu\u00b7re\u00b7rey", "und", "eu\u00b7ren", "Diebs\u00b7tal", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "KON", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So soll der Teutschen Soff auch bald ged\u00e4mpffet seyn.", "tokens": ["So", "soll", "der", "Teut\u00b7schen", "Soff", "auch", "bald", "ge\u00b7d\u00e4mpf\u00b7fet", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "ADV", "ADV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}