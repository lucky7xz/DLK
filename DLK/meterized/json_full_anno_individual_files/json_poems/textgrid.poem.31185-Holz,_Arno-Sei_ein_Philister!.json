{"textgrid.poem.31185": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Sei ein Philister!", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei ein Philister, der sich stillvergn\u00fcgt", "tokens": ["Sei", "ein", "Phi\u00b7lis\u00b7ter", ",", "der", "sich", "still\u00b7ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Marseillaise auf den Bierbauch trommelt,", "tokens": ["Die", "Mar\u00b7seil\u00b7laise", "auf", "den", "Bier\u00b7bauch", "trom\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch beiss dir deine Finger ab, mein Junge,", "tokens": ["Doch", "beiss", "dir", "dei\u00b7ne", "Fin\u00b7ger", "ab", ",", "mein", "Jun\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn du Talent zu einem Herrgott hast!", "tokens": ["Wenn", "du", "Ta\u00b7lent", "zu", "ei\u00b7nem", "Herr\u00b7gott", "hast", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Auch sieh dich vor, dass du um Mitternacht,", "tokens": ["Auch", "sieh", "dich", "vor", ",", "dass", "du", "um", "Mit\u00b7ter\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PTKVZ", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wenn dir der Vollmond schneeweis ins Gesicht scheint,", "tokens": ["Wenn", "dir", "der", "Voll\u00b7mond", "schnee\u00b7weis", "ins", "Ge\u00b7sicht", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-++-+-+-+", "measure": "unknown.measure.hexa"}, "line.7": {"text": "Nicht einmal unversehns pathetisch wirst,", "tokens": ["Nicht", "ein\u00b7mal", "un\u00b7ver\u00b7sehns", "pa\u00b7the\u00b7tisch", "wirst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADJD", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mit dem Revolver vor den Spiegel tappst,", "tokens": ["Mit", "dem", "Re\u00b7vol\u00b7ver", "vor", "den", "Spie\u00b7gel", "tappst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-++-+-+-+", "measure": "zehnsilber"}, "line.9": {"text": "Ihn deinem Doppelg\u00e4nger vor die Brust setzt", "tokens": ["Ihn", "dei\u00b7nem", "Dop\u00b7pel\u00b7g\u00e4n\u00b7ger", "vor", "die", "Brust", "setzt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und theatralisch \u00e0 la Hamlet fragst,", "tokens": ["Und", "the\u00b7at\u00b7ra\u00b7lisch", "\u00e0", "la", "Ham\u00b7let", "fragst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "FM", "FM", "NN", "VVFIN", "$,"], "meter": "---+-+-+-+", "measure": "zehnsilber"}, "line.11": {