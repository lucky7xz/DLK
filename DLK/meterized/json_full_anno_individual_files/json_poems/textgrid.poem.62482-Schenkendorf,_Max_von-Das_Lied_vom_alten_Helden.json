{"textgrid.poem.62482": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "Das Lied vom alten Helden", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt ein Held begraben", "tokens": ["Es", "liegt", "ein", "Held", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief in des Berges Nacht,", "tokens": ["Tief", "in", "des", "Ber\u00b7ges", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wol viele Jahre haben", "tokens": ["Wol", "vie\u00b7le", "Jah\u00b7re", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schon \u00fcber ihm gewacht.", "tokens": ["Schon", "\u00fc\u00b7ber", "ihm", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Allein er schl\u00e4ft nur fester,", "tokens": ["Al\u00b7lein", "er", "schl\u00e4ft", "nur", "fes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und auch das Tr\u00e4umen l\u00e4\u00dft er.", "tokens": ["Und", "auch", "das", "Tr\u00e4u\u00b7men", "l\u00e4\u00dft", "er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Er war ein ries'ger Streiter", "tokens": ["Er", "war", "ein", "ries'\u00b7ger", "Strei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Einst in dem deutschen Land;", "tokens": ["Einst", "in", "dem", "deut\u00b7schen", "Land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das heil'ge Reich befreit' er,", "tokens": ["Das", "heil'\u00b7ge", "Reich", "be\u00b7freit'", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Warf Ritter in den Sand,", "tokens": ["Warf", "Rit\u00b7ter", "in", "den", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "War stets in Kampfes Mitte,", "tokens": ["War", "stets", "in", "Kamp\u00b7fes", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und doch voll frommer Sitte.", "tokens": ["Und", "doch", "voll", "from\u00b7mer", "Sit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Auf dir lag Gottes Segen,", "tokens": ["Auf", "dir", "lag", "Got\u00b7tes", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du deutsches Vaterland,", "tokens": ["Du", "deut\u00b7sches", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So lang den m\u00e4cht'gen Degen", "tokens": ["So", "lang", "den", "m\u00e4cht'\u00b7gen", "De\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er hielt in starker Hand.", "tokens": ["Er", "hielt", "in", "star\u00b7ker", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O, da\u00df nicht ew'ges Leben", "tokens": ["O", ",", "da\u00df", "nicht", "ew'\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "KOUS", "PTKNEG", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Dem Helden war gegeben! \u2013", "tokens": ["Dem", "Hel\u00b7den", "war", "ge\u00b7ge\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Viel edle K\u00e4mpfer starben", "tokens": ["Viel", "ed\u00b7le", "K\u00e4mp\u00b7fer", "star\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wol in dem blut'gen Strau\u00df,", "tokens": ["Wol", "in", "dem", "blut'\u00b7gen", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NE", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Da stieg, bedeckt mit Narben,", "tokens": ["Da", "stieg", ",", "be\u00b7deckt", "mit", "Nar\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er auf sein Felsenhaus,", "tokens": ["Er", "auf", "sein", "Fel\u00b7sen\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und von dem V\u00e4tersaale", "tokens": ["Und", "von", "dem", "V\u00e4\u00b7ter\u00b7saa\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hinab zum Todtenmahle.", "tokens": ["Hin\u00b7ab", "zum", "Tod\u00b7ten\u00b7mah\u00b7le", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wol viele Jahre schlief er,", "tokens": ["Wol", "vie\u00b7le", "Jah\u00b7re", "schlief", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVFIN", "PPER", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Versank im Berge ganz,", "tokens": ["Ver\u00b7sank", "im", "Ber\u00b7ge", "ganz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit ihm immer tiefer", "tokens": ["Und", "mit", "ihm", "im\u00b7mer", "tie\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Versank der deutsche Glanz;", "tokens": ["Ver\u00b7sank", "der", "deut\u00b7sche", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auch wie die Helden bieder", "tokens": ["Auch", "wie", "die", "Hel\u00b7den", "bie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NE"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Erstarben ihre Lieder. \u2013", "tokens": ["Er\u00b7star\u00b7ben", "ih\u00b7re", "Lie\u00b7der", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Seit nicht des Helden Lanze", "tokens": ["Seit", "nicht", "des", "Hel\u00b7den", "Lan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PTKNEG", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im deutschen Kampf mehr sauft,", "tokens": ["Im", "deut\u00b7schen", "Kampf", "mehr", "sauft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der feile Welsch' und Franze", "tokens": ["Der", "fei\u00b7le", "Wel\u00b7sch'", "und", "Fran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Im heil'gen Reiche haust.", "tokens": ["Im", "heil'\u00b7gen", "Rei\u00b7che", "haust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der freie Stamm in Ketten,", "tokens": ["Der", "frei\u00b7e", "Stamm", "in", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und Niemand ihn zu retten? \u2013", "tokens": ["Und", "Nie\u00b7mand", "ihn", "zu", "ret\u00b7ten", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "PPER", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Es bricht ein Morgen helle", "tokens": ["Es", "bricht", "ein", "Mor\u00b7gen", "hel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit in das Land hinein,", "tokens": ["Weit", "in", "das", "Land", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Selbst bis zur Grabesschwelle", "tokens": ["Selbst", "bis", "zur", "Gra\u00b7bes\u00b7schwel\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Helden dringt sein Schein.", "tokens": ["Des", "Hel\u00b7den", "dringt", "sein", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Beim Klang der alten Lieder", "tokens": ["Beim", "Klang", "der", "al\u00b7ten", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Regt er die starken Glieder;", "tokens": ["Regt", "er", "die", "star\u00b7ken", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Greift zu der r\u00fcst'gen Wehre,", "tokens": ["Greift", "zu", "der", "r\u00fcst'\u00b7gen", "Weh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Erhebt sich aus der Gruft,", "tokens": ["Er\u00b7hebt", "sich", "aus", "der", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wol zu der V\u00e4ter Ehre,", "tokens": ["Wol", "zu", "der", "V\u00e4\u00b7ter", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Empor zum Himmelsduft;", "tokens": ["Em\u00b7por", "zum", "Him\u00b7mels\u00b7duft", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Auf hohem Thurm die Lanze", "tokens": ["Auf", "ho\u00b7hem", "Thurm", "die", "Lan\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Schwingt er im Morgenglanze.", "tokens": ["Schwingt", "er", "im", "Mor\u00b7gen\u00b7glan\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Und schnell auf allen H\u00f6hen,", "tokens": ["Und", "schnell", "auf", "al\u00b7len", "H\u00f6\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ger\u00fcstet ganz in Stahl,", "tokens": ["Ge\u00b7r\u00fcs\u00b7tet", "ganz", "in", "Stahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sieht man die Ritter stehen,", "tokens": ["Sieht", "man", "die", "Rit\u00b7ter", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es jauchzt das ganze Thal;", "tokens": ["Es", "jauchzt", "das", "gan\u00b7ze", "Thal", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und weit in deutschen Landen", "tokens": ["Und", "weit", "in", "deut\u00b7schen", "Lan\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist neu die Kraft erstanden.", "tokens": ["Ist", "neu", "die", "Kraft", "er\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Vom hohen Felsensitze", "tokens": ["Vom", "ho\u00b7hen", "Fel\u00b7sen\u00b7sit\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "St\u00fcrzt in verj\u00fcngter Pracht", "tokens": ["St\u00fcrzt", "in", "ver\u00b7j\u00fcng\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wol an des Volkes Spitze", "tokens": ["Wol", "an", "des", "Vol\u00b7kes", "Spit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Held sich in die Schlacht.", "tokens": ["Der", "Held", "sich", "in", "die", "Schlacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Um deutsche Schmach zu r\u00e4chen,", "tokens": ["Um", "deut\u00b7sche", "Schmach", "zu", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Str\u00f6mt Frankenblut in B\u00e4chen.", "tokens": ["Str\u00f6mt", "Fran\u00b7ken\u00b7blut", "in", "B\u00e4\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Es liegt ein Held begraben", "tokens": ["Es", "liegt", "ein", "Held", "be\u00b7gra\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tief in des Berges Nacht,", "tokens": ["Tief", "in", "des", "Ber\u00b7ges", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wol viele Jahre haben", "tokens": ["Wol", "vie\u00b7le", "Jah\u00b7re", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schon \u00fcber ihm gewacht.", "tokens": ["Schon", "\u00fc\u00b7ber", "ihm", "ge\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPER", "VVPP", "