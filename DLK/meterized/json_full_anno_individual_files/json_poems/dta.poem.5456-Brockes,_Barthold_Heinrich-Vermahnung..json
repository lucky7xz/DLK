{"dta.poem.5456": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vermahnung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df die Erde noch so sch\u00f6n, und voll bunter Blumen,\ngr\u00fcnen;", "tokens": ["La\u00df", "die", "Er\u00b7de", "noch", "so", "sch\u00f6n", ",", "und", "voll", "bun\u00b7ter", "Blu\u00b7men", ",", "gr\u00fc\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "ADV", "ADJD", "$,", "KON", "ADJD", "ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "La\u00df das Wasser, noch so klar, ihrer Sch\u00f6nheit Spiegel seyn;", "tokens": ["La\u00df", "das", "Was\u00b7ser", ",", "noch", "so", "klar", ",", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", "Spie\u00b7gel", "seyn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "ADV", "ADV", "ADJD", "$,", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "La\u00df noch einst so herrlich gl\u00e4ntzen selbst der Sonnen Wun-", "tokens": ["La\u00df", "noch", "einst", "so", "herr\u00b7lich", "gl\u00e4nt\u00b7zen", "selbst", "der", "Son\u00b7nen", "Wun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "ADV", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Wozu solt\u2019 es einer GOttheit, die es ja nicht brauchet,", "tokens": ["Wo\u00b7zu", "solt'", "es", "ei\u00b7ner", "Got\u00b7theit", ",", "die", "es", "ja", "nicht", "brau\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Wenn nicht in den Welt- und Himmeln Kr\u00e4ft\u2019 und Gei-", "tokens": ["Wenn", "nicht", "in", "den", "Welt", "und", "Him\u00b7meln", "Kr\u00e4ft'", "und", "Gei"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "APPR", "ART", "TRUNC", "KON", "NN", "NN", "KON", "TRUNC"], "meter": "+---+-+-+-+", "measure": "dactylic.init"}, "line.6": {"text": "Die an solchen, durch die GOttheit wunderbar formirten,", "tokens": ["Die", "an", "sol\u00b7chen", ",", "durch", "die", "Got\u00b7theit", "wun\u00b7der\u00b7bar", "for\u00b7mir\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "$,", "APPR", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "F\u00e4hig w\u00e4ren, sich zu laben, sich zu n\u00e4hren,", "tokens": ["F\u00e4\u00b7hig", "w\u00e4\u00b7ren", ",", "sich", "zu", "la\u00b7ben", ",", "sich", "zu", "n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,", "PRF", "PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Zu vergn\u00fcgen, zu erqvicken, zu ergetzen,", "tokens": ["Zu", "ver\u00b7gn\u00fc\u00b7gen", ",", "zu", "er\u00b7qvi\u00b7cken", ",", "zu", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Zu bewundern, sie zu achten,", "tokens": ["Zu", "be\u00b7wun\u00b7dern", ",", "sie", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Sie zu nutzen, zu betrachten?", "tokens": ["Sie", "zu", "nut\u00b7zen", ",", "zu", "be\u00b7trach\u00b7ten", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Hiedurch litte (von der Ehre nichts zu sagen) GOttes", "tokens": ["Hie\u00b7durch", "lit\u00b7te", "(", "von", "der", "Eh\u00b7re", "nichts", "zu", "sa\u00b7gen", ")", "Got\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "$(", "APPR", "ART", "NN", "PIS", "PTKZU", "VVINF", "$(", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.12": {"text": "Welche ja sein wahres Wesen.", "tokens": ["Wel\u00b7che", "ja", "sein", "wah\u00b7res", "We\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Liebe, sonder Gegenwurf, kann ja keine Liebe seyn;", "tokens": ["Lie\u00b7be", ",", "son\u00b7der", "Ge\u00b7gen\u00b7wurf", ",", "kann", "ja", "kei\u00b7ne", "Lie\u00b7be", "seyn", ";"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KON", "NN", "$,", "VMFIN", "ADV", "PIAT", "NN", "VAINF", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.14": {"text": "Da uns GOtt nun blo\u00df allein,", "tokens": ["Da", "uns", "Gott", "nun", "blo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df wir seiner Vater-Triebe", "tokens": ["Da\u00df", "wir", "sei\u00b7ner", "Va\u00b7ter\u00b7Trie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Alle m\u00f6chten theilhaft seyn,", "tokens": ["Al\u00b7le", "m\u00f6ch\u00b7ten", "theil\u00b7haft", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "So gar wunderbar gemacht,", "tokens": ["So", "gar", "wun\u00b7der\u00b7bar", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Und f\u00fcr seiner Wercke Pracht,", "tokens": ["Und", "f\u00fcr", "sei\u00b7ner", "Wer\u00b7cke", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Nebst dem Geist sie zu erkennen,", "tokens": ["Nebst", "dem", "Geist", "sie", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Viele Sinnen wollen g\u00f6nnen;", "tokens": ["Vie\u00b7le", "Sin\u00b7nen", "wol\u00b7len", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Wollen wir denn unsre Pflicht", "tokens": ["Wol\u00b7len", "wir", "denn", "uns\u00b7re", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Str\u00e4flich aus den Augen setzen,", "tokens": ["Str\u00e4f\u00b7lich", "aus", "den", "Au\u00b7gen", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Und uns, recht mit Vorsatz, nicht", "tokens": ["Und", "uns", ",", "recht", "mit", "Vor\u00b7satz", ",", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "PPER", "$,", "ADV", "APPR", "NN", "$,", "PTKNEG"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.24": {"text": "An der Liebe Gr\u00f6\u00df\u2019 ergetzen,", "tokens": ["An", "der", "Lie\u00b7be", "Gr\u00f6\u00df'", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Die er, in den sch\u00f6nen Wercken,", "tokens": ["Die", "er", ",", "in", "den", "sch\u00f6\u00b7nen", "Wer\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Uns so v\u00e4terlich zu mercken,", "tokens": ["Uns", "so", "v\u00e4\u00b7ter\u00b7lich", "zu", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und, wie br\u00fcnstig er uns liebet,", "tokens": ["Und", ",", "wie", "br\u00fcns\u00b7tig", "er", "uns", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ADJD", "PPER", "PPER", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Ueberall zu f\u00fchlen giebet?", "tokens": ["Ue\u00b7be\u00b7rall", "zu", "f\u00fch\u00b7len", "gie\u00b7bet", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Will man lieber selber leiden,", "tokens": ["Will", "man", "lie\u00b7ber", "sel\u00b7ber", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als von GOTT geschenckte Freuden,", "tokens": ["Als", "von", "GoTT", "ge\u00b7schenck\u00b7te", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jm Geniessen, recht bedencken?", "tokens": ["Jm", "Ge\u00b7nies\u00b7sen", ",", "recht", "be\u00b7den\u00b7cken", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche Thorheit! sich zu kr\u00e4ncken,", "tokens": ["Wel\u00b7che", "Thor\u00b7heit", "!", "sich", "zu", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "$.", "PRF", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zugleich des Sch\u00f6pfers Ehr\u2019", "tokens": ["Und", "zu\u00b7gleich", "des", "Sch\u00f6p\u00b7fers", "Ehr'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu verringern! da vielmehr", "tokens": ["Zu", "ver\u00b7rin\u00b7gern", "!", "da", "viel\u00b7mehr"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$.", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wir an so viel tausend Sch\u00e4tzen", "tokens": ["Wir", "an", "so", "viel", "tau\u00b7send", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ADV", "ADV", "CARD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Uns, mit tausend Lust, ergetzen,", "tokens": ["Uns", ",", "mit", "tau\u00b7send", "Lust", ",", "er\u00b7get\u00b7zen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "APPR", "CARD", "NN", "$,", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und, selbst in der Lust, den Willen", "tokens": ["Und", ",", "selbst", "in", "der", "Lust", ",", "den", "Wil\u00b7len"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "$,", "ADV", "APPR", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Unsers Sch\u00f6pfers hier erf\u00fcllen,", "tokens": ["Un\u00b7sers", "Sch\u00f6p\u00b7fers", "hier", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Gegen ihn in Lieb\u2019 entbrennen,", "tokens": ["Ge\u00b7gen", "ihn", "in", "Lieb'", "ent\u00b7bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Jhm gef\u00e4llig leben k\u00f6nnen.", "tokens": ["Jhm", "ge\u00b7f\u00e4l\u00b7lig", "le\u00b7ben", "k\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}