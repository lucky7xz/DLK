{"textgrid.poem.36923": {"metadata": {"author": {"name": "Gla\u00dfbrenner, Adolf", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es zog ein J\u00fcngling Sangesmuth", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zog ein J\u00fcngling Sangesmuth", "tokens": ["Es", "zog", "ein", "J\u00fcng\u00b7ling", "San\u00b7ges\u00b7muth"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl \u00fcber Berg und Thal,", "tokens": ["Wohl", "\u00fc\u00b7ber", "Berg", "und", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Herz war stolz, sein Sinnen gut,", "tokens": ["Sein", "Herz", "war", "stolz", ",", "sein", "Sin\u00b7nen", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er liebte die Menschen zumal.", "tokens": ["Er", "lieb\u00b7te", "die", "Men\u00b7schen", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Eichen, Rosen, Veilchen,", "tokens": ["Ei\u00b7chen", ",", "Ro\u00b7sen", ",", "Veil\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Warte noch ein Weilchen.", "tokens": ["War\u00b7te", "noch", "ein", "Weil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Da kam er vor ein K\u00f6nigsschlo\u00df,", "tokens": ["Da", "kam", "er", "vor", "ein", "K\u00f6\u00b7nigs\u00b7schlo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Laute in der Hand,", "tokens": ["Die", "Lau\u00b7te", "in", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wo manche stille Thr\u00e4ne flo\u00df", "tokens": ["Wo", "man\u00b7che", "stil\u00b7le", "Thr\u00e4\u00b7ne", "flo\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herum im weiten Land.", "tokens": ["He\u00b7rum", "im", "wei\u00b7ten", "Land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eichen, Rosen, Veilchen,", "tokens": ["Ei\u00b7chen", ",", "Ro\u00b7sen", ",", "Veil\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Warte noch ein Weilchen.", "tokens": ["War\u00b7te", "noch", "ein", "Weil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Der K\u00f6nig hat ihn kaum erschaut,", "tokens": ["Der", "K\u00f6\u00b7nig", "hat", "ihn", "kaum", "er\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Rief er ihn vor den Thron:", "tokens": ["Rief", "er", "ihn", "vor", "den", "Thron", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Nun singe mir von deiner Braut,", "tokens": ["Nun", "sin\u00b7ge", "mir", "von", "dei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von der Liebe s\u00fc\u00dfem Lohn!", "tokens": ["Von", "der", "Lie\u00b7be", "s\u00fc\u00b7\u00dfem", "Lohn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eichen, Rosen, Veilchen,", "tokens": ["Ei\u00b7chen", ",", "Ro\u00b7sen", ",", "Veil\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Warte noch ein Weilchen.", "tokens": ["War\u00b7te", "noch", "ein", "Weil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Die Freiheit, das ist meine Braut,", "tokens": ["Die", "Frei\u00b7heit", ",", "das", "ist", "mei\u00b7ne", "Braut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herr K\u00f6nig auf dem Thron!", "tokens": ["Herr", "K\u00f6\u00b7nig", "auf", "dem", "Thron", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Menschen lieb' ich, Menschengl\u00fcck", "tokens": ["Die", "Men\u00b7schen", "lieb'", "ich", ",", "Men\u00b7schen\u00b7gl\u00fcck"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist meiner Liebe Lohn.", "tokens": ["Ist", "mei\u00b7ner", "Lie\u00b7be", "Lohn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eichen, Rosen, Veilchen.", "tokens": ["Ei\u00b7chen", ",", "Ro\u00b7sen", ",", "Veil\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Warte noch ein Weilchen.", "tokens": ["War\u00b7te", "noch", "ein", "Weil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Der K\u00f6nig warf ihn in den Thurm:", "tokens": ["Der", "K\u00f6\u00b7nig", "warf", "ihn", "in", "den", "Thurm", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da singe Freiheitssang!", "tokens": ["Da", "sin\u00b7ge", "Frei\u00b7heits\u00b7sang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da sei dir H\u00f6rer Kr\u00f6t' und Wurm", "tokens": ["Da", "sei", "dir", "H\u00f6\u00b7rer", "Kr\u00f6t'", "und", "Wurm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dein ganzes Leben lang!", "tokens": ["Dein", "gan\u00b7zes", "Le\u00b7ben", "lang", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Eichen, Rosen, Veilchen,", "tokens": ["Ei\u00b7chen", ",", "Ro\u00b7sen", ",", "Veil\u00b7chen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Warte noch ein Weilchen.", "tokens": ["War\u00b7te", "noch", "ein", "Weil\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Wie ist das Land umher so gr\u00fcn,", "tokens": ["Wie", "ist", "das", "Land", "um\u00b7her", "so", "gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "PTKVZ", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie rauscht der Eichenwald!", "tokens": ["Wie", "rauscht", "der", "Ei\u00b7chen\u00b7wald", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie duftig hier die Blumen bl\u00fchn,", "tokens": ["Wie", "duf\u00b7tig", "hier", "die", "Blu\u00b7men", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "l