{"dta.poem.13533": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Ankl\u00e4nge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bl\u00fcthen licht' nun Bl\u00fcthen dr\u00e4ngen,", "tokens": ["Bl\u00fc\u00b7then", "licht'", "nun", "Bl\u00fc\u00b7then", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er m\u00f6cht' vor Glanz erblinden;", "tokens": ["Da\u00df", "er", "m\u00f6cht'", "vor", "Glanz", "er\u00b7blin\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den dunklen Zauberg\u00e4ngen,", "tokens": ["In", "den", "dunk\u00b7len", "Zau\u00b7ber\u00b7g\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von den eigenen Ges\u00e4ngen", "tokens": ["Von", "den", "ei\u00b7ge\u00b7nen", "Ge\u00b7s\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Hold gelockt, kann er nicht finden", "tokens": ["Hold", "ge\u00b7lockt", ",", "kann", "er", "nicht", "fin\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus dem Labyrinth der Brust.", "tokens": ["Aus", "dem", "La\u00b7by\u00b7rinth", "der", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles, alles will's verk\u00fcnden", "tokens": ["Al\u00b7les", ",", "al\u00b7les", "will's", "ver\u00b7k\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PIS", "$,", "PIS", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "In den Wogen s\u00fc\u00dfer Lust.", "tokens": ["In", "den", "Wo\u00b7gen", "s\u00fc\u00b7\u00dfer", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}