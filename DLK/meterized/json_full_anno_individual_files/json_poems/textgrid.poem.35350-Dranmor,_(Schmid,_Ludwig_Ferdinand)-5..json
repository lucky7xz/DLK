{"textgrid.poem.35350": {"metadata": {"author": {"name": "Dranmor, (Schmid, Ludwig Ferdinand)", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Am Kreuze blutend, frei von jeder Schuld,", "tokens": ["Am", "Kreu\u00b7ze", "blu\u00b7tend", ",", "frei", "von", "je\u00b7der", "Schuld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In tiefster Brust Vergebung und Erbarmen,", "tokens": ["In", "tiefs\u00b7ter", "Brust", "Ver\u00b7ge\u00b7bung", "und", "Er\u00b7bar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So schiedest du, mit offnen Bruderarmen,", "tokens": ["So", "schie\u00b7dest", "du", ",", "mit", "off\u00b7nen", "Bru\u00b7der\u00b7ar\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf ewig \u2013 in verhei\u00dfungsvoller Huld;", "tokens": ["Auf", "e\u00b7wig", "\u2013", "in", "ver\u00b7hei\u00b7\u00dfungs\u00b7vol\u00b7ler", "Huld", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "$(", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch dein Verm\u00e4chtnis, deines Opfers Lohn,", "tokens": ["Doch", "dein", "Ver\u00b7m\u00e4cht\u00b7nis", ",", "dei\u00b7nes", "Op\u00b7fers", "Lohn", ","], "token_info": ["word", "word", "word", "punct", "w