{"textgrid.poem.63415": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "Waterm\u00f6hm", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kind, grip nich nah de Bl\u00f6meken \u2013", "tokens": ["Kind", ",", "grip", "nich", "nah", "de", "Bl\u00f6\u00b7me\u00b7ken", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PTKNEG", "ADJD", "NE", "NN", "$("], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.2": {"text": "dor lurt dat Waterm\u00f6hmeken,", "tokens": ["dor", "lurt", "dat", "Wa\u00b7ter\u00b7m\u00f6h\u00b7me\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "wu sonn g\u00e4l M\u00fcmmelk fl\u00fctt.", "tokens": ["wu", "sonn", "g\u00e4l", "M\u00fcm\u00b7melk", "fl\u00fctt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Still is de See dor, still un deep,", "tokens": ["Still", "is", "de", "See", "dor", ",", "still", "un", "deep", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "FM", "FM", "NN", "NE", "$,", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "un mennigeen, de dornah grep,", "tokens": ["un", "men\u00b7ni\u00b7geen", ",", "de", "dor\u00b7nah", "grep", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM", "FM", "$,", "NE", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "kopp\u0153wer sch\u00f6t he un vers\u00f6p,", "tokens": ["kopp\u0153wer", "sch\u00f6t", "he", "un", "ver\u00b7s\u00f6p", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "FM", "FM", "FM", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "as M\u00fcs' inne Melkenb\u00fctt.", "tokens": ["as", "M\u00fcs'", "in\u00b7ne", "Mel\u00b7ken\u00b7b\u00fctt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "De M\u00fcmmelk stahn so wit af all,", "tokens": ["De", "M\u00fcm\u00b7melk", "stahn", "so", "wit", "af", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADV", "APPR", "NE", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat een se nich aflangen schall,", "tokens": ["dat", "e\u00b7en", "se", "nich", "af\u00b7lan\u00b7gen", "schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "ob een ok meent, he k\u00fcnn.", "tokens": ["ob", "e\u00b7en", "ok", "meent", ",", "he", "k\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "NE", "$,", "NE", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nimm nich sonn oll dumm St\u00fcck di v\u00f6r!", "tokens": ["Nimm", "nich", "sonn", "oll", "dumm", "St\u00fcck", "di", "v\u00f6r", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "ADJD", "ADJD", "NN", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "S\u00fch, grippst du all to drist nah ehr,", "tokens": ["S\u00fch", ",", "grippst", "du", "all", "to", "drist", "nah", "ehr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PIAT", "NN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "denn ward de Kopp up eens di swer,", "tokens": ["denn", "ward", "de", "Kopp", "up", "e\u00b7ens", "di", "swer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+----+-+", "measure": "dactylic.init"}, "line.7": {"text": "denn plumpst du batsch eens r\u00fcnn.", "tokens": ["denn", "plumpst", "du", "batsch", "e\u00b7ens", "r\u00fcnn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Denn f\u00f6tt de M\u00f6hm di an de Been", "tokens": ["Denn", "f\u00f6tt", "de", "M\u00f6hm", "di", "an", "de", "Be\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "FM", "APPR", "NE", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "un h\u00e4ngt sick an di as'n Steen", "tokens": ["un", "h\u00e4ngt", "sick", "an", "di", "as'n", "Steen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "VVFIN", "PRF", "APPR", "NE", "NE", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "un treckt di inne Gr\u00fcnd;", "tokens": ["un", "treckt", "di", "in\u00b7ne", "Gr\u00fcnd", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wu du ok ampelst, wat du h\u00fcppst", "tokens": ["wu", "du", "ok", "am\u00b7pelst", ",", "wat", "du", "h\u00fcppst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,", "PWS", "PPER", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "un \u00fcm di sleihst un st\u00f6ttst un grippst,", "tokens": ["un", "\u00fcm", "di", "sleihst", "un", "st\u00f6ttst", "un", "grippst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "fast h\u00f6llt se, fast \u2013 bet du vers\u00fcppst,", "tokens": ["fast", "h\u00f6llt", "se", ",", "fast", "\u2013", "bet", "du", "ver\u00b7s\u00fcppst", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "FM", "$,", "ADV", "$(", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "as v\u00f6r di v\u00e4l all s\u00fcnd.", "tokens": ["as", "v\u00f6r", "di", "v\u00e4l", "all", "s\u00fcnd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Keen Ungl\u00fcck sl\u00f6ppt; un wenn't gesch\u00fcht,", "tokens": ["Ke\u00b7en", "Un\u00b7gl\u00fcck", "sl\u00f6ppt", ";", "un", "wenn't", "ge\u00b7sch\u00fcht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVPP", "$.", "FM", "VVFIN", "VVPP", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "wen weet, wat denn grad wen dat s\u00fcht", "tokens": ["wen", "weet", ",", "wat", "denn", "grad", "wen", "dat", "s\u00fcht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "PRELS", "ADV", "ADV", "PWS", "ART", "NN"], "meter": "+------+", "measure": "dactylic.init"}, "line.3": {"text": "un l\u00f6ppt noch flink nog ran;", "tokens": ["un", "l\u00f6ppt", "noch", "flink", "nog", "ran", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Wen weet, wat, wenn di dat mall\u00fcrt,", "tokens": ["Wen", "weet", ",", "wat", ",", "wenn", "di", "dat", "mal\u00b7l\u00fcrt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "NE", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "\u00fcns' Herrgott wen v\u00f6rbi dor f\u00fchrt,", "tokens": ["\u00fcns'", "Herr\u00b7gott", "wen", "v\u00f6r\u00b7bi", "dor", "f\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "de din Geschricht un Krischen h\u00fcrt", "tokens": ["de", "din", "Ge\u00b7schricht", "un", "Kri\u00b7schen", "h\u00fcrt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "FM", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "un helpen will un kann.", "tokens": ["un", "hel\u00b7pen", "will", "un", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVINF", "VMFIN", "FM", "VMFIN", "$."], "meter": "+---+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Nahst steiht din Vader dor un st\u00e4hnt,", "tokens": ["Nahst", "steiht", "din", "Va\u00b7der", "dor", "un", "st\u00e4hnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "nahst sitt din Moder dor un weent", "tokens": ["nahst", "sitt", "din", "Mo\u00b7der", "dor", "un", "weent"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "ehr bl\u00f6dig, bl\u00f6dig Tran,", "tokens": ["ehr", "bl\u00f6\u00b7dig", ",", "bl\u00f6\u00b7dig", "Tran", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "wat se so wat afl\u00e4wen m\u00f6t,", "tokens": ["wat", "se", "so", "wat", "a\u00b7fl\u00e4\u00b7wen", "m\u00f6t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "wat du sonn gallenbitter Leed \u2013", "tokens": ["wat", "du", "sonn", "gal\u00b7len\u00b7bit\u00b7ter", "Leed", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "wu bitterb\u00f6s, \u00fcns' Herrgott weet't \u2013", "tokens": ["wu", "bit\u00b7ter\u00b7b\u00f6s", ",", "\u00fcns'", "Herr\u00b7gott", "weet't", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "$,", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "di hest un ehr andan.", "tokens": ["di", "hest", "un", "ehr", "an\u00b7dan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Du \u0153werst hest keen Rauh, keen Rast,", "tokens": ["Du", "\u0153werst", "hest", "ke\u00b7en", "Rauh", ",", "ke\u00b7en", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "un ob du teihnmal graben wast,", "tokens": ["un", "ob", "du", "teihn\u00b7mal", "gra\u00b7ben", "wast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "KOUS", "PPER", "ADV", "VVINF", "VAFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "di h\u00f6llt nich Sark nich Steen;", "tokens": ["di", "h\u00f6llt", "nich", "Sark", "nich", "Steen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "NN", "PTKNEG", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "di h\u00f6llt keen Kr\u00fcz, keen Kirchhoffm\u00fcr,", "tokens": ["di", "h\u00f6llt", "ke\u00b7en", "Kr\u00fcz", ",", "ke\u00b7en", "Kirch\u00b7hoff\u00b7m\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "un gr\u00e4sen makst du nachts de L\u00fcr", "tokens": ["un", "gr\u00e4\u00b7sen", "makst", "du", "nachts", "de", "L\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "PPER", "APPR", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "as sonn oll sp\u00f6kig Fl\u0153kerf\u00fcr,", "tokens": ["as", "sonn", "oll", "sp\u00f6\u00b7kig", "Fl\u0153ker\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos