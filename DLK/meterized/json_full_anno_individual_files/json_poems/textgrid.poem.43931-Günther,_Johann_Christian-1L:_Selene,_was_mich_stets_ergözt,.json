{"textgrid.poem.43931": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Selene, was mich stets erg\u00f6zt,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Selene, was mich stets erg\u00f6zt,", "tokens": ["Se\u00b7le\u00b7ne", ",", "was", "mich", "stets", "er\u00b7g\u00f6zt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die Freyheit, dir zu dienen,", "tokens": ["Das", "ist", "die", "Frey\u00b7heit", ",", "dir", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ich hier auch aufgesezt,", "tokens": ["Und", "was", "ich", "hier", "auch", "auf\u00b7ge\u00b7sezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entdeckt ein wohlgemeint Erk\u00fchnen.", "tokens": ["Ent\u00b7deckt", "ein", "wohl\u00b7ge\u00b7meint", "Er\u00b7k\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kranckheit, so dir jezo droht,", "tokens": ["Die", "Kran\u00b7ck\u00b7heit", ",", "so", "dir", "je\u00b7zo", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erschr\u00f6ckt mich \u00e4rger als du denckest;", "tokens": ["Er\u00b7schr\u00f6ckt", "mich", "\u00e4r\u00b7ger", "als", "du", "den\u00b7ckest", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "PPER", "VVFIN", "$."], "meter": "-+-+--+--", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Doch, wo du mir Erh\u00f6rung schenckest,", "tokens": ["Doch", ",", "wo", "du", "mir", "Er\u00b7h\u00f6\u00b7rung", "schen\u00b7ckest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PPER", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So hat der Ansto\u00df keine Noth.", "tokens": ["So", "hat", "der", "An\u00b7sto\u00df", "kei\u00b7ne", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich weis es zwar, dein hoher Geist", "tokens": ["Ich", "weis", "es", "zwar", ",", "dein", "ho\u00b7her", "Geist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermag sich allemahl zu fa\u00dfen,", "tokens": ["Ver\u00b7mag", "sich", "al\u00b7le\u00b7mahl", "zu", "fa\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und wie sein Wesen himmlisch heist,", "tokens": ["Und", "wie", "sein", "We\u00b7sen", "himm\u00b7lisch", "heist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPOSAT", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So kan er leicht die Welt verla\u00dfen;", "tokens": ["So", "kan", "er", "leicht", "die", "Welt", "ver\u00b7la\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJD", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Allein du kanst zu jeder Zeit", "tokens": ["Al\u00b7lein", "du", "kanst", "zu", "je\u00b7der", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch fr\u00fch genug zum Engel werden,", "tokens": ["Noch", "fr\u00fch", "ge\u00b7nug", "zum", "En\u00b7gel", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "APPRART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und also g\u00f6nne doch der Erden", "tokens": ["Und", "al\u00b7so", "g\u00f6n\u00b7ne", "doch", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Den Schmuck von deiner Seltenheit.", "tokens": ["Den", "Schmuck", "von", "dei\u00b7ner", "Sel\u00b7ten\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bedencke doch nur den Verlust,", "tokens": ["Be\u00b7den\u00b7cke", "doch", "nur", "den", "Ver\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Wofern ein fr\u00fch Verh\u00e4ngn\u00fc\u00df wollte,", "tokens": ["Wo\u00b7fern", "ein", "fr\u00fch", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df so viel Sch\u00f6nheit kluger Brust", "tokens": ["Da\u00df", "so", "viel", "Sch\u00f6n\u00b7heit", "klu\u00b7ger", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PIAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In besten Jahren sterben sollte!", "tokens": ["In", "bes\u00b7ten", "Jah\u00b7ren", "ster\u00b7ben", "soll\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Las die getrost zur Grube gehn,", "tokens": ["Las", "die", "ge\u00b7trost", "zur", "Gru\u00b7be", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Freude, Wiz und Muth verlieren;", "tokens": ["Die", "Freu\u00b7de", ",", "Wiz", "und", "Muth", "ver\u00b7lie\u00b7ren", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du solt hinfort von neuem sp\u00fcren,", "tokens": ["Du", "solt", "hin\u00b7fort", "von", "neu\u00b7em", "sp\u00fc\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wie artig frische Myrthen stehn.", "tokens": ["Wie", "ar\u00b7tig", "fri\u00b7sche", "Myr\u00b7then", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du bist ja sonst so sehr bem\u00fcht,", "tokens": ["Du", "bist", "ja", "sonst", "so", "sehr", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich nett und kostbar anzukleiden;", "tokens": ["Dich", "nett", "und", "kost\u00b7bar", "an\u00b7zu\u00b7klei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Denn wenn dich deines gleichen sieht,", "tokens": ["Denn", "wenn", "dich", "dei\u00b7nes", "glei\u00b7chen", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So h\u00f6rt und sieht man dich beneiden;", "tokens": ["So", "h\u00f6rt", "und", "sieht", "man", "dich", "be\u00b7nei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PIS", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch bey aller dieser Tracht,", "tokens": ["Je\u00b7doch", "bey", "al\u00b7ler", "die\u00b7ser", "Tracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du magst sie noch so sch\u00f6n ergr\u00fcnden,", "tokens": ["Du", "magst", "sie", "noch", "so", "sch\u00f6n", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ist doch kein be\u00dfer Kleid zu finden", "tokens": ["Ist", "doch", "kein", "be\u00b7\u00dfer", "Kleid", "zu", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PIAT", "ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Als was dir die Natur gemacht.", "tokens": ["Als", "was", "dir", "die", "Na\u00b7tur", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Trag Sorge vor den sch\u00f6nen Leib", "tokens": ["Trag", "Sor\u00b7ge", "vor", "den", "sch\u00f6\u00b7nen", "Leib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Arbeit, Speisen, Luft und Wachen", "tokens": ["In", "Ar\u00b7beit", ",", "Spei\u00b7sen", ",", "Luft", "und", "Wa\u00b7chen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nimm bequemen Zeitvertreib,", "tokens": ["Und", "nimm", "be\u00b7que\u00b7men", "Zeit\u00b7ver\u00b7treib", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn weder faul noch schwach zu machen.", "tokens": ["Ihn", "we\u00b7der", "faul", "noch", "schwach", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KON", "ADJD", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Fleuch Salz und E\u00dfig als das Gift,", "tokens": ["Fleuch", "Salz", "und", "E\u00b7\u00dfig", "als", "das", "Gift", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "ADJD", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bezwinge Zorn, Verdru\u00df und Schr\u00f6cken", "tokens": ["Be\u00b7zwin\u00b7ge", "Zorn", ",", "Ver\u00b7dru\u00df", "und", "Schr\u00f6\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und las dich niemahls eh erwecken,", "tokens": ["Und", "las", "dich", "nie\u00b7mahls", "eh", "er\u00b7we\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "KOUS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als bis dein Ohr die Stunde trift.", "tokens": ["Als", "bis", "dein", "Ohr", "die", "Stun\u00b7de", "trift", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Verzeih, Selene, meiner Hand,", "tokens": ["Ver\u00b7zeih", ",", "Se\u00b7le\u00b7ne", ",", "mei\u00b7ner", "Hand", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie schreibt nur kurze Grundgeseze,", "tokens": ["Sie", "schreibt", "nur", "kur\u00b7ze", "Grund\u00b7ge\u00b7se\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Damit kein gr\u00f6\u00dfer \u00dcbelstand", "tokens": ["Da\u00b7mit", "kein", "gr\u00f6\u00b7\u00dfer", "\u00dc\u00b7bel\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der sch\u00f6nen Glieder Bau verleze.", "tokens": ["Der", "sch\u00f6\u00b7nen", "Glie\u00b7der", "Bau", "ver\u00b7le\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sechs Wochen las dies Thee nicht ruhn;", "tokens": ["Sechs", "Wo\u00b7chen", "las", "dies", "Thee", "nicht", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PDS", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Wuntsch hat Kr\u00e4fte beygetragen,", "tokens": ["Mein", "Wunt\u00b7sch", "hat", "Kr\u00e4f\u00b7te", "bey\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ich weis, du wirst in kurzem sagen:", "tokens": ["Ich", "weis", ",", "du", "wirst", "in", "kur\u00b7zem", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "VAFIN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Kan Wa\u00dfer solche Dinge thun?", "tokens": ["Kan", "Wa\u00b7\u00dfer", "sol\u00b7che", "Din\u00b7ge", "thun", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Selene, was mich stets erg\u00f6zt,", "tokens": ["Se\u00b7le\u00b7ne", ",", "was", "mich", "stets", "er\u00b7g\u00f6zt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das ist die Freyheit, dir zu dienen,", "tokens": ["Das", "ist", "die", "Frey\u00b7heit", ",", "dir", "zu", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und was ich hier auch aufgesezt,", "tokens": ["Und", "was", "ich", "hier", "auch", "auf\u00b7ge\u00b7sezt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entdeckt ein wohlgemeint Erk\u00fchnen.", "tokens": ["Ent\u00b7deckt", "ein", "wohl\u00b7ge\u00b7meint", "Er\u00b7k\u00fch\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Kranckheit, so dir jezo droht,", "tokens": ["Die", "Kran\u00b7ck\u00b7heit", ",", "so", "dir", "je\u00b7zo", "droht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Erschr\u00f6ckt mich \u00e4rger als du denckest;", "tokens": ["Er\u00b7schr\u00f6ckt", "mich", "\u00e4r\u00b7ger",