{"dta.poem.21459": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Fragment.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "O da\u00df ich f\u00e4nde eine Seele,", "tokens": ["O", "da\u00df", "ich", "f\u00e4n\u00b7de", "ei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die f\u00fchlte gleich mir \u2026", "tokens": ["Die", "f\u00fchl\u00b7te", "gleich", "mir", "\u2026"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PPER", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "O da\u00df mir endlich", "tokens": ["O", "da\u00df", "mir", "end\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "ADV"], "meter": "++-+-", "measure": "iambic.di"}, "line.4": {"text": "In keuscher Sch\u00f6nheit", "tokens": ["In", "keu\u00b7scher", "Sch\u00f6n\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Thaufrisch erbl\u00fchte", "tokens": ["Thauf\u00b7risch", "er\u00b7bl\u00fch\u00b7te"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Das Wunder der Liebe!", "tokens": ["Das", "Wun\u00b7der", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "O da\u00df endlich dem Verschmachtenden w\u00fcrde", "tokens": ["O", "da\u00df", "end\u00b7lich", "dem", "Ver\u00b7schmach\u00b7ten\u00b7den", "w\u00fcr\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "ART", "NN", "VAFIN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Das einzige Gl\u00fcck, das die Erde kennt,", "tokens": ["Das", "ein\u00b7zi\u00b7ge", "Gl\u00fcck", ",", "das", "die", "Er\u00b7de", "kennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "In dem alle Seligkeit wurzelt:", "tokens": ["In", "dem", "al\u00b7le", "Se\u00b7lig\u00b7keit", "wur\u00b7zelt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Der s\u00fc\u00dfe Einklang", "tokens": ["Der", "s\u00fc\u00b7\u00dfe", "Ein\u00b7klang"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Zweier Menschenherzen", "tokens": ["Zwei\u00b7er", "Men\u00b7schen\u00b7her\u00b7zen"], "token_info": ["word", "word"], "pos": ["CARD", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Zur ewigen Harmonie \u2026", "tokens": ["Zur", "e\u00b7wi\u00b7gen", "Har\u00b7mo\u00b7nie", "\u2026"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "\u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014", "tokens": [], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.14": {"text": "Niederthau\u2019st du, o Friede,", "tokens": ["Nie\u00b7dert\u00b7hau'st", "du", ",", "o", "Frie\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Der in den Himmeln flutet,", "tokens": ["Der", "in", "den", "Him\u00b7meln", "flu\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Du Demant der Erkenntni\u00df,", "tokens": ["Du", "De\u00b7mant", "der", "Er\u00b7kennt\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.17": {"text": "Darin sich spiegelt", "tokens": ["Da\u00b7rin", "sich", "spie\u00b7gelt"], "token_info": ["word", "word", "word"], "pos": ["PAV", "PRF", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Alles Gute und B\u00f6se.", "tokens": ["Al\u00b7les", "Gu\u00b7te", "und", "B\u00f6\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.19": {"text": "Und wundervoll sprichst du,", "tokens": ["Und", "wun\u00b7der\u00b7voll", "sprichst", "du", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Ewiger Wechsel,", "tokens": ["E\u00b7wi\u00b7ger", "Wech\u00b7sel", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.21": {"text": "Zu den Traumvergessenen.", "tokens": ["Zu", "den", "Traum\u00b7ver\u00b7ges\u00b7se\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.22": {"text": "O k\u00f6stliches Weben", "tokens": ["O", "k\u00f6st\u00b7li\u00b7ches", "We\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.23": {"text": "Im Tempel der Gottheit!", "tokens": ["Im", "Tem\u00b7pel", "der", "Got\u00b7theit", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.24": {"text": "O trunkenes Schwelgen", "tokens": ["O", "trun\u00b7ke\u00b7nes", "Schwel\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.25": {"text": "In Wonn\u2019 ohne Ende!", "tokens": ["In", "Wonn'", "oh\u00b7ne", "En\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Es wandeln die Monde \u2014", "tokens": ["Es", "wan\u00b7deln", "die", "Mon\u00b7de"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NE", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.27": {"text": "Es bleibt der Seelenumarmung", "tokens": ["Es", "bleibt", "der", "See\u00b7le\u00b7num\u00b7ar\u00b7mung"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Unaussprechliche Wollust.", "tokens": ["Un\u00b7aus\u00b7sprech\u00b7li\u00b7che", "Wol\u00b7lust", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}