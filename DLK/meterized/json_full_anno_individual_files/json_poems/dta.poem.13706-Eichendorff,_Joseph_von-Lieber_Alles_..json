{"dta.poem.13706": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Lieber Alles .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Soldat sein ist gef\u00e4hrlich,", "tokens": ["Sol\u00b7dat", "sein", "ist", "ge\u00b7f\u00e4hr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Studiren sehr beschwerlich,", "tokens": ["Stu\u00b7di\u00b7ren", "sehr", "be\u00b7schwer\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Dichten s\u00fc\u00df und zierlich,", "tokens": ["Das", "Dich\u00b7ten", "s\u00fc\u00df", "und", "zier\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Dichter gar possierlich", "tokens": ["Der", "Dich\u00b7ter", "gar", "pos\u00b7sier\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In diesen wilden Zeiten.", "tokens": ["In", "die\u00b7sen", "wil\u00b7den", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich m\u00f6cht' am liebsten reiten,", "tokens": ["Ich", "m\u00f6cht'", "am", "liebs\u00b7ten", "rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein gutes Schwert zur Seiten,", "tokens": ["Ein", "gu\u00b7tes", "Schwert", "zur", "Sei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die Laute in der Rechten,", "tokens": ["Die", "Lau\u00b7te", "in", "der", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Studentenherz zum Fechten.", "tokens": ["Stu\u00b7den\u00b7ten\u00b7herz", "zum", "Fech\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Ein wildes Ro\u00df ist's Leben,", "tokens": ["Ein", "wil\u00b7des", "Ro\u00df", "ist's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die Hufe Funken geben,", "tokens": ["Die", "Hu\u00b7fe", "Fun\u00b7ken", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Wer's ehrlich wagt, bezwingt es,", "tokens": ["Wer's", "ehr\u00b7lich", "wagt", ",", "be\u00b7zwingt", "es", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "VVFIN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und wo es tritt, da klingt es!", "tokens": ["Und", "wo", "es", "tritt", ",", "da", "klingt", "es", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "$,", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}