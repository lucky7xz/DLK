{"dta.poem.18301": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "Felix Dahn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Schon als ein J\u00fcngling mit lockigen Haaren;", "tokens": ["Schon", "als", "ein", "J\u00fcng\u00b7ling", "mit", "lo\u00b7cki\u00b7gen", "Haa\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Achtung, in seinem Sch\u00e4del rumort", "tokens": ["Ach\u00b7tung", ",", "in", "sei\u00b7nem", "Sch\u00e4\u00b7del", "ru\u00b7mort"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-++", "measure": "unknown.measure.penta"}, "line.3": {"text": "Ledern die Weisheit von tausend Jahren!", "tokens": ["Le\u00b7dern", "die", "Weis\u00b7heit", "von", "tau\u00b7send", "Jah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "CARD", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Aber, verbrach er auch manchen Quark,", "tokens": ["A\u00b7ber", ",", "ver\u00b7brach", "er", "auch", "man\u00b7chen", "Quark", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "PPER", "ADV", "PIAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Unser Volk wird ihn ewig lieben,", "tokens": ["Un\u00b7ser", "Volk", "wird", "ihn", "e\u00b7wig", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.3": {"text": "Hat er doch einst, die Knochen voll Mark,", "tokens": ["Hat", "er", "doch", "einst", ",", "die", "Kno\u00b7chen", "voll", "Mark", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "$,", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Herrlich den \u201eKampf um Rom\u201c beschrieben!", "tokens": ["Herr\u00b7lich", "den", "\u201e", "Kampf", "um", "Rom", "\u201c", "be\u00b7schrie\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "$(", "NN", "APPR", "NE", "$(", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}