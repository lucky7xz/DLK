{"textgrid.poem.49364": {"metadata": {"author": {"name": "Ball, Hugo", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1906, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oh du mein Hyazinth, die Wade knackte", "tokens": ["Oh", "du", "mein", "Hy\u00b7a\u00b7zinth", ",", "die", "Wa\u00b7de", "knack\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "PPER", "PPOSAT", "NN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Rolf, der Mops. fra\u00df j\u00e4h das Strumpfband auf.", "tokens": ["Und", "Rolf", ",", "der", "Mops", ".", "fra\u00df", "j\u00e4h", "das", "Strumpf\u00b7band", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "$,", "ART", "NN", "$.", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nach Grammophonen in dem Twosteptakte", "tokens": ["Nach", "Gram\u00b7mo\u00b7pho\u00b7nen", "in", "dem", "Twos\u00b7tep\u00b7tak\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vollzog sich Notdurft Coitus und Lebenslauf.", "tokens": ["Voll\u00b7zog", "sich", "Not\u00b7durft", "Coi\u00b7tus", "und", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "NE", "KON", "NN", "$."], "meter": "---+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Lampionen blutgeduns'nes Schwirren", "tokens": ["Der", "Lam\u00b7pi\u00b7o\u00b7nen", "blut\u00b7ge\u00b7duns'\u00b7nes", "Schwir\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schuf gro\u00dfe Monde aus den Wassergl\u00e4sern.", "tokens": ["Schuf", "gro\u00b7\u00dfe", "Mon\u00b7de", "aus", "den", "Was\u00b7ser\u00b7gl\u00e4\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ein Schlachtget\u00f6se gab es und ein Klirren", "tokens": ["Ein", "Schlacht\u00b7ge\u00b7t\u00f6\u00b7se", "gab", "es", "und", "ein", "Klir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Kneifer von Beamten und Verwesern.", "tokens": ["Der", "Knei\u00b7fer", "von", "Be\u00b7am\u00b7ten", "und", "Ver\u00b7we\u00b7sern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da war auch Dame Wueh in einer Prunkkarosse,", "tokens": ["Da", "war", "auch", "Da\u00b7me", "Wueh", "in", "ei\u00b7ner", "Prunk\u00b7ka\u00b7ros\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Uns schrak nicht Kino mehr, nicht die Picassofratze.", "tokens": ["Uns", "schrak", "nicht", "Ki\u00b7no", "mehr", ",", "nicht", "die", "Pi\u00b7cas\u00b7so\u00b7frat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "NN", "ADV", "$,", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alex