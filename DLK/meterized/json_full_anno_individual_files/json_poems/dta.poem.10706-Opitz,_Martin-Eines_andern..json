{"dta.poem.10706": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Eines andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Der Todt vollbracht hat seine Lust/", "tokens": ["Der", "Todt", "voll\u00b7bracht", "hat", "sei\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es hat den Geist hier vffgegeben/", "tokens": ["Es", "hat", "den", "Geist", "hier", "vff\u00b7ge\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der/ so darumb absterben must/", "tokens": ["Der", "/", "so", "da\u00b7rumb", "abs\u00b7ter\u00b7ben", "must", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PAV", "VVINF", "VMFIN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Da\u00df er nit lenger konte leben.", "tokens": ["Da\u00df", "er", "nit", "len\u00b7ger", "kon\u00b7te", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}