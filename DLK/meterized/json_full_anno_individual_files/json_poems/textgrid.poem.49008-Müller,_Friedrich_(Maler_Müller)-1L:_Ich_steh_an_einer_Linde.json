{"textgrid.poem.49008": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich steh an einer Linde", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich steh an einer Linde", "tokens": ["Ich", "steh", "an", "ei\u00b7ner", "Lin\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und lausche durch den Hain;", "tokens": ["Und", "lau\u00b7sche", "durch", "den", "Hain", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rufe dir, Belinde,", "tokens": ["Und", "ru\u00b7fe", "dir", ",", "Be\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Voll stiller Liebespein.", "tokens": ["Voll", "stil\u00b7ler", "Lie\u00b7be\u00b7spein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit purpurroten Wangen", "tokens": ["Mit", "pur\u00b7pur\u00b7ro\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Voll schmelzendem Verlangen", "tokens": ["Voll", "schmel\u00b7zen\u00b7dem", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "H\u00fcpft Zephyr durch den Hain", "tokens": ["H\u00fcpft", "Ze\u00b7phyr", "durch", "den", "Hain"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "NN", "APPR", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Und suchet seine Flora", "tokens": ["Und", "su\u00b7chet", "sei\u00b7ne", "Flo\u00b7ra"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Am Busen der Aurora,", "tokens": ["Am", "Bu\u00b7sen", "der", "Au\u00b7ro\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Geweckt von gleicher Pein.", "tokens": ["Ge\u00b7weckt", "von", "glei\u00b7cher", "Pein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Noch schlummernd unter Rosen,", "tokens": ["Noch", "schlum\u00b7mernd", "un\u00b7ter", "Ro\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die ihre Brust umgl\u00fchn", "tokens": ["Die", "ih\u00b7re", "Brust", "um\u00b7gl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spielend sie liebkosen,", "tokens": ["Und", "spie\u00b7lend", "sie", "lieb\u00b7ko\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieht ihn die G\u00f6ttin fliehn.", "tokens": ["Sieht", "ihn", "die", "G\u00f6t\u00b7tin", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Sie hascht ihn bei den Locken,", "tokens": ["Sie", "hascht", "ihn", "bei", "den", "Lo\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die seinen Nacken fliehn,", "tokens": ["Die", "sei\u00b7nen", "Na\u00b7cken", "fliehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Flicht Hyacinthenglocken", "tokens": ["Flicht", "Hya\u00b7cin\u00b7then\u00b7glo\u00b7cken"], "token_info": ["word", "word"], "pos": ["NN", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In seine goldnen Locken", "tokens": ["In", "sei\u00b7ne", "gold\u00b7nen", "Lo\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und herzt und k\u00fcsset ihn.", "tokens": ["Und", "herzt", "und", "k\u00fcs\u00b7set", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun tanzen sie an Quellen", "tokens": ["Nun", "tan\u00b7zen", "sie", "an", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verliebt im goldnen Hain,", "tokens": ["Ver\u00b7liebt", "im", "gold\u00b7nen", "Hain", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Versilberen die Wellen,", "tokens": ["Ver\u00b7sil\u00b7be\u00b7ren", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umsticken rund die Quellen", "tokens": ["Um\u00b7sti\u00b7cken", "rund", "die", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADJD", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Maienbl\u00fcmelein.", "tokens": ["Mit", "Mai\u00b7en\u00b7bl\u00fc\u00b7me\u00b7lein", "."], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wo find' ich dich, Belinde,", "tokens": ["Wo", "find'", "ich", "dich", ",", "Be\u00b7lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "$,", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Belinde, du mein Licht?", "tokens": ["Be\u00b7lin\u00b7de", ",", "du", "mein", "Licht", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich suche, ach ich finde", "tokens": ["Ich", "su\u00b7che", ",", "ach", "ich", "fin\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "XY", "PPER", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dich unter Rosen nicht.", "tokens": ["Dich", "un\u00b7ter", "Ro\u00b7sen", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ich suche dich in Str\u00e4uchen,", "tokens": ["Ich", "su\u00b7che", "dich", "in", "Str\u00e4u\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die V\u00f6gelchen durchschleichen,", "tokens": ["Die", "V\u00f6\u00b7gel\u00b7chen", "durch\u00b7schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die, wie ich, br\u00fcnstig gl\u00fchn.", "tokens": ["Die", ",", "wie", "ich", ",", "br\u00fcns\u00b7tig", "gl\u00fchn", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PPER", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich tappe unter Eichen,", "tokens": ["Ich", "tap\u00b7pe", "un\u00b7ter", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo Lilj' und Veilchen bl\u00fchn.", "tokens": ["Wo", "Lil\u00b7j'", "und", "Veil\u00b7chen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Der Tau f\u00e4llt von den Zweigen", "tokens": ["Der", "Tau", "f\u00e4llt", "von", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auf meine Locken hin.", "tokens": ["Auf", "mei\u00b7ne", "Lo\u00b7cken", "hin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Schon ist ein Heer von Westen", "tokens": ["Schon", "ist", "ein", "Heer", "von", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Myrthenbusch erwacht", "tokens": ["Im", "Myr\u00b7then\u00b7busch", "er\u00b7wacht"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und spielet unter Aesten.", "tokens": ["Und", "spie\u00b7let", "un\u00b7ter", "A\u00b7es\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Schon eilet von den Festen", "tokens": ["Schon", "ei\u00b7let", "von", "den", "Fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nach durchgelachter Nacht", "tokens": ["Nach", "durch\u00b7ge\u00b7lach\u00b7ter", "Nacht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "---+-+", "measure": "unknown.measure.di"}, "line.6": {"text": "Der frohe Schwarm von Scherzen", "tokens": ["Der", "fro\u00b7he", "Schwarm", "von", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Mit abgebrannten Kerzen", "tokens": ["Mit", "ab\u00b7ge\u00b7brann\u00b7ten", "Ker\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In bunter Fl\u00fcgel Tracht.", "tokens": ["In", "bun\u00b7ter", "Fl\u00fc\u00b7gel", "Tracht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Schon steigt vom goldnen Wagen,", "tokens": ["Schon", "steigt", "vom", "gold\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Silberwolken tragen,", "tokens": ["Den", "Sil\u00b7ber\u00b7wol\u00b7ken", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aurora in das Tal,", "tokens": ["Au\u00b7ro\u00b7ra", "in", "das", "Tal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "In dicht verflochtnen Buchen", "tokens": ["In", "dicht", "ver\u00b7flocht\u00b7nen", "Bu\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Den Liebling aufzusuchen,", "tokens": ["Den", "Lieb\u00b7ling", "auf\u00b7zu\u00b7su\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gejagt von Amors Qual.", "tokens": ["Ge\u00b7jagt", "von", "A\u00b7mors", "Qual", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Die goldnen Locken fliegen,", "tokens": ["Die", "gold\u00b7nen", "Lo\u00b7cken", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von Rosen durchgeschm\u00fcckt \u2013", "tokens": ["Von", "Ro\u00b7sen", "durch\u00b7ge\u00b7schm\u00fcckt", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie sieht den Sch\u00f6nen liegen", "tokens": ["Sie", "sieht", "den", "Sch\u00f6\u00b7nen", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seligem Vergn\u00fcgen,", "tokens": ["Mit", "se\u00b7li\u00b7gem", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der ihre Brust begl\u00fcckt.", "tokens": ["Der", "ih\u00b7re", "Brust", "be\u00b7gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie geht und pfl\u00fcckt Narcissen", "tokens": ["Sie", "geht", "und", "pfl\u00fcckt", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und kr\u00e4nzt ihn unter K\u00fcssen.", "tokens": ["Und", "kr\u00e4nzt", "ihn", "un\u00b7ter", "K\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von ihres Busens Schl\u00e4gen", "tokens": ["Von", "ih\u00b7res", "Bu\u00b7sens", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwacht der sch\u00f6ne Knab \u2013", "tokens": ["Er\u00b7wacht", "der", "sch\u00f6\u00b7ne", "Knab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ein bunter Blumenregen", "tokens": ["Ein",