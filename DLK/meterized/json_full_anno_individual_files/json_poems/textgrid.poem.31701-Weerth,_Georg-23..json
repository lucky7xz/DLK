{"textgrid.poem.31701": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dich lieb ich, und ach, kann es \u00e4ndern nicht,", "tokens": ["Dich", "lieb", "ich", ",", "und", "ach", ",", "kann", "es", "\u00e4n\u00b7dern", "nicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KON", "XY", "$,", "VMFIN", "PPER", "VVINF", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich liebe dich um dein sch\u00f6n Gesicht,", "tokens": ["Ich", "lie\u00b7be", "dich", "um", "dein", "sch\u00f6n", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Ich liebe dich um deine zarten Br\u00fcste", "tokens": ["Ich", "lie\u00b7be", "dich", "um", "dei\u00b7ne", "zar\u00b7ten", "Br\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und weil ich dich dreimal k\u00fc\u00dfte, k\u00fc\u00dfte!", "tokens": ["Und", "weil", "ich", "dich", "drei\u00b7mal", "k\u00fc\u00df\u00b7te", ",", "k\u00fc\u00df\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PRF", "ADV", "VVFIN", "$,", "VVFIN", "$."], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Ich ging zum Dome, da sah ich bald", "tokens": ["Ich", "ging", "zum", "Do\u00b7me", ",", "da", "sa