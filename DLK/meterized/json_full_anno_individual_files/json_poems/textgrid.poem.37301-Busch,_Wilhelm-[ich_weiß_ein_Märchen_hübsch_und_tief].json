{"textgrid.poem.37301": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ich wei\u00df ein M\u00e4rchen h\u00fcbsch und tief]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df ein M\u00e4rchen h\u00fcbsch und tief.", "tokens": ["Ich", "wei\u00df", "ein", "M\u00e4r\u00b7chen", "h\u00fcbsch", "und", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Hirtenknabe lag und schlief.", "tokens": ["Ein", "Hir\u00b7ten\u00b7kna\u00b7be", "lag", "und", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sprang heraus aus seinem Mund", "tokens": ["Da", "sprang", "he\u00b7raus", "aus", "sei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein M\u00e4uslein auf den Heidegrund.", "tokens": ["Ein", "M\u00e4us\u00b7lein", "auf", "den", "Hei\u00b7de\u00b7grund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das wei\u00dfe M\u00e4uslein lief sogleich", "tokens": ["Das", "wei\u00b7\u00dfe", "M\u00e4us\u00b7lein", "lief", "sog\u00b7leich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach einem Pferdesch\u00e4del bleich,", "tokens": ["Nach", "ei\u00b7nem", "Pfer\u00b7de\u00b7sch\u00e4\u00b7del", "bleich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der da schon manchen lieben Tag", "tokens": ["Der", "da", "schon", "man\u00b7chen", "lie\u00b7ben", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Sonnenschein und Regen lag.", "tokens": ["In", "Son\u00b7nen\u00b7schein", "und", "Re\u00b7gen", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Husch! ist das kleine M\u00e4uslein drin,", "tokens": ["Husch", "!", "ist", "das", "klei\u00b7ne", "M\u00e4us\u00b7lein", "drin", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "L\u00e4uft hin und her und her und hin,", "tokens": ["L\u00e4uft", "hin", "und", "her", "und", "her", "und", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "PTKVZ", "KON", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Besieht sich all die leeren F\u00e4cher,", "tokens": ["Be\u00b7sieht", "sich", "all", "die", "lee\u00b7ren", "F\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PIAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Schaut listig durch die Augenl\u00f6cher", "tokens": ["Schaut", "lis\u00b7tig", "durch", "die", "Au\u00b7gen\u00b7l\u00f6\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und raschelt so die Kreuz und Quer", "tokens": ["Und", "ra\u00b7schelt", "so", "die", "Kreuz", "und", "Quer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "KON", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.14": {"text": "Im alten Pferdekopf umher. \u2013", "tokens": ["Im", "al\u00b7ten", "Pfer\u00b7de\u00b7kopf", "um\u00b7her", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auf einmal kommt 'ne alte Kuh,", "tokens": ["Auf", "ein\u00b7mal", "kommt", "'ne", "al\u00b7te", "Kuh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Stellt sich da hin und macht Hamuh!", "tokens": ["Stellt", "sich", "da", "hin", "und", "macht", "Ha\u00b7muh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "PTKVZ", "KON", "VVFIN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das M\u00e4uslein, welches sehr erschreckt,", "tokens": ["Das", "M\u00e4us\u00b7lein", ",", "wel\u00b7ches", "sehr", "er\u00b7schreckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df da auf einmal wer so bl\u00f6kt,", "tokens": ["Da\u00df", "da", "auf", "ein\u00b7mal", "wer", "so", "bl\u00f6kt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "ADV", "PWS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Springt, hutschi, \u00fcbern Heidegrund", "tokens": ["Springt", ",", "hut\u00b7schi", ",", "\u00fc\u00b7bern", "Hei\u00b7de\u00b7grund"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wieder in des Knaben Mund. \u2013", "tokens": ["Und", "wie\u00b7der", "in", "des", "Kna\u00b7ben", "Mund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Knab erwacht und seufzte: Oh,", "tokens": ["Der", "Knab", "er\u00b7wacht", "und", "seufz\u00b7te", ":", "Oh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVFIN", "$.", "ITJ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie war ich doch im Traum so froh!", "tokens": ["Wie", "war", "ich", "doch", "im", "Traum", "so", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich ging in einen Wald hinaus,", "tokens": ["Ich", "ging", "in", "ei\u00b7nen", "Wald", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da kam ich vor ein hohes Haus,", "tokens": ["Da", "kam", "ich", "vor", "ein", "ho\u00b7hes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das war ein Schlo\u00df von Marmelstein.", "tokens": ["Das", "war", "ein", "Schlo\u00df", "von", "Mar\u00b7mel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ich ging in dieses Schlo\u00df hinein.", "tokens": ["Ich", "ging", "in", "die\u00b7ses", "Schlo\u00df", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Im Schlo\u00df sah ich ein M\u00e4dchen stehn,", "tokens": ["Im", "Schlo\u00df", "sah", "ich", "ein", "M\u00e4d\u00b7chen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das war Prinzessin Wundersch\u00f6n.", "tokens": ["Das", "war", "Prin\u00b7zes\u00b7sin", "Wun\u00b7der\u00b7sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "NN", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Sie l\u00e4chelt freundlich und bekannt,", "tokens": ["Sie", "l\u00e4\u00b7chelt", "freund\u00b7lich", "und", "be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Sie reicht mir ihre wei\u00dfe Hand,", "tokens": ["Sie", "reicht", "mir", "ih\u00b7re", "wei\u00b7\u00dfe", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sie spricht: \u00bbSchau her, ich habe Geld,", "tokens": ["Sie", "spricht", ":", "\u00bb", "Schau", "her", ",", "ich", "ha\u00b7be", "Geld", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "NN", "PTKVZ", "$,", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und mir geh\u00f6rt die halbe Welt.;", "tokens": ["Und", "mir", "ge\u00b7h\u00f6rt", "die", "hal\u00b7be", "Welt", ".", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ich liebe dich nur ganz allein,", "tokens": ["Ich", "lie\u00b7be", "dich", "nur", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Du sollst mein Herr und K\u00f6nig sein.\u00ab", "tokens": ["Du", "sollst", "mein", "Herr", "und", "K\u00f6\u00b7nig", "sein", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und wie ich fall in ihren Scho\u00df,", "tokens": ["Und", "wie", "ich", "fall", "in", "ih\u00b7ren", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Ratuh! kommt ein Trompetensto\u00df.", "tokens": ["Ra\u00b7tuh", "!", "kommt", "ein", "Trom\u00b7pe\u00b7ten\u00b7sto\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.17": {"text": "Und weg ist Liebchen, Schlo\u00df und alles", "tokens": ["Und", "weg", "ist", "Lieb\u00b7chen", ",", "Schlo\u00df", "und", "al\u00b7les"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "NN", "$,", "NN", "KON", "PIS"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Infolge des Trompetenschalles.", "tokens": ["In\u00b7fol\u00b7ge", "des", "Trom\u00b7pe\u00b7ten\u00b7schal\u00b7les", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Ich wei\u00df ein M\u00e4rchen h\u00fcbsch und tief.", "tokens": ["Ich", "wei\u00df", "ein", "M\u00e4r\u00b7chen", "h\u00fcbsch", "und", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Hirtenknabe lag und schlief.", "tokens": ["Ein", "Hir\u00b7ten\u00b7kna\u00b7be", "lag", "und", "schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da sprang heraus aus seinem Mund", "tokens": ["Da", "sprang", "he\u00b7raus", "aus", "sei\u00b7nem", "Mund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PTKVZ", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein M\u00e4uslein auf den Heidegrund.", "tokens": ["Ein", "M\u00e4us\u00b7lein", "auf", "den", "Hei\u00b7de\u