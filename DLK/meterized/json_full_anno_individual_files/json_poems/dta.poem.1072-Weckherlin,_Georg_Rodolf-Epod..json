{"dta.poem.1072": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Epod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aber/ zarte Liebelein/", "tokens": ["A\u00b7ber", "/", "zar\u00b7te", "Lie\u00b7be\u00b7lein", "/"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["KON", "$(", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie geh ich mich zu verlieren?", "tokens": ["Wie", "geh", "ich", "mich", "zu", "ver\u00b7lie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Kan ich dises lied volf\u00fchren/", "tokens": ["Kan", "ich", "di\u00b7ses", "lied", "vol\u00b7f\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6n-\u00e4ugige Nymfelein?", "tokens": ["Sch\u00f6n\u00b7\u00e4u\u00b7gi\u00b7ge", "Nym\u00b7fel\u00b7ein", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "++--+-+", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Ach", "tokens": ["Ach"], "token_info": ["word"], "pos": ["ITJ"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Seiner Tugent b\u00fcrd so schwer/", "tokens": ["Sei\u00b7ner", "Tu\u00b7gent", "b\u00fcrd", "so", "schwer", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Das das gl\u00fcck sich nimmermehr", "tokens": ["Das", "das", "gl\u00fcck", "sich", "nim\u00b7mer\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "ART", "NN", "PRF", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Jhrem verdienst kan vergleichen/", "tokens": ["Ih\u00b7rem", "ver\u00b7dienst", "kan", "ver\u00b7glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.9": {"text": "Vnd lasset mich nu vmb wenden/", "tokens": ["Vnd", "las\u00b7set", "mich", "nu", "vmb", "wen\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "VVINF", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Mein lied hipschlich zu vollenden.", "tokens": ["Mein", "lied", "hip\u00b7schlich", "zu", "voll\u00b7en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}