{"dta.poem.10322": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Iv.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Im Traum sah ich ein M\u00e4nnchen klein und putzig,", "tokens": ["Im", "Traum", "sah", "ich", "ein", "M\u00e4nn\u00b7chen", "klein", "und", "put\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das ging auf Stelzen, Schritte ellenweit,", "tokens": ["Das", "ging", "auf", "Stel\u00b7zen", ",", "Schrit\u00b7te", "el\u00b7len\u00b7weit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "$,", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Trug wei\u00dfe W\u00e4sche und ein feines Kleid,", "tokens": ["Trug", "wei\u00b7\u00dfe", "W\u00e4\u00b7sche", "und", "ein", "fei\u00b7nes", "Kleid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Inwendig aber war es grob und schmutzig.", "tokens": ["In\u00b7wen\u00b7dig", "a\u00b7ber", "war", "es", "grob", "und", "schmut\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Inwendig war es j\u00e4mmerlich, nichtsnutzig,", "tokens": ["In\u00b7wen\u00b7dig", "war", "es", "j\u00e4m\u00b7mer\u00b7lich", ",", "nichts\u00b7nut\u00b7zig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Jedoch von au\u00dfen voller W\u00fcrdigkeit;", "tokens": ["Je\u00b7doch", "von", "au\u00b7\u00dfen", "vol\u00b7ler", "W\u00fcr\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Von der Courage sprach es lang und breit,", "tokens": ["Von", "der", "Cou\u00b7ra\u00b7ge", "sprach", "es", "lang", "und", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Und that sogar recht trotzig und recht stutzig.", "tokens": ["Und", "that", "so\u00b7gar", "recht", "trot\u00b7zig", "und", "recht", "stut\u00b7zig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "\u201eund wei\u00dft du, wer das ist? Komm her und schau'!\u201c", "tokens": ["\u201e", "und", "wei\u00dft", "du", ",", "wer", "das", "ist", "?", "Komm", "her", "und", "schau'", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VVFIN", "PPER", "$,", "PWS", "PDS", "VAFIN", "$.", "VVFIN", "PTKVZ", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "So sprach der Traumgott, und er zeigt mir schlau", "tokens": ["So", "sprach", "der", "Traum\u00b7gott", ",", "und", "er", "zeigt", "mir", "schlau"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Bilderfluth in eines Spiegels Rahmen.", "tokens": ["Die", "Bil\u00b7der\u00b7fluth", "in", "ei\u00b7nes", "Spie\u00b7gels", "Rah\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Vor einem Altar stand das M\u00e4nnchen da,", "tokens": ["Vor", "ei\u00b7nem", "Al\u00b7tar", "stand", "das", "M\u00e4nn\u00b7chen", "da", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Mein Lieb daneben, Beide sprachen: Ja!", "tokens": ["Mein", "Lieb", "da\u00b7ne\u00b7ben", ",", "Bei\u00b7de", "spra\u00b7chen", ":", "Ja", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "PAV", "$,", "PIS", "VVFIN", "$.", "PTKANT", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Und tausend Teufel riefen lachend: Amen!", "tokens": ["Und", "tau\u00b7send", "Teu\u00b7fel", "rie\u00b7fen", "la\u00b7chend", ":", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ADJD", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}