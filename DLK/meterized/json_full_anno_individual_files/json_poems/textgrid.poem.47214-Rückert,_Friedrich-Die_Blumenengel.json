{"textgrid.poem.47214": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Blumenengel", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Engelein, liebes Maidelein,", "tokens": ["Die", "En\u00b7ge\u00b7lein", ",", "lie\u00b7bes", "Mai\u00b7de\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sind auch wohl so sch\u00f6n, wie du, und so fein;", "tokens": ["Sind", "auch", "wohl", "so", "sch\u00f6n", ",", "wie", "du", ",", "und", "so", "fein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "KON", "ADV", "ADJD", "$."], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Halt da\u00df wir sie nur nicht k\u00f6nnen sehen,", "tokens": ["Halt", "da\u00df", "wir", "sie", "nur", "nicht", "k\u00f6n\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn sie vom Himmel zur Erde gehen.", "tokens": ["Wenn", "sie", "vom", "Him\u00b7mel", "zur", "Er\u00b7de", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Wenn du's aber noch nicht wei\u00dft,", "tokens": ["Wenn", "du's", "a\u00b7ber", "noch", "nicht", "wei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Engele wohnen zumeist,", "tokens": ["Wo", "die", "En\u00b7ge\u00b7le", "woh\u00b7nen", "zu\u00b7meist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVINF", "VVFIN", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Wenn sie vom Himmel zur Erde kummen,", "tokens": ["Wenn", "sie", "vom", "Him\u00b7mel", "zur", "Er\u00b7de", "kum\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So will ich dir's sagen: das sind die Blummen.", "tokens": ["So", "will", "ich", "dir's", "sa\u00b7gen", ":", "das", "sind", "die", "Blum\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Jegliche Blum' ist ein Gezelt,", "tokens": ["Jeg\u00b7li\u00b7che", "Blum'", "ist", "ein", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Das sich ein Engelein hat bestellt,", "tokens": ["Das", "sich", "ein", "En\u00b7ge\u00b7lein", "hat", "be\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Wo's von seiner Wanderung h\u00e4lt Ruh',", "tokens": ["Wo's", "von", "sei\u00b7ner", "Wan\u00b7de\u00b7rung", "h\u00e4lt", "Ruh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PPOSAT", "NN", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Bis's wieder fleugt dem Himmel zu.", "tokens": ["Bis's", "wie\u00b7der", "fleugt", "dem", "Him\u00b7mel", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und's Engelein ist auf sein H\u00e4uslein bedacht,", "tokens": ["Un\u00b7d's", "En\u00b7ge\u00b7lein", "ist", "auf", "sein", "H\u00e4us\u00b7lein", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+--+", "measure": "iambic.hexa.chol"}, "line.2": {"text": "Wie's jeder Mensch mit sei'm eigenen macht,", "tokens": ["Wie's", "je\u00b7der", "Mensch", "mit", "sei'm", "ei\u00b7ge\u00b7nen", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es ziert's und schm\u00fcckt's aus um und an,", "tokens": ["Es", "ziert's", "und", "schm\u00fcckt's", "aus", "um", "und", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "APPR", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df es ihm drinne gefallen kann.", "tokens": ["Da\u00df", "es", "ihm", "drin\u00b7ne", "ge\u00b7fal\u00b7len", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Es holt sich goldigen Sonnenschein", "tokens": ["Es", "holt", "sich", "gol\u00b7di\u00b7gen", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und legt ihn rings au\u00dfen ums D\u00e4chelein,", "tokens": ["Und", "legt", "ihn", "rings", "au\u00b7\u00dfen", "ums", "D\u00e4\u00b7che\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Es holt sich Farben mancherhand", "tokens": ["Es", "holt", "sich", "Far\u00b7ben", "man\u00b7cher\u00b7hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bemalt sich von innen des H\u00e4usleins Wand.", "tokens": ["Und", "be\u00b7malt", "sich", "von", "in\u00b7nen", "des", "H\u00e4us\u00b7leins", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ADV", "ART", "NN", "NN", "$."], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}}, "stanza.6": {"line.1": {"text": "Es backt sich von Blumenmehl Himmelsbrot,", "tokens": ["Es", "backt", "sich", "von", "Blu\u00b7men\u00b7mehl", "Him\u00b7mels\u00b7brot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Da\u00df es auf Erden nicht leidet Not;", "tokens": ["Da\u00df", "es", "auf", "Er\u00b7den", "nicht", "lei\u00b7det", "Not", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PTKNEG", "VVFIN", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Es br\u00e4ut sich aus Tau sein Tr\u00e4nklein frisch", "tokens": ["Es", "br\u00e4ut", "sich", "aus", "Tau", "sein", "Tr\u00e4nk\u00b7lein", "frisch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "PPOSAT", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und schickt sich in allem ganz haush\u00e4lterisch.", "tokens": ["Und", "schickt", "sich", "in", "al\u00b7lem", "ganz", "haus\u00b7h\u00e4l\u00b7te\u00b7risch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PIS", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Und das Bl\u00fcmmele hat recht seine Freud',", "tokens": ["Und", "das", "Bl\u00fcm\u00b7me\u00b7le", "hat", "recht", "sei\u00b7ne", "Freud'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Wie sein Hausherr so drin schafft und br\u00e4ut;", "tokens": ["Wie", "sein", "Haus\u00b7herr", "so", "drin", "schafft", "und", "br\u00e4ut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "--++-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und wenn's Englein dann wieder gen Himmel wandert,", "tokens": ["Und", "wenn's", "En\u00b7glein", "dann", "wie\u00b7der", "gen", "Him\u00b7mel", "wan\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So f\u00e4llt das H\u00e4uslein vor Weh auseinander.", "tokens": ["So", "f\u00e4llt", "das", "H\u00e4us\u00b7lein", "vor", "Weh", "aus\u00b7ein\u00b7an\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Liebes Maidelein, wenn du dann", "tokens": ["Lie\u00b7bes", "Mai\u00b7de\u00b7lein", ",", "wenn", "du", "dann"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KOUS", "PPER", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Willst allweil die Englein um dich han,", "tokens": ["Willst", "all\u00b7weil", "die", "En\u00b7glein", "um", "dich", "han", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "APPR", "PPER", "VAFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So mu\u00dft du's nur mit den Bl\u00fcmlein halten,", "tokens": ["So", "mu\u00dft", "du's", "nur", "mit", "den", "Bl\u00fcm\u00b7lein", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So wer'n auch die Englein um dich walten.", "tokens": ["So", "wer'n", "auch", "die", "En\u00b7glein", "um", "dich", "wal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Stell' eine Blume vor das Fenster dein,", "tokens": ["Stell'", "ei\u00b7ne", "Blu\u00b7me", "vor", "das", "Fens\u00b7ter", "dein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So l\u00e4\u00dft sie dir keinen b\u00f6sen Gedanken herein;", "tokens": ["So", "l\u00e4\u00dft", "sie", "dir", "kei\u00b7nen", "b\u00f6\u00b7sen", "Ge\u00b7dan\u00b7ken", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stecke vor deine Brust einen Blumenstrau\u00df,", "tokens": ["Ste\u00b7cke", "vor", "dei\u00b7ne", "Brust", "ei\u00b7nen", "Blu\u00b7men\u00b7strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "So gehst du allweg mit einem Engelein aus.", "tokens": ["So", "gehst", "du", "all\u00b7weg", "mit", "ei\u00b7nem", "En\u00b7ge\u00b7lein", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "Begie\u00dfe fr\u00fchmorgens ein Lilienreis,", "tokens": ["Be\u00b7gie\u00b7\u00dfe", "fr\u00fch\u00b7mor\u00b7gens", "ein", "Li\u00b7li\u00b7en\u00b7reis", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So bleibst du den ganzen Tag lilienwei\u00df;", "tokens": ["So", "bleibst", "du", "den", "gan\u00b7zen", "Tag", "li\u00b7li\u00b7en\u00b7wei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Stell' nachts an dein Bett eine Rose zur Hut,", "tokens": ["Stell'", "nachts", "an", "dein", "Bett", "ei\u00b7ne", "Ro\u00b7se", "zur", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "So wiegt dich ein Engel auf Rosen gut.", "tokens": ["So", "wiegt", "dich", "ein", "En\u00b7gel", "auf", "Ro\u00b7sen", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "NN", "ADJD", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.11": {"line.1": {"text": "Kein arg Tr\u00e4umen kann dich schrecken,", "tokens": ["Kein", "arg", "Tr\u00e4u\u00b7men", "kann", "dich", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn ein Engelein wird dich decken;", "tokens": ["Denn", "ein", "En\u00b7ge\u00b7lein", "wird", "dich", "de\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und welche Tr\u00e4um' es zu dir l\u00e4\u00dft ein,", "tokens": ["Und", "wel\u00b7che", "Tr\u00e4um'", "es", "zu", "dir", "l\u00e4\u00dft", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "PPER", "APPR", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das m\u00fcssen gute Tr\u00e4ume sein.", "tokens": ["Das", "m\u00fcs\u00b7sen", "gu\u00b7te", "Tr\u00e4u\u00b7me", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Wenn du dann in solcher Hut", "tokens": ["Wenn", "du", "dann", "in", "sol\u00b7cher", "Hut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wirst tr\u00e4umen von meiner Liebesglut,", "tokens": ["Wirst", "tr\u00e4u\u00b7men", "von", "mei\u00b7ner", "Lie\u00b7bes\u00b7glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So denke, da\u00df sie ist treu und rein,", "tokens": ["So", "den\u00b7ke", ",", "da\u00df", "sie", "ist", "treu", "und", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonst lie\u00df sie das Englein nicht zu dir ein.", "tokens": ["Sonst", "lie\u00df", "sie", "das", "En\u00b7glein", "nicht", "zu", "dir", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.13": {"li