{"textgrid.poem.42731": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "[frau Teemaschine sang auf dem Feuer]", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau Teemaschine sang auf dem Feuer.", "tokens": ["Frau", "Tee\u00b7ma\u00b7schi\u00b7ne", "sang", "auf", "dem", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Beifall war ganz ungeheuer.", "tokens": ["Der", "Bei\u00b7fall", "war", "ganz", "un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, ihre Base Petroleumkanne", "tokens": ["Ja", ",", "ih\u00b7re", "Ba\u00b7se", "Pet\u00b7ro\u00b7le\u00b7um\u00b7kan\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "NE"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "War von dem Liede ganz gefangen.", "tokens": ["War", "von", "dem", "Lie\u00b7de", "ganz", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr rannen die Tr\u00e4nen \u00fcber die Wangen", "tokens": ["Ihr", "ran\u00b7nen", "die", "Tr\u00e4\u00b7nen", "\u00fc\u00b7ber", "die", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und tropften gerade in eine Pfanne,", "tokens": ["Und", "tropf\u00b7ten", "ge\u00b7ra\u00b7de", "in", "ei\u00b7ne", "Pfan\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "In der ein Schweinebraten briet,", "tokens": ["In", "der", "ein", "Schwei\u00b7ne\u00b7bra\u00b7ten", "briet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der ausgezeichnet dann geriet.", "tokens": ["Der", "aus\u00b7ge\u00b7zeich\u00b7net", "dann", "ge\u00b7riet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "War auch Petroleum drauf geflossen,", "tokens": ["War", "auch", "Pet\u00b7ro\u00b7le\u00b7um", "drauf", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "PAV", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.10": {"text": "Er wurde trotzdem doch genossen.", "tokens": ["Er", "wur\u00b7de", "trotz\u00b7dem", "doch", "ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Sein Herr war mit dem Koch zufrieden.", "tokens": ["Sein", "Herr", "war", "mit", "dem", "Koch", "zu\u00b7frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NE", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "(besagter Herr war ein Kosak;", "tokens": ["(", "be\u00b7sag\u00b7ter", "Herr", "war", "ein", "Ko\u00b7sak", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Leibgericht war Siegellack.)", "tokens": ["Sein", "Leib\u00b7ge\u00b7richt", "war", "Sie\u00b7gel\u00b7lack", ".", ")"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, die Geschm\u00e4cker sind verschieden.", "tokens": ["Ja", ",", "die", "Ge\u00b7schm\u00e4\u00b7cker", "sind", "ver\u00b7schie\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Frau Teemaschine sang auf dem Feuer.", "tokens": ["Frau", "Tee\u00b7ma\u00b7schi\u00b7ne", "sang", "auf", "dem", "Feu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Beifall war ganz ungeheuer.", "tokens": ["Der", "Bei\u00b7fall", "war", "ganz", "un\u00b7ge\u00b7heu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ja, ihre Base Petroleumkanne", "tokens": ["Ja", ",", "ih\u00b7re", "Ba\u00b7se", "Pet\u00b7ro\u00b7le\u00b7um\u00b7kan\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "NE"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "War von dem Liede ganz gefangen.", "tokens": ["War", "von", "dem", "Lie\u00b7de", "ganz", "