{"dta.poem.8878": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xi.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Wer h\u00e4tte je dich anzuschau'n bereuet?", "tokens": ["Wer", "h\u00e4t\u00b7te", "je", "dich", "an\u00b7zu\u00b7schau'n", "be\u00b7re\u00b7u\u00b7et", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPER", "VVIZU", "VVFIN", "$."], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wie viele Reize liegen hingestreuet", "tokens": ["Wie", "vie\u00b7le", "Rei\u00b7ze", "lie\u00b7gen", "hin\u00b7ge\u00b7streu\u00b7et"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "VVINF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf diesen Wangen, diesen sch\u00f6nen Haaren!", "tokens": ["Auf", "die\u00b7sen", "Wan\u00b7gen", ",", "die\u00b7sen", "sch\u00f6\u00b7nen", "Haa\u00b7ren", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist so zart, du bist so jung an Jahren,", "tokens": ["Du", "bist", "so", "zart", ",", "du", "bist", "so", "jung", "an", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "ADV", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch jede Huldigung des Gl\u00fccks erfreuet;", "tokens": ["Durch", "je\u00b7de", "Hul\u00b7di\u00b7gung", "des", "Gl\u00fccks", "er\u00b7freu\u00b7et", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch wer die List in deinem Busen scheuet,", "tokens": ["Doch", "wer", "die", "List", "in", "dei\u00b7nem", "Bu\u00b7sen", "scheu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der mag vor dir sich Tag und Nacht bewahren!", "tokens": ["Der", "mag", "vor", "dir", "sich", "Tag", "und", "Nacht", "be\u00b7wah\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "PRF", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Noch prahlt ein Baum mit manchem frischen Aste,", "tokens": ["Noch", "prahlt", "ein", "Baum", "mit", "man\u00b7chem", "fri\u00b7schen", "As\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Bl\u00e4tter bilden noch ger\u00e4um'ge Lauben,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "bil\u00b7den", "noch", "ge\u00b7r\u00e4um'\u00b7ge", "Lau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da schon Zerst\u00f6rung w\u00fcthet unter'm Baste.", "tokens": ["Da", "schon", "Zer\u00b7st\u00f6\u00b7rung", "w\u00fct\u00b7het", "un\u00b7ter'm", "Bas\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch soll mir frostige Betrachtung rauben", "tokens": ["Doch", "soll", "mir", "fros\u00b7ti\u00b7ge", "Be\u00b7trach\u00b7tung", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den s\u00fc\u00dfen Schatten, unter dem ich raste?", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Schat\u00b7ten", ",", "un\u00b7ter", "dem", "ich", "ras\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nein, deine Sch\u00f6nheit fordert blinden Glauben!", "tokens": ["Nein", ",", "dei\u00b7ne", "Sch\u00f6n\u00b7heit", "for\u00b7dert", "blin\u00b7den", "Glau\u00b7ben", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}