{"textgrid.poem.64323": {"metadata": {"author": {"name": "Stolberg, Friedrich Leopold Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wie freudig die Lerche", "genre": "verse", "period": "N.A.", "pub_year": 1776, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie freudig die Lerche", "tokens": ["Wie", "freu\u00b7dig", "die", "Ler\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Schwebet entgegen", "tokens": ["Schwe\u00b7bet", "ent\u00b7ge\u00b7gen"], "token_info": ["word", "word"], "pos": ["VVFIN", "PTKVZ"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dem r\u00f6tlichen Morgen;", "tokens": ["Dem", "r\u00f6t\u00b7li\u00b7chen", "Mor\u00b7gen", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "So schwebet in melodischem Fluge des Gesangs", "tokens": ["So", "schwe\u00b7bet", "in", "me\u00b7lo\u00b7di\u00b7schem", "Flu\u00b7ge", "des", "Ge\u00b7sangs"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Lieblichste Tochter der Natur,", "tokens": ["Lieb\u00b7lichs\u00b7te", "Toch\u00b7ter", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sch\u00f6nheit, meine d\u00fcrstende Seele dir nach.", "tokens": ["Sch\u00f6n\u00b7heit", ",", "mei\u00b7ne", "d\u00fcrs\u00b7ten\u00b7de", "See\u00b7le", "dir", "nach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "PPER", "PTKVZ", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Deine heimische Laube", "tokens": ["Dei\u00b7ne", "hei\u00b7mi\u00b7sche", "Lau\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Bl\u00fchet unter den Sternen nicht:", "tokens": ["Bl\u00fc\u00b7het", "un\u00b7ter", "den", "Ster\u00b7nen", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Aber auf Strahlen des Himmels", "tokens": ["A\u00b7ber", "auf", "Strah\u00b7len", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Schwebest oft zu Sterblichen du hinab,", "tokens": ["Schwe\u00b7best", "oft", "zu", "Sterb\u00b7li\u00b7chen", "du", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "L\u00e4cheltest mir oft", "tokens": ["L\u00e4\u00b7chel\u00b7test", "mir", "oft"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Von purpurnen Wangen des Morgens,", "tokens": ["Von", "pur\u00b7pur\u00b7nen", "Wan\u00b7gen", "des", "Mor\u00b7gens", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADV", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Oft vom Schimmer des Mondes,", "tokens": ["Oft", "vom", "Schim\u00b7mer", "des", "Mon\u00b7des", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und vom Spiegel des Sees, den der Hain umkr\u00e4nzt,", "tokens": ["Und", "vom", "Spie\u00b7gel", "des", "Sees", ",", "den", "der", "Hain", "um\u00b7kr\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.9": {"text": "Sanfte Ruh in die Seele,", "tokens": ["Sanf\u00b7te", "Ruh", "in", "die", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.10": {"text": "Ahndungen und Himmelsgef\u00fchl.", "tokens": ["Ahn\u00b7dun\u00b7gen", "und", "Him\u00b7mels\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Ach! auf Wangen des M\u00e4dchens", "tokens": ["Ach", "!", "auf", "Wan\u00b7gen", "des", "M\u00e4d\u00b7chens"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "APPR", "NN", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Sah ich dich himmlischer noch!", "tokens": ["Sah", "ich", "dich", "himm\u00b7li\u00b7scher", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "ADV", "$."], "meter": "+--+-++", "measure": "iambic.tetra.invert"}, "line.3": {"text": "In sanftrollender Unschuld", "tokens": ["In", "sanf\u00b7trol\u00b7len\u00b7der", "Un\u00b7schuld"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ihrer schmelzenden Augen", "tokens": ["Ih\u00b7rer", "schmel\u00b7zen\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Sah ich dich himmlischer noch!", "tokens": ["Sah", "ich", "dich", "himm\u00b7li\u00b7scher", "noch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADJD", "ADV", "$."], "meter": "+--+-++", "measure": "iambic.tetra.invert"}, "line.6": {"text": "H\u00f6rte dich in den bebenden Melodieen", "tokens": ["H\u00f6r\u00b7te", "dich", "in", "den", "be\u00b7ben\u00b7den", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Ihrer schwebenden Stimme!", "tokens": ["Ih\u00b7rer", "schwe\u00b7ben\u00b7den", "Stim\u00b7me", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "H\u00f6rte dich! sah dich, f\u00fchlte dich!", "tokens": ["H\u00f6r\u00b7te", "dich", "!", "sah", "dich", ",", "f\u00fchl\u00b7te", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Und in Flammen der Liebe...", "tokens": ["Und", "in", "Flam\u00b7men", "der", "Lie\u00b7be", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ART", "NN", "$("], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Wehe mir! wehe!", "tokens": ["We\u00b7he", "mir", "!", "we\u00b7he", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADJD", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Was bebt meine Seele", "tokens": ["Was", "bebt", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Pl\u00f6tzlich in die Ebbe des Gesangs zur\u00fcck!", "tokens": ["Pl\u00f6tz\u00b7lich", "in", "die", "Eb\u00b7be", "des", "Ge\u00b7sangs", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Selinde! Selinde!", "tokens": ["Se\u00b7lin\u00b7de", "!", "Se\u00b7lin\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$.", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Versiegt bei deinem Namen mein Gesang?", "tokens": ["Ver\u00b7siegt", "bei", "dei\u00b7nem", "Na\u00b7men", "mein", "Ge\u00b7sang", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Stolberg, sei ein Mann!", "tokens": ["Stol\u00b7berg", ",", "sei", "ein", "Mann", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Str\u00f6me wieder, Gesang!", "tokens": ["Str\u00f6\u00b7me", "wie\u00b7der", ",", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Str\u00f6m', ich beschw\u00f6re dich bei deiner Kraft!", "tokens": ["Str\u00f6m'", ",", "ich", "be\u00b7schw\u00f6\u00b7re", "dich", "bei", "dei\u00b7ner", "Kraft", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Denn die heimische Laube", "tokens": ["Denn", "die", "hei\u00b7mi\u00b7sche", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Der seligen G\u00f6ttin", "tokens": ["Der", "se\u00b7li\u00b7gen", "G\u00f6t\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+---+-", "measure": "dactylic.init"}, "line.11": {"text": "Bl\u00fchet unter den Sternen nicht!", "tokens": ["Bl\u00fc\u00b7het", "un\u00b7ter", "den", "Ster\u00b7nen", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKNEG", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Himmlische Ursch\u00f6nheit!", "tokens": ["Himm\u00b7li\u00b7sche", "Ur\u00b7sch\u00f6n\u00b7heit", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Oder wie nennen die Unsterblichen dich,", "tokens": ["O\u00b7der", "wie", "nen\u00b7nen", "die", "U\u00b7nsterb\u00b7li\u00b7chen", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "ART", "NN", "PPER", "$,"], "meter": "+-+---+---+", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Welche besser dich kennen, als Homer,", "tokens": ["Wel\u00b7che", "bes\u00b7ser", "dich", "ken\u00b7nen", ",", "als", "Ho\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "VVINF", "$,", "KOUS", "NE", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Plato, Klopstock und Ossian?", "tokens": ["Pla\u00b7to", ",", "Klops\u00b7tock", "und", "Os\u00b7si\u00b7an", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "KON", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Bist du der olympischen Tugend", "tokens": ["Bist", "du", "der", "o\u00b7lym\u00b7pi\u00b7schen", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Schwester? oder sie selbst?", "tokens": ["Schwes\u00b7ter", "?", "o\u00b7der", "sie", "selbst", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "PPER", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Selige Bewohner des Lichts,", "tokens": ["Se\u00b7li\u00b7ge", "Be\u00b7woh\u00b7ner", "des", "Lichts", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Welche sich sonnen in deinem Strahl,", "tokens": ["Wel\u00b7che", "sich", "son\u00b7nen", "in", "dei\u00b7nem", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Und mit schwellendem Segel", "tokens": ["Und", "mit", "schwel\u00b7len\u00b7dem", "Se\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Schiffen auf der Wahrheit unendlichem Oceanus!", "tokens": ["Schif\u00b7fen", "auf", "der", "Wahr\u00b7heit", "un\u00b7end\u00b7li\u00b7chem", "O\u00b7ce\u00b7a\u00b7nus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+--+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Weise der Erde", "tokens": ["Wei\u00b7se", "der", "Er\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Stehn am sandichten Ufer,", "tokens": ["Stehn", "am", "san\u00b7dich\u00b7ten", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.7": {"text": "Und freun sich, wie Kinder,", "tokens": ["Und", "freun", "sich", ",", "wie", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "PWAV", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Wenn die kleine Kenntnis", "tokens": ["Wenn", "die", "klei\u00b7ne", "Kennt\u00b7nis"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Zappelt an der Angel schwankendem Rohr,", "tokens": ["Zap\u00b7pelt", "an", "der", "An\u00b7gel", "schwan\u00b7ken\u00b7dem", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.10": {"text": "L\u00e4cheln wie Kinder", "tokens": ["L\u00e4\u00b7cheln", "wie", "Kin\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["NN", "KOKOM", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "\u00dcber den wei\u00dfen Schaum", "tokens": ["\u00dc\u00b7ber", "den", "wei\u00b7\u00dfen", "Schaum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.12": {"text": "Und die bunte Blase,", "tokens": ["Und", "die", "bun\u00b7te", "Bla\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Ehe sie am Gestade zerplatzt!", "tokens": ["E\u00b7he", "sie", "am", "Ge\u00b7sta\u00b7de", "zer\u00b7platzt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Lieber wall' ich am Ufer,", "tokens": ["Lie\u00b7ber", "wall'", "ich", "am", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Ruhig und gedankenvoll!", "tokens": ["Ru\u00b7hig", "und", "ge\u00b7dan\u00b7ken\u00b7voll", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So h\u00f6rt doch mein Ohr", "tokens": ["So", "h\u00f6rt", "doch", "mein", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Der ernsten Wogen rauschenden Fall!", "tokens": ["Der", "erns\u00b7ten", "Wo\u00b7gen", "rau\u00b7schen\u00b7den", "Fall", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Es sp\u00e4het mein Blick", "tokens": ["Es", "sp\u00e4\u00b7het", "mein", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": 