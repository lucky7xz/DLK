], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "O la\u00df mich werden ein stolzer B\u00e4r,", "tokens": ["O", "la\u00df", "mich", "wer\u00b7den", "ein", "stol\u00b7zer", "B\u00e4r", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dann geh ich im warmen Rock daher!", "tokens": ["Dann", "geh", "ich", "im", "war\u00b7men", "Rock", "da\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "PAV", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O la\u00df mich werden ein sch\u00f6ner Schwan,", "tokens": ["O", "la\u00df", "mich", "wer\u00b7den", "ein", "sch\u00f6\u00b7ner", "Schwan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Dann wohn ich auf Strom und Ozean!", "tokens": ["Dann", "wohn", "ich", "auf", "Strom", "und", "O\u00b7ze\u00b7an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "O mach aus mir einen Panther wild,", "tokens": ["O", "mach", "aus", "mir", "ei\u00b7nen", "Pan\u00b7ther", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Einen Leu, da\u00df hoch meine M\u00e4hne schwillt,", "tokens": ["Ei\u00b7nen", "Leu", ",", "da\u00df", "