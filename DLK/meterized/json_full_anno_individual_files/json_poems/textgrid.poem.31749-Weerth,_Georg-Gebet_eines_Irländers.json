{"textgrid.poem.31749": {"metadata": {"author": {"name": "Weerth, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gebet eines Irl\u00e4nders", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sankt Patrick, gro\u00dfer Schutzpatron,", "tokens": ["Sankt", "Pat\u00b7rick", ",", "gro\u00b7\u00dfer", "Schutz\u00b7pat\u00b7ron", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du sitzt auf dem warmen Himmelsthron;", "tokens": ["Du", "sitzt", "auf", "dem", "war\u00b7men", "Him\u00b7melst\u00b7hron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O sieh mich an mit freundlichem Sinn,", "tokens": ["O", "sieh", "mich", "an", "mit", "freund\u00b7li\u00b7chem", "Sinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "APPR", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Dieweil ich ein armer Paddy bin!", "tokens": ["Die\u00b7weil", "ich", "ein", "ar\u00b7mer", "Pad\u00b7dy", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Sankt Patrick, sieh, die Nacht kommt bald,", "tokens": ["Sankt", "Pat\u00b7rick", ",", "sieh", ",", "die", "Nacht", "kommt", "bald", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "$,", "ART", "NN", "VVFIN", "ADV", "$,"], "mete