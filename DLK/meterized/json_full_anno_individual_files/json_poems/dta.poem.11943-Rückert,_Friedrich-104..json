{"dta.poem.11943": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "104.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist dir ein Freund verstimmt, so sieh aus welchem Grunde;", "tokens": ["Ist", "dir", "ein", "Freund", "ver\u00b7stimmt", ",", "so", "sieh", "aus", "wel\u00b7chem", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$,", "ADV", "VVFIN", "APPR", "PWAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und findest du den Grund, so ists zur guten Stunde.", "tokens": ["Und", "fin\u00b7dest", "du", "den", "Grund", ",", "so", "ists", "zur", "gu\u00b7ten", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$,", "ADV", "VAFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du brauchest nur den Grund hinwegzur\u00e4umen eben,", "tokens": ["Du", "brau\u00b7chest", "nur", "den", "Grund", "hin\u00b7weg\u00b7zu\u00b7r\u00e4u\u00b7men", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die Verstimmung wird von selbst sich wieder heben.", "tokens": ["Und", "die", "Ver\u00b7stim\u00b7mung", "wird", "von", "selbst", "sich", "wie\u00b7der", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "ADV", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch wenn du keinen Grund im Stand zu finden bist,", "tokens": ["Doch", "wenn", "du", "kei\u00b7nen", "Grund", "im", "Stand", "zu", "fin\u00b7den", "bist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIAT", "NN", "APPRART", "NN", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das eben ist ein Grund, der nicht zu heben ist.", "tokens": ["Das", "e\u00b7ben", "ist", "ein", "Grund", ",", "der", "nicht", "zu", "he\u00b7ben", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "PTKNEG", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}