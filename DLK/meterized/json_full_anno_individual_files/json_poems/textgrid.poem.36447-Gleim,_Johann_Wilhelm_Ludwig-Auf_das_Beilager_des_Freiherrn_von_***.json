{"textgrid.poem.36447": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf das Beilager des Freiherrn von ***", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, nun hab ich sie gesehen.", "tokens": ["Ja", ",", "nun", "hab", "ich", "sie", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie reitzte sie, die Sch\u00f6ne!", "tokens": ["O", "wie", "reitz\u00b7te", "sie", ",", "die", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche kluge Schmeichelworte!", "tokens": ["Wel\u00b7che", "klu\u00b7ge", "Schmei\u00b7chel\u00b7wor\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche sanfte Venusminen!", "tokens": ["Wel\u00b7che", "sanf\u00b7te", "Ve\u00b7nus\u00b7mi\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche holde Rosenwangen!", "tokens": ["Wel\u00b7che", "hol\u00b7de", "Ro\u00b7sen\u00b7wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tausend sch\u00f6ne Liebesg\u00f6tter", "tokens": ["Tau\u00b7send", "sch\u00f6\u00b7ne", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schw\u00e4rmten um die sch\u00f6nen Glieder.", "tokens": ["Schw\u00e4rm\u00b7ten", "um", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drei\u00dfig schw\u00e4rmten um die Wangen,", "tokens": ["Drei\u00b7\u00dfig", "schw\u00e4rm\u00b7ten", "um", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zwanzig scherzten mit den Lokken;", "tokens": ["Zwan\u00b7zig", "scherz\u00b7ten", "mit", "den", "Lok\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber, wo die meisten schw\u00e4rmten,", "tokens": ["A\u00b7ber", ",", "wo", "die", "meis\u00b7ten", "schw\u00e4rm\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo die meisten schw\u00e4rmen werden,", "tokens": ["Wo", "die", "meis\u00b7ten", "schw\u00e4r\u00b7men", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIS", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Soll der sch\u00f6ne Br\u00e4utgam sagen.", "tokens": ["Soll", "der", "sch\u00f6\u00b7ne", "Br\u00e4ut\u00b7gam", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Br\u00e4utgam, sagst du es dem Dichter,", "tokens": ["Br\u00e4ut\u00b7gam", ",", "sagst", "du", "es", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "O so soll er nebst der Sch\u00f6nen,", "tokens": ["O", "so", "soll", "er", "nebst", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Alle Liebesg\u00f6tter malen.", "tokens": ["Al\u00b7le", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Itzund soll er nur die Sch\u00f6ne", "tokens": ["It\u00b7zund", "soll", "er", "nur", "die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Pesnen oder Harpern malen.", "tokens": ["Pes\u00b7nen", "o\u00b7der", "Har\u00b7pern", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbauf! Vortreflichster der Maler!", "tokens": ["\u00bb", "auf", "!", "Vor\u00b7tre\u00b7flichs\u00b7ter", "der", "Ma\u00b7ler", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf, und schildre, Preis der Maler!", "tokens": ["Auf", ",", "und", "schild\u00b7re", ",", "Preis", "der", "Ma\u00b7ler", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meister in der Kunst der Rhoder,", "tokens": ["Meis\u00b7ter", "in", "der", "Kunst", "der", "Rho\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm, und schildre diese Sch\u00f6ne,", "tokens": ["Komm", ",", "und", "schild\u00b7re", "die\u00b7se", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ich sie beschreiben werde!", "tokens": ["Wie", "ich", "sie", "be\u00b7schrei\u00b7ben", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Male mir vor allen Dingen,", "tokens": ["Ma\u00b7le", "mir", "vor", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zarte rabenschwarze Haare,", "tokens": ["Zar\u00b7te", "ra\u00b7ben\u00b7schwar\u00b7ze", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und, wofern es anders m\u00f6glich,", "tokens": ["Und", ",", "wo\u00b7fern", "es", "an\u00b7ders", "m\u00f6g\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Male sie auch lieblich d\u00fcftend.", "tokens": ["Ma\u00b7le", "sie", "auch", "lieb\u00b7lich", "d\u00fcf\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Male zwischen schwarze Lokken,", "tokens": ["Ma\u00b7le", "zwi\u00b7schen", "schwar\u00b7ze", "Lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da, wo sich die Wangen schliessen,", "tokens": ["Da", ",", "wo", "sich", "die", "Wan\u00b7gen", "schlies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Eine Stirn von Elfenbeine.", "tokens": ["Ei\u00b7ne", "Stirn", "von", "El\u00b7fen\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "La\u00df sich nicht die schwarzen Bogen,", "tokens": ["La\u00df", "sich", "nicht", "die", "schwar\u00b7zen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die sich um die Augen kr\u00fcmmen,", "tokens": ["Die", "sich", "um", "die", "Au\u00b7gen", "kr\u00fcm\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "G\u00e4nzlich trennen, noch vermischen;", "tokens": ["G\u00e4nz\u00b7lich", "tren\u00b7nen", ",", "noch", "ver\u00b7mi\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sondern, wie bei meinem M\u00e4dchen,", "tokens": ["Son\u00b7dern", ",", "wie", "bei", "mei\u00b7nem", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "In einander sanft verlieren.", "tokens": ["In", "ein\u00b7an\u00b7der", "sanft", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ihrer Augen Reitz zu treffen,", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Reitz", "zu", "tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Male sie wie reges Feuer,", "tokens": ["Ma\u00b7le", "sie", "wie", "re\u00b7ges", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.20": {"text": "Und auch blau, wie Pallasaugen,", "tokens": ["Und", "auch", "blau", ",", "wie", "Pal\u00b7las\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und auch z\u00e4rtlich, wie Citherens.", "tokens": ["Und", "auch", "z\u00e4rt\u00b7lich", ",", "wie", "Cit\u00b7he\u00b7rens", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Mische Milch, zu iungen Rosen,", "tokens": ["Mi\u00b7sche", "Milch", ",", "zu", "i\u00b7un\u00b7gen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Wann du Nas und Wangen malest.", "tokens": ["Wann", "du", "Nas", "und", "Wan\u00b7gen", "ma\u00b7lest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Gib ihr Lippen, wie der Suada,", "tokens": ["Gib", "ihr", "Lip\u00b7pen", ",", "wie", "der", "Sua\u00b7da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Die den Mund zum K\u00fcssen laden.", "tokens": ["Die", "den", "Mund", "zum", "K\u00fcs\u00b7sen", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Um das sanfte Kinn der Sch\u00f6nen,", "tokens": ["Um", "das", "sanf\u00b7te", "Kinn", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und um ihren Hals, wie Marmor,", "tokens": ["Und", "um", "ih\u00b7ren", "Hals", ",", "wie", "Mar\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "La\u00df die Huldg\u00f6ttinnen fliegen.", "tokens": ["La\u00df", "die", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Kleide sie nunmehr in Purpur.", "tokens": ["Klei\u00b7de", "sie", "nun\u00b7mehr", "in", "Pur\u00b7pur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.30": {"text": "Aber la\u00df vom zarten Leibe", "tokens": ["A\u00b7ber", "la\u00df", "vom", "zar\u00b7ten", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Etwas wenigs unverh\u00fcllet,", "tokens": ["Et\u00b7was", "we\u00b7nigs", "un\u00b7ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Das verh\u00fcllte zu verraten.", "tokens": ["Das", "ver\u00b7h\u00fcll\u00b7te", "zu", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Geh itzt hin. Dis ist die Sch\u00f6ne.", "tokens": ["Geh", "itzt", "hin", ".", "Dis", "ist", "die", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.34": {"text": "Wirst du Bild nicht auch bald reden?\u00ab", "tokens": ["Wirst", "du", "Bild", "nicht", "auch", "bald", "re\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Rede wie das Urbild redet,", "tokens": ["Re\u00b7de", "wie", "das", "Ur\u00b7bild", "re\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wann es dem Geliebten schmeichelt,", "tokens": ["Wann", "es", "dem", "Ge\u00b7lieb\u00b7ten", "schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Holde, zarte Schmeichelworte;", "tokens": ["Hol\u00b7de", ",", "zar\u00b7te", "Schmei\u00b7chel\u00b7wor\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Rede was es k\u00fcnftig redet,", "tokens": ["Re\u00b7de", "was", "es", "k\u00fcnf\u00b7tig", "re\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Wann er es allein nur h\u00f6ret,", "tokens": ["Wann", "er", "es", "al\u00b7lein", "nur", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Und der Kleine, mit dem Bogen,", "tokens": ["Und", "der", "Klei\u00b7ne", ",", "mit", "dem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Welcher der vertrauten Mutter", "tokens": ["Wel\u00b7cher", "der", "ver\u00b7trau\u00b7ten", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Alles l\u00e4chelnd wieder saget.", "tokens": ["Al\u00b7les", "l\u00e4\u00b7chelnd", "wie\u00b7der", "sa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "O! wie sch\u00f6n wird es sich h\u00f6ren!", "tokens": ["O", "!", "wie", "sch\u00f6n", "wird", "es", "sich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.44": {"text": "Wolt ihrs auch, ihr Sch\u00f6nen, h\u00f6ren?", "tokens": ["Wolt", "ihrs", "auch", ",", "ihr", "Sch\u00f6\u00b7nen", ",", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ja, nun hab ich sie gesehen.", "tokens": ["Ja", ",", "nun", "hab", "ich", "sie", "ge\u00b7se\u00b7hen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "O wie reitzte sie, die Sch\u00f6ne!", "tokens": ["O", "wie", "reitz\u00b7te", "sie", ",", "die", "Sch\u00f6\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VVFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche kluge Schmeichelworte!", "tokens": ["Wel\u00b7che", "klu\u00b7ge", "Schmei\u00b7chel\u00b7wor\u00b7te", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Welche sanfte Venusminen!", "tokens": ["Wel\u00b7che", "sanf\u00b7te", "Ve\u00b7nus\u00b7mi\u00b7nen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Welche holde Rosenwangen!", "tokens": ["Wel\u00b7che", "hol\u00b7de", "Ro\u00b7sen\u00b7wan\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tausend sch\u00f6ne Liebesg\u00f6tter", "tokens": ["Tau\u00b7send", "sch\u00f6\u00b7ne", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Schw\u00e4rmten um die sch\u00f6nen Glieder.", "tokens": ["Schw\u00e4rm\u00b7ten", "um", "die", "sch\u00f6\u00b7nen", "Glie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drei\u00dfig schw\u00e4rmten um die Wangen,", "tokens": ["Drei\u00b7\u00dfig", "schw\u00e4rm\u00b7ten", "um", "die", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Zwanzig scherzten mit den Lokken;", "tokens": ["Zwan\u00b7zig", "scherz\u00b7ten", "mit", "den", "Lok\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Aber, wo die meisten schw\u00e4rmten,", "tokens": ["A\u00b7ber", ",", "wo", "die", "meis\u00b7ten", "schw\u00e4rm\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo die meisten schw\u00e4rmen werden,", "tokens": ["Wo", "die", "meis\u00b7ten", "schw\u00e4r\u00b7men", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIS", "VVINF", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Soll der sch\u00f6ne Br\u00e4utgam sagen.", "tokens": ["Soll", "der", "sch\u00f6\u00b7ne", "Br\u00e4ut\u00b7gam", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Br\u00e4utgam, sagst du es dem Dichter,", "tokens": ["Br\u00e4ut\u00b7gam", ",", "sagst", "du", "es", "dem", "Dich\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "O so soll er nebst der Sch\u00f6nen,", "tokens": ["O", "so", "soll", "er", "nebst", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VMFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Alle Liebesg\u00f6tter malen.", "tokens": ["Al\u00b7le", "Lie\u00b7bes\u00b7g\u00f6t\u00b7ter", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Itzund soll er nur die Sch\u00f6ne", "tokens": ["It\u00b7zund", "soll", "er", "nur", "die", "Sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Pesnen oder Harpern malen.", "tokens": ["Pes\u00b7nen", "o\u00b7der", "Har\u00b7pern", "ma\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbauf! Vortreflichster der Maler!", "tokens": ["\u00bb", "auf", "!", "Vor\u00b7tre\u00b7flichs\u00b7ter", "der", "Ma\u00b7ler", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$.", "NN", "ART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Auf, und schildre, Preis der Maler!", "tokens": ["Auf", ",", "und", "schild\u00b7re", ",", "Preis", "der", "Ma\u00b7ler", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KON", "VVFIN", "$,", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Meister in der Kunst der Rhoder,", "tokens": ["Meis\u00b7ter", "in", "der", "Kunst", "der", "Rho\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Komm, und schildre diese Sch\u00f6ne,", "tokens": ["Komm", ",", "und", "schild\u00b7re", "die\u00b7se", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie ich sie beschreiben werde!", "tokens": ["Wie", "ich", "sie", "be\u00b7schrei\u00b7ben", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Male mir vor allen Dingen,", "tokens": ["Ma\u00b7le", "mir", "vor", "al\u00b7len", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Zarte rabenschwarze Haare,", "tokens": ["Zar\u00b7te", "ra\u00b7ben\u00b7schwar\u00b7ze", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und, wofern es anders m\u00f6glich,", "tokens": ["Und", ",", "wo\u00b7fern", "es", "an\u00b7ders", "m\u00f6g\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ADJD", "$,"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.9": {"text": "Male sie auch lieblich d\u00fcftend.", "tokens": ["Ma\u00b7le", "sie", "auch", "lieb\u00b7lich", "d\u00fcf\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Male zwischen schwarze Lokken,", "tokens": ["Ma\u00b7le", "zwi\u00b7schen", "schwar\u00b7ze", "Lok\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da, wo sich die Wangen schliessen,", "tokens": ["Da", ",", "wo", "sich", "die", "Wan\u00b7gen", "schlies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Eine Stirn von Elfenbeine.", "tokens": ["Ei\u00b7ne", "Stirn", "von", "El\u00b7fen\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "La\u00df sich nicht die schwarzen Bogen,", "tokens": ["La\u00df", "sich", "nicht", "die", "schwar\u00b7zen", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PRF", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Die sich um die Augen kr\u00fcmmen,", "tokens": ["Die", "sich", "um", "die", "Au\u00b7gen", "kr\u00fcm\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "G\u00e4nzlich trennen, noch vermischen;", "tokens": ["G\u00e4nz\u00b7lich", "tren\u00b7nen", ",", "noch", "ver\u00b7mi\u00b7schen", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sondern, wie bei meinem M\u00e4dchen,", "tokens": ["Son\u00b7dern", ",", "wie", "bei", "mei\u00b7nem", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "In einander sanft verlieren.", "tokens": ["In", "ein\u00b7an\u00b7der", "sanft", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Ihrer Augen Reitz zu treffen,", "tokens": ["Ih\u00b7rer", "Au\u00b7gen", "Reitz", "zu", "tref\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Male sie wie reges Feuer,", "tokens": ["Ma\u00b7le", "sie", "wie", "re\u00b7ges", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KOKOM", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.20": {"text": "Und auch blau, wie Pallasaugen,", "tokens": ["Und", "auch", "blau", ",", "wie", "Pal\u00b7las\u00b7au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Und auch z\u00e4rtlich, wie Citherens.", "tokens": ["Und", "auch", "z\u00e4rt\u00b7lich", ",", "wie", "Cit\u00b7he\u00b7rens", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,", "PWAV", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.22": {"text": "Mische Milch, zu iungen Rosen,", "tokens": ["Mi\u00b7sche", "Milch", ",", "zu", "i\u00b7un\u00b7gen", "Ro\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Wann du Nas und Wangen malest.", "tokens": ["Wann", "du", "Nas", "und", "Wan\u00b7gen", "ma\u00b7lest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Gib ihr Lippen, wie der Suada,", "tokens": ["Gib", "ihr", "Lip\u00b7pen", ",", "wie", "der", "Sua\u00b7da", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "$,", "PWAV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Die den Mund zum K\u00fcssen laden.", "tokens": ["Die", "den", "Mund", "zum", "K\u00fcs\u00b7sen", "la\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Um das sanfte Kinn der Sch\u00f6nen,", "tokens": ["Um", "das", "sanf\u00b7te", "Kinn", "der", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Und um ihren Hals, wie Marmor,", "tokens": ["Und", "um", "ih\u00b7ren", "Hals", ",", "wie", "Mar\u00b7mor", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "$,", "PWAV", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "La\u00df die Huldg\u00f6ttinnen fliegen.", "tokens": ["La\u00df", "die", "Huld\u00b7g\u00f6t\u00b7tin\u00b7nen", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Kleide sie nunmehr in Purpur.", "tokens": ["Klei\u00b7de", "sie", "nun\u00b7mehr", "in", "Pur\u00b7pur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+--++-+-", "measure": "dactylic.init"}, "line.30": {"text": "Aber la\u00df vom zarten Leibe", "tokens": ["A\u00b7ber", "la\u00df", "vom", "zar\u00b7ten", "Lei\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "Etwas wenigs unverh\u00fcllet,", "tokens": ["Et\u00b7was", "we\u00b7nigs", "un\u00b7ver\u00b7h\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Das verh\u00fcllte zu verraten.", "tokens": ["Das", "ver\u00b7h\u00fcll\u00b7te", "zu", "ver\u00b7ra\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Geh itzt hin. Dis ist die Sch\u00f6ne.", "tokens": ["Geh", "itzt", "hin", ".", "Dis", "ist", "die", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PTKVZ", "$.", "PDS", "VAFIN", "ART", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.34": {"text": "Wirst du Bild nicht auch bald reden?\u00ab", "tokens": ["Wirst", "du", "Bild", "nicht", "auch", "bald", "re\u00b7den", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "NN", "PTKNEG", "ADV", "ADV", "VVINF", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Rede wie das Urbild redet,", "tokens": ["Re\u00b7de", "wie", "das", "Ur\u00b7bild", "re\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wann es dem Geliebten schmeichelt,", "tokens": ["Wann", "es", "dem", "Ge\u00b7lieb\u00b7ten", "schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Holde, zarte Schmeichelworte;", "tokens": ["Hol\u00b7de", ",", "zar\u00b7te", "Schmei\u00b7chel\u00b7wor\u00b7te", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Rede was es k\u00fcnftig redet,", "tokens": ["Re\u00b7de", "was", "es", "k\u00fcnf\u00b7tig", "re\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Wann er es allein nur h\u00f6ret,", "tokens": ["Wann", "er", "es", "al\u00b7lein", "nur", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Und der Kleine, mit dem Bogen,", "tokens": ["Und", "der", "Klei\u00b7ne", ",", "mit", "dem", "Bo\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Welcher der vertrauten Mutter", "tokens": ["Wel\u00b7cher", "der", "ver\u00b7trau\u00b7ten", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.42": {"text": "Alles l\u00e4chelnd wieder saget.", "tokens": ["Al\u00b7les", "l\u00e4\u00b7chelnd", "wie\u00b7der", "sa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "O! wie sch\u00f6n wird es sich h\u00f6ren!", "tokens": ["O", "!", "wie", "sch\u00f6n", "wird", "es", "sich", "h\u00f6\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ADJD", "VAFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.44": {"text": "Wolt ihrs auch, ihr Sch\u00f6nen, h\u00f6ren?", "tokens": ["Wolt", "ihrs", "auch", ",", "ihr", "Sch\u00f6\u00b7nen", ",", "h\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}