{"dta.poem.21450": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Nie wieder \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Die Stra\u00dfen d\u00e4mmern so tief verschneit,", "tokens": ["Die", "Stra\u00b7\u00dfen", "d\u00e4m\u00b7mern", "so", "tief", "ver\u00b7schneit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Wie ist der Sommer so weit, so weit!", "tokens": ["Wie", "ist", "der", "Som\u00b7mer", "so", "weit", ",", "so", "weit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "J\u00fcngst tr\u00e4umte die Welt des Fr\u00fchlings Traum:", "tokens": ["J\u00fcngst", "tr\u00e4um\u00b7te", "die", "Welt", "des", "Fr\u00fch\u00b7lings", "Traum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Vieltausend Knospen trug jeder Baum.", "tokens": ["Viel\u00b7tau\u00b7send", "Knos\u00b7pen", "trug", "je\u00b7der", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Nun pl\u00f6tzlich all\u2019 die Keime erstickt,", "tokens": ["Nun", "pl\u00f6tz\u00b7lich", "all'", "die", "Kei\u00b7me", "er\u00b7stickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PIS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Vom Frosteshauch die Bl\u00fcthen geknickt!", "tokens": ["Vom", "Fros\u00b7tes\u00b7hauch", "die", "Bl\u00fc\u00b7then", "ge\u00b7knickt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Unnennbar traurig die Seele ist,", "tokens": ["Un\u00b7nenn\u00b7bar", "trau\u00b7rig", "die", "See\u00b7le", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Der sonnigen Tage sie nicht vergi\u00dft.", "tokens": ["Der", "son\u00b7ni\u00b7gen", "Ta\u00b7ge", "sie", "nicht", "ver\u00b7gi\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Nun findet sie nirgends der Liebe Trost,", "tokens": ["Nun", "fin\u00b7det", "sie", "nir\u00b7gends", "der", "Lie\u00b7be", "Trost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Sie ahnt: nie wieder ein West sie umkost.", "tokens": ["Sie", "ahnt", ":", "nie", "wie\u00b7der", "ein", "West", "sie", "um\u00b7kost", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Nie wieder labt sie der Blumen Duft,", "tokens": ["Nie", "wie\u00b7der", "labt", "sie", "der", "Blu\u00b7men", "Duft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Ihr bettet der Schnee die k\u00fchle Gruft,", "tokens": ["Ihr", "bet\u00b7tet", "der", "Schnee", "die", "k\u00fch\u00b7le", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Sterben mu\u00df sie tief-einsamen Tod \u2014", "tokens": ["Ster\u00b7ben", "mu\u00df", "sie", "tie\u00b7fein\u00b7sa\u00b7men", "Tod"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.14": {"text": "Nie wieder gr\u00fc\u00dft sie das Morgenroth.", "tokens": ["Nie", "wie\u00b7der", "gr\u00fc\u00dft", "sie", "das", "Mor\u00b7gen\u00b7roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}