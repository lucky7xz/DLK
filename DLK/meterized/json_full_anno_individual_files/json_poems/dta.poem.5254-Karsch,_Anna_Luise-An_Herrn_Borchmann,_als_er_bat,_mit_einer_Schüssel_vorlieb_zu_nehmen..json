{"dta.poem.5254": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Borchmann,  \n als er bat, mit einer Sch\u00fcssel vorlieb zu  \n nehmen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "O Borchmann, sprich! wann essen", "tokens": ["O", "Borch\u00b7mann", ",", "sprich", "!", "wann", "es\u00b7sen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NE", "NE", "$,", "ADJD", "$.", "PWAV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Menschen mehr als k\u00f6niglich?", "tokens": ["Die", "Men\u00b7schen", "mehr", "als", "k\u00f6\u00b7nig\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KOKOM", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn sie der ganzen Welt vergessen", "tokens": ["Wenn", "sie", "der", "gan\u00b7zen", "Welt", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und so vergn\u00fcgt, wie Gleim und du, und ich", "tokens": ["Und", "so", "ver\u00b7gn\u00fcgt", ",", "wie", "Gleim", "und", "du", ",", "und", "ich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADV", "VVPP", "$,", "PWAV", "NN", "KON", "PPER", "$,", "KON", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mit einer Sch\u00fcssel sind, und niemand f\u00fcrchten d\u00fcrfen,", "tokens": ["Mit", "ei\u00b7ner", "Sch\u00fcs\u00b7sel", "sind", ",", "und", "nie\u00b7mand", "f\u00fcrch\u00b7ten", "d\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "$,", "KON", "PIS", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und ihren Wein mit einer Zunge schl\u00fcrfen,", "tokens": ["Und", "ih\u00b7ren", "Wein", "mit", "ei\u00b7ner", "Zun\u00b7ge", "schl\u00fcr\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Die frey und unbeurtheilt spricht;", "tokens": ["Die", "frey", "und", "un\u00b7be\u00b7urt\u00b7heilt", "spricht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "O welch ein Mahl! der K\u00f6nig hat es nicht!", "tokens": ["O", "welch", "ein", "Mahl", "!", "der", "K\u00f6\u00b7nig", "hat", "es", "nicht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}