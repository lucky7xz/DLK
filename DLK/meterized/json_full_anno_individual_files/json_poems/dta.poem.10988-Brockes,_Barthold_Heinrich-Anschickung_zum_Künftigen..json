{"dta.poem.10988": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anschickung zum K\u00fcnftigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ja wohl mehr, als gl\u00e4ublich,", "tokens": ["Es", "ist", "ja", "wohl", "mehr", ",", "als", "gl\u00e4ub\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$,", "KOUS", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und vermuthlich unausbleiblich,", "tokens": ["Und", "ver\u00b7muth\u00b7lich", "un\u00b7aus\u00b7bleib\u00b7lich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df wir uns, nach dieser Erden,", "tokens": ["Da\u00df", "wir", "uns", ",", "nach", "die\u00b7ser", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsers  Thuns erinnern werden.", "tokens": ["Un\u00b7sers", "Thuns", "e\u00b7rin\u00b7nern", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Will man sich denn nicht bestreben,", "tokens": ["Will", "man", "sich", "denn", "nicht", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Hier auf Erden so zu leben,", "tokens": ["Hier", "auf", "Er\u00b7den", "so", "zu", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Da\u00df man sich, in Gottes Werken,", "tokens": ["Da\u00df", "man", "sich", ",", "in", "Got\u00b7tes", "Wer\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "$,", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Seine Liebe zu bemerken,", "tokens": ["Sei\u00b7ne", "Lie\u00b7be", "zu", "be\u00b7mer\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und an seinen vielen Gaben,", "tokens": ["Und", "an", "sei\u00b7nen", "vie\u00b7len", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Wenigstens an jedem Tag,", "tokens": ["We\u00b7nigs\u00b7tens", "an", "je\u00b7dem", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Einmal sich erfreut zu haben,", "tokens": ["Ein\u00b7mal", "sich", "er\u00b7freut", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PRF", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dort erinnern kann und mag?", "tokens": ["Dort", "e\u00b7rin\u00b7nern", "kann", "und", "mag", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "KON", "VMFIN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}