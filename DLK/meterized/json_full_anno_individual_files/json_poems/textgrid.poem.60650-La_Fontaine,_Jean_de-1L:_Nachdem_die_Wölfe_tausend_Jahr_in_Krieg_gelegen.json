 "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "ADJD", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und, ganz nach Wolfsnatur, l\u00fcstern auf blutiges Morden,", "tokens": ["Und", ",", "ganz", "nach", "Wolfs\u00b7na\u00b7tur", ",", "l\u00fcs\u00b7tern", "auf", "blu\u00b7ti\u00b7ges", "Mor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Und als die Sch\u00e4fer einmal fern der Herde waren,", "tokens": ["Und", "als", "die", "Sch\u00e4\u00b7fer", "ein\u00b7mal", "fern", "der", "Her\u00b7de", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Benutzten sie die Zeit, die H\u00e4lfte aller Schafe", "tokens": ["Be\u00b7nutz\u00b7ten", "sie", "die", "Zeit", ",", "die", "H\u00e4lf\u00b7te", "al\u00b7ler", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zu w\u00fcrgen und hinwegzuschleppen in den Wald.", "tokens": ["Zu", "w\u00fcr\u00b7gen", "und", "hin\u00b7weg\u00b7zu\u00b7schlep\u00b7pen", "in", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie hatten ihren hinterlistigen Plan", "tokens": ["Sie", "hat\u00b7ten", "ih\u00