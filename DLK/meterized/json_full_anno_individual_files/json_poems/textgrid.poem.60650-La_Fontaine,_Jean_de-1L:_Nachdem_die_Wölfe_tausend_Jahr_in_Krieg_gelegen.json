{"textgrid.poem.60650": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Nachdem die W\u00f6lfe tausend Jahr in Krieg gelegen", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem die W\u00f6lfe tausend Jahr in Krieg gelegen", "tokens": ["Nach\u00b7dem", "die", "W\u00f6l\u00b7fe", "tau\u00b7send", "Jahr", "in", "Krieg", "ge\u00b7le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "CARD", "NN", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Schaf und Hirt, hat Frieden man geschlossen.", "tokens": ["Mit", "Schaf", "und", "Hirt", ",", "hat", "Frie\u00b7den", "man", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,", "VAFIN", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00fcr beide Teile war's ein wahrer Segen,", "tokens": ["F\u00fcr", "bei\u00b7de", "Tei\u00b7le", "wa\u00b7r's", "ein", "wah\u00b7rer", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.4": {"text": "Den sie fortan in stillem Gl\u00fcck genossen;", "tokens": ["Den", "sie", "for\u00b7tan", "in", "stil\u00b7lem", "Gl\u00fcck", "ge\u00b7nos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn wenn die W\u00f6lfe sich vordem auch von verirrten", "tokens": ["Denn", "wenn", "die", "W\u00f6l\u00b7fe", "sich", "vor\u00b7dem", "auch", "von", "ver\u00b7irr\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "ADV", "ADV", "APPR", "VVFIN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wolltieren oft ern\u00e4hrten, machten doch die Hirten", "tokens": ["Woll\u00b7tie\u00b7ren", "oft", "er\u00b7n\u00e4hr\u00b7ten", ",", "mach\u00b7ten", "doch", "die", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVINF", "$,", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Aus frischem Wolfspelz oft sich ein Gewand,", "tokens": ["Aus", "fri\u00b7schem", "Wolf\u00b7spelz", "oft", "sich", "ein", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Kein unbedrohter Schritt war m\u00f6glich rings im Land,", "tokens": ["Kein", "un\u00b7be\u00b7droh\u00b7ter", "Schritt", "war", "m\u00f6g\u00b7lich", "rings", "im", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Aus steter Todesfurcht bestand", "tokens": ["Aus", "ste\u00b7ter", "To\u00b7des\u00b7furcht", "be\u00b7stand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der einen wie der andern Leben.", "tokens": ["Der", "ei\u00b7nen", "wie", "der", "an\u00b7dern", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Der Friedenspakt schrieb vor, man solle Geiseln geben:", "tokens": ["Der", "Frie\u00b7dens\u00b7pakt", "schrieb", "vor", ",", "man", "sol\u00b7le", "Gei\u00b7seln", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PIS", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die W\u00f6lfe ihre Brut, die Schafe ihre Hunde.", "tokens": ["Die", "W\u00f6l\u00b7fe", "ih\u00b7re", "Brut", ",", "die", "Scha\u00b7fe", "ih\u00b7re", "Hun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$,", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Das gab Gew\u00e4hr, so glaubte man, dem Friedensbunde.", "tokens": ["Das", "gab", "Ge\u00b7w\u00e4hr", ",", "so", "glaub\u00b7te", "man", ",", "dem", "Frie\u00b7dens\u00b7bun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "NN", "$,", "ADV", "VVFIN", "PIS", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Geregelt ward der Austausch gleich von Kommissaren.", "tokens": ["Ge\u00b7re\u00b7gelt", "ward", "der", "Aus\u00b7tausch", "gleich", "von", "Kom\u00b7mis\u00b7sa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die W\u00f6lflein aber sind allm\u00e4hlich gro\u00df geworden", "tokens": ["Die", "W\u00f6lf\u00b7lein", "a\u00b7ber", "sind", "all\u00b7m\u00e4h\u00b7lich", "gro\u00df", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VAFIN", "ADJD", "ADJD", "VAPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und, ganz nach Wolfsnatur, l\u00fcstern auf blutiges Morden,", "tokens": ["Und", ",", "ganz", "nach", "Wolfs\u00b7na\u00b7tur", ",", "l\u00fcs\u00b7tern", "auf", "blu\u00b7ti\u00b7ges", "Mor\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "NN", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Und als die Sch\u00e4fer einmal fern der Herde waren,", "tokens": ["Und", "als", "die", "Sch\u00e4\u00b7fer", "ein\u00b7mal", "fern", "der", "Her\u00b7de", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "ADJD", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Benutzten sie die Zeit, die H\u00e4lfte aller Schafe", "tokens": ["Be\u00b7nutz\u00b7ten", "sie", "die", "Zeit", ",", "die", "H\u00e4lf\u00b7te", "al\u00b7ler", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "ART", "NN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Zu w\u00fcrgen und hinwegzuschleppen in den Wald.", "tokens": ["Zu", "w\u00fcr\u00b7gen", "und", "hin\u00b7weg\u00b7zu\u00b7schlep\u00b7pen", "in", "den", "Wald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Sie hatten ihren hinterlistigen Plan", "tokens": ["Sie", "hat\u00b7ten", "ih\u00b7ren", "hin\u00b7ter\u00b7lis\u00b7ti\u00b7gen", "Plan"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.21": {"text": "Den alten W\u00f6lfen heimlich kundgetan,", "tokens": ["Den", "al\u00b7ten", "W\u00f6l\u00b7fen", "heim\u00b7lich", "kund\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Und diese mordeten die ahnungslos im Schlafe", "tokens": ["Und", "die\u00b7se", "mor\u00b7de\u00b7ten", "die", "ah\u00b7nungs\u00b7los", "im", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PDS", "VMFIN", "ART", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ruhenden Hunde alsobald,", "tokens": ["Ru\u00b7hen\u00b7den", "Hun\u00b7de", "al\u00b7so\u00b7bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.24": {"text": "Und das Gemetzel ging so schnell vonstatten,", "tokens": ["Und", "das", "Ge\u00b7met\u00b7zel", "ging", "so", "schnell", "von\u00b7stat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.25": {"text": "Da\u00df sie nicht Zeit zu Wehr und Hilferufen hatten.", "tokens": ["Da\u00df", "sie", "nicht", "Zeit", "zu", "Wehr", "und", "Hil\u00b7fe\u00b7ru\u00b7fen", "hat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "NN", "APPR", "NN", "KON", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Hieraus ergibt sich nun der Schlu\u00df,", "tokens": ["Hier\u00b7aus", "er\u00b7gibt", "sich", "nun", "der", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man die B\u00f6sen fortgesetzt bekriegen mu\u00df.", "tokens": ["Da\u00df", "man", "die", "B\u00f6\u00b7sen", "fort\u00b7ge\u00b7setzt", "be\u00b7krie\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Friede ist an sich ein segensreicher Hort,", "tokens": ["Der", "Frie\u00b7de", "ist", "an", "sich", "ein", "se\u00b7gens\u00b7rei\u00b7cher", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Doch schafft er Schlimmeres als nur Verdru\u00df,", "tokens": ["Doch", "schafft", "er", "Schlim\u00b7me\u00b7res", "als", "nur", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KOKOM", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn man nicht trauen kann auf Gegners Wort.", "tokens": ["Wenn", "man", "nicht", "trau\u00b7en", "kann", "auf", "Geg\u00b7ners", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "