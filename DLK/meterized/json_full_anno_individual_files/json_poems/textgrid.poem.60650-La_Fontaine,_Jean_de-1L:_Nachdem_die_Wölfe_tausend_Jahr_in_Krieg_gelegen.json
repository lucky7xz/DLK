, "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Hieraus ergibt sich nun der Schlu\u00df,", "tokens": ["Hier\u00b7aus", "er\u00b7gibt", "sich", "nun", "der", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df man die B\u00f6sen fortgesetzt bekriegen mu\u00df.", "tokens": ["Da\u00df", "man", "die", "B\u00f6\u00b7sen", "fort\u00b7ge\u00b7setzt", "be\u00b7krie\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVPP", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der Friede ist an sich ein segensreicher Hort,", "tokens": ["Der", "Frie\u00b7de", "ist", "an", "sich", "ein", "se\u00b7gens\u00b7rei\u00b7cher", "Hort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Doch schafft er Schlimmeres als nur Verdru\u00df,", "tokens": ["Doch", "schafft", "er", "Schlim\u00b7me\u00b7res", "als", "nur", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "KOKOM", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn man nicht trauen kann auf Gegners Wort.", "tokens": ["Wenn", "man", "nicht", "trau\u00b7en", "kann", "auf", "Geg\u00b7ners", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}