{"dta.poem.5340": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das t\u00e4gliche Mond-Licht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie unter so viel tausend Gaben", "tokens": ["Wie", "un\u00b7ter", "so", "viel", "tau\u00b7send", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADV", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auch billig f\u00fcr des Mondes Schein,", "tokens": ["Auch", "bil\u00b7lig", "f\u00fcr", "des", "Mon\u00b7des", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo wir nicht unerk\u00e4nntlich seyn,", "tokens": ["Wo", "wir", "nicht", "un\u00b7er\u00b7k\u00e4nnt\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir GOtt zu dancken Ursach haben,", "tokens": ["Wir", "Gott", "zu", "dan\u00b7cken", "Ur\u00b7sach", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKZU", "VVINF", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als wodurch, wenn der Sonnen-Licht", "tokens": ["Als", "wo\u00b7durch", ",", "wenn", "der", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PWAV", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit seinen Strahlen uns gebricht,", "tokens": ["Mit", "sei\u00b7nen", "Strah\u00b7len", "uns", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wir doch dasselbe wundersch\u00f6n", "tokens": ["Wir", "doch", "das\u00b7sel\u00b7be", "wun\u00b7der\u00b7sch\u00f6n"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jm Wieder-Schein, im Duncklen, sehn;", "tokens": ["Jm", "Wie\u00b7der\u00b7Schein", ",", "im", "Dunck\u00b7len", ",", "sehn", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "APPRART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So find ich, wenn mans recht bedencket,", "tokens": ["So", "find", "ich", ",", "wenn", "mans", "recht", "be\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df GOtt uns durch der Sonnen Glut", "tokens": ["Da\u00df", "Gott", "uns", "durch", "der", "Son\u00b7nen", "Glut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Noch ein nicht minder herrlich Gut", "tokens": ["Noch", "ein", "nicht", "min\u00b7der", "herr\u00b7lich", "Gut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "PTKNEG", "ADV", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und einen Mond-Schein t\u00e4glich schencket.", "tokens": ["Und", "ei\u00b7nen", "Mon\u00b7dSchein", "t\u00e4g\u00b7lich", "schen\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie an den Mond die Strahlen fallen", "tokens": ["Wie", "an", "den", "Mond", "die", "Strah\u00b7len", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dadurch, da\u00df sie r\u00fcckwerts prallen,", "tokens": ["Und", "da\u00b7durch", ",", "da\u00df", "sie", "r\u00fcck\u00b7werts", "pral\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Glantz, Schimmer, Klarheit, Licht und Schein,", "tokens": ["Glantz", ",", "Schim\u00b7mer", ",", "Klar\u00b7heit", ",", "Licht", "und", "Schein", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die sonst nicht w\u00fcrden sichtbar seyn,", "tokens": ["Die", "sonst", "nicht", "w\u00fcr\u00b7den", "sicht\u00b7bar", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PTKNEG", "VAFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Uns in der Dunckelheit gew\u00e4hren;", "tokens": ["Uns", "in", "der", "Dun\u00b7ckel\u00b7heit", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So fallen Strahlen an den Duft", "tokens": ["So", "fal\u00b7len", "Strah\u00b7len", "an", "den", "Duft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der unsre Welt verh\u00fcll\u2019nden Luft,", "tokens": ["Der", "uns\u00b7re", "Welt", "ver\u00b7h\u00fcll'n\u00b7den", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wodurch sie uns ein Licht geb\u00e4hren,", "tokens": ["Wo\u00b7durch", "sie", "uns", "ein", "Licht", "ge\u00b7b\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Da\u00df man zwar alle Tage sieht,", "tokens": ["Da\u00df", "man", "zwar", "al\u00b7le", "Ta\u00b7ge", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jedoch sich leider nicht bem\u00fcht", "tokens": ["Je\u00b7doch", "sich", "lei\u00b7der", "nicht", "be\u00b7m\u00fcht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PRF", "ADV", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die\u00df grosse Wunder zu betrachten,", "tokens": ["Die\u00df", "gros\u00b7se", "Wun\u00b7der", "zu", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und es des Denckens wehrt zu achten.", "tokens": ["Und", "es", "des", "Den\u00b7ckens", "wehrt", "zu", "ach\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Zweymahl an einem Tag\u2019 allein", "tokens": ["Zwey\u00b7mahl", "an", "ei\u00b7nem", "Tag'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vertritt die Luft des Monden Stelle,", "tokens": ["Ver\u00b7tritt", "die", "Luft", "des", "Mon\u00b7den", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Und machet, da der Sonnen Licht", "tokens": ["Und", "ma\u00b7chet", ",", "da", "der", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Der Erden noch und schon gebricht,", "tokens": ["Der", "Er\u00b7den", "noch", "und", "schon", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Dennoch den Kreis der Erden helle.", "tokens": ["Den\u00b7noch", "den", "Kreis", "der", "Er\u00b7den", "hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "ART", "NN", "ADJA", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Wenn nicht die Luft die Welt bedeckte,", "tokens": ["Wenn", "nicht", "die", "Luft", "die", "Welt", "be\u00b7deck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "W\u00e4r unvermeidlich alsobald,", "tokens": ["W\u00e4r", "un\u00b7ver\u00b7meid\u00b7lich", "al\u00b7so\u00b7bald", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bald die Sonne sich versteckte,", "tokens": ["So", "bald", "die", "Son\u00b7ne", "sich", "ver\u00b7steck\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Welt der Schatten Auffenthalt.", "tokens": ["Die", "Welt", "der", "Schat\u00b7ten", "Auf\u00b7fent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ja so pech-schwartze Finsternissen", "tokens": ["Ja", "so", "pech\u00b7schwart\u00b7ze", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKANT", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "W\u00fcrd\u2019 alles pl\u00f6tzlich sincken m\u00fcssen,", "tokens": ["W\u00fcrd'", "al\u00b7les", "pl\u00f6tz\u00b7lich", "sin\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df fr\u00fch der Sonnen Blick und Pracht,", "tokens": ["Da\u00df", "fr\u00fch", "der", "Son\u00b7nen", "Blick", "und", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Durch ein so schnell und strenges Funckeln;", "tokens": ["Durch", "ein", "so", "schnell", "und", "stren\u00b7ges", "Fun\u00b7ckeln", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Des Abends die stock-finstre Nacht,", "tokens": ["Des", "A\u00b7bends", "die", "stock\u00b7fin\u00b7stre", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In ja so schnell und strengem Dunckeln,", "tokens": ["In", "ja", "so", "schnell", "und", "stren\u00b7gem", "Dun\u00b7ckeln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADV", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Das zarte Wesen unsrer Augen", "tokens": ["Das", "zar\u00b7te", "We\u00b7sen", "uns\u00b7rer", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht w\u00fcrde zu ertragen taugen.", "tokens": ["Nicht", "w\u00fcr\u00b7de", "zu", "er\u00b7tra\u00b7gen", "tau\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Es w\u00fcrde was da lebt auf Erden", "tokens": ["Es", "w\u00fcr\u00b7de", "was", "da", "lebt", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PWS", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Gewi\u00df dadurch geblendet werden.", "tokens": ["Ge\u00b7wi\u00df", "da\u00b7durch", "ge\u00b7blen\u00b7det", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PAV", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Ist es denn nicht der M\u00fche wehrt,", "tokens": ["Ist", "es", "denn", "nicht", "der", "M\u00fc\u00b7he", "wehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dis Wunder wol zu \u00fcberlegen,", "tokens": ["Dis", "Wun\u00b7der", "wol", "zu", "\u00fc\u00b7berl\u00b7e\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Der D\u00e4mmrung Nutzen zu erwegen,", "tokens": ["Der", "D\u00e4mm\u00b7rung", "Nut\u00b7zen", "zu", "er\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Wodurch uns GOtt das Licht vermehrt;", "tokens": ["Wo\u00b7durch", "uns", "Gott", "das", "Licht", "ver\u00b7mehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Indem man fr\u00fch so wol, als spat,", "tokens": ["In\u00b7dem", "man", "fr\u00fch", "so", "wol", ",", "als", "spat", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "ADV", "$,", "KOUS", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Das Licht viel ehr und l\u00e4nger hat.", "tokens": ["Das", "Licht", "viel", "ehr", "und", "l\u00e4n\u00b7ger", "hat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Man wird daher mit Rechte k\u00f6nnen", "tokens": ["Man", "wird", "da\u00b7her", "mit", "Rech\u00b7te", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PAV", "APPR", "NN", "VMFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Den Luft-Kreis einen Mond fast nennen,", "tokens": ["Den", "Luft\u00b7Kreis", "ei\u00b7nen", "Mond", "fast", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Da er die Sonne, eh sie steiget,", "tokens": ["Da", "er", "die", "Son\u00b7ne", ",", "eh", "sie", "stei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Auch wenn sie sich bereits geneiget,", "tokens": ["Auch", "wenn", "sie", "sich", "be\u00b7reits", "ge\u00b7nei\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Nicht anders, wie der Mond, uns zeiget.", "tokens": ["Nicht", "an\u00b7ders", ",", "wie", "der", "Mond", ",", "uns", "zei\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$,", "PWAV", "ART", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "li