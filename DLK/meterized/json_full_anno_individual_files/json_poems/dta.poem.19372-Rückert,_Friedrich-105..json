{"dta.poem.19372": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "105.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hat doch des Kindes Fu\u00df das Gehn gelernt durch Fallen,", "tokens": ["Hat", "doch", "des", "Kin\u00b7des", "Fu\u00df", "das", "Gehn", "ge\u00b7lernt", "durch", "Fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Zunge auch das Reden nur durch Lallen.", "tokens": ["Und", "sei\u00b7ne", "Zun\u00b7ge", "auch", "das", "Re\u00b7den", "nur", "durch", "Lal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich selber falle noch, wenn ich will zu dir gehn,", "tokens": ["Ich", "sel\u00b7ber", "fal\u00b7le", "noch", ",", "wenn", "ich", "will", "zu", "dir", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "$,", "KOUS", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Herr, ich lalle noch, soll ich dir Rede stehn!", "tokens": ["O", "Herr", ",", "ich", "lal\u00b7le", "noch", ",", "soll", "ich", "dir", "Re\u00b7de", "stehn", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "ADV", "$,", "VMFIN", "PPER", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich bin vor dir ein Kind, und wei\u00df, an Einsicht blind,", "tokens": ["Ich", "bin", "vor", "dir", "ein", "Kind", ",", "und", "wei\u00df", ",", "an", "Ein\u00b7sicht", "blind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "$,", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur dis aus mir, wie lieb mir meine Kinder sind.", "tokens": ["Nur", "dis", "aus", "mir", ",", "wie", "lieb", "mir", "mei\u00b7ne", "Kin\u00b7der", "sind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "APPR", "PPER", "$,", "PWAV", "ADJD", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Kinder wissen nicht, wie sie der Vater liebt;", "tokens": ["Die", "Kin\u00b7der", "wis\u00b7sen", "nicht", ",", "wie", "sie", "der", "Va\u00b7ter", "liebt", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das wei\u00df nur der, dem selbst der Vater Kinder giebt.", "tokens": ["Das", "wei\u00df", "nur", "der", ",", "dem", "selbst", "der", "Va\u00b7ter", "Kin\u00b7der", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "$,", "PRELS", "ADV", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sie selber wissen nicht, wie lieb mir sei ihr Lallen,", "tokens": ["Sie", "sel\u00b7ber", "wis\u00b7sen", "nicht", ",", "wie", "lieb", "mir", "sei", "ihr", "Lal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKNEG", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und da\u00df nicht um die Welt ich eines lie\u00dfe fallen.", "tokens": ["Und", "da\u00df", "nicht", "um", "die", "Welt", "ich", "ei\u00b7nes", "lie\u00b7\u00dfe", "fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PTKNEG", "APPR", "ART", "NN", "PPER", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}