{"textgrid.poem.57870": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Vor\u00fcbung", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es zeigt der Lindener B\u00fcrgerverein", "tokens": ["Es", "zeigt", "der", "Lin\u00b7de\u00b7ner", "B\u00fcr\u00b7ger\u00b7ver\u00b7ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sich riesig neuerungss\u00fcchtig;", "tokens": ["Sich", "rie\u00b7sig", "neu\u00b7e\u00b7rungs\u00b7s\u00fcch\u00b7tig", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ADJD", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Jetzt schuf eine Turnerriege er \u2013", "tokens": ["Jetzt", "schuf", "ei\u00b7ne", "Tur\u00b7ner\u00b7rie\u00b7ge", "er", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PPER", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die Sache ist \u00e4u\u00dferst wichtig.", "tokens": ["Die", "Sa\u00b7che", "ist", "\u00e4u\u00b7\u00dferst", "wich\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Es kommt dem Lindener Magistrat", "tokens": ["Es", "kommt", "dem", "Lin\u00b7de\u00b7ner", "Ma\u00b7gist\u00b7rat"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Die Gr\u00fcndung sehr gelegen,", "tokens": ["Die", "Gr\u00fcn\u00b7dung", "sehr", "ge\u00b7le\u00b7gen", ","], 