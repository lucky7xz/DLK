{"dta.poem.11833": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn dich am fr\u00fchen Tag ein frommer Vorsatz hebet,", "tokens": ["Wenn", "dich", "am", "fr\u00fc\u00b7hen", "Tag", "ein", "from\u00b7mer", "Vor\u00b7satz", "he\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein froher Herzenschlag dem dankt durch den er lebet;", "tokens": ["Dein", "fro\u00b7her", "Her\u00b7zen\u00b7schlag", "dem", "dankt", "durch", "den", "er", "le\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "VVFIN", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Als k\u00fchler Sommerhut wird dies Gef\u00fchl dir schatten,", "tokens": ["Als", "k\u00fch\u00b7ler", "Som\u00b7mer\u00b7hut", "wird", "dies", "Ge\u00b7f\u00fchl", "dir", "schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VAFIN", "PDS", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und an des Mittags Glut nicht deine Seel' ermatten.", "tokens": ["Und", "an", "des", "Mit\u00b7tags", "Glut", "nicht", "dei\u00b7ne", "Seel'", "er\u00b7mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "PTKNEG", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dann wenn du dir zur Nacht das Zeugnis geben kannst,", "tokens": ["Dann", "wenn", "du", "dir", "zur", "Nacht", "das", "Zeug\u00b7nis", "ge\u00b7ben", "kannst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PPER", "APPRART", "NN", "ART", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df etwas du vollbracht, dir etwas angewannst;", "tokens": ["Da\u00df", "et\u00b7was", "du", "voll\u00b7bracht", ",", "dir", "et\u00b7was", "an\u00b7ge\u00b7wannst", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$,", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "So wird bei Nacht ein Traum der Seele Kraft dir st\u00e4rken,", "tokens": ["So", "wird", "bei", "Nacht", "ein", "Traum", "der", "See\u00b7le", "Kraft", "dir", "st\u00e4r\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "NN", "ART", "NN", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df morgens sie im Raum erwacht zu neuen Werken.", "tokens": ["Da\u00df", "mor\u00b7gens", "sie", "im", "Raum", "er\u00b7wacht", "zu", "neu\u00b7en", "Wer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPER", "APPRART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}