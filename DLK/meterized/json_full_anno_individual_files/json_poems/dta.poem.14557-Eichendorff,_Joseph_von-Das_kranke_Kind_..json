{"dta.poem.14557": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das kranke Kind .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201each Vater, liebe Mutter,", "tokens": ["\u201e", "ach", "Va\u00b7ter", ",", "lie\u00b7be", "Mut\u00b7ter", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Helft mir in meiner Noth! \u2014\u201c", "tokens": ["Helft", "mir", "in", "mei\u00b7ner", "Noth", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du armes Kind! die ruhen", "tokens": ["Du", "ar\u00b7mes", "Kind", "!", "die", "ru\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja unter'm Grase todt.", "tokens": ["Ja", "un\u00b7ter'm", "Gra\u00b7se", "todt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPRART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}