{"dta.poem.13022": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Staats-Kunst wandt nichts ein; fuhr fort: \u201eNur ich kann", "tokens": ["Die", "Staats\u00b7Kunst", "wandt", "nichts", "ein", ";", "fuhr", "fort", ":", "\u201e", "Nur", "ich", "kann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PIS", "PTKVZ", "$.", "VVFIN", "PTKVZ", "$.", "$(", "ADV", "PPER", "VMFIN"], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "wissen,", "tokens": ["wis\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "120\u201dWo man sich zu dem Kampf, zum Angriff soll entschliessen?", "tokens": ["\"", "Wo", "man", "sich", "zu", "dem", "Kampf", ",", "zum", "An\u00b7griff", "soll", "ent\u00b7schlies\u00b7sen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "PRF", "APPR", "ART", "NN", "$,", "APPRART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund diese Kunst hab ich die K\u00f6niginn gelehrt;", "tokens": ["\u201e", "und", "die\u00b7se", "Kunst", "hab", "ich", "die", "K\u00f6\u00b7ni\u00b7ginn", "ge\u00b7lehrt", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDAT", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201edurch diese blieb ihr Thron und Zepter unversehrt.", "tokens": ["\u201e", "durch", "die\u00b7se", "blieb", "ihr", "Thron", "und", "Zep\u00b7ter", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PDS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Gegnerinn erhohlt: \u201eDein Augen-volles Reden", "tokens": ["Die", "Geg\u00b7ne\u00b7rinn", "er\u00b7hohlt", ":", "\u201e", "Dein", "Au\u00b7gen\u00b7vol\u00b7les", "Re\u00b7den"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201emag \u00fcbersichtige, Wiz-lose Sinnen bl\u00f6den,", "tokens": ["\u201e", "mag", "\u00fc\u00b7bers\u00b7ich\u00b7ti\u00b7ge", ",", "Wi\u00b7zlo\u00b7se", "Sin\u00b7nen", "bl\u00f6\u00b7den", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "ADJA", "$,", "ADJA", "NN", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "125\u201dNicht aber mein Gem\u00fcth. Mein Herz vermag so viel,", "tokens": ["\"", "Nicht", "a\u00b7ber", "mein", "Ge\u00b7m\u00fcth", ".", "Mein", "Herz", "ver\u00b7mag", "so", "viel", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "ADV", "PPOSAT", "NN", "$.", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201eals deiner Wissenschaft Verwicklung-volles Spiel. \u201e", "tokens": ["\u201e", "als", "dei\u00b7ner", "Wis\u00b7sen\u00b7schaft", "Ver\u00b7wick\u00b7lung\u00b7vol\u00b7les", "Spiel", ".", "\u201e"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "PPOSAT", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie wie\u00df ein Herz und sprach: \u201eHier kannst du es betrachten,", "tokens": ["Sie", "wie\u00df", "ein", "Herz", "und", "sprach", ":", "\u201e", "Hier", "kannst", "du", "es", "be\u00b7trach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "$.", "$(", "ADV", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eich wei\u00df, man pflegt es mehr, als deine Kunst zu achten.", "tokens": ["\u201e", "ich", "wei\u00df", ",", "man", "pflegt", "es", "mehr", ",", "als", "dei\u00b7ne", "Kunst", "zu", "ach\u00b7ten", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "PIS", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201ewas meine Zunge spricht, das wiederhohlt mein Herz;", "tokens": ["\u201e", "was", "mei\u00b7ne", "Zun\u00b7ge", "spricht", ",", "das", "wie\u00b7der\u00b7hohlt", "mein", "Herz", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PPOSAT", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "130\u201dDein Reden aber ist nur Heicheln, oder Scherz.", "tokens": ["\"", "Dein", "Re\u00b7den", "a\u00b7ber", "ist", "nur", "Hei\u00b7cheln", ",", "o\u00b7der", "Scherz", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "ADV", "VAFIN", "ADV", "NN", "$,", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}