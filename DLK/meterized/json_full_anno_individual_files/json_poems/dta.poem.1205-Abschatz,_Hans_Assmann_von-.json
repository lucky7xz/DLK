{"dta.poem.1205": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du stiller Wald/ du rauhe Felsen-Klufft/", "tokens": ["Du", "stil\u00b7ler", "Wald", "/", "du", "rau\u00b7he", "Fel\u00b7sen\u00b7Klufft", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du helle Bach/ ihr Quellen in den H\u00e4ynen/", "tokens": ["Du", "hel\u00b7le", "Bach", "/", "ihr", "Quel\u00b7len", "in", "den", "H\u00e4y\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPOSAT", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Last eure Schos seyn meiner Sorgen Grufft:", "tokens": ["Last", "eu\u00b7re", "Schos", "seyn", "mei\u00b7ner", "Sor\u00b7gen", "Grufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAINF", "PPOSAT", "NN", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.4": {"text": "Ihr/ denen wissend ist mein Klagen und mein Weinen/", "tokens": ["Ihr", "/", "de\u00b7nen", "wis\u00b7send", "ist", "mein", "Kla\u00b7gen", "und", "mein", "Wei\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "PRELS", "ADJD", "VAFIN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sagt/ ob mir nicht/ wenn ich mu\u00df sperren meinen Mund/", "tokens": ["Sagt", "/", "ob", "mir", "nicht", "/", "wenn", "ich", "mu\u00df", "sper\u00b7ren", "mei\u00b7nen", "Mund", "/"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "PTKNEG", "$(", "KOUS", "PPER", "VMFIN", "VVINF", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Zu seufftzen ist vergunnt?", "tokens": ["Zu", "seufft\u00b7zen", "ist", "ver\u00b7gunnt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VAFIN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach/ Seufftzer geht/ doch sonder laut zu seyn/", "tokens": ["Ach", "/", "Seufft\u00b7zer", "geht", "/", "doch", "son\u00b7der", "laut", "zu", "seyn", "/"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "NN", "VVFIN", "$(", "ADV", "ADV", "ADJD", "PTKZU", "VAINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Weist wie ich mu\u00df mein treues Hertze zwingen/", "tokens": ["Weist", "wie", "ich", "mu\u00df", "mein", "treu\u00b7es", "Hert\u00b7ze", "zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "PPER", "VMFIN", "PPOSAT", "ADJA", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Blast ihrem Ohr in meinem Nahmen ein:", "tokens": ["Blast", "ih\u00b7rem", "Ohr", "in", "mei\u00b7nem", "Nah\u00b7men", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Darff ich dir/ s\u00fcsse Frucht/ kein redend Opffer bringen/", "tokens": ["Darff", "ich", "dir", "/", "s\u00fcs\u00b7se", "Frucht", "/", "kein", "re\u00b7dend", "Opf\u00b7fer", "brin\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "$(", "ADJA", "NN", "$(", "PIAT", "ADJD", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der hei\u00dfre Widerhall schreyt Tag und Nacht f\u00fcr mich/", "tokens": ["Der", "hei\u00df\u00b7re", "Wi\u00b7der\u00b7hall", "schreyt", "Tag", "und", "Nacht", "f\u00fcr", "mich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NN", "KON", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich liebe nichts/ als dich.", "tokens": ["Ich", "lie\u00b7be", "nichts", "/", "als", "dich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$(", "KOUS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}