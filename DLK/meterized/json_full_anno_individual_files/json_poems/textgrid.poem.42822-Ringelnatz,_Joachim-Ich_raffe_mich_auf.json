{"textgrid.poem.42822": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Ich raffe mich auf", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Nachttopf klirrt. Ich bin entschlossen!", "tokens": ["Der", "Nacht\u00b7topf", "klirrt", ".", "Ich", "bin", "ent\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Doornkaat hat mich umgestimmt.", "tokens": ["Der", "Do\u00b7orn\u00b7kaat", "hat", "mich", "um\u00b7ge\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Wenn jetzt auch alles in der Stube schwimmt,", "tokens": ["Wenn", "jetzt", "auch", "al\u00b7les", "in", "der", "Stu\u00b7be", "schwimmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "PIS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist doch noch lang kein Blut vergossen.", "tokens": ["Ist", "doch", "noch", "lang", "kein", "Blut", "ver\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Spiegel kracht. Was will das hei\u00dfen?", "tokens": ["Der", "Spie\u00b7gel", "kracht", ".", "Was", "will", "das", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", 