{"dta.poem.660": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "Gegensatz.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1650", "urn": "urn:nbn:de:kobv:b4-20218-7", "language": ["de:0.99"], "booktitle": "Gryphius, Andreas: Teutsche Reim-Gedichte. Frankfurt (Main), 1650."}, "poem": {"stanza.1": {"line.1": {"text": "Dvrchsucht das weite landt: ", "tokens": ["Dvrch\u00b7sucht", "das", "wei\u00b7te", "landt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Suche Sion in den feldern/ ", "tokens": ["Su\u00b7che", "Si\u00b7on", "in", "den", "fel\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Suche Sion in den W\u00e4ldern/ ", "tokens": ["Su\u00b7che", "Si\u00b7on", "in", "den", "W\u00e4l\u00b7dern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo Menschen nur bekandt: ", "tokens": ["Wo", "Men\u00b7schen", "nur", "be\u00b7kandt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Ob eine Mutter sey die auch jhr eigen Kind/ ", "tokens": ["Ob", "ei\u00b7ne", "Mut\u00b7ter", "sey", "die", "auch", "jhr", "ei\u00b7gen", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ART", "ADV", "PPER", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Aus jhrem hertzen setz/ ob eine schlag in wind/ ", "tokens": ["Aus", "jhrem", "hert\u00b7zen", "setz", "/", "ob", "ei\u00b7ne", "schlag", "in", "wind", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$(", "KOUS", "ART", "NN", "APPR", "NE", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.2": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Das recht das Seel vnd sinn durchdringt ", "tokens": ["Das", "recht", "das", "Seel", "vnd", "sinn", "durch\u00b7dringt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ART", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das die natur selbst vorgeschriben. ", "tokens": ["Das", "die", "na\u00b7tur", "selbst", "vor\u00b7ge\u00b7schri\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "W", "tokens": ["W"], "token_info": ["word"], "pos": ["XY"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Jhr eigen fleisch das sie gebohren/ ", "tokens": ["Ihr", "ei\u00b7gen", "fleisch", "das", "sie", "ge\u00b7boh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "PRELS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Des leibes zarte frucht verlohren? ", "tokens": ["Des", "lei\u00b7bes", "zar\u00b7te", "frucht", "ver\u00b7loh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcrwar der Geist will schier verschwinden: ", "tokens": ["F\u00fcr\u00b7war", "der", "Geist", "will", "schier", "ver\u00b7schwin\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VMFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie zittert/ sie erschrickt/ als f\u00fcr der Todten-grufft. ", "tokens": ["Sie", "zit\u00b7tert", "/", "sie", "er\u00b7schrickt", "/", "als", "f\u00fcr", "der", "Tod\u00b7ten\u00b7grufft", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVPP", "$(", "KOKOM", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Jm fall der kleine Sohn: Ach Mutter/ Mutter/ rufft. ", "tokens": ["Jm", "fall", "der", "klei\u00b7ne", "Sohn", ":", "Ach", "Mut\u00b7ter", "/", "Mut\u00b7ter", "/", "rufft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "$.", "ITJ", "NN", "$(", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}