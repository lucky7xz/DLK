{"textgrid.poem.41628": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bald tauchen fr\u00f6stelnd wir ins kalte Dunkel nieder;", "tokens": ["Bald", "tau\u00b7chen", "fr\u00f6s\u00b7telnd", "wir", "ins", "kal\u00b7te", "Dun\u00b7kel", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "PPER", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebt, schnelle Sommer, wohl, die unser Herz erhellt!", "tokens": ["Lebt", ",", "schnel\u00b7le", "Som\u00b7mer", ",", "wohl", ",", "die", "un\u00b7ser", "Herz", "er\u00b7hellt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$,", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich h\u00f6re schon, wie dumpf mit finstrem Schalle wieder", "tokens": ["Ich", "h\u00f6\u00b7re", "schon", ",", "wie", "dumpf", "mit", "fins\u00b7trem", "Schal\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das Holz erdr\u00f6hnend auf der H\u00f6fe Pflaster f\u00e4llt.", "tokens": ["Das", "Holz", "er\u00b7dr\u00f6h\u00b7nend", "auf", "der", "H\u00f6\u00b7fe", "Pflas\u00b7ter", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In meinen Busen kehrt des Winters herb Bedr\u00e4ngnis,", "tokens": ["In", "mei\u00b7nen", "Bu\u00b7sen", "kehrt", "des", "Win\u00b7ters", "herb", "Be\u00b7dr\u00e4ng\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zorn, Schauer, Schrecken, Ha\u00df und Arbeit, scharf und hart,", "tokens": ["Zorn", ",", "Schau\u00b7er", ",", "Schre\u00b7cken", ",", "Ha\u00df", "und", "Ar\u00b7beit", ",", "scharf", "und", "hart", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "NN", "$,", "NN", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gleichwie der Sonnenball in seinem Eisgef\u00e4ngnis", "tokens": ["Gleich\u00b7wie", "der", "Son\u00b7nen\u00b7ball", "in", "sei\u00b7nem", "Eis\u00b7ge\u00b7f\u00e4ng\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ist bald mein Herz ein Block, blutfarben und erstarrt.", "tokens": ["Ist", "bald", "mein", "Herz", "ein", "Block", ",", "blut\u00b7far\u00b7ben", "und", "er\u00b7starrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Erzitternd h\u00f6re ich das Fallen aller Scheite;", "tokens