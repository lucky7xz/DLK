{"dta.poem.21161": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Geige .  \n An den Freyherrn von Zedlitz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hier liegt sie wartend und still, die Cremonesische Laute,", "tokens": ["Hier", "liegt", "sie", "war\u00b7tend", "und", "still", ",", "die", "Cre\u00b7mo\u00b7ne\u00b7si\u00b7sche", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "KON", "ADJD", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Kein Glanz verr\u00e4th den bezaubernden Ton.", "tokens": ["Kein", "Glanz", "ver\u00b7r\u00e4\u00b7th", "den", "be\u00b7zau\u00b7bern\u00b7den", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "In prachtloser Einfalt hat sie der welsche K\u00fcnstler er-", "tokens": ["In", "pracht\u00b7lo\u00b7ser", "Ein\u00b7falt", "hat", "sie", "der", "wel\u00b7sche", "K\u00fcnst\u00b7ler", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+--++-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Noch schlafen die Harmonien in ihr.", "tokens": ["Noch", "schla\u00b7fen", "die", "Har\u00b7mo\u00b7ni\u00b7en", "in", "ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Wer nimmt den Bogen, o Freund, und folget dem", "tokens": ["Wer", "nimmt", "den", "Bo\u00b7gen", ",", "o", "Freund", ",", "und", "fol\u00b7get", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "$,", "FM", "NN", "$,", "KON", "VVFIN", "ART"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O! singt uns niemand vom Benda ein Lied?", "tokens": ["O", "!", "singt", "uns", "nie\u00b7mand", "vom", "Ben\u00b7da", "ein", "Lied", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VVFIN", "PPER", "PIS", "APPRART", "NE", "ART", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Was h\u00f6r ich? T\u00e4uschet das Ohr der z\u00e4rtlichsten S\u00e4n-", "tokens": ["Was", "h\u00f6r", "ich", "?", "T\u00e4u\u00b7schet", "das", "Ohr", "der", "z\u00e4rt\u00b7lichs\u00b7ten", "S\u00e4n"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "VVFIN", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wenn sie verschwindende Triller hinseufzt?", "tokens": ["Wenn", "sie", "ver\u00b7schwin\u00b7den\u00b7de", "Tril\u00b7ler", "hin\u00b7seufzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ist dies ein K\u00fcnstler allein? Auf einer einzigen Geige", "tokens": ["Ist", "dies", "ein", "K\u00fcnst\u00b7ler", "al\u00b7lein", "?", "Auf", "ei\u00b7ner", "ein\u00b7zi\u00b7gen", "Gei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PDS", "ART", "NN", "ADV", "$.", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+----+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Rauscht er vollstimmig, als wie ein Concert?", "tokens": ["Rauscht", "er", "voll\u00b7stim\u00b7mig", ",", "als", "wie", "ein", "Con\u00b7cert", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,", "KOUS", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Welch ein entz\u00fcckender Ton, der sich, wie Farben in", "tokens": ["Welch", "ein", "ent\u00b7z\u00fc\u00b7cken\u00b7der", "Ton", ",", "der", "sich", ",", "wie", "Far\u00b7ben", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "$,", "PRELS", "PRF", "$,", "PWAV", "NN", "APPR"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In andern T\u00f6nen unmerklich verliert!", "tokens": ["In", "an\u00b7dern", "T\u00f6\u00b7nen", "un\u00b7mer\u00b7klich", "ver\u00b7liert", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Tief unten brauset das G, mit einer donnernden Stim-", "tokens": ["Tief", "un\u00b7ten", "brau\u00b7set", "das", "G", ",", "mit", "ei\u00b7ner", "don\u00b7nern\u00b7den", "Stim"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ADV", "VVFIN", "ART", "NE", "$,", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Furcht und Entsetzen zum staunenden Ohr.", "tokens": ["Furcht", "und", "Ent\u00b7set\u00b7zen", "zum", "stau\u00b7nen\u00b7den", "Ohr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "So wie ein wilder Orkan, in H\u00f6hlen des Harzes ver-", "tokens": ["So", "wie", "ein", "wil\u00b7der", "Or\u00b7kan", ",", "in", "H\u00f6h\u00b7len", "des", "Har\u00b7zes", "ver"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN", "$,", "APPR", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die schallenden Felsen murmelnd durchbr\u00fcllt.", "tokens": ["Die", "schal\u00b7len\u00b7den", "Fel\u00b7sen", "mur\u00b7melnd", "durch\u00b7br\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und in der hellesten H\u00f6h, der oft der St\u00fcmper ent-", "tokens": ["Und", "in", "der", "hel\u00b7les\u00b7ten", "H\u00f6h", ",", "der", "oft", "der", "St\u00fcm\u00b7per", "ent"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ADV", "ART", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ert\u00f6nt reinklingend der silberne Ton.", "tokens": ["Er\u00b7t\u00f6nt", "rein\u00b7klin\u00b7gend", "der", "sil\u00b7ber\u00b7ne", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die h\u00f6chste Note klingt stark, wie an dem Thurm der", "tokens": ["Die", "h\u00f6chs\u00b7te", "No\u00b7te", "klingt", "stark", ",", "wie", "an", "dem", "Thurm", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$,", "PWAV", "APPR", "ART", "NN", "ART"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Das kleinste Gl\u00f6ckchen harmonisch erklingt.", "tokens": ["Das", "kleins\u00b7te", "Gl\u00f6ck\u00b7chen", "har\u00b7mo\u00b7nisch", "er\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Auf Virtuosen sey stolz, Germanien, die du gezeuget.", "tokens": ["Auf", "Vir\u00b7tu\u00b7o\u00b7sen", "sey", "stolz", ",", "Ger\u00b7ma\u00b7ni\u00b7en", ",", "die", "du", "ge\u00b7zeu\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADJD", "$,", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "In Frankreich und Welschland sind Gr\u00f6\u00dfere nicht.", "tokens": ["In", "Fran\u00b7kreich", "und", "Wel\u00b7schland", "sind", "Gr\u00f6\u00b7\u00dfe\u00b7re", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VAFIN", "NN", "PTKNEG", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Klopstocke z\u00e4hlst du nicht viel. Jhn lohnt der nordische", "tokens": ["Klops\u00b7to\u00b7cke", "z\u00e4hlst", "du", "nicht", "viel", ".", "Jhn", "lohnt", "der", "nor\u00b7di\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "O! hattest du keine Belohnung f\u00fcr ihn?", "tokens": ["O", "!", "hat\u00b7test", "du", "kei\u00b7ne", "Be\u00b7loh\u00b7nung", "f\u00fcr", "ihn", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VAFIN", "PPER", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}