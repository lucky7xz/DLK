{"textgrid.poem.58572": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Erheb dich nicht, o Seel', h\u00e4tt'st du auch tausend Gaben", "tokens": ["Er\u00b7heb", "dich", "nicht", ",", "o", "Seel'", ",", "h\u00e4tt'st", "du", "auch", "tau\u00b7send", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "FM", "NN", "$,", "VAFIN", "PPER", "ADV", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und w\u00e4rst du noch so fromm bei dir und aller Welt,", "tokens": ["Und", "w\u00e4rst", "du", "noch", "so", "fromm", "bei", "dir", "und", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPER", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es kommt, es kommt der Tag, da wird in Staub begraben,", "tokens": ["Es", "kommt", ",", "es", "kommt", "der", "Tag", ",", "da", "wird", "in", "Staub", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$,", "ADV", "VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Was sich zum G\u00f6tzen hat so hoch in dir gestellt!", "tokens": ["Was", "sich", "zum", "G\u00f6t\u00b7zen", "hat", "so", "hoch", "in", "dir", "ge\u00b7stellt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "APPRART", "NN", "VAFIN", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hinunter, b\u00fccke dich ins Nichts, du Wurm der Erden,", "tokens": ["Hin\u00b7un\u00b7ter", ",", "b\u00fc\u00b7cke", "dich", "ins", "Nichts", ",", "du", "Wurm", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "PPER", "APPRART", "PIS", "$,", "PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wirf alle G\u00f6tzen weg, der Herr sei hoch allein!", "tokens": ["Wirf", "al\u00b7le", "G\u00f6t\u00b7zen", "weg", ",", "der", "Herr", "sei", "hoch", "al\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PTKVZ", "$,", "ART", "NN", "VAFIN", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dann kann Gott recht und rein in dir verehret werden,", "tokens": ["Dann", "kann", "Gott", "recht", "und", "rein", "in", "dir", "ver\u00b7eh\u00b7ret", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "ADJD", "KON", "ADJD", "APPR", "PPER", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wenn du im Geist bist arm, entbl\u00f6\u00dft und wahrlich klein.", "tokens": ["Wenn", "du", "im", "Geist", "bist", "arm", ",", "ent\u00b7bl\u00f6\u00dft", "und", "wahr\u00b7lich", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "ADJD", "$,", "VVFIN", "KON"