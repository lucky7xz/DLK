{"textgrid.poem.47227": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1. Im Gebirg", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohin, ach! sollen aus des Markts Gew\u00fchle", "tokens": ["Wo\u00b7hin", ",", "ach", "!", "sol\u00b7len", "aus", "des", "Markts", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "$,", "ITJ", "$.", "VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich eure G\u00f6tter retten, wenn die Dichten", "tokens": ["Sich", "eu\u00b7re", "G\u00f6t\u00b7ter", "ret\u00b7ten", ",", "wenn", "die", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "VVINF", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des ew'gen Hains auch unterm Beil sich lichten,", "tokens": ["Des", "ew'\u00b7gen", "Hains", "auch", "un\u00b7term", "Beil", "sich", "lich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPRART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Qualm des Gewerks auch d\u00e4mpft die heil'ge K\u00fchle?", "tokens": ["Qualm", "des", "Ge\u00b7werks", "auch", "d\u00e4mpft", "die", "heil'\u00b7ge", "K\u00fch\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Es seufzt der Fels, da\u00df ihr sein Steingest\u00fchle", "tokens": ["Es", "seufzt", "der", "Fels", ",", "da\u00df", "ihr", "sein", "Stein\u00b7ge\u00b7st\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zerbrecht, um eure W\u00e4nde draus zu schichten;", "tokens": ["Zer\u00b7brecht", ",", "um", "eu\u00b7re", "W\u00e4n\u00b7de", "draus", "zu", "schich\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUI", "PPOSAT", "NN", "PAV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der freie Waldbach z\u00fcrnt, da\u00df er verpflichten", "tokens": ["Der", "frei\u00b7e", "Wald\u00b7bach", "z\u00fcrnt", ",", "da\u00df", "er", "ver\u00b7pflich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sich mu\u00df, nutzbar zu treiben Rad und M\u00fchle.", "tokens": ["Sich", "mu\u00df", ",", "nutz\u00b7bar", "zu", "trei\u00b7ben", "Rad", "und", "M\u00fch\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VMFIN", "$,", "ADJD", "PTKZU", "VVINF", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Echo klagt, da\u00df statt der Heldenlieder,", "tokens": ["Die", "E\u00b7cho", "klagt", ",", "da\u00df", "statt", "der", "Hel\u00b7den\u00b7lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ins orgelnde Gebraus des Sturms g