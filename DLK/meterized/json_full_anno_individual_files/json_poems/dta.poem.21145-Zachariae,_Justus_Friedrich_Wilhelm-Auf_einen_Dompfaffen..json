{"dta.poem.21145": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen Dompfaffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Vogel, den ein gutes Gl\u00fcck", "tokens": ["O", "Vo\u00b7gel", ",", "den", "ein", "gu\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zu einem Dichter brachte,", "tokens": ["Zu", "ei\u00b7nem", "Dich\u00b7ter", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der dich im ersten Augenblick", "tokens": ["Der", "dich", "im", "ers\u00b7ten", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu seinem Liebling machte;", "tokens": ["Zu", "sei\u00b7nem", "Lieb\u00b7ling", "mach\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Papchen, sey nicht so betr\u00fcbt,", "tokens": ["Mein", "Pap\u00b7chen", ",", "sey", "nicht", "so", "be\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df nun ein K\u00e4ficht dich umgiebt.", "tokens": ["Da\u00df", "nun", "ein", "K\u00e4\u00b7ficht", "dich", "um\u00b7giebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du kanst zwar nichts, und sitzest stumm,", "tokens": ["Du", "kanst", "zwar", "nichts", ",", "und", "sit\u00b7zest", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PIS", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch niemand soll dich h\u00f6hnen.", "tokens": ["Doch", "nie\u00b7mand", "soll", "dich", "h\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du bist, mein Papchen, sch\u00f6n und dumm;", "tokens": ["Du", "bist", ",", "mein", "Pap\u00b7chen", ",", "sch\u00f6n", "und", "dumm", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind das doch viele Sch\u00f6nen.", "tokens": ["Sind", "das", "doch", "vie\u00b7le", "Sch\u00f6\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Soll deiner Farben Pracht vergehn,", "tokens": ["Soll", "dei\u00b7ner", "Far\u00b7ben", "Pracht", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So macht dich deine Treu doch sch\u00f6n.", "tokens": ["So", "macht", "dich", "dei\u00b7ne", "Treu", "doch", "sch\u00f6n", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach lieber Vogel, k\u00f6ntest du", "tokens": ["Ach", "lie\u00b7ber", "Vo\u00b7gel", ",", "k\u00f6n\u00b7test", "du"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "ADV", "NE", "$,", "VMFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich zu Selinden schwingen,", "tokens": ["Dich", "zu", "Se\u00b7lin\u00b7den", "schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und vor der s\u00fc\u00dfen Abendruh", "tokens": ["Und", "vor", "der", "s\u00fc\u00b7\u00dfen", "A\u00b7ben\u00b7druh"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir Nachricht von ihr bringen!", "tokens": ["Mir", "Nach\u00b7richt", "von", "ihr", "brin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ach Papchen, fliege doch zu ihr!", "tokens": ["Ach", "Pap\u00b7chen", ",", "flie\u00b7ge", "doch", "zu", "ihr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "VVFIN", "ADV", "APPR", "PPOSAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den besten Zucker geb ich dir.", "tokens": ["Den", "bes\u00b7ten", "Zu\u00b7cker", "geb", "ich", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}