{"dta.poem.3648": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Kinderlied zu Weihnachten .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gott's Wunder, lieber Bu,               ", "tokens": ["Gott's", "Wun\u00b7der", ",", "lie\u00b7ber", "Bu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geh, horch ein wenig zu,", "tokens": ["Geh", ",", "horch", "ein", "we\u00b7nig", "zu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich dir will erz\u00e4hlen,", "tokens": ["Was", "ich", "dir", "will", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was geschah in aller Fruh.", "tokens": ["Was", "ge\u00b7schah", "in", "al\u00b7ler", "Fruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Da geh ich \u00fcber ein Heid,", "tokens": ["Da", "geh", "ich", "\u00fc\u00b7ber", "ein", "Heid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wo man die Sch\u00e4flein weidt,", "tokens": ["Wo", "man", "die", "Sch\u00e4f\u00b7lein", "weidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da kam ein kleiner Bu gerennt,", "tokens": ["Da", "kam", "ein", "klei\u00b7ner", "Bu", "ge\u00b7rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab ihn all mein Tag nicht kennt.", "tokens": ["Ich", "hab", "ihn", "all", "mein", "Tag", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIAT", "PPOSAT", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Gott's Wunder, lieber Bu,", "tokens": ["Gott's", "Wun\u00b7der", ",", "lie\u00b7ber", "Bu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geh, horch ein wenig zu!", "tokens": ["Geh", ",", "horch", "ein", "we\u00b7nig", "zu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Den alten Zimmermann,", "tokens": ["Den", "al\u00b7ten", "Zim\u00b7mer\u00b7mann", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den schaun wir alle an,", "tokens": ["Den", "schaun", "wir", "al\u00b7le", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hat dem kleinen Kindelein", "tokens": ["Der", "hat", "dem", "klei\u00b7nen", "Kin\u00b7de\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Viel Gutes angethan.", "tokens": ["Viel", "Gu\u00b7tes", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Er hat es so erku\u00dft,", "tokens": ["Er", "hat", "es", "so", "er\u00b7ku\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Es war ein wahre Lust,", "tokens": ["Es", "war", "ein", "wah\u00b7re", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er schafft das Brod, i\u00dft selber nicht,", "tokens": ["Er", "schafft", "das", "Brod", ",", "i\u00dft", "sel\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist auch sein rechter Vater nicht.", "tokens": ["Ist", "auch", "sein", "rech\u00b7ter", "Va\u00b7ter", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Gott's Wunder, lieber Bu,", "tokens": ["Gott's", "Wun\u00b7der", ",", "lie\u00b7ber", "Bu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geh, lausch ein wenig zu.", "tokens": ["Geh", ",", "lausch", "ein", "we\u00b7nig", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "H\u00e4tt' ich nur dran gedenkt,", "tokens": ["H\u00e4tt'", "ich", "nur", "dran", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dem Kind h\u00e4tt ich was g'schenkt;", "tokens": ["Dem", "Kind", "h\u00e4tt", "ich", "was", "g'\u00b7schenkt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "PIS", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwei Aepfel hab ich bei mir g'habt,", "tokens": ["Zwei", "A\u00b7e\u00b7pfel", "hab", "ich", "bei", "mir", "g'\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Es hat mich freundlich angelacht.", "tokens": ["Es", "hat", "mich", "freund\u00b7lich", "an\u00b7ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Gott's Wunder, lieber Bu,", "tokens": ["Gott's", "Wun\u00b7der", ",", "lie\u00b7ber", "Bu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADV", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Geh, horch ein wenig zu.", "tokens": ["Geh", ",", "horch", "ein", "we\u00b7nig", "zu", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "ART", "PIS", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}