{"textgrid.poem.52820": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Lesend mit poetischem Intresse", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Lesend mit poetischem Intresse", "tokens": ["Le\u00b7send", "mit", "po\u00b7e\u00b7ti\u00b7schem", "I\u00b7ntres\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In des H\u00f6lderlin's Gedichtenbuch,", "tokens": ["In", "des", "H\u00f6l\u00b7der\u00b7lin's", "Ge\u00b7dich\u00b7ten\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Von dem Plato und dem Sophlokesse,", "tokens": ["Von", "dem", "Pla\u00b7to", "und", "dem", "Sop\u00b7hlo\u00b7kes\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kann ich mich verwundern nicht genug,", "tokens": ["Kann", "ich", "mich", "ver\u00b7wun\u00b7dern", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df er nicht von ", "tokens": ["Da\u00df", "er", "nicht", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wo ganz anders die Angora l\u00e4rmte,", "tokens": ["Wo", "ganz", "an\u00b7ders", "die", "An\u00b7go\u00b7ra", "l\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "Weil sein Album Jeder bei sich trug.", "tokens": ["Weil", "sein", "Al\u00b7bum", "Je\u00b7der", "bei", "sich", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Damals war zu Grabe l\u00e4ngst gegangen", "tokens": ["Da\u00b7mals", "war", "zu", "Gra\u00b7be", "l\u00e4ngst", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die zeitraubend leid'ge Politik,", "tokens": ["Die", "zeit\u00b7rau\u00b7bend", "lei\u00b7d'\u00b7ge", "Po\u00b7li\u00b7tik", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Alle dichteten und Alle sangen,", "tokens": ["Al\u00b7le", "dich\u00b7te\u00b7ten", "und", "Al\u00b7le", "san\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "KON", "PIS", "VVFIN", "$,"], "meter": "--+---+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Und bekamen's dennoch gar nicht dick.", "tokens": ["Und", "be\u00b7ka\u00b7men's", "den\u00b7noch", "gar", "nicht", "dick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADV", "ADV", "PTKNEG", "ADJD", "$."], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Sophlokes war Jeder aus dem Volke,", "tokens": ["Sop\u00b7hlo\u00b7kes", "war", "Je\u00b7der", "aus", "dem", "Vol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Drohend wie die dichte Regenwolke,", "tokens": ["Dro\u00b7hend", "wie", "die", "dich\u00b7te", "Re\u00b7gen\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Auf dem Markte stand die Dichterklik.", "tokens": ["Auf", "dem", "Mark\u00b7te", "stand", "die", "Dich\u00b7ter\u00b7klik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Keiner konnte da vollj\u00e4hrig werden,", "tokens": ["Kei\u00b7ner", "konn\u00b7te", "da", "voll\u00b7j\u00e4h\u00b7rig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der nicht schon ein Trauerspiel ersann,", "tokens": ["Der", "nicht", "schon", "ein", "Trau\u00b7er\u00b7spiel", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit guter Lunge und Geberden,", "tokens": ["Und", "mit", "gu\u00b7ter", "Lun\u00b7ge", "und", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn nat\u00fcrlich war das schwer, weil Damen,", "tokens": ["Denn", "na\u00b7t\u00fcr\u00b7lich", "war", "das", "schwer", ",", "weil", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJD", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Greis und J\u00fcngling vorzulesen kamen,", "tokens": ["Greis", "und", "J\u00fcng\u00b7ling", "vor\u00b7zu\u00b7le\u00b7sen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo durch Blumen der Ulysses", "tokens": ["Wo", "durch", "Blu\u00b7men", "der", "U\u00b7lys\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Herrlich war's im Schatten der Planeten,", "tokens": ["Herr\u00b7lich", "wa\u00b7r's", "im", "Schat\u00b7ten", "der", "Pla\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo man las und sprach in Einem fort,", "tokens": ["Wo", "man", "las", "und", "sprach", "in", "Ei\u00b7nem", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jeder war der Erste der Poeten,", "tokens": ["Je\u00b7der", "war", "der", "Ers\u00b7te", "der", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kaum vernahm man noch sein eigen Wort.", "tokens": ["Kaum", "ver\u00b7nahm", "man", "noch", "sein", "ei\u00b7gen", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ganz verschwand bei solchen Elementen", "tokens": ["Ganz", "ver\u00b7schwand", "bei", "sol\u00b7chen", "E\u00b7le\u00b7men\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Das fatale Volk der Rezensenten,", "tokens": ["Das", "fa\u00b7ta\u00b7le", "Volk", "der", "Re\u00b7zen\u00b7sen\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Denn zum Kriteln war die Zeit nicht dort.", "tokens": ["Denn", "zum", "Kri\u00b7teln", "war", "die", "Zeit", "nicht", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Sch\u00f6ne Welt, Du bist hinabgeglitten,", "tokens": ["Sch\u00f6\u00b7ne", "Welt", ",", "Du", "bist", "hin\u00b7ab\u00b7ge\u00b7glit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Lorbeer jedes Haupt umragt,", "tokens": ["Wo", "der", "Lor\u00b7beer", "je\u00b7des", "Haupt", "um\u00b7ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur Gesichter werden jetzt geschnitten,", "tokens": ["Nur", "Ge\u00b7sich\u00b7ter", "wer\u00b7den", "jetzt", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn sich Einer vor mit Versen wagt.", "tokens": ["Wenn", "sich", "Ei\u00b7ner", "vor", "mit", "Ver\u00b7sen", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "APPR", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Im Materialismus ganz versunken,", "tokens": ["Im", "Ma\u00b7te\u00b7ri\u00b7a\u00b7lis\u00b7mus", "ganz", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dient man knechtisch, ohne G\u00f6tterfunken,", "tokens": ["Dient", "man", "knech\u00b7tisch", ",", "oh\u00b7ne", "G\u00f6t\u00b7ter\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dem Gesetz der Schwere, wie man sagt.", "tokens": ["Dem", "Ge\u00b7setz", "der", "Schwe\u00b7re", ",", "wie", "man", "sagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Freilich ward auch Unfug viel geduldet", "tokens": ["Frei\u00b7lich", "ward", "auch", "Un\u00b7fug", "viel", "ge\u00b7dul\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In Achaia, wie ich h\u00f6ren mu\u00df,", "tokens": ["In", "A\u00b7chai\u00b7a", ",", "wie", "ich", "h\u00f6\u00b7ren", "mu\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "PWAV", "PPER", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Die Gemeinden waren arg verschuldet,", "tokens": ["Die", "Ge\u00b7mein\u00b7den", "wa\u00b7ren", "arg", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und man schwelgte nicht im Ueberflu\u00df.", "tokens": ["Und", "man", "schwelg\u00b7te", "nicht", "im", "Ue\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Aber das hat manchmal auch sein Gutes,", "tokens": ["A\u00b7ber", "das", "hat", "manch\u00b7mal", "auch", "sein", "Gu\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die Griechen blieben guten Muthes,", "tokens": ["Und", "die", "Grie\u00b7chen", "blie\u00b7ben", "gu\u00b7ten", "Mu\u00b7thes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "--+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Denn nicht bitter schmeckt der Musen Ku\u00df!", "tokens": ["Denn", "nicht", "bit\u00b7ter", "schmeckt", "der", "Mu\u00b7sen", "Ku\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADJD", "VVFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Lesend mit poetischem Intresse", "tokens": ["Le\u00b7send", "mit", "po\u00b7e\u00b7ti\u00b7schem", "I\u00b7ntres\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In des H\u00f6lderlin's Gedichtenbuch,", "tokens": ["In", "des", "H\u00f6l\u00b7der\u00b7lin's", "Ge\u00b7dich\u00b7ten\u00b7buch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Von dem Plato und dem Sophlokesse,", "tokens": ["Von", "dem", "Pla\u00b7to", "und", "dem", "Sop\u00b7hlo\u00b7kes\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kann ich mich verwundern nicht genug,", "tokens": ["Kann", "ich", "mich", "ver\u00b7wun\u00b7dern", "nicht", "ge\u00b7nug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Da\u00df er nicht von ", "tokens": ["Da\u00df", "er", "nicht", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Wo ganz anders die Angora l\u00e4rmte,", "tokens": ["Wo", "ganz", "an\u00b7ders", "die", "An\u00b7go\u00b7ra", "l\u00e4rm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.7": {"text": "W