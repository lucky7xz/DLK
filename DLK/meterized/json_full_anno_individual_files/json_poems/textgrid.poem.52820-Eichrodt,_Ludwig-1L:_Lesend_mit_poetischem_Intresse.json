 "Drohend wie die dichte Regenwolke,", "tokens": ["Dro\u00b7hend", "wie", "die", "dich\u00b7te", "Re\u00b7gen\u00b7wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Auf dem Markte stand die Dichterklik.", "tokens": ["Auf", "dem", "Mark\u00b7te", "stand", "die", "Dich\u00b7ter\u00b7klik", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Keiner konnte da vollj\u00e4hrig werden,", "tokens": ["Kei\u00b7ner", "konn\u00b7te", "da", "voll\u00b7j\u00e4h\u00b7rig", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "VAINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der nicht schon ein Trauerspiel ersann,", "tokens": ["Der", "nicht", "schon", "ein", "Trau\u00b7er\u00b7spiel", "er\u00b7sann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und mit guter Lunge und Geberden,", "tokens": ["Und", "mit", "gu\u00b7ter", "Lun\u00b7ge", "und", "Ge\u00b7ber\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn nat\u00fcrlich war das schwer, weil Damen,", "tokens": ["Denn", "na\u00b7t\u00fcr\u00b7lich", "war", "das", "schwer", ",", "weil", "Da\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ART", "ADJD", "$,", "KOUS", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Greis und J\u00fcngling vorzulesen kamen,", "tokens": ["Greis", "und", "J\u00fcng\u00b7ling", "vor\u00b7zu\u00b7le\u00b7sen", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVIZU", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wo durch Blumen der Ulysses", "tokens": ["Wo", "durch", "Blu\u00b7men", "der", "U\u00b7lys\u00b7ses"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Herrlich war's im Schatten der Planeten,", "tokens": ["Herr\u00b7lich", "wa\u00b7r's", "im", "Schat\u00b7ten", "der", "Pla\u00b7ne\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Wo man las und sprach in Einem fort,", "tokens": ["Wo", "man", "las", "und", "sprach", "in", "Ei\u00b7nem", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "KON", "VVFIN", "APPR", "PIS", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Jeder war der Erste der Poeten,", "tokens": ["Je\u00b7der", "war", "der", "Ers\u00b7te", "der", "Po\u00b7et\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Kaum vernahm man noch sein eigen Wort.", "tokens": ["Kaum", "ver\u00b7nahm", "man", "noch", "sein", "ei\u00b7gen", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ganz verschwand bei solchen Elementen", "tokens": ["Ganz", "ver\u00b7schwand", "bei", "sol\u00b7chen", "E\u00b7le\u00b7men\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.6": {"text": "Das fatale Volk der Rezensenten,", "tokens": ["Das", "fa\u00b7ta\u00b7le", "Volk", "der", "Re\u00b7zen\u00b7sen\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Denn zum Kriteln war die Zeit nicht dort.", "tokens": ["Denn", "zum", "Kri\u00b7teln", "war", "die", "Zeit", "nicht", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VAFIN", "ART", "NN", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Sch\u00f6ne Welt, Du bist hinabgeglitten,", "tokens": ["Sch\u00f6\u00b7ne", "Welt", ",", "Du", "bist", "hin\u00b7ab\u00b7ge\u00b7glit\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo der Lorbeer jedes Haupt umragt,", "tokens": ["Wo", "der", "Lor\u00b7beer", "je\u00b7des", "Haupt", "um\u00b7ragt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nur Gesichter werden jetzt geschnitten,", "tokens": ["Nur", "Ge\u00b7sich\u00b7ter", "wer\u00b7den", "jetzt", "ge\u00b7schnit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wenn sich Einer vor mit Versen wagt.", "tokens": ["Wenn", "sich", "Ei\u00b7ner", "vor", "mit", "Ver\u00b7sen", "wagt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PIS", "APPR", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Im Materialismus ganz versunken,", "tokens": ["Im", "Ma\u00b7te\u00b7ri\u00b7a\u00b7lis\u00b7mus", "ganz", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Dient man knechtisch, ohne G\u00f6tterfunken,", "tokens": ["Dient", "man", "knech\u00b7tisch", ",", "oh\u00b7ne", "G\u00f6t\u00b7ter\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Dem Gesetz der Schwere, wie man sagt.", "tokens": ["Dem", "Ge\u00b7setz", "der", "Schwe\u00b7re", ",", "wie", "man", "sagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Freilich ward auch Unfug viel geduldet", "tokens": ["Frei\u00b7lich", "ward", "auch", "Un\u00b7fug", "viel", "ge\u00b7dul\u00b7det"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "NN", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In Achaia, wie ich h\u00f6ren mu\u00df,", "tokens": ["In", "A\u00b7chai\u00b7a", ",", "wie", "ich", "h\u00f6\u00b7ren", "mu\u