{"dta.poem.17195": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "46.  Auff die Wunden JESU.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ich seh die Wunden an als offne Himmelspforten/", "tokens": ["Ich", "seh", "die", "Wun\u00b7den", "an", "als", "off\u00b7ne", "Him\u00b7mel\u00b7spfor\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "KOKOM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und kan numehr hinein an f\u00fcnff gewissen orten.", "tokens": ["Und", "kan", "nu\u00b7mehr", "hin\u00b7ein", "an", "f\u00fcnff", "ge\u00b7wis\u00b7sen", "or\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "CARD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wo komm ich aber straks bey meinem GOtt zustehn?", "tokens": ["Wo", "komm", "ich", "a\u00b7ber", "straks", "bey", "mei\u00b7nem", "Gott", "zu\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich wil durch F\u00fc\u00df und H\u00e4nd\u2019 ins Hertz der Liebe gehn.", "tokens": ["Ich", "wil", "durch", "F\u00fc\u00df", "und", "H\u00e4nd'", "ins", "Hertz", "der", "Lie\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "NN", "KON", "NN", "APPRART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}