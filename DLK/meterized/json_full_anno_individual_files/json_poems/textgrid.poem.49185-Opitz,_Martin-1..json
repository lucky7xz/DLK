{"textgrid.poem.49185": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist irgend zu erfragen", "tokens": ["Ist", "ir\u00b7gend", "zu", "er\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sch\u00e4ffer umb den Rein,", "tokens": ["Ein", "Sch\u00e4f\u00b7fer", "umb", "den", "Rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sehnlich sich beklagen", "tokens": ["Der", "sehn\u00b7lich", "sich", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df \u00fcber Liebespein,", "tokens": ["Mu\u00df", "\u00fc\u00b7ber", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird mir m\u00fcssen weichen,", "tokens": ["Der", "wird", "mir", "m\u00fcs\u00b7sen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich wei\u00df, sie plagt mich mehr;", "tokens": ["Ich", "wei\u00df", ",", "sie", "plagt", "mich", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Niemand ist mir zu gleichen,", "tokens": ["Nie\u00b7mand", "ist", "mir", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und liebt er noch so sehr.", "tokens": ["Und", "liebt", "er", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es ist vorbey gegangen", "tokens": ["Es", "ist", "vor\u00b7bey", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fa\u00dft jetzt ein volles Jahr,", "tokens": ["Fa\u00dft", "jetzt", "ein", "vol\u00b7les", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Phyllis mich gefangen", "tokens": ["Da\u00df", "Phyl\u00b7lis", "mich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Liebe gantz und gar,", "tokens": ["Mit", "Lie\u00b7be", "gantz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie mir hat genommen", "tokens": ["Da\u00df", "sie", "mir", "hat", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gedancken, Muth und Sinn;", "tokens": ["Ge\u00b7dan\u00b7cken", ",", "Muth", "und", "Sinn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Jahr ist's, da\u00df ich kommen", "tokens": ["Ein", "Jahr", "ist's", ",", "da\u00df", "ich", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ihre Liebe bin.", "tokens": ["In", "ih\u00b7re", "Lie\u00b7be", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Seyt dem bin ich verwirret", "tokens": ["Seyt", "dem", "bin", "ich", "ver\u00b7wir\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "VAFIN", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Gewesen f\u00fcr und f\u00fcr,", "tokens": ["Ge\u00b7we\u00b7sen", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es haben auch geirret", "tokens": ["Es", "ha\u00b7ben", "auch", "ge\u00b7ir\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schaffe neben mir;", "tokens": ["Die", "Schaf\u00b7fe", "ne\u00b7ben", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Feld hab' ich verlassen,", "tokens": ["Das", "Feld", "hab'", "ich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gelebt in Einsambkeit,", "tokens": ["Ge\u00b7lebt", "in", "Ein\u00b7samb\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hab' alles m\u00fcssen hassen,", "tokens": ["Hab'", "al\u00b7les", "m\u00fcs\u00b7sen", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Worumb ein Mensch sich freut.", "tokens": ["Wo\u00b7rumb", "ein", "Mensch", "sich", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Nichts hab' ich k\u00f6nnen singen", "tokens": ["Nichts", "hab'", "ich", "k\u00f6n\u00b7nen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als nur ihr klares Liecht;", "tokens": ["Als", "nur", "ihr", "kla\u00b7res", "Liecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ihr hab' ich zu klingen", "tokens": ["Von", "ihr", "hab'", "ich", "zu", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Lauten abgericht;", "tokens": ["Die", "Lau\u00b7ten", "ab\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie sehr ich sie mu\u00df lieben", "tokens": ["Wie", "sehr", "ich", "sie", "mu\u00df", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ihre grosse Ziehr,", "tokens": ["Und", "ih\u00b7re", "gros\u00b7se", "Ziehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das hab' ich fast geschrieben", "tokens": ["Das", "hab'", "ich", "fast", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An alle B\u00e4um' allhier.", "tokens": ["An", "al\u00b7le", "B\u00e4um'", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Kein Trincken und kein Essen,", "tokens": ["Kein", "Trin\u00b7cken", "und", "kein", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja, nichts hat mir behagt,", "tokens": ["Ja", ",", "nichts", "hat", "mir", "be\u00b7hagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nur stets gesessen", "tokens": ["Ich", "bin", "nur", "stets", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und habe mich beklagt;", "tokens": ["Und", "ha\u00b7be", "mich", "be\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In diesen schweren Orden", "tokens": ["In", "die\u00b7sen", "schwe\u00b7ren", "Or\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verendert alles sich,", "tokens": ["Ver\u00b7en\u00b7dert", "al\u00b7les", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Herd' ist mager worden,", "tokens": ["Die", "Herd'", "ist", "ma\u00b7ger", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ich bin nicht mehr ich.", "tokens": ["Und", "ich", "bin", "nicht", "mehr", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Sie aber hat die Sinnen", "tokens": ["Sie", "a\u00b7ber", "hat", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit von mir abgekehrt,", "tokens": ["Weit", "von", "mir", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ist gar nicht zu gewinnen,", "tokens": ["Ist", "gar", "nicht", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wer' ich ihr nicht werth,", "tokens": ["Als", "wer'", "ich", "ihr", "nicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da doch, was ich gesungen", "tokens": ["Da", "doch", ",", "was", "ich", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Brittenland' erschallt", "tokens": ["Im", "Brit\u00b7ten\u00b7land'", "er\u00b7schallt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auch mein Thon gedrungen", "tokens": ["Und", "auch", "mein", "Thon", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bi\u00df durch den B\u00f6hmer Waldt.", "tokens": ["Bi\u00df", "durch", "den", "B\u00f6h\u00b7mer", "Waldt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "So hab' ich auch daneben,", "tokens": ["So", "hab'", "ich", "auch", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe was bey mir,", "tokens": ["Ich", "ha\u00b7be", "was", "bey", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich nicht wolte geben", "tokens": ["Da\u00df", "ich", "nicht", "wol\u00b7te", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umb alles Vieh allhier", "tokens": ["Umb", "al\u00b7les", "Vieh", "all\u00b7hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das an des Neckers Rande", "tokens": ["Das", "an", "des", "Ne\u00b7ckers", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im gr\u00fcnen Grase geht;", "tokens": ["Im", "gr\u00fc\u00b7nen", "Gra\u00b7se", "geht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Lob wird auff dem Lande", "tokens": ["Mein", "Lob", "wird", "auff", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und in der Stadt erh\u00f6ht.", "tokens": ["Und", "in", "der", "Stadt", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Jedoch nach diesem allen", "tokens": ["Je\u00b7doch", "nach", "die\u00b7sem", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frag' ich nicht sonders viel,", "tokens": ["Frag'", "ich", "nicht", "son\u00b7ders", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Phyllis zu gefallen", "tokens": ["Der", "Phyl\u00b7lis", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich einig singen wil,", "tokens": ["Ich", "ei\u00b7nig", "sin\u00b7gen", "wil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil nichts ist, das auff Erden", "tokens": ["Weil", "nichts", "ist", ",", "das", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VAFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir ohne sie gefellt;", "tokens": ["Mir", "oh\u00b7ne", "sie", "ge\u00b7fellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kan ihre Gunst mir werden,", "tokens": ["Kan", "ih\u00b7re", "Gunst", "mir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So hab' ich alle Welt.", "tokens": ["So", "hab'", "ich", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Ist irgend zu erfragen", "tokens": ["Ist", "ir\u00b7gend", "zu", "er\u00b7fra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Sch\u00e4ffer umb den Rein,", "tokens": ["Ein", "Sch\u00e4f\u00b7fer", "umb", "den", "Rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der sehnlich sich beklagen", "tokens": ["Der", "sehn\u00b7lich", "sich", "be\u00b7kla\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mu\u00df \u00fcber Liebespein,", "tokens": ["Mu\u00df", "\u00fc\u00b7ber", "Lie\u00b7be\u00b7spein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der wird mir m\u00fcssen weichen,", "tokens": ["Der", "wird", "mir", "m\u00fcs\u00b7sen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich wei\u00df, sie plagt mich mehr;", "tokens": ["Ich", "wei\u00df", ",", "sie", "plagt", "mich", "mehr", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Niemand ist mir zu gleichen,", "tokens": ["Nie\u00b7mand", "ist", "mir", "zu", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.8": {"text": "Und liebt er noch so sehr.", "tokens": ["Und", "liebt", "er", "noch", "so", "sehr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Es ist vorbey gegangen", "tokens": ["Es", "ist", "vor\u00b7bey", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fa\u00dft jetzt ein volles Jahr,", "tokens": ["Fa\u00dft", "jetzt", "ein", "vol\u00b7les", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df Phyllis mich gefangen", "tokens": ["Da\u00df", "Phyl\u00b7lis", "mich", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Liebe gantz und gar,", "tokens": ["Mit", "Lie\u00b7be", "gantz", "und", "gar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df sie mir hat genommen", "tokens": ["Da\u00df", "sie", "mir", "hat", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gedancken, Muth und Sinn;", "tokens": ["Ge\u00b7dan\u00b7cken", ",", "Muth", "und", "Sinn", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ein Jahr ist's, da\u00df ich kommen", "tokens": ["Ein", "Jahr", "ist's", ",", "da\u00df", "ich", "kom\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ihre Liebe bin.", "tokens": ["In", "ih\u00b7re", "Lie\u00b7be", "bin", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Seyt dem bin ich verwirret", "tokens": ["Seyt", "dem", "bin", "ich", "ver\u00b7wir\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ART", "VAFIN", "PPER", "VVFIN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Gewesen f\u00fcr und f\u00fcr,", "tokens": ["Ge\u00b7we\u00b7sen", "f\u00fcr", "und", "f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es haben auch geirret", "tokens": ["Es", "ha\u00b7ben", "auch", "ge\u00b7ir\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Schaffe neben mir;", "tokens": ["Die", "Schaf\u00b7fe", "ne\u00b7ben", "mir", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das Feld hab' ich verlassen,", "tokens": ["Das", "Feld", "hab'", "ich", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gelebt in Einsambkeit,", "tokens": ["Ge\u00b7lebt", "in", "Ein\u00b7samb\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Hab' alles m\u00fcssen hassen,", "tokens": ["Hab'", "al\u00b7les", "m\u00fcs\u00b7sen", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Worumb ein Mensch sich freut.", "tokens": ["Wo\u00b7rumb", "ein", "Mensch", "sich", "freut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Nichts hab' ich k\u00f6nnen singen", "tokens": ["Nichts", "hab'", "ich", "k\u00f6n\u00b7nen", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als nur ihr klares Liecht;", "tokens": ["Als", "nur", "ihr", "kla\u00b7res", "Liecht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ihr hab' ich zu klingen", "tokens": ["Von", "ihr", "hab'", "ich", "zu", "klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Die Lauten abgericht;", "tokens": ["Die", "Lau\u00b7ten", "ab\u00b7ge\u00b7richt", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wie sehr ich sie mu\u00df lieben", "tokens": ["Wie", "sehr", "ich", "sie", "mu\u00df", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und ihre grosse Ziehr,", "tokens": ["Und", "ih\u00b7re", "gros\u00b7se", "Ziehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Das hab' ich fast geschrieben", "tokens": ["Das", "hab'", "ich", "fast", "ge\u00b7schrie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "An alle B\u00e4um' allhier.", "tokens": ["An", "al\u00b7le", "B\u00e4um'", "all\u00b7hier", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Kein Trincken und kein Essen,", "tokens": ["Kein", "Trin\u00b7cken", "und", "kein", "Es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ja, nichts hat mir behagt,", "tokens": ["Ja", ",", "nichts", "hat", "mir", "be\u00b7hagt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PIS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich bin nur stets gesessen", "tokens": ["Ich", "bin", "nur", "stets", "ge\u00b7ses\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und habe mich beklagt;", "tokens": ["Und", "ha\u00b7be", "mich", "be\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In diesen schweren Orden", "tokens": ["In", "die\u00b7sen", "schwe\u00b7ren", "Or\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verendert alles sich,", "tokens": ["Ver\u00b7en\u00b7dert", "al\u00b7les", "sich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Herd' ist mager worden,", "tokens": ["Die", "Herd'", "ist", "ma\u00b7ger", "wor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und ich bin nicht mehr ich.", "tokens": ["Und", "ich", "bin", "nicht", "mehr", "ich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PTKNEG", "ADV", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie aber hat die Sinnen", "tokens": ["Sie", "a\u00b7ber", "hat", "die", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Weit von mir abgekehrt,", "tokens": ["Weit", "von", "mir", "ab\u00b7ge\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Ist gar nicht zu gewinnen,", "tokens": ["Ist", "gar", "nicht", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als wer' ich ihr nicht werth,", "tokens": ["Als", "wer'", "ich", "ihr", "nicht", "werth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Da doch, was ich gesungen", "tokens": ["Da", "doch", ",", "was", "ich", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "PWS", "PPER", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im Brittenland' erschallt", "tokens": ["Im", "Brit\u00b7ten\u00b7land'", "er\u00b7schallt"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und auch mein Thon gedrungen", "tokens": ["Und", "auch", "mein", "Thon", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bi\u00df durch den B\u00f6hmer Waldt.", "tokens": ["Bi\u00df", "durch", "den", "B\u00f6h\u00b7mer", "Waldt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "NE", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "So hab' ich auch daneben,", "tokens": ["So", "hab'", "ich", "auch", "da\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich habe was bey mir,", "tokens": ["Ich", "ha\u00b7be", "was", "bey", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df ich nicht wolte geben", "tokens": ["Da\u00df", "ich", "nicht", "wol\u00b7te", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Umb alles Vieh allhier", "tokens": ["Umb", "al\u00b7les", "Vieh", "all\u00b7hier"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "PIAT", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das an des Neckers Rande", "tokens": ["Das", "an", "des", "Ne\u00b7ckers", "Ran\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Im gr\u00fcnen Grase geht;", "tokens": ["Im", "gr\u00fc\u00b7nen", "Gra\u00b7se", "geht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Mein Lob wird auff dem Lande", "tokens": ["Mein", "Lob", "wird", "auff", "dem", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und in der Stadt erh\u00f6ht.", "tokens": ["Und", "in", "der", "Stadt", "er\u00b7h\u00f6ht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Jedoch nach diesem allen", "tokens": ["Je\u00b7doch", "nach", "die\u00b7sem", "al\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "PIAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Frag' ich nicht sonders viel,", "tokens": ["Frag'", "ich", "nicht", "son\u00b7ders", "viel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKNEG", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Phyllis zu gefallen", "tokens": ["Der", "Phyl\u00b7lis", "zu", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich einig singen wil,", "tokens": ["Ich", "ei\u00b7nig", "sin\u00b7gen", "wil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Weil nichts ist, das auff Erden", "tokens": ["Weil", "nichts", "ist", ",", "das", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PIS", "VAFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Mir ohne sie gefellt;", "tokens": ["Mir", "oh\u00b7ne", "sie", "ge\u00b7fellt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Kan ihre Gunst mir werden,", "tokens": ["Kan", "ih\u00b7re", "Gunst", "mir", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "So hab' ich alle Welt.", "tokens": ["So", "hab'", "ich", "al\u00b7le", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}