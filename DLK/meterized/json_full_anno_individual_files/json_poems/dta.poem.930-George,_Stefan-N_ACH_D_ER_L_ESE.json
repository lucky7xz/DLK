{"dta.poem.930": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N ACH   D ER   L ESE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr rufe junger jahre die befahlen               ", "tokens": ["Ihr", "ru\u00b7fe", "jun\u00b7ger", "jah\u00b7re", "die", "be\u00b7fah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach IHR zu suchen unter diesen zweigen               ", "tokens": ["Nach", "IhR", "zu", "su\u00b7chen", "un\u00b7ter", "die\u00b7sen", "zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "PTKZU", "VVINF", "APPR", "PDAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich muss vor euch die stirn verneinend neigen               ", "tokens": ["Ich", "muss", "vor", "euch", "die", "stirn", "ver\u00b7nei\u00b7nend", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "NN", "VVPP", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Denn meine liebe schl&#228;ft im land der strahlen             ", "tokens": ["Denn", "mei\u00b7ne", "lie\u00b7be", "schl", "&#228;", "ft", "im", "land", "der", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NE", "$(", "VVFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch schickt ihr Sie mir wieder die im brennen               ", "tokens": ["Doch", "schickt", "ihr", "Sie", "mir", "wie\u00b7der", "die", "im", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PPER", "ADV", "ART", "APPRART", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des sommers und im flattern der Eroten               ", "tokens": ["Des", "som\u00b7mers", "und", "im", "flat\u00b7tern", "der", "E\u00b7ro\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "APPRART", "VVINF", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich als geleit mir sch&#252;chtern dargeboten               ", "tokens": ["Sich", "als", "ge\u00b7leit", "mir", "sch", "&#252;", "ch\u00b7tern", "dar\u00b7ge\u00b7bo\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["PRF", "KOUS", "NN", "PPER", "ADJD", "$(", "VVINF", "VMFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ich will sie diesmal freudig anerkennen             ", "tokens": ["Ich", "will", "sie", "dies\u00b7mal", "freu\u00b7dig", "an\u00b7er\u00b7ken\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die reifen trauben g&#228;hren in den b&#252;tten               ", "tokens": ["Die", "rei\u00b7fen", "trau\u00b7ben", "g", "&#228;", "hren", "in", "den", "b", "&#252;", "tten"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "ADJA", "NN", "NE", "$(", "VVFIN", "APPR", "ART", "XY", "XY", "XY"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch will ich alles was an edlen trieben               ", "tokens": ["Doch", "will", "ich", "al\u00b7les", "was", "an", "ed\u00b7len", "trie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "PIS", "PWS", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und sch\u00f6ner saat vom sommer mir geblieben               ", "tokens": ["Und", "sch\u00f6\u00b7ner", "saat", "vom", "som\u00b7mer", "mir", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPRART", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus vollen h&#228;nden vor ihr niedersch&#252;tten.             ", "tokens": ["Aus", "vol\u00b7len", "h", "&#228;", "nden", "vor", "ihr", "nie\u00b7dersch", "&#252;", "tten", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "$(", "VVFIN", "APPR", "PPOSAT", "ADJD", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}