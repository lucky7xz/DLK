{"dta.poem.13988": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Das Zaubernetz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und die blauen Augen sprachen,", "tokens": ["Und", "die", "blau\u00b7en", "Au\u00b7gen", "spra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da ich waldw\u00e4rts wollte jagen:", "tokens": ["Da", "ich", "wald\u00b7w\u00e4rts", "woll\u00b7te", "ja\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u201ezieh' mir, Sch\u00f6ner, nicht von dannen!\u201c", "tokens": ["\u201e", "zieh'", "mir", ",", "Sch\u00f6\u00b7ner", ",", "nicht", "von", "dan\u00b7nen", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "NN", "$,", "PTKNEG", "APPR", "ADV", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, da war ich Dein Gefangner!", "tokens": ["Ach", ",", "da", "war", "ich", "Dein", "Ge\u00b7fang\u00b7ner", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}