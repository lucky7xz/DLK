{"textgrid.poem.64044": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Die verwelkten Blumen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Aus des Gl\u00fcckes unumw\u00f6lkten Tagen", "tokens": ["Aus", "des", "Gl\u00fc\u00b7ckes", "un\u00b7um\u00b7w\u00f6lk\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hab' ich Euch mir liebend aufgespart,", "tokens": ["Hab'", "ich", "Euch", "mir", "lie\u00b7bend", "auf\u00b7ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Am ger\u00fchrten Herzen Euch getragen,", "tokens": ["Am", "ge\u00b7r\u00fchr\u00b7ten", "Her\u00b7zen", "Euch", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und mit stiller Sorgfalt Euch verwahrt.", "tokens": ["Und", "mit", "stil\u00b7ler", "Sorg\u00b7falt", "Euch", "ver\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Dennoch \u2013 f\u00fchllos gegen treue Pflege,", "tokens": ["Den\u00b7noch", "\u2013", "f\u00fchl\u00b7los", "ge\u00b7gen", "treu\u00b7e", "Pfle\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "