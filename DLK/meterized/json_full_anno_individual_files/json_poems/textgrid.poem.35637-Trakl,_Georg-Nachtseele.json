{"textgrid.poem.35637": {"metadata": {"author": {"name": "Trakl, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Nachtseele", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweigsam stieg vom schwarzen Wald ein blaues Wild", "tokens": ["Schweig\u00b7sam", "stieg", "vom", "schwar\u00b7zen", "Wald", "ein", "blau\u00b7es", "Wild"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Seele nieder,", "tokens": ["Die", "See\u00b7le", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Da es Nacht war, \u00fcber moosige Stufen ein schneeiger Quell.", "tokens": ["Da", "es", "Nacht", "war", ",", "\u00fc\u00b7ber", "moo\u00b7si\u00b7ge", "Stu\u00b7fen", "ein", "schne\u00b7e\u00b7i\u00b7ger", "Quell", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VAFIN", "$,", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "--+-+-+--+--++--+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Blut und Waffenget\u00fcmmel vergangner Zeiten", "tokens": ["Blut", "und", "Waf\u00b7fen\u00b7ge\u00b7t\u00fcm\u00b7mel", "ver\u00b7gang\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "ADJA", "NN"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Rauscht im F\u00f6hrengrund.", "tokens": ["Rauscht", "im", "F\u00f6h\u00b7ren\u00b7grund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Der Mond scheint leise in verfallene Zimmer,", "tokens": ["Der", "Mond", "scheint", "lei\u00b7se", "in", "ver\u00b7fal\u00b7le\u00b7ne", "Zim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Trunken von dunklen Giften, silberne Larve", "tokens": ["Trun\u00b7ken", "von", "dunk\u00b7len", "Gif\u00b7ten", ",", "sil\u00b7ber\u00b7ne", "Lar\u00b7ve"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "ADJA", "NN"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "\u00dcber den Schlummer der Hirten geneigt;", "tokens": ["\u00dc\u00b7ber", "den", "Schlum\u00b7mer", "der", "Hir\u00b7ten", "ge\u00b7neigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Haupt, das schweigend seine Sagen verlassen.", "tokens": ["Haupt", ",", "das", "schwei\u00b7gend", "sei\u00b7ne", "Sa\u00b7gen", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "O, dann \u00f6ffnet jener die langsamen H\u00e4nde", "tokens": ["O", ",", "dann", "\u00f6ff\u00b7net", "je\u00b7ner", "die", "lang\u00b7sa\u00b7men", "H\u00e4n\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "ADV", "VVFIN", "PDAT", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Verwesend in purpurnem Schlaf", "tokens":