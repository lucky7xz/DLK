{"dta.poem.13155": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ewas man auf Felsen baut, von Stahl und Eisen gie\u00dft,", "tokens": ["\u201e", "was", "man", "auf", "Fel\u00b7sen", "baut", ",", "von", "Stahl", "und", "Ei\u00b7sen", "gie\u00dft", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PIS", "APPR", "NN", "VVFIN", "$,", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "\u201ewird endlich von der Zeit geschliffen und verw\u00fcst.", "tokens": ["\u201e", "wird", "end\u00b7lich", "von", "der", "Zeit", "ge\u00b7schlif\u00b7fen", "und", "ver\u00b7w\u00fcst", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "ADV", "APPR", "ART", "NN", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201edie\u00df Werck hingegen trozt, was st\u00fcrmen kann und wettern;", "tokens": ["\u201e", "die\u00df", "Werck", "hin\u00b7ge\u00b7gen", "trozt", ",", "was", "st\u00fcr\u00b7men", "kann", "und", "wet\u00b7tern", ";"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "NN", "ADV", "VVFIN", "$,", "PWS", "VVINF", "VMFIN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201edie Zeit hat kein Ger\u00e4th dasselbe zu zerschmettern.", "tokens": ["\u201e", "die", "Zeit", "hat", "kein", "Ge\u00b7r\u00e4\u00b7th", "das\u00b7sel\u00b7be", "zu", "zer\u00b7schmet\u00b7tern", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PIAT", "NN", "PDAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "285\u201dEs ist so fest gegr\u00fcndt, mit solcher Kunst gebaut,", "tokens": ["\"", "Es", "ist", "so", "fest", "ge\u00b7gr\u00fcndt", ",", "mit", "sol\u00b7cher", "Kunst", "ge\u00b7baut", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201eda\u00df ihm f\u00fcr keinem Fall als f\u00fcr des Himmels graut.", "tokens": ["\u201e", "da\u00df", "ihm", "f\u00fcr", "kei\u00b7nem", "Fall", "als", "f\u00fcr", "des", "Him\u00b7mels", "graut", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "APPR", "PIAT", "NN", "KOKOM", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201enichts als die Tadelsucht erblickt vielleicht darinnen,", "tokens": ["\u201e", "nichts", "als", "die", "Ta\u00b7del\u00b7sucht", "er\u00b7blickt", "viel\u00b7leicht", "da\u00b7rin\u00b7nen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "KOKOM", "ART", "NN", "VVFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ewas weder ich noch ihr wi\u00dft besser auszusinnen.", "tokens": ["\u201e", "was", "we\u00b7der", "ich", "noch", "ihr", "wi\u00dft", "bes\u00b7ser", "aus\u00b7zu\u00b7sin\u00b7nen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "KON", "PPER", "ADV", "PPER", "VVFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edann findet ihr Gebi\u00df nicht einen Fra\u00df daran,", "tokens": ["\u201e", "dann", "fin\u00b7det", "ihr", "Ge\u00b7bi\u00df", "nicht", "ei\u00b7nen", "Fra\u00df", "da\u00b7ran", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "290\u201dAn dem sie die Begier der Mi\u00dfgunst laben kann;", "tokens": ["\"", "An", "dem", "sie", "die", "Be\u00b7gier", "der", "Mi\u00df\u00b7gunst", "la\u00b7ben", "kann", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "PRELS", "PPER", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201eso bleckt sie doch den Zahn, und s\u00e4ttigt sich mit Schm\u00e4hen,", "tokens": ["\u201e", "so", "bleckt", "sie", "doch", "den", "Zahn", ",", "und", "s\u00e4t\u00b7tigt", "sich", "mit", "Schm\u00e4\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "\u201eauch nur weil sie daran nichts Tadel-werth gesehen.", "tokens": ["\u201e", "auch", "nur", "weil", "sie", "da\u00b7ran", "nichts", "Ta\u00b7del\u00b7werth", "ge\u00b7se\u00b7hen", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "KOUS", "PPER", "PAV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201edes Kummers bin ich frey: sie tadle, l\u00e4stre mich;", "tokens": ["\u201e", "des", "Kum\u00b7mers", "bin", "ich", "frey", ":", "sie", "tad\u00b7le", ",", "l\u00e4st\u00b7re", "mich", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201emein Werck bleibt unverlezt und unver\u00e4nderlich.", "tokens": ["\u201e", "mein", "Werck", "bleibt", "un\u00b7ver\u00b7lezt", "und", "un\u00b7ver\u00b7\u00e4n\u00b7der\u00b7lich", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "295\u201dDes Mondes Pracht erscheint mit unbesorgter Helle,", "tokens": ["\"", "Des", "Mon\u00b7des", "Pracht", "er\u00b7scheint", "mit", "un\u00b7be\u00b7sorg\u00b7ter", "Hel\u00b7le", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "\u201eobgleich der freche Neid nach dessen Schimmer belle.", "tokens": ["\u201e", "ob\u00b7gleich", "der", "fre\u00b7che", "Neid", "nach", "des\u00b7sen", "Schim\u00b7mer", "bel\u00b7le", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "ADJA", "NN", "APPR", "PRELAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "\u201eich r\u00fchme das Geb\u00e4u der K\u00fcnste wegen nicht:", "tokens": ["\u201e", "ich", "r\u00fch\u00b7me", "das", "Ge\u00b7b\u00e4u", "der", "K\u00fcns\u00b7te", "we\u00b7gen", "nicht", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "ART", "NN", "APPR", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "\u201ees ist derselben Hand um keine Zier verpflicht.", "tokens": ["\u201e", "es", "ist", "der\u00b7sel\u00b7ben", "Hand", "um", "kei\u00b7ne", "Zier", "ver\u00b7pflicht", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "PDAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}