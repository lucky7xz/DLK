{"textgrid.poem.63875": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "11.", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hast du nicht so viel genossen, altes Herz, was willst du mehr?", "tokens": ["Hast", "du", "nicht", "so", "viel", "ge\u00b7nos\u00b7sen", ",", "al\u00b7tes", "Herz", ",", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVPP", "$,", "ADJA", "NN", "$,", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Siebzig Jahr sind dir verflossen tr\u00fcb und hell, \u2013 was willst du mehr?", "tokens": ["Sieb\u00b7zig", "Jahr", "sind", "dir", "ver\u00b7flos\u00b7sen", "tr\u00fcb", "und", "hell", ",", "\u2013", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,", "$(", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Hast du doch geliebt, gelitten, redlich deine Pflicht getan,", "tokens": ["Hast", "du", "doch", "ge\u00b7liebt", ",", "ge\u00b7lit\u00b7ten", ",", "red\u00b7lich", "dei\u00b7ne", "Pflicht", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "VVPP", "$,", "ADJD", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Mit den traulichsten Genossen Hand in Hand, \u2013 was willst du mehr?", "tokens": ["Mit", "den", "trau\u00b7lichs\u00b7ten", "Ge\u00b7nos\u00b7sen", "Hand", "in", "Hand", ",", "\u2013", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "APPR", "NN", "$,", "$(", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.5": {"text": "Fr\u00fch ward aufgetan dein Auge, da\u00df du sahst die Niedertracht", "tokens": ["Fr\u00fch", "ward", "auf\u00b7ge\u00b7tan", "dein", "Au\u00b7ge", ",", "da\u00df", "du", "sahst", "die", "Nie\u00b7der\u00b7tracht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "APPR", "PPOSAT", "NN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Dieser Welt und ihrer Possen lachen konntst, \u2013 was willst du mehr?", "tokens": ["Die\u00b7ser", "Welt", "und", "ih\u00b7rer", "Pos\u00b7sen", "la\u00b7chen", "konntst", ",", "\u2013", "was", "willst", "du", "mehr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "KON", "PPOSAT", "NN", "VVINF", "VMFIN", "$,", "$(", "PWS", "VMFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Und an reichbesetzter Tafel durftst du schmausen fr\u00fch und sp\u00e4t,", "tokens": ["Und", "an", "reich\u00b7be\u00b7setz\u00b7ter", "Ta\u00b7fel", "durftst", "du", "schmau\u00b7sen", "fr\u00fch", "und", "sp\u00e4t", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "PPER", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambi