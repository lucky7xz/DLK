{"textgrid.poem.61550": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem See", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nun flie\u00dft die Welt in k\u00fchlem Mondenlicht,", "tokens": ["Nun", "flie\u00dft", "die", "Welt", "in", "k\u00fch\u00b7lem", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Berge sind in wei\u00dfem Duft versunken;", "tokens": ["Die", "Ber\u00b7ge", "sind", "in", "wei\u00b7\u00dfem", "Duft", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der See, der leis um meinen Kahn sich bricht,", "tokens": ["Der", "See", ",", "der", "leis", "um", "mei\u00b7nen", "Kahn", "sich", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "PPOSAT", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Spielt fern hinaus in irren Silberfunken,", "tokens": ["Spielt", "fern", "hin\u00b7aus", "in", "ir\u00b7ren", "Sil\u00b7ber\u00b7fun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch sein Gestad' erkenn' ich nicht.", "tokens": ["Doch", "sein", "Ge\u00b7stad'", "er\u00b7kenn'", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie weit! Wie still! Da schlie\u00dft in mir ein Sinn", "tokens": ["Wie", "weit", "!", "Wie", "still", "!", "Da", "schlie\u00dft", "in", "mir", "ein", "Sinn"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$.", "PWAV", "ADJD", "$.", "ADV", "VVFIN", "APPR", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sich auf, das Unnennbarste zu verstehen;", "tokens": ["Sich", "auf", ",", "das", "Un\u00b7nenn\u00b7bars\u00b7te", "zu", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PTKVZ", "$,", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Uralte Melodieen gehen", "tokens": ["Ur\u00b7al\u00b7te", "Me\u00b7lo\u00b7di\u00b7e\u00b7en", "ge\u00b7hen"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Durch meine Brust ged\u00e4mpft dahin.", "tokens": ["Durch", "mei\u00b7ne", "Brust", "ge\u00b7d\u00e4mpft", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Es sinkt, wie Tau, der Ewigkeit Gedanke", "tokens": ["Es", "sinkt", ",", "wie", "Tau", ",", "der", "E\u00b7wig\u00b7keit", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "$,", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "K\u00fchl schauernd \u00fcber mich und f\u00fcllt mich ganz,", "tokens": ["K\u00fchl", "schau\u00b7ernd", "\u00fc\u00b7ber", "mich", "und", "f\u00fcllt", "mich", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PPER", "KON", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und mich umflutet sonder Schranke", "tokens": ["Und", "mich", "um\u00b7flu\u00b7tet", "son\u00b7der", "Schran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Ein uferloses Meer von wei\u00dfem Glanz.", "tokens": ["Ein", "u\u00b7fer\u00b7lo\u00b7ses", "Meer", "von", "wei\u00b7\u00dfem", "Glanz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nun flie\u00dft die Welt in k\u00fchlem Mondenlicht,", "tokens": ["Nun", "flie\u00dft", "die", "Welt", "in", "k\u00fch\u00b7lem", "Mon\u00b7den\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Berge sind in wei\u00dfem Duft versunken;", "tokens": ["Die", "Ber\u00b7ge", "sind", "in", "wei\u00b7\u00dfem", "Duft", "ver\u00b7sun\u00b7ken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der See, der leis um meinen Kahn sich bricht,", "tokens": ["Der", "See", ",", "der", "leis", "um", "mei\u00b7nen", "Kahn", "sich", "bricht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word