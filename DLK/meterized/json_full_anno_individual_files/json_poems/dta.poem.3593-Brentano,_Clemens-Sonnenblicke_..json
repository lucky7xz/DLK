{"dta.poem.3593": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Sonnenblicke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Sommer und der Sonnenschein,               ", "tokens": ["Der", "Som\u00b7mer", "und", "der", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ganz lieblich mir das Herze mein", "tokens": ["Ganz", "lieb\u00b7lich", "mir", "das", "Her\u00b7ze", "mein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "PDS", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erquicken und erfreuen;", "tokens": ["Er\u00b7qui\u00b7cken", "und", "er\u00b7freu\u00b7en", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df ich mit Lust im gr\u00fcnen Gras", "tokens": ["Da\u00df", "ich", "mit", "Lust", "im", "gr\u00fc\u00b7nen", "Gras"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mag springen an dem Reihen.", "tokens": ["Mag", "sprin\u00b7gen", "an", "dem", "Rei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Des lacht die Allerliebste mein,", "tokens": ["Des", "lacht", "die", "Al\u00b7ler\u00b7liebs\u00b7te", "mein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "VVFIN", "ART", "ADJA", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wollt Gott, ich sollt heut bei ihr seyn,", "tokens": ["Wollt", "Gott", ",", "ich", "sollt", "heut", "bei", "ihr", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PPER", "VMFIN", "ADV", "APPR", "PPOSAT", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In Z\u00fcchten und in Ehren;", "tokens": ["In", "Z\u00fcch\u00b7ten", "und", "in", "Eh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das w\u00e4r meins Herzens gr\u00f6\u00dfte Freud,", "tokens": ["Das", "w\u00e4r", "meins", "Her\u00b7zens", "gr\u00f6\u00df\u00b7te", "Freud", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Darauf darf ich wohl schw\u00f6ren.", "tokens": ["Da\u00b7rauf", "darf", "ich", "wohl", "schw\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Demselben wackren M\u00e4gdelein,", "tokens": ["Dem\u00b7sel\u00b7ben", "wack\u00b7ren", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schickt neulich ich ein Kr\u00e4nzelein,", "tokens": ["Schickt", "neu\u00b7lich", "ich", "ein", "Kr\u00e4n\u00b7zel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit rothem Gold umwunden;", "tokens": ["Mit", "ro\u00b7them", "Gold", "um\u00b7wun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dabei sie mein gedenken sollt,", "tokens": ["Da\u00b7bei", "sie", "mein", "ge\u00b7den\u00b7ken", "sollt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PPOSAT", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu hunderttausend Stunden.", "tokens": ["Zu", "hun\u00b7dert\u00b7tau\u00b7send", "Stun\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich ritt durch einen gr\u00fcnen Wald,", "tokens": ["Ich", "ritt", "durch", "ei\u00b7nen", "gr\u00fc\u00b7nen", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sangen die V\u00f6glein wohlgestalt,", "tokens": ["Da", "san\u00b7gen", "die", "V\u00f6\u00b7glein", "wohl\u00b7ge\u00b7stalt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Frau Nachtigall mit ihnen;", "tokens": ["Frau", "Nach\u00b7ti\u00b7gall", "mit", "ih\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nun singt ihr klein Waldv\u00f6gelein,", "tokens": ["Nun", "singt", "ihr", "klein", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+++-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "Um meines Buhlen willen.", "tokens": ["Um", "mei\u00b7nes", "Buh\u00b7len", "wil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}