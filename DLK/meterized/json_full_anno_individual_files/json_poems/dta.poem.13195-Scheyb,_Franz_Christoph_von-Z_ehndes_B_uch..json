{"dta.poem.13195": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Theresia vernahm den Vortrag zwar mit Huld;", "tokens": ["The\u00b7re\u00b7sia", "ver\u00b7nahm", "den", "Vor\u00b7trag", "zwar", "mit", "Huld", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "150Doch wiese sie den Rath auf einige Geduld,", "tokens": ["wie\u00b7se", "sie", "den", "Rath", "auf", "ei\u00b7ni\u00b7ge", "Ge\u00b7duld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Und sprach: da\u00df er damit nunmehr verziehen sollte,", "tokens": ["Und", "sprach", ":", "da\u00df", "er", "da\u00b7mit", "nun\u00b7mehr", "ver\u00b7zie\u00b7hen", "soll\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "KOUS", "PPER", "PAV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sie von diesem Streit nichts mehr vernehmen wollte.", "tokens": ["Weil", "sie", "von", "die\u00b7sem", "Streit", "nichts", "mehr", "ver\u00b7neh\u00b7men", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "PIS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}