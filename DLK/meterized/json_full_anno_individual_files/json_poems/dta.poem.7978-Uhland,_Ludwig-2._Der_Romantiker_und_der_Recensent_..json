{"dta.poem.7978": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "2.  Der Romantiker und der Recensent .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.71", "sv:0.28"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "B\u00e4nkels\u00e4nger, Hackbretschl\u00e4ger,", "tokens": ["B\u00e4n\u00b7kel\u00b7s\u00e4n\u00b7ger", ",", "Hack\u00b7bret\u00b7schl\u00e4\u00b7ger", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Volk, das Nachts die Stadt durchleiert,", "tokens": ["Volk", ",", "das", "Nachts", "die", "Stadt", "durch\u00b7lei\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nennt sich jetzt der Musen Pfleger;", "tokens": ["Nennt", "sich", "jetzt", "der", "Mu\u00b7sen", "Pfle\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "N\u00e4chstens, wenn Apoll noch feiert,", "tokens": ["N\u00e4chs\u00b7tens", ",", "wenn", "A\u00b7poll", "noch", "fei\u00b7ert", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "NE", "ADV", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Dichten selbst die Schornsteinfeger.", "tokens": ["Dich\u00b7ten", "selbst", "die", "Schorn\u00b7stein\u00b7fe\u00b7ger", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Zeit, wo man mit Wohlbedacht", "tokens": ["Zeit", ",", "wo", "man", "mit", "Wohl\u00b7be\u00b7dacht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PIS", "APPR", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur latein\u2019schen Vers gemacht,", "tokens": ["Nur", "la\u00b7tein'\u00b7schen", "Vers", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Zeit gepuderter Per\u00fccken,", "tokens": ["Zeit", "ge\u00b7pu\u00b7der\u00b7ter", "Pe\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Drauf Pfalzgrafen Lorbern dr\u00fccken,", "tokens": ["Drauf", "Pfalz\u00b7gra\u00b7fen", "Lor\u00b7bern", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Steig auf in der alten Pracht!", "tokens": ["Steig", "auf", "in", "der", "al\u00b7ten", "Pracht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}