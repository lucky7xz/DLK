{"textgrid.poem.53367": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[vnd dieser Mann entgeht mir auch]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vnd dieser Mann entgeht mir auch,", "tokens": ["Vnd", "die\u00b7ser", "Mann", "ent\u00b7geht", "mir", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auch der Freund ist verlohren,", "tokens": ["Auch", "der", "Freund", "ist", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.3": {"text": "Mit dem ich jung nach Liebes-Brauch", "tokens": ["Mit", "dem", "ich", "jung", "nach", "Lie\u00b7bes\u00b7Brauch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich Br\u00fcderlich verschworen:", "tokens": ["Mich", "Br\u00fc\u00b7der\u00b7lich", "ver\u00b7schwo\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wie legt, jhr Leute, hin und her", "tokens": ["Wie", "legt", ",", "jhr", "Leu\u00b7te", ",", "hin", "und", "her"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "$,", "PPOSAT", "NN", "$,", "ADV", "KON", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So unverhofft euch nieder?", "tokens": ["So", "un\u00b7ver\u00b7hofft", "euch", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr wi\u00dft, wer sich dem Todten-Meer", "tokens": ["Ihr", "wi\u00dft", ",", "wer", "sich", "dem", "Tod\u00b7ten\u00b7Meer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PRF", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vertrawet k\u00f6mmt nicht wieder.", "tokens": ["Ver\u00b7tra\u00b7wet", "k\u00f6mmt", "nicht", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Vielleicht ist euch viel besser dort", "tokens": ["Viel\u00b7leicht", "ist", "euch", "viel", "bes\u00b7ser", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als auff der schn\u00f6den Erden,", "tokens": ["Als", "auff", "der", "schn\u00f6\u00b7den", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Di\u00df spricht die Schrifft, ihr wares Wort", "tokens": ["Di\u00df", "spricht", "die", "Schrifft", ",", "ihr", "wa\u00b7res", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "NN", "$,", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan nicht geleugnet werden.", "tokens": ["Kan", "nicht", "ge\u00b7leug\u00b7net", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVPP", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Wol euch, ihr wi\u00dft woran ihr seyd,", "tokens": ["Wol", "euch", ",", "ihr", "wi\u00dft", "wo\u00b7ran", "ihr", "seyd", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "PPER", "VVFIN", "PWAV", "PPER", "VAFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Seyd selig durchgedrungen,", "tokens": ["Seyd", "se\u00b7lig", "durch\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Schwebt in der g\u00fcldnen Ewigkeit", "tokens": ["Schwebt", "in", "der", "g\u00fcld\u00b7nen", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd singt mit Engel-Zungen.", "tokens": ["Vnd", "singt", "mit", "En\u00b7gel\u00b7Zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ihr la\u00dft euch heilig das Gebein", "tokens": ["Ihr", "la\u00dft", "euch", "hei\u00b7lig", "das", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die stillen Gr\u00e4ber decken,", "tokens": ["Die", "stil\u00b7len", "Gr\u00e4\u00b7ber", "de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fiel' auch der Himmel zehnmal ein", "tokens": ["Fiel'", "auch", "der", "Him\u00b7mel", "zehn\u00b7mal", "ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es wird euch nicht erschrecken,", "tokens": ["Es", "wird", "euch", "nicht", "er\u00b7schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Wir aber m\u00fcssen nach wie vor", "tokens": ["Wir", "a\u00b7ber", "m\u00fcs\u00b7sen", "nach", "wie", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "APPR", "KOKOM", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An dieser Erden kleben,", "tokens": ["An", "die\u00b7ser", "Er\u00b7den", "kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wolten gern das Hertz empor", "tokens": ["Vnd", "wol\u00b7ten", "gern", "das", "Hertz", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dahin wo ihr seyd heben.", "tokens": ["Da\u00b7hin", "wo", "ihr", "seyd", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PWAV", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-++--", "measure": "unknown.measure.tri"}}, "stanza.7": {"line.1": {"text": "Die Last des Leibes ist zu schwer", "tokens": ["Die", "Last", "des", "Lei\u00b7bes", "ist", "zu", "schwer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd h\u00e4lt den Geist gefangen", "tokens": ["Vnd", "h\u00e4lt", "den", "Geist", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie l\u00e4hmt die Fl\u00fcgel ihm, da\u00df er", "tokens": ["Sie", "l\u00e4hmt", "die", "Fl\u00fc\u00b7gel", "ihm", ",", "da\u00df", "er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bleibt an der Erden hangen;", "tokens": ["Bleibt", "an", "der", "Er\u00b7den", "han\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie am Gev\u00f6gel wird gesp\u00fcrt,", "tokens": ["Wie", "am", "Ge\u00b7v\u00f6\u00b7gel", "wird", "ge\u00b7sp\u00fcrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das klebt mit dem Gefieder,", "tokens": ["Das", "klebt", "mit", "dem", "Ge\u00b7fie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wenn es der Stangen Leim ber\u00fchrt,", "tokens": ["Wenn", "es", "der", "Stan\u00b7gen", "Leim", "be\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd flattert hin und wieder.", "tokens": ["Vnd", "flat\u00b7tert", "hin", "und", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Der gibt sich gantz dem Ehrgeitz hin,", "tokens": ["Der", "gibt", "sich", "gantz", "dem", "Ehr\u00b7geitz", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der l\u00e4\u00dft den Zorn sich blenden,", "tokens": ["Der", "l\u00e4\u00dft", "den", "Zorn", "sich", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Geld der, der den Eigen-Sinn,", "tokens": ["Das", "Geld", "der", ",", "der", "den", "Ei\u00b7gen\u00b7Sinn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "$,", "PRELS", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der andre Lust sich sch\u00e4nden.", "tokens": ["Der", "and\u00b7re", "Lust", "sich", "sch\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Wenn bricht sie an die sch\u00f6ne Zeit", "tokens": ["Wenn", "bricht", "sie", "an", "die", "sch\u00f6\u00b7ne", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die von den eiteln Sachen", "tokens": ["Die", "von", "den", "ei\u00b7teln", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd dieser strengen Dienstbarkeit", "tokens": ["Vnd", "die\u00b7ser", "stren\u00b7gen", "Dienst\u00b7bar\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vns ewig frey sol machen?", "tokens": ["Vns", "e\u00b7wig", "frey", "sol", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Wo, Bruder, du anjetzund bist,", "tokens": ["Wo", ",", "Bru\u00b7der", ",", "du", "an\u00b7jet\u00b7zund", "bist", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "NN", "$,", "PPER", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da kein Betrug der Erden,", "tokens": ["Da", "kein", "Be\u00b7trug", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd keiner schn\u00f6den S\u00fcnden List", "tokens": ["Vnd", "kei\u00b7ner", "schn\u00f6\u00b7den", "S\u00fcn\u00b7den", "List"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kan deiner m\u00e4chtig werden?", "tokens": ["Kan", "dei\u00b7ner", "m\u00e4ch\u00b7tig", "wer\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Du schwebest in der stoltzen Rhu", "tokens": ["Du", "schwe\u00b7best", "in", "der", "stolt\u00b7zen", "Rhu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd siehst herab von oben,", "tokens": ["Vnd", "siehst", "her\u00b7ab", "von", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wie Gl\u00fcck und Thorheit immerzu", "tokens": ["Wie", "Gl\u00fcck", "und", "Thor\u00b7heit", "im\u00b7mer\u00b7zu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erb\u00e4rmlich mit uns toben,", "tokens": ["Er\u00b7b\u00e4rm\u00b7lich", "mit", "uns", "to\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Was uns bekriegt vor Angst und Weh,", "tokens": ["Was", "uns", "be\u00b7kriegt", "vor", "Angst", "und", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als die am Ufer stehen,", "tokens": ["Als", "die", "am", "U\u00b7fer", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd sehn, wie auff ergrimmter See", "tokens": ["Vnd", "sehn", ",", "wie", "auff", "er\u00b7grimm\u00b7ter", "See"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schiff mu\u00df untergehen.", "tokens": ["Ein", "Schiff", "mu\u00df", "un\u00b7ter\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Du hieltst dich in der Jugend hart", "tokens": ["Du", "hieltst", "dich", "in", "der", "Ju\u00b7gend", "hart"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warst embsig aller massen,", "tokens": ["Warst", "emb\u00b7sig", "al\u00b7ler", "mas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hast keiner Arbeit je gespart,", "tokens": ["Hast", "kei\u00b7ner", "Ar\u00b7beit", "je", "ge\u00b7spart", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Rechte wol zu fassen,", "tokens": ["Die", "Rech\u00b7te", "wol", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Darnach hast du getrew gef\u00fchrt", "tokens": ["Dar\u00b7nach", "hast", "du", "ge\u00b7trew", "ge\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bedruckter Leute Sachen,", "tokens": ["Be\u00b7druck\u00b7ter", "Leu\u00b7te", "Sa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Man hat dich, h\u00f6r' ich, nicht gesp\u00fcrt", "tokens": ["Man", "hat", "dich", ",", "h\u00f6r'", "ich", "