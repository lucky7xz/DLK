{"textgrid.poem.37060": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An eine Tobakspfeife", "genre": "verse", "period": "N.A.", "pub_year": 1772, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, braune Pfeife, die du dem z\u00f6gernden", "tokens": ["Dir", ",", "brau\u00b7ne", "Pfei\u00b7fe", ",", "die", "du", "dem", "z\u00f6\u00b7gern\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "ADJA"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Decemberabend schnellere Flucht gebeutst,", "tokens": ["De\u00b7cem\u00b7ber\u00b7a\u00b7bend", "schnel\u00b7le\u00b7re", "Flucht", "ge\u00b7beutst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Vertraute meiner Einsamkeiten,", "tokens": ["Ver\u00b7trau\u00b7te", "mei\u00b7ner", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Will ich ein Fidibusopfer bringen:", "tokens": ["Will", "ich", "ein", "Fi\u00b7di\u00b7bu\u00b7sop\u00b7fer", "brin\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Dies ganze B\u00fcndel, das mir mein Agathon", "tokens": ["Dies", "gan\u00b7ze", "B\u00fcn\u00b7del", ",", "das", "mir", "mein", "A\u00b7gat\u00b7hon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "ADJA", "NN", "$,", "PRELS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aus schalen Reimen, Bibliotheken und", "tokens": ["Aus", "scha\u00b7len", "Rei\u00b7men", ",", "Bib\u00b7lio\u00b7the\u00b7ken", "und"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Roman drehte. Schwelgt, ihr Flammen,", "tokens": ["Ro\u00b7man", "dreh\u00b7te", ".", "Schwelgt", ",", "ihr", "Flam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "An den ertr\u00e4umeten Ewigkeiten!", "tokens": ["An", "den", "er\u00b7tr\u00e4u\u00b7me\u00b7ten", "E\u00b7wig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Bestraft den Narren, welcher ins Waffenfeld,", "tokens": ["Be\u00b7straft", "den", "Nar\u00b7ren", ",", "wel\u00b7cher", "ins", "Waf\u00b7fen\u00b7feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit G\u00e4nsespulen stattlich bewaffnet, zog,", "tokens": ["Mit", "G\u00e4n\u00b7ses\u00b7pu\u00b7len", "statt\u00b7lich", "be\u00b7waff\u00b7net", ",", "zog", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Tumult aus ehrnem Rachen br\u00fcllte,", "tokens": ["Tu\u00b7mult", "aus", "ehr\u00b7nem", "Ra\u00b7chen", "br\u00fcll\u00b7te", ","], "t