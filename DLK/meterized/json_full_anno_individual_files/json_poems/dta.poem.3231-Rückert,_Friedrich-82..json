{"dta.poem.3231": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "82.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Umsonst ereiferst du dich gegen etwas heftig,", "tokens": ["Um\u00b7sonst", "er\u00b7ei\u00b7ferst", "du", "dich", "ge\u00b7gen", "et\u00b7was", "hef\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das todt f\u00fcr dich, doch f\u00fcr die Welt ist zauberkr\u00e4ftig.", "tokens": ["Das", "todt", "f\u00fcr", "dich", ",", "doch", "f\u00fcr", "die", "Welt", "ist", "zau\u00b7ber\u00b7kr\u00e4f\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Wirkliches ist da, das Wirkungen verk\u00fcnden,", "tokens": ["Ein", "Wirk\u00b7li\u00b7ches", "ist", "da", ",", "das", "Wir\u00b7kun\u00b7gen", "ver\u00b7k\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "$,", "ART", "NN", "VVINF", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Nicht l\u00e4ugnen magst du es, nur suchen zu ergr\u00fcnden.", "tokens": ["Nicht", "l\u00e4ug\u00b7nen", "magst", "du", "es", ",", "nur", "su\u00b7chen", "zu", "er\u00b7gr\u00fcn\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VMFIN", "PPER", "PPER", "$,", "ADV", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ob es ein wei\u00dfer nun, ob schwarzer Zauber sei,", "tokens": ["Ob", "es", "ein", "wei\u00b7\u00dfer", "nun", ",", "ob", "schwar\u00b7zer", "Zau\u00b7ber", "sei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "$,", "KOUS", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Begreifen mu\u00dft du ihn, so bist du zauberfrei.", "tokens": ["Be\u00b7grei\u00b7fen", "mu\u00dft", "du", "ihn", ",", "so", "bist", "du", "zau\u00b7ber\u00b7frei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "$,", "ADV", "VAFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}