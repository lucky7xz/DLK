{"textgrid.poem.25993": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Mein Stuhl steht im Himmel", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein Stuhl steht im Himmel, wenn ich an dich denke.", "tokens": ["Mein", "Stuhl", "steht", "im", "Him\u00b7mel", ",", "wenn", "ich", "an", "dich", "den\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPRART", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+--+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sitze bei mir und lege deinen Schmuck in mein Herz,", "tokens": ["Sit\u00b7ze", "bei", "mir", "und", "le\u00b7ge", "dei\u00b7nen", "Schmuck", "in", "mein", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+------+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Du sollst in meinen Augen dich beschauen, wie sch\u00f6n du bist.", "tokens": ["Du", "sollst", "in", "mei\u00b7nen", "Au\u00b7gen", "dich", "be\u00b7schau\u00b7en", ",", "wie", "sch\u00f6n", "du", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,", "PWAV", "ADJD", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein L\u00e4cheln hat H\u00e4nde und beschenkt mich reich.", "tokens": ["Dein", "L\u00e4\u00b7cheln", "hat", "H\u00e4n\u00b7de", "und", "be\u00b7schenkt", "mich", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "NN", "KON", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich gehe vor dir wie ein selig Gestorbener,", "tokens": ["Ich", "ge\u00b7he", "vor", "dir", "wie", "ein", "se\u00b7lig", "Ge\u00b7stor\u00b7be\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "KOKOM", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mein Herz steht still und feiert.", "tokens": ["Mein", "Herz", "steht", "still", "und", "fei\u00b7ert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Feuer, das auf den Scheitern sich wiegt,", "tokens": ["Ein", "Feu\u00b7er", ",", "das", "auf", "den", "Schei\u00b7tern", "sich", "wiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Liegt dein Auge auf mir, meine F\u00fc\u00dfe sind Stahl, ich bin dein Schatten,", "tokens": ["Liegt", "dein", "Au\u00b7ge", "auf", "mir", ",", "mei\u00b7ne", "F\u00fc\u00b7\u00dfe", "sind", "Stahl", ",", "ich", "bin", "dein", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+--+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.3": {"text": "Ich folge dir ohne Ermatten und o