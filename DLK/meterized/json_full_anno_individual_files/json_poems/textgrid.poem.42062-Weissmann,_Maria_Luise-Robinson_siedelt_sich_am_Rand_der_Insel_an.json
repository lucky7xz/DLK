{"textgrid.poem.42062": {"metadata": {"author": {"name": "Weissmann, Maria Luise", "birth": "N.A.", "death": "N.A."}, "title": "Robinson siedelt sich am Rand der Insel an", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und mit dem Wort wars, da\u00df er dann begann", "tokens": ["Und", "mit", "dem", "Wort", "wars", ",", "da\u00df", "er", "dann", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die lange Zwiesprach mit sich selbst. Denn, sieh,", "tokens": ["Die", "lan\u00b7ge", "Zwie\u00b7sprach", "mit", "sich", "selbst", ".", "Denn", ",", "sieh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PRF", "ADV", "$.", "KON", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er war allein. Er war im Ozean", "tokens": ["Er", "war", "al\u00b7lein", ".", "Er", "war", "im", "O\u00b7ze\u00b7an"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "$.", "PPER", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein kleines Eiland. Und es kam ihm nie", "tokens": ["Ein", "klei\u00b7nes", "Ei\u00b7land", ".", "Und", "es", "kam", "ihm", "nie"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Von drau\u00dfen Antwort. Keine Flucht gelang.", "tokens": ["Von", "drau\u00b7\u00dfen", "Ant\u00b7wort", ".", "Kei\u00b7ne", "Flucht", "ge\u00b7lang", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$.", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch blieb ihm Sehnsucht stets. Er mochte tauschen", "tokens": ["Doch", "blieb", "ihm", "Sehn\u00b7sucht", "stets", ".", "Er", "moch\u00b7te", "tau\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "VVFIN", "ADV", "$.", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den kargen Strand nicht um den Traubenhang", "tokens": ["Den", "kar\u00b7gen", "Strand", "nicht", "um", "den", "Trau\u00b7ben\u00b7hang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der reichen Gr\u00fcnde, denn ihm war das Rauschen", "tokens": ["Der", "rei\u00b7chen", "Gr\u00fcn\u00b7de", ",", "denn", "ihm", "war", "das", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Meeres Hoffnung. So wuchs er am Rand", "tokens": ["Des", "Mee\u00b7res", "Hoff\u00b7nung", ".", "So", "wuchs", "er", "am", "Rand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$.", "ADV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Von Erde, Wasser, von sich selbst. Es fingen", "tokens": ["Von", "Er\u00b7de", ",", "Was\u00b7ser", ",", "von", "sich", "selbst", ".", "Es", "fin\u00b7gen"], "token_info": ["word", "word", "punct", "word", "pu