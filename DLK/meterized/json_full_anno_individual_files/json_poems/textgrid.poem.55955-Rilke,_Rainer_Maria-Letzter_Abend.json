{"textgrid.poem.55955": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Letzter Abend", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und Nacht und fernes Fahren; denn der Train", "tokens": ["Und", "Nacht", "und", "fer\u00b7nes", "Fah\u00b7ren", ";", "denn", "der", "Train"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "des ganzen Heeres zog am Park vor\u00fcber.", "tokens": ["des", "gan\u00b7zen", "Hee\u00b7res", "zog", "am", "Park", "vor\u00b7\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Er aber hob den Blick vom Clavecin", "tokens": ["Er", "a\u00b7ber", "hob", "den", "Blick", "vom", "Cla\u00b7ve\u00b7cin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ART", "NN", "APPRART", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "und spielte noch und sah zu ihr hin\u00fcber", "tokens": ["und", "spiel\u00b7te", "noch", "und", "sah", "zu", "ihr", "hin\u00b7\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "PPOSAT", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "beinah wie man in einen Spiegel schaut:", "tokens": ["bei\u00b7nah", "wie", "man", "in", "ei\u00b7nen", "Spie\u00b7gel", "schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "PIS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "so sehr erf\u00fcllt von seinen jungen Z\u00fcgen", "tokens": ["so", "sehr", "er\u00b7f\u00fcllt", "von", "sei\u00b7nen", "jun\u00b7gen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wissend, wie sie seine Trauer tr\u00fcgen,", "tokens": ["und", "wis\u00b7send", ",", "wie", "sie", "sei\u00b7ne", "Trau\u00b7er", "tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "sch\u00f6n und verf\u00fchrender bei jedem Laut.", "tokens": ["sch\u00f6n", "und", "ver\u00b7f\u00fch\u00b7ren\u00b7der", "bei", "je\u00b7dem", "Laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPR", "PIAT", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.3": {"line.1": {"text": "Doch pl\u00f6tzlich wars, als ob sich das verwische:", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "wars", ",", "als", "ob", "sich", "das", "ver\u00b7wi\u00b7sche", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "$,", "KOKOM", "KOUS", "PRF", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "sie stand wie m\u00fchsam in der Fensternische", "tokens": ["sie", "stand", "wie", "m\u00fch\u00b7sam", "in", "der", "Fens\u00b7ter\u00b7ni\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und hielt des Herzens dr\u00e4ngendes Geklopf.", "tokens": ["und", "hielt", "des", "Her\u00b7zens", "dr\u00e4n\u00b7gen\u00b7des", "Ge\u00b7klopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sein Spiel gab nach. Von drau\u00dfen wehte Frische.", "tokens": ["Sein", "Spiel", "gab", "nach", ".", "Von", "drau\u00b7\u00dfen", "weh\u00b7te", "Fri\u00b7sche", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "APPR", "ADV", "VVFIN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und seltsam fremd stand auf dem Spiegeltische", "tokens": ["Und", "selt\u00b7sam", "fremd", "stand", "auf", "dem", "Spie\u00b7gel\u00b7ti\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "der schwarze Tschako mit dem Totenkopf.", "tokens": ["der", "schwar\u00b7ze", "Tscha\u00b7ko", "mit", "dem", "To\u00b7ten\u00b7kopf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und Nacht und fernes Fahren; denn der Train", "tokens": ["Und", "Nacht", "und", "fer\u00b7nes", "Fah\u00b7ren", ";", "denn", "der", "Train"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "KON", "ADJA", "NN", "$.", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "des ganzen Heeres zog am Park vor\u00fcber.", "tokens": ["