{"textgrid.poem.53362": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[dv Menschen-Kind, erschrick]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv Menschen-Kind, erschrick", "tokens": ["Dv", "Men\u00b7schen\u00b7Kind", ",", "er\u00b7schrick"], "token_info": ["word", "word", "punct", "word"], "pos": ["NE", "NN", "$,", "XY"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Vnd wach all' Augenblick,", "tokens": ["Vnd", "wach", "all'", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "La\u00df sichre Leute schertzen,", "tokens": ["La\u00df", "sich\u00b7re", "Leu\u00b7te", "schert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Horch du in deinem Hertzen", "tokens": ["Horch", "du", "in", "dei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was deine Glocke schl\u00e4gt,", "tokens": ["Was", "dei\u00b7ne", "Glo\u00b7cke", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Dein Puls der sich bewegt,", "tokens": ["Dein", "Puls", "der", "sich", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PRF", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Ob auch sein letzter Schlag", "tokens": ["Ob", "auch", "sein", "letz\u00b7ter", "Schlag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dir deinen J\u00fcngsten Tag,", "tokens": ["Dir", "dei\u00b7nen", "J\u00fcng\u00b7sten", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Den Lebens-Abend, tr\u00e4gt.", "tokens": ["Den", "Le\u00b7bens\u00b7A\u00b7bend", ",", "tr\u00e4gt", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Betreug dich selber nicht,", "tokens": ["Be\u00b7treug", "dich", "sel\u00b7ber", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Dein j\u00fcngstes Hal\u00df-Gericht", "tokens": ["Dein", "j\u00fcngs\u00b7tes", "Ha\u00b7l\u00df\u00b7Ge\u00b7richt"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wird dann nicht erst ergehen,", "tokens": ["Wird", "dann", "nicht", "erst", "er\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wann wir gesampt erstehen,", "tokens": ["Wann", "wir", "ge\u00b7sampt", "er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wann selbst der Sonnen Tracht", "tokens": ["Wann", "selbst", "der", "Son\u00b7nen", "Tracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der K\u00f6nigreiche Pracht,", "tokens": ["Der", "K\u00f6\u00b7nig\u00b7rei\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Lufft, Himmel, Erd vnd Fluth", "tokens": ["Lufft", ",", "Him\u00b7mel", ",", "Erd", "vnd", "Fluth"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Durch die gemeine Glut", "tokens": ["Durch", "die", "ge\u00b7mei\u00b7ne", "Glut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Zu Rauch' wird seyn gemacht.", "tokens": ["Zu", "Rauch'", "wird", "seyn", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Wir sind vns selbst die Welt,", "tokens": ["Wir", "sind", "vns", "selbst", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Wenn vnsre H\u00fctte f\u00e4llt", "tokens": ["Wenn", "vns\u00b7re", "H\u00fct\u00b7te", "f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vnd wil nun in der Erden", "tokens": ["Vnd", "wil", "nun", "in", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Staub vnd Asche werden,", "tokens": ["Zu", "Staub", "vnd", "A\u00b7sche", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Stracks wird vns vnser Lohn,", "tokens": ["Stracks", "wird", "vns", "vn\u00b7ser", "Lohn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Den tragen wir davon", "tokens": ["Den", "tra\u00b7gen", "wir", "da\u00b7von"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "PAV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Von jenem Richter-Stul", "tokens": ["Von", "je\u00b7nem", "Rich\u00b7ter\u00b7Stul"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Dort der der Hellen Pful,", "tokens": ["Dort", "der", "der", "Hel\u00b7len", "Pful", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ART", "NN", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.9": {"text": "Der hie die Himmels-Krohn'.", "tokens": ["Der", "hie", "die", "Him\u00b7mels\u00b7Krohn'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ergetzt dich nun voraus", "tokens": ["Er\u00b7getzt", "dich", "nun", "vo\u00b7raus"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PTKVZ"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Der ewign Frewden Haus,", "tokens": ["Der", "e\u00b7wign", "Frew\u00b7den", "Haus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Seelen wahres Leben,", "tokens": ["Der", "See\u00b7len", "wah\u00b7res", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Hier mustu darnach streben", "tokens": ["Hier", "mus\u00b7tu", "dar\u00b7nach", "stre\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PAV", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durch stete Rew vnd Buss'", "tokens": ["Durch", "ste\u00b7te", "Rew", "vnd", "Buss'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd durch der Thr\u00e4nen Flu\u00df,", "tokens": ["Vnd", "durch", "der", "Thr\u00e4\u00b7nen", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Must durch des Glaubens Macht", "tokens": ["Must", "durch", "des", "Glau\u00b7bens", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die Welt sampt jhrem Pracht", "tokens": ["Die", "Welt", "sampt", "jhrem", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Thun vnter deinen Fu\u00df.", "tokens": ["Thun", "vn\u00b7ter", "dei\u00b7nen", "Fu\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zeuchst du der Hellen Qual", "tokens": ["Zeuchst", "du", "der", "Hel\u00b7len", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Weit vor dem Himmels-Saal,", "tokens": ["Weit", "vor", "dem", "Him\u00b7mels\u00b7Saal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "So la\u00df dich immer finden", "tokens": ["So", "la\u00df", "dich", "im\u00b7mer", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im Grewel deiner S\u00fcnden:", "tokens": ["Im", "Gre\u00b7wel", "dei\u00b7ner", "S\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wi\u00df aber, da\u00df dein Leid", "tokens": ["Wi\u00df", "a\u00b7ber", ",", "da\u00df", "dein", "Leid"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "ADV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Nicht wehrt nur kurtze Zeit,", "tokens": ["Nicht", "wehrt", "nur", "kurt\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "O nein, der Flammen Sturm", "tokens": ["O", "nein", ",", "der", "Flam\u00b7men", "Sturm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PTKANT", "$,", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Vnd dein Gewissens-Wurm", "tokens": ["Vnd", "dein", "Ge\u00b7wis\u00b7sens\u00b7Wurm"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Stirbt nicht in Ewigkeit.", "tokens": ["Stirbt", "nicht", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Zehntausent Jahr ist viel,", "tokens": ["Zehn\u00b7tau\u00b7sent", "Jahr", "ist", "viel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Doch haben sie ein Ziel,", "tokens": ["Doch", "ha\u00b7ben", "sie", "ein", "Ziel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ob hunderttausent w\u00e4ren,", "tokens": ["Ob", "hun\u00b7dert\u00b7tau\u00b7sent", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Zeit kan sie verzehren,", "tokens": ["Die", "Zeit", "kan", "sie", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Ewigkeit allein", "tokens": ["Die", "E\u00b7wig\u00b7keit", "al\u00b7lein"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Vnd jhrer Flammen Pein,", "tokens": ["Vnd", "jhrer", "Flam\u00b7men", "Pein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Der hier ich umb vnd an", "tokens": ["Der", "hier", "ich", "umb", "vnd", "an"], "token_info": ["word", "word", "wor