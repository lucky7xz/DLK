{"dta.poem.8668": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Mein Stern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Um meine wunde Brust geschlagen", "tokens": ["Um", "mei\u00b7ne", "wun\u00b7de", "Brust", "ge\u00b7schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Mantel der Melancholei,", "tokens": ["Den", "Man\u00b7tel", "der", "Me\u00b7lan\u00b7cho\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Flog ich, vom Lebenssturm getragen,", "tokens": ["Flog", "ich", ",", "vom", "Le\u00b7bens\u00b7sturm", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An dir, du Herrliche, vorbei.", "tokens": ["An", "dir", ",", "du", "Herr\u00b7li\u00b7che", ",", "vor\u00b7bei", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Vom Himmel deiner Augen stiegen", "tokens": ["Vom", "Him\u00b7mel", "dei\u00b7ner", "Au\u00b7gen", "stie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Engel Thr\u00e4nen niederw\u00e4rts", "tokens": ["Wie", "En\u00b7gel", "Thr\u00e4\u00b7nen", "nie\u00b7der\u00b7w\u00e4rts"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NE", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An deinen holdger\u00fchrten Z\u00fcgen,", "tokens": ["An", "dei\u00b7nen", "hold\u00b7ge\u00b7r\u00fchr\u00b7ten", "Z\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und priesen mir dein gutes Herz.", "tokens": ["Und", "prie\u00b7sen", "mir", "dein", "gu\u00b7tes", "Herz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und alle Welten um mich schwanden,", "tokens": ["Und", "al\u00b7le", "Wel\u00b7ten", "um", "mich", "schwan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben starrt' in seinem Lauf,", "tokens": ["Mein", "Le\u00b7ben", "starrt'", "in", "sei\u00b7nem", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Im s\u00fc\u00dfemp\u00f6rten Busen standen", "tokens": ["Im", "s\u00fc\u00b7\u00dfem\u00b7p\u00f6r\u00b7ten", "Bu\u00b7sen", "stan\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die alten G\u00f6tter wieder auf.", "tokens": ["Die", "al\u00b7ten", "G\u00f6t\u00b7ter", "wie\u00b7der", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da ri\u00df der Sturm von dir mich wieder", "tokens": ["Da", "ri\u00df", "der", "Sturm", "von", "dir", "mich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hinaus in seine w\u00fcste Nacht,", "tokens": ["Hin\u00b7aus", "in", "sei\u00b7ne", "w\u00fcs\u00b7te", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch strahlt nun Frieden auf mich nieder", "tokens": ["Doch", "strahlt", "nun", "Frie\u00b7den", "auf", "mich", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "NN", "APPR", "PPER", "PTKVZ"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Stern mit ewig heller Pracht.", "tokens": ["Ein", "Stern", "mit", "e\u00b7wig", "hel\u00b7ler", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Denn wie, vom Tode schon umfangen,", "tokens": ["Denn", "wie", ",", "vom", "To\u00b7de", "schon", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der J\u00fcngling nach der holden Braut", "tokens": ["Der", "J\u00fcng\u00b7ling", "nach", "der", "hol\u00b7den", "Braut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Arme streckt mit Glutverlangen,", "tokens": ["Die", "Ar\u00b7me", "streckt", "mit", "Glut\u00b7ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sterbend ihr ins Auge schaut:", "tokens": ["Und", "ster\u00b7bend", "ihr", "ins", "Au\u00b7ge", "schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So griff nach deinem holden Bilde", "tokens": ["So", "griff", "nach", "dei\u00b7nem", "hol\u00b7den", "Bil\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Seele, schaut es ewig an,", "tokens": ["Die", "See\u00b7le", ",", "schaut", "es", "e\u00b7wig", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieht nichts vom tr\u00fcben Erdgefilde,", "tokens": ["Sieht", "nichts", "vom", "tr\u00fc\u00b7ben", "Erd\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fchlt nicht die Dornen ihrer Bahn.", "tokens": ["F\u00fchlt", "nicht", "die", "Dor\u00b7nen", "ih\u00b7rer", "Bahn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Entriss' auch einst der Tod mir strenge,", "tokens": ["Ent\u00b7riss'", "auch", "einst", "der", "Tod", "mir", "stren\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was mir das Leben Liebes gab;", "tokens": ["Was", "mir", "das", "Le\u00b7ben", "Lie\u00b7bes", "gab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er nehm' es hin! doch Eines r\u00e4nge \u2014", "tokens": ["Er", "nehm'", "es", "hin", "!", "doch", "Ei\u00b7nes", "r\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "ADV", "PIS", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich r\u00e4nge k\u00fchn dein Bild ihm ab.", "tokens": ["Ich", "r\u00e4n\u00b7ge", "k\u00fchn", "dein", "Bild", "ihm", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPOSAT", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}