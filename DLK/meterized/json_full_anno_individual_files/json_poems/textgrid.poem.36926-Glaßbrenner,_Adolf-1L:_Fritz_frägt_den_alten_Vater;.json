unct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinunter mit den Ketten,", "tokens": ["Hin\u00b7un\u00b7ter", "mit", "den", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So weit die Sonne lacht!", "tokens": ["So", "weit", "die", "Son\u00b7ne", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Fort mit den staub'gen B\u00fcchern,", "tokens": ["Fort", "mit", "den", "staub'\u00b7gen", "B\u00fc\u00b7chern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fort mit der Wissenschaft!", "tokens": ["Fort", "mit", "der", "Wis\u00b7sen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Der Freiheit will ich opfern", "tokens": ["Der", "Frei\u00b7heit", "will", "ich", "op\u00b7fern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens Thatenkraft!", "tokens": ["Des", "Le\u00b7bens", "Tha\u00b7ten\u00b7kraft", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es ruft mich in die Schranken,", "tokens": ["Es", "ruft", "mich", "in", "die", "Schran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"]