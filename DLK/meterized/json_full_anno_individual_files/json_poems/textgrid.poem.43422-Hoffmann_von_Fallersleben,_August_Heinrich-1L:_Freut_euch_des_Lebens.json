{"textgrid.poem.43422": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freut euch des Lebens", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u00b7rer", "Jour\u00b7na\u00b7le", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind sie gestorben,", "tokens": ["Sind", "sie", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freut euch noch mehr!", "tokens": ["Freut", "euch", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Bek\u00fcmmert ihr euch gar nicht drum,", "tokens": ["Be\u00b7k\u00fcm\u00b7mert", "ihr", "euch", "gar", "nicht", "drum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "PTKNEG", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das nehmen sie gewaltig krumm,", "tokens": ["Das", "neh\u00b7men", "sie", "ge\u00b7wal\u00b7tig", "krumm", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und machen euch herunter dann,", "tokens": ["Und", "ma\u00b7chen", "euch", "her\u00b7un\u00b7ter", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "So oft und gut es geht.", "tokens": ["So", "oft", "und", "gut", "es", "geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KON", "ADJD", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u00b7rer", "Jour\u00b7na\u00b7le", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind sie gestorben,", "tokens": ["Sind", "sie", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freut euch noch mehr!", "tokens": ["Freut", "euch", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Doch wollt ihr Mitarbeiter sein,", "tokens": ["Doch", "wollt", "ihr", "Mit\u00b7ar\u00b7bei\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Schickt manchmal einen Beitrag ein,", "tokens": ["Schickt", "manch\u00b7mal", "ei\u00b7nen", "Bei\u00b7trag", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So kauft ihr euch in ihre Zunft", "tokens": ["So", "kauft", "ihr", "euch", "in", "ih\u00b7re", "Zunft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und seid dann hochgeehrt.", "tokens": ["Und", "seid", "dann", "hoch\u00b7geehrt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u00b7rer", "Jour\u00b7na\u00b7le", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind sie gestorben,", "tokens": ["Sind", "sie", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freut euch noch mehr!", "tokens": ["Freut", "euch", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Ihr m\u00f6gt dann schreiben was ihr schreibt,", "tokens": ["Ihr", "m\u00f6gt", "dann", "schrei\u00b7ben", "was", "ihr", "schreibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn ihr der Zunft nur treu verbleibt,", "tokens": ["Wenn", "ihr", "der", "Zunft", "nur", "treu", "ver\u00b7bleibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So seid ihr, wie's noch keiner war:", "tokens": ["So", "seid", "ihr", ",", "wie's", "noch", "kei\u00b7ner", "war", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "VVFIN", "ADV", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Neu, eigenth\u00fcmlich, gro\u00df!", "tokens": ["Neu", ",", "ei\u00b7gent\u00b7h\u00fcm\u00b7lich", ",", "gro\u00df", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u00b7rer", "Jour\u00b7na\u00b7le", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind Sie gestorben,", "tokens": ["Sind", "Sie", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freut euch noch mehr!", "tokens": ["Freut", "euch", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Thut ein Journal von euch das dar,", "tokens": ["Thut", "ein", "Jour\u00b7nal", "von", "euch", "das", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "APPR", "PPER", "PDS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So folgt ihm der Journale Schaar,", "tokens": ["So", "folgt", "ihm", "der", "Jour\u00b7na\u00b7le", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wie die Heerde Schafe geht", "tokens": ["So", "wie", "die", "Heer\u00b7de", "Scha\u00b7fe", "geht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wohin's dem Sch\u00f6ps beliebt.", "tokens": ["Wo\u00b7hin's", "dem", "Sch\u00f6ps", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u00b7rer", "Jour\u00b7na\u00b7le", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind sie gestorben,", "tokens": ["Sind", "sie", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freut euch noch mehr!", "tokens": ["Freut", "euch", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Das Lob gebrauchen sie f\u00fcr sich", "tokens": ["Das", "Lob", "ge\u00b7brau\u00b7chen", "sie", "f\u00fcr", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "PPER", "APPR", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihres Gleichen meisterlich;", "tokens": ["Und", "ih\u00b7res", "Glei\u00b7chen", "meis\u00b7ter\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr Spott und Tadel trifft nur den", "tokens": ["Ihr", "Spott", "und", "Ta\u00b7del", "trifft", "nur", "den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "NN", "VVFIN", "ADV", "ART"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Der nicht zu ihnen h\u00e4lt.", "tokens": ["Der", "nicht", "zu", "ih\u00b7nen", "h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u00b7rer", "Jour\u00b7na\u00b7le", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind sie gestorben,", "tokens": ["Sind", "sie", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freut euch noch mehr!", "tokens": ["Freut", "euch", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Der eine lobt den andern sehr,", "tokens": ["Der", "ei\u00b7ne", "lobt", "den", "an\u00b7dern", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "ADJA", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den andern lobt dann ", "tokens": ["Den", "an\u00b7dern", "lobt", "dann"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So w\u00e4scht dann immer eine Hand", "tokens": ["So", "w\u00e4scht", "dann", "im\u00b7mer", "ei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die andre ganz gewi\u00df.", "tokens": ["Die", "and\u00b7re", "ganz", "ge\u00b7wi\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u00b7rer", "Jour\u00b7na\u00b7le", "sehr", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Sind sie gestorben,", "tokens": ["Sind", "sie", "ge\u00b7stor\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Freut euch noch mehr!", "tokens": ["Freut", "euch", "noch", "mehr", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "O bindet nicht mit ihnen an,", "tokens": ["O", "bin\u00b7det", "nicht", "mit", "ih\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst seid ihr wahrlich schlimm daran,", "tokens": ["Sonst", "seid", "ihr", "wahr\u00b7lich", "schlimm", "da\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie werfen euch mit Koth also,", "tokens": ["Sie", "wer\u00b7fen", "euch", "mit", "Koth", "al\u00b7so", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Da\u00df ihr euch selbst nicht kennt.", "tokens": ["Da\u00df", "ihr", "euch", "selbst", "nicht", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Freut euch des Lebens", "tokens": ["Freut", "euch", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Unsrer Journale sehr!", "tokens": ["Uns\u0