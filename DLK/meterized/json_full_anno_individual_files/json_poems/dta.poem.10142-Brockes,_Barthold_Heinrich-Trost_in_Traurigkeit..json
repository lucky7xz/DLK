{"dta.poem.10142": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Trost in Traurigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist etwan euer Geist beschweret und betr\u00fcbet;", "tokens": ["Ist", "et\u00b7wan", "eu\u00b7er", "Geist", "be\u00b7schwe\u00b7ret", "und", "be\u00b7tr\u00fc\u00b7bet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So nehmet diesen Raht von einem an,", "tokens": ["So", "neh\u00b7met", "die\u00b7sen", "Raht", "von", "ei\u00b7nem", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "APPR", "ART", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der aus Erfahrung sprechen kann,", "tokens": ["Der", "aus", "Er\u00b7fah\u00b7rung", "spre\u00b7chen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er in Wiedrigkeit besondre Lindrung giebet.", "tokens": ["Da\u00df", "er", "in", "Wied\u00b7rig\u00b7keit", "be\u00b7sond\u00b7re", "Lind\u00b7rung", "gie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schliesst ein paar Augenblick die Augen-Lieder zu.", "tokens": ["Schliesst", "ein", "paar", "Au\u00b7gen\u00b7blick", "die", "Au\u00b7gen\u00b7Lie\u00b7der", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch die ", "tokens": ["Durch", "die"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ob es sich gleich nicht gantz der Traurigkeit entzieht,", "tokens": ["Ob", "es", "sich", "gleich", "nicht", "gantz", "der", "Trau\u00b7rig\u00b7keit", "ent\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In welcher es zu tieff versencket,", "tokens": ["In", "wel\u00b7cher", "es", "zu", "tieff", "ver\u00b7sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PTKA", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch etwas von dem Weg der Schwermuth abgelencket,", "tokens": ["Doch", "et\u00b7was", "von", "dem", "Weg", "der", "Schwer\u00b7muth", "ab\u00b7ge\u00b7len\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und kommt aufs wenigste zum Anfang einer Ruh.", "tokens": ["Und", "kommt", "aufs", "we\u00b7nigs\u00b7te", "zum", "An\u00b7fang", "ei\u00b7ner", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hebt dann mit Langsamkeit die Augen wieder auf;", "tokens": ["Hebt", "dann", "mit", "Lang\u00b7sam\u00b7keit", "die", "Au\u00b7gen", "wie\u00b7der", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "NN", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So werden, sonderlich bey hellem Sonnen-Schein,", "tokens": ["So", "wer\u00b7den", ",", "son\u00b7der\u00b7lich", "bey", "hel\u00b7lem", "Son\u00b7nen\u00b7Schein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAINF", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Vorw\u00fcrff der bestrahlten Erden", "tokens": ["Die", "Vor\u00b7w\u00fcrff", "der", "be\u00b7strahl\u00b7ten", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch, durch den Mi\u00dfbrauch unsrer Sinnen,", "tokens": ["Doch", ",", "durch", "den", "Mi\u00df\u00b7brauch", "uns\u00b7rer", "Sin\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und durch Gewohnheit blind, vorhin nicht angesehn)", "tokens": ["Und", "durch", "Ge\u00b7wohn\u00b7heit", "blind", ",", "vor\u00b7hin", "nicht", "an\u00b7ge\u00b7sehn", ")"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADJD", "$,", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Uns recht zu Freuden-Quellen werden:", "tokens": ["Uns", "recht", "zu", "Freu\u00b7den\u00b7Quel\u00b7len", "wer\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Da sie, indem sie uns durchs Aug\u2019 ins Hertze dringen,", "tokens": ["Da", "sie", ",", "in\u00b7dem", "sie", "uns", "durchs", "Aug'", "ins", "Hert\u00b7ze", "drin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "APPRART", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Zugleich ein lieblichs Gnaden-Bild", "tokens": ["Zu\u00b7gleich", "ein", "lieb\u00b7lichs", "Gna\u00b7den\u00b7Bild"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Von dem, der sie gemacht, der alle Ding erf\u00fcllt,", "tokens": ["Von", "dem", ",", "der", "sie", "ge\u00b7macht", ",", "der", "al\u00b7le", "Ding", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,", "PRELS", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Von ihrem Sch\u00f6pfer, mit sich bringen.", "tokens": ["Von", "ih\u00b7rem", "Sch\u00f6p\u00b7fer", ",", "mit", "sich", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die\u00df kann nun ohne Trost und Freude nicht geschehn,", "tokens": ["Die\u00df", "kann", "nun", "oh\u00b7ne", "Trost", "und", "Freu\u00b7de", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ADV", "APPR", "NN", "KON", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil die Erinnerung, den Sch\u00f6pfer nah zu sehn,", "tokens": ["Weil", "die", "E\u00b7rin\u00b7ne\u00b7rung", ",", "den", "Sch\u00f6p\u00b7fer", "nah", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Uns billig zur Gelassenheit", "tokens": ["Uns", "bil\u00b7lig", "zur", "Ge\u00b7las\u00b7sen\u00b7heit"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am st\u00e4rcksten treiben sollt, und zu der Zuversicht,", "tokens": ["Am", "st\u00e4rcks\u00b7ten", "trei\u00b7ben", "sollt", ",", "und", "zu", "der", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVINF", "VMFIN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der gegenw\u00e4rtge ", "tokens": ["Der", "ge\u00b7gen\u00b7w\u00e4rt\u00b7ge"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Dem es an Lieb\u2019 und Macht und Weisheit nicht gebricht,", "tokens": ["Dem", "es", "an", "Lieb'", "und", "Macht", "und", "Weis\u00b7heit", "nicht", "ge\u00b7bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "KON", "NN", "KON", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und ohne Dem nichts, was geschicht, geschicht;", "tokens": ["Und", "oh\u00b7ne", "Dem", "nichts", ",", "was", "ge\u00b7schicht", ",", "ge\u00b7schicht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "PIS", "$,", "PRELS", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Werd, wenn Er uns, nach unsrer M\u00f6glichkeit,", "tokens": ["Werd", ",", "wenn", "Er", "uns", ",", "nach", "uns\u00b7rer", "M\u00f6g\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Gelassen findet;", "tokens": ["Ge\u00b7las\u00b7sen", "fin\u00b7det", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "Zu rechter Zeit, die Er allein ergr\u00fcndet,", "tokens": ["Zu", "rech\u00b7ter", "Zeit", ",", "die", "Er", "al\u00b7lein", "er\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und die kein Mensch vermag zu fassen,", "tokens": ["Und", "die", "kein", "Mensch", "ver\u00b7mag", "zu", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIAT", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die wechselnde Beschaffenheit", "tokens": ["Die", "wech\u00b7seln\u00b7de", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.13": {"text": "Der meisten Dinge dieser Zeit", "tokens": ["Der", "meis\u00b7ten", "Din\u00b7ge", "die\u00b7ser", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Uns gn\u00e4dig wiederfahren lassen.", "tokens": ["Uns", "gn\u00e4\u00b7dig", "wie\u00b7der\u00b7fah\u00b7ren", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}