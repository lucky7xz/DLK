oken_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie sprach zu ihm \u2013 sie sang zu ihm \u2013", "tokens": ["Sie", "sprach", "zu", "ihm", "\u2013", "sie", "sang", "zu", "ihm", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$(", "PPER", "VVFIN", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da wars um ihn geschehn \u2013", "tokens": ["Da", "wars", "um", "ihn", "ge\u00b7schehn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Halb zog sie ihn, halb sank er hin", "tokens": ["Halb", "zog", "sie", "ihn", ",", "halb", "sank", "er", "hin"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "ADJD", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und ward nicht mehr gesehn.", "tokens": ["Und", "ward", "nicht", "mehr", "ge\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}