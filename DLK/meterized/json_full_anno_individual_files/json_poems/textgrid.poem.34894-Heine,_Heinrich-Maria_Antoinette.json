{"textgrid.poem.34894": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Maria Antoinette", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie heiter im Tuilerienschlo\u00df", "tokens": ["Wie", "hei\u00b7ter", "im", "Tu\u00b7i\u00b7le\u00b7ri\u00b7en\u00b7schlo\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Blinken die Spiegelfenster,", "tokens": ["Blin\u00b7ken", "die", "Spie\u00b7gel\u00b7fens\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Und dennoch dort am hellen Tag", "tokens": ["Und", "den\u00b7noch", "dort", "am", "hel\u00b7len", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADV", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gehn um die alten Gespenster.", "tokens": ["Gehn", "um", "die", "al\u00b7ten", "Ge\u00b7spens\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Es spukt im Pavillon de Flor'", "tokens": ["Es", "spukt", "im", "Pa\u00b7vil\u00b7lon", "de", "Flor'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "NE", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Maria Antoinette;", "tokens": ["Ma\u00b7ria", "An\u00b7to\u00b7i\u00b7net\u00b7te", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Sie h\u00e4lt dort morgens ihr Lever", "tokens": ["Sie", "h\u00e4lt", "dort", "mor\u00b7gens", "ihr", "Le\u00b7ver"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PPOSAT", "NN"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mit strenger Etikette.", "tokens": ["Mit", "stren\u00b7ger", "E\u00b7ti\u00b7ket\u00b7te", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Geputzte Hofdamen. Die meisten stehn,", "tokens": ["Ge\u00b7putz\u00b7te", "Hof\u00b7da\u00b7men", ".", "Die", "meis\u00b7ten", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ART", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auf Taburetts andre sitzen;", "tokens": ["Auf", "Ta\u00b7bu\u00b7retts", "and\u00b7re", "sit\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die Kleider von Atlas und Goldbrokat,", "tokens": ["Die", "Klei\u00b7der", "von", "At\u00b7las", "und", "Gold\u00b7bro\u00b7kat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "KON", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Beh\u00e4ngt mit Juwelen und Spitzen.", "tokens": ["Be\u00b7h\u00e4ngt", "mit", "Ju\u00b7we\u00b7len", "und", "Spit\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.4": {"line.1": {"text": "Die Taille ist schmal, der Reifrock bauscht,", "tokens": ["Die", "Tail\u00b7le", "ist", "schmal", ",", "der", "Rei\u00b7frock", "bauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Darunter lauschen die netten", "tokens": ["Da\u00b7run\u00b7ter", "lau\u00b7schen", "die", "net\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hochhackigen F\u00fc\u00dfchen so klug hervor \u2013", "tokens": ["Hoch\u00b7ha\u00b7cki\u00b7gen", "F\u00fc\u00df\u00b7chen", "so", "klug", "her\u00b7vor", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "ADJD", "PTKVZ", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ach, wenn sie nur K\u00f6pfe h\u00e4tten!", "tokens": ["Ach", ",", "wenn", "sie", "nur", "K\u00f6p\u00b7fe", "h\u00e4t\u00b7ten", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sie haben alle keinen Kopf,", "tokens": ["Sie", "ha\u00b7ben", "al\u00b7le", "kei\u00b7nen", "Kopf", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der K\u00f6nigin selbst manquieret", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "selbst", "man\u00b7qui\u00b7e\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Der Kopf, und Ihro Majest\u00e4t", "tokens": ["Der", "Kopf", ",", "und", "Ih\u00b7ro", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist deshalb nicht frisieret.", "tokens": ["Ist", "des\u00b7halb", "nicht", "fri\u00b7sie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PAV", "PTKNEG", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, sie, die mit turmhohem Toupet", "tokens": ["Ja", ",", "sie", ",", "die", "mit", "turm\u00b7ho\u00b7hem", "Tou\u00b7pet"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "So stolz sich konnte gebaren,", "tokens": ["So", "stolz", "sich", "konn\u00b7te", "ge\u00b7ba\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "VMFIN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Tochter Maria Theresias,", "tokens": ["Die", "Toch\u00b7ter", "Ma\u00b7ria", "The\u00b7re\u00b7si\u00b7as", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NE", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Die Enkelin deutscher C\u00e4saren,", "tokens": ["Die", "En\u00b7ke\u00b7lin", "deut\u00b7scher", "C\u00e4\u00b7sa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Sie mu\u00df jetzt spuken ohne Frisur", "tokens": ["Sie", "mu\u00df", "jetzt", "spu\u00b7ken", "oh\u00b7ne", "Fri\u00b7sur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "APPR", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und ohne Kopf, im Kreise", "tokens": ["Und", "oh\u00b7ne", "Kopf", ",", "im", "Krei\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "APPR", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von unfrisierten Edelfraun,", "tokens": ["Von", "un\u00b7fri\u00b7sier\u00b7ten", "E\u00b7del\u00b7fraun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die kopflos gleicherweise.", "tokens": ["Die", "kopf\u00b7los", "glei\u00b7cher\u00b7wei\u00b7se", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Das sind die Folgen der Revolution", "tokens": ["Das", "sind", "die", "Fol\u00b7gen", "der", "Re\u00b7vo\u00b7lu\u00b7ti\u00b7on"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und ihrer fatalen Doktrine;", "tokens": ["Und", "ih\u00b7rer", "fa\u00b7ta\u00b7len", "Dokt\u00b7ri\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "An allem ist schuld Jean Jacques Rousseau,", "tokens": ["An", "al\u00b7lem", "ist", "schuld", "Jean", "Jac\u00b7ques", "Rous\u00b7se\u00b7au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "VAFIN", "ADJD", "NE", "NE", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Voltaire und die Guillotine.", "tokens": ["Vol\u00b7tai\u00b7re", "und", "die", "Guil\u00b7lo\u00b7ti\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch sonderbar! es d\u00fcnkt mich schier,", "tokens": ["Doch", "son\u00b7der\u00b7bar", "!", "es", "d\u00fcnkt", "mich", "schier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$.", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als h\u00e4tten die armen Gesch\u00f6pfe", "tokens": ["Als", "h\u00e4t\u00b7ten", "die", "ar\u00b7men", "Ge\u00b7sch\u00f6p\u00b7fe"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Gar nicht bemerkt, wie tot sie sind", "tokens": ["Gar", "nicht", "be\u00b7merkt", ",", "wie", "tot", "sie", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PTKNEG", "VVPP", "$,", "PWAV", "ADJD", "PPER", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df sie verloren die K\u00f6pfe.", "tokens": ["Und", "da\u00df", "sie", "ver\u00b7lo\u00b7ren", "die", "K\u00f6p\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.10": {"line.1": {"text": "Ein leeres Gespreize, ganz wie sonst,", "tokens": ["Ein", "lee\u00b7res", "Ge\u00b7sprei\u00b7ze", ",", "ganz", "wie", "sonst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "KOKOM", "ADV", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein abgeschmacktes Scherwenzen \u2013", "tokens": ["Ein", "ab\u00b7ge\u00b7schmack\u00b7tes", "Scher\u00b7wen\u00b7zen", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Possierlich sind und schauderhaft", "tokens": ["Pos\u00b7sier\u00b7lich", "sind", "und", "schau\u00b7der\u00b7haft"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die kopflosen Reverenzen.", "tokens": ["Die", "kopf\u00b7lo\u00b7sen", "Re\u00b7ve\u00b7ren\u00b7zen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.11": {"line.1": {"text": "Es knickst die erste Dame d'atour", "tokens": ["Es", "knickst", "die", "ers\u00b7te", "Da\u00b7me", "d'\u00b7a\u00b7tour"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bringt ein Hemd von Linnen;", "tokens": ["Und", "bringt", "ein", "Hemd", "von", "Lin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die zweite reicht es der K\u00f6nigin,", "tokens": ["Die", "zwei\u00b7te", "reicht", "es", "der", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und beide knicksen von hinnen.", "tokens": ["Und", "bei\u00b7de", "knick\u00b7sen", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Die dritte Dam' und die vierte Dam'", "tokens": ["Die", "drit\u00b7te", "Dam'", "und", "die", "vier\u00b7te", "Dam'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Knicksen und niederknien", "tokens": ["Knick\u00b7sen", "und", "nie\u00b7der\u00b7kni\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor Ihrer Majest\u00e4t, um Ihr", "tokens": ["Vor", "Ih\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t", ",", "um", "Ihr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Str\u00fcmpfe anzuziehen.", "tokens": ["Die", "Str\u00fcmp\u00b7fe", "an\u00b7zu\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ein Ehrenfr\u00e4ulein kommt und knickst", "tokens": ["Ein", "Eh\u00b7ren\u00b7fr\u00e4u\u00b7lein", "kommt", "und", "knickst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bringt das Morgenj\u00e4ckchen;", "tokens": ["Und", "bringt", "das", "Mor\u00b7gen\u00b7j\u00e4ck\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein andres Fr\u00e4ulein knickst und bringt", "tokens": ["Ein", "and\u00b7res", "Fr\u00e4u\u00b7lein", "knickst", "und", "bringt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der K\u00f6nigin Unterr\u00f6ckchen.", "tokens": ["Der", "K\u00f6\u00b7ni\u00b7gin", "Un\u00b7ter\u00b7r\u00f6ck\u00b7chen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Die Oberhofmeisterin steht dabei,", "tokens": ["Die", "O\u00b7ber\u00b7hof\u00b7meis\u00b7te\u00b7rin", "steht", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sie f\u00e4chert die Brust, die wei\u00dfe,", "tokens": ["Sie", "f\u00e4\u00b7chert", "die", "Brust", ",", "die", "wei\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und in Ermanglung eines Kopfs", "tokens": ["Und", "in", "Er\u00b7mang\u00b7lung", "ei\u00b7nes", "Kopfs"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "L\u00e4chelt sie mit dem Stei\u00dfe.", "tokens": ["L\u00e4\u00b7chelt", "sie", "mit", "dem", "Stei\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}}, "stanza.15": {"line.1": {"text": "Wohl durch die verh\u00e4ngten Fenster wirft", "tokens": ["Wohl", "durch", "die", "ver\u00b7h\u00e4ng\u00b7ten", "Fens\u00b7ter", "wirft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Die Sonne neugierige Blicke