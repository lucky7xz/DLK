gin,", "tokens": ["Die", "zwei\u00b7te", "reicht", "es", "der", "K\u00f6\u00b7ni\u00b7gin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und beide knicksen von hinnen.", "tokens": ["Und", "bei\u00b7de", "knick\u00b7sen", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.27": {"line.1": {"text": "Die dritte Dam' und die vierte Dam'", "tokens": ["Die", "drit\u00b7te", "Dam'", "und", "die", "vier\u00b7te", "Dam'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Knicksen und niederknien", "tokens": ["Knick\u00b7sen", "und", "nie\u00b7der\u00b7kni\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NE"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Vor Ihrer Majest\u00e4t, um Ihr", "tokens": ["Vor", "Ih\u00b7rer", "Ma\u00b7jes\u00b7t\u00e4t", ",", "um", "Ihr"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUI", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Str\u00fcmpfe anzuziehen.", "tokens": ["Die", "Str\u00fcmp\u00b7fe", "an\u00b7zu\u00b7zie\u00b7hen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "Ein Ehrenfr\u00e4ulein kommt und knickst", "tokens": ["Ein", "Eh\u00b7ren\u00b7fr\u00e4u\u00b7lein", "kommt", "und", "knickst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KON", "