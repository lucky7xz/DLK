{"dta.poem.7552": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "51.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Geist an seinen Leib kn\u00fcpft ein nat\u00fcrlich Band,", "tokens": ["Den", "Geist", "an", "sei\u00b7nen", "Leib", "kn\u00fcpft", "ein", "na\u00b7t\u00fcr\u00b7lich", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das l\u00f6st er nicht, wenn er sich jedem sonst entwand.", "tokens": ["Das", "l\u00f6st", "er", "nicht", ",", "wenn", "er", "sich", "je\u00b7dem", "sonst", "ent\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "PRF", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er hat es nicht gekn\u00fcpft, und soll es drum nicht l\u00f6sen;", "tokens": ["Er", "hat", "es", "nicht", "ge\u00b7kn\u00fcpft", ",", "und", "soll", "es", "drum", "nicht", "l\u00f6\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "VVPP", "$,", "KON", "VMFIN", "PPER", "PAV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verstricken soll er nur sich nicht darein zum B\u00f6sen.", "tokens": ["Ver\u00b7stri\u00b7cken", "soll", "er", "nur", "sich", "nicht", "da\u00b7rein", "zum", "B\u00f6\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "ADV", "PRF", "PTKNEG", "PAV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Der Leib ist zwischen Geist und Welt zwar ein Verband,", "tokens": ["Der", "Leib", "ist", "zwi\u00b7schen", "Geist", "und", "Welt", "zwar", "ein", "Ver\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch zwischen Geist und Welt auch eine Scheidewand.", "tokens": ["Doch", "zwi\u00b7schen", "Geist", "und", "Welt", "auch", "ei\u00b7ne", "Schei\u00b7de\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Geist kann durch den Leib sich in den Weltschmutz tauchen,", "tokens": ["Der", "Geist", "kann", "durch", "den", "Leib", "sich", "in", "den", "Welt\u00b7schmutz", "tau\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch gegen ihre Flut ihn auch zum Damme brauchen.", "tokens": ["Doch", "ge\u00b7gen", "ih\u00b7re", "Flut", "ihn", "auch", "zum", "Dam\u00b7me", "brau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Es f\u00fchlt ein reiner Geist, vom reinen Leib befangen,", "tokens": ["Es", "f\u00fchlt", "ein", "rei\u00b7ner", "Geist", ",", "vom", "rei\u00b7nen", "Leib", "be\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sich frei vom Dienst der Welt, allein in Gott gefangen.", "tokens": ["Sich", "frei", "vom", "Dienst", "der", "Welt", ",", "al\u00b7lein", "in", "Gott", "ge\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "ART", "NN", "$,", "ADV", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}