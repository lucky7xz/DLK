{"dta.poem.21737": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Die Blumenchiffer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Blick auf, blick auf zur Sonne", "tokens": ["Blick", "auf", ",", "blick", "auf", "zur", "Son\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "APPR", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O Auge, durch den Thr\u00e4nenflor!", "tokens": ["O", "Au\u00b7ge", ",", "durch", "den", "Thr\u00e4\u00b7nen\u00b7flor", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Gram entknospt die Wonne.", "tokens": ["Dem", "Gram", "ent\u00b7knospt", "die", "Won\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus Thr\u00e4nen keimt der Trost empor.", "tokens": ["Aus", "Thr\u00e4\u00b7nen", "keimt", "der", "Trost", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Iris Farbenfeuer", "tokens": ["Der", "I\u00b7ris", "Far\u00b7ben\u00b7feu\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["ART", "NE", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Durchstrahlt den grauen Duft,", "tokens": ["Durch\u00b7strahlt", "den", "grau\u00b7en", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Es hebt die Brust sich freyer", "tokens": ["Es", "hebt", "die", "Brust", "sich", "frey\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PRF", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In abgest\u00fcrmter Luft.", "tokens": ["In", "ab\u00b7ge\u00b7st\u00fcrm\u00b7ter", "Luft", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "S\u00fcss mundet uns die Wonne,", "tokens": ["S\u00fcss", "mun\u00b7det", "uns", "die", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Die wir mit Quaal bezahlt,", "tokens": ["Die", "wir", "mit", "Qua\u00b7al", "be\u00b7zahlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.11": {"text": "Und sch\u00f6ner strahlt die Sonne,", "tokens": ["Und", "sch\u00f6\u00b7ner", "strahlt", "die", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Die nach Gewittern strahlt.", "tokens": ["Die", "nach", "Ge\u00b7wit\u00b7tern", "strahlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Sonn' entgegen breitet", "tokens": ["Der", "Sonn'", "ent\u00b7ge\u00b7gen", "brei\u00b7tet"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PTKVZ", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lilie ihre weisse Brust.", "tokens": ["Die", "Li\u00b7lie", "ih\u00b7re", "weis\u00b7se", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von Hoffnung aufgeweitet,", "tokens": ["Von", "Hoff\u00b7nung", "auf\u00b7ge\u00b7wei\u00b7tet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ahnt die Verzweiflung Himmelslust.", "tokens": ["Ahnt", "die", "Ver\u00b7zwei\u00b7flung", "Him\u00b7mels\u00b7lust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Erde t\u00e4uscht das Hoffen.", "tokens": ["Die", "Er\u00b7de", "t\u00e4uscht", "das", "Hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der Durst bleibt ungestillt.", "tokens": ["Der", "Durst", "bleibt", "un\u00b7ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Doch steht die Heimath offen,", "tokens": ["Doch", "steht", "die", "Hei\u00b7math", "of\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "F\u00fcr die das Herz erschwillt.", "tokens": ["F\u00fcr", "die", "das", "Herz", "er\u00b7schwillt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Was mich erg\u00f6tzt bl\u00fcht droben.", "tokens": ["Was", "mich", "er\u00b7g\u00f6tzt", "bl\u00fcht", "dro\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "VVFIN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Nur droben gr\u00fcnt mein Gl\u00fcck.", "tokens": ["Nur", "dro\u00b7ben", "gr\u00fcnt", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Nach oben drum, nach oben", "tokens": ["Nach", "o\u00b7ben", "drum", ",", "nach", "o\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ADV", "PAV", "$,", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Schaut der bethr\u00e4nte Blick.", "tokens": ["Schaut", "der", "be\u00b7thr\u00e4n\u00b7te", "Blick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}