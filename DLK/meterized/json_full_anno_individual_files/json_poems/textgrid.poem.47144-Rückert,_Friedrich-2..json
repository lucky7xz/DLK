{"textgrid.poem.47144": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebster! nur dich sehn, dich h\u00f6ren", "tokens": ["Liebs\u00b7ter", "!", "nur", "dich", "sehn", ",", "dich", "h\u00f6\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "PPER", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dir schweigend angeh\u00f6ren;", "tokens": ["Und", "dir", "schwei\u00b7gend", "an\u00b7ge\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht umstricken dich mit Armen,", "tokens": ["Nicht", "um\u00b7stri\u00b7cken", "dich", "mit", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht am Busen dir erwarmen,", "tokens": ["Nicht", "am", "Bu\u00b7sen", "dir", "er\u00b7war\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht dich k\u00fcssen, nicht dich fassen \u2013", "tokens": ["Nicht", "dich", "k\u00fcs\u00b7sen", ",", "nicht", "dich", "fas\u00b7sen", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPER", "VVINF", "$,", "PTKNEG", "PPER", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dieses alles kann ich lassen,", "tokens": ["Die\u00b7ses", "al\u00b7les", "kann", "ich", "las\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Nur nicht das Gef\u00fchl vermissen,", "tokens": ["Nur", "nicht", "das", "Ge\u00b7f\u00fchl", "ver\u00b7mis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mein dich und mich dein zu wissen.", "tokens": ["Mein", "dich", "und", "mich", "dein", "zu", "wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "PPER", "KON", "PPER", "PPOSAT", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Liebster! nur dich sehn, dich h\u00f6ren", "tokens": ["Liebs\u00b7ter", "!", "nur", "dich", "sehn", ",", "dich", "h\u00f6\u00b7ren"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "$.", "ADV", "PPER", "VVINF", "$,", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dir schweigend angeh\u00f6ren;", "tokens": ["Und", "dir", "schwei\u00b7gend", "an\u00b7ge\u00b7h\u00f6\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht umstricken dich mit Armen,", "tokens": ["Nicht", "um\u00b7stri\u00b7cken", "dich", "mit", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht am Busen dir erwarmen,", "tokens": ["Nicht", "am", "Bu\u00b7sen", "dir", "er\u00b7war\u00b7men", ","], "token_in