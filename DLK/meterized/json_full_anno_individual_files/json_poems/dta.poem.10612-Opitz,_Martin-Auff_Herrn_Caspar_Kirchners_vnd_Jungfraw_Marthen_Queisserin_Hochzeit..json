{"dta.poem.10612": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Auff Herrn Caspar Kirchners/ vnd Jungfraw  \n Marthen Queisserin Hochzeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.57", "nl:0.42"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist in Engelland/ wo sonst Diana hetzet/", "tokens": ["Es", "ist", "in", "En\u00b7gel\u00b7land", "/", "wo", "sonst", "Di\u00b7a\u00b7na", "het\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NE", "$(", "PWAV", "ADV", "NE", "VVFIN", "$("], "meter": "-+-+-+-++-+--", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Vnd an der Temse randt sich mit der Jagd ergetzet/", "tokens": ["Vnd", "an", "der", "Tem\u00b7se", "randt", "sich", "mit", "der", "Jagd", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nicht weit von Windesoor ein lustig gr\u00fcnes Thal/", "tokens": ["Nicht", "weit", "von", "Win\u00b7de\u00b7soor", "ein", "lus\u00b7tig", "gr\u00fc\u00b7nes", "Thal", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "APPR", "NN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Gaben der Natur gezieret vberal;", "tokens": ["Mit", "Ga\u00b7ben", "der", "Na\u00b7tur", "ge\u00b7zie\u00b7ret", "vbe\u00b7ral", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Die Klippen oben zu sein f\u00fcrgesetzt der Sonnen/", "tokens": ["Die", "Klip\u00b7pen", "o\u00b7ben", "zu", "sein", "f\u00fcr\u00b7ge\u00b7setzt", "der", "Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VAINF", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Wiese wirdt erfrischt von vielen s\u00fcssen Bronnen/", "tokens": ["Die", "Wie\u00b7se", "wirdt", "er\u00b7frischt", "von", "vie\u00b7len", "s\u00fcs\u00b7sen", "Bron\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPR", "PIAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Blumen vnd das Gras ist niemahls abgemeiht/", "tokens": ["Die", "Blu\u00b7men", "vnd", "das", "Gras", "ist", "nie\u00b7mahls", "ab\u00b7ge\u00b7meiht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "VAFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An Winters statt ist Herbst/ an Sommers Fr\u00fclingzeit.", "tokens": ["An", "Win\u00b7ters", "statt", "ist", "Herbst", "/", "an", "Som\u00b7mers", "Fr\u00fc\u00b7ling\u00b7zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "VAFIN", "NN", "$(", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In dieses edel Orth ist/ sagt man/ Venus kommen/", "tokens": ["In", "die\u00b7ses", "e\u00b7del", "Orth", "ist", "/", "sagt", "man", "/", "Ve\u00b7nus", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "$(", "VVFIN", "PIS", "$(", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nach dem der Selymus jhr Cypern eingenommen/", "tokens": ["Nach", "dem", "der", "Se\u00b7ly\u00b7mus", "jhr", "Cy\u00b7pern", "ein\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da hat jhr Volck ein Schlo\u00df von Rosen auffgebaut/", "tokens": ["Da", "hat", "jhr", "Volck", "ein", "Schlo\u00df", "von", "Ro\u00b7sen", "auff\u00b7ge\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So die Poeten nur/ sonst niemandt/ angeschaut.", "tokens": ["So", "die", "Po\u00b7et\u00b7en", "nur", "/", "sonst", "nie\u00b7mandt", "/", "an\u00b7ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "$(", "ADV", "VVPP", "$(", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Der Mond ist viertzig mahl nun fast mit frischen Pferden", "tokens": ["Der", "Mond", "ist", "viert\u00b7zig", "mahl", "nun", "fast", "mit", "fri\u00b7schen", "Pfer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "CARD", "ADV", "ADV", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gereiset vmb vnd vmb den runden Krai\u00df der Erden/", "tokens": ["Ge\u00b7rei\u00b7set", "vmb", "vnd", "vmb", "den", "run\u00b7den", "Krai\u00df", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Als jhr/ Herr Breutigam/ alldar seit angelangt/", "tokens": ["Als", "jhr", "/", "Herr", "Breu\u00b7ti\u00b7gam", "/", "all\u00b7dar", "seit", "an\u00b7ge\u00b7langt", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NN", "NE", "$(", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd euch ward zuerkennt/ was jhr erst jetzt empfangt/", "tokens": ["Vnd", "euch", "ward", "zu\u00b7er\u00b7kennt", "/", "was", "jhr", "erst", "jetzt", "emp\u00b7fangt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "VVPP", "$(", "PWS", "PPER", "ADV", "ADV", "VVFIN", "$("], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.17": {"text": "Dann als die G\u00f6ttin sah/ wie jhr/ au\u00df Lust der Tugendt/", "tokens": ["Dann", "als", "die", "G\u00f6t\u00b7tin", "sah", "/", "wie", "jhr", "/", "au\u00df", "Lust", "der", "Tu\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "VVFIN", "$(", "PWAV", "PPER", "$(", "APPR", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Auffgabet williglich die Bl\u00fcth an ewrer Jugendt/", "tokens": ["Auff\u00b7ga\u00b7bet", "wil\u00b7lig\u00b7lich", "die", "Bl\u00fcth", "an", "ew\u00b7rer", "Ju\u00b7gendt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "(nicht wie die meisten thun/ so wandern vber Meer/", "tokens": ["(", "nicht", "wie", "die", "meis\u00b7ten", "thun", "/", "so", "wan\u00b7dern", "vber", "Meer", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKNEG", "KOKOM", "ART", "PIS", "VVINF", "$(", "ADV", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Vnd bringen vor Verstandt frembd\u2019 Art von Lastern her/", "tokens": ["Vnd", "brin\u00b7gen", "vor", "Ver\u00b7standt", "frembd'", "Art", "von", "Las\u00b7tern", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ADJA", "NN", "APPR", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ertappen newe Tracht/ an statt der Weisheit G\u00fcter/", "tokens": ["Er\u00b7tap\u00b7pen", "ne\u00b7we", "Tracht", "/", "an", "statt", "der", "Weis\u00b7heit", "G\u00fc\u00b7ter", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$(", "APPR", "APPR", "ART", "NN", "NN", "$("], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}, "line.22": {"text": "Verendern nur die Lufft/ behalten die Gem\u00fcther:)", "tokens": ["Ver\u00b7en\u00b7dern", "nur", "die", "Lufft", "/", "be\u00b7hal\u00b7ten", "die", "Ge\u00b7m\u00fc\u00b7ther", ":)"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "emoticon"], "pos": ["NN", "ADV", "ART", "NN", "$(", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Hatt sie es so versehn/ weil sie Poeten holdt/", "tokens": ["Hatt", "sie", "es", "so", "ver\u00b7sehn", "/", "weil", "sie", "Po\u00b7et\u00b7en", "holdt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "VVINF", "$(", "KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df jhr in diesem Port euch hier erholen solt.", "tokens": ["Da\u00df", "jhr", "in", "die\u00b7sem", "Port", "euch", "hier", "er\u00b7ho\u00b7len", "solt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Wo ist nun die Natur/ wo seindt die grossen Sinnen/", "tokens": ["Wo", "ist", "nun", "die", "Na\u00b7tur", "/", "wo", "seindt", "die", "gros\u00b7sen", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ADV", "ART", "NN", "$(", "PWAV", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mit derer Hochheit jhr zuvor ersch\u00f6pffen k\u00fcnnen", "tokens": ["Mit", "de\u00b7rer", "Hoch\u00b7heit", "jhr", "zu\u00b7vor", "er\u00b7sch\u00f6pf\u00b7fen", "k\u00fcn\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "PPER", "ADV", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den grundt der Wissenschafft? Wo ist der Wei\u00dfheit Zier/", "tokens": ["Den", "grundt", "der", "Wis\u00b7sen\u00b7schafft", "?", "Wo", "ist", "der", "Wei\u00df\u00b7heit", "Zier", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "PWAV", "VAFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Mit der jhr/ hoher Geist/ giengt vielen andern f\u00fcr?", "tokens": ["Mit", "der", "jhr", "/", "ho\u00b7her", "Geist", "/", "giengt", "vie\u00b7len", "an\u00b7dern", "f\u00fcr", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$(", "ADJA", "NN", "$(", "VVFIN", "PIAT", "PIS", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Wo ist der Circkel denn/ mit welchem jhr der Sternen", "tokens": ["Wo", "ist", "der", "Cir\u00b7ckel", "denn", "/", "mit", "wel\u00b7chem", "jhr", "der", "Ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "NN", "KON", "$(", "APPR", "PRELS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Vnd Himmels eygenschafft gepfleget zu erlehrnen?", "tokens": ["Vnd", "Him\u00b7mels", "ey\u00b7gen\u00b7schafft", "ge\u00b7pfle\u00b7get", "zu", "er\u00b7lehr\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVPP", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Hier/ hier ist ewre Sonn/ ist ewer Firmament/", "tokens": ["Hier", "/", "hier", "ist", "ew\u00b7re", "Sonn", "/", "ist", "e\u00b7wer", "Fir\u00b7ma\u00b7ment", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "ADV", "VAFIN", "PPOSAT", "NN", "$(", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Ist ewer Erdenkrei\u00df: Hier hat die Welt ein endt.", "tokens": ["Ist", "e\u00b7wer", "Er\u00b7den\u00b7krei\u00df", ":", "Hier", "hat", "die", "Welt", "ein", "endt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Last Aristotelem/ last liegen Epicteten/", "tokens": ["Last", "A\u00b7ris\u00b7to\u00b7te\u00b7lem", "/", "last", "lie\u00b7gen", "E\u00b7pic\u00b7te\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$(", "ADV", "VVFIN", "NN", "$("], "meter": "+--+-+-+--+--", "measure": "iambic.penta.invert"}, "line.34": {"text": "Verge\u00dft der hohen Kunst der Himmlischen Poeten:", "tokens": ["Ver\u00b7ge\u00dft", "der", "ho\u00b7hen", "Kunst", "der", "Himm\u00b7li\u00b7schen", "Po\u00b7et\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Was mein Ovidius geschrieben/ hilfft euch nicht/", "tokens": ["Was", "mein", "O\u00b7vi\u00b7dius", "ge\u00b7schrie\u00b7ben", "/", "hilfft", "euch", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NE", "VVPP", "$(", "VVFIN", "PPER", "PTKNEG", "$("], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.36": {"text": "Was Maro nie gewust/ wirdt jetzt ins Werck gericht.", "tokens": ["Was", "Ma\u00b7ro", "nie", "ge\u00b7wust", "/", "wirdt", "jetzt", "ins", "Werck", "ge\u00b7richt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "VVPP", "$(", "VAFIN", "ADV", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Verg\u00f6nnet mir denlauff der tapffern werthen Helden/", "tokens": ["Ver\u00b7g\u00f6n\u00b7net", "mir", "den\u00b7lauff", "der", "tapf\u00b7fern", "wert\u00b7hen", "Hel\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PAV", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Die vor das Vatterlandt sich opffern/ zu vermelden.", "tokens": ["Die", "vor", "das", "Vat\u00b7ter\u00b7landt", "sich", "opf\u00b7fern", "/", "zu", "ver\u00b7mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PRF", "NN", "$(", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Das/ so jhr jetzundt thut/ ich mir noch nicht begehr/", "tokens": ["Das", "/", "so", "jhr", "je\u00b7tzundt", "thut", "/", "ich", "mir", "noch", "nicht", "be\u00b7gehr", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "ADV", "PPER", "ADV", "VVFIN", "$(", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Was aber ich will thun/ wer\u2019 euch mit dem zuschwer.", "tokens": ["Was", "a\u00b7ber", "ich", "will", "thun", "/", "wer'", "euch", "mit", "dem", "zu\u00b7schwer", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "PPER", "VMFIN", "VVINF", "$(", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.4