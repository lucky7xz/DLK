{"dta.poem.7603": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "18.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein frommer Bettler stand an Kr\u00e4merladenwand,", "tokens": ["Ein", "from\u00b7mer", "Bett\u00b7ler", "stand", "an", "Kr\u00e4\u00b7mer\u00b7la\u00b7den\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "H\u00e4tt' einer Gabe noth, doch streckte nicht die Hand.", "tokens": ["H\u00e4tt'", "ei\u00b7ner", "Ga\u00b7be", "noth", ",", "doch", "streck\u00b7te", "nicht", "die", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,", "ADV", "VVFIN", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der geiz'ge Kr\u00e4mer denkt, sein Schweigen sei ein Heischen;", "tokens": ["Der", "gei\u00b7z'\u00b7ge", "Kr\u00e4\u00b7mer", "denkt", ",", "sein", "Schwei\u00b7gen", "sei", "ein", "Hei\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PPOSAT", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "In seinem Kram gest\u00f6rt, begann er aufzukreischen.", "tokens": ["In", "sei\u00b7nem", "Kram", "ge\u00b7st\u00f6rt", ",", "be\u00b7gann", "er", "auf\u00b7zu\u00b7krei\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er hatt' in manchem Sack zu w\u00fchlen und zu kramen,", "tokens": ["Er", "hatt'", "in", "man\u00b7chem", "Sack", "zu", "w\u00fch\u00b7len", "und", "zu", "kra\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sprach zum Bettler barsch: Geh hin in Gottes Namen!", "tokens": ["Und", "sprach", "zum", "Bett\u00b7ler", "barsch", ":", "Geh", "hin", "in", "Got\u00b7tes", "Na\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ADJD", "$.", "NE", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Bettler sprach: Ich geh' in Gottes Namen leicht,", "tokens": ["Der", "Bett\u00b7ler", "sprach", ":", "Ich", "geh'", "in", "Got\u00b7tes", "Na\u00b7men", "leicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPR", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da mir zum Hindernis kein schwerer Pack gereicht.", "tokens": ["Da", "mir", "zum", "Hin\u00b7der\u00b7nis", "kein", "schwe\u00b7rer", "Pack", "ge\u00b7reicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du aber, der du hast so manchen Sack zu tragen,", "tokens": ["Du", "a\u00b7ber", ",", "der", "du", "hast", "so", "man\u00b7chen", "Sack", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "PPER", "VAFIN", "ADV", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie gehst du, wann man wird des Aufbruchs Trommel schlagen?", "tokens": ["Wie", "gehst", "du", ",", "wann", "man", "wird", "des", "Auf\u00b7bruchs", "Trom\u00b7mel", "schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VAFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Von diesem Worte ward des Kr\u00e4mers Herz getroffen,", "tokens": ["Von", "die\u00b7sem", "Wor\u00b7te", "ward", "des", "Kr\u00e4\u00b7mers", "Herz", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem Bettler gieng er nach, und lie\u00df den Laden offen.", "tokens": ["Dem", "Bett\u00b7ler", "gieng", "er", "nach", ",", "und", "lie\u00df", "den", "La\u00b7den", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Er nahm den Bettelstab und wanderte durchs Leben.", "tokens": ["Er", "nahm", "den", "Bet\u00b7tel\u00b7stab", "und", "wan\u00b7der\u00b7te", "durchs", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "KON", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So gute Lehren kann ein Bettler Kr\u00e4mern geben.", "tokens": ["So", "gu\u00b7te", "Leh\u00b7ren", "kann", "ein", "Bett\u00b7ler", "Kr\u00e4\u00b7mern", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Wol jenem Weisen gleich, der, als vor Feindesdrohn", "tokens": ["Wol", "je\u00b7nem", "Wei\u00b7sen", "gleich", ",", "der", ",", "als", "vor", "Fein\u00b7des\u00b7drohn"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PDAT", "NN", "ADV", "$,", "PRELS", "$,", "KOUS", "APPR", "NN"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Die B\u00fcrger, um Verlust der Habe klagend, flohn,", "tokens": ["Die", "B\u00fcr\u00b7ger", ",", "um", "Ver\u00b7lust", "der", "Ha\u00b7be", "kla\u00b7gend", ",", "flohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "NN", "ART", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Im schwerbepackten Zug gieng leicht an seinem Stabe,", "tokens": ["Im", "schwer\u00b7be\u00b7pack\u00b7ten", "Zug", "gieng", "leicht", "an", "sei\u00b7nem", "Sta\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sagte, da\u00df er all das Seine bei sich habe.", "tokens": ["Und", "sag\u00b7te", ",", "da\u00df", "er", "all", "das", "Sei\u00b7ne", "bei", "sich", "ha\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PIAT", "PDS", "PPOSAT", "APPR", "PRF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}