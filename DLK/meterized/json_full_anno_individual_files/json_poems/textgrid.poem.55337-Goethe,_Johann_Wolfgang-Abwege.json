{"textgrid.poem.55337": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Abwege", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00fcnstler, wird's im Innern steif,", "tokens": ["K\u00fcnst\u00b7ler", ",", "wird's", "im", "In\u00b7nern", "steif", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "APPRART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das ist nicht erfreulich;", "tokens": ["Das", "ist", "nicht", "er\u00b7freu\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Auch der vagen Z\u00fcge Schweif", "tokens": ["Auch", "der", "va\u00b7gen", "Z\u00fc\u00b7ge", "Schweif"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Ist uns ganz abscheulich;", "tokens": ["Ist", "uns", "ganz", "ab\u00b7scheu\u00b7lich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-++-", "measure": "unknown.measure.tri"}, "line.5": {"text": "Kommst du aber auf die Spur,", "tokens": ["Kommst", "du", "a\u00b7ber", "auf", "die", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df du's nicht getroffen,", "tokens": ["Da\u00df", "du's", "nicht", "ge\u00b7trof\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-", "mea