{"textgrid.poem.64792": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "14. Auf meinen seligen Vater", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Noch meinen Schmerz durch Dichterkunst zu zeigen", "tokens": ["Noch", "mei\u00b7nen", "Schmerz", "durch", "Dich\u00b7ter\u00b7kunst", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weyh' ich dies Lied, dir, liebster Vater, nicht:", "tokens": ["Wey\u00b7h'", "ich", "dies", "Lied", ",", "dir", ",", "liebs\u00b7ter", "Va\u00b7ter", ",", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "PDS", "NN", "$,", "PPER", "$,", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nichts dr\u00fcckt ihn aus, als ein bet\u00e4ubtes Schweigen,", "tokens": ["Nichts", "dr\u00fcckt", "ihn", "aus", ",", "als", "ein", "be\u00b7t\u00e4ub\u00b7tes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das manchmal nur ein banges Ach! durchbricht:", "tokens": ["Das", "manch\u00b7mal", "nur", "ein", "ban\u00b7ges", "Ach", "!", "durch\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "ART", "ADJA", "NN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und wenn man dich durch unerkauft Bedauern", "tokens": ["Und", "wenn", "man", "dich", "durch", "un\u00b7er\u00b7kauft", "Be\u00b7dau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PIS", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nah' und entfernt, bey deinem Abschied ehrt,", "tokens": ["Nah'", "und", "ent\u00b7fernt", ",", "bey", "dei\u00b7nem", "Ab\u00b7schied", "ehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.7": {"text": "So braucht es nicht, da\u00df eines Sohnes Trauern", "tokens": ["So", "braucht", "es", "nicht", ",", "da\u00df", "ei\u00b7nes", "Soh\u00b7nes", "Trau\u00b7ern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Erst deinen Ruhm der Welt verd\u00e4chtig lehrt.", "tokens": ["Erst", "dei\u00b7nen", "Ruhm", "der", "Welt", "ver\u00b7d\u00e4ch\u00b7tig", "lehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nur w\u00fcnsch' ich noch, weil ich dich mu\u00df entbehren,", "tokens": ["Nur", "w\u00fcn\u00b7sch'", "ich", "noch", ",", "weil", "ich", "dich", "mu\u00df", "ent\u00b7beh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Da\u00df nie in mir dein lebhaft Bild vergeht:", "tokens": ["Da\u00df", "nie", "in", "mir", "dein", "leb\u00b7haft", "Bild", "ver\u00b7geht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das kann mir noch ein Theil von dir gew\u00e4hren,", "tokens": ["Das", "kann", "mir", "noch", "ein", "Theil", "von", "dir", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn es vor mir, der Tugend Antrieb, steht.", "tokens": ["Wenn", "es", "vor", "mir", ",", "der", "Tu\u00b7gend", "An\u00b7trieb", ",", "steht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "$,", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "So hab' ich sonst, o Vater, dich zu kr\u00e4nken", "tokens": ["So", "hab'", "ich", "sonst", ",", "o", "Va\u00b7ter", ",", "dich", "zu", "kr\u00e4n\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur deinen Schmerz, nie deinen Zorn gescheut:", "tokens": ["Nur", "dei\u00b7nen", "Schmerz", ",", "nie", "dei\u00b7nen", "Zorn", "ge\u00b7scheut", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$,", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So wirk' in mir dein r\u00fchrend Angedenken,", "tokens": ["So", "wirk'", "in", "mir", "dein", "r\u00fch\u00b7rend", "An\u00b7ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als w\u00fcrd' ich noch durch deine Huld erfreut.", "tokens": ["Als", "w\u00fcrd'", "ich", "noch", "durch", "dei\u00b7ne", "Huld", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Mehr hat als du mit M\u00fch und scharfem Blicke", "tokens": ["Mehr", "hat", "als", "du", "mit", "M\u00fch", "und", "schar\u00b7fem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "KOUS", "PPER", "APPR", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr seinen Sohn kein Vater noch gewacht;", "tokens": ["F\u00fcr", "sei\u00b7nen", "Sohn", "kein", "Va\u00b7ter", "noch", "ge\u00b7wacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch hab' ich nie aus Sorgfalt f\u00fcr mein Gl\u00fccke", "tokens": ["Noch", "hab'", "ich", "nie", "aus", "Sorg\u00b7falt", "f\u00fcr", "mein", "Gl\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Wunsch um dich der Vorsicht dargebracht,", "tokens": ["Den", "Wunsch", "um", "dich", "der", "Vor\u00b7sicht", "dar\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du hie\u00dfest mich, durch Beyspiel und durch Lehren", "tokens": ["Du", "hie\u00b7\u00dfest", "mich", ",", "durch", "Bey\u00b7spiel", "und", "durch", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Nur Dem vertraun, den Alles Vater nennt,", "tokens": ["Nur", "Dem", "ver\u00b7traun", ",", "den", "Al\u00b7les", "Va\u00b7ter", "nennt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "VVINF", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "In dem wir noch den treusten Beystand ehren,", "tokens": ["In", "dem", "wir", "noch", "den", "treus\u00b7ten", "Beys\u00b7tand", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wenn Aeltern fliehn, wenn uns kein Freund mehr kennt.", "tokens": ["Wenn", "A\u00b7el\u00b7tern", "fliehn", ",", "wenn", "uns", "kein", "Freund", "mehr", "kennt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVINF", "$,", "KOUS", "PPER", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "--+-+-+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Nur deiner Huld ein dankbar Herz zu zeigen,", "tokens": ["Nur", "dei\u00b7ner", "Huld", "ein", "dank\u00b7bar", "Herz", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ART", "ADJD", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch meinen Werth den deinen zu erh\u00f6hn,", "tokens": ["Durch", "mei\u00b7nen", "Werth", "den", "dei\u00b7nen", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und, w\u00fcrde mir ein m\u00e4\u00dfig Gl\u00fcck zu eigen,", "tokens": ["Und", ",", "w\u00fcr\u00b7de", "mir", "ein", "m\u00e4\u00b7\u00dfig", "Gl\u00fcck", "zu", "ei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ART", "ADJD", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dich, mehr als mich dadurch erfreut zu sehn,", "tokens": ["Dich", ",", "mehr", "als", "mich", "da\u00b7durch", "er\u00b7freut", "zu", "sehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ADV", "KOUS", "PPER", "PAV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das w\u00fcnscht' ich dir! das solltest du erleben!", "tokens": ["Das", "w\u00fcnscht'", "ich", "dir", "!", "das", "soll\u00b7test", "du", "er\u00b7le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$.", "PDS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Lebenssatt! - wie dacht ich doch so klein!", "tokens": ["Und", "Le\u00b7bens\u00b7satt", "!", "wie", "dacht", "ich", "doch", "so", "klein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$.", "$(", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was f\u00fcr ein Wunsch! dir dieses Gl\u00fcck zu geben,", "tokens": ["Was", "f\u00fcr", "ein", "Wunsch", "!", "dir", "die\u00b7ses", "Gl\u00fcck", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "$.", "PPER", "PDAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "War das wohl werth im Himmel sp\u00e4t zu seyn?", "tokens": ["War", "das", "wohl", "werth", "im", "Him\u00b7mel", "sp\u00e4t", "zu", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "ADV", "ADJD", "APPRART", "NN", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch Er, dein Gott, an welchen stets zu denken", "tokens": ["Doch", "Er", ",", "dein", "Gott", ",", "an", "wel\u00b7chen", "stets", "zu", "den\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$,", "APPR", "PWAT", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du dich bestrebt, und deinen Sohn gelehrt,", "tokens": ["Du", "dich", "be\u00b7strebt", ",", "und", "dei\u00b7nen", "Sohn", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVFIN", "$,", "KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der st\u00e4rke mich, dir eine Lust zu schenken,", "tokens": ["Der", "st\u00e4r\u00b7ke", "mich", ",", "dir", "ei\u00b7ne", "Lust", "zu", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Engel r\u00fchrt und Ewigkeiten w\u00e4hrt.", "tokens": ["Die", "En\u00b7gel", "r\u00fchrt", "und", "E\u00b7wig\u00b7kei\u00b7ten", "w\u00e4hrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Du sollst mich sehn, am gr\u00f6\u00dften von den Tagen,", "tokens": ["Du", "sollst", "mich", "sehn", ",", "am", "gr\u00f6\u00df\u00b7ten", "von", "den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "VVINF", "$,", "APPRART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo sich vereint das Volk der Erden schaut,", "tokens": ["Wo", "sich", "ver\u00b7eint", "das", "Volk", "der", "Er\u00b7den", "schaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "VVFIN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und sollst getrost zu unserm Richter sagen:", "tokens": ["Und", "sollst", "ge\u00b7trost", "zu", "un\u00b7serm", "Rich\u00b7ter", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "VVPP", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hier bin ich, Herr! und den du mir vertraut.", "tokens": ["Hier", "bin", "ich", ",", "Herr", "!", "und", "den", "du", "mir", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "NN", "$.", "KON", "ART", "PPER", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Noch meinen Schmerz durch Dichterkunst zu zeigen", "tokens": ["Noch", "mei\u00b7nen", "Schmerz", "durch", "Dich\u00b7ter\u00b7kunst", "zu", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "APPR", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weyh' ich dies Lied, dir, liebster Vater, nicht:", "tokens": ["Wey\u00b7h'", "ich", "dies", "Lied", ",", "dir", ",", "liebs\u00b7ter", "Va\u00b7ter", ",", "nicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "PDS", "NN", "$,", "PPER", "$,", "ADJA", "NN", "$,", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.3": {"text": "Nichts dr\u00fcckt ihn aus, als ein bet\u00e4ubtes Schweigen,", "tokens": ["Nichts", "dr\u00fcckt", "ihn", "aus", ",", "als", "ein", "be\u00b7t\u00e4ub\u00b7tes", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKVZ", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das manchmal nur ein banges Ach! durchbricht:", "tokens": ["Das", "manch\u00b7mal", "nur", "ein", "ban\u00b7ges", "Ach", "!", "durch\u00b7bricht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "A