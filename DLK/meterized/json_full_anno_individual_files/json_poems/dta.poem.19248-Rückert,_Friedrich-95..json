{"dta.poem.19248": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "95.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du erkennen willst den Ruhm in seiner Bl\u00f6\u00dfe,", "tokens": ["Wenn", "du", "er\u00b7ken\u00b7nen", "willst", "den", "Ruhm", "in", "sei\u00b7ner", "Bl\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vergleich am Himmel ihn mit Sternen erster Gr\u00f6\u00dfe.", "tokens": ["Ver\u00b7gleich", "am", "Him\u00b7mel", "ihn", "mit", "Ster\u00b7nen", "ers\u00b7ter", "Gr\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "PPER", "APPR", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die letzter Gr\u00f6\u00dfe, sind sie etwa minder gro\u00df?", "tokens": ["Die", "letz\u00b7ter", "Gr\u00f6\u00b7\u00dfe", ",", "sind", "sie", "et\u00b7wa", "min\u00b7der", "gro\u00df", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie scheinen kleiner dir durch ihre H\u00f6he blo\u00df.", "tokens": ["Sie", "schei\u00b7nen", "klei\u00b7ner", "dir", "durch", "ih\u00b7re", "H\u00f6\u00b7he", "blo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum l\u00e4chle, r\u00fcckt man dich zum letzten Range nieder;", "tokens": ["Drum", "l\u00e4ch\u00b7le", ",", "r\u00fcckt", "man", "dich", "zum", "letz\u00b7ten", "Ran\u00b7ge", "nie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,", "VVFIN", "PIS", "PRF", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und r\u00fcckt man dich empor zum ersten, l\u00e4chle wieder!", "tokens": ["Und", "r\u00fcckt", "man", "dich", "em\u00b7por", "zum", "ers\u00b7ten", ",", "l\u00e4ch\u00b7le", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PPER", "PTKVZ", "APPRART", "ADJA", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}