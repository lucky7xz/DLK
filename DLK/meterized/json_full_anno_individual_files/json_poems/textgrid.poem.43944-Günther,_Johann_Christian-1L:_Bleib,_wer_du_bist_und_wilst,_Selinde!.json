{"textgrid.poem.43944": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bleib, wer du bist und wilst, Selinde!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bleib, wer du bist und wilst, Selinde!", "tokens": ["Bleib", ",", "wer", "du", "bist", "und", "wilst", ",", "Se\u00b7lin\u00b7de", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PWS", "PPER", "VAFIN", "KON", "VMFIN", "$,", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich bleibe gleichfalls, wer ich bin.", "tokens": ["Ich", "blei\u00b7be", "gleich\u00b7falls", ",", "wer", "ich", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herz besteht wie Rohr am Winde;", "tokens": ["Dein", "Herz", "be\u00b7steht", "wie", "Rohr", "am", "Win\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Daf\u00fcr bedanckt sich nun mein Sinn", "tokens": ["Da\u00b7f\u00fcr", "be\u00b7danckt", "sich", "nun", "mein", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und w\u00fcntscht dir zu der guten Zeit", "tokens": ["Und", "w\u00fcnt\u00b7scht", "dir", "zu", "der", "gu\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nichts weiter als Best\u00e4ndigkeit.", "tokens": ["Nichts", "wei\u00b7ter", "als", "Be\u00b7st\u00e4n\u00b7dig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du h\u00e4ngst dich, wie ich seh, an alle", "tokens": ["Du", "h\u00e4ngst", "dich", ",", "wie", "ich", "seh", ",", "an", "al\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPR", "PIAT"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und siehst das Herze nicht mehr an.", "tokens": ["Und", "siehst", "das", "Her\u00b7ze", "nicht", "mehr", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PDS", "VVFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich geh und r\u00e4ume deinem Falle;", "tokens": ["Ich", "geh", "und", "r\u00e4u\u00b7me", "dei\u00b7nem", "Fal\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er kommt, der Hochmuth kommt voran,", "tokens": ["Er", "kommt", ",", "der", "Hoch\u00b7muth", "kommt", "vo\u00b7ran", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Spott aber, Reue, Gram und Schmach", "tokens": ["Spott", "a\u00b7ber", ",", "Reu\u00b7e", ",", "Gram", "und", "Schmach"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Folgt wie der Rauch dem Brande nach.", "tokens": ["Folgt", "wie", "der", "Rauch", "dem", "Bran\u00b7de", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Eh soll der Himmel B\u00e4ume tragen", "tokens": ["Eh", "soll", "der", "Him\u00b7mel", "B\u00e4u\u00b7me", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "NN", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Und unser Queis voll Flammen stehn", "tokens": ["Und", "un\u00b7ser", "Que\u00b7is", "voll", "Flam\u00b7men", "stehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "NN", "VVINF"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Als jemand auf der Erde sagen:", "tokens": ["Als", "je\u00b7mand", "auf", "der", "Er\u00b7de", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Selinde l\u00e4st den Philimen.", "tokens": ["Se\u00b7lin\u00b7de", "l\u00e4st", "den", "Phi\u00b7li\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.5": {"text": "Besinnstu dich noch auf die Nacht,", "tokens": ["Be\u00b7sinns\u00b7tu", "dich", "noch", "auf", "die", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die dieser Schwur vergn\u00fcgt gemacht?", "tokens": ["Die", "die\u00b7ser", "Schwur", "ver\u00b7gn\u00fcgt", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun gr\u00fcne, lieber Himmel, gr\u00fcne", "tokens": ["Nun", "gr\u00fc\u00b7ne", ",", "lie\u00b7ber", "Him\u00b7mel", ",", "gr\u00fc\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "ADV", "NN", "$,", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und gieb dem Quei\u00dfe deine Glut,", "tokens": ["Und", "gieb", "dem", "Que\u00b7i\u00b7\u00dfe", "dei\u00b7ne", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Damit es der zur Ausflucht diene,", "tokens": ["Da\u00b7mit", "es", "der", "zur", "Aus\u00b7flucht", "die\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die wider ihr Geseze thut", "tokens": ["Die", "wi\u00b7der", "ihr", "Ge\u00b7se\u00b7ze", "thut"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und, wo kein Wunderwerck geschieht,", "tokens": ["Und", ",", "wo", "kein", "Wun\u00b7der\u00b7werck", "ge\u00b7schieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Rache nimmermehr entflieht!", "tokens": ["Der", "Ra\u00b7che", "nim\u00b7mer\u00b7mehr", "ent\u00b7flieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Mit was vor Ruh und vor Gewi\u00dfen", "tokens": ["Mit", "was", "vor", "Ruh", "und", "vor", "Ge\u00b7wi\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gedenckstu, falsches Kind, der Lust", "tokens": ["Ge\u00b7dencks\u00b7tu", ",", "fal\u00b7sches", "Kind", ",", "der", "Lust"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ADJA", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In fremden Armen zu genie\u00dfen,", "tokens": ["In", "frem\u00b7den", "Ar\u00b7men", "zu", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wobey du allzeit f\u00fcrchten must,", "tokens": ["Wo\u00b7bey", "du", "all\u00b7zeit", "f\u00fcrch\u00b7ten", "must", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jezt trenne Donner, Bliz und Streich", "tokens": ["Jezt", "tren\u00b7ne", "Don\u00b7ner", ",", "Bliz", "und", "Streich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ku\u00df, Mund und Herzen unter euch?", "tokens": ["Ku\u00df", ",", "Mund", "und", "Her\u00b7zen", "un\u00b7ter", "euch", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein andrer w\u00fcrd es w\u00fcntschen k\u00f6nnen,", "tokens": ["Ein", "an\u00b7drer", "w\u00fcrd", "es", "w\u00fcnt\u00b7schen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "PPER", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich aber bin nicht aufgelegt,", "tokens": ["Ich", "a\u00b7ber", "bin", "nicht", "auf\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Feinden meinen Zorn zu g\u00f6nnen;", "tokens": ["Den", "Fein\u00b7den", "mei\u00b7nen", "Zorn", "zu", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe, so mich treibt und regt,", "tokens": ["Die", "Lie\u00b7be", ",", "so", "mich", "treibt", "und", "regt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4st fahren, was nicht bleiben will,", "tokens": ["L\u00e4st", "fah\u00b7ren", ",", "was", "nicht", "blei\u00b7ben", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "$,", "PRELS", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schweigt wie fromme Kinder still.", "tokens": ["Und", "schweigt", "wie", "from\u00b7me", "Kin\u00b7der", "still", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Genug, da\u00df du dich selbst betrogen", "tokens": ["Ge\u00b7nug", ",", "da\u00df", "du", "dich", "selbst", "be\u00b7tro\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und etwas wider dich gethan.", "tokens": ["Und", "et\u00b7was", "wi\u00b7der", "dich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bedenck, ich war dir so gewogen,", "tokens": ["Be\u00b7denck", ",", "ich", "war", "dir", "so", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als keiner ist und werden kan,", "tokens": ["Als", "kei\u00b7ner", "ist", "und", "wer\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "KON", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich zeigte dir durch wahre Treu,", "tokens": ["Ich", "zeig\u00b7te", "dir", "durch", "wah\u00b7re", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Leben und was Lieben sey.", "tokens": ["Was", "Le\u00b7ben", "und", "was", "Lie\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "PWS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Eintracht zwo vertrauter Herzen", "tokens": ["Die", "Ein\u00b7tracht", "zwo", "ver\u00b7trau\u00b7ter", "Her\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "CARD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Macht aus der Welt ein Himmelreich,", "tokens": ["Macht", "aus", "der", "Welt", "ein", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Ihr reiner Ku\u00df verbei\u00dft den Schmerzen,", "tokens": ["Ihr", "rei\u00b7ner", "Ku\u00df", "ver\u00b7bei\u00dft", "den", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr Auge kommt der Sonne gleich,", "tokens": ["Ihr", "Au\u00b7ge", "kommt", "der", "Son\u00b7ne", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wolck und Regen um sich sieht", "tokens": ["Die", "Wolck", "und", "Re\u00b7gen", "um", "sich", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und doch davon nichts in sich zieht.", "tokens": ["Und", "doch", "da\u00b7von", "nichts", "in", "sich", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PAV", "PIS", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Den Vorschmack hastu schon geno\u00dfen,", "tokens": ["Den", "Vor\u00b7schmack", "has\u00b7tu", "schon", "ge\u00b7no\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Betrachte Felsen, Bach und Wald,", "tokens": ["Be\u00b7trach\u00b7te", "Fel\u00b7sen", ",", "Bach", "und", "Wald", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "NE", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo ich dich oft in Arm geschlo\u00dfen", "tokens": ["Wo", "ich", "dich", "oft", "in", "Arm", "ge\u00b7schlo\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PRF", "ADV", "APPR", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und unser Scherz noch widerschallt;", "tokens": ["Und", "un\u00b7ser", "Scherz", "noch", "wi\u00b7der\u00b7schallt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die V\u00f6gel wurden selbst erweckt", "tokens": ["Die", "V\u00f6\u00b7gel", "wur\u00b7den", "selbst", "er\u00b7weckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und durch Exempel angesteckt.", "tokens": ["Und", "durch", "Ex\u00b7em\u00b7pel", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "me