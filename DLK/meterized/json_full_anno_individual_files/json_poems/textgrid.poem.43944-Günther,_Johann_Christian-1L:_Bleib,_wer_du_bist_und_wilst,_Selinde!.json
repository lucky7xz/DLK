}}, "stanza.19": {"line.1": {"text": "Genug, da\u00df du dich selbst betrogen", "tokens": ["Ge\u00b7nug", ",", "da\u00df", "du", "dich", "selbst", "be\u00b7tro\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PPER", "PRF", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und etwas wider dich gethan.", "tokens": ["Und", "et\u00b7was", "wi\u00b7der", "dich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bedenck, ich war dir so gewogen,", "tokens": ["Be\u00b7denck", ",", "ich", "war", "dir", "so", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als keiner ist und werden kan,", "tokens": ["Als", "kei\u00b7ner", "ist", "und", "wer\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VAFIN", "KON", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich zeigte dir durch wahre Treu,", "tokens": ["Ich", "zeig\u00b7te", "dir", "durch", "wah\u00b7re", "Treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Leben und was Lieben sey.", "tokens": ["Was", "Le\u00b7ben", "und", "was", "Lie\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "PWS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Die Eintracht zwo vertrauter