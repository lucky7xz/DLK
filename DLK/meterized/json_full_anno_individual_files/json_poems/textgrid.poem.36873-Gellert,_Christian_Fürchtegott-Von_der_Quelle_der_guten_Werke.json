s": ["KOUS", "NN", "KON", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So ist dir alles kein Gewinn.", "tokens": ["So", "ist", "dir", "al\u00b7les", "kein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Tu Taten, die der Heldenmut", "tokens": ["Tu", "Ta\u00b7ten", ",", "die", "der", "Hel\u00b7den\u00b7mut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch jemals hat verrichten k\u00f6nnen:", "tokens": ["Noch", "je\u00b7mals", "hat", "ver\u00b7rich\u00b7ten", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergie\u00df f\u00fcrs Vaterland dein Blut,", "tokens": ["Ver\u00b7gie\u00df", "f\u00fcrs", "Va\u00b7ter\u00b7land", "dein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df deinen Leib f\u00fcr andre brennen!", "tokens": ["La\u00df", "dei\u00b7nen", "Leib", "f\u00fcr", "and\u00b7re", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Beseelet dich nicht Lieb und Pflicht;", "tokens": ["Be\u00b7see\u00b7let", "dich", "nicht", "Lieb", "und", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bist du die Absicht deiner Taten:", "tokens": ["Bist", "du", "die", "Ab\u00b7sicht", "dei\u00b7ner", "Ta\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sch\u00fctz und rette ganze Staaten;", "tokens": ["So", "sch\u00fctz", "und", "ret\u00b7te", "gan\u00b7ze", "Staa\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gott achtet deiner Werke nicht.", "tokens": ["Gott", "ach\u00b7tet", "dei\u00b7ner", "Wer\u00b7ke", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "L\u00e4g ihm an unsern Werken nur:", "tokens": ["L\u00e4g", "ihm", "an", "un\u00b7sern", "Wer\u00b7ken", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So k\u00f6nnt er uns, sie zu vollbringen,", "tokens": ["So", "k\u00f6nnt", "er", "uns", ",", "sie", "zu", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sehr leicht durch Fessel der Natur,", "tokens": ["Sehr", "leicht", "durch", "Fes\u00b7sel", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Kr\u00e4fte seiner Allmacht zwingen.", "tokens": ["Durch", "Kr\u00e4f\u00b7te", "sei\u00b7ner", "All\u00b7macht", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor ihm, der alles schafft und gibt,", "tokens": ["Vor", "ihm", ",", "der", "al\u00b7les", "schafft", "und", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gilt Weisheit nichts, nichts Macht und St\u00e4rke.", "tokens": ["Gilt", "Weis\u00b7heit", "nichts", ",", "nichts", "Macht", "und", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PIS", "$,", "PIS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er will die Absicht deiner Werke,", "tokens": ["Er", "will", "die", "Ab\u00b7sicht", "dei\u00b7ner", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Herz, das ihn verehrt und liebt.", "tokens": ["Ein", "Herz", ",", "das", "ihn", "ver\u00b7ehrt", "und", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Ein Herz, von Eigenliebe fern,", "tokens": ["Ein", "Herz", ",", "von", "Ei\u00b7gen\u00b7lie\u00b7be", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern von des Stolzes eitlem Triebe,", "tokens": ["Fern", "von", "des", "Stol\u00b7zes", "eit\u00b7lem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Geheiligt durch die Furcht des Herrn,", "tokens": ["Ge\u00b7hei\u00b7ligt", "durch", "die", "Furcht", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erneut durch Glauben zu der Liebe;", "tokens": ["Er\u00b7neut", "durch", "Glau\u00b7ben", "zu", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dies ist's, was Gott von uns verlangt.", "tokens": ["Dies", "ist's", ",", "was", "Gott", "von", "uns", "ver\u00b7langt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn wir nicht dies Herz besitzen:", "tokens": ["Und", "wenn", "wir", "nicht", "dies", "Herz", "be\u00b7sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So wird ein Leben uns nichts n\u00fctzen,", "tokens": ["So", "wird", "ein", "Le\u00b7ben", "uns", "nichts", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das mit den gr\u00f6\u00dften Taten prangt.", "tokens": ["Das", "mit", "den", "gr\u00f6\u00df\u00b7ten", "Ta\u00b7ten", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Drum t\u00e4usche dich nicht durch den Schein,", "tokens": ["Drum", "t\u00e4u\u00b7sche", "dich", "nicht", "durch", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht durch der Tugend blo\u00dfen Namen.", "tokens": ["Nicht", "durch", "der", "Tu\u00b7gend", "blo\u00b7\u00dfen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh nicht auf deine Werk allein;", "tokens": ["Sieh", "nicht", "auf", "dei\u00b7ne", "Werk", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh auf den Quell, aus dem sie kamen.", "tokens": ["Sieh", "auf", "den", "Quell", ",", "aus", "dem", "sie", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Pr\u00fcf dic