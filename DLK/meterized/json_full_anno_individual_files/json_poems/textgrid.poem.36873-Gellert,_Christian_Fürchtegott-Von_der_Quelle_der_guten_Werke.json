", "tokens": ["Du", "wirst", "die", "Ge\u00b7gen\u00b7wart", "von", "ihr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An Liebe zu dem N\u00e4chsten merken.", "tokens": ["An", "Lie\u00b7be", "zu", "dem", "N\u00e4chs\u00b7ten", "mer\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die Liebe, die dich schm\u00fccken soll,", "tokens": ["Die", "Lie\u00b7be", ",", "die", "dich", "schm\u00fc\u00b7cken", "soll", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist g\u00fctig; ohne List und T\u00fccke;", "tokens": ["Ist", "g\u00fc\u00b7tig", ";", "oh\u00b7ne", "List", "und", "T\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$.", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Beneidet nicht des N\u00e4chsten Gl\u00fccke;", "tokens": ["Be\u00b7nei\u00b7det", "nicht", "des", "N\u00e4chs\u00b7ten", "Gl\u00fc\u00b7cke", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sie bl\u00e4ht sich nicht; ist langmutsvoll.", "tokens": ["Sie", "bl\u00e4ht", "sich", "nicht", ";", "ist", "lang\u00b7muts\u00b7voll", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$.", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.