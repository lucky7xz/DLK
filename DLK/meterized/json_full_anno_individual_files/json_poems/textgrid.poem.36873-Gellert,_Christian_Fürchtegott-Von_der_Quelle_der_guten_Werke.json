{"textgrid.poem.36873": {"metadata": {"author": {"name": "Gellert, Christian F\u00fcrchtegott", "birth": "N.A.", "death": "N.A."}, "title": "Von der Quelle der guten Werke", "genre": "verse", "period": "N.A.", "pub_year": 1742, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn zur Vollf\u00fchrung deiner Pflicht", "tokens": ["Wenn", "zur", "Voll\u00b7f\u00fch\u00b7rung", "dei\u00b7ner", "Pflicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPOSAT", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Dich Gottes Liebe nicht beseelet:", "tokens": ["Dich", "Got\u00b7tes", "Lie\u00b7be", "nicht", "be\u00b7see\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So r\u00fchme dich der Tugend nicht,", "tokens": ["So", "r\u00fch\u00b7me", "dich", "der", "Tu\u00b7gend", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wisse, da\u00df dir alles fehlet.", "tokens": ["Und", "wis\u00b7se", ",", "da\u00df", "dir", "al\u00b7les", "feh\u00b7let", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Vorteil, Wollust, Eigensinn", "tokens": ["Wenn", "Vor\u00b7teil", ",", "Wol\u00b7lust", ",", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["KOUS", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Stolz dir nur das Gute raten;", "tokens": ["Und", "Stolz", "dir", "nur", "das", "Gu\u00b7te", "ra\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So tue noch so gute Taten;", "tokens": ["So", "tue", "noch", "so", "gu\u00b7te", "Ta\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Du hast vor Gott den Lohn dahin.", "tokens": ["Du", "hast", "vor", "Gott", "den", "Lohn", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sei durch die Gaben der Natur", "tokens": ["Sei", "durch", "die", "Ga\u00b7ben", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Wunder und das Gl\u00fcck der Erden!", "tokens": ["Das", "Wun\u00b7der", "und", "das", "Gl\u00fcck", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Begl\u00fcckest du die Menschen nur,", "tokens": ["Be\u00b7gl\u00fc\u00b7ckest", "du", "die", "Men\u00b7schen", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um vor der Welt geehrt zu werden;", "tokens": ["Um", "vor", "der", "Welt", "ge\u00b7ehrt", "zu", "wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "NN", "VVPP", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erf\u00fclle die Liebe nicht dein Herz:", "tokens": ["Er\u00b7f\u00fcl\u00b7le", "die", "Lie\u00b7be", "nicht", "dein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "So bist du bei den gr\u00f6\u00dften Gaben,", "tokens": ["So", "bist", "du", "bei", "den", "gr\u00f6\u00df\u00b7ten", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Bei dem Verstand, den Engel haben,", "tokens": ["Bei", "dem", "Ver\u00b7stand", ",", "den", "En\u00b7gel", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor Gott doch nur ein t\u00f6nend Erz.", "tokens": ["Vor", "Gott", "doch", "nur", "ein", "t\u00f6\u00b7nend", "Erz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bau H\u00e4user auf, und brich dein Brot,", "tokens": ["Bau", "H\u00e4u\u00b7ser", "auf", ",", "und", "brich", "dein", "Brot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,", "KON", "VVIMP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Volk der Armen zu verpflegen;", "tokens": ["Das", "Volk", "der", "Ar\u00b7men", "zu", "ver\u00b7pfle\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Entrei\u00df die Witwen ihrer Not,", "tokens": ["Ent\u00b7rei\u00df", "die", "Wit\u00b7wen", "ih\u00b7rer", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sei der Waisen Schutz und Segen!", "tokens": ["Und", "sei", "der", "Wai\u00b7sen", "Schutz", "und", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gib alle deine Habe hin!", "tokens": ["Gib", "al\u00b7le", "dei\u00b7ne", "Ha\u00b7be", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch hast du nichts vor Gott gegeben.", "tokens": ["Noch", "hast", "du", "nichts", "vor", "Gott", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn Lieb und Pflicht dich nicht beleben:", "tokens": ["Wenn", "Lieb", "und", "Pflicht", "dich", "nicht", "be\u00b7le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So ist dir alles kein Gewinn.", "tokens": ["So", "ist", "dir", "al\u00b7les", "kein", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIS", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Tu Taten, die der Heldenmut", "tokens": ["Tu", "Ta\u00b7ten", ",", "die", "der", "Hel\u00b7den\u00b7mut"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch jemals hat verrichten k\u00f6nnen:", "tokens": ["Noch", "je\u00b7mals", "hat", "ver\u00b7rich\u00b7ten", "k\u00f6n\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vergie\u00df f\u00fcrs Vaterland dein Blut,", "tokens": ["Ver\u00b7gie\u00df", "f\u00fcrs", "Va\u00b7ter\u00b7land", "dein", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "La\u00df deinen Leib f\u00fcr andre brennen!", "tokens": ["La\u00df", "dei\u00b7nen", "Leib", "f\u00fcr", "and\u00b7re", "bren\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Beseelet dich nicht Lieb und Pflicht;", "tokens": ["Be\u00b7see\u00b7let", "dich", "nicht", "Lieb", "und", "Pflicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bist du die Absicht deiner Taten:", "tokens": ["Bist", "du", "die", "Ab\u00b7sicht", "dei\u00b7ner", "Ta\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sch\u00fctz und rette ganze Staaten;", "tokens": ["So", "sch\u00fctz", "und", "ret\u00b7te", "gan\u00b7ze", "Staa\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Gott achtet deiner Werke nicht.", "tokens": ["Gott", "ach\u00b7tet", "dei\u00b7ner", "Wer\u00b7ke", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "L\u00e4g ihm an unsern Werken nur:", "tokens": ["L\u00e4g", "ihm", "an", "un\u00b7sern", "Wer\u00b7ken", "nur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So k\u00f6nnt er uns, sie zu vollbringen,", "tokens": ["So", "k\u00f6nnt", "er", "uns", ",", "sie", "zu", "voll\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sehr leicht durch Fessel der Natur,", "tokens": ["Sehr", "leicht", "durch", "Fes\u00b7sel", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Kr\u00e4fte seiner Allmacht zwingen.", "tokens": ["Durch", "Kr\u00e4f\u00b7te", "sei\u00b7ner", "All\u00b7macht", "zwin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vor ihm, der alles schafft und gibt,", "tokens": ["Vor", "ihm", ",", "der", "al\u00b7les", "schafft", "und", "gibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PRELS", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Gilt Weisheit nichts, nichts Macht und St\u00e4rke.", "tokens": ["Gilt", "Weis\u00b7heit", "nichts", ",", "nichts", "Macht", "und", "St\u00e4r\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PIS", "$,", "PIS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er will die Absicht deiner Werke,", "tokens": ["Er", "will", "die", "Ab\u00b7sicht", "dei\u00b7ner", "Wer\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Herz, das ihn verehrt und liebt.", "tokens": ["Ein", "Herz", ",", "das", "ihn", "ver\u00b7ehrt", "und", "liebt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Herz, von Eigenliebe fern,", "tokens": ["Ein", "Herz", ",", "von", "Ei\u00b7gen\u00b7lie\u00b7be", "fern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fern von des Stolzes eitlem Triebe,", "tokens": ["Fern", "von", "des", "Stol\u00b7zes", "eit\u00b7lem", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Geheiligt durch die Furcht des Herrn,", "tokens": ["Ge\u00b7hei\u00b7ligt", "durch", "die", "Furcht", "des", "Herrn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erneut durch Glauben zu der Liebe;", "tokens": ["Er\u00b7neut", "durch", "Glau\u00b7ben", "zu", "der", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dies ist's, was Gott von uns verlangt.", "tokens": ["Dies", "ist's", ",", "was", "Gott", "von", "uns", "ver\u00b7langt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wenn wir nicht dies Herz besitzen:", "tokens": ["Und", "wenn", "wir", "nicht", "dies", "Herz", "be\u00b7sit\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "PDS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So wird ein Leben uns nichts n\u00fctzen,", "tokens": ["So", "wird", "ein", "Le\u00b7ben", "uns", "nichts", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das mit den gr\u00f6\u00dften Taten prangt.", "tokens": ["Das", "mit", "den", "gr\u00f6\u00df\u00b7ten", "Ta\u00b7ten", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Drum t\u00e4usche dich nicht durch den Schein,", "tokens": ["Drum", "t\u00e4u\u00b7sche", "dich", "nicht", "durch", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht durch der Tugend blo\u00dfen Namen.", "tokens": ["Nicht", "durch", "der", "Tu\u00b7gend", "blo\u00b7\u00dfen", "Na\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh nicht auf deine Werk allein;", "tokens": ["Sieh", "nicht", "auf", "dei\u00b7ne", "Werk", "al\u00b7lein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieh auf den Quell, aus dem sie kamen.", "tokens": ["Sieh", "auf", "den", "Quell", ",", "aus", "dem", "sie", "ka\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Pr\u00fcf dich vor Gottes Angesicht,", "tokens": ["Pr\u00fcf", "dich", "vor", "Got\u00b7tes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF",