{"dta.poem.3092": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "137.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dich irrt der ew'ge Krieg in Wasser, Luft und Erden,", "tokens": ["Dich", "irrt", "der", "ew'\u00b7ge", "Krieg", "in", "Was\u00b7ser", ",", "Luft", "und", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Fressen der Gesch\u00f6pf' und ihr Gefressenwerden.", "tokens": ["Das", "Fres\u00b7sen", "der", "Ge\u00b7sch\u00f6pf'", "und", "ihr", "Ge\u00b7fres\u00b7sen\u00b7wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du fragst, ob keine Welt geschaffen konnte seyn,", "tokens": ["Du", "fragst", ",", "ob", "kei\u00b7ne", "Welt", "ge\u00b7schaf\u00b7fen", "konn\u00b7te", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PIAT", "NN", "VVINF", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ganz Leben, ohne Tod? mein Sohn, ich denke, nein!", "tokens": ["Ganz", "Le\u00b7ben", ",", "oh\u00b7ne", "Tod", "?", "mein", "Sohn", ",", "ich", "den\u00b7ke", ",", "nein", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUI", "NN", "$.", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "$,", "PTKANT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich frage: F\u00fchlest du dich selbst nicht wohlgemacht?", "tokens": ["Ich", "fra\u00b7ge", ":", "F\u00fch\u00b7lest", "du", "dich", "selbst", "nicht", "wohl\u00b7ge\u00b7macht", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVFIN", "PPER", "PRF", "ADV", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denk alles andre denn f\u00fcr dich hervorgebracht,", "tokens": ["Denk", "al\u00b7les", "and\u00b7re", "denn", "f\u00fcr", "dich", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIS", "PIS", "ADV", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Um dich und alle, die du liebest, zu ern\u00e4hren.", "tokens": ["Um", "dich", "und", "al\u00b7le", ",", "die", "du", "lie\u00b7best", ",", "zu", "er\u00b7n\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "PPER", "KON", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nun aber: kann der Tod das Leben wol geb\u00e4ren?", "tokens": ["Nun", "a\u00b7ber", ":", "kann", "der", "Tod", "das", "Le\u00b7ben", "wol", "ge\u00b7b\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$.", "VMFIN", "ART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum lebt und n\u00e4hret sich, was dir soll Nahrung geben;", "tokens": ["Drum", "lebt", "und", "n\u00e4h\u00b7ret", "sich", ",", "was", "dir", "soll", "Nah\u00b7rung", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "KON", "VVFIN", "PRF", "$,", "PWS", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du freue dich, wieviel' um deinetwillen leben!", "tokens": ["Du", "freu\u00b7e", "dich", ",", "wie\u00b7viel'", "um", "dei\u00b7net\u00b7wil\u00b7len", "le\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "VVFIN", "APPR", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und was nicht deinem Leib, gibt Nahrung deinem Geist;", "tokens": ["Und", "was", "nicht", "dei\u00b7nem", "Leib", ",", "gibt", "Nah\u00b7rung", "dei\u00b7nem", "Geist", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PTKNEG", "PPOSAT", "NN", "$,", "VVFIN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du freu der Tafel dich, der reichen, die dich speist!", "tokens": ["Du", "freu", "der", "Ta\u00b7fel", "dich", ",", "der", "rei\u00b7chen", ",", "die", "dich", "speist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}