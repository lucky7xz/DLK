{"dta.poem.8742": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XxI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Jene Schatten", "tokens": ["Je\u00b7ne", "Schat\u00b7ten"], "token_info": ["word", "word"], "pos": ["PDAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ueber Matten", "tokens": ["Ue\u00b7ber", "Mat\u00b7ten"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Um den Quell,", "tokens": ["Um", "den", "Quell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Reich an losen", "tokens": ["Reich", "an", "lo\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Hagerosen,", "tokens": ["Ha\u00b7ge\u00b7ro\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Kommt zu kosen,", "tokens": ["Kommt", "zu", "ko\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVIMP", "PTKZU", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Br\u00fcder, schnell!", "tokens": ["Br\u00fc\u00b7der", ",", "schnell", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Kaum gefunden,", "tokens": ["Kaum", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Schon umwunden,", "tokens": ["Schon", "um\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Schon verbunden,", "tokens": ["Schon", "ver\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Wei\u00df ich wie?", "tokens": ["Wei\u00df", "ich", "wie", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PWAV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Keiner h\u00f6hne,", "tokens": ["Kei\u00b7ner", "h\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Musens\u00f6hne,", "tokens": ["Mu\u00b7sen\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Diese sch\u00f6ne", "tokens": ["Die\u00b7se", "sch\u00f6\u00b7ne"], "token_info": ["word", "word"], "pos": ["PDAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Sympathie!", "tokens": ["Sym\u00b7pa\u00b7thie", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+--", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Jubelt, bringet", "tokens": ["Ju\u00b7belt", ",", "brin\u00b7get"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Dank und singet,", "tokens": ["Dank", "und", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Welle klinget,", "tokens": ["Wel\u00b7le", "klin\u00b7get", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Rose bl\u00fcht:", "tokens": ["Ro\u00b7se", "bl\u00fcht", ":"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Das in Wonnen", "tokens": ["Das", "in", "Won\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["PDS", "APPR", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nie zerronnen,", "tokens": ["Nie", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Welch besonnen,", "tokens": ["Welch", "be\u00b7son\u00b7nen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Kalt Gem\u00fcth!", "tokens": ["Kalt", "Ge\u00b7m\u00fcth", "!"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "V\u00f6gel neigen", "tokens": ["V\u00f6\u00b7gel", "nei\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Aus den Zweigen,", "tokens": ["Aus", "den", "Zwei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.3": {"text": "Hei\u00dfen schweigen", "tokens": ["Hei\u00b7\u00dfen", "schwei\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Mich zulezt:", "tokens": ["Mich", "zu\u00b7lezt", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPER", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wer beschriebe", "tokens": ["Wer", "be\u00b7schrie\u00b7be"], "token_info": ["word", "word"], "pos": ["PWS", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Lenzestriebe,", "tokens": ["Len\u00b7zes\u00b7trie\u00b7be", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Wer die Liebe,", "tokens": ["Wer", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Wer das Jezt?", "tokens": ["Wer", "das", "Jezt", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PDS", "ADV", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}