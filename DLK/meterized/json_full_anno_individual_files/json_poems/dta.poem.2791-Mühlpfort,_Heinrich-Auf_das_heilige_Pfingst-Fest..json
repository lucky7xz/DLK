{"dta.poem.2791": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf das heilige Pfingst-Fest.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wesendliche Flamme/", "tokens": ["We\u00b7send\u00b7li\u00b7che", "Flam\u00b7me", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+---+-", "measure": "dactylic.init"}, "line.2": {"text": "Heisse Liebes-Amme/", "tokens": ["Heis\u00b7se", "Lie\u00b7bes\u00b7Am\u00b7me", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "O GOtt heil\u2019ger Geist/", "tokens": ["O", "Gott", "heil'\u00b7ger", "Geist", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADJA", "NN", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Brenne mit der Kertze/", "tokens": ["Bren\u00b7ne", "mit", "der", "Kert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Doch mein tr\u00e4ges Hertze/", "tokens": ["Doch", "mein", "tr\u00e4\u00b7ges", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Das mit Frost beeist.", "tokens": ["Das", "mit", "Frost", "be\u00b7eist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Weil auf allen Seiten/", "tokens": ["Weil", "auf", "al\u00b7len", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PIAT", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.8": {"text": "St\u00fcndlich ich mu\u00df streiten/", "tokens": ["St\u00fcnd\u00b7lich", "ich", "mu\u00df", "strei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Ach/ so steh mir bey!", "tokens": ["Ach", "/", "so", "steh", "mir", "bey", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "ADV", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.10": {"text": "Hilff mir hurtig k\u00e4mpffen/", "tokens": ["Hilff", "mir", "hur\u00b7tig", "k\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.11": {"text": "Da\u00df ich m\u00f6ge d\u00e4mpffen/", "tokens": ["Da\u00df", "ich", "m\u00f6\u00b7ge", "d\u00e4mpf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.12": {"text": "Der Welt Gauckeley.", "tokens": ["Der", "Welt", "Gau\u00b7cke\u00b7ley", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.13": {"text": "Finstern\u00fc\u00df und Schrecken", "tokens": ["Fins\u00b7ter\u00b7n\u00fc\u00df", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Wollen mich bedecken/", "tokens": ["Wol\u00b7len", "mich", "be\u00b7de\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.15": {"text": "Fleisch und Blut ist schlimm/", "tokens": ["Fleisch", "und", "Blut", "ist", "schlimm", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.16": {"text": "Hilfft die Waffen sch\u00e4rffen/", "tokens": ["Hilfft", "die", "Waf\u00b7fen", "sch\u00e4rf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.17": {"text": "Dem/ der mich verwerffen", "tokens": ["Dem", "/", "der", "mich", "ver\u00b7werf\u00b7fen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ART", "$(", "PRELS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Will in seinem Grimm.", "tokens": ["Will", "in", "sei\u00b7nem", "Grimm", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Wenn es blitzt und schneiet/", "tokens": ["Wenn", "es", "blitzt", "und", "schnei\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.20": {"text": "Wenn es Donner speyet/", "tokens": ["Wenn", "es", "Don\u00b7ner", "spey\u00b7et", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.21": {"text": "So bewahre mich/", "tokens": ["So", "be\u00b7wah\u00b7re", "mich", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.22": {"text": "Wenn die Welt erhitzet/", "tokens": ["Wenn", "die", "Welt", "er\u00b7hit\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.23": {"text": "Jhre Pfeile spitzet/", "tokens": ["Ih\u00b7re", "Pfei\u00b7le", "spit\u00b7zet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.24": {"text": "Lehn ich mich auf dich.", "tokens": ["Lehn", "ich", "mich", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.25": {"text": "Du kanst mir im Leben", "tokens": ["Du", "kanst", "mir", "im", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Kraft und Tr\u00f6stung geben;", "tokens": ["Kraft", "und", "Tr\u00f6s\u00b7tung", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.27": {"text": "Unn wenn ich nun soll", "tokens": ["Unn", "wenn", "ich", "nun", "soll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "ADV", "VMFIN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.28": {"text": "Von dem Jrrweg scheiden/", "tokens": ["Von", "dem", "Jrr\u00b7weg", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.29": {"text": "F\u00fchr mich ein zur Freuden/", "tokens": ["F\u00fchr", "mich", "ein", "zur", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "APPRART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.30": {"text": "Da mir ewig wohl.", "tokens": ["Da", "mir", "e\u00b7wig", "wohl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.31": {"text": "St\u00e4rcke mich im Glauben/", "tokens": ["St\u00e4r\u00b7cke", "mich", "im", "Glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.32": {"text": "Da\u00df mir nicht kan rauben", "tokens": ["Da\u00df", "mir", "nicht", "kan", "rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.33": {"text": "Satan meine Seel!", "tokens": ["Sa\u00b7tan", "mei\u00b7ne", "Seel", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.34": {"text": "Mu\u00df ich auch gleich fliehen/", "tokens": ["Mu\u00df", "ich", "auch", "gleich", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.35": {"text": "So wil ich beziehen", "tokens": ["So", "wil", "ich", "be\u00b7zie\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Jesu Seiten H\u00f6l.", "tokens": ["Je\u00b7su", "Sei\u00b7ten", "H\u00f6l", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.37": {"text": "Heilger Geist erquicke", "tokens": ["Heil\u00b7ger", "Geist", "er\u00b7qui\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.38": {"text": "Mich/ wenn ich zudr\u00fccke", "tokens": ["Mich", "/", "wenn", "ich", "zu\u00b7dr\u00fc\u00b7cke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "KOUS", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.39": {"text": "Meiner Augen paar!", "tokens": ["Mei\u00b7ner", "Au\u00b7gen", "paar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PIAT", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.40": {"text": "Auf Elias Wagen", "tokens": ["Auf", "E\u00b7lias", "Wa\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "La\u00df die Seele tragen", "tokens": ["La\u00df", "die", "See\u00b7le", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.42": {"text": "Zu der Seelgen Schaar!", "tokens": ["Zu", "der", "Seel\u00b7gen", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}