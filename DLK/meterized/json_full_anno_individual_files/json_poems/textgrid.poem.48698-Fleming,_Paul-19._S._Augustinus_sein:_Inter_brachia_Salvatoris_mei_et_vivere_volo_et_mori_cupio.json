{"textgrid.poem.48698": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "19. S. Augustinus sein: Inter brachia Salvatoris mei et vivere volo et mori cupio", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Des Donners wilder Plitz schlug von sich manchen Sto\u00df,", "tokens": ["Des", "Don\u00b7ners", "wil\u00b7der", "Plitz", "schlug", "von", "sich", "man\u00b7chen", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "APPR", "PRF", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "das feige Volk stund bla\u00df. Das scheuche Wild erzittert'", "tokens": ["das", "fei\u00b7ge", "Volk", "stund", "bla\u00df", ".", "Das", "scheu\u00b7che", "Wild", "er\u00b7zit\u00b7tert'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$.", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.3": {"text": "vom Schmettern dieses Knalls, die Erde ward ersch\u00fcttert,", "tokens": ["vom", "Schmet\u00b7tern", "die\u00b7ses", "Knalls", ",", "die", "Er\u00b7de", "ward", "er\u00b7sch\u00fct\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PDAT", "NN", "$,", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "mein Fu\u00df sank unter sich, der Grund war bodenlos.", "tokens": ["mein", "Fu\u00df", "sank", "un\u00b7ter", "sich", ",", "der", "Grund", "war", "bo\u00b7den\u00b7los", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PRF", "$,", "ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Gruft, die fiel ihr nach, schlung mich in ihren Scho\u00df.", "tokens": ["Die", "Gruft", ",", "die", "fiel", "ihr", "nach", ",", "schlung", "mich", "in", "ih\u00b7ren", "Scho\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich gab mich in die See, in der es grausam wittert'", "tokens": ["Ich", "gab", "mich", "in", "die", "See", ",", "in", "der", "es", "grau\u00b7sam", "wit\u00b7tert'"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "der Sturm flog klippenhoch, mein Schiff das ward gesplittert,", "tokens": ["der", "Sturm", "flog", "klip\u00b7pen\u00b7hoch", ",", "mein", "Schiff", "das", "ward", "ge\u00b7split\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "PDS", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ward leck, ward Anker quit, ward Mast- und Segel blo\u00df.", "tokens": ["ward", "leck", ",", "ward", "An\u00b7ker", "quit", ",", "ward", "Mast", "und", "Se\u00b7gel", "blo\u00df", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VAFIN", "NN", "NE", "$,", "VAFIN", "TRUNC", "KON", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Vor, um und hinter mir war nichts als eine Not,", "tokens": ["Vor", ",", "um", "und", "hin\u00b7ter", "mir", "war", "nichts", "als", "ei\u00b7ne", "Not", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "KOUI", "KON", "APPR", "PPER", "VAFIN", "PIS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "von oben Untergang, von unten auf der Tod,", "tokens": ["von", "o\u00b7ben", "Un\u00b7ter\u00b7gang", ",", "von", "un\u00b7ten", "auf", "der", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "NN", "$,", "APPR", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "es war kein Muttermensch, der mit mir hatt' Erbarmen.", "tokens": ["es", "war", "kein", "Mut\u00b7ter\u00b7mensch", ",", "der", "mit", "mir", "hatt'", "Er\u00b7bar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PRELS", "APPR", "PPER", "VAFIN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich aber war mir gleich, zum Leben frisch und froh,", "tokens": ["Ich", "a\u00b7ber", "war", "mir", "gleich", ",", "zum", "Le\u00b7ben", "frisch", "und", "froh", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PPER", "ADV", "$,", "APPRART", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"t