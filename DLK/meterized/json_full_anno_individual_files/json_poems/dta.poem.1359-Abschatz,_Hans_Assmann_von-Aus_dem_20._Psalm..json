{"dta.poem.1359": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem 20. Psalm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Es h\u00f6re der HErr dein Klagen in Noth/", "tokens": ["Es", "h\u00f6\u00b7re", "der", "Herr", "dein", "Kla\u00b7gen", "in", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN", "APPR", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es sch\u00fctze dich Jacobs gewaltiger GOtt/", "tokens": ["Es", "sch\u00fct\u00b7ze", "dich", "Ja\u00b7cobs", "ge\u00b7wal\u00b7ti\u00b7ger", "Gott", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Er sende dir H\u00fclffe vom heiligen Thron", "tokens": ["Er", "sen\u00b7de", "dir", "H\u00fclf\u00b7fe", "vom", "hei\u00b7li\u00b7gen", "Thron"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "NN", "APPRART", "ADJA", "NN"], "meter": "----+--+--+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Labsal und St\u00e4rcke vom Berge Zion.", "tokens": ["Und", "Lab\u00b7sal", "und", "St\u00e4r\u00b7cke", "vom", "Ber\u00b7ge", "Zi\u00b7on", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "APPRART", "NN", "NE", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Er dencke gen\u00e4dig an dein Geschrey/", "tokens": ["Er", "den\u00b7cke", "ge\u00b7n\u00e4\u00b7dig", "an", "dein", "Ge\u00b7schrey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dein Opffer der Lippen ihm angenehm sey.", "tokens": ["Dein", "Opf\u00b7fer", "der", "Lip\u00b7pen", "ihm", "an\u00b7ge\u00b7nehm", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "PPER", "ADJD", "VAFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Er gebe dir/ was dein Hertze begehrt/", "tokens": ["Er", "ge\u00b7be", "dir", "/", "was", "dein", "Hert\u00b7ze", "be\u00b7gehrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWS", "PPOSAT", "VVFIN", "VVPP", "$("], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Und mache dein Christliches W\u00fcnschen gewehrt/", "tokens": ["Und", "ma\u00b7che", "dein", "Christ\u00b7li\u00b7ches", "W\u00fcn\u00b7schen", "ge\u00b7wehrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.9": {"text": "Damit du nach selig vollendeter Zeit", "tokens": ["Da\u00b7mit", "du", "nach", "se\u00b7lig", "voll\u00b7en\u00b7de\u00b7ter", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Ihn lobest und r\u00fchmest in Ewigkeit.", "tokens": ["Ihn", "lo\u00b7best", "und", "r\u00fch\u00b7mest", "in", "E\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}}}}