{"dta.poem.1638": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Beschwer \u00fcber den Bart.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist bey sch\u00f6nem Mund ein starck gewachsner Bart/", "tokens": ["Was", "ist", "bey", "sch\u00f6\u00b7nem", "Mund", "ein", "starck", "ge\u00b7wachs\u00b7ner", "Bart", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Liebe Wespen-Nest/ ein Dornstrauch um die Rosen/", "tokens": ["Der", "Lie\u00b7be", "Wes\u00b7pen\u00b7Nest", "/", "ein", "Dorn\u00b7strauch", "um", "die", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$(", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Stoppel s\u00fcsser Frucht/ ein scharffer Distel-Zaun/", "tokens": ["Ein", "Stop\u00b7pel", "s\u00fcs\u00b7ser", "Frucht", "/", "ein", "scharf\u00b7fer", "Dis\u00b7tel\u00b7Zaun", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Schrancken/ welchen wir den Hafen sperren schaun/", "tokens": ["Ein", "Schran\u00b7cken", "/", "wel\u00b7chen", "wir", "den", "Ha\u00b7fen", "sper\u00b7ren", "schaun", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAT", "PPER", "ART", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein spitzer Schifer-Fel\u00df in stiller Venus-Fahrt?", "tokens": ["Ein", "spit\u00b7zer", "Schi\u00b7fer\u00b7Fel\u00df", "in", "stil\u00b7ler", "Ve\u00b7nus\u00b7Fahrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer preist die K\u00e4ste/ so die Stachel-Schale deckt.", "tokens": ["Wer", "preist", "die", "K\u00e4s\u00b7te", "/", "so", "die", "Sta\u00b7chel\u00b7Scha\u00b7le", "deckt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "$(", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Perle/ welche noch in rauher Muschel steckt?", "tokens": ["Die", "Per\u00b7le", "/", "wel\u00b7che", "noch", "in", "rau\u00b7her", "Mu\u00b7schel", "steckt", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PRELS", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Mit was f\u00fcr Anmutt ist dem Barte liebzukosen?", "tokens": ["Mit", "was", "f\u00fcr", "An\u00b7mutt", "ist", "dem", "Bar\u00b7te", "lieb\u00b7zu\u00b7ko\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}