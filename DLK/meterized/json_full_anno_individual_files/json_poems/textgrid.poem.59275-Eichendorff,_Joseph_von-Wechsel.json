{"textgrid.poem.59275": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wechsel", "genre": "verse", "period": "N.A.", "pub_year": 1840, "urn": "N.A.", "language": ["de:0.85", "sv:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es f\u00e4llt nichts vor, mir f\u00e4llt nichts ein,", "tokens": ["Es", "f\u00e4llt", "nichts", "vor", ",", "mir", "f\u00e4llt", "nichts", "ein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PTKVZ", "$,", "PPER", "VVFIN", "PIS", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich glaub die Welt steht still,", "tokens": ["Ich", "glaub", "die", "Welt", "steht", "still", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Zeit tritt auf so leis und fein,", "tokens": ["Die", "Zeit", "tritt", "auf", "so", "leis", "und", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Man wei\u00df nicht, was sie will.", "tokens": ["Man", "wei\u00df", "nicht", ",", "was", "sie", "will", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKNEG", "$,", "PRELS", "PPER", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Auf einmal r\u00fchrt sich's dort und hier \u2013", "tokens": ["Auf", "ein\u00b7mal", "r\u00fchrt", "sich's", "dort", "und", "hier", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PIS", "ADV", "KON", 