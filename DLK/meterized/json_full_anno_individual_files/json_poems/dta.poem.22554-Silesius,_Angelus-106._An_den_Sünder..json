{"dta.poem.22554": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "106. An den S\u00fcnder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Ach S\u00fcnder trane nicht/ weil du die Magdalen", "tokens": ["Ach", "S\u00fcn\u00b7der", "tra\u00b7ne", "nicht", "/", "weil", "du", "die", "Mag\u00b7da\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "VVFIN", "PTKNEG", "$(", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Befridigt und getrost von unsrem HErrn sihst gehn:", "tokens": ["Be\u00b7fri\u00b7digt", "und", "ge\u00b7trost", "von", "uns\u00b7rem", "Herrn", "sihst", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du bist jhr noch nicht gleich ", "tokens": ["Du", "bist", "jhr", "noch", "nicht", "gleich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So lege dich zuvor wie sie zu seinen F\u00fcssen. ", "tokens": ["So", "le\u00b7ge", "dich", "zu\u00b7vor", "wie", "sie", "zu", "sei\u00b7nen", "F\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KOKOM", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}