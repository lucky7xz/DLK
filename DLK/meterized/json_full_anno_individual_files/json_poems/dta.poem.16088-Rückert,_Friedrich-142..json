{"dta.poem.16088": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "142.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn gelten zwischen zwein die Freundschaft soll und taugen,", "tokens": ["Wenn", "gel\u00b7ten", "zwi\u00b7schen", "zwein", "die", "Freund\u00b7schaft", "soll", "und", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "APPR", "ADV", "ART", "NN", "VMFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Bunde m\u00fc\u00dfen seyn die beiden wie zwei Augen.", "tokens": ["Im", "Bun\u00b7de", "m\u00fc\u00b7\u00dfen", "seyn", "die", "bei\u00b7den", "wie", "zwei", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPOSAT", "ART", "PIAT", "KOKOM", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wohin das eine zielt, dahin das andre spielt,", "tokens": ["Wo\u00b7hin", "das", "ei\u00b7ne", "zielt", ",", "da\u00b7hin", "das", "and\u00b7re", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "PIS", "VVFIN", "$,", "PAV", "ART", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und selber schielen wird dis mit, wenn jenes schielt.", "tokens": ["Und", "sel\u00b7ber", "schie\u00b7len", "wird", "dis", "mit", ",", "wenn", "je\u00b7nes", "schielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVINF", "VAFIN", "PDS", "PT