{"dta.poem.5718": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie kan man doch die ew'ge Weisheit schliessen,", "tokens": ["Wie", "kan", "man", "doch", "die", "ew'\u00b7ge", "Weis\u00b7heit", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In eine niedrige Gleichg\u00fcltigkeit,", "tokens": ["In", "ei\u00b7ne", "nied\u00b7ri\u00b7ge", "Gleich\u00b7g\u00fcl\u00b7tig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In eine schimpfliche Vergessenheit?", "tokens": ["In", "ei\u00b7ne", "schimpf\u00b7li\u00b7che", "Ver\u00b7ges\u00b7sen\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kan Jhr, von Arbeit matt, zu w\u00fcrcken je verdr\u00fc\u00dfen?", "tokens": ["Kan", "Ihr", ",", "von", "Ar\u00b7beit", "matt", ",", "zu", "w\u00fcr\u00b7cken", "je", "ver\u00b7dr\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "APPR", "NN", "PTKVZ", "$,", "PTKZU", "VVINF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "GoTT ", "tokens": ["GoTT"], "token_info": ["word"], "pos": ["NE"], "meter": "-", "measure": "single.down"}, "line.6": {"text": "Es ist JHM alles leicht, nichts hemmet Seine Macht,", "tokens": ["Es", "ist", "JhM", "al\u00b7les", "leicht", ",", "nichts", "hem\u00b7met", "Sei\u00b7ne", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PIS", "ADJD", "$,", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und nichts verheelt dem heitren Weisheits-Licht", "tokens": ["Und", "nichts", "ver\u00b7heelt", "dem", "hei\u00b7tren", "Weis\u00b7heits\u00b7Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Die Ordnung, die ES ", "tokens": ["Die", "Ord\u00b7nung", ",", "die", "Es"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.9": {"text": "Hat in der Unerm\u00e4\u00dflichkeit,", "tokens": ["Hat", "in", "der", "Un\u00b7er\u00b7m\u00e4\u00df\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die ewig fruchtbar ist, die nichts erf\u00fcllen", "tokens": ["Die", "e\u00b7wig", "frucht\u00b7bar", "ist", ",", "die", "nichts", "er\u00b7f\u00fcl\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJD", "VAFIN", "$,", "PRELS", "PIS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Und nichts umschr\u00e4ncken kan, es seinem Willen", "tokens": ["Und", "nichts", "um\u00b7schr\u00e4n\u00b7cken", "kan", ",", "es", "sei\u00b7nem", "Wil\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PIS", "VVINF", "VMFIN", "$,", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Gar keine M\u00fch\u2019 gemacht, die Welt hervor zu f\u00fchren:", "tokens": ["Gar", "kei\u00b7ne", "M\u00fch'", "ge\u00b7macht", ",", "die", "Welt", "her\u00b7vor", "zu", "f\u00fch\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VVPP", "$,", "ART", "NN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sollt\u2019 es JHM M\u00fche seyn, dieselbe zu regieren?", "tokens": ["Sollt'", "es", "JhM", "M\u00fc\u00b7he", "seyn", ",", "die\u00b7sel\u00b7be", "zu", "re\u00b7gie\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NE", "NN", "VAINF", "$,", "PDAT", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}