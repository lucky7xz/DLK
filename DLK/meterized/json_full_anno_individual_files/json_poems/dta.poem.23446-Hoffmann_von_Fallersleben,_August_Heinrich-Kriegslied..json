{"dta.poem.23446": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kriegslied.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00f6rt wie die Trommel schl\u00e4gt!", "tokens": ["H\u00f6rt", "wie", "die", "Trom\u00b7mel", "schl\u00e4gt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Seht wie das Volk sich regt!", "tokens": ["Seht", "wie", "das", "Volk", "sich", "regt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Fahne voran!", "tokens": ["Die", "Fah\u00b7ne", "vo\u00b7ran", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Wir folgen Mann f\u00fcr Mann.", "tokens": ["Wir", "fol\u00b7gen", "Mann", "f\u00fcr", "Mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Hinaus, hinaus", "tokens": ["Hin\u00b7aus", ",", "hin\u00b7aus"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "APZR"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Von Hof und Haus!", "tokens": ["Von", "Hof", "und", "Haus", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Ihr Weiber und Kinder, gute Nacht!", "tokens": ["Ihr", "Wei\u00b7ber", "und", "Kin\u00b7der", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Wir ziehen hinaus, hinaus in die Schlacht", "tokens": ["Wir", "zie\u00b7hen", "hin\u00b7aus", ",", "hin\u00b7aus", "in", "die", "Schlacht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.9": {"text": "Mit Gott f\u00fcr K\u00f6nig und Vaterland.", "tokens": ["Mit", "Gott", "f\u00fcr", "K\u00f6\u00b7nig", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}