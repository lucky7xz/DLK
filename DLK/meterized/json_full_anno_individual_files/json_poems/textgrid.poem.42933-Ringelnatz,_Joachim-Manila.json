{"textgrid.poem.42933": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Manila", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als ein altes Tau durch derbe,", "tokens": ["Als", "ein", "al\u00b7tes", "Tau", "durch", "der\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch verst\u00e4ndniswarme H\u00e4nde glitt,", "tokens": ["Doch", "ver\u00b7st\u00e4nd\u00b7nis\u00b7war\u00b7me", "H\u00e4n\u00b7de", "glitt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Sagte eine Stimme: \u00bbBob, ich sterbe,", "tokens": ["Sag\u00b7te", "ei\u00b7ne", "Stim\u00b7me", ":", "\u00bb", "Bob", ",", "ich", "ster\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "$(", "NE", "$,", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ehe Land in Sicht. Und du stirbst mit.\u00ab", "tokens": ["E\u00b7he", "Land", "in", "Sicht", ".", "Und", "du", "stirbst", "mit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "$.", "KON", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Noch bevor die Stimme Antwort kriegte,", "tokens": ["Noch", "be\u00b7vor", "die", "Stim\u00b7me", "Ant\u00b7wort", "krieg\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["AD