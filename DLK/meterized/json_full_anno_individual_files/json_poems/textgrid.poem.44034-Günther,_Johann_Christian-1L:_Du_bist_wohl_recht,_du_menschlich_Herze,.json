{"textgrid.poem.44034": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du bist wohl recht, du menschlich Herze,", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du bist wohl recht, du menschlich Herze,", "tokens": ["Du", "bist", "wohl", "recht", ",", "du", "menschlich", "Her\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$,", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.2": {"text": "Ein trozig und verzagtes Ding:", "tokens": ["Ein", "tro\u00b7zig", "und", "ver\u00b7zag\u00b7tes", "Ding", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Glauben wanckt in kleinem Schmerze,", "tokens": ["Dein", "Glau\u00b7ben", "wanckt", "in", "klei\u00b7nem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da es dir nach Wuntsche gieng,", "tokens": ["Und", "da", "es", "dir", "nach", "Wunt\u00b7sche", "gieng", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da schwall dein wildes Fleisch und Blut", "tokens": ["Da", "schwall", "dein", "wil\u00b7des", "Fleisch", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPOSAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von Geilheit und von \u00dcbermuth.", "tokens": ["Von", "Geil\u00b7heit", "und", "von", "\u00dc\u00b7ber\u00b7muth", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo schreibt der Thon dem klugen T\u00f6pfer,", "tokens": ["Wo", "schreibt", "der", "Thon", "dem", "klu\u00b7gen", "T\u00f6p\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein grobes Holz dem K\u00fcnstler vor?", "tokens": ["Ein", "gro\u00b7bes", "Holz", "dem", "K\u00fcnst\u00b7ler", "vor", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du murrest wider deinen Sch\u00f6pfer", "tokens": ["Du", "mur\u00b7rest", "wi\u00b7der", "dei\u00b7nen", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und r\u00fcttelst sein geduldig Ohr,", "tokens": ["Und", "r\u00fct\u00b7telst", "sein", "ge\u00b7dul\u00b7dig", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als w\u00e4re dies geringe Leid", "tokens": ["Als", "w\u00e4\u00b7re", "dies", "ge\u00b7rin\u00b7ge", "Leid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "PDS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Zeugn\u00fc\u00df scharfer Grausamkeit.", "tokens": ["Ein", "Zeug\u00b7n\u00fc\u00df", "schar\u00b7fer", "Grau\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du hast wohl Ursach, viel zu klagen;", "tokens": ["Du", "hast", "wohl", "Ur\u00b7sach", ",", "viel", "zu", "kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Besieh dich doch nur um und an!", "tokens": ["Be\u00b7sieh", "dich", "doch", "nur", "um", "und", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "APPR", "KON", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat dich als ein Kind getragen?", "tokens": ["Wer", "hat", "dich", "als", "ein", "Kind", "ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "KOUS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dies hat der Mutter Schoos gethan.", "tokens": ["Dies", "hat", "der", "Mut\u00b7ter", "Schoos", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer aber gab der Mutter Kraft?", "tokens": ["Wer", "a\u00b7ber", "gab", "der", "Mut\u00b7ter", "Kraft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Herr, der allen Nahrung schaft.", "tokens": ["Der", "Herr", ",", "der", "al\u00b7len", "Nah\u00b7rung", "schaft", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie vielmahl bistu nicht geglitten", "tokens": ["Wie", "viel\u00b7mahl", "bis\u00b7tu", "nicht", "ge\u00b7glit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hast den Fall gesund belacht?", "tokens": ["Und", "hast", "den", "Fall", "ge\u00b7sund", "be\u00b7lacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer hat dich an Vernunft und Sitten", "tokens": ["Wer", "hat", "dich", "an", "Ver\u00b7nunft", "und", "Sit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PRF", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der klugen Welt beliebt gemacht?", "tokens": ["Der", "klu\u00b7gen", "Welt", "be\u00b7liebt", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durch we\u00dfen Gnade tr\u00e4gstu noch", "tokens": ["Durch", "we\u00b7\u00dfen", "Gna\u00b7de", "tr\u00e4gs\u00b7tu", "noch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Des armen Lebens schweres Joch?", "tokens": ["Des", "ar\u00b7men", "Le\u00b7bens", "schwe\u00b7res", "Joch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ach unerkenntliches Gem\u00fcthe!", "tokens": ["Ach", "un\u00b7er\u00b7kennt\u00b7li\u00b7ches", "Ge\u00b7m\u00fc\u00b7the", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer hat an solcher Unruh Schuld?", "tokens": ["Wer", "hat", "an", "sol\u00b7cher", "Un\u00b7ruh", "Schuld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "APPR", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du selbst und nicht des H\u00f6chsten G\u00fcte.", "tokens": ["Du", "selbst", "und", "nicht", "des", "H\u00f6chs\u00b7ten", "G\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KON", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was darbstu? Gar nichts als Gedult.", "tokens": ["Was", "darbs\u00b7tu", "?", "Gar", "nichts", "als", "Ge\u00b7dult", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "$.", "ADV", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach k\u00f6ntestu vergn\u00fcglich seyn,", "tokens": ["Ach", "k\u00f6n\u00b7tes\u00b7tu", "ver\u00b7gn\u00fcg\u00b7lich", "seyn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So s\u00e4hstu deinen Reichthum ein.", "tokens": ["So", "s\u00e4h\u00b7stu", "dei\u00b7nen", "Reicht\u00b7hum", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du siehst den Tisch der reichen Pra\u00dfer,", "tokens": ["Du", "siehst", "den", "Tisch", "der", "rei\u00b7chen", "Pra\u00b7\u00dfer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und daran \u00e4rgert sich dein Mund?", "tokens": ["Und", "da\u00b7ran", "\u00e4r\u00b7gert", "sich", "dein", "Mund", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du aber bist bey Korn und Wa\u00dfer", "tokens": ["Du", "a\u00b7ber", "bist", "bey", "Korn", "und", "Wa\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VAFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "An Gliedern und Verstand gesund.", "tokens": ["An", "Glie\u00b7dern", "und", "Ver\u00b7stand", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer dies besizt und mehr begehrt,", "tokens": ["Wer", "dies", "be\u00b7sizt", "und", "mehr", "be\u00b7gehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDS", "VVFIN", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist de\u00dfen, was er hat, nicht werth.", "tokens": ["Ist", "de\u00b7\u00dfen", ",", "was", "er", "hat", ",", "nicht", "werth", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "$,", "PWS", "PPER", "VAFIN", "$,", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vielleicht verdient es mancher be\u00dfer", "tokens": ["Viel\u00b7leicht", "ver\u00b7dient", "es", "man\u00b7cher", "be\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und leidet noch wohl \u00e4rger Noth;", "tokens": ["Und", "lei\u00b7det", "noch", "wohl", "\u00e4r\u00b7ger", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie manchen reizen Strick und Me\u00dfer", "tokens": ["Wie", "man\u00b7chen", "rei\u00b7zen", "Strick", "und", "Me\u00b7\u00dfer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bey Kleien, Rind und Eichelbrodt;", "tokens": ["Bey", "Klei\u00b7en", ",", "Rind", "und", "Ei\u00b7chel\u00b7brodt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Beklagt nicht jezt so manches Land", "tokens": ["Be\u00b7klagt", "nicht", "jezt", "so", "man\u00b7ches", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den allgemeinen Jammerstand?", "tokens": ["Den", "all\u00b7ge\u00b7mei\u00b7nen", "Jam\u00b7mer\u00b7stand", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Du hast Verfolger. Nun, was weiter?", "tokens": ["Du", "hast", "Ver\u00b7fol\u00b7ger", ".", "Nun", ",", "was", "wei\u00b7ter", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "$.", "ADV", "$,", "PWS", "PTKVZ", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nirgends einen wahren Freund.", "tokens": ["Und", "nir\u00b7gends", "ei\u00b7nen", "wah\u00b7ren", "Freund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was macht's? Der Himmel scheint nicht heiter.", "tokens": ["Was", "macht's", "?", "Der", "Him\u00b7mel", "scheint", "nicht", "hei\u00b7ter", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$.", "ART", "NN", "VVFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gut, sey nur nicht dein gr\u00f6ster Feind", "tokens": ["Gut", ",", "sey", "nur", "nicht", "dein", "gr\u00f6s\u00b7ter", "Feind"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "VAFIN", "ADV", "PTKNEG", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und werde, wenn dich alles hast,", "tokens": ["Und", "wer\u00b7de", ",", "wenn", "dich", "al\u00b7les", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "PIS", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dir auch nicht endlich selbst zur Last.", "tokens": ["Dir", "auch", "nicht", "end\u00b7lich", "selbst", "zur", "Last", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Du siehst in Arbeit und Studiren", "tokens": ["Du", "siehst", "in", "Ar\u00b7beit", "und", "Stu\u00b7di\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+++-", "measure": "unknown.measure.penta"}, "line.2": {"text": "So wenig Gl\u00fcck als Hofnung bl\u00fchn.", "tokens": ["So", "we\u00b7nig", "Gl\u00fcck", "als", "Hof\u00b7nung", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KOUS", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Halt an und schweig und las dich f\u00fchren,", "tokens": ["Halt", "an", "und", "schweig", "und", "las", "dich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KON", "ADJD", "KON", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Vorsicht weis emporzuziehn.", "tokens": ["Die", "Vor\u00b7sicht", "weis", "em\u00b7por\u00b7zu\u00b7ziehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Betriegt auch dies, so ist ja schon", "tokens": ["Be\u00b7triegt", "auch", "dies", ",", "so", "ist", "ja", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PDS", "$,", "ADV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Wei\u00dfheit an sich selbst ihr Lohn.", "tokens": ["Die", "Wei\u00df\u00b7heit", "an", "sich", "selbst", "ihr", "Lohn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PRF", "ADV", "PPOSAT", "NN", "$."], "me