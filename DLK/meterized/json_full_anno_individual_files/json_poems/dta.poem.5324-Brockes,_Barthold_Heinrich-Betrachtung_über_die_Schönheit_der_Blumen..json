{"dta.poem.5324": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber die Sch\u00f6nheit der  \n Blumen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich hier vergn\u00fcgt im Garten gehe,", "tokens": ["In\u00b7dem", "ich", "hier", "ver\u00b7gn\u00fcgt", "im", "Gar\u00b7ten", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und bey so mancher Art gef\u00e4rbter Blumen stehe,", "tokens": ["Und", "bey", "so", "man\u00b7cher", "Art", "ge\u00b7f\u00e4rb\u00b7ter", "Blu\u00b7men", "ste\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADV", "PIAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "F\u00e4llt mir, da mein Gem\u00fcth von ihrem Glantz und Schein", "tokens": ["F\u00e4llt", "mir", ",", "da", "mein", "Ge\u00b7m\u00fcth", "von", "ih\u00b7rem", "Glantz", "und", "Schein"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gantz eingenommen wird, bey ihrem Schimmer ein:", "tokens": ["Gantz", "ein\u00b7ge\u00b7nom\u00b7men", "wird", ",", "bey", "ih\u00b7rem", "Schim\u00b7mer", "ein", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man sieht, durch der Natur Geheimni\u00df-reiche Kraft,", "tokens": ["Man", "sieht", ",", "durch", "der", "Na\u00b7tur", "Ge\u00b7heim\u00b7ni\u00df\u00b7rei\u00b7che", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gewachs\u2019nen Atlas hier, und dort gewachs\u2019nen Taft,", "tokens": ["Ge\u00b7wachs'\u00b7nen", "At\u00b7las", "hier", ",", "und", "dort", "ge\u00b7wachs'\u00b7nen", "Taft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ADV", "$,", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gef\u00e4rbten Damast dort, Sammt, Moor, Brocad, Satin,", "tokens": ["Ge\u00b7f\u00e4rb\u00b7ten", "Da\u00b7mast", "dort", ",", "Sammt", ",", "Moor", ",", "Bro\u00b7cad", ",", "Sa\u00b7tin", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "ADV", "$,", "VVFIN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Nebst silbernen und g\u00fcldnen St\u00fccken", "tokens": ["Nebst", "sil\u00b7ber\u00b7nen", "und", "g\u00fcld\u00b7nen", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Voll Rancken-Werck, bald roth, bald gr\u00fcn,", "tokens": ["Voll", "Ran\u00b7cken\u00b7\u00b7Werck", ",", "bald", "roth", ",", "bald", "gr\u00fcn", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Bald blau, bald incarnat, des Gartens Fluren schm\u00fccken.", "tokens": ["Bald", "blau", ",", "bald", "in\u00b7car\u00b7nat", ",", "des", "Gar\u00b7tens", "Flu\u00b7ren", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Bewundre doch, geliebter Mensch, wie glatt,", "tokens": ["Be\u00b7wund\u00b7re", "doch", ",", "ge\u00b7lieb\u00b7ter", "Mensch", ",", "wie", "glatt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "ADJA", "NN", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Wie bunt, wie gl\u00e4ntzend jedes Blatt!", "tokens": ["Wie", "bunt", ",", "wie", "gl\u00e4nt\u00b7zend", "je\u00b7des", "Blatt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Erwege doch der sch\u00f6nen Creaturen", "tokens": ["Er\u00b7we\u00b7ge", "doch", "der", "sch\u00f6\u00b7nen", "Crea\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Bewunderns-wehrte Pracht und zierliche Figuren!", "tokens": ["Be\u00b7wun\u00b7derns\u00b7wehr\u00b7te", "Pracht", "und", "zier\u00b7li\u00b7che", "Fi\u00b7gu\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hat jemand auf der Welt gelebet,", "tokens": ["Hat", "je\u00b7mand", "auf", "der", "Welt", "ge\u00b7le\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der solch ein k\u00fcnstliches Gewebe je gewebet,", "tokens": ["Der", "solch", "ein", "k\u00fcnst\u00b7li\u00b7ches", "Ge\u00b7we\u00b7be", "je", "ge\u00b7we\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "ART", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In welchem, ob sie noch so sch\u00f6n", "tokens": ["In", "wel\u00b7chem", ",", "ob", "sie", "noch", "so", "sch\u00f6n"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PWAT", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Kein Faden, kein Gewirck zu sehn?", "tokens": ["Kein", "Fa\u00b7den", ",", "kein", "Ge\u00b7wirck", "zu", "sehn", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Erwege doch in ihrer Pracht", "tokens": ["Er\u00b7we\u00b7ge", "doch", "in", "ih\u00b7rer", "Pracht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Liebe, Weisheit und die Macht", "tokens": ["Die", "Lie\u00b7be", ",", "Weis\u00b7heit", "und", "die", "Macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Des Wesens, welches sie aus Nichts hervorgebracht,", "tokens": ["Des", "We\u00b7sens", ",", "wel\u00b7ches", "sie", "aus", "Nichts", "her\u00b7vor\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "APPR", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Nicht nur so wundersch\u00f6n formirt,", "tokens": ["Nicht", "nur", "so", "wun\u00b7der\u00b7sch\u00f6n", "for\u00b7mirt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Sie so an Farb\u2019 als an Figur geziert,", "tokens": ["Sie", "so", "an", "Fa\u00b7rb'", "als", "an", "Fi\u00b7gur", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "KOKOM", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.24": {"text": "Roch mehr, so mancherley Figur in sie gesencket", "tokens": ["Roch", "mehr", ",", "so", "man\u00b7cher\u00b7ley", "Fi\u00b7gur", "in", "sie", "ge\u00b7sen\u00b7cket"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "ADV", "PIAT", "NN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und uns, zu dem Genu\u00df, des Riechens Kraft geschencket!", "tokens": ["Und", "uns", ",", "zu", "dem", "Ge\u00b7nu\u00df", ",", "des", "Rie\u00b7chens", "Kraft", "ge\u00b7schen\u00b7cket", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "APPR", "ART", "NN", "$,", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Auf denn mein Geist! du must von GOttes Wercken", "tokens": ["Auf", "denn", "mein", "Geist", "!", "du", "must", "von", "Got\u00b7tes", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "KON", "PPOSAT", "NN", "$.", "PPER", "VMFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die Pracht auf andre Weis\u2019, als wie das Vieh, bemercken!", "tokens": ["Die", "Pracht", "auf", "and\u00b7re", "Weis'", ",", "als", "wie", "das", "Vieh", ",", "be\u00b7mer\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wir k\u00f6nnen uns zu GOtt durch nichts so sehr erheben,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "uns", "zu", "Gott", "durch", "nichts", "so", "sehr", "er\u00b7he\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "NN", "APPR", "PIS", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als wenn, in seinem Werck, wir uns mit Lust bestreben,", "tokens": ["Als", "wenn", ",", "in", "sei\u00b7nem", "Werck", ",", "wir", "uns", "mit", "Lust", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "$,", "APPR", "PPOSAT", "NN", "$,", "PPER", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auf seine Weisheit, Lieb\u2019 und seine Macht zu achten,", "tokens": ["Auf", "sei\u00b7ne", "Weis\u00b7heit", ",", "Lieb'", "und", "sei\u00b7ne", "Macht", "zu", "ach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "NN", "KON", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und in der Creatur, die blos dazu erlesen,", "tokens": ["Und", "in", "der", "Crea\u00b7tur", ",", "die", "blos", "da\u00b7zu", "er\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "PAV", "VVINF", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Da\u00df sie uns zeigen soll sein sonst verborgnes Wesen,", "tokens": ["Da\u00df", "sie", "uns", "zei\u00b7gen", "soll", "sein", "sonst", "ver\u00b7borg\u00b7nes", "We\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Ehrfurcht, Lieb\u2019 und Lust den Sch\u00f6pfer zu betrachten.", "tokens": ["Mit", "Ehr\u00b7furcht", ",", "Lieb'", "und", "Lust", "den", "Sch\u00f6p\u00b7fer", "zu", "be\u00b7trach\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Mir fiel hier\u00fcber ein, was ich hievon geschrieben,", "tokens": ["Mir", "fiel", "hier\u00b7\u00fc\u00b7ber", "ein", ",", "was", "ich", "hie\u00b7von", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PAV", "PTKVZ", "$,", "PWS", "PPER", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und welches mir noch im Ged\u00e4chtni\u00df blieben:", "tokens": ["Und", "wel\u00b7ches", "mir", "noch", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "blie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+++-+-+-", "measure": "unknown.measure.hexa"}, "line.9": {"text": "Kann sie sich selbst so zierlich bilden?", "tokens": ["Kann", "sie", "sich", "selbst", "so", "zier\u00b7lich", "bil\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Kann sie der holden Bl\u00e4tter Pracht", "tokens": ["Kann", "sie", "der", "hol\u00b7den", "Bl\u00e4t\u00b7ter", "Pracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Aus eigner Macht", "tokens": ["Aus", "eig\u00b7ner", "Macht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Hier sch\u00f6n versilbern, da verg\u00fclden?", "tokens": ["Hier", "sch\u00f6n", "ver\u00b7sil\u00b7bern", ",", "da", "ver\u00b7g\u00fcl\u00b7den", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Kann sie von ungefehr wie Demant und Carbunckeln", "tokens": ["Kann", "sie", "von", "un\u00b7ge\u00b7fehr", "wie", "De\u00b7mant", "und", "Car\u00b7bun\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "ADJD", "KOKOM", "NN", "KON", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.14": {"text": "In buntem Feuer gl\u00fchn? so lieb-als herrlich funckeln?", "tokens": ["In", "bun\u00b7tem", "Feu\u00b7er", "gl\u00fchn", "?", "so", "lie\u00b7bals", "herr\u00b7lich", "fun\u00b7ckeln", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$.", "ADV", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ich sencke mich durch deine Wunder in dich, allm\u00e4ch-", "tokens": ["Ich", "sen\u00b7cke", "mich", "durch", "dei\u00b7ne", "Wun\u00b7der", "in", "dich", ",", "all\u00b7m\u00e4ch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPR", "PPER", "$,", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und sp\u00fcr\u2019 in ihnen von der GOttheit den sicht- und un-", "tokens": ["Und", "sp\u00fcr'", "in", "ih\u00b7nen", "von", "der", "Got\u00b7theit", "den", "sicht", "und", "un"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPER", "APPR", "ART", "NN", "ART", "TRUNC", "KON", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Durch sie, als einen sch\u00f6nen Nebel, seh\u2019 ich das Licht der", "tokens": ["Durch", "sie", ",", "als", "ei\u00b7nen", "sch\u00f6\u00b7nen", "Ne\u00b7bel", ",", "seh'", "ich", "das", "Licht", "der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "KOUS", "ART", "ADJA"