 "tokens": ["die", "vor\u00b7mals", "dort", "kein", "Mensch", "ge\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir k\u00f6nnten Dir so manches zeigen", "tokens": ["Wir", "k\u00f6nn\u00b7ten", "Dir", "so", "man\u00b7ches", "zei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PIS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "von Polenarbeit, Menschenpflicht . . .", "tokens": ["von", "Po\u00b7len\u00b7ar\u00b7beit", ",", "Men\u00b7schen\u00b7pflicht", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "punct", "punct", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es ist jetzt Krieg \u2013 und wir, wir schweigen.", "tokens": ["Es", "ist", "jetzt", "Krieg", "\u2013", "und", "wir", ",", "wir", "schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$(", "KON", "PPER", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du nicht.", "tokens": ["Du", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Wir sind durchaus nicht so begeistert,", "tokens": ["Wir", "sind", "durc\u00b7haus", "nicht", "so", "be\u00b7geis\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "von allem, was die Panke beut:", "tokens": ["von", "al\u00b7lem", ",", "was", "die", "Pan\u00b7ke", "beut", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "der Schieber, der die Wechsel meistert,", "tokens": ["der", "Schie\u00b7ber", ",", "der", "die", "Wech\u00b7sel", "meis\u00b7tert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "die Dame, die den Schieber freut;", "tokens": ["die", "Da\u00b7me", ",", "die", "den", "Schie\u00b7ber", "freut", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "das Kino-Caf\u00e9 gegen\u00fcber,", "tokens": ["das", "Ki\u00b7no\u00b7Ca\u00b7f\u00e9", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "der H\u00e4ndler, den der Hafer sticht . . .", "tokens": ["der", "H\u00e4nd\u00b7ler", ",", "den", "der", "Ha\u00b7fer", "sticht", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es gibt ja manche, die stehn dr\u00fcber.", "tokens": ["Es", "gibt", "ja", "man\u00b7che", ",", "die", "stehn", "dr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du nicht.", "token