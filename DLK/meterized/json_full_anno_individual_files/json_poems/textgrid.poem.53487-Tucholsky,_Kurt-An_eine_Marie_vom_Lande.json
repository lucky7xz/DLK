punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es gibt ja manche, die stehn dr\u00fcber.", "tokens": ["Es", "gibt", "ja", "man\u00b7che", ",", "die", "stehn", "dr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "$,", "PRELS", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du nicht.", "tokens": ["Du", "nicht", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKNEG", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.8": {"line.1": {"text": "H\u00f6r auf, uns sauer anzumucken \u2013", "tokens": ["H\u00f6r", "auf", ",", "uns", "sau\u00b7er", "an\u00b7zu\u00b7mu\u00b7cken", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "bei uns hast Du damit kein Gl\u00fcck.", "tokens": ["bei", "uns", "hast", "Du", "da\u00b7mit", "kein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "PAV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man kann zwar leicht nach unten spucken,", "tokens": ["Man", "kann", "zwar", "leicht", "nach", "un\u00b7ten", "spu\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADV", "ADJD", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "nach oben nicht \u2013 das f\u00e4llt zur\u00fcck.", "tokens": ["nach", "o\u00b7ben", "nicht", "\u2013", "das", "f\u00e4llt", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "pu