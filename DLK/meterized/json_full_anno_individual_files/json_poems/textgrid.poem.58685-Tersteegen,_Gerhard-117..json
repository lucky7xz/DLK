{"textgrid.poem.58685": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "117.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du h\u00f6chste Majest\u00e4t, ich soll mit dir umgehen,", "tokens": ["Du", "h\u00f6chs\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", ",", "ich", "soll", "mit", "dir", "um\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Und doch bin ich nicht bang, weil du mich angesehen", "tokens": ["Und", "doch", "bin", "ich", "nicht", "bang", ",", "weil", "du", "mich", "an\u00b7ge\u00b7se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,", "KOUS", "PPER", "PRF", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In menschlicher Gestalt, die mir leutselig winkt.", "tokens": ["In", "menschli\u00b7cher", "Ge\u00b7stalt", ",", "die", "mir", "leut\u00b7se\u00b7lig", "winkt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Du wirst ein Menschenkind, willst kindlich zu mir kommen,", "tokens": ["Du", "wirst", "ein", "Men\u00b7schen\u00b7kind", ",", "willst", "kind\u00b7lich", "zu", "mir", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "VMFIN", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df ich im Herzen frei dich sehn und h\u00f6ren kann;", "tokens": ["Da\u00df", "ich", "im", "Her\u00b7zen", "frei", "dich", "sehn", "und", "h\u00f6\u00b7ren", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ADJD", "PPER", "VVINF", "KON", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So rede denn, mein Herr, du hast die Furcht benommen,", "tokens": ["So", "re\u00b7de", "denn", ",", "mein", "Herr", ",", "du", "hast", "die", "Furcht", "be\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Ja rede, bis ich werd' dein, ", "tokens": ["Ja", "re\u00b7de", ",", "bis", "ich", "werd'", "dein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "PPOSAT", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Du h\u00f6chste Majest\u00e4t, ich soll mit dir umgehen,", "tokens": ["Du", "h\u00f6chs\u00b7te", "Ma\u00b7jes\u00b7t\u00e4t", ",", "ich", "soll", "mit", "dir", "um\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ", "tokens"