{"textgrid.poem.39379": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Oft, wenn sich ihre reine Stimm' erschwungen,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Oft, wenn sich ihre reine Stimm' erschwungen,", "tokens": ["Oft", ",", "wenn", "sich", "ih\u00b7re", "rei\u00b7ne", "Stimm'", "er\u00b7schwun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PRF", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sch\u00fcchtern und k\u00fchn, und Saiten drein gerauschet,", "tokens": ["Sch\u00fcch\u00b7tern", "und", "k\u00fchn", ",", "und", "Sai\u00b7ten", "drein", "ge\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "Hab' ich das unbewu\u00dfte Herz belauschet,", "tokens": ["Hab'", "ich", "das", "un\u00b7be\u00b7wu\u00df\u00b7te", "Herz", "be\u00b7lau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das aus der Brust melodisch vorgedrungen.", "tokens": ["Das", "aus", "der", "Brust", "me\u00b7lo\u00b7disch", "vor\u00b7ge\u00b7drun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vom Becher, den die Wellen eingedrungen,", "tokens": ["Vom", "Be\u00b7cher", ",", "den", "die", "Wel\u00b7len", "ein\u00b7ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als aus dem Pfand, das Lieb' und Treu getauschet,", "tokens": ["Als", "aus", "dem", "Pfand", ",", "das", "Lieb'", "und", "Treu", "ge\u00b7tau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "ART", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der alte K\u00f6nig sterbend sich berauschet,", "tokens": ["Der", "al\u00b7te", "K\u00f6\u00b7nig", "ster\u00b7bend", "sich", "be\u00b7rau\u00b7schet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das war das letzte Lied, so sie gesungen.", "tokens": ["Das", "war", "das", "letz\u00b7te", "Lied", ",", "so", "sie", "ge\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wohl ziemt sich's, da\u00df der lebensm\u00fcde Zecher,", "tokens": ["Wohl", "ziemt", "sich's", ",", "da\u00df", "der", "le\u00b7bens\u00b7m\u00fc\u00b7de", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn dunkle Fluten still sein Ufer k\u00fc\u00dfen,", "tokens": ["Wenn", "dunk\u00b7le", "Flu\u00b7ten", "st