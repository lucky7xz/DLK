{"textgrid.poem.34366": {"metadata": {"author": {"name": "Lenz, Jakob Michael Reinhold", "birth": "N.A.", "death": "N.A."}, "title": "92.", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwas machst du hier, lieb M\u00e4gdelein,", "tokens": ["\u00bb", "was", "machst", "du", "hier", ",", "lieb", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PWS", "VVFIN", "PPER", "ADV", "$,", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Wasser tief und schnelle?", "tokens": ["Am", "Was\u00b7ser", "tief", "und", "schnel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sitzest da am Bach allein", "tokens": ["Und", "sit\u00b7zest", "da", "am", "Bach", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit nassen rothen B\u00e4ckelein", "tokens": ["Mit", "nas\u00b7sen", "ro\u00b7then", "B\u00e4\u00b7ckel\u00b7ein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und gukst auf eine Stelle?", "tokens": ["Und", "gukst", "auf", "ei\u00b7ne", "Stel\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Hat dich die Mutter was bedroht?", "tokens": ["Hat", "dich", "die", "Mut\u00b7ter", "was", "be\u00b7droht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "PWS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bekamst du heut kein Morgenbrod?", "tokens": ["Be\u00b7kamst", "du", "heut", "kein", "Mor\u00b7gen\u00b7brod", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Hat Bruder dich geschlagen?", "tokens": ["Hat", "Bru\u00b7der", "dich", "ge\u00b7schla\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Du kanst mir alles sagen.\u00ab", "tokens": ["Du", "kanst", "mir", "al\u00b7les", "sa\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Das M\u00e4gdlein schaut ihm ins Gesicht", "tokens": ["Das", "M\u00e4gd\u00b7lein", "schaut", "ihm", "ins", "Ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sieht, kehrt sich weg und redet nicht.", "tokens": ["Sieht", ",", "kehrt", "sich", "weg", "und", "re\u00b7det", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "PRF", "PTKVZ", "KON", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbsag, wo bist du zu Hause?\u00ab", "tokens": ["\u00bb", "sag", ",", "wo", "bist", "du", "zu", "Hau\u00b7se", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "PWAV", "VAFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "---+-+-", "measure": "unknown.measure.di"}, "line.4": {"text": "\u00bbherr! dort in jener Klause.\u00ab \u2013", "tokens": ["\u00bb", "herr", "!", "dort", "in", "je\u00b7ner", "Klau\u00b7se", ".", "\u00ab", "\u2013"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["$(", "PTKVZ", "$.", "ADV", "APPR", "PDAT", "NN", "$.", "$(", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Er kriecht zur kleinen Th\u00fcr herein", "tokens": ["Er", "kriecht", "zur", "klei\u00b7nen", "Th\u00fcr", "her\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und find't ein hagres M\u00fctterlein", "tokens": ["Und", "find't", "ein", "hag\u00b7res", "M\u00fct\u00b7ter\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf schlechten Binsen liegen.", "tokens": ["Auf", "schlech\u00b7ten", "Bin\u00b7sen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "\u00bbsagt, liebe Frau, was fehlt dem Kind?", "tokens": ["\u00bb", "sagt", ",", "lie\u00b7be", "Frau", ",", "was", "fehlt", "dem", "Kind", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "ADJA", "NN", "$,", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Es sitzt da draussen in dem Wind", "tokens": ["Es", "sitzt", "da", "draus\u00b7sen", "in", "dem", "Wind"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und ist nicht still zu kriegen.\u00ab", "tokens": ["Und", "ist", "nicht", "still", "zu", "krie\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PTKNEG", "ADJD", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u00bbach, lieber Herr,\u00ab das M\u00fctterlein", "tokens": ["\u00bb", "ach", ",", "lie\u00b7ber", "Herr", ",", "\u00ab", "das", "M\u00fct\u00b7ter\u00b7lein"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["$(", "ITJ", "$,", "ADV", "NN", "$,", "$(", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit schwerem Husten saget,", "tokens": ["Mit", "schwe\u00b7rem", "Hus\u00b7ten", "sa\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbes geht den ganzen Tag allein", "tokens": ["\u00bb", "es", "geht", "den", "gan\u00b7zen", "Tag", "al\u00b7lein"], "token_info": ["punct", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leid't nicht, da\u00df mans fraget.", "tokens": ["Und", "leid't", "nicht", ",", "da\u00df", "mans", "fra\u00b7get", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Es hat von seiner Kindheit an", "tokens": ["Es", "hat", "von", "sei\u00b7ner", "Kind\u00b7heit", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nichts als best\u00e4ndig weinen 'than.\u00ab", "tokens": ["Nichts", "als", "be\u00b7st\u00e4n\u00b7dig", "wei\u00b7nen", "'than", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "KOKOM", "ADJD", "VVINF", "NE", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "\u00bbso wahr ein Gott im Himmel ist:", "tokens": ["\u00bb", "so", "wahr", "ein", "Gott", "im", "Him\u00b7mel", "ist", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "ART", "NN", "APPRART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Euch mu\u00df was heimlich qu\u00e4len,", "tokens": ["Euch", "mu\u00df", "was", "heim\u00b7lich", "qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PWS", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ihr sagt nicht alles, was ihr wi\u00dft;", "tokens": ["Ihr", "sagt", "nicht", "al\u00b7les", ",", "was", "ihr", "wi\u00dft", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PIS", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr sollt mir nichts verheelen.\u00ab", "tokens": ["Ihr", "sollt", "mir", "nichts", "ver\u00b7hee\u00b7len", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbnun lieber Herr\u00ab \u2013 und fa\u00dft den Mann", "tokens": ["\u00bb", "nun", "lie\u00b7ber", "Herr", "\u00ab", "\u2013", "und", "fa\u00dft", "den", "Mann"], "token_info": ["punct", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "NN", "$(", "$(", "KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit beiden welken H\u00e4nden an:", "tokens": ["Mit", "bei\u00b7den", "wel\u00b7ken", "H\u00e4n\u00b7den", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbgeht an den Strom, fallt auf die Knie", "tokens": ["\u00bb", "geht", "an", "den", "Strom", ",", "fallt", "auf", "die", "Knie"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann kommt wieder morgen fr\u00fch;", "tokens": ["Und", "dann", "kommt", "wie\u00b7der", "mor\u00b7gen", "fr\u00fch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird sich mein Husten kehren,", "tokens": ["Wird", "sich", "mein", "Hus\u00b7ten", "keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So sollt ihr alles h\u00f6ren.\u00ab", "tokens": ["So", "sollt", "ihr", "al\u00b7les", "h\u00f6\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Blick, der Ton, der H\u00e4ndedruck", "tokens": ["Der", "Blick", ",", "der", "Ton", ",", "der", "H\u00e4n\u00b7de\u00b7druck"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Fremden an die Seele schlug,", "tokens": ["Dem", "Frem\u00b7den", "an", "die", "See\u00b7le", "schlug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er geht zum Bach, f\u00e4llt auf die Knie;", "tokens": ["Er", "geht", "zum", "Bach", ",", "f\u00e4llt", "auf", "die", "Knie", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kommt zu dem Weiblein morgens fr\u00fch,", "tokens": ["Kommt", "zu", "dem", "Weib\u00b7lein", "mor\u00b7gens", "fr\u00fch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Find't sie in bittren Z\u00e4hren.", "tokens": ["Find't", "sie", "in", "bit\u00b7tren", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "\u00bbach, Herr! was uns verlohren ging", "tokens": ["\u00bb", "ach", ",", "Herr", "!", "was", "uns", "ver\u00b7loh\u00b7ren", "ging"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ITJ", "$,", "NN", "$.", "PWS", "PPER", "VVPP", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Kann dieses Blatt und dieser Ring", "tokens": ["Kann", "die\u00b7ses", "Blatt", "und", "die\u00b7ser", "Ring"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PDAT", "NN", "KON", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Euch ba\u00df, denn ich erkl\u00e4ren.\u00ab", "tokens": ["Euch", "ba\u00df", ",", "denn", "ich", "er\u00b7kl\u00e4\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "ADV", "$,", "KON", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Mit diesem Wort zieht sie ein Tuch", "tokens": ["Mit", "die\u00b7sem", "Wort", "zieht", "sie", "ein", "Tuch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus ihrer Brust, darinn ein Buch", "tokens": ["Aus", "ih\u00b7rer", "Brust", ",", "da\u00b7rinn", "ein", "Buch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PAV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und in dem Buch ein Bl\u00e4ttlein war,", "tokens": ["Und", "in", "dem", "Buch", "ein", "Bl\u00e4tt\u00b7lein", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bemalt mit plumpen Farben zwar,", "tokens": ["Be\u00b7malt", "mit", "plum\u00b7pen", "Far\u00b7ben", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und an dem Farben-Bl\u00e4ttlein hing", "tokens": ["Und", "an", "dem", "Fa\u00b7rben\u00b7Bl\u00e4tt\u00b7lein", "hing"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als Siegel ihr Verl\u00f6bnis-Ring.", "tokens": ["Als", "Sie\u00b7gel", "ihr", "Ver\u00b7l\u00f6b\u00b7nis\u00b7Ring", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Auf diesem Bl\u00e4ttlein schwamm ein Weib", "tokens": ["Auf", "die\u00b7sem", "Bl\u00e4tt\u00b7lein", "schwamm", "ein", "Weib"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im h\u00f6chsten Strom mit halbem Leib.", "tokens": ["Im", "h\u00f6chs\u00b7ten", "Strom", "mit", "hal\u00b7bem", "Leib", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Kahn war umgeschlagen,", "tokens": ["Ihr", "Kahn", "war", "um\u00b7ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und an des Weibes Zipfel fa\u00dft", "tokens": ["Und", "an", "des", "Wei\u00b7bes", "Zip\u00b7fel", "fa\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Ehmann sich, doch diese Last", "tokens": ["Ihr", "Eh\u00b7mann", "sich", ",", "doch", "die\u00b7se", "Last"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "PRF", "$,", "ADV", "PDAT", "NN"], "meter": "-+-+-+-+", "measure