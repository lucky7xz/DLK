{"textgrid.poem.43954": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Kind, ach liebstes Kind, was war das vor Vergn\u00fcgen!", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Kind, ach liebstes Kind, was war das vor Vergn\u00fcgen!", "tokens": ["Ach", "Kind", ",", "ach", "liebs\u00b7tes", "Kind", ",", "was", "war", "das", "vor", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "ADJA", "NN", "$,", "PWS", "VAFIN", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmel geb uns doch dergleichen N\u00e4chte viel", "tokens": ["Der", "Him\u00b7mel", "geb", "uns", "doch", "derg\u00b7lei\u00b7chen", "N\u00e4ch\u00b7te", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PIS", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und las uns so vertraut bis an das lezte Ziel", "tokens": ["Und", "las", "uns", "so", "ver\u00b7traut", "bis", "an", "das", "lez\u00b7te", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Brust und Geist vermehlt in Eintrachtsbanden liegen;", "tokens": ["Mit", "Brust", "und", "Geist", "ver\u00b7mehlt", "in", "Ein\u00b7trachts\u00b7ban\u00b7den", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn au\u00dfer jener Welt und ohne diese Lust", "tokens": ["Denn", "au\u00b7\u00dfer", "je\u00b7ner", "Welt", "und", "oh\u00b7ne", "die\u00b7se", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist doch wohl der Natur kein gr\u00f6\u00dfrer Schaz bewust.", "tokens": ["Ist", "doch", "wohl", "der", "Na\u00b7tur", "kein", "gr\u00f6\u00df\u00b7rer", "Schaz", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir spielen unverst\u00f6rt mit Redligkeit und K\u00fc\u00dfen,", "tokens": ["Wir", "spie\u00b7len", "un\u00b7ver\u00b7st\u00f6rt", "mit", "Red\u00b7lig\u00b7keit", "und", "K\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir haben gleichen Sinn, wir w\u00fcntschen einerley,", "tokens": ["Wir", "ha\u00b7ben", "glei\u00b7chen", "Sinn", ",", "wir", "w\u00fcnt\u00b7schen", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind Sclaven s\u00fc\u00dfer Macht, und niemand lebt so frey;", "tokens": ["Sind", "Scla\u00b7ven", "s\u00fc\u00b7\u00dfer", "Macht", ",", "und", "nie\u00b7mand", "lebt", "so", "frey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,", "KON", "PIS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir schwazen, da\u00df uns auch die Worte mangeln m\u00fc\u00dfen,", "tokens": ["Wir", "schwa\u00b7zen", ",", "da\u00df", "uns", "auch", "die", "Wor\u00b7te", "man\u00b7geln", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir schencken uns an uns und n\u00e4hmen, k\u00f6nt es seyn,", "tokens": ["Wir", "schen\u00b7cken", "uns", "an", "uns", "und", "n\u00e4h\u00b7men", ",", "k\u00f6nt", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "$,", "VVFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Seelen wahrer Treu nur einen C\u00f6rper ein.", "tokens": ["Als", "See\u00b7len", "wah\u00b7rer", "Treu", "nur", "ei\u00b7nen", "C\u00f6r\u00b7per", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Uns darf kein Modebrief kein Ehverl\u00f6bn\u00fc\u00df stiften,", "tokens": ["Uns", "darf", "kein", "Mo\u00b7de\u00b7brief", "kein", "Eh\u00b7ver\u00b7l\u00f6b\u00b7n\u00fc\u00df", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Kuppler und kein Geld verbindet unsre Glut,", "tokens": ["Kein", "Kupp\u00b7ler", "und", "kein", "Geld", "ver\u00b7bin\u00b7det", "uns\u00b7re", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Mahlschaz ist mein Herz, dein Herz mein Heiratsgut", "tokens": ["Dein", "Mahl\u00b7schaz", "ist", "mein", "Herz", ",", "dein", "Herz", "mein", "Hei\u00b7rats\u00b7gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unser beider Ruhm die Dichtkunst meiner Schriften,", "tokens": ["Und", "un\u00b7ser", "bei\u00b7der", "Ruhm", "die", "Dicht\u00b7kunst", "mei\u00b7ner", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In welchen Lieb und Scherz so lange Lob gewinnt,", "tokens": ["In", "wel\u00b7chen", "Lieb", "und", "Scherz", "so", "lan\u00b7ge", "Lob", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Kunst und Wi\u00dfenschaft in Deutschland fruchtbahr sind.", "tokens": ["Als", "Kunst", "und", "Wi\u00b7\u00dfen\u00b7schaft", "in", "Deutschland", "frucht\u00b7bahr", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NE", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wir haben unsern Bund die Zeit bew\u00e4hren la\u00dfen;", "tokens": ["Wir", "ha\u00b7ben", "un\u00b7sern", "Bund", "die", "Zeit", "be\u00b7w\u00e4h\u00b7ren", "la\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor dich ist auf der Welt kein be\u00dfrer Mann als ich,", "tokens": ["Vor", "dich", "ist", "auf", "der", "Welt", "kein", "be\u00df\u00b7rer", "Mann", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich find auch auf der Welt kein treuer Weib als dich,", "tokens": ["Ich", "find", "auch", "auf", "der", "Welt", "kein", "treu\u00b7er", "Weib", "als", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir m\u00fcsten sonder uns das beste Leben ha\u00dfen;", "tokens": ["Wir", "m\u00fcs\u00b7ten", "son\u00b7der", "uns", "das", "bes\u00b7te", "Le\u00b7ben", "ha\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da, wo ich dich nicht seh, da ist mir alles leer,", "tokens": ["Da", ",", "wo", "ich", "dich", "nicht", "seh", ",", "da", "ist", "mir", "al\u00b7les", "leer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "PTKNEG", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wenn es auch der Schwarm des gr\u00f6sten Hofes w\u00e4r.", "tokens": ["Und", "wenn", "es", "auch", "der", "Schwarm", "des", "gr\u00f6s\u00b7ten", "Ho\u00b7fes", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Versuchte mich Eugen und b\u00f6the mir der Kayser", "tokens": ["Ver\u00b7such\u00b7te", "mich", "Eu\u00b7gen", "und", "b\u00f6\u00b7the", "mir", "der", "Kay\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor dich, du frommes Kind, Gold, Thron und Purpur an,", "tokens": ["Vor", "dich", ",", "du", "from\u00b7mes", "Kind", ",", "Gold", ",", "Thron", "und", "Pur\u00b7pur", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So spr\u00e4ch ich, wie ich dir mit Warheit schw\u00f6ren kan:", "tokens": ["So", "spr\u00e4ch", "ich", ",", "wie", "ich", "dir", "mit", "War\u00b7heit", "schw\u00f6\u00b7ren", "kan", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich ehre, gro\u00dfer Held, die vielen Siegesreiser,", "tokens": ["Ich", "eh\u00b7re", ",", "gro\u00b7\u00dfer", "Held", ",", "die", "vie\u00b7len", "Sie\u00b7ges\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich weis auch, gro\u00dfer Carl, was Macht und Cronen sind \u2013", "tokens": ["Ich", "weis", "auch", ",", "gro\u00b7\u00dfer", "Carl", ",", "was", "Macht", "und", "Cro\u00b7nen", "sind", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "$,", "ADJA", "NE", "$,", "PWS", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Behaltet, was ihr habt, und last mir nur mein Kind!", "tokens": ["Be\u00b7hal\u00b7tet", ",", "was", "ihr", "habt", ",", "und", "last", "mir", "nur", "mein", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Geseegnet sey der Tag, geseegnet sey die Kammer,", "tokens": ["Ge\u00b7seeg\u00b7net", "sey", "der", "Tag", ",", "ge\u00b7seeg\u00b7net", "sey", "die", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der unsern Bund gesehn, die unsern Ku\u00df geh\u00f6rt!", "tokens": ["Der", "un\u00b7sern", "Bund", "ge\u00b7sehn", ",", "die", "un\u00b7sern", "Ku\u00df", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer jenen durch Verdru\u00df und die mit Fluch entehrt,", "tokens": ["Wer", "je\u00b7nen", "durch", "Ver\u00b7dru\u00df", "und", "die", "mit", "Fluch", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "APPR", "NN", "KON", "ART", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem mach ein b\u00f6ses Weib den Ehstand voller Jammer.", "tokens": ["Dem", "mach", "ein", "b\u00f6\u00b7ses", "Weib", "den", "Eh\u00b7stand", "vol\u00b7ler", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geseegnet sey auch gar der Kummer und der Neid,", "tokens": ["Ge\u00b7seeg\u00b7net", "sey", "auch", "gar", "der", "Kum\u00b7mer", "und", "der", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der wegen deiner Gunst mir manchen Sto\u00df verleiht!", "tokens": ["Der", "we\u00b7gen", "dei\u00b7ner", "Gunst", "mir", "man\u00b7chen", "Sto\u00df", "ver\u00b7leiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "O k\u00f6nt ich doch, mein Kind, in allen Sprachen dichten", "tokens": ["O", "k\u00f6nt", "ich", "doch", ",", "mein", "Kind", ",", "in", "al\u00b7len", "Spra\u00b7chen", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(so w\u00fcntsch ich dann und wann wie einst Petrarchens Mund),", "tokens": ["(", "so", "w\u00fcnt\u00b7sch", "ich", "dann", "und", "wann", "wie", "einst", "Pe\u00b7trar\u00b7chens", "Mund", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "ADV", "KON", "PWAV", "KOKOM", "ADV", "NE", "NN", "$(", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "So th\u00e4t ich deinen Werth den meisten L\u00e4ndern kund,", "tokens": ["So", "th\u00e4t", "ich", "dei\u00b7nen", "Werth", "den", "meis\u00b7ten", "L\u00e4n\u00b7dern", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lies ich jedes Volck von unsrer Liebe richten;", "tokens": ["So", "lies", "ich", "je\u00b7des", "Volck", "von", "uns\u00b7rer", "Lie\u00b7be", "rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Kl\u00fcgsten w\u00fcrden sehn, wie z\u00e4rtlich meine Treu,", "tokens": ["Die", "Kl\u00fcgs\u00b7ten", "w\u00fcr\u00b7den", "sehn", ",", "wie", "z\u00e4rt\u00b7lich", "mei\u00b7ne", "Treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie redlich meine Brust, wie rein dein Herze sey.", "tokens": ["Wie", "red\u00b7lich", "mei\u00b7ne", "Brust", ",", "wie", "rein", "dein", "Her\u00b7ze", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ich thu, so viel ich kan, dein Denckmahl auszubreiten,", "tokens": ["Ich", "thu", ",", "so", "viel", "ich", "kan", ",", "dein", "Denck\u00b7mahl", "aus\u00b7zu\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um bey der sp\u00e4ten Welt durch deinen Ruhm zu bl\u00fchn;", "tokens": ["Um", "bey", "der", "sp\u00e4\u00b7ten", "Welt", "durch", "dei\u00b7nen", "Ruhm", "zu", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie mancher wird noch Trost aus meinen Liedern ziehn,", "tokens": ["Wie", "man\u00b7cher", "wird", "noch", "Trost", "aus", "mei\u00b7nen", "Lie\u00b7dern", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie manchen wird mein Vers zur s\u00fc\u00dfen Regung leiten!", "tokens": ["Wie", "man\u00b7chen", "wird", "mein", "Vers", "zur", "s\u00fc\u00b7\u00dfen", "Re\u00b7gung", "lei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So merck ich, wenn mein Mund der Alten Arbeit list,", "tokens": ["So", "merck", "ich", ",", "wenn", "mein", "Mund", "der", "Al\u00b7ten", "Ar\u00b7beit", "list", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df unsre Liebe schon vordem gewesen ist.", "tokens": ["Da\u00df", "uns\u00b7re", "Lie\u00b7be", "schon", "vor\u00b7dem", "ge\u00b7we\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Was hat wohl unser Wuntsch mehr auf der Welt zu suchen,", "tokens": ["Was", "hat", "wohl", "un\u00b7ser", "Wunt\u00b7sch", "mehr", "auf", "der", "Welt", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und welches Gl\u00fcck ist noch wohl unsers Neides werth?", "tokens": ["Und", "wel\u00b7ches", "Gl\u00fcck", "ist", "noch", "wohl", "un\u00b7sers", "Nei\u00b7des", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn mir des Himmels Huld dich vollends ganz gew\u00e4hrt,", "tokens": ["Wenn", "mir", "des", "Him\u00b7mels", "Huld", "dich", "vol\u00b7lends", "ganz", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00fcte Feind und Groll, so mag der Sp\u00f6tter fluchen;", "tokens": ["So", "w\u00fc\u00b7te", "Feind", "und", "Groll", ",", "so", "mag", "der", "Sp\u00f6t\u00b7ter", "flu\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "KON", "NN", "$,", "ADV", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Drey Dinge sind mein Trost: Gott, Wi\u00dfenschaft und du;", "tokens": ["Drey", "Din\u00b7ge", "sind", "mein", "Trost", ":", "Gott", ",", "Wi\u00b7\u00dfen\u00b7schaft", "und", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPOSAT", "NN", "$.", "NN", "$,", "NN", "KON", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bey diesen seh ich stets den St\u00fcrmen ruhig zu.", "tokens": ["Bey", "die\u00b7sen", "seh", "ich", "stets", "den", "St\u00fcr\u00b7men", "ru\u00b7hig", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "ADV", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ach Kind, ach liebstes Kind, was war das vor Vergn\u00fcgen!", "tokens": ["Ach", "Kind", ",", "ach", "liebs\u00b7tes", "Kind", ",", "was", "war", "das", "vor", "Ver\u00b7gn\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "XY", "ADJA", "NN", "$,", "PWS", "VAFIN", "PDS", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Himmel geb uns doch dergleichen N\u00e4chte viel", "tokens": ["Der", "Him\u00b7mel", "geb", "uns", "doch", "derg\u00b7lei\u00b7chen", "N\u00e4ch\u00b7te", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PIS", "NN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und las uns so vertraut bis an das lezte Ziel", "tokens": ["Und", "las", "uns", "so", "ver\u00b7traut", "bis", "an", "das", "lez\u00b7te", "Ziel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Brust und Geist vermehlt in Eintrachtsbanden liegen;", "tokens": ["Mit", "Brust", "und", "Geist", "ver\u00b7mehlt", "in", "Ein\u00b7trachts\u00b7ban\u00b7den", "lie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn au\u00dfer jener Welt und ohne diese Lust", "tokens": ["Denn", "au\u00b7\u00dfer", "je\u00b7ner", "Welt", "und", "oh\u00b7ne", "die\u00b7se", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PDAT", "NN", "KON", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist doch wohl der Natur kein gr\u00f6\u00dfrer Schaz bewust.", "tokens": ["Ist", "doch", "wohl", "der", "Na\u00b7tur", "kein", "gr\u00f6\u00df\u00b7rer", "Schaz", "be\u00b7wust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ART", "NN", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.11": {"line.1": {"text": "Wir spielen unverst\u00f6rt mit Redligkeit und K\u00fc\u00dfen,", "tokens": ["Wir", "spie\u00b7len", "un\u00b7ver\u00b7st\u00f6rt", "mit", "Red\u00b7lig\u00b7keit", "und", "K\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir haben gleichen Sinn, wir w\u00fcntschen einerley,", "tokens": ["Wir", "ha\u00b7ben", "glei\u00b7chen", "Sinn", ",", "wir", "w\u00fcnt\u00b7schen", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sind Sclaven s\u00fc\u00dfer Macht, und niemand lebt so frey;", "tokens": ["Sind", "Scla\u00b7ven", "s\u00fc\u00b7\u00dfer", "Macht", ",", "und", "nie\u00b7mand", "lebt", "so", "frey", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADJA", "NN", "$,", "KON", "PIS", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir schwazen, da\u00df uns auch die Worte mangeln m\u00fc\u00dfen,", "tokens": ["Wir", "schwa\u00b7zen", ",", "da\u00df", "uns", "auch", "die", "Wor\u00b7te", "man\u00b7geln", "m\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wir schencken uns an uns und n\u00e4hmen, k\u00f6nt es seyn,", "tokens": ["Wir", "schen\u00b7cken", "uns", "an", "uns", "und", "n\u00e4h\u00b7men", ",", "k\u00f6nt", "es", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPER", "KON", "VVFIN", "$,", "VVFIN", "PPER", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Seelen wahrer Treu nur einen C\u00f6rper ein.", "tokens": ["Als", "See\u00b7len", "wah\u00b7rer", "Treu", "nur", "ei\u00b7nen", "C\u00f6r\u00b7per", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Uns darf kein Modebrief kein Ehverl\u00f6bn\u00fc\u00df stiften,", "tokens": ["Uns", "darf", "kein", "Mo\u00b7de\u00b7brief", "kein", "Eh\u00b7ver\u00b7l\u00f6b\u00b7n\u00fc\u00df", "stif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Kein Kuppler und kein Geld verbindet unsre Glut,", "tokens": ["Kein", "Kupp\u00b7ler", "und", "kein", "Geld", "ver\u00b7bin\u00b7det", "uns\u00b7re", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "KON", "PIAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dein Mahlschaz ist mein Herz, dein Herz mein Heiratsgut", "tokens": ["Dein", "Mahl\u00b7schaz", "ist", "mein", "Herz", ",", "dein", "Herz", "mein", "Hei\u00b7rats\u00b7gut"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und unser beider Ruhm die Dichtkunst meiner Schriften,", "tokens": ["Und", "un\u00b7ser", "bei\u00b7der", "Ruhm", "die", "Dicht\u00b7kunst", "mei\u00b7ner", "Schrif\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "PIAT", "NN", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In welchen Lieb und Scherz so lange Lob gewinnt,", "tokens": ["In", "wel\u00b7chen", "Lieb", "und", "Scherz", "so", "lan\u00b7ge", "Lob", "ge\u00b7winnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "KON", "NN", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als Kunst und Wi\u00dfenschaft in Deutschland fruchtbahr sind.", "tokens": ["Als", "Kunst", "und", "Wi\u00b7\u00dfen\u00b7schaft", "in", "Deutschland", "frucht\u00b7bahr", "sind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "APPR", "NE", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Wir haben unsern Bund die Zeit bew\u00e4hren la\u00dfen;", "tokens": ["Wir", "ha\u00b7ben", "un\u00b7sern", "Bund", "die", "Zeit", "be\u00b7w\u00e4h\u00b7ren", "la\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vor dich ist auf der Welt kein be\u00dfrer Mann als ich,", "tokens": ["Vor", "dich", "ist", "auf", "der", "Welt", "kein", "be\u00df\u00b7rer", "Mann", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich find auch auf der Welt kein treuer Weib als dich,", "tokens": ["Ich", "find", "auch", "auf", "der", "Welt", "kein", "treu\u00b7er", "Weib", "als", "dich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PIAT", "ADJA", "NN", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir m\u00fcsten sonder uns das beste Leben ha\u00dfen;", "tokens": ["Wir", "m\u00fcs\u00b7ten", "son\u00b7der", "uns", "das", "bes\u00b7te", "Le\u00b7ben", "ha\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "KON", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da, wo ich dich nicht seh, da ist mir alles leer,", "tokens": ["Da", ",", "wo", "ich", "dich", "nicht", "seh", ",", "da", "ist", "mir", "al\u00b7les", "leer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "PPER", "PRF", "PTKNEG", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und wenn es auch der Schwarm des gr\u00f6sten Hofes w\u00e4r.", "tokens": ["Und", "wenn", "es", "auch", "der", "Schwarm", "des", "gr\u00f6s\u00b7ten", "Ho\u00b7fes", "w\u00e4r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Versuchte mich Eugen und b\u00f6the mir der Kayser", "tokens": ["Ver\u00b7such\u00b7te", "mich", "Eu\u00b7gen", "und", "b\u00f6\u00b7the", "mir", "der", "Kay\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "NE", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Vor dich, du frommes Kind, Gold, Thron und Purpur an,", "tokens": ["Vor", "dich", ",", "du", "from\u00b7mes", "Kind", ",", "Gold", ",", "Thron", "und", "Pur\u00b7pur", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So spr\u00e4ch ich, wie ich dir mit Warheit schw\u00f6ren kan:", "tokens": ["So", "spr\u00e4ch", "ich", ",", "wie", "ich", "dir", "mit", "War\u00b7heit", "schw\u00f6\u00b7ren", "kan", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PPER", "PPER", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich ehre, gro\u00dfer Held, die vielen Siegesreiser,", "tokens": ["Ich", "eh\u00b7re", ",", "gro\u00b7\u00dfer", "Held", ",", "die", "vie\u00b7len", "Sie\u00b7ges\u00b7rei\u00b7ser", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJA", "NN", "$,", "PRELS", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich weis auch, gro\u00dfer Carl, was Macht und Cronen sind \u2013", "tokens": ["Ich", "weis", "auch", ",", "gro\u00b7\u00dfer", "Carl", ",", "was", "Macht", "und", "Cro\u00b7nen", "sind", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "ADV", "$,", "ADJA", "NE", "$,", "PWS", "NN", "KON", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Behaltet, was ihr habt, und last mir nur mein Kind!", "tokens": ["Be\u00b7hal\u00b7tet", ",", "was", "ihr", "habt", ",", "und", "last", "mir", "nur", "mein", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,", "KON", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Geseegnet sey der Tag, geseegnet sey die Kammer,", "tokens": ["Ge\u00b7seeg\u00b7net", "sey", "der", "Tag", ",", "ge\u00b7seeg\u00b7net", "sey", "die", "Kam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ART", "NN", "$,", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der unsern Bund gesehn, die unsern Ku\u00df geh\u00f6rt!", "tokens": ["Der", "un\u00b7sern", "Bund", "ge\u00b7sehn", ",", "die", "un\u00b7sern", "Ku\u00df", "ge\u00b7h\u00f6rt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer jenen durch Verdru\u00df und die mit Fluch entehrt,", "tokens": ["Wer", "je\u00b7nen", "durch", "Ver\u00b7dru\u00df", "und", "die", "mit", "Fluch", "ent\u00b7ehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PDAT", "APPR", "NN", "KON", "ART", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem mach ein b\u00f6ses Weib den Ehstand voller Jammer.", "tokens": ["Dem", "mach", "ein", "b\u00f6\u00b7ses", "Weib", "den", "Eh\u00b7stand", "vol\u00b7ler", "Jam\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geseegnet sey auch gar der Kummer und der Neid,", "tokens": ["Ge\u00b7seeg\u00b7net", "sey", "auch", "gar", "der", "Kum\u00b7mer", "und", "der", "Neid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "ADV", "ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der wegen deiner Gunst mir manchen Sto\u00df verleiht!", "tokens": ["Der", "we\u00b7gen", "dei\u00b7ner", "Gunst", "mir", "man\u00b7chen", "Sto\u00df", "ver\u00b7leiht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "O k\u00f6nt ich doch, mein Kind, in allen Sprachen dichten", "tokens": ["O", "k\u00f6nt", "ich", "doch", ",", "mein", "Kind", ",", "in", "al\u00b7len", "Spra\u00b7chen", "dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "$,", "APPR", "PIAT", "NN", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "(so w\u00fcntsch ich dann und wann wie einst Petrarchens Mund),", "tokens": ["(", "so", "w\u00fcnt\u00b7sch", "ich", "dann", "und", "wann", "wie", "einst", "Pe\u00b7trar\u00b7chens", "Mund", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADJD", "PPER", "ADV", "KON", "PWAV", "KOKOM", "ADV", "NE", "NN", "$(", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "So th\u00e4t ich deinen Werth den meisten L\u00e4ndern kund,", "tokens": ["So", "th\u00e4t", "ich", "dei\u00b7nen", "Werth", "den", "meis\u00b7ten", "L\u00e4n\u00b7dern", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ART", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So lies ich jedes Volck von unsrer Liebe richten;", "tokens": ["So", "lies", "ich", "je\u00b7des", "Volck", "von", "uns\u00b7rer", "Lie\u00b7be", "rich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Kl\u00fcgsten w\u00fcrden sehn, wie z\u00e4rtlich meine Treu,", "tokens": ["Die", "Kl\u00fcgs\u00b7ten", "w\u00fcr\u00b7den", "sehn", ",", "wie", "z\u00e4rt\u00b7lich", "mei\u00b7ne", "Treu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie redlich meine Brust, wie rein dein Herze sey.", "tokens": ["Wie", "red\u00b7lich", "mei\u00b7ne", "Brust", ",", "wie", "rein", "dein", "Her\u00b7ze", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "$,", "PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Ich thu, so viel ich kan, dein Denckmahl auszubreiten,", "tokens": ["Ich", "thu", ",", "so", "viel", "ich", "kan", ",", "dein", "Denck\u00b7mahl", "aus\u00b7zu\u00b7brei\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "PPER", "VMFIN", "$,", "PPOSAT", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um bey der sp\u00e4ten Welt durch deinen Ruhm zu bl\u00fchn;", "tokens": ["Um", "bey", "der", "sp\u00e4\u00b7ten", "Welt", "durch", "dei\u00b7nen", "Ruhm", "zu", "bl\u00fchn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wie mancher wird noch Trost aus meinen Liedern ziehn,", "tokens": ["Wie", "man\u00b7cher", "wird", "noch", "Trost", "aus", "mei\u00b7nen", "Lie\u00b7dern", "ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "ADV", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie manchen wird mein Vers zur s\u00fc\u00dfen Regung leiten!", "tokens": ["Wie", "man\u00b7chen", "wird", "mein", "Vers", "zur", "s\u00fc\u00b7\u00dfen", "Re\u00b7gung", "lei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So merck ich, wenn mein Mund der Alten Arbeit list,", "tokens": ["So", "merck", "ich", ",", "wenn", "mein", "Mund", "der", "Al\u00b7ten", "Ar\u00b7beit", "list", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df unsre Liebe schon vordem gewesen ist.", "tokens": ["Da\u00df", "uns\u00b7re", "Lie\u00b7be", "schon", "vor\u00b7dem", "ge\u00b7we\u00b7sen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADV", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Was hat wohl unser Wuntsch mehr auf der Welt zu suchen,", "tokens": ["Was", "hat", "wohl", "un\u00b7ser", "Wunt\u00b7sch", "mehr", "auf", "der", "Welt", "zu", "su\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und welches Gl\u00fcck ist noch wohl unsers Neides werth?", "tokens": ["Und", "wel\u00b7ches", "Gl\u00fcck", "ist", "noch", "wohl", "un\u00b7sers", "Nei\u00b7des", "werth", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn mir des Himmels Huld dich vollends ganz gew\u00e4hrt,", "tokens": ["Wenn", "mir", "des", "Him\u00b7mels", "Huld", "dich", "vol\u00b7lends", "ganz", "ge\u00b7w\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00fcte Feind und Groll, so mag der Sp\u00f6tter fluchen;", "tokens": ["So", "w\