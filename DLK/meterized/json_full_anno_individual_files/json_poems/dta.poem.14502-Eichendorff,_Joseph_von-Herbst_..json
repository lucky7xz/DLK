{"dta.poem.14502": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Herbst .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Und die V\u00f6glein hoch in L\u00fcften", "tokens": ["Und", "die", "V\u00f6\u00b7glein", "hoch", "in", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber blaue Berg' und Seen", "tokens": ["Ue\u00b7ber", "blau\u00b7e", "Ber\u00b7g'", "und", "Seen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Zieh'n zur Ferne nach den Kl\u00fcften,", "tokens": ["Zieh'n", "zur", "Fer\u00b7ne", "nach", "den", "Kl\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo die hohen Cedern steh'n,", "tokens": ["Wo", "die", "ho\u00b7hen", "Ce\u00b7dern", "steh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo mit ihren gold'nen Schwingen", "tokens": ["Wo", "mit", "ih\u00b7ren", "gold'\u00b7nen", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf des Benedeiten Gruft", "tokens": ["Auf", "des", "Be\u00b7ne\u00b7dei\u00b7ten", "Gruft"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Engel Hosiannah singen", "tokens": ["En\u00b7gel", "Ho\u00b7si\u00b7an\u00b7nah", "sin\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "NE", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "N\u00e4chtens durch die stille Luft.", "tokens": ["N\u00e4ch\u00b7tens", "durch", "die", "stil\u00b7le", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}