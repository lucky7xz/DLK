{"dta.poem.9200": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "7.  Die Vers\u00f6hnung.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Mein liebgen/ so hast du dich besser besonnen/", "tokens": ["Mein", "lieb\u00b7gen", "/", "so", "hast", "du", "dich", "bes\u00b7ser", "be\u00b7son\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$(", "ADV", "VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Und darff ich wieder zu dir gehn?", "tokens": ["Und", "darff", "ich", "wie\u00b7der", "zu", "dir", "gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat endlich die unschuld dein hertze gewonnen/", "tokens": ["Hat", "end\u00b7lich", "die", "un\u00b7schuld", "dein", "hert\u00b7ze", "ge\u00b7won\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Und lernst du meinen sinn verstehn?", "tokens": ["Und", "lernst", "du", "mei\u00b7nen", "sinn", "ver\u00b7stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mein freundliches hertzgen/ versichere dich frey/", "tokens": ["Mein", "freund\u00b7li\u00b7ches", "hertz\u00b7gen", "/", "ver\u00b7si\u00b7che\u00b7re", "dich", "frey", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Ich bleibe dein diener und ewig getreu.", "tokens": ["Ich", "blei\u00b7be", "dein", "die\u00b7ner", "und", "e\u00b7wig", "ge\u00b7treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "ADJD", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}}}}