{"dta.poem.8501": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Sylvesterabend.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am letzten Tage des Jahres,", "tokens": ["Am", "letz\u00b7ten", "Ta\u00b7ge", "des", "Jah\u00b7res", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da dacht\u2019 ich, wie Mancher todt,", "tokens": ["Da", "dacht'", "ich", ",", "wie", "Man\u00b7cher", "todt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWAV", "PIS", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Den ich bei seinem Beginne", "tokens": ["Den", "ich", "bei", "sei\u00b7nem", "Be\u00b7gin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Noch lustig gesehen und roth;", "tokens": ["Noch", "lus\u00b7tig", "ge\u00b7se\u00b7hen", "und", "roth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "KON", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Wie mancher am Sargesbaume", "tokens": ["Wie", "man\u00b7cher", "am", "Sar\u00b7ges\u00b7bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "PIS", "APPRART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Gelacht, unter\u2019m laubigen Zelt,", "tokens": ["Ge\u00b7lacht", ",", "un\u00b7ter'm", "lau\u00b7bi\u00b7gen", "Zelt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Und wie vielleicht auch der meine", "tokens": ["Und", "wie", "viel\u00b7leicht", "auch", "der", "mei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADV", "ART", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Zur Stunde schon sei gef\u00e4llt.", "tokens": ["Zur", "Stun\u00b7de", "schon", "sei", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wer wird dann meiner gedenken,", "tokens": ["Wer", "wird", "dann", "mei\u00b7ner", "ge\u00b7den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "PPOSAT", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn ich nun gestorben bin?", "tokens": ["Wenn", "ich", "nun", "ge\u00b7stor\u00b7ben", "bin", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wohl wird man Thr\u00e4nen mir weihen,", "tokens": ["Wohl", "wird", "man", "Thr\u00e4\u00b7nen", "mir", "wei\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Doch diese sind bald dahin!", "tokens": ["Doch", "die\u00b7se", "sind", "bald", "da\u00b7hin", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "PAV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Wohl wird man Lieder mir singen,", "tokens": ["Wohl", "wird", "man", "Lie\u00b7der", "mir", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Doch diese verweht die Zeit!", "tokens": ["Doch", "die\u00b7se", "ver\u00b7weht", "die", "Zeit", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Vielleicht einen Stein mir setzen,", "tokens": ["Viel\u00b7leicht", "ei\u00b7nen", "Stein", "mir", "set\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Den bald der Winter verschneit.", "tokens": ["Den", "bald", "der", "Win\u00b7ter", "ver\u00b7schneit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und wenn die Flocke zerronnen", "tokens": ["Und", "wenn", "die", "Flo\u00b7cke", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und kehrt der Nachtigall Schlag,", "tokens": ["Und", "kehrt", "der", "Nach\u00b7ti\u00b7gall", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Dann blieb nur die heilige Messe", "tokens": ["Dann", "blieb", "nur", "die", "hei\u00b7li\u00b7ge", "Mes\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "An meinem Ged\u00e4chtni\u00dftag;", "tokens": ["An", "mei\u00b7nem", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7tag", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Nur auf zerrissenem Blatte", "tokens": ["Nur", "auf", "zer\u00b7ris\u00b7se\u00b7nem", "Blat\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ein Lied von fl\u00fcchtigem Stift,", "tokens": ["Ein", "Lied", "von", "fl\u00fcch\u00b7ti\u00b7gem", "Stift", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Und mir zu H\u00e4upten die Decke", "tokens": ["Und", "mir", "zu", "H\u00e4up\u00b7ten", "die", "De\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "ART", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Mit mooszerfressener Schrift.", "tokens": ["Mit", "moos\u00b7zer\u00b7fres\u00b7se\u00b7ner", "Schrift", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Wohl hab\u2019 ich viele Bekannte,", "tokens": ["Wohl", "hab'", "ich", "vie\u00b7le", "Be\u00b7kann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die gern mir \u00f6ffnen ihr Haus,", "tokens": ["Die", "gern", "mir", "\u00f6ff\u00b7nen", "ihr", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch wenn die Th\u00fcre geschlossen,", "tokens": ["Doch", "wenn", "die", "Th\u00fc\u00b7re", "ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Dann schaut man nimmer hinaus;", "tokens": ["Dann", "schaut", "man", "nim\u00b7mer", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADV", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Dann haben sie einen Andern", "tokens": ["Dann", "ha\u00b7ben", "sie", "ei\u00b7nen", "An\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ART", "ADJA"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "An meiner Stelle erw\u00e4hlt,", "tokens": ["An", "mei\u00b7ner", "Stel\u00b7le", "er\u00b7w\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Der ihnen singt meine Lieder", "tokens": ["Der", "ih\u00b7nen", "singt", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Und meine Geschichten erz\u00e4hlt.", "tokens": ["Und", "mei\u00b7ne", "Ge\u00b7schich\u00b7ten", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Wohl hab\u2019 ich ehrliche Freunde,", "tokens": ["Wohl", "hab'", "ich", "ehr\u00b7li\u00b7che", "Freun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die greift es h\u00e4rter schon an;", "tokens": ["Die", "greift", "es", "h\u00e4r\u00b7ter", "schon", "an", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "ADV", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Doch wenn die Kette zerrissen,", "tokens": ["Doch", "wenn", "die", "Ket\u00b7te", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Man flickt sie so gut man kann;", "tokens": ["Man", "flickt", "sie", "so", "gut", "man", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "ADJD", "PIS", "VMFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Zwei Tage blieben sie d\u00fcster,", "tokens": ["Zwei", "Ta\u00b7ge", "blie\u00b7ben", "sie", "d\u00fcs\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "\u2014 Sie meinten es ernst und treu, \u2014", "tokens": ["Sie", "mein\u00b7ten", "es", "ernst", "und", "treu", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "$("], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.7": {"text": "Und gingen dann in die Oper", "tokens": ["Und", "gin\u00b7gen", "dann", "in", "die", "O\u00b7per"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am dritten Tage auf\u2019s Neu.", "tokens": ["Am", "drit\u00b7ten", "Ta\u00b7ge", "auf's", "Neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Ich habe liebe Verwandte,", "tokens": ["Ich", "ha\u00b7be", "lie\u00b7be", "Ver\u00b7wand\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die tragen im Herzen das Leid;", "tokens": ["Die", "tra\u00b7gen", "im", "Her\u00b7zen", "das", "Leid", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Allein wie d\u00fcrfte verk\u00fcmmern", "tokens": ["Al\u00b7lein", "wie", "d\u00fcrf\u00b7te", "ver\u00b7k\u00fcm\u00b7mern"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PWAV", "VMFIN", "VVINF"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.4": {"text": "Ein Leben so Vielen geweiht?", "tokens": ["Ein", "Le\u00b7ben", "so", "Vie\u00b7len", "ge\u00b7weiht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Sie haben sich eben bezwungen,", "tokens": ["Sie", "ha\u00b7ben", "sich", "e\u00b7ben", "be\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "F\u00fcr andre Pflichten geschont,", "tokens": ["F\u00fcr", "and\u00b7re", "Pflich\u00b7ten", "ge\u00b7schont", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "Nur schweben wohl meine Z\u00fcge", "tokens": ["Nur", "schwe\u00b7ben", "wohl", "mei\u00b7ne", "Z\u00fc\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Zuweilen noch \u00fcber den Mond.", "tokens": ["Zu\u00b7wei\u00b7len", "noch", "\u00fc\u00b7ber", "den", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.7": {"line.1": {"text": "Ich habe Bruder und Schwester,", "tokens": ["Ich", "ha\u00b7be", "Bru\u00b7der", "und", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da ging in\u2019s Leben der Stich,", "tokens": ["Da", "ging", "in's", "Le\u00b7ben", "der", "Stich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Da sind viel Thr\u00e4nen geflossen", "tokens": ["Da", "sind", "viel", "Thr\u00e4\u00b7nen", "ge\u00b7flos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und viele Seufzer um mich.", "tokens": ["Und", "vie\u00b7le", "Seuf\u00b7zer", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "APPR", "PPER", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.5": {"text": "O h\u00e4tten sie einsam gestanden,", "tokens": ["O", "h\u00e4t\u00b7ten", "sie", "ein\u00b7sam", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Ich lebte im ewigen Licht!", "tokens": ["Ich", "leb\u00b7te", "im", "e\u00b7wi\u00b7gen", "Licht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Nun haben sie meines vergessen", "tokens": ["Nun", "ha\u00b7ben", "sie", "mei\u00b7nes", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.8": {"text": "Um ihres Kindes Gesicht.", "tokens": ["Um", "ih\u00b7res", "Kin\u00b7des", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Ich hab\u2019, ich hab\u2019 eine Mutter,", "tokens": ["Ich", "hab'", ",", "ich", "hab'", "ei\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der kehr\u2019 ich im Traum bei Nacht,", "tokens": ["Der", "kehr'", "ich", "im", "Traum", "bei", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die kann das Auge nicht schlie\u00dfen,", "tokens": ["Die", "kann", "das", "Au\u00b7ge", "nicht", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Bis mein sie betend gedacht;", "tokens": ["Bis", "mein", "sie", "be\u00b7tend", "ge\u00b7dacht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die sieht mich in jedem Grabe,", "tokens": ["Die", "sieht", "mich", "in", "je\u00b7dem", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Die h\u00f6rt mich im Rauschen des Hains, \u2014", "tokens": ["Die", "h\u00f6rt", "mich", "im", "Rau\u00b7schen", "des", "Hains", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN", "ART", "NN", "$,", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "O vergessen kann eine Mutter", "tokens": ["O", "ver\u00b7ges\u00b7sen", "kann", "ei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVINF", "VMFIN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.8": {"text": "Von zwanzig Kindern nicht eins.", "tokens": ["Von", "zwan\u00b7zig", "Kin\u00b7dern", "nicht", "eins", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "PTKNEG", "PIS", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}