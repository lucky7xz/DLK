["KON", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wer wagt, den Liebreiz zu bestreiten", "tokens": ["Wer", "wagt", ",", "den", "Lieb\u00b7reiz", "zu", "be\u00b7strei\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "der jungen M\u00e4dchen aus dem Osten,", "tokens": ["der", "jun\u00b7gen", "M\u00e4d\u00b7chen", "aus", "dem", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "indem, da\u00df sie so ville kosten?", "tokens": ["in\u00b7dem", ",", "da\u00df", "sie", "so", "vil\u00b7le", "kos\u00b7ten", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "\u00bbder Stein is Tineff!\u00ab haucht sie lind.", "tokens": ["\u00bb", "der", "Stein", "is", "Ti\u00b7neff", "!", "\u00ab", "haucht", "sie", "lind", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "FM", "NN", "$.", "$(", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbund der \u2013 der will mein Schklave sind?\u00ab", "tokens": ["\u00bb", "und", "der", "\u2013", "der", "will", "mein", "Schkla\u00b7ve", "sind", "?", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "ART", "$(", "PDS", "VMFIN", "PPOSAT", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als deiner anderswo gefeiert,", "tokens": ["Als", "dei\u00b7ner", "an\u00b7ders\u00b7wo", "ge\u00b7fei\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "mein Kind, hast du dich entgeschleiert,", "tokens": ["mein", "Kind", ",", "hast", "du", "dich", "ent\u00b7ge\u00b7schlei\u00b7ert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VAFIN", "PPER", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "so tief, da\u00df ich nach hinten prallte . . .", "tokens": ["so", "tief", ",", "da\u00df", "ich", "nach", "hin\u00b7ten", "prall\u00b7te", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "APPR", "ADV", "VVFIN", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der ganze Bauch war eine Falte!", "tokens": ["Der", "gan\u00b7ze", "Bauch", "war", "ei\u00b7ne", "Fal\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und das soll alles ich verlassen?", "tokens": ["Und", "das", "soll", "al\u00b7les", "ich", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VMFIN", "PIS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Berlin \u2013 ich kann es noch nicht fassen!", "tokens": ["Ber\u00b7lin", "\u2013", "ich", "kann", "es", "noch", "nicht", "fas\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PPER", "VMFIN", "PPER",