{"dta.poem.7300": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Meine Todten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Wer eine ernste Fahrt beginnt,", "tokens": ["Wer", "ei\u00b7ne", "erns\u00b7te", "Fahrt", "be\u00b7ginnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Muth bedarf und frischen Wind,", "tokens": ["Die", "Muth", "be\u00b7darf", "und", "fri\u00b7schen", "Wind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er schaut verlangend in die Weite", "tokens": ["Er", "schaut", "ver\u00b7lan\u00b7gend", "in", "die", "Wei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach eines treuen Auges Brand,", "tokens": ["Nach", "ei\u00b7nes", "treu\u00b7en", "Au\u00b7ges", "Brand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach einem warmen Druck der Hand,", "tokens": ["Nach", "ei\u00b7nem", "war\u00b7men", "Druck", "der", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nach einem Wort, das ihn geleite.", "tokens": ["Nach", "ei\u00b7nem", "Wort", ",", "das", "ihn", "ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein ernstes Wagen heb' ich an,", "tokens": ["Ein", "erns\u00b7tes", "Wa\u00b7gen", "heb'", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So tret' ich denn zu euch hinan,", "tokens": ["So", "tret'", "ich", "denn", "zu", "euch", "hi\u00b7nan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr meine stillen strengen Todten;", "tokens": ["Ihr", "mei\u00b7ne", "stil\u00b7len", "stren\u00b7gen", "Tod\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich bin erwacht an eurer Gruft,", "tokens": ["Ich", "bin", "er\u00b7wacht", "an", "eu\u00b7rer", "Gruft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Aus Wasser, Feuer, Erde, Luft,", "tokens": ["Aus", "Was\u00b7ser", ",", "Feu\u00b7er", ",", "Er\u00b7de", ",", "Luft", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat eure Stimme mir geboten.", "tokens": ["Hat", "eu\u00b7re", "Stim\u00b7me", "mir", "ge\u00b7bo\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn die Natur in Hader lag,", "tokens": ["Wenn", "die", "Na\u00b7tur", "in", "Ha\u00b7der", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und durch die Wolkenwirbel brach", "tokens": ["Und", "durch", "die", "Wol\u00b7ken\u00b7wir\u00b7bel", "brach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Funke jener tausend Sonnen, \u2014", "tokens": ["Ein", "Fun\u00b7ke", "je\u00b7ner", "tau\u00b7send", "Son\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PDAT", "CARD", "NN", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sprecht aus der Elemente Streit", "tokens": ["Sprecht", "aus", "der", "E\u00b7le\u00b7men\u00b7te", "Streit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Ihr nicht von einer Ewigkeit", "tokens": ["Ihr", "nicht", "von", "ei\u00b7ner", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und unersch\u00f6pften Lichtes Bronnen?", "tokens": ["Und", "un\u00b7er\u00b7sch\u00f6pf\u00b7ten", "Lich\u00b7tes", "Bron\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Am Hange schlich ich, krank und matt,", "tokens": ["Am", "Han\u00b7ge", "schlich", "ich", ",", "krank", "und", "matt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da habt ihr mir das welke Blatt", "tokens": ["Da", "habt", "ihr", "mir", "das", "wel\u00b7ke", "Blatt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Warnungsfl\u00fcstern zugetragen,", "tokens": ["Mit", "War\u00b7nungs\u00b7fl\u00fcs\u00b7tern", "zu\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gel\u00e4chelt aus der Welle Kreis,", "tokens": ["Ge\u00b7l\u00e4\u00b7chelt", "aus", "der", "Wel\u00b7le", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Habt aus des Angers starrem Eis", "tokens": ["Habt", "aus", "des", "An\u00b7gers", "star\u00b7rem", "Eis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Blumenaugen aufgeschlagen.", "tokens": ["Die", "Blu\u00b7men\u00b7au\u00b7gen", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Was meine Adern mu\u00df durchziehn,", "tokens": ["Was", "mei\u00b7ne", "A\u00b7dern", "mu\u00df", "durch\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sah ich's nicht flammen und vergl\u00fchn,", "tokens": ["Sah", "ich's", "nicht", "flam\u00b7men", "und", "ver\u00b7gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PTKNEG", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An eurem Schreine nicht erkalten?", "tokens": ["An", "eu\u00b7rem", "Schrei\u00b7ne", "nicht", "er\u00b7kal\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Auge hauchtet ihr den Schein,", "tokens": ["Vom", "Au\u00b7ge", "hauch\u00b7tet", "ihr", "den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr meine Richter, die allein", "tokens": ["Ihr", "mei\u00b7ne", "Rich\u00b7ter", ",", "die", "al\u00b7lein"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In treuer Hand die Wage halten.", "tokens": ["In", "treu\u00b7er", "Hand", "die", "Wa\u00b7ge", "hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Kalt ist der Druck von eurer Hand,", "tokens": ["Kalt", "ist", "der", "Druck", "von", "eu\u00b7rer", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erloschen eures Blickes Brand,", "tokens": ["Er\u00b7lo\u00b7schen", "eu\u00b7res", "Bli\u00b7ckes", "Brand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euer Laut der Oede Odem,", "tokens": ["Und", "eu\u00b7er", "Laut", "der", "O\u00b7e\u00b7de", "O\u00b7dem", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Doch keine andre Rechte dr\u00fcckt", "tokens": ["Doch", "kei\u00b7ne", "and\u00b7re", "Rech\u00b7te", "dr\u00fcckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So traut, so hat kein Aug' geblickt,", "tokens": ["So", "traut", ",", "so", "hat", "kein", "Aug'", "ge\u00b7blickt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADV", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So spricht kein Wort, wie Grabesbrodem!", "tokens": ["So", "spricht", "kein", "Wort", ",", "wie", "Gra\u00b7bes\u00b7bro\u00b7dem", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "PWAV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich fasse eures Kreuzes Stab,", "tokens": ["Ich", "fas\u00b7se", "eu\u00b7res", "Kreu\u00b7zes", "Stab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und beuge meine Stirn hinab", "tokens": ["Und", "beu\u00b7ge", "mei\u00b7ne", "Stirn", "hin\u00b7ab"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu eurem Gr\u00e4serhauch, dem stillen,", "tokens": ["Zu", "eu\u00b7rem", "Gr\u00e4\u00b7ser\u00b7hauch", ",", "dem", "stil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Zumeist geliebt, zuerst gegr\u00fc\u00dft,", "tokens": ["Zu\u00b7meist", "ge\u00b7liebt", ",", "zu\u00b7erst", "ge\u00b7gr\u00fc\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00dft, lauter wie der Aether flie\u00dft,", "tokens": ["La\u00dft", ",", "lau\u00b7ter", "wie", "der", "A\u00b7e\u00b7ther", "flie\u00dft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PIAT", "KOKOM", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Mir Wahrheit in die Seele quillen.", "tokens": ["Mir", "Wahr\u00b7heit", "in", "die", "See\u00b7le", "quil\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}