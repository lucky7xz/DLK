{"textgrid.poem.65909": {"metadata": {"author": {"name": "D\u00e4ubler, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Anbruch", "genre": "verse", "period": "N.A.", "pub_year": 1905, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur Pflugschar tritt, von ferngebautem Wagen,", "tokens": ["Zur", "Pflug\u00b7schar", "tritt", ",", "von", "fern\u00b7ge\u00b7bau\u00b7tem", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Tript\u00f3lemos, ein nackter Knabe noch;", "tokens": ["Trip\u00b7t\u00f3le\u00b7mos", ",", "ein", "nack\u00b7ter", "Kna\u00b7be", "noch", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "ADJA", "NN", "ADV", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sein Schlangenpaar entledigt er vom Joch,", "tokens": ["Sein", "Schlan\u00b7gen\u00b7paar", "ent\u00b7le\u00b7digt", "er", "vom", "Joch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Denn fr\u00f6hlich mag es durch die R\u00f6te jagen.", "tokens": ["Denn", "fr\u00f6h\u00b7lich", "mag", "es", "durch", "die", "R\u00f6\u00b7te", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Gott ist hoch, wie \u00c4hren reifend ragen.", "tokens": ["Der", "Gott", "ist", "hoch", ",", "wie", "\u00c4h\u00b7ren", "rei\u00b7fend", "ra\u00b7gen", "."], "token_info": ["word",