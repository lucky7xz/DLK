{"dta.poem.14039": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Wahl .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Herz m\u00f6cht' mir zerspringen,", "tokens": ["Mein", "Herz", "m\u00f6cht'", "mir", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Darum so wein' ich nicht,", "tokens": ["Da\u00b7rum", "so", "wein'", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Darum so mu\u00df ich singen", "tokens": ["Da\u00b7rum", "so", "mu\u00df", "ich", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis da\u00df der Tag anbricht.", "tokens": ["Bis", "da\u00df", "der", "Tag", "an\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}