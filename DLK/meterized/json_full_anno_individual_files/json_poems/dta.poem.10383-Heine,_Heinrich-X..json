{"dta.poem.10383": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "X.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Die Lotosblume \u00e4ngstigt", "tokens": ["Die", "Lo\u00b7tos\u00b7blu\u00b7me", "\u00e4ngs\u00b7tigt"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sich vor der Sonne Pracht,", "tokens": ["Sich", "vor", "der", "Son\u00b7ne", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und mit gesenktem Haupte", "tokens": ["Und", "mit", "ge\u00b7senk\u00b7tem", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erwartet sie tr\u00e4umend die Nacht.", "tokens": ["Er\u00b7war\u00b7tet", "sie", "tr\u00e4u\u00b7mend", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Der Mond, der ist ihr Buhle.", "tokens": ["Der", "Mond", ",", "der", "ist", "ihr", "Buh\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er weckt sie mit seinem Licht',", "tokens": ["Er", "weckt", "sie", "mit", "sei\u00b7nem", "Licht'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ihm entschleiert sie freundlich", "tokens": ["Und", "ihm", "ent\u00b7schlei\u00b7ert", "sie", "freund\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ADJD"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr frommes Blumengesicht.", "tokens": ["Ihr", "from\u00b7mes", "Blu\u00b7men\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Sie bl\u00fcht und gl\u00fcht und leuchtet,", "tokens": ["Sie", "bl\u00fcht", "und", "gl\u00fcht", "und", "leuch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und starret stumm in die H\u00f6h';", "tokens": ["Und", "star\u00b7ret", "stumm", "in", "die", "H\u00f6h'", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie duftet und weinet und zittert", "tokens": ["Sie", "duf\u00b7tet", "und", "wei\u00b7net", "und", "zit\u00b7tert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Vor Liebe und Liebesweh'.", "tokens": ["Vor", "Lie\u00b7be", "und", "Lie\u00b7bes\u00b7weh'", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}