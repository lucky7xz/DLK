"text": "O lieb M\u00e4del, wie schlecht bist du!", "tokens": ["O", "lieb", "M\u00e4\u00b7del", ",", "wie", "schlecht", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Ich gieng nicht zu dem Feste", "tokens": ["Ich", "gieng", "nicht", "zu", "dem", "Fes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Trank nicht den edlen Wein", "tokens": ["Trank", "nicht", "den", "ed\u00b7len", "Wein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ertrug den Spott der G\u00e4ste", "tokens": ["Er\u00b7trug", "den", "Spott", "der", "G\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Um nur bei dir zu sein.", "tokens": ["Um", "nur", "bei", "dir", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ADV", "APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "O lieb M\u00e4del, wie schlecht bist du!", "tokens": ["O", "lieb", "M\u00e4\u00b7del", ",", "wie", "schlecht", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.6": {"line.1": {"text": "Bin zitternd zu dir gekommen", "tokens": ["Bin", "zit\u00b7ternd", "zu", "dir", "ge\u00b7kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "PPER", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Als w\u00e4rst du ein Jungfr\u00e4ulein,", "tokens": ["Als", "w\u00e4rst", "du", "ein", "Jung\u00b7fr\u00e4u\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Hab' dich in Arm genommen", "tokens": ["Hab'", "dich", "in", "Arm", "ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PRF", "APPR", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Als w\u00e4rst du mein allein, allein.", "tokens": ["Als", "w\u00e4rst", "du", "mein", "al\u00b7lein", ",", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "PPOSAT", "ADV", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O lieb M\u00e4del, wie schlecht bist du!", "tokens": ["O", "lieb", "M\u00e4\u00b7del", ",", "wie", "schlecht", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.7": {"line.1": {"text": "Wie schlecht du sonst gewesen", "tokens": ["Wie", "schlecht", "du", "sonst", "ge\u00b7we\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VAPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verga\u00df ich liebend in mir", "tokens": ["Ver\u00b7ga\u00df", "ich", "lie\u00b7bend", "in", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und all dein elendes Wesen", "tokens": ["Und", "all", "dein", "e\u00b7len\u00b7des", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vergab ich herzlich dir ach dir,", "tokens": ["Ver\u00b7gab", "ich", "herz\u00b7lich", "dir", "ach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PPER", "ADV", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O lieb M\u00e4del, wie schlecht bist du!", "tokens": ["O", "lieb", "M\u00e4\u00b7del", ",", "wie", "schlecht", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.8": {"line.1": {"text": "Als du mir nackt gegeben", "tokens": ["Als", "du", "mir", "nackt", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zur Nacht den k\u00fchlen Trank", "tokens": ["Zur", "Nacht", "den", "k\u00fch\u00b7len", "Trank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Vergiftetest du mein Leben,", "tokens": ["Ver\u00b7gif\u00b7te\u00b7test", "du", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Da war meine Seele so krank so krank,", "tokens": ["Da", "war", "mei\u00b7ne", "See\u00b7le", "so", "krank", "so", "krank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "