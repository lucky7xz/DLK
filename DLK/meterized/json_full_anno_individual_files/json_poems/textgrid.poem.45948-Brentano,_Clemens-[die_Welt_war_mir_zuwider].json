{"textgrid.poem.45948": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[die Welt war mir zuwider]", "genre": "verse", "period": "N.A.", "pub_year": 1812, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Welt war mir zuwider", "tokens": ["Die", "Welt", "war", "mir", "zu\u00b7wi\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Berge lagen auf mir", "tokens": ["Die", "Ber\u00b7ge", "la\u00b7gen", "auf", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Der Himmel war mir zu nieder", "tokens": ["Der", "Him\u00b7mel", "war", "mir", "zu", "nie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "PTKZU", "PTKVZ"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ich sehnte mich nach dir, nach dir,", "tokens": ["Ich", "sehn\u00b7te", "mich", "nach", "dir", ",", "nach", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "$,", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O lieb M\u00e4del, wie schlecht bist du!", "tokens": ["O", "lieb", "M\u00e4\u00b7del", ",", "wie", "schlecht", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Ich trieb wohl durch die Gassen", "tokens": ["Ich", "trieb", "wohl", "durch", "die", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zwei lange Jahre mich", "tokens": ["Zwei", "lan\u00b7ge", "Jah\u00b7re", "mich"], "token_info": ["word", "word", "word", "word"], "pos": ["CARD", "ADJA", "NN", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "An den Ecken mu\u00dft' ich passen", "tokens": ["An", "den", "E\u00b7cken", "mu\u00dft'", "ich", "pas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und harren nur auf dich, auf dich.", "tokens": ["Und", "har\u00b7ren", "nur", "auf", "dich", ",", "auf", "dich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPER", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O lieb M\u00e4del, wie schlecht bist du!", "tokens": ["O", "lieb", "M\u00e4\u00b7del", ",", "wie", "schlecht", "bist", "du", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "NN", "$,", "PWAV", "ADJD", "VAFIN", "PPER", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Und alle Liebeswunden", "tokens": ["Und", "al\u00b7le", "Lie\u00b7bes\u00b7wun\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die brachen auf in mir", "tokens": ["Die", "bra\u00b7chen", "auf", "in", "mir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als ich dich endlich gefunden", "tokens": ["Als", "ich", "dich", "end\u00b7lich", "