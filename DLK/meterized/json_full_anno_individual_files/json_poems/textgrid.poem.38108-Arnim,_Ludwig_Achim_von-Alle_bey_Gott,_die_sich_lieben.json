{"textgrid.poem.38108": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Alle bey Gott, die sich lieben", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es hatt' ein Herr ein T\u00f6chterlein,", "tokens": ["Es", "hatt'", "ein", "Herr", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Nahmen hie\u00df es Annelein,", "tokens": ["Mit", "Nah\u00b7men", "hie\u00df", "es", "An\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herrn wollt man ihr geben,", "tokens": ["Ein", "Herrn", "wollt", "man", "ihr", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frau Markgr\u00e4fin sollte es werden.", "tokens": ["Frau", "Mark\u00b7gr\u00e4\u00b7fin", "soll\u00b7te", "es", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach Vater ich nehm noch keinen Mann,", "tokens": ["Ach", "Va\u00b7ter", "ich", "nehm", "noch", "kei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin nicht \u00e4lter dann elf Jahr,", "tokens": ["Ich", "bin", "nicht", "\u00e4l\u00b7ter", "dann", "elf", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein Kind und sterb f\u00fcrwahr.", "tokens": ["Ich", "bin", "ein", "Kind", "und", "sterb", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es stund nicht an ein halbes Jahr,", "tokens": ["Es", "stund", "nicht", "an", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fr\u00e4ulein mit dem Kinde ging,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "mit", "dem", "Kin\u00b7de", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bat ihren Herrn im Guten,", "tokens": ["Sie", "bat", "ih\u00b7ren", "Herrn", "im", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er sollt jezt holen ihre Mutter.", "tokens": ["Er", "sollt", "jezt", "ho\u00b7len", "ih\u00b7re", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als er in den finstern Wald einritt,", "tokens": ["Und", "als", "er", "in", "den", "fins\u00b7tern", "Wald", "ein\u00b7ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihm seine Schwieger entgegen schritt:", "tokens": ["Ihm", "sei\u00b7ne", "Schwie\u00b7ger", "ent\u00b7ge\u00b7gen", "schritt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwo habt ihr dann euer Fr\u00e4ulein?\u00ab", "tokens": ["\u00bb", "wo", "habt", "ihr", "dann", "eu\u00b7er", "Fr\u00e4u\u00b7lein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Mein Fr\u00e4ulein liegt in gro\u00dfer Noth,", "tokens": ["Mein", "Fr\u00e4u\u00b7lein", "liegt", "in", "gro\u00b7\u00dfer", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrcht, wenn wir kommen, sei sie schon todt;", "tokens": ["F\u00fcrcht", ",", "wenn", "wir", "kom\u00b7men", ",", "sei", "sie", "schon", "todt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mein Fr\u00e4ulein liegt in Ehren", "tokens": ["Mein", "Fr\u00e4u\u00b7lein", "liegt", "in", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Kind soll sie geb\u00e4hren.", "tokens": ["Ein", "Kind", "soll", "sie", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und als er \u00fcber die Heide ritt,", "tokens": ["Und", "als", "er", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Hirtlein h\u00f6rt er pfeifen,", "tokens": ["Ein", "Hirt\u00b7lein", "h\u00f6rt", "er", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gl\u00f6cklein h\u00f6rt er l\u00e4uten.", "tokens": ["Ein", "Gl\u00f6c\u00b7klein", "h\u00f6rt", "er", "l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ei Hirtlein, liebes Hirtlein mein,", "tokens": ["Ei", "Hirt\u00b7lein", ",", "lie\u00b7bes", "Hirt\u00b7lein", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was l\u00e4utet man im Kl\u00f6sterlein,", "tokens": ["Was", "l\u00e4u\u00b7tet", "man", "im", "Kl\u00f6s\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4utet man um die Vesperzeit,", "tokens": ["L\u00e4u\u00b7tet", "man", "um", "die", "Ves\u00b7per\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Oder l\u00e4utet man um eine Todten Leich?", "tokens": ["O\u00b7der", "l\u00e4u\u00b7tet", "man", "um", "ei\u00b7ne", "Tod\u00b7ten", "Leich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "Man l\u00e4utet um eine Todten Leich!", "tokens": ["Man", "l\u00e4u\u00b7tet", "um", "ei\u00b7ne", "Tod\u00b7ten", "Leich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist dem jungen Markgrafen", "tokens": ["Es", "ist", "dem", "jun\u00b7gen", "Mark\u00b7gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sein Fr\u00e4ulein mit dem Kind entschlafen.", "tokens": ["Sein", "Fr\u00e4u\u00b7lein", "mit", "dem", "Kind", "ent\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Und als er zu dem Thor einritt,", "tokens": ["Und", "als", "er", "zu", "dem", "Thor", "ein\u00b7ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und als er in den Hof einritt,", "tokens": ["Und", "als", "er", "in", "den", "Hof", "ein\u00b7ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drei Lichter sieht er brennen,", "tokens": ["Drei", "Lich\u00b7ter", "sieht", "er", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drei Sch\u00fcler Knaben singen.", "tokens": ["Drei", "Sch\u00fc\u00b7ler", "Kna\u00b7ben", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und als er in die Stube kam", "tokens": ["Und", "als", "er", "in", "die", "Stu\u00b7be", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Fr\u00e4ulein in der Bahre lag,", "tokens": ["Sein", "Fr\u00e4u\u00b7lein", "in", "der", "Bah\u00b7re", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kindlein in ihren Armen lag.", "tokens": ["Das", "Kin\u00b7dlein", "in", "ih\u00b7ren", "Ar\u00b7men", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Er k\u00fc\u00dft sie an ihren bleichen Mund,", "tokens": ["Er", "k\u00fc\u00dft", "sie", "an", "ih\u00b7ren", "blei\u00b7chen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jezt bist du todt und nimmer gesund.", "tokens": ["Jezt", "bist", "du", "todt", "und", "nim\u00b7mer", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er k\u00fc\u00dft sein Kindlein an ihrem Arm,", "tokens": ["Er", "k\u00fc\u00dft", "sein", "Kin\u00b7dlein", "an", "ih\u00b7rem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Gott erbarm, das Gott erbarm.", "tokens": ["Das", "Gott", "er\u00b7barm", ",", "das", "Gott", "er\u00b7barm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Die Mutter die war ganz allein,", "tokens": ["Die", "Mut\u00b7ter", "die", "war", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sezt sich an ein harten Stein,", "tokens": ["Die", "sezt", "sich", "an", "ein", "har\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Leid brach ihr das Herz entzwei.", "tokens": ["Vor", "Leid", "brach", "ihr", "das", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Da zog er aus sein glitzerich Schwerd,", "tokens": ["Da", "zog", "er", "aus", "sein", "glit\u00b7ze\u00b7rich", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und stachs sich selber durch sein Herz:", "tokens": ["Und", "stachs", "sich", "sel\u00b7ber", "durch", "sein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sprach, ists nicht ein Straf von Gott,", "tokens": ["Er", "sprach", ",", "ists", "nicht", "ein", "Straf", "von", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vier Leichen in eines F\u00fcrsten Schlo\u00df.", "tokens": ["Vier", "Lei\u00b7chen", "in", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Es stand nicht l\u00e4nger als drei Tag,", "tokens": ["Es", "stand", "nicht", "l\u00e4n\u00b7ger", "als", "drei", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drei Lilien wuchsen auf des Fr\u00e4uleins Grab,", "tokens": ["Drei", "Li\u00b7li\u00b7en", "wuch\u00b7sen", "auf", "des", "Fr\u00e4u\u00b7leins", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die erste wei\u00df, die andre schwarz.", "tokens": ["Die", "ers\u00b7te", "wei\u00df", ",", "die", "and\u00b7re", "schwarz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PRELS", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Die schwarz dem kleinen Kindlein war,", "tokens": ["Die", "schwarz", "dem", "klei\u00b7nen", "Kin\u00b7dlein", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil es noch nicht getaufet war;", "tokens": ["Weil", "es", "noch", "nicht", "ge\u00b7tau\u00b7fet", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf der dritten war wohl geschrieben:", "tokens": ["Auf", "der", "drit\u00b7ten", "war", "wohl", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VAFIN", "ADV", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie sind all bei Gott, die sich lieben.", "tokens": ["Sie", "sind", "all", "bei", "Gott", ",", "die", "sich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.16": {"line.1": {"text": "Den Herrn, den gr\u00e4bt man wieder aus,", "tokens": ["Den", "Herrn", ",", "den", "gr\u00e4bt", "man", "wie\u00b7der", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt ihn zum Annelein ins Gotteshaus,", "tokens": ["Legt", "ihn", "zum", "An\u00b7nel\u00b7ein", "ins", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da liegen vier Leichen zusammen,", "tokens": ["Da", "lie\u00b7gen", "vier", "Lei\u00b7chen", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Gott erbarme. Amen!", "tokens": ["Das", "Gott", "er\u00b7bar\u00b7me", ".", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Es hatt' ein Herr ein T\u00f6chterlein,", "tokens": ["Es", "hatt'", "ein", "Herr", "ein", "T\u00f6ch\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Nahmen hie\u00df es Annelein,", "tokens": ["Mit", "Nah\u00b7men", "hie\u00df", "es", "An\u00b7nel\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Herrn wollt man ihr geben,", "tokens": ["Ein", "Herrn", "wollt", "man", "ihr", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Frau Markgr\u00e4fin sollte es werden.", "tokens": ["Frau", "Mark\u00b7gr\u00e4\u00b7fin", "soll\u00b7te", "es", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.18": {"line.1": {"text": "Ach Vater ich nehm noch keinen Mann,", "tokens": ["Ach", "Va\u00b7ter", "ich", "nehm", "noch", "kei\u00b7nen", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "PPER", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich bin nicht \u00e4lter dann elf Jahr,", "tokens": ["Ich", "bin", "nicht", "\u00e4l\u00b7ter", "dann", "elf", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich bin ein Kind und sterb f\u00fcrwahr.", "tokens": ["Ich", "bin", "ein", "Kind", "und", "sterb", "f\u00fcr\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "KON", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.19": {"line.1": {"text": "Es stund nicht an ein halbes Jahr,", "tokens": ["Es", "stund", "nicht", "an", "ein", "hal\u00b7bes", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Fr\u00e4ulein mit dem Kinde ging,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "mit", "dem", "Kin\u00b7de", "ging", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie bat ihren Herrn im Guten,", "tokens": ["Sie", "bat", "ih\u00b7ren", "Herrn", "im", "Gu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Er sollt jezt holen ihre Mutter.", "tokens": ["Er", "sollt", "jezt", "ho\u00b7len", "ih\u00b7re", "Mut\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.20": {"line.1": {"text": "Und als er in den finstern Wald einritt,", "tokens": ["Und", "als", "er", "in", "den", "fins\u00b7tern", "Wald", "ein\u00b7ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihm seine Schwieger entgegen schritt:", "tokens": ["Ihm", "sei\u00b7ne", "Schwie\u00b7ger", "ent\u00b7ge\u00b7gen", "schritt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "PTKVZ", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "\u00bbwo habt ihr dann euer Fr\u00e4ulein?\u00ab", "tokens": ["\u00bb", "wo", "habt", "ihr", "dann", "eu\u00b7er", "Fr\u00e4u\u00b7lein", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.21": {"line.1": {"text": "Mein Fr\u00e4ulein liegt in gro\u00dfer Noth,", "tokens": ["Mein", "Fr\u00e4u\u00b7lein", "liegt", "in", "gro\u00b7\u00dfer", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcrcht, wenn wir kommen, sei sie schon todt;", "tokens": ["F\u00fcrcht", ",", "wenn", "wir", "kom\u00b7men", ",", "sei", "sie", "schon", "todt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF", "$,", "VAFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Mein Fr\u00e4ulein liegt in Ehren", "tokens": ["Mein", "Fr\u00e4u\u00b7lein", "liegt", "in", "Eh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ein Kind soll sie geb\u00e4hren.", "tokens": ["Ein", "Kind", "soll", "sie", "ge\u00b7b\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "Und als er \u00fcber die Heide ritt,", "tokens": ["Und", "als", "er", "\u00fc\u00b7ber", "die", "Hei\u00b7de", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein Hirtlein h\u00f6rt er pfeifen,", "tokens": ["Ein", "Hirt\u00b7lein", "h\u00f6rt", "er", "pfei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Gl\u00f6cklein h\u00f6rt er l\u00e4uten.", "tokens": ["Ein", "Gl\u00f6c\u00b7klein", "h\u00f6rt", "er", "l\u00e4u\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Ei Hirtlein, liebes Hirtlein mein,", "tokens": ["Ei", "Hirt\u00b7lein", ",", "lie\u00b7bes", "Hirt\u00b7lein", "mein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADJA", "NN", "PPOSAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was l\u00e4utet man im Kl\u00f6sterlein,", "tokens": ["Was", "l\u00e4u\u00b7tet", "man", "im", "Kl\u00f6s\u00b7ter\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "L\u00e4utet man um die Vesperzeit,", "tokens": ["L\u00e4u\u00b7tet", "man", "um", "die", "Ves\u00b7per\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Oder l\u00e4utet man um eine Todten Leich?", "tokens": ["O\u00b7der", "l\u00e4u\u00b7tet", "man", "um", "ei\u00b7ne", "Tod\u00b7ten", "Leich", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "APPR", "ART", "NN", "ADJD", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.24": {"line.1": {"text": "Man l\u00e4utet um eine Todten Leich!", "tokens": ["Man", "l\u00e4u\u00b7tet", "um", "ei\u00b7ne", "Tod\u00b7ten", "Leich", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Es ist dem jungen Markgrafen", "tokens": ["Es", "ist", "dem", "jun\u00b7gen", "Mark\u00b7gra\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Sein Fr\u00e4ulein mit dem Kind entschlafen.", "tokens": ["Sein", "Fr\u00e4u\u00b7lein", "mit", "dem", "Kind", "ent\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.25": {"line.1": {"text": "Und als er zu dem Thor einritt,", "tokens": ["Und", "als", "er", "zu", "dem", "Thor", "ein\u00b7ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und als er in den Hof einritt,", "tokens": ["Und", "als", "er", "in", "den", "Hof", "ein\u00b7ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Drei Lichter sieht er brennen,", "tokens": ["Drei", "Lich\u00b7ter", "sieht", "er", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Drei Sch\u00fcler Knaben singen.", "tokens": ["Drei", "Sch\u00fc\u00b7ler", "Kna\u00b7ben", "sin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Und als er in die Stube kam", "tokens": ["Und", "als", "er", "in", "die", "Stu\u00b7be", "kam"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Fr\u00e4ulein in der Bahre lag,", "tokens": ["Sein", "Fr\u00e4u\u00b7lein", "in", "der", "Bah\u00b7re", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Kindlein in ihren Armen lag.", "tokens": ["Das", "Kin\u00b7dlein", "in", "ih\u00b7ren", "Ar\u00b7men", "lag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.27": {"line.1": {"text": "Er k\u00fc\u00dft sie an ihren bleichen Mund,", "tokens": ["Er", "k\u00fc\u00dft", "sie", "an", "ih\u00b7ren", "blei\u00b7chen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Jezt bist du todt und nimmer gesund.", "tokens": ["Jezt", "bist", "du", "todt", "und", "nim\u00b7mer", "ge\u00b7sund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Er k\u00fc\u00dft sein Kindlein an ihrem Arm,", "tokens": ["Er", "k\u00fc\u00dft", "sein", "Kin\u00b7dlein", "an", "ih\u00b7rem", "Arm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Gott erbarm, das Gott erbarm.", "tokens": ["Das", "Gott", "er\u00b7barm", ",", "das", "Gott", "er\u00b7barm", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.28": {"line.1": {"text": "Die Mutter die war ganz allein,", "tokens": ["Die", "Mut\u00b7ter", "die", "war", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VAFIN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die sezt sich an ein harten Stein,", "tokens": ["Die", "sezt", "sich", "an", "ein", "har\u00b7ten", "Stein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor Leid brach ihr das Herz entzwei.", "tokens": ["Vor", "Leid", "brach", "ihr", "das", "Herz", "ent\u00b7zwei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.29": {"line.1": {"text": "Da zog er aus sein glitzerich Schwerd,", "tokens": ["Da", "zog", "er", "aus", "sein", "glit\u00b7ze\u00b7rich", "Schwerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Und stachs sich selber durch sein Herz:", "tokens": ["Und", "stachs", "sich", "sel\u00b7ber", "durch", "sein", "Herz", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er sprach, ists nicht ein Straf von Gott,", "tokens": ["Er", "sprach", ",", "ists", "nicht", "ein", "Straf", "von", "Gott", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vier Leichen in eines F\u00fcrsten Schlo\u00df.", "tokens": ["Vier", "Lei\u00b7chen", "in", "ei\u00b7nes", "F\u00fcrs\u00b7ten", "Schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "ART", "NN", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.30": {"line.1": {"text": "Es stand nicht l\u00e4nger als drei Tag,", "tokens": ["Es", "stand", "nicht", "l\u00e4n\u00b7ger", "als", "drei", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJD", "KOKOM", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Drei Lilien wuchsen auf des Fr\u00e4uleins Grab,", "tokens": ["Drei", "Li\u00b7li\u00b7en", "wuch\u00b7sen", "auf", "des", "Fr\u00e4u\u00b7leins", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die erste wei\u00df, die andre schwarz.", "tokens": ["Die", "ers\u00b7te", "wei\u00df", ",", "die", "and\u00b7re", "schwarz", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "$,", "PRELS", "PIS", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.31": {"line.1": {"text": "Die schwarz dem kleinen Kindlein war,", "tokens": ["Die", "schwarz", "dem", "klei\u00b7nen", "Kin\u00b7dlein", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil es noch nicht getaufet war;", "tokens": ["Weil", "es", "noch", "nicht", "ge\u00b7tau\u00b7fet", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf der dritten war wohl geschrieben:", "tokens": ["Auf", "der", "drit\u00b7ten", "war", "wohl", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "VAFIN", "ADV", "VVPP", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Sie sind all bei Gott, die sich lieben.", "tokens": ["Sie", "sind", "all", "bei", "Gott", ",", "die", "sich", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "APPR", "NN", "$,", "PRELS", "PRF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.32": {"line.1": {"text": "Den Herrn, den gr\u00e4bt man wieder aus,", "tokens": ["Den", "Herrn", ",", "den", "gr\u00e4bt", "man", "wie\u00b7der", "aus", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "PIS", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Legt ihn zum Annelein ins Gotteshaus,", "tokens": ["Legt", "ihn", "zum", "An\u00b7nel\u00b7ein", "ins", "Got\u00b7tes\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da liegen vier Leichen zusammen,", "tokens": ["Da", "lie\u00b7gen", "vier", "Lei\u00b7chen", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "CARD", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Das Gott erbarme. Amen!", "tokens": ["Das", "Gott", "er\u00b7bar\u00b7me", ".", "A\u00b7men", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}