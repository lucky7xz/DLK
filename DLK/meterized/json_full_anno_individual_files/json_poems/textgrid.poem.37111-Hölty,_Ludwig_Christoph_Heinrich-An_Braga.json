{"textgrid.poem.37111": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Braga", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm', du Geber des Sangs, Apollens Besieger o Braga,", "tokens": ["Komm'", ",", "du", "Ge\u00b7ber", "des", "Sangs", ",", "A\u00b7pol\u00b7lens", "Be\u00b7sie\u00b7ger", "o", "Bra\u00b7ga", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "NN", "ART", "NN", "$,", "NE", "NN", "FM", "NE", "$,"], "meter": "+-+--+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bey mir warten dein Braten und Fisch,", "tokens": ["Bey", "mir", "war\u00b7ten", "dein", "Bra\u00b7ten", "und", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Komm', sonst hohl' dich der Teufel, Papa der Barden und Aga,", "tokens": ["Komm'", ",", "sonst", "hohl'", "dich", "der", "Teu\u00b7fel", ",", "Pa\u00b7pa", "der", "Bar\u00b7den", "und", "A\u00b7ga", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,", "NN", "ART", "NN", "KON", "NE", "$,"], "meter": "-+-+--+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Komm' an meinen besch\u00fc\u00dfelten Tisch!", "tokens": ["Komm'", "an", "mei\u00b7nen", "be\u00b7sch\u00fc\u00b7\u00dfel\u00b7ten", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+--++-+", "measure": "trochaic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Dein Antece\u00dfor, Apoll, sang von Cyther' und Adonis,", "tokens": ["Dein", "An\u00b7te\u00b7ce\u00b7\u00dfor", ",", "A\u00b7poll", ",", "sang", "von", "Cy\u00b7ther'", "und", "A\u00b7do\u00b7nis", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "NE", "$,", "VVFIN", "APPR", "NE", "KON", "NE", "$,"], "meter": "-+-+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Man h\u00f6rt's, und mu\u00dfte sich brechen und speyn,", "tokens": ["Man", "h\u00f6rt's", ",", "und", "mu\u00df\u00b7te", "sich", "bre\u00b7chen", "und", "speyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KON", "VMFIN", "PRF", "VVINF", "KON", "ADJD", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sein Gebr\u00fclle war voll von ", "tokens": ["Sein", "Ge\u00b7br\u00fcl\u00b7le", "war", "voll", "von"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ein Gewebe von K\u00fc\u00dfen und Freyn.", "tokens": ["Ein", "Ge\u00b7we\u00b7be", "von", "K\u00fc\u00b7\u00dfen", "und", "Freyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Setz' dem tollen Patron auf beyde Augen die Daumen,", "tokens": ["Setz'", "dem", "tol\u00b7len", "Pat\u00b7ron", "auf", "bey\u00b7de", "Au\u00b7gen", "die", "Dau\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+--+-+--+-", "meas