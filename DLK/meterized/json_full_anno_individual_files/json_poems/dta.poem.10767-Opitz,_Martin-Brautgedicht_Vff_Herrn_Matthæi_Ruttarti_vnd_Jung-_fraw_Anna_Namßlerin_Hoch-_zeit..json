{"dta.poem.10767": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "Brautgedicht/  \n Vff Herrn   Matth\u00e6i Ruttarti   vnd Jung-  \n fraw Anna Nam\u00dflerin Hoch-  \n zeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1624", "urn": "urn:nbn:de:kobv:b4-200905197859", "language": ["de:0.99"], "booktitle": "Opitz, Martin: Teutsche P\u00f6emata und: Aristarchvs Wieder die verachtung Teutscher Sprach. Stra\u00dfburg, 1624."}, "poem": {"stanza.1": {"line.1": {"text": "Als Juppiter die Welt hat g\u00e4ntzlich au\u00dfgemachet/", "tokens": ["Als", "Jup\u00b7pi\u00b7ter", "die", "Welt", "hat", "g\u00e4ntz\u00b7lich", "au\u00df\u00b7ge\u00b7ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "VAFIN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd auff dem Erdenkrey\u00df schon alles gr\u00fcnt vnd lachet/", "tokens": ["Vnd", "auff", "dem", "Er\u00b7den\u00b7krey\u00df", "schon", "al\u00b7les", "gr\u00fcnt", "vnd", "la\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "PIS", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wand er sich dreymal vmb/ vnd schawet hin vnd her/", "tokens": ["Wand", "er", "sich", "drey\u00b7mal", "vmb", "/", "vnd", "scha\u00b7wet", "hin", "vnd", "her", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "$(", "KON", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Ob in dem grossen Hau\u00df irgendt ein mangel we", "tokens": ["Ob", "in", "dem", "gros\u00b7sen", "Hau\u00df", "ir\u00b7gendt", 