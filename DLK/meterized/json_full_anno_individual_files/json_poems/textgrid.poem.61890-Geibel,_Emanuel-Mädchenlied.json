{"textgrid.poem.61890": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4dchenlied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Blumen wollt' ich warten,", "tokens": ["Der", "Blu\u00b7men", "wollt'", "ich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vergessend, was mein Herz erfuhr,", "tokens": ["Ver\u00b7ges\u00b7send", ",", "was", "mein", "Herz", "er\u00b7fuhr", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch jede Blum' im Garten", "tokens": ["Doch", "je\u00b7de", "Blum'", "im", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spricht mir von Liebe nur.", "tokens": ["Spricht", "mir", "von", "Lie\u00b7be", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.2": {"line.1": {"text": "Die Rose will vergluten,", "tokens": ["Die", "Ro\u00b7se", "will", "ver\u00b7glu\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Lilie ward vor Sehnsucht bleich,", "tokens": ["Die", "Li\u00b7lie", "ward", "vor", "Sehn\u00b7sucht", "bleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und die Granaten bluten", "tokens": ["Und", "die", "Gra\u00b7na\u00b7ten", "blu\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Zerspaltnen Herzen gleich.", "tokens": ["Zer\u00b7spalt\u00b7nen", "Her\u00b7zen", "gleich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es weint aus hundert Sprossen", "tokens": ["Es", "weint", "aus", "hun\u00b7dert", "Spros\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Rebe, die zum Stock sich zweigt,", "tokens": ["Die", "Re\u00b7be", ",", "die", "zum", "Stock", "sich", "zweigt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Tr\u00e4nen, reich ergossen,", "tokens": ["Und", "Tr\u00e4\u00b7nen", ",", "reich", "er\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gestehn, was sie verschweigt.", "tokens": ["Ge\u00b7stehn", ",", "was", "sie", "ver\u00b7schweigt", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und was ich nie zu sagen,", "tokens": ["Und", "was", "ich", "nie", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos":