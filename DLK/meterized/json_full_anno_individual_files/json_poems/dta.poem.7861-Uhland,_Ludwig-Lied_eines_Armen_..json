{"dta.poem.7861": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines Armen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bin so gar ein armer Mann", "tokens": ["Ich", "bin", "so", "gar", "ein", "ar\u00b7mer", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und gehe ganz allein.", "tokens": ["Und", "ge\u00b7he", "ganz", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich m\u00f6chte wohl nur einmal noch", "tokens": ["Ich", "m\u00f6ch\u00b7te", "wohl", "nur", "ein\u00b7mal", "noch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Recht frohen Muthes seyn.", "tokens": ["Recht", "fro\u00b7hen", "Mu\u00b7thes", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "In meiner lieben Eltern Haus", "tokens": ["In", "mei\u00b7ner", "lie\u00b7ben", "El\u00b7tern", "Haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "War ich ein frohes Kind,", "tokens": ["War", "ich", "ein", "fro\u00b7hes", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der bittre Kummer ist mein Theil", "tokens": ["Der", "bitt\u00b7re", "Kum\u00b7mer", "ist", "mein", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Seit sie begraben sind.", "tokens": ["Seit", "sie", "be\u00b7gra\u00b7ben", "sind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Reichen G\u00e4rten seh\u2019 ich bl\u00fchn,", "tokens": ["Der", "Rei\u00b7chen", "G\u00e4r\u00b7ten", "seh'", "ich", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich seh\u2019 die goldne Saat:", "tokens": ["Ich", "seh'", "die", "gold\u00b7ne", "Saat", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein ist der unfruchtbare Weg,", "tokens": ["Mein", "ist", "der", "un\u00b7frucht\u00b7ba\u00b7re", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.4": {"text": "Den Sorg\u2019 und M\u00fche trat.", "tokens": ["Den", "Sor\u00b7g'", "und", "M\u00fc\u00b7he", "trat", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Doch weil\u2019 ich gern mit stillem Weh", "tokens": ["Doch", "weil'", "ich", "gern", "mit", "stil\u00b7lem", "Weh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In froher Menschen Schwarm", "tokens": ["In", "fro\u00b7her", "Men\u00b7schen", "Schwarm"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00fcnsche Jedem guten Tag,", "tokens": ["Und", "w\u00fcn\u00b7sche", "Je\u00b7dem", "gu\u00b7ten", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So herzlich und so warm.", "tokens": ["So", "herz\u00b7lich", "und", "so", "warm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "O reicher Gott! du liessest doch", "tokens": ["O", "rei\u00b7cher", "Gott", "!", "du", "lies\u00b7sest", "doch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJD", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht ganz mich freudenleer:", "tokens": ["Nicht", "ganz", "mich", "freu\u00b7den\u00b7leer", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "PPER", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein s\u00fc\u00dfer Trost f\u00fcr alle Welt", "tokens": ["Ein", "s\u00fc\u00b7\u00dfer", "Trost", "f\u00fcr", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ergie\u00dft sich himmelher.", "tokens": ["Er\u00b7gie\u00dft", "sich", "him\u00b7mel\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Noch steigt in jedem D\u00f6rflein ja", "tokens": ["Noch", "steigt", "in", "je\u00b7dem", "D\u00f6r\u00b7flein", "ja"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein heilig Haus empor;", "tokens": ["Dein", "hei\u00b7lig", "Haus", "em\u00b7por", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Orgel und der Chorgesang", "tokens": ["Die", "Or\u00b7gel", "und", "der", "Chor\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ert\u00f6net jedem Ohr.", "tokens": ["Er\u00b7t\u00f6\u00b7net", "je\u00b7dem", "Ohr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Noch leuchtet Sonne, Mond und Stern", "tokens": ["Noch", "leuch\u00b7tet", "Son\u00b7ne", ",", "Mond", "und", "Stern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So liebevoll auch mir,", "tokens": ["So", "lie\u00b7be\u00b7voll", "auch", "mir", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wann die Abendglocke hallt,", "tokens": ["Und", "wann", "die", "A\u00b7bend\u00b7glo\u00b7cke", "hallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da red\u2019 ich, Herr, mit dir.", "tokens": ["Da", "red'", "ich", ",", "Herr", ",", "mit", "dir", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Einst \u00f6ffnet jedem Guten sich", "tokens": ["Einst", "\u00f6ff\u00b7net", "je\u00b7dem", "Gu\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dein hoher Freudensaal,", "tokens": ["Dein", "ho\u00b7her", "Freu\u00b7den\u00b7saal", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann komm\u2019 auch ich im Feierkleid", "tokens": ["Dann", "komm'", "auch", "ich", "im", "Fei\u00b7er\u00b7kleid"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und setze mich an\u2019s Mahl.", "tokens": ["Und", "set\u00b7ze", "mich", "an's", "Mahl", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}