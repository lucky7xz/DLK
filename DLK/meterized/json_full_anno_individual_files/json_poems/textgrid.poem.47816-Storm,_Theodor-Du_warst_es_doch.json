{"textgrid.poem.47816": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Du warst es doch", "genre": "verse", "period": "N.A.", "pub_year": 1858, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In buntem Zug zum Walde ging's hinaus;", "tokens": ["In", "bun\u00b7tem", "Zug", "zum", "Wal\u00b7de", "ging's", "hin\u00b7aus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du bei den Kindern bliebst allein zu Haus.", "tokens": ["Du", "bei", "den", "Kin\u00b7dern", "bliebst", "al\u00b7lein", "zu", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und drau\u00dfen haben wir getanzt, gelacht,", "tokens": ["Und", "drau\u00b7\u00dfen", "ha\u00b7ben", "wir", "ge\u00b7tanzt", ",", "ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und kaum, so war mir, hatt ich dein gedacht. \u2013", "tokens": ["Und", "kaum", ",", "so", "war", "mir", ",", "hatt", "ich", "dein", "ge\u00b7dacht", ".", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$,", "ADV", "VAFIN", "PPER", "$,", "VAFIN", "PPER", "PPOSAT", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nun kommt der Abend, und die Zeit beginnt,", "tokens": ["Nun", "kommt", "der", "A\u00b7bend", ",", "und", "die", "