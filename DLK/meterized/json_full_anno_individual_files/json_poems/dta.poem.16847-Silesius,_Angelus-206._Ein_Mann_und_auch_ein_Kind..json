{"dta.poem.16847": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "206.  Ein Mann und auch ein Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann ist nicht ein Kind: doch wisse da\u00df ein Man\u0304/", "tokens": ["Ein", "Mann", "ist", "nicht", "ein", "Kind", ":", "doch", "wis\u00b7se", "da\u00df", "ein", "Man\u0304", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "$.", "ADV", "VVFIN", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So du nur wilt in dir mein Kind/ wol Leben kann.", "tokens": ["So", "du", "nur", "wilt", "in", "dir", "mein", "Kind", "/", "wol", "Le\u00b7ben", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "VMFIN", "APPR", "PPER", "PPOSAT", "NN", "$(", "ADV", "NN", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}