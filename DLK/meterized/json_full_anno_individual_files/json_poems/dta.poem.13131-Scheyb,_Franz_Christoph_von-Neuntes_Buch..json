{"dta.poem.13131": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Neuntes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als die Gesundheit kaum von ihrem Helffer schwieg,", "tokens": ["Als", "die", "Ge\u00b7sund\u00b7heit", "kaum", "von", "ih\u00b7rem", "Helf\u00b7fer", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So war schon einerseits ein neuer W\u00f6rter-Krieg.", "tokens": ["So", "war", "schon", "ei\u00b7ner\u00b7seits", "ein", "neu\u00b7er", "W\u00f6r\u00b7ter\u00b7Krieg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201ewas wird ein solcher Rath, wo man gesund", "tokens": ["\u201e", "was", "wird", "ein", "sol\u00b7cher", "Rath", ",", "wo", "man", "ge\u00b7sund"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VAFIN", "ART", "PIAT", "NN", "$,", "PWAV", "PIS", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "ist, stiften? \u201d", "tokens": ["ist", ",", "stif\u00b7ten", "?", "\""], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["VAFIN", "$,", "VVFIN", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Sprach jemand. Eine dort: \u201egeht! leset sei-", "tokens": ["Sprach", "je\u00b7mand", ".", "Ei\u00b7ne", "dort", ":", "\u201e", "geht", "!", "le\u00b7set", "sei"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PIS", "$.", "ART", "ADV", "$.", "$(", "VVFIN", "$.", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "ne Schriften!", "tokens": ["ne", "Schrif\u00b7ten", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "Hier sezte man darauf: \u201eUnwissenheit veracht", "tokens": ["Hier", "sez\u00b7te", "man", "da\u00b7rauf", ":", "\u201e", "Un\u00b7wis\u00b7sen\u00b7heit", "ver\u00b7acht"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "PAV", "$.", "$(", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201eder Wissenschaften Werth, Glanz, Adel, Prei\u00df und Macht.", "tokens": ["\u201e", "der", "Wis\u00b7sen\u00b7schaf\u00b7ten", "Werth", ",", "Glanz", ",", "A\u00b7del", ",", "Prei\u00df", "und", "Macht", "."], "token_info": ["punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201ewas hei\u00dft der eitle Streit? Gesundheit zu bewahren", "tokens": ["\u201e", "was", "hei\u00dft", "der", "eit\u00b7le", "Streit", "?", "Ge\u00b7sund\u00b7heit", "zu", "be\u00b7wah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PWS", "VVFIN", "ART", "ADJA", "NN", "$.", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201everdient ein gr\u00f6ssers Lob, als sie von den Gefahren", "tokens": ["\u201e", "ver\u00b7dient", "ein", "gr\u00f6s\u00b7sers", "Lob", ",", "als", "sie", "von", "den", "Ge\u00b7fah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ART", "ADJA", "NN", "$,", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "\u201eder Kranckheit zu befreyn \u201e, sprach unser guter Rath,", "tokens": ["\u201e", "der", "Kran\u00b7ck\u00b7heit", "zu", "be\u00b7freyn", "\u201e", ",", "sprach", "un\u00b7ser", "gu\u00b7ter", "Rath", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "PTKZU", "VVINF", "$(", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "10\u201dMich wundert, da\u00df man die\u00df in acht genommen hat.", "tokens": ["\"", "Mich", "wun\u00b7dert", ",", "da\u00df", "man", "die\u00df", "in", "acht", "ge\u00b7nom\u00b7men", "hat", "."], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "KOUS", "PIS", "PDS", "APPR", "CARD", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "\u201ewohin zielt ihr damit? was steckt in dem Gespr\u00e4che?", "tokens": ["\u201e", "wo\u00b7hin", "zielt", "ihr", "da\u00b7mit", "?", "was", "steckt", "in", "dem", "Ge\u00b7spr\u00e4\u00b7che", "?"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PPER", "PAV", "$.", "PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "\u201ewer solche Fragen sezt, entdeckt der Sinnen Schw\u00e4che.", "tokens": ["\u201e", "wer", "sol\u00b7che", "Fra\u00b7gen", "sezt", ",", "ent\u00b7deckt", "der", "Sin\u00b7nen", "Schw\u00e4\u00b7che", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "PIAT", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}