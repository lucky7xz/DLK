{"dta.poem.14310": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnette .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Da sah ich drau\u00dfen auch das Land sich regen,", "tokens": ["Da", "sah", "ich", "drau\u00b7\u00dfen", "auch", "das", "Land", "sich", "re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PRF", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die W\u00e4lder rauschen und Aurora steigen \u2014", "tokens": ["Die", "W\u00e4l\u00b7der", "rau\u00b7schen", "und", "Au\u00b7ro\u00b7ra", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "KON", "NE", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die alten Zeiten all' weckt' mir die Liebe.", "tokens": ["Die", "al\u00b7ten", "Zei\u00b7ten", "all'", "weckt'", "mir", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}}}}