{"dta.poem.13006": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Florettens antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich will Leandern nur mit fl\u00fccht'ger feder schreiben,", "tokens": ["Ich", "will", "Le\u00b7an\u00b7dern", "nur", "mit", "fl\u00fccht'\u00b7ger", "fe\u00b7der", "schrei\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NE", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Berelisens hertz mir etwas fl\u00fcchtig scheint.", "tokens": ["Weil", "Be\u00b7re\u00b7li\u00b7sens", "hertz", "mir", "et\u00b7was", "fl\u00fcch\u00b7tig", "scheint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "PPER", "ADV", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich meyn\u2019, es war zu schwach, und pflegte bald zu gl\u00e4uben,", "tokens": ["Ich", "meyn'", ",", "es", "war", "zu", "schwach", ",", "und", "pfleg\u00b7te", "bald", "zu", "gl\u00e4u\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "$,", "KON", "VVFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und darum war sie auch vielleicht der untreu feind.", "tokens": ["Und", "da\u00b7rum", "war", "sie", "auch", "viel\u00b7leicht", "der", "un\u00b7treu", "feind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VAFIN", "PPER", "ADV", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "W\u00e4r aber ihre brust nicht allzu weich gewesen,", "tokens": ["W\u00e4r", "a\u00b7ber", "ih\u00b7re", "brust", "nicht", "all\u00b7zu", "weich", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "PTKA", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So d\u00f6rfften wir auch itzt nicht ihre klagen lesen.", "tokens": ["So", "d\u00f6rff\u00b7ten", "wir", "auch", "itzt", "nicht", "ih\u00b7re", "kla\u00b7gen", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "PTKNEG", "PPOSAT", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wohl der, so demnach wei\u00df, wie m\u00e4nner-kreide schreibt,", "tokens": ["Wohl", "der", ",", "so", "dem\u00b7nach", "wei\u00df", ",", "wie", "m\u00e4n\u00b7ner\u00b7krei\u00b7de", "schreibt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "ADV", "PAV", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und da\u00df der meisten hertz ein wetter-han verbleibt.", "tokens": ["Und", "da\u00df", "der", "meis\u00b7ten", "hertz", "ein", "wet\u00b7ter\u00b7\u00b7han", "ver\u00b7bleibt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}