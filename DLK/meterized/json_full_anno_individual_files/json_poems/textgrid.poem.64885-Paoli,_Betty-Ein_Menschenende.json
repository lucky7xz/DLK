{"textgrid.poem.64885": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "Ein Menschenende", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich sah dich, wie der wilde Lebensdrang", "tokens": ["Ich", "sah", "dich", ",", "wie", "der", "wil\u00b7de", "Le\u00b7bens\u00b7drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Ruf des Weh's von meinen Lippen rang,", "tokens": ["Den", "Ruf", "des", "Weh's", "von", "mei\u00b7nen", "Lip\u00b7pen", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie an des Schmerzaltares nassen Stufen", "tokens": ["Wie", "an", "des", "Schmer\u00b7zal\u00b7ta\u00b7res", "nas\u00b7sen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verblutend du die Gottheit angerufen.", "tokens": ["Ver\u00b7blu\u00b7tend", "du", "die", "Got\u00b7theit", "an\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie dann, als dir die Hilfe ward versagt,", "tokens": ["Wie", "dann", ",", "als", "dir", "die", "Hil\u00b7fe", "ward", "ver\u00b7sagt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "KOUS", "PPER", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Erd' und Himmel jammernd angeklagt;", "tokens": ["Du", "Erd'", "und", "Him\u00b7mel", "jam\u00b7mernd", "an\u00b7ge\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wie du kraftlos und verzweiflungstrunken", "tokens": ["Und", "wie", "du", "kraft\u00b7los", "und", "ver\u00b7zwei\u00b7flungs\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Auf deines Gl\u00fcckes Grabmahl hingesunken! \u2013", "tokens": ["Auf", "dei\u00b7nes", "Gl\u00fc\u00b7ckes", "Grab\u00b7mahl", "hin\u00b7ge\u00b7sun\u00b7ken", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Jetzt hat sich die\u00df ge\u00e4ndert wunderbar!", "tokens": ["Jetzt", "hat", "sich", "die\u00df", "ge\u00b7\u00e4n\u00b7dert", "wun\u00b7der\u00b7bar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "PDS", "VVPP", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus heit'rem Antlitz strahlt dein Aug' so klar,", "tokens": ["Aus", "heit'\u00b7rem", "Ant\u00b7litz", "strahlt", "dein", "Aug'", "so", "klar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da\u00df nur, wer selber schritt auf solchen Bahnen,", "tokens": ["Da\u00df", "nur", ",", "wer", "sel\u00b7ber", "schritt", "auf", "sol\u00b7chen", "Bah\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "PWS", "ADV", "VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der milden Ruhe bittren Grund kann ahnen.", "tokens": ["Der", "mil\u00b7den", "Ru\u00b7he", "bit\u00b7tren", "Grund", "kann", "ah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein sorglos L\u00e4cheln spielt um deinen Mund,", "tokens": ["Ein", "sorg\u00b7los", "L\u00e4\u00b7cheln", "spielt", "um", "dei\u00b7nen", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als w\u00e4r' die Seele kr\u00e4ftig und gesund;", "tokens": ["Als", "w\u00e4r'", "die", "See\u00b7le", "kr\u00e4f\u00b7tig", "und", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und also harmlos klinget deine Rede,", "tokens": ["Und", "al\u00b7so", "harm\u00b7los", "klin\u00b7get", "dei\u00b7ne", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als w\u00e4r' geendigt jede Lebensfehde.", "tokens": ["Als", "w\u00e4r'", "ge\u00b7en\u00b7digt", "je\u00b7de", "Le\u00b7bens\u00b7feh\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "VVPP", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So staunt die Welt in dir den Menschen an,", "tokens": ["So", "staunt", "die", "Welt", "in", "dir", "den", "Men\u00b7schen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der k\u00fchn sein Gl\u00fcck dem Schicksal abgewann;", "tokens": ["Der", "k\u00fchn", "sein", "Gl\u00fcck", "dem", "Schick\u00b7sal", "ab\u00b7ge\u00b7wann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPOSAT", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein Aug' nur kann in deiner Seele lesen,", "tokens": ["Mein", "Aug'", "nur", "kann", "in", "dei\u00b7ner", "See\u00b7le", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df du so elend bist, wie du's gewesen.", "tokens": ["Da\u00df", "du", "so", "e\u00b7lend", "bist", ",", "wie", "du's", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VAFIN", "$,", "PWAV", "PIS", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die Ruhe, die aus deinem Angesicht,", "tokens": ["Die", "Ru\u00b7he", ",", "die", "aus", "dei\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus deines Mundes stillem L\u00e4cheln spricht,", "tokens": ["Aus", "dei\u00b7nes", "Mun\u00b7des", "stil\u00b7lem", "L\u00e4\u00b7cheln", "spricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ist nicht des Friedens heil'ge Offenbarung:", "tokens": ["Ist", "nicht", "des", "Frie\u00b7dens", "heil'\u00b7ge", "Of\u00b7fen\u00b7ba\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie ist der Seele t\u00f6dtliche Erstarrung.", "tokens": ["Sie", "ist", "der", "See\u00b7le", "t\u00f6dt\u00b7li\u00b7che", "Er\u00b7star\u00b7rung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Der letzte Schmerzenschrei, der als Gebet", "tokens": ["Der", "letz\u00b7te", "Schmer\u00b7zen\u00b7schrei", ",", "der", "als", "Ge\u00b7bet"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "KOUS", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Um Rettung und Erbarmung hei\u00df gefleht,", "tokens": ["Um", "Ret\u00b7tung", "und", "Er\u00b7bar\u00b7mung", "hei\u00df", "ge\u00b7fleht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die letzte Klage ob der Schicksalswunde \u2013", "tokens": ["Die", "letz\u00b7te", "Kla\u00b7ge", "ob", "der", "Schick\u00b7sals\u00b7wun\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Sie waren deines Herzens letzte Stunde.", "tokens": ["Sie", "wa\u00b7ren", "dei\u00b7nes", "Her\u00b7zens", "letz\u00b7te", "Stun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Und was seitdem geschah, ist T\u00e4uschung nur,", "tokens": ["Und", "was", "seit\u00b7dem", "ge\u00b7schah", ",", "ist", "T\u00e4u\u00b7schung", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PAV", "VVFIN", "$,", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du lebst auf deines Ich's Ruinenspur!", "tokens": ["Du", "lebst", "auf", "dei\u00b7nes", "Ich's", "Ru\u00b7i\u00b7nen\u00b7spur", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wenig kann das Sein dich mehr bewegen,", "tokens": ["So", "we\u00b7nig", "kann", "das", "Sein", "dich", "mehr", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VMFIN", "ART", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df du die M\u00fche scheust, es wegzulegen.", "tokens": ["Da\u00df", "du", "die", "M\u00fc\u00b7he", "scheust", ",", "es", "weg\u00b7zu\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "So fern, so fremd, so fahl scheint dir die Welt,", "tokens": ["So", "fern", ",", "so", "fremd", ",", "so", "fahl", "scheint", "dir", "die", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df keine Sehnsucht mehr die Brust dir schwellt,", "tokens": ["Da\u00df", "kei\u00b7ne", "Sehn\u00b7sucht", "mehr", "die", "Brust", "dir", "schwellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ADV", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schon l\u00e4ngst erdr\u00fcckt von qualvollem Entsagen", "tokens": ["Schon", "l\u00e4ngst", "er\u00b7dr\u00fcckt", "von", "qual\u00b7vol\u00b7lem", "Ent\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Kannst du vor keinem neuen Leid mehr zagen.", "tokens": ["Kannst", "du", "vor", "kei\u00b7nem", "neu\u00b7en", "Leid", "mehr", "za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PIAT", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.10": {"line.1": {"text": "So wird durch deine Ruhe selbst mir kund", "tokens": ["So", "wird", "durch", "dei\u00b7ne", "Ru\u00b7he", "selbst", "mir", "kund"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "APPR", "PPOSAT", "NN", "ADV", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der ew'ge Schmerz in deines Busens Grund!", "tokens": ["Der", "ew'\u00b7ge", "Schmerz", "in", "dei\u00b7nes", "Bu\u00b7sens", "Grund", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wolltest meinen Ausspruch du verneinen", "tokens": ["Und", "woll\u00b7test", "mei\u00b7nen", "Aus\u00b7spruch", "du", "ver\u00b7nei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und stark vor mir wie vor den Andern scheinen", "tokens": ["Und", "stark", "vor", "mir", "wie", "vor", "den", "An\u00b7dern", "schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "PPER", "KOKOM", "APPR", "ART", "ADJA", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "So fragt' ich dich \u2013 die Frag' ist von Gewicht:", "tokens": ["So", "fragt'", "ich", "dich", "\u2013", "die", "Frag'", "ist", "von", "Ge\u00b7wicht", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$(", "ART", "NN", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ob man bei dunkel mildem Abendlicht", "tokens": ["Ob", "man", "bei", "dun\u00b7kel", "mil\u00b7dem", "A\u00b7bend\u00b7licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Durch eines Wiegenliedes schlichte T\u00f6ne", "tokens": ["Durch", "ei\u00b7nes", "Wie\u00b7gen\u00b7lie\u00b7des", "schlich\u00b7te", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nicht hei\u00dfe Thr\u00e4nen dir entlocken k\u00f6nne?", "tokens": ["Nicht", "hei\u00b7\u00dfe", "Thr\u00e4\u00b7nen", "dir", "ent\u00b7lo\u00b7cken", "k\u00f6n\u00b7ne", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJA", "NN", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Ich sah dich, wie der wilde Lebensdrang", "tokens": ["Ich", "sah", "dich", ",", "wie", "der", "wil\u00b7de", "Le\u00b7bens\u00b7drang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den Ruf des Weh's von meinen Lippen rang,", "tokens": ["Den", "Ruf", "des", "Weh's", "von", "mei\u00b7nen", "Lip\u00b7pen", "rang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie an des Schmerzaltares nassen Stufen", "tokens": ["Wie", "an", "des", "Schmer\u00b7zal\u00b7ta\u00b7res", "nas\u00b7sen", "Stu\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verblutend du die Gottheit angerufen.", "tokens": ["Ver\u00b7blu\u00b7tend", "du", "die", "Got\u00b7theit", "an\u00b7ge\u00b7ru\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "