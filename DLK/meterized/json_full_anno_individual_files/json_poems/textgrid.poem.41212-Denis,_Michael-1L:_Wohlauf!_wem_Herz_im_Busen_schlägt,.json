{"textgrid.poem.41212": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlauf! wem Herz im Busen schl\u00e4gt,", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlauf! wem Herz im Busen schl\u00e4gt,", "tokens": ["Wohl\u00b7auf", "!", "wem", "Herz", "im", "Bu\u00b7sen", "schl\u00e4gt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PWS", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer Feuerrohr und S\u00e4bel tr\u00e4gt", "tokens": ["Wer", "Feu\u00b7er\u00b7rohr", "und", "S\u00e4\u00b7bel", "tr\u00e4gt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr's hohe Kaiserhaus.", "tokens": ["F\u00fcr's", "ho\u00b7he", "Kai\u00b7ser\u00b7haus", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Heut, Br\u00fcder! ist f\u00fcr uns ein Tag.", "tokens": ["Heut", ",", "Br\u00fc\u00b7der", "!", "ist", "f\u00fcr", "uns", "ein", "Tag", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "VAFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es breche, was da jauchzen mag,", "tokens": ["Es", "bre\u00b7che", ",", "was", "da", "jauch\u00b7zen", "mag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In frohes Jauchzen aus.", "tokens": ["In", "fro\u00b7hes", "Jauch\u00b7zen", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo Joseph und Therese thront,", "tokens": ["Wo", "Jo\u00b7se\u00b7ph", "und", "The\u00b7re\u00b7se", "thront", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Da, Br\u00fcder! wird Verdienst belohnt,", "tokens": ["Da", ",", "Br\u00fc\u00b7der", "!", "wird", "Ver\u00b7dienst", "be\u00b7lohnt", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$.", "VAFIN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da gilt die Tapferkeit;", "tokens": ["Da", "gilt", "die", "Tap\u00b7fer\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da beut man blutiger Gefahr", "tokens": ["Da", "beut", "man", "blu\u00b7ti\u00b7ger", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Stirne nicht vergebens dar,", "tokens": ["Die", "Stir\u00b7ne", "nicht", "ver\u00b7ge\u00b7bens", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das, Br\u00fcder! zeigt sich heut'.", "tokens": ["Das", ",", "Br\u00fc\u00b7der", "!", "zeigt", "sich", "heut'", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$.", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Mann, der uns wie S\u00f6hne liebt,", "tokens": ["Der", "Mann", ",", "der", "uns", "wie", "S\u00f6h\u00b7ne", "liebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "KOKOM", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So Beispiel, wie Befehle giebt,", "tokens": ["So", "Bei\u00b7spiel", ",", "wie", "Be\u00b7feh\u00b7le", "giebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und weise stets befahl,", "tokens": ["Und", "wei\u00b7se", "stets", "be\u00b7fahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der durch so manche gro\u00dfe That", "tokens": ["Der", "durch", "so", "man\u00b7che", "gro\u00b7\u00dfe", "That"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des ganzen Heer's Vertrauen hat,", "tokens": ["Des", "gan\u00b7zen", "Heer's", "Ver\u00b7trau\u00b7en", "hat", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Heut wird er Feldmarschall.", "tokens": ["Heut", "wird", "er", "Feld\u00b7mar\u00b7schall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Oft wenn er durch die Glieder ritt,", "tokens": ["Oft", "wenn", "er", "durch", "die", "Glie\u00b7der", "ritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da flogen stille W\u00fcnsche mit,", "tokens": ["Da", "flo\u00b7gen", "stil\u00b7le", "W\u00fcn\u00b7sche", "mit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lieb' ins Herz uns gab:", "tokens": ["Die", "Lieb'", "ins", "Herz", "uns", "gab", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ha! Vater Laudon! Eines noch:", "tokens": ["Ha", "!", "Va\u00b7ter", "Lau\u00b7don", "!", "Ei\u00b7nes", "noch", ":"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "NN", "NE", "$.", "PIS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Therese! Joseph! gebt ihm doch", "tokens": ["The\u00b7re\u00b7se", "!", "Jo\u00b7se\u00b7ph", "!", "gebt", "ihm", "doch"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "NE", "$.", "VVFIN", "PPER", "ADV"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Nur bald den Marschallstab!", "tokens": ["Nur", "bald", "den", "Mar\u00b7schall\u00b7stab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Zwar da\u00df er auf so manchen Sieg", "tokens": ["Zwar", "da\u00df", "er", "auf", "so", "man\u00b7chen", "Sieg"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "APPR", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch immer von sich selber schwieg,", "tokens": ["Doch", "im\u00b7mer", "von", "sich", "sel\u00b7ber", "schwieg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das, Br\u00fcder wi\u00dft ihr wohl;", "tokens": ["Das", ",", "Br\u00fc\u00b7der", "wi\u00dft", "ihr", "wohl", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch half ihm die Bescheidenheit?", "tokens": ["Doch", "half", "ihm", "die", "Be\u00b7schei\u00b7den\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O nein! Europa weit und breit", "tokens": ["O", "nein", "!", "Eu\u00b7ro\u00b7pa", "weit", "und", "breit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKANT", "$.", "NE", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ist seines Namens voll.", "tokens": ["Ist", "sei\u00b7nes", "Na\u00b7mens", "voll", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Vereinet aller Trommeln Schall,", "tokens": ["Ver\u00b7ei\u00b7net", "al\u00b7ler", "Trom\u00b7meln", "Schall", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und aller uns'rer St\u00fccke Knall,", "tokens": ["Und", "al\u00b7ler", "un\u00b7s'\u00b7rer", "St\u00fc\u00b7cke", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie weit vernimmt man sie!", "tokens": ["Wie", "weit", "ver\u00b7nimmt", "man", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PIS", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Doch setzet mir an ihren Platz", "tokens": ["Doch", "set\u00b7zet", "mir", "an", "ih\u00b7ren", "Platz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun Olm\u00fctz, Landshut, Schweidnitz, Glatz!", "tokens": ["Nun", "Ol\u00b7m\u00fctz", ",", "Lands\u00b7hut", ",", "Schweid\u00b7nitz", ",", "Glatz", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "NE", "$,", "NN", "$,", "NE", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch lauter donnern die.", "tokens": ["Noch", "lau\u00b7ter", "don\u00b7nern", "die", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Weich, Vater Laudon! weich zur\u00fcck!", "tokens": ["Weich", ",", "Va\u00b7ter", "Lau\u00b7don", "!", "weich", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "NE", "$.", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein scharfes Aug' ist unser Gl\u00fcck,", "tokens": ["Ein", "schar\u00b7fes", "Aug'", "ist", "un\u00b7ser", "Gl\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Aug' entgehst du nicht.", "tokens": ["Dem", "Aug'", "ent\u00b7gehst", "du", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Therese, Joseph winken dir:", "tokens": ["The\u00b7re\u00b7se", ",", "Jo\u00b7se\u00b7ph", "win\u00b7ken", "dir", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NE", "VVFIN", "PPER", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Hervor! ergreif der Helden Zier,", "tokens": ["Her\u00b7vor", "!", "er\u00b7greif", "der", "Hel\u00b7den", "Zier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$.", "ADJD", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Stab! hervor ans Licht!", "tokens": ["Den", "Stab", "!", "her\u00b7vor", "ans", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "PTKVZ", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dein Schwung begeistert uns're Brust,", "tokens": ["Dein", "Schwung", "be\u00b7geis\u00b7tert", "un\u00b7s'\u00b7re", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Wir f\u00fchlen k\u00fchner Thaten Lust", "tokens": ["Wir", "f\u00fch\u00b7len", "k\u00fch\u00b7ner", "Tha\u00b7ten", "Lust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf uns'