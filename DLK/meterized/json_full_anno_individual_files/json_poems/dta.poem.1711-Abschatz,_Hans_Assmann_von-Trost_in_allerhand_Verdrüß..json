{"dta.poem.1711": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Trost in allerhand Verdr\u00fc\u00df.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "So gehts mein Pithias/ mir m\u00fcssen immer leiden", "tokens": ["So", "gehts", "mein", "Pi\u00b7thi\u00b7as", "/", "mir", "m\u00fcs\u00b7sen", "im\u00b7mer", "lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Beschwerden ohne Maa\u00df/ ungleicher B\u00fcrde Last/", "tokens": ["Be\u00b7schwer\u00b7den", "oh\u00b7ne", "Maa\u00df", "/", "un\u00b7glei\u00b7cher", "B\u00fcr\u00b7de", "Last", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "ADJA", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aufflagen ohne Grund/ Verleumdung/ Ehr-Abschneiden/", "tokens": ["Auf\u00b7fla\u00b7gen", "oh\u00b7ne", "Grund", "/", "Ver\u00b7leum\u00b7dung", "/", "Ehr\u00b7Ab\u00b7schnei\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "NN", "$(", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Neyd/ Undanck/ falsche List/ und was di\u00df Blatt nicht fast.", "tokens": ["Neyd", "/", "Un\u00b7danck", "/", "fal\u00b7sche", "List", "/", "und", "was", "di\u00df", "Blatt", "nicht", "fast", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "ADJA", "NN", "$(", "KON", "PWS", "PDS", "NN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Man wei\u00df die L\u00fcgen-Wort auffs sch\u00f6nste zu bekleiden/", "tokens": ["Man", "wei\u00df", "die", "L\u00fc\u00b7gen\u00b7Wort", "auffs", "sch\u00f6ns\u00b7te", "zu", "be\u00b7klei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPRART", "ADJA", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Lieb und Redligkeit ist ein verachter Gast.", "tokens": ["Die", "Lieb", "und", "Red\u00b7lig\u00b7keit", "ist", "ein", "ver\u00b7ach\u00b7ter", "Gast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der Himmel hat mir auch hiervon mein Theil bescheiden/", "tokens": ["Der", "Him\u00b7mel", "hat", "mir", "auch", "hier\u00b7von", "mein", "Theil", "be\u00b7schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "PAV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie du vielleicht/ und mehr als ich/ vernommen hast.", "tokens": ["Wie", "du", "viel\u00b7leicht", "/", "und", "mehr", "als", "ich", "/", "ver\u00b7nom\u00b7men", "hast", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "$(", "KON", "ADV", "KOUS", "PPER", "$(", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Gedult! dieweil wir hier mit rauhem Dorn und Hecken/", "tokens": ["Ge\u00b7dult", "!", "die\u00b7weil", "wir", "hier", "mit", "rau\u00b7hem", "Dorn", "und", "He\u00b7cken", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "ADV", "APPR", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie GOttes Wort f\u00fcrl\u00e4ngst gemeldt/ umgeben stecken;", "tokens": ["Wie", "Got\u00b7tes", "Wort", "f\u00fcr\u00b7l\u00e4ngst", "ge\u00b7meldt", "/", "um\u00b7ge\u00b7ben", "ste\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "ADV", "VVPP", "$(", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "So w\u00fcrden wir uns mit viel r\u00fchren weher thun/", "tokens": ["So", "w\u00fcr\u00b7den", "wir", "uns", "mit", "viel", "r\u00fch\u00b7ren", "we\u00b7her", "thun", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "APPR", "PIS", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df den gesezten Geist in GOtt gelassen ruhn;", "tokens": ["La\u00df", "den", "ge\u00b7sez\u00b7ten", "Geist", "in", "Gott", "ge\u00b7las\u00b7sen", "ruhn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "ADJA", "NN", "APPR", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wenn/ was uns rizt und sticht am Ende mu\u00df verbrennen/", "tokens": ["Wenn", "/", "was", "uns", "rizt", "und", "sticht", "am", "En\u00b7de", "mu\u00df", "ver\u00b7bren\u00b7nen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWS", "PPER", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wird GOtt und alle Welt/ was treu und redlich/ kennen.", "tokens": ["Wird", "Gott", "und", "al\u00b7le", "Welt", "/", "was", "treu", "und", "red\u00b7lich", "/", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "PIAT", "NN", "$(", "PWS", "ADJD", "KON", "ADJD", "$(", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}