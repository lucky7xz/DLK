: ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Den Fr\u00fchling seiner Jahre", "tokens": ["Den", "Fr\u00fch\u00b7ling", "sei\u00b7ner", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wil in der Bl\u00fchte sehn/", "tokens": ["Wil", "in", "der", "Bl\u00fch\u00b7te", "sehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "VVINF", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der gebe ja kein Lieben zu/", "tokens": ["Der", "ge\u00b7be", "ja", "kein", "Lie\u00b7ben", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "ADJA", "PTKZU", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sonst f\u00e4lt er zeitig auf die Bahre/", "tokens": ["Sonst", "f\u00e4lt", "er", "zei\u00b7tig", "auf", "die", "Bah\u00b7re", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und mu\u00df in kurtzer Zeit vergehn.", "tokens": ["Und", "mu\u00df", "in", "kurt\u00b7zer", "Zeit", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Liebe ist die Pest der Seelen/", "tokens": ["Lie\u00b7be", "ist", "die", "Pest", "der", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Liebe ist ein scharffes Gifft;", "tokens": ["Lie\u00b7be", "ist", "ein", "scharf\u00b7fes", "Gifft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zucker scheint sie zwar zu seyn/", "tokens": ["Zu\u00b7cker", "scheint", "sie", "zwar", "zu", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "PTKZU", "VAINF", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Doch der herbe Wermuhts-Wein/", "tokens": ["Doch", "der", "her\u00b7be", "Werm\u00b7uhts\u00b7Wein", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mu\u00df zuletzt die Geister qu\u00e4hlen.", "tokens": ["Mu\u00df", "zu\u00b7letzt", "die", "Geis\u00b7ter", "qu\u00e4h\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Liebe ist die Pest der Seelen/", "tokens": ["Lie\u00b7be", "ist", "die", "Pest", "der", "See\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Liebe ist ein scharffes Gifft.", "tokens": ["Lie\u00b7be", "ist", "ein", "scharf\u00b7fes", "Gifft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Wie! ", "tokens": ["Wie", "!"], "token_info": ["word", "punct"], "pos": ["PWAV", "$."], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Ist ", "tokens": ["Ist"], "t