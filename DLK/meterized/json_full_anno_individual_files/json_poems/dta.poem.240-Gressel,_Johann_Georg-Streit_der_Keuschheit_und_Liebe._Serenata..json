{"dta.poem.240": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Streit der Keuschheit und Liebe.  \n  Serenata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Augen/ die dem Glantz der Sonnen", "tokens": ["Au\u00b7gen", "/", "die", "dem", "Glantz", "der", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "ART", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mehr als doppelt abgewonnen/", "tokens": ["Mehr", "als", "dop\u00b7pelt", "ab\u00b7ge\u00b7won\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "KOKOM", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "M\u00fcssen meine Fackeln seyn.", "tokens": ["M\u00fcs\u00b7sen", "mei\u00b7ne", "Fa\u00b7ckeln", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Solche Blicke zu verstricken/", "tokens": ["Sol\u00b7che", "Bli\u00b7cke", "zu", "ver\u00b7stri\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df sie gleich ein Hertz ber\u00fccken", "tokens": ["Da\u00df", "sie", "gleich", "ein", "Hertz", "be\u00b7r\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gebe ich den ", "tokens": ["Ge\u00b7be", "ich", "den"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Augen/ die den Glantz der Sonnen", "tokens": ["Au\u00b7gen", "/", "die", "den", "Glantz", "der", "Son\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "ART", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mehr als doppelt abgewonnen", "tokens": ["Mehr", "als", "dop\u00b7pelt", "ab\u00b7ge\u00b7won\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["PIAT", "KOKOM", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "M\u00fcssen meine Fackeln seyn.", "tokens": ["M\u00fcs\u00b7sen", "mei\u00b7ne", "Fa\u00b7ckeln", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich zwinge alle Welt", "tokens": ["Ich", "zwin\u00b7ge", "al\u00b7le", "Welt"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Zu meiner s\u00fcssen Dienstbarkeit/", "tokens": ["Zu", "mei\u00b7ner", "s\u00fcs\u00b7sen", "Dienst\u00b7bar\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Hertz von Stein und Stahl/", "tokens": ["Ein", "Hertz", "von", "Stein", "und", "Stahl", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wird bald durch einen Strahl/", "tokens": ["Wird", "bald", "durch", "ei\u00b7nen", "Strahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und holden Blick gef\u00e4lt.", "tokens": ["Und", "hol\u00b7den", "Blick", "ge\u00b7f\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Es kan sich keiner retten/", "tokens": ["Es", "kan", "sich", "kei\u00b7ner", "ret\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "PIS", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus meinen starcken Ketten.", "tokens": ["Aus", "mei\u00b7nen", "star\u00b7cken", "Ket\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Zu mahl da solche Sch\u00f6nen", "tokens": ["Zu", "mahl", "da", "sol\u00b7che", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Zu meinem Dienst bereit/", "tokens": ["Zu", "mei\u00b7nem", "Dienst", "be\u00b7reit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Die Pracht und Anmuht kr\u00f6hnen.", "tokens": ["Die", "Pracht", "und", "An\u00b7muht", "kr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Auch keine Lust/", "tokens": ["Auch", "kei\u00b7ne", "Lust", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Jm Himmel und auf Erden/", "tokens": ["Jm", "Him\u00b7mel", "und", "auf", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Kan einer Brust", "tokens": ["Kan", "ei\u00b7ner", "Brust"], "token_info": ["word", "word", "word"], "pos": ["VMFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Beliebter werden/", "tokens": ["Be\u00b7lieb\u00b7ter", "wer\u00b7den", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAINF", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Als wenn man liebt/", "tokens": ["Als", "wenn", "man", "liebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "VVFIN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Und Sch\u00f6nheit wieder K\u00fcsse giebt.", "tokens": ["Und", "Sch\u00f6n\u00b7heit", "wie\u00b7der", "K\u00fcs\u00b7se", "giebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo reine Zucht und Tugend bl\u00fcht/", "tokens": ["Wo", "rei\u00b7ne", "Zucht", "und", "Tu\u00b7gend", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da ist der Himmel auf der Erden.", "tokens": ["Da", "ist", "der", "Him\u00b7mel", "auf", "der", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts giebt dem klugen Frauen-Zimmer", "tokens": ["Nichts", "giebt