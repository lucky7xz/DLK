{"textgrid.poem.53222": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dein Zorn wil, Herr, mir vnertr\u00e4glich werden,", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dein Zorn wil, Herr, mir vnertr\u00e4glich werden,", "tokens": ["Dein", "Zorn", "wil", ",", "Herr", ",", "mir", "vner\u00b7tr\u00e4g\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "$,", "NN", "$,", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ich sincke hin gedrucket bi\u00df zur Erden,", "tokens": ["Ich", "sin\u00b7cke", "hin", "ge\u00b7dru\u00b7cket", "bi\u00df", "zur", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVFIN", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die schwere Last heisst in mir Marck vnd Bein", "tokens": ["Die", "schwe\u00b7re", "Last", "heisst", "in", "mir", "Marck", "vnd", "Bein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Nicht tauglich seyn.", "tokens": ["Nicht", "taug\u00b7lich", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Wenn du ergrimmst, so mu\u00df das Erdreich zittern,", "tokens": ["Wenn", "du", "er\u00b7grimmst", ",", "so", "mu\u00df", "das", "Er\u00b7dreich", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "ADV", "VMFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Berge Grund aus schrecken sich ersch\u00fcttern,", "tokens": ["Der", "Ber\u00b7ge", "Grund", "aus", "schre\u00b7cken", "sich", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "VVFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die wilde See zu fliehen seyn bedacht", "tokens": ["Die", "wil\u00b7de", "See", "zu", "flie\u00b7hen", "seyn", "be\u00b7dacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr deiner Macht.", "tokens": ["F\u00fcr", "dei\u00b7ner", "Macht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Du darffst herab nicht eins recht zornig schawen,", "tokens": ["Du", "darffst", "her\u00b7ab", "nicht", "eins", "recht", "zor\u00b7nig", "scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "PIS", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "So brennen schon Gep\u00fcsche, Feld vnd Awen,", "tokens": ["So", "bren\u00b7nen", "schon", "Ge\u00b7p\u00fcsc\u00b7he", ",", "Feld", "vnd", "A\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Tannen Zier, der sch\u00f6ne Cedern-Waldt", "tokens": ["Der", "Tan\u00b7nen", "Zier", ",", "der", "sch\u00f6\u00b7ne", "Ce\u00b7dern\u00b7Waldt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist vngestalt.", "tokens": ["Ist", "vn\u00b7ge\u00b7stalt", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Du l\u00e4ssest St\u00e4dt' vnd gantze V\u00f6lcker heulen,", "tokens": ["Du", "l\u00e4s\u00b7sest", "St\u00e4dt'", "vnd", "gant\u00b7ze", "V\u00f6l\u00b7cker", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd reissest vmb der L\u00e4nder starcke Seulen,", "tokens": ["Vnd", "reis\u00b7sest", "vmb", "der", "L\u00e4n\u00b7der", "star\u00b7cke", "Seu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du t\u00f6dtest, was in N\u00f6then vnd Gefahr", "tokens": ["Du", "t\u00f6d\u00b7test", ",", "was", "in", "N\u00f6\u00b7then", "vnd", "Ge\u00b7fahr"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "F\u00fcr Mawren war.", "tokens": ["F\u00fcr", "Maw\u00b7ren", "war", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Wie hastu mich so kl\u00e4glich zugerichtet?", "tokens": ["Wie", "has\u00b7tu", "mich", "so", "kl\u00e4g\u00b7lich", "zu\u00b7ge\u00b7rich\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein Haupt ist hin, mein ansehn ligt zernichtet,", "tokens": ["Mein", "Haupt", "ist", "hin", ",", "mein", "an\u00b7sehn", "ligt", "zer\u00b7nich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "PPOSAT", "VVINF", "VVFIN", "VVPP", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Die Krohne, so mich vormals hoch geziert,", "tokens": ["Die", "Kroh\u00b7ne", ",", "so", "mich", "vor\u00b7mals", "hoch", "ge\u00b7ziert", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wird nicht gesp\u00fcrt.", "tokens": ["Wird", "nicht", "ge\u00b7sp\u00fcrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Ich wil mich nur mit Witwen-Kleidern tragen,", "tokens": ["Ich", "wil", "mich", "nur", "mit", "Wit\u00b7wen\u00b7Klei\u00b7dern", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd anders nicht beginnen, als mich klagen.", "tokens": ["Vnd", "an\u00b7ders", "nicht", "be\u00b7gin\u00b7nen", ",", "als", "mich", "kla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PTKNEG", "VVINF", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer Wollust liebt vnd Frewde sucht, der hat", "tokens": ["Wer", "Wol\u00b7lust", "liebt", "vnd", "Frew\u00b7de", "sucht", ",", "der", "hat"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "KON", "NN", "VVFIN", "$,", "PRELS", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Bey mir nicht stat.", "tokens": ["Bey", "mir", "nicht", "stat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKNEG", "VVFIN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.7": {"line.1": {"text": "Ihr Witwen kompt, Ihr [Bilder meiner Schmertzen],", "tokens": ["Ihr", "Wit\u00b7wen", "kompt", ",", "Ihr", "Bil\u00b7der", "mei\u00b7ner", "Schmert\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "PPOSAT", "$(", "NN", "PPOSAT", "NN", "$(", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Auch die jhr geht mit sonst bedrucktem Hertzen,", "tokens": ["Auch", "die", "jhr", "geht", "mit", "sonst", "be\u00b7druck\u00b7tem", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PPER", "VVFIN", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kompt, saget mir den Jammer vnd Beschwer", "tokens": ["Kompt", ",", "sa\u00b7get", "mir", "den", "Jam\u00b7mer", "vnd", "Be\u00b7schwer"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "PPER", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mit Wehmuth her.", "tokens": ["Mit", "Weh\u00b7muth", "her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Ihr findet hie, der Noth nach, ewres gleichen,", "tokens": ["Ihr", "fin\u00b7det", "hie", ",", "der", "Noth", "nach", ",", "ew\u00b7res", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "ART", "NN", "PTKVZ", "$,", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich werd' euch nicht in Klag vnd Schmertzen weichen,", "tokens": ["Ich", "werd'", "euch", "nicht", "in", "Klag", "vnd", "Schmert\u00b7zen", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vnd kan vieleicht in nicht gemeiner Pein", "tokens": ["Vnd", "kan", "vie\u00b7leicht", "in", "nicht", "ge\u00b7mei\u00b7ner", "Pein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "APPR", "PTKNEG", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch F\u00fcrstin seyn.", "tokens": ["Auch", "F\u00fcrs\u00b7tin", "seyn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Du aber, Gott, erkennest meine Zehren", "tokens": ["Du", "a\u00b7ber", ",", "Gott", ",", "er\u00b7ken\u00b7nest", "mei\u00b7ne", "Zeh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Vnd wirst zuletzt mir dennoch Trost gewehren,", "tokens": ["Vnd", "wirst", "zu\u00b7letzt", "mir", "den\u00b7noch", "Trost", "ge\u00b7weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht machen, da\u00df auch deine Vater-Trew", "tokens": ["Nicht", "ma\u00b7chen", ",", "da\u00df", "auch", "dei\u00b7ne", "Va\u00b7ter\u00b7Trew"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "$,", "KOUS", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Begraben sey.", "tokens": ["Be\u00b7gra\u00b7ben", "sey", "."], "token_info": ["word", "word", "punct"], "pos": ["VVPP", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "La\u00df deine Ruh mir doch im Hertzen walten,", "tokens": ["La\u00df", "dei\u00b7ne", "Ruh", "mir", "doch", "im", "Hert\u00b7zen", "wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "ADV", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich wil nur dich f\u00fcr meine Zuflucht halten,", "tokens": ["Ich", "wil", "nur", "dich", "f\u00fcr", "mei\u00b7ne", "Zu\u00b7flucht", "hal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es ist ja sonst mit allem Thun der Welt", "tokens": ["Es", "ist", "ja", "sonst", "mit", "al\u00b7lem", "Thun", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "PIS", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zu schlecht bestellt.", "tokens": ["Zu", "schlecht", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Hie mu\u00df Gewalt und Herrligkeit vergehen,", "tokens": ["Hie", "mu\u00df", "Ge\u00b7walt", "und", "Herr\u00b7lig\u00b7keit", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der herrschet erst, der fest in Gott kan stehen,", "tokens": ["Der", "herr\u00b7schet", "erst", ",", "der", "fest", "in", "Gott", "kan", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "$,", "PRELS", "ADJD", "APPR", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die\u00df Reich wird seyn, wenn Zeit, Welt, Krohn vnd Pracht", "tokens": ["Die\u00df", "Reich", "wird", "seyn", ",", "wenn", "Zeit", ",", "Welt", ",", "Krohn", "vnd", "Pracht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PDS", "NN", "VAFIN", "VAINF", "$,", "KOUS", "NN", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Giebt gute Nacht.", "tokens": ["Giebt", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Dein Zorn wil, Herr, mir vnertr\u00e4glich werden,", "tokens": ["Dein", "Zorn", "wil", ",", "Herr", ",", "mir", "vner\u00b7tr\u00e4g\u00b7lich", "wer\u00b7den", ","], "token_info": ["word", "word", "w