{"dta.poem.2593": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. S. R. u. J. A. E. G. Hochzeit den  \n 10. Febr. 1681.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm Krantz du Krohne meiner Jahr'/", "tokens": ["Komm", "Krantz", "du", "Kroh\u00b7ne", "mei\u00b7ner", "Jahr'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ziere noch einmahl das Haar\u2019/", "tokens": ["Und", "zie\u00b7re", "noch", "ein\u00b7mahl", "das", "Haa\u00b7r'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh\u2019 ich der Freyheit Urlaub gebe.", "tokens": ["Eh'", "ich", "der", "Frey\u00b7heit", "Ur\u00b7laub", "ge\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.4": {"text": "Bestreut ihr Blumen mich zuletzt/", "tokens": ["Be\u00b7streut", "ihr", "Blu\u00b7men", "mich", "zu\u00b7letzt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Eh ich den Fu\u00df ins Garn gesetzt/", "tokens": ["Eh", "ich", "den", "Fu\u00df", "ins", "Garn", "ge\u00b7setzt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und in den s\u00fcssen Fesseln lebe.", "tokens": ["Und", "in", "den", "s\u00fcs\u00b7sen", "Fes\u00b7seln", "le\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Jhr Myrten die ihr immer bl\u00fcht/", "tokens": ["Ihr", "Myr\u00b7ten", "die", "ihr", "im\u00b7mer", "bl\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "PPER", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die keine Zeit verwelcken siht/", "tokens": ["Die", "kei\u00b7ne", "Zeit", "ver\u00b7wel\u00b7cken", "siht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVINF", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bekr\u00e4ntzet mich mit euren Zweigen/", "tokens": ["Be\u00b7kr\u00b7\u00e4nt\u00b7zet", "mich", "mit", "eu\u00b7ren", "Zwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$("], "meter": "-++-+-+-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Auch mitten unter Frost und Schnee/", "tokens": ["Auch", "mit\u00b7ten", "un\u00b7ter", "Frost", "und", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wird Flora Lilien und Klee/", "tokens": ["Wird", "Flo\u00b7ra", "Li\u00b7li\u00b7en", "und", "Klee", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "NE", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und einen Blumen Monat zeigen.", "tokens": ["Und", "ei\u00b7nen", "Blu\u00b7men", "Mo\u00b7nat", "zei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was Schwestern schaut ihr mich so an/", "tokens": ["Was", "Schwes\u00b7tern", "schaut", "ihr", "mich", "so", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVFIN", "PPER", "PRF", "ADV", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich den Vorsatz \u00e4ndern kan/", "tokens": ["Da\u00df", "ich", "den", "Vor\u00b7satz", "\u00e4n\u00b7dern", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und euren treuen Orden lassen?", "tokens": ["Und", "eu\u00b7ren", "treu\u00b7en", "Or\u00b7den", "las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich von euer sch\u00f6nen Reyh/", "tokens": ["Da\u00df", "ich", "von", "eu\u00b7er", "sch\u00f6\u00b7nen", "Reyh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hinf\u00fchro gantz entsondert sey/", "tokens": ["Hin\u00b7f\u00fch\u00b7ro", "gantz", "ent\u00b7son\u00b7dert", "sey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Und gebe mich dem Garn zu fassen?", "tokens": ["Und", "ge\u00b7be", "mich", "dem", "Garn", "zu", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich bin zu schwach der grossen Gluth/", "tokens": ["Ich", "bin", "zu", "schwach", "der", "gros\u00b7sen", "Gluth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die bey den Menschen Wunder thut/", "tokens": ["Die", "bey", "den", "Men\u00b7schen", "Wun\u00b7der", "thut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Gegenwehr zu widerstreben/", "tokens": ["Durch", "Ge\u00b7gen\u00b7wehr", "zu", "wi\u00b7der\u00b7stre\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ich kan auff Erden nicht allein", "tokens": ["Ich", "kan", "auff", "Er\u00b7den", "nicht", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "PTKNEG", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gantz ohne Geist und Flamme seyn/", "tokens": ["Gantz", "oh\u00b7ne", "Geist", "und", "Flam\u00b7me", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und als ein kalter Marmel leben.", "tokens": ["Und", "als", "ein", "kal\u00b7ter", "Mar\u00b7mel", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich f\u00fchle wol was mich bestrickt/", "tokens": ["Ich", "f\u00fch\u00b7le", "wol", "was", "mich", "be\u00b7strickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mir ist zu tieff ins Hertz gedr\u00fcckt/", "tokens": ["Mir", "ist", "zu", "tieff", "ins", "Hertz", "ge\u00b7dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Bildn\u00fc\u00df das ich mir erlesen/", "tokens": ["Ein", "Bild\u00b7n\u00fc\u00df", "das", "ich", "mir", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PPER", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vor wust ich zwar von keiner Gunst/", "tokens": ["Vor", "wust", "ich", "zwar", "von", "kei\u00b7ner", "Gunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "ADV", "APPR", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nun aber f\u00fchl\u2019 ich Lieb und Brunst/", "tokens": ["Nun", "a\u00b7ber", "f\u00fchl'", "ich", "Lieb", "und", "Brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Durch die ich eintzig kan genesen.", "tokens": ["Durch", "die", "ich", "eint\u00b7zig", "kan", "ge\u00b7ne\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Mein Br\u00e4utigam/ mein Artzt und Rath/", "tokens": ["Mein", "Br\u00e4u\u00b7ti\u00b7gam", "/", "mein", "Artzt", "und", "Rath", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$(", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sich mit mir verbunden hat/", "tokens": ["Der", "sich", "mit", "mir", "ver\u00b7bun\u00b7den", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Erweckt in mir dergleichen Freuden/", "tokens": ["Er\u00b7weckt", "in", "mir", "derg\u00b7lei\u00b7chen", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PIS", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df ich von euch entweichen will/", "tokens": ["Da\u00df", "ich", "von", "euch", "ent\u00b7wei\u00b7chen", "will", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Weg von der Nymfen Schertz und Spiel/", "tokens": ["Weg", "von", "der", "Nym\u00b7fen", "Schertz", "und", "Spiel", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "KON", "NN", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hin in der Frauen Orden scheiden.", "tokens": ["Hin", "in", "der", "Frau\u00b7en", "Or\u00b7den", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Mein Krantz/ der itzt so frisch und sch\u00f6n/", "tokens": ["Mein", "Krantz", "/", "der", "itzt", "so", "frisch", "und", "sch\u00f6n", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ADV", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird in viel gr\u00f6\u00dfrem Zierath stehn/", "tokens": ["Wird", "in", "viel", "gr\u00f6\u00df\u00b7rem", "Zie\u00b7rath", "stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So bald ihn hat ein Netz umbfangen.", "tokens": ["So", "bald", "ihn", "hat", "ein", "Netz", "umb\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Faden der schon an mir klebt/", "tokens": ["Der", "Fa\u00b7den", "der", "schon", "an", "mir", "klebt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den hat Cupido selbst gewebt/", "tokens": ["Den", "hat", "Cu\u00b7pi\u00b7do", "selbst", "ge\u00b7webt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NE", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als er der Mutter nachgegangen.", "tokens": ["Als", "er", "der", "Mut\u00b7ter", "nach\u00b7ge\u00b7gan\u00b7gen", "."], "token_