{"textgrid.poem.57398": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als Dolmetscherin, hatte vollendet den Kampf Thuiskona", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als Dolmetscherin, hatte vollendet den Kampf Thuiskona", "tokens": ["Als", "Dol\u00b7met\u00b7sche\u00b7rin", ",", "hat\u00b7te", "voll\u00b7en\u00b7det", "den", "Kampf", "Thu\u00b7is\u00b7ko\u00b7na"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "$,", "VAFIN", "VVPP", "ART", "NN", "NE"], "meter": "-+-+-+--+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Mit Romana, und dir, Hell\u00e4nis. Sie senkte das Auge", "tokens": ["Mit", "Ro\u00b7ma\u00b7na", ",", "und", "dir", ",", "Hel\u00b7l\u00e4\u00b7nis", ".", "Sie", "senk\u00b7te", "das", "Au\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "KON", "PPER", "$,", "NE", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Nieder; denn sie errang nur Sprosslinge weniger Lorber", "tokens": ["Nie\u00b7der", ";", "denn", "sie", "er\u00b7rang", "nur", "Spross\u00b7lin\u00b7ge", "we\u00b7ni\u00b7ger", "Lor\u00b7ber"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "KON", "PPER", "VVFIN", "ADV", "NN", "PIAT", "NN"], "meter": "-+-+-+-++-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Zweige: doch war auch die Wange der frohen entgl\u00fcht.", "tokens": ["Zwei\u00b7ge", ":", "doch", "war", "auch", "die", "Wan\u00b7ge", "der", "fro\u00b7hen", "ent\u00b7gl\u00fcht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"