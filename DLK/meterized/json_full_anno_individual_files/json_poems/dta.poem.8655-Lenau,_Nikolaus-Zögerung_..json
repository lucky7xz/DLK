{"dta.poem.8655": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Z\u00f6gerung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Beschritten schon von seinem Reiter", "tokens": ["Be\u00b7schrit\u00b7ten", "schon", "von", "sei\u00b7nem", "Rei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Rafft auf der Weide noch das Ro\u00df", "tokens": ["Rafft", "auf", "der", "Wei\u00b7de", "noch", "das", "Ro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die lezten Halme, will nicht weiter,", "tokens": ["Die", "lez\u00b7ten", "Hal\u00b7me", ",", "will", "nicht", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VMFIN", "PTKNEG", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bis ihm der Sporen scharfer Sto\u00df", "tokens": ["Bis", "ihm", "der", "Spo\u00b7ren", "schar\u00b7fer", "Sto\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Gewaltig in die Seiten dringt,", "tokens": ["Ge\u00b7wal\u00b7tig", "in", "die", "Sei\u00b7ten", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und es im Sturm von dannen zwingt.", "tokens": ["Und", "es", "im", "Sturm", "von", "dan\u00b7nen", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPRART", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und f\u00fchlt der Mensch mit bleichem Beben", "tokens": ["Und", "f\u00fchlt", "der", "Mensch", "mit", "blei\u00b7chem", "Be\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Tod ihm sitzen am Genick,", "tokens": ["Den", "Tod", "ihm", "sit\u00b7zen", "am", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So klammert sich sein Fu\u00df an's Leben,", "tokens": ["So", "klam\u00b7mert", "sich", "sein", "Fu\u00df", "an's", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Er bettelt um den Augenblick,", "tokens": ["Er", "bet\u00b7telt", "um", "den", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bis wild der Tod die Gei\u00dfel schwingt,", "tokens": ["Bis", "wild", "der", "Tod", "die", "Gei\u00b7\u00dfel", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihn mit Macht von dannen zwingt.", "tokens": ["Und", "ihn", "mit", "Macht", "von", "dan\u00b7nen", "zwingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}