{"textgrid.poem.63505": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbdenn ward he dat Upstahn woll lihrn.\u00ab", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbdenn ward he dat Upstahn woll lihrn.\u00ab", "tokens": ["\u00bb", "denn", "ward", "he", "dat", "U\u00b7pstahn", "woll", "lihrn", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "VAFIN", "NE", "ART", "NN", "VMFIN", "VVINF", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "\u00bbje, dor ward se sick sch\u00f6n an kihrn!", "tokens": ["\u00bb", "je", ",", "dor", "ward", "se", "sick", "sch\u00f6n", "an", "kihrn", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$,", "ADV", "VAFIN", "FM", "FM", "ADJD", "APPR", "NN", "$."], "meter": "+----+-+", "measure": "dactylic.init"}, "line.3": {"text": "Wenn he ok upstahn will Klock dree,", "tokens": ["Wenn", "he", "ok", "ups\u00b7tahn", "will", "Klock", "dree", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "FM", "VMFIN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "denn is se lang all up v\u00f6r twee,", "tokens": ["denn", "is", "se", "lang", "all", "up", "v\u00f6r", "twee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "ADJD", "PIAT", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "un wu he h\u00fct toirst henk\u00fcmmt,", "tokens": ["un", "wu", "he", "h\u00fct", "to\u00b7irst", "hen\u00b7k\u00fcmmt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "was se all gistern ganz bestimmt.", "tokens": ["was", "se", "all", "gis\u00b7tern", "ganz", "be\u00b7stimmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PIAT", "NN", "ADV", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Ick kenn ehr jo von l\u00fctt up an", "tokens": ["Ick", "kenn", "ehr", "jo", "von", "l\u00fctt", "up", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "un weit jo, wat se leisten kann.", "tokens": ["un", "weit", "jo", ",", "wat", "se", "leis\u00b7ten", "kann", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "ADJD", "NE", "$,", "FM", "FM", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nee, Stine, nee, du kannst mi gl\u00f6wen,", "tokens": ["Nee", ",", "Sti\u00b7ne", ",", "nee", ",", "du", "kannst", "mi", "gl\u00f6\u00b7wen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "NE", "VVINF", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "se treckt em sick; he schall woll t\u00f6wen.", "tokens": ["se", "treckt", "em", "sick", ";", "he", "schall", "woll", "t\u00f6\u00b7wen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$.", "VVFIN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "\u00dcns' Lotting sick geh\u00fcrig trecken!", "tokens": ["\u00dcns'", "Lot\u00b7ting", "sick", "ge\u00b7h\u00fc\u00b7rig", "tre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Je, dor w\u00fcrr he in't Nettel leggen.\u00ab", "tokens": ["Je", ",", "dor", "w\u00fcrr", "he", "in't", "Net\u00b7tel", "leg\u00b7gen", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.13": {"text": "\u00bbherr Je, Karlin, mak mi nich bang!\u00ab", "tokens": ["\u00bb", "herr", "Je", ",", "Kar\u00b7lin", ",", "mak", "mi", "nich", "bang", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "NE", "$,", "NE", "NE", "PTKNEG", "ADJD", "$.", "$("], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.14": {"text": "\u00bbna, Stine, sorg di man nich lang.", "tokens": ["\u00bb", "na", ",", "Sti\u00b7ne", ",", "sorg", "di", "man", "nich", "lang", "."], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "NN", "$,", "VVFIN", "NE", "PIS", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Ick weit'n Weg, wu he ehr treckt.", "tokens": ["Ick", "weit'n", "Weg", ",", "wu", "he", "ehr", "treckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "FM", "FM", "NN", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.16": {"text": "Kik, wenn he, wat sin Og verspreckt,", "tokens": ["Kik", ",", "wenn", "he", ",", "wat", "sin", "Og", "ver\u00b7spreckt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "NE", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "ok richtig h\u00f6llt \u2013 een Mann een Wurt \u2013", "tokens": ["ok", "rich\u00b7tig", "h\u00f6llt", "\u2013", "e\u00b7en", "Mann", "e\u00b7en", "Wurt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "un dat ehr ganzes L\u00e4ben durt;", "tokens": ["un", "dat", "ehr", "gan\u00b7zes", "L\u00e4\u00b7ben", "durt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "wenn he ehr ganzes Recht ehr g\u00fcnnt,", "tokens": ["wenn", "he", "ehr", "gan\u00b7zes", "Recht", "ehr", "g\u00fcnnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADJA", "NN", "NN", "ADJD", "$,"], "meter": "+--+-+--", "measure": "iambic.tri.invert"}, "line.20": {"text": "sick nich in anner Ogen s\u00fcnnt", "tokens": ["sick", "nich", "in", "an\u00b7ner", "O\u00b7gen", "s\u00fcnnt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PTKNEG", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "un nie nich as sonn Kullerhahn", "tokens": ["un", "nie", "nich", "as", "sonn", "Kul\u00b7ler\u00b7hahn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "ADV", "PTKNEG", "NE", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.22": {"text": "upprusten deit, denn mag dat gahn.", "tokens": ["up\u00b7prus\u00b7ten", "deit", ",", "denn", "mag", "dat", "gahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "VMFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Wenn b\u00e4ter he s' as Gaut un Geld", "tokens": ["Wenn", "b\u00e4\u00b7ter", "he", "s'", "as", "Gaut", "un", "Geld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "FM", "FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.24": {"text": "un h\u00f6ger as sin Wapen h\u00f6llt;", "tokens": ["un", "h\u00f6\u00b7ger", "as", "sin", "Wa\u00b7pen", "h\u00f6llt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "mit een Wurt, wenn he klauk dat makt", "tokens": ["mit", "e\u00b7en", "Wurt", ",", "wenn", "he", "klauk", "dat", "makt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KOUS", "NE", "NE", "ART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.26": {"text": "un ehr taurecht sick kost un strakt,", "tokens": ["un", "ehr", "tau\u00b7recht", "sick", "kost", "un", "strakt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "denn geiht se, denk du an min W\u00fcr!", "tokens": ["denn", "geiht", "se", ",", "denk", "du", "an", "min", "W\u00fcr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "FM", "FM", "$,", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "f\u00f6r em un mit em d\u00f6rch dat F\u00fcr.", "tokens": ["f\u00f6r", "em", "un", "mit", "em", "d\u00f6rch", "dat", "F\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.29": {"text": "Denn kriggt he dat bi ehr so gaut,", "tokens": ["Denn", "kriggt", "he", "dat", "bi", "ehr", "so", "gaut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NE", "ART", "ADJA", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "as set he Abraham in Schot.\u00ab", "tokens": ["as", "set", "he", "Ab\u00b7ra\u00b7ham", "in", "Schot", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM", "FM", "FM", "NE", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "\u00bbna, denn so m\u00fccht ick em woll raden:", "tokens": ["\u00bb", "na", ",", "denn", "so", "m\u00fccht", "ick", "em", "woll", "ra\u00b7den", ":"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "KON", "ADV", "VMFIN", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.32": {"text": "Mak jo din Gl\u00fcck nich to din'n Schaden!\u00ab", "tokens": ["Mak", "jo", "din", "Gl\u00fcck", "nich", "to", "din'n", "Scha\u00b7den", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "PPOSAT", "NN", "PTKNEG", "FM", "FM", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbun ick m\u00f6t uns' l\u00fctt Lotting seggen:", "tokens": ["\u00bb", "un", "ick", "m\u00f6t", "un\u00b7s'", "l\u00fctt", "Lot\u00b7ting", "seg\u00b7gen", ":"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.34": {"text": "Treck du em, denn ward he di trecken.\u00ab", "tokens": ["Treck", "du", "em", ",", "denn", "ward", "he", "di", "tre\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "ADJA", "$,", "KON", "VAFIN", "NE", "NE", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "\u00bbdin Junker kriggt ehr eenmal nu.", "tokens": ["\u00bb", "din", "Jun\u00b7ker", "kriggt", "ehr", "e\u00b7en\u00b7mal", "nu", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "VVFIN", "ADV", "ADV", "ADV", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.36": {"text": "Ick \u0153werst meen, 'ne rechte Fru", "tokens": ["Ick", "\u0153werst", "meen", ",", "'ne", "rech\u00b7te", "Fru"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "VVINF", "$,", "ART", "ADJA", "NN"], "meter": "----+-+", "measure": "unknown.measure.di"}, "line.37": {"text": "de lett sick nich mit F\u00f6ten pedden", "tokens": ["de", "lett", "sick", "nich", "mit", "F\u00f6\u00b7ten", "ped\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "un up keen slichtes Laken bedden.", "tokens": ["un", "up", "ke\u00b7en", "slich\u00b7tes", "La\u00b7ken", "bed\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "VVFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.39": {"text": "Un f\u00f6r den Mann dor h\u00fcrt se sick", "tokens": ["Un", "f\u00f6r", "den", "Mann", "dor", "h\u00fcrt", "se", "sick"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "ART", "NN", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "grad so as f\u00f6r de Sei\u00df de Strick.", "tokens": ["grad", "so", "as", "f\u00f6r", "de", "Sei\u00df", "de", "Strick", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Dor makt se blank un scharp em recht,", "tokens": ["Dor", "makt", "se", "blank", "un", "scharp", "em", "recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.42": {"text": "dat f\u00f6r de Aust he richtig d\u00f6cht", "tokens": ["dat", "f\u00f6r", "de", "Aust", "he", "rich\u00b7tig", "d\u00f6cht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "un sch\u00f6nes Kurn dorvon ok dreggt.\u00ab", "tokens": ["un", "sch\u00f6\u00b7nes", "Kurn", "dor\u00b7von", "ok", "dreggt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbdenn ward he dat Upstahn woll lihrn.\u00ab", "tokens": ["\u00bb", "denn", "ward", "he", "dat", "U\u