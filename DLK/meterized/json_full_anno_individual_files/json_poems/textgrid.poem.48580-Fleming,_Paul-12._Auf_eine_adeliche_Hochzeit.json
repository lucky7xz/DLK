{"textgrid.poem.48580": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "12. Auf eine adeliche Hochzeit", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zwar ich mu\u00df von fernen schauen,", "tokens": ["Zwar", "ich", "mu\u00df", "von", "fer\u00b7nen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie man euch itzund wird trauen,", "tokens": ["wie", "man", "euch", "it\u00b7zund", "wird", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wie man euch wird legen bei,", "tokens": ["wie", "man", "euch", "wird", "le\u00b7gen", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "VAFIN", "VVINF", "PTKVZ", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.4": {"text": "und den Tag der s\u00fc\u00dfen Freuden", "tokens": ["und", "den", "Tag", "der", "s\u00fc\u00b7\u00dfen", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "mu\u00df ich wider Willen meiden,", "tokens": ["mu\u00df", "ich", "wi\u00b7der", "Wil\u00b7len", "mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "weit von euch, ihr liebsten zwei.", "tokens": ["weit", "von", "euch", ",", "ihr", "liebs\u00b7ten", "zwei", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "$,", "PPER", "VVFIN", "CARD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gott und Gl\u00fccke hei\u00dft mich reisen,", "tokens": ["Gott", "und", "Gl\u00fc\u00b7cke", "hei\u00dft", "mich", "rei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "dieses, was wir hier h\u00f6r'n preisen,", "tokens": ["die\u00b7ses", ",", "was", "wir", "hier", "h\u00f6r'n", "prei\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "$,", "PRELS", "PPER", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "selbst zu sehen anderweit.", "tokens": ["selbst", "zu", "se\u00b7hen", "an\u00b7der\u00b7weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Euch hei\u00dft Gott und Gl\u00fccke freien,", "tokens": ["Euch", "hei\u00dft", "Gott", "und", "Gl\u00fc\u00b7cke", "frei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die es lassen wol gedeien,", "tokens": ["die", "es", "las\u00b7sen", "wol", "ge\u00b7dei\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "VVFIN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "was ihr Zwei Eins worden seid.", "tokens": ["was", "ihr", "Zwei", "Eins", "wor\u00b7den", "seid", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "CARD", "NN", "VAPP", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Doch soll dieses Liedlein gehen", "tokens": ["Doch", "soll", "die\u00b7ses", "Lied\u00b7lein", "ge\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PDAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "und vor eurer Tafel stehen", "tokens": ["und", "vor", "eu\u00b7rer", "Ta\u00b7fel", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "und erheben seinen Ton,", "tokens": ["und", "er\u00b7he\u00b7ben", "sei\u00b7nen", "Ton", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "den Ton, den die Libethrinnen", "tokens": ["den", "Ton", ",", "den", "die", "Li\u00b7be\u00b7th\u00b7rin\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "und die s\u00fc\u00dfen Kastalinnen", "tokens": ["und", "die", "s\u00fc\u00b7\u00dfen", "Kas\u00b7ta\u00b7lin\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "singen auf dem Helikon.", "tokens": ["sin\u00b7gen", "auf", "dem", "He\u00b7li\u00b7kon", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hymen, Venus und der Knabe,", "tokens": ["Hy\u00b7men", ",", "Ve\u00b7nus", "und", "der", "Kna\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "ART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "der die Lieben f\u00fchrt zu Grabe,", "tokens": ["der", "die", "Lie\u00b7ben", "f\u00fchrt", "zu", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "da nichts als der Tod bleibt tot,", "tokens": ["da", "nichts", "als", "der", "Tod", "bleibt", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "die begleiten sie, die Lieben,", "tokens": ["die", "be\u00b7glei\u00b7ten", "sie", ",", "die", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VVFIN", "PPER", "$,", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "die sich freuen und betr\u00fcben", "tokens": ["die", "sich", "freu\u00b7en", "und", "be\u00b7tr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRELS", "PRF", "VVINF", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00fcber der nun nahen Not.", "tokens": ["\u00fc\u00b7ber", "der", "nun", "na\u00b7hen", "Not", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Seht, sie geht, die Braut, die sch\u00f6ne!", "tokens": ["Seht", ",", "sie", "geht", ",", "die", "Braut", ",", "die", "sch\u00f6\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "$,", "ART", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die so manche Ritters-S\u00f6hne", "tokens": ["Die", "so", "man\u00b7che", "Rit\u00b7ter\u00b7sS\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "auserw\u00e4hlt, kriegt Einer nun,", "tokens": ["au\u00b7ser\u00b7w\u00e4hlt", ",", "kriegt", "Ei\u00b7ner", "nun", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PIS", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Einer, den der Himmel liebet,", "tokens": ["Ei\u00b7ner", ",", "den", "der", "Him\u00b7mel", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "der sich ihr in Lust ergiebet,", "tokens": ["der", "sich", "ihr", "in", "Lust", "er\u00b7gie\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "soll in ihren Armen ruhn.", "tokens": ["soll", "in", "ih\u00b7ren", "Ar\u00b7men", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Edles Paar an Blut und Gaben,", "tokens": ["Ed\u00b7les", "Paar", "an", "Blut", "und", "Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "nehmt nun, was ihr k\u00f6nnet haben,", "tokens": ["nehmt", "nun", ",", "was", "ihr", "k\u00f6n\u00b7net", "ha\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PWS", "PPER", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "nehmt und gebet, gebt und nehmt!", "tokens": ["nehmt", "und", "ge\u00b7bet", ",", "gebt", "und", "nehmt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Edles Paar an gleicher Tugend,", "tokens": ["Ed\u00b7les", "Paar", "an", "glei\u00b7cher", "Tu\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "pfleget eurer frischen Jugend,", "tokens": ["pfle\u00b7get", "eu\u00b7rer", "fri\u00b7schen", "Ju\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "der sich alle Lust bequemt!", "tokens": ["der", "sich", "al\u00b7le", "Lust", "be\u00b7quemt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PIAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Teilt, vermischt, vermehrt das Feuer,", "tokens": ["Teilt", ",", "ver\u00b7mischt", ",", "ver\u00b7mehrt", "das", "Feu\u00b7er", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "$,", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das der Himmel, euer Freier,", "tokens": ["das", "der", "Him\u00b7mel", ",", "eu\u00b7er", "Frei\u00b7er", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "in euch Beiden angesteckt,", "tokens": ["in", "euch", "Bei\u00b7den", "an\u00b7ge\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PIS", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "da\u00df aus dieser Liebesflamme", "tokens": ["da\u00df", "aus", "die\u00b7ser", "Lie\u00b7bes\u00b7flam\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "eurem hochgepreisten Stamme", "tokens": ["eu\u00b7rem", "hoch\u00b7ge\u00b7preis\u00b7ten", "Stam\u00b7me"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "eine neue werd' erweckt!", "tokens": ["ei\u00b7ne", "neu\u00b7e", "werd'", "er\u00b7weckt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Zwar ich mu\u00df von fernen schauen,", "tokens": ["Zwar", "ich", "mu\u00df", "von", "fer\u00b7nen", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VMFIN", "APPR", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "wie man euch itzund wird trauen,", "tokens": ["wie", "man", "euch", "it\u00b7zund", "wird", "trau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPER", "ADV", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "wie man e