{"dta.poem.9763": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Grabschrifft des Aretini. J. P.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer lieget Aretin/ der F\u00fcrsten nur gesch\u00e4ndet/", "tokens": ["Hjer", "lie\u00b7get", "A\u00b7re\u00b7tin", "/", "der", "F\u00fcrs\u00b7ten", "nur", "ge\u00b7sch\u00e4n\u00b7det", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "$(", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem seine schm\u00e4hsucht nichts als dieses grab geendet.", "tokens": ["Dem", "sei\u00b7ne", "schm\u00e4h\u00b7sucht", "nichts", "als", "die\u00b7ses", "grab", "ge\u00b7en\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "VVFIN", "PIS", "KOKOM", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nur GOtt und himmel blieb von ihm noch unvernicht/", "tokens": ["Nur", "Gott", "und", "him\u00b7mel", "blieb", "von", "ihm", "noch", "un\u00b7ver\u00b7nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verwundre dich nicht drob: Er kante beyde nicht.", "tokens": ["Ver\u00b7wund\u00b7re", "dich", "nicht", "drob", ":", "Er", "kan\u00b7te", "bey\u00b7de", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ADV", "$.", "PPER", "VMFIN", "