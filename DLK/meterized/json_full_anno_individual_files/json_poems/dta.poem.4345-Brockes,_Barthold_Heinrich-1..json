{"dta.poem.4345": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was man itzt sieht, ist alles wei\u00df,", "tokens": ["Was", "man", "itzt", "sieht", ",", "ist", "al\u00b7les", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VVFIN", "$,", "VAFIN", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So Luft, als Land deckt Schnee und Eis,", "tokens": ["So", "Luft", ",", "als", "Land", "deckt", "Schnee", "und", "Eis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dem Felsen gleichen starre Fluhten,", "tokens": ["Dem", "Fel\u00b7sen", "glei\u00b7chen", "star\u00b7re", "Fluh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Mond gleicht fast der Erden Ball,", "tokens": ["Dem", "Mond", "gleicht", "fast", "der", "Er\u00b7den", "Ball", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schollen gleichen dem Krystall,", "tokens": ["Die", "Schol\u00b7len", "glei\u00b7chen", "dem", "Krys\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die kahlen B\u00e4ume grossen Ruhten.", "tokens": ["Die", "kah\u00b7len", "B\u00e4u\u00b7me", "gros\u00b7sen", "Ruh\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Glanz der weissen Erde scheint,", "tokens": ["Der", "Glanz", "der", "weis\u00b7sen", "Er\u00b7de", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df er sich mit der Luft vereint;", "tokens": ["Da\u00df", "er", "sich", "mit", "der", "Luft", "ver\u00b7eint", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn kein Gesicht-Kreis ist zu sehen.", "tokens": ["Denn", "kein", "Ge\u00b7sicht\u00b7Kreis", "ist", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PTKZU", "VVINF", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nur leget er sich da zur Schau,", "tokens": ["Nur", "le\u00b7get", "er", "sich", "da", "zur", "Schau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wo selbst die Luft so dunkel- grau,", "tokens": ["Wo", "selbst", "die", "Luft", "so", "dun\u00b7kel", "grau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "ADV", "TRUNC", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df wir den Unterscheid verstehen.", "tokens": ["Da\u00df", "wir", "den", "Un\u00b7ter\u00b7scheid", "ver\u00b7ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie ist doch, dacht\u2019 ich, itzt die Welt", "tokens": ["Wie", "ist", "doch", ",", "dacht'", "ich", ",", "itzt", "die", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "$,", "VVFIN", "PPER", "$,", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ver\u00e4ndert, wie so sehr verstellt,", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", ",", "wie", "so", "sehr", "ver\u00b7stellt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sehr hat sie der Frost mi\u00dfhandelt,", "tokens": ["Wie", "sehr", "hat", "sie", "der", "Frost", "mi\u00df\u00b7han\u00b7delt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wohin ist ihr bebl\u00fchmtes Gr\u00fcn,", "tokens": ["Wo\u00b7hin", "ist", "ihr", "be\u00b7bl\u00fchm\u00b7tes", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und was an ihr so reizend schien,", "tokens": ["Und", "was", "an", "ihr", "so", "rei\u00b7zend", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hat sich denn die Natur verwandelt?", "tokens": ["Hat", "sich", "denn", "die", "Na\u00b7tur", "ver\u00b7wan\u00b7delt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O ja! doch nur auf kurze Zeit.", "tokens": ["O", "ja", "!", "doch", "nur", "auf", "kur\u00b7ze", "Zeit", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ITJ", "$.", "ADV", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wird bald ihre Lieblichkeit,", "tokens": ["Sie", "wird", "bald", "ih\u00b7re", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und holden Schimmer wieder kriegen.", "tokens": ["Und", "hol\u00b7den", "Schim\u00b7mer", "wie\u00b7der", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bald wird sie, mit verneuter Pracht,", "tokens": ["Bald", "wird", "sie", ",", "mit", "ver\u00b7neu\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nach der verschwundnen Winter-Nacht,", "tokens": ["Nach", "der", "ver\u00b7schwund\u00b7nen", "Win\u00b7ter\u00b7Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Am Fr\u00fchlings-Morgen uns vergn\u00fcegn.", "tokens": ["Am", "Fr\u00fch\u00b7lings\u00b7Mor\u00b7gen", "uns", "ver\u00b7gn\u00fcegn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Des Winters rauher Gegensatz", "tokens": ["Des", "Win\u00b7ters", "rau\u00b7her", "Ge\u00b7gen\u00b7satz"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Versch\u00f6nert ihren Bluhmen-Schatz,", "tokens": ["Ver\u00b7sch\u00f6\u00b7nert", "ih\u00b7ren", "Bluh\u00b7men\u00b7Schatz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wenn alles lieblich wieder gr\u00fcnet.", "tokens": ["Wenn", "al\u00b7les", "lieb\u00b7lich", "wie\u00b7der", "gr\u00fc\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie wird man denn so kl\u00e4rlich sehn,", "tokens": ["Wie", "wird", "man", "denn", "so", "kl\u00e4r\u00b7lich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PIS", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df alles, was im Frost geschehn,", "tokens": ["Da\u00df", "al\u00b7les", ",", "was", "im", "Frost", "ge\u00b7schehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "$,", "PRELS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns zu vermehrter Lust gedienet.", "tokens": ["Uns", "zu", "ver\u00b7mehr\u00b7ter", "Lust", "ge\u00b7die\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "So sehe denn doch jedermann", "tokens": ["So", "se\u00b7he", "denn", "doch", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Ordnungen des Sch\u00f6pfers an,", "tokens": ["Die", "Ord\u00b7nun\u00b7gen", "des", "Sch\u00f6p\u00b7fers", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch wenn man scharfe K\u00e4lte f\u00fchlet!", "tokens": ["Auch", "wenn", "man", "schar\u00b7fe", "K\u00e4l\u00b7te", "f\u00fch\u00b7let", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PIS", "VVFIN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erwegt, wie Gottes G\u00fctigkeit,", "tokens": ["Er\u00b7wegt", ",", "wie", "Got\u00b7tes", "G\u00fc\u00b7tig\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst in der rauhen Winter-Zeit,", "tokens": ["Selbst", "in", "der", "rau\u00b7hen", "Win\u00b7ter\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Auf unsre k\u00fcnft\u2019ge Lust gezielet.", "tokens": ["Auf", "uns\u00b7re", "k\u00fcnft'\u00b7ge", "Lust", "ge\u00b7zie\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Der Wechsel ist, was auf der Welt", "tokens": ["Der", "Wech\u00b7sel", "ist", ",", "was", "auf", "der", "Welt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Uns mehr als der Bestand gef\u00e4llt,", "tokens": ["Uns", "mehr", "als", "der", "Be\u00b7stand", "ge\u00b7f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Der Wechsel mehrt auch hier die Freude.", "tokens": ["Der", "Wech\u00b7sel", "mehrt", "auch", "hier", "die", "Freu\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Damit ein stetes Einerley", "tokens": ["Da\u00b7mit", "ein", "ste\u00b7tes", "Ei\u00b7ner\u00b7ley"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Den Sterblichen nicht widrig sey;", "tokens": ["Den", "Sterb\u00b7li\u00b7chen", "nicht", "wid\u00b7rig", "sey", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So \u00e4ndert sichs im Welt-Geb\u00e4ude.", "tokens": ["So", "\u00e4n\u00b7dert", "sichs", "im", "Welt\u00b7Ge\u00b7b\u00e4u\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Jm Winter ruhet die Natur.", "tokens": ["Jm", "Win\u00b7ter", "ru\u00b7het", "die", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hierinn ist eine neue Spur", "tokens": ["Hie\u00b7rinn", "ist", "ei\u00b7ne", "neu\u00b7e", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von weiser Vorsicht zu erblicken.", "tokens": ["Von", "wei\u00b7ser", "Vor\u00b7sicht", "zu", "er\u00b7bli\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch Ruh ersetzet sich die Kraft,", "tokens": ["Durch", "Ruh", "er\u00b7set\u00b7zet", "sich", "die", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erneut sich der ersch\u00f6pfte Saft,", "tokens": ["Er\u00b7neut", "sich", "der", "er\u00b7sch\u00f6pf\u00b7te", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Welt noch lieblicher zu schm\u00fccken.", "tokens": ["Die", "Welt", "noch", "lieb\u00b7li\u00b7cher", "zu", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "So w\u00fcrd\u2019 auch, wich der Sonnen Licht", "tokens": ["So", "w\u00fcrd'", "auch", ",", "wich", "der", "Son\u00b7nen", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "$,", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von unsern Grenzen nimmer nicht;", "tokens": ["Von", "un\u00b7sern", "Gren\u00b7zen", "nim\u00b7mer", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Theil der Welt erfrieren m\u00fcssen.", "tokens": ["Ein", "Theil", "der", "Welt", "er\u00b7frie\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So aber macht Gott ihre Gluht", "tokens": ["So", "a\u00b7ber", "macht", "Gott", "ih\u00b7re", "Gluht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "NN", "PPOSAT", "NN"], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.5": {"text": "Zu einem allgemeinen Gut", "tokens": ["Zu", "ei\u00b7nem", "all\u00b7ge\u00b7mei\u00b7nen", "Gut"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00e4\u00dft es alle Welt geniessen.", "tokens": ["Und", "l\u00e4\u00dft", "es", "al\u00b7le", "Welt", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Wie \u00fcberzeuglich, hell und klar", "tokens": ["Wie", "\u00fc\u00b7berz\u00b7eug\u00b7lich", ",", "hell", "und", "klar"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Macht selbst der Frost denn offenbar,", "tokens": ["Macht", "selbst", "der", "Frost", "denn", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie weise Gott die Welt regieret?", "tokens": ["Wie", "wei\u00b7se", "Gott", "die", "Welt", "re\u00b7gie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Es sehe denn doch jedermann", "tokens": ["Es", "se\u00b7he", "denn", "doch", "je\u00b7der\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch dieses als ein Wunder an,", "tokens": ["Auch", "die\u00b7ses", "als", "ein", "Wun\u00b7der", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "KOUS", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und preise Gott, auch wenn es frieret!", "tokens": ["Und", "prei\u00b7se", "Gott", ",", "auch", "wenn", "es", "frie\u00b7ret", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "$,", "ADV", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}