{"dta.poem.20714": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n den Herrn Ober-Consistorialrath  \n  Gedicke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was ich von Dir an Spaldings Herz geschrieben,                 ", "tokens": ["Was", "ich", "von", "Dir", "an", "Spal\u00b7dings", "Herz", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PPER", "APPR", "NE", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "War nicht Aufmunterung f\u00fcr Dich!", "tokens": ["War", "nicht", "Auf\u00b7mun\u00b7te\u00b7rung", "f\u00fcr", "Dich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Herzgef\u00fchl hat mich dazu getrieben,", "tokens": ["Mein", "Herz\u00b7ge\u00b7f\u00fchl", "hat", "mich", "da\u00b7zu", "ge\u00b7trie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "O mit Begeisterung hab\u2019 ich", "tokens": ["O", "mit", "Be\u00b7geis\u00b7te\u00b7rung", "hab'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "VAFIN", "PPER"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dein hohes Lied schon sechsmal vorgelesen,", "tokens": ["Dein", "ho\u00b7hes", "Lied", "schon", "sechs\u00b7mal", "vor\u00b7ge\u00b7le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und jedes Auge rings um mich", "tokens": ["Und", "je\u00b7des", "Au\u00b7ge", "rings", "um", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ist thr\u00e4nenvoll wie meins gewesen.", "tokens": ["Ist", "thr\u00e4\u00b7nen\u00b7voll", "wie", "meins", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KOKOM", "NE", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Du stellst den Vater Friederich", "tokens": ["Du", "stellst", "den", "Va\u00b7ter", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So liebensw\u00fcrdig vor, so pr\u00e4chtig,", "tokens": ["So", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "vor", ",", "so", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Da\u00df unsre Seele niederf\u00e4llt", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7le", "nie\u00b7der\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Und den anbetet, der ihn m\u00e4chtig", "tokens": ["Und", "den", "an\u00b7be\u00b7tet", ",", "der", "ihn", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "VVFIN", "$,", "PRELS", "PPER", "ADJD"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.12": {"text": "Hat ausger\u00fcstet in der Welt", "tokens": ["Hat", "aus\u00b7ge\u00b7r\u00fcs\u00b7tet", "in", "der", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Mit allen Gaben, die vor Alters", "tokens": ["Mit", "al\u00b7len", "Ga\u00b7ben", ",", "die", "vor", "Al\u00b7ters"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der K\u00f6nig Israels gehabt,", "tokens": ["Der", "K\u00f6\u00b7nig", "Is\u00b7raels", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.15": {"text": "Der jede Nachwelt mit den Hymnen seines Psalters", "tokens": ["Der", "je\u00b7de", "Nach\u00b7welt", "mit", "den", "Hym\u00b7nen", "sei\u00b7nes", "Psal\u00b7ters"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und seines Harfenspiels begabt,", "tokens": ["Und", "sei\u00b7nes", "Har\u00b7fen\u00b7spiels", "be\u00b7gabt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und streitbar war und bei der Landesstrafe", "tokens": ["Und", "streit\u00b7bar", "war", "und", "bei", "der", "Lan\u00b7des\u00b7stra\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "KON", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Sich tief geb\u00fcckt auf seiner Bahn,", "tokens": ["Sich", "tief", "ge\u00b7b\u00fcckt", "auf", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Gen Himmel rief und frug: \u201eWas haben diese Schafe", "tokens": ["Gen", "Him\u00b7mel", "rief", "und", "frug", ":", "\u201e", "Was", "ha\u00b7ben", "die\u00b7se", "Scha\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NN", "NN", "VVFIN", "KON", "VVFIN", "$.", "$(", "PWS", "VAFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dir Herzensk\u00fcndiger gethan?", "tokens": ["Dir", "Her\u00b7zen\u00b7sk\u00fcn\u00b7di\u00b7ger", "ge\u00b7than", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Dir, du Vergelter! dir, du R\u00e4cher!", "tokens": ["Dir", ",", "du", "Ver\u00b7gel\u00b7ter", "!", "dir", ",", "du", "R\u00e4\u00b7cher", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "NN", "$.", "PPER", "$,", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "So sprach der K\u00f6nig Israels, so spricht", "tokens": ["So", "sprach", "der", "K\u00f6\u00b7nig", "Is\u00b7raels", ",", "so", "spricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE", "$,", "ADV", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Der K\u00f6nig Friedrich, wenn ein stolzer Friedenbrecher", "tokens": ["Der", "K\u00f6\u00b7nig", "Fried\u00b7rich", ",", "wenn", "ein", "stol\u00b7zer", "Frie\u00b7den\u00b7bre\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ihn in das Feld getrotzt, wo ihm das Herze bricht", "tokens": ["Ihn", "in", "das", "Feld", "ge\u00b7trotzt", ",", "wo", "ihm", "das", "Her\u00b7ze", "bricht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,", "PWAV", "PPER", "PDS", "VVFIN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Bei so viel Menschenblutvergie\u00dfen.", "tokens": ["Bei", "so", "viel", "Men\u00b7schen\u00b7blut\u00b7ver\u00b7gie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Vielleicht geschiehts, da\u00df diesesmal", "tokens": ["Viel\u00b7leicht", "ge\u00b7schiehts", ",", "da\u00df", "die\u00b7ses\u00b7mal"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "KOUS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Sein Feind ihn um Vers\u00f6hnung wird begr\u00fc\u00dfen,", "tokens": ["Sein", "Feind", "ihn", "um", "Ver\u00b7s\u00f6h\u00b7nung", "wird", "be\u00b7gr\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPR", "NN", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Eh noch vom Blut ein B\u00f6hmerthal", "tokens": ["Eh", "noch", "vom", "Blut", "ein", "B\u00f6h\u00b7mert\u00b7hal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Wird \u00fcberschwemmt und Berge zittern", "tokens": ["Wird", "\u00fc\u00b7bersc\u00b7hwemmt", "und", "Ber\u00b7ge", "zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Vom Fall der Helden in der Schlacht,", "tokens": ["Vom", "Fall", "der", "Hel\u00b7den", "in", "der", "Schlacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wie wenn aus Gottes Ungewittern", "tokens": ["Wie", "wenn", "aus", "Got\u00b7tes", "Un\u00b7ge\u00b7wit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "APPR", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Der Bliz bald hier bald da die B\u00e4ume niederkracht,", "tokens": ["Der", "Bliz", "bald", "hier", "bald", "da", "die", "B\u00e4u\u00b7me", "nie\u00b7der\u00b7kracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADV", "ADV", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mit der Gewalt des Sturms vereinet \u2014", "tokens": ["Mit", "der", "Ge\u00b7walt", "des", "Sturms", "ver\u00b7ei\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Alsdann singst Du ein Wonnelied,", "tokens": ["Als\u00b7dann", "singst", "Du", "ein", "Won\u00b7ne\u00b7lied", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Wor\u00fcber Braut und Mutter weinet,", "tokens": ["Wo\u00b7r\u00fc\u00b7ber", "Braut", "und", "Mut\u00b7ter", "wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Die ihren Liebling wiedersieht \u2014", "tokens": ["Die", "ih\u00b7ren", "Lieb\u00b7ling", "wie\u00b7der\u00b7sieht"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}