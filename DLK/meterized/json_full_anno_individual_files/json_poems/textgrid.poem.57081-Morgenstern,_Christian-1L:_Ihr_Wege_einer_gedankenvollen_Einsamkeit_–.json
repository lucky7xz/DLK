{"textgrid.poem.57081": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ihr Wege einer gedankenvollen Einsamkeit \u2013", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr Wege einer gedankenvollen Einsamkeit \u2013", "tokens": ["Ihr", "We\u00b7ge", "ei\u00b7ner", "ge\u00b7dan\u00b7ken\u00b7vol\u00b7len", "Ein\u00b7sam\u00b7keit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "wie wandelt oft mein Fu\u00df im Traum euch wieder!", "tokens": ["wie", "wan\u00b7delt", "oft", "mein", "Fu\u00df", "im", "Traum", "euch", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "PPOSAT", "NN", "APPRART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Von neuem t\u00f6nen einst empfangne Lieder,", "tokens": ["Von", "neu\u00b7em", "t\u00f6\u00b7nen", "einst", "emp\u00b7fang\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VVFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "und meine Seele wird von Liebe weit.", "tokens": ["und", "mei\u00b7ne", "See\u00b7le", "wird", "von", "Lie\u00b7be", "weit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es eilt der Bach durch abendfeuchte Wiesen,", "tokens": ["Es", "eilt", "der", "Bach", "durch", "a\u00b7bend\u00b7feuch\u00b7te", "Wie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "die Uferb\u00fcsche regt ein herber Hauch,", "tokens": ["die", "U\u00b7fer\u00b7b\u00fc\u00b7sche", "regt", "ein", "her\u00b7ber", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "die Berge gl\u00fchn von goldnen Matten-Vliesen,", "tokens": ["die", "Ber\u00b7ge", "gl\u00fchn", "von", "gold\u00b7nen", "Mat\u00b7ten\u00b7\u00b7V\u00b7lie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "und dr\u00fcber geistert veilchenroter Rauch.", "tokens": ["und", "dr\u00fc\u00b7ber", "geis\u00b7tert", "veil\u00b7chen\u00b7ro\u00b7ter", "Rauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VVPP", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich s\u00e4ume bis zur Nacht auf dunklem Stege,", "tokens": ["Ich", "s\u00e4u\u00b7me", "bis", "zur", "Nacht", "auf", "dunk\u00b7lem", "Ste\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "des Tales volles Bild im Angesicht ...", "tokens": ["des", "Ta\u00b7les", "vol\u00b7les", "Bild", "im", "An\u00b7ge\u00b7sicht", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPRART", "NN", "$("], 