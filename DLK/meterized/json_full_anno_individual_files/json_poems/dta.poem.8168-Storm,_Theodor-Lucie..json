{"dta.poem.8168": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Lucie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.85", "nl:0.14"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Nach jener Bank dort an der Gartenwand", "tokens": ["Nach", "je\u00b7ner", "Bank", "dort", "an", "der", "Gar\u00b7ten\u00b7wand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Vom Spiel der andern Kinder sich entfernen;", "tokens": ["Vom", "Spiel", "der", "an\u00b7dern", "Kin\u00b7der", "sich", "ent\u00b7fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie wu\u00dfte wohl, es m\u00fchte sie das Lernen.", "tokens": ["Sie", "wu\u00df\u00b7te", "wohl", ",", "es", "m\u00fch\u00b7te", "sie", "das", "Ler\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VMFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht war sie klug, nicht sch\u00f6n; mir aber war", "tokens": ["Nicht", "war", "sie", "klug", ",", "nicht", "sch\u00f6n", ";", "mir", "a\u00b7ber", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "VAFIN", "PPER", "ADJD", "$,", "PTKNEG", "ADJD", "$.", "PPER", "ADV", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ihr bla\u00df Gesichtchen und ihr blondes Haar,", "tokens": ["Ihr", "bla\u00df", "Ge\u00b7sicht\u00b7chen", "und", "ihr", "blon\u00b7des", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir war es lieb; aus der Erinnrung D\u00fcster", "tokens": ["Mir", "war", "es", "lieb", ";", "aus", "der", "E\u00b7rinn\u00b7rung", "D\u00fcs\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$.", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Schaut es mich an; wir waren recht Geschwister.", "tokens": ["Schaut", "es", "mich", "an", ";", "wir", "wa\u00b7ren", "recht", "Ge\u00b7schwis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$.", "PPER", "VAFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ihr schmales Bettchen theilte sie mit mir,", "tokens": ["Ihr", "schma\u00b7les", "Bett\u00b7chen", "theil\u00b7te", "sie", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und N\u00e4chtens Wang' an Wange schliefen wir;", "tokens": ["Und", "N\u00e4ch\u00b7tens", "Wang'", "an", "Wan\u00b7ge", "schlie\u00b7fen", "wir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "APPR", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Das war so sch\u00f6n! Noch weht ein Kinderfrieden", "tokens": ["Das", "war", "so", "sch\u00f6n", "!", "Noch", "weht", "ein", "Kin\u00b7der\u00b7frie\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADV", "ADJD", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mich an aus jenen Zeiten, die geschieden.", "tokens": ["Mich", "an", "aus", "je\u00b7nen", "Zei\u00b7ten", ",", "die", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "PDAT", "NN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Ende kam, \u2014 ein Tag, sie wurde krank,", "tokens": ["Ein", "En\u00b7de", "kam", ",", "ein", "Tag", ",", "sie", "wur\u00b7de", "krank", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "$(", "ART", "NN", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und lag im Fieber viele Wochen lang;", "tokens": ["Und", "lag", "im", "Fie\u00b7ber", "vie\u00b7le", "Wo\u00b7chen", "lang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein Morgen dann, wo sanft die Winde gingen,", "tokens": ["Ein", "Mor\u00b7gen", "dann", ",", "wo", "sanft", "die", "Win\u00b7de", "gin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "PWAV", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da ging sie heim; es bl\u00fchten die Syringen.", "tokens": ["Da", "ging", "sie", "heim", ";", "es", "bl\u00fch\u00b7ten", "die", "Sy\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Die Sonne schien; ich lief in's Feld hinaus", "tokens": ["Die", "Son\u00b7ne", "schien", ";", "ich", "lief", "in's", "Feld", "hin\u00b7aus"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$.", "PPER", "VVFIN", "APPRART", "NN", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und weinte laut; dann kam ich still nach Haus.", "tokens": ["Und", "wein\u00b7te", "laut", ";", "dann", "kam", "ich", "still", "nach", "Haus", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "$.", "ADV", "VVFIN", "PPER", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wohl zwanzig Jahr, und dr\u00fcber sind vergangen \u2014", "tokens": ["Wohl", "zwan\u00b7zig", "Jahr", ",", "und", "dr\u00fc\u00b7ber", "sind", "ver\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "$,", "KON", "PAV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An wie viel Andrem hat mein Herz gehangen!", "tokens": ["An", "wie", "viel", "A\u00b7ndrem", "hat", "mein", "Herz", "ge\u00b7han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOKOM", "PIAT", "PIS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.6": {"line.1": {"text": "Was hab' ich heute denn nach dir gebangt?", "tokens": ["Was", "hab'", "ich", "heu\u00b7te", "denn", "nach", "dir", "ge\u00b7bangt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ADV", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bist du mir nah, und hast nach mir verlangt?", "tokens": ["Bist", "du", "mir", "nah", ",", "und", "hast", "nach", "mir", "ver\u00b7langt", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADJD", "$,", "KON", "VAFIN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Willst du, wie einst nach unsren Kinderspielen,", "tokens": ["Willst", "du", ",", "wie", "einst", "nach", "un\u00b7sren", "Kin\u00b7der\u00b7spie\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "$,", "PWAV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mein Knabenhaupt an deinem Herzen f\u00fchlen?", "tokens": ["Mein", "Kna\u00b7ben\u00b7haupt", "an", "dei\u00b7nem", "Her\u00b7zen", "f\u00fch\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}