{"dta.poem.19024": {"metadata": {"author": {"name": "Zinzendorf, Nicolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.   Eingang in die Schmach JEsu.   Zu Ebersdorff im Mertz.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20688-6", "language": ["de:0.99"], "booktitle": "Zinzendorf, Nicolaus Ludwig von: Teutscher Gedichte Erster Theil. Herrnhuth, 1735."}, "poem": {"stanza.1": {"line.1": {"text": "Herr JEsu! Du hast mich in deinen Schirm genommen,", "tokens": ["Herr", "Je\u00b7su", "!", "Du", "hast", "mich", "in", "dei\u00b7nen", "Schirm", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "PPER", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich darinnen stets genau verwahret seyn!", "tokens": ["La\u00df", "mich", "da\u00b7rin\u00b7nen", "stets", "ge\u00b7nau", "ver\u00b7wah\u00b7ret", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mag der Teufel selbst mit seinem Heere kommen,", "tokens": ["So", "mag", "der", "Teu\u00b7fel", "selbst", "mit", "sei\u00b7nem", "Hee\u00b7re", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er legt an meiner Ehr nur Schimpf und Schande ein.", "tokens": ["Er", "legt", "an", "mei\u00b7ner", "Ehr", "nur", "Schimpf", "und", "Schan\u00b7de", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADV", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich bin durch dich gerecht, und deine tiefe Wunden", "tokens": ["Ich", "bin", "durch", "dich", "ge\u00b7recht", ",", "und", "dei\u00b7ne", "tie\u00b7fe", "Wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "ADJD", "$,", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sind mir ein freyer Ort und eine Artzeney;", "tokens": ["Sind", "mir", "ein", "frey\u00b7er", "Ort", "und", "ei\u00b7ne", "Art\u00b7ze\u00b7ney", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den Krancken helfen sie, nicht aber den Gesunden:", "tokens": ["Den", "Kran\u00b7cken", "hel\u00b7fen", "sie", ",", "nicht", "a\u00b7ber", "den", "Ge\u00b7sun\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gib, da\u00df ich nur recht kranck nach deiner Liebe sey!", "tokens": ["Gib", ",", "da\u00df", "ich", "nur", "recht", "kranck", "nach", "dei\u00b7ner", "Lie\u00b7be", "sey", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADV", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Will mir die Welt nicht wohl; wohlan, es wird mir gehen,", "tokens": ["Will", "mir", "die", "Welt", "nicht", "wohl", ";", "wo\u00b7hlan", ",", "es", "wird", "mir", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "ADV", "$.", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wie es dem Haupte selbst vor dem ergangen ist;", "tokens": ["Wie", "es", "dem", "Haup\u00b7te", "selbst", "vor", "dem", "er\u00b7gan\u00b7gen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADV", "APPR", "ART", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verdammt mich jederman, so werd ich besser sehen,", "tokens": ["Ver\u00b7dammt", "mich", "je\u00b7der\u00b7man", ",", "so", "werd", "ich", "bes\u00b7ser", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "$,", "ADV", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Was du selbst vor ein Fluch und Scheusal worden bist.", "tokens": ["Was", "du", "selbst", "vor", "ein", "Fluch", "und", "Scheu\u00b7sal", "wor\u00b7den", "bist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "ART", "NN", "KON", "NN", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ich lege mich getrost zu deinen F\u00fcssen nieder,", "tokens": ["Ich", "le\u00b7ge", "mich", "ge\u00b7trost", "zu", "dei\u00b7nen", "F\u00fcs\u00b7sen", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVPP", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und h\u00f6re meine Pflicht aus deinem Munde an:", "tokens": ["Und", "h\u00f6\u00b7re", "mei\u00b7ne", "Pflicht", "aus", "dei\u00b7nem", "Mun\u00b7de", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du singest in der Nacht die allersch\u00f6nsten Lieder,", "tokens": ["Du", "sin\u00b7gest", "in", "der", "Nacht", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7ten", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ja einen Lobgesang, eh man dich abgethan.", "tokens": ["Ja", "ei\u00b7nen", "Lob\u00b7ge\u00b7sang", ",", "eh", "man", "dich", "ab\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ART", "NN", "$,", "KOUS", "PIS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und ich soll in der Noth nur Klage-Lieder heulen,", "tokens": ["Und", "ich", "soll", "in", "der", "Noth", "nur", "Kla\u00b7ge\u00b7Lie\u00b7der", "heu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "APPR", "ART", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ich soll bis in den Tod betr\u00fcbt zu sehen seyn:", "tokens": ["Ich", "soll", "bis", "in", "den", "Tod", "be\u00b7tr\u00fcbt", "zu", "se\u00b7hen", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VVPP", "PTKZU", "VVINF", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Das \u00fcberlasse ich der Welt und ihren Eulen,", "tokens": ["Das", "\u00fc\u00b7ber\u00b7las\u00b7se", "ich", "der", "Welt", "und", "ih\u00b7ren", "Eu\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Ich dringe mit Gedult in deinen Willen ein.", "tokens": ["Ich", "drin\u00b7ge", "mit", "Ge\u00b7dult", "in", "dei\u00b7nen", "Wil\u00b7len", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Vollkommner Prediger, der in der That erwiesen,", "tokens": ["Voll\u00b7komm\u00b7ner", "Pre\u00b7di\u00b7ger", ",", "der", "in", "der", "That", "er\u00b7wie\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Was er von dieser Kunst die Seinigen gelehrt,", "tokens": ["Was", "er", "von", "die\u00b7ser", "Kunst", "die", "Sei\u00b7ni\u00b7gen", "ge\u00b7lehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "PDAT", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Ach! w\u00fcrde doch an mir dein Ebenbild gepriesen,", "tokens": ["Ach", "!", "w\u00fcr\u00b7de", "doch", "an", "mir", "dein", "E\u00b7ben\u00b7bild", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "ADV", "APPR", "PPER", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.24": {"text": "Und mein Bek\u00e4nntni\u00df bald in Geist und Kraft verkehrt!", "tokens": ["Und", "mein", "Be\u00b7k\u00e4nnt\u00b7ni\u00df", "bald", "in", "Geist", "und", "Kraft", "ver\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Ach! zieh mich doch hinein in den geheimen Willen,", "tokens": ["Ach", "!", "zieh", "mich", "doch", "hin\u00b7ein", "in", "den", "ge\u00b7hei\u00b7men", "Wil\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Der deiner Kinder Winck und Gl\u00fcck zu nennen ist;", "tokens": ["Der", "dei\u00b7ner", "Kin\u00b7der", "Win\u00b7ck", "und", "Gl\u00fcck", "zu", "nen\u00b7nen", "ist", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "KON", "NN", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.27": {"text": "Wird sich in dessen Rath mein armes Hertze stillen;", "tokens": ["Wird", "sich", "in", "des\u00b7sen", "Rath", "mein", "ar\u00b7mes", "Hert\u00b7ze", "stil\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "APPR", "PRELAT", "NN", "PPOSAT", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "So wei\u00df ich gantz gewi\u00df, da\u00df er mich nicht vergist.", "tokens": ["So", "wei\u00df", "ich", "gantz", "ge\u00b7wi\u00df", ",", "da\u00df", "er", "mich", "nicht", "ver\u00b7gist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "$,", "KOUS", "PPER", "PRF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Du f\u00fchrst es wohl hinaus, die Ruhe folgt aufs K\u00e4mpffen,", "tokens": ["Du", "f\u00fchrst", "es", "wohl", "hin\u00b7aus", ",", "die", "Ru\u00b7he", "folgt", "aufs", "K\u00e4mpf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und werd ich im Gebet recht ernstlich und getreu,", "tokens": ["Und", "werd", "ich", "im", "Ge\u00b7bet", "recht", "ernst\u00b7lich", "und", "ge\u00b7treu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "APPRART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So wird dein Arm vor mich der Feinde Kr\u00e4ffte d\u00e4mpffen,", "tokens": ["So", "wird", "dein", "Arm", "vor", "mich", "der", "Fein\u00b7de", "Kr\u00e4ff\u00b7te", "d\u00e4mpf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPR", "PPER", "ART", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und deine G\u00fcte mir an jedem Morgen neu.", "tokens": ["Und", "dei\u00b7ne", "G\u00fc\u00b7te", "mir", "an", "je\u00b7dem", "Mor\u00b7gen", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PPER", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}