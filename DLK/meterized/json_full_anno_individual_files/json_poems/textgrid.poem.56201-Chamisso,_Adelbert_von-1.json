{"textgrid.poem.56201": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "1", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geh\u00f6rt vom Lindwurm habt ihr oft,", "tokens": ["Ge\u00b7h\u00f6rt", "vom", "Lind\u00b7wurm", "habt", "ihr", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr meine Spielgesellen,", "tokens": ["Ihr", "mei\u00b7ne", "Spiel\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun wird es wahr, was ich gehofft,", "tokens": ["Nun", "wird", "es", "wahr", ",", "was", "ich", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Drachen werd ich f\u00e4llen.", "tokens": ["Den", "Dra\u00b7chen", "werd", "ich", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er liegt gekr\u00fcmmt am dunklen Ort", "tokens": ["Er", "liegt", "ge\u00b7kr\u00fcmmt", "am", "dunk\u00b7len", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im kleinen Schrank am Spiegel dort,", "tokens": ["Im", "klei\u00b7nen", "Schrank", "am", "Spie\u00b7gel", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da hat er seine H\u00f6hle.", "tokens": ["Da", "hat", "er", "sei\u00b7ne", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ihr seid die beiden Doggen traut,", "tokens": ["Ihr", "seid", "die", "bei\u00b7den", "Dog\u00b7gen", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich zum Kampfe brauche,", "tokens": ["Die", "ich", "zum", "Kamp\u00b7fe", "brau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich treib euch an, ihr heulet laut", "tokens": ["Ich", "treib", "euch", "an", ",", "ihr", "heu\u00b7let", "laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und packt ihn unterm Bauche.", "tokens": ["Und", "packt", "ihn", "un\u00b7term", "Bau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich geh mit Schwert und Schild voran,", "tokens": ["Ich", "geh", "mit", "Schwert", "und", "Schild", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Helm und Panzer angetan,", "tokens": ["Mit", "Helm", "und", "Pan\u00b7zer", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schrei ihn aus dem Schlafe.", "tokens": ["Und", "schrei", "ihn", "aus", "dem", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hervor, hervor! du H\u00f6llenbrut!", "tokens": ["Her\u00b7vor", ",", "her\u00b7vor", "!", "du", "H\u00f6l\u00b7len\u00b7brut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da, seht den grimmen Drachen!", "tokens": ["Da", ",", "seht", "den", "grim\u00b7men", "Dra\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hu wie er Feuer speit und Blut", "tokens": ["Hu", "wie", "er", "Feu\u00b7er", "speit", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "NN", "VVFIN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Aus weit gesperrtem Rachen!", "tokens": ["Aus", "weit", "ge\u00b7sperr\u00b7tem", "Ra\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir kamen unbedachtsam nicht", "tokens": ["Wir", "ka\u00b7men", "un\u00b7be\u00b7dacht\u00b7sam", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu diesem Strau\u00df, tut eure Pflicht,", "tokens": ["Zu", "die\u00b7sem", "Strau\u00df", ",", "tut", "eu\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr meine guten Doggen.", "tokens": ["Ihr", "mei\u00b7ne", "gu\u00b7ten", "Dog\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und schnappt er gierig erst nach mir,", "tokens": ["Und", "schnappt", "er", "gie\u00b7rig", "erst", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich werd ihn listig fassen,", "tokens": ["Ich", "werd", "ihn", "lis\u00b7tig", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die aufgeh\u00e4uften B\u00fccher hier", "tokens": ["Die", "auf\u00b7ge\u00b7h\u00e4uf\u00b7ten", "B\u00fc\u00b7cher", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind schwere Felsenmassen,", "tokens": ["Sind", "schwe\u00b7re", "Fel\u00b7sen\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In seinen Rachen werf ich sie,", "tokens": ["In", "sei\u00b7nen", "Ra\u00b7chen", "werf", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du Untier, erst verschlucke die,", "tokens": ["Du", "Un\u00b7tier", ",", "erst", "ver\u00b7schlu\u00b7cke", "die", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "VVFIN", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bevor du mich kannst bei\u00dfen.", "tokens": ["Be\u00b7vor", "du", "mich", "kannst", "bei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Schlacht beginnt, wohl aufgepa\u00dft!", "tokens": ["Die", "Schlacht", "be\u00b7ginnt", ",", "wohl", "auf\u00b7ge\u00b7pa\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen Gutes hoffen;", "tokens": ["Wir", "wol\u00b7len", "Gu\u00b7tes", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er denkt: er h\u00e4lt mich schon gefa\u00dft,", "tokens": ["Er", "denkt", ":", "er", "h\u00e4lt", "mich", "schon", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein weites Maul ist offen, \u2013", "tokens": ["Sein", "wei\u00b7tes", "Maul", "ist", "of\u00b7fen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der dicke Scheller fliegt hinein,", "tokens": ["Der", "di\u00b7cke", "Schel\u00b7ler", "fliegt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die andern folgen, gro\u00df und klein,", "tokens": ["Die", "an\u00b7dern", "fol\u00b7gen", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Br\u00f6der und der Buttmann.", "tokens": ["Der", "Br\u00f6\u00b7der", "und", "der", "Butt\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "O Buttmann! o was tust du mir,", "tokens": ["O", "Butt\u00b7mann", "!", "o", "was", "tust", "du", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "FM", "PWS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du dummer, zum Verderben?!", "tokens": ["Du", "dum\u00b7mer", ",", "zum", "Ver\u00b7der\u00b7ben", "?!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du triffst den Spiegel, nicht das Tier,", "tokens": ["Du", "triffst", "den", "Spie\u00b7gel", ",", "nicht", "das", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da liegen, ach, die Scherben!", "tokens": ["Da", "lie\u00b7gen", ",", "ach", ",", "die", "Scher\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ITJ", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der dumme Spiegel nur ist Schuld,", "tokens": ["Der", "dum\u00b7me", "Spie\u00b7gel", "nur", "ist", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tragen soll ich in Geduld", "tokens": ["Und", "tra\u00b7gen", "soll", "ich", "in", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Deshalb noch viele Schl\u00e4ge.", "tokens": ["Des\u00b7halb", "noch", "vie\u00b7le", "Schl\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Das Gl\u00fcck hat feindlich sich erprobt,", "tokens": ["Das", "Gl\u00fcck", "hat", "feind\u00b7lich", "sich", "er\u00b7probt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getrost, ihr Spielgesellen!", "tokens": ["Ge\u00b7trost", ",", "ihr", "Spiel\u00b7ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich werde, wenn der Meister tobt,", "tokens": ["Ich", "wer\u00b7de", ",", "wenn", "der", "Meis\u00b7ter", "tobt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich selbst f\u00fcr alle stellen.", "tokens": ["Mich", "selbst", "f\u00fcr", "al\u00b7le", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er schlage mich nach Herzenslust,", "tokens": ["Er", "schla\u00b7ge", "mich", "nach", "Her\u00b7zens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er es kann, ist mir bewu\u00dft,", "tokens": ["Da\u00df", "er", "es", "kann", ",", "ist", "mir", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wird es so nicht dauern.", "tokens": ["Doch", "wird", "es", "so", "nicht", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ich bin auf immer nicht ein Kind,", "tokens": ["Ich", "bin", "auf", "im\u00b7mer", "nicht", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird das Blatt sich wenden,", "tokens": ["Es", "wird", "das", "Blatt", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die durch die Rute m\u00e4chtig sind,", "tokens": ["Die", "durch", "die", "Ru\u00b7te", "m\u00e4ch\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ruten werden enden.", "tokens": ["Die", "Ru\u00b7ten", "wer\u00b7den", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab als Kind den Schwur getan,", "tokens": ["Ich", "hab", "als", "Kind", "den", "Schwur", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bin ich erst erwachsner Mann,", "tokens": ["Und", "bin", "ich", "erst", "er\u00b7wachs\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann weh den Rutenf\u00fchrern!", "tokens": ["Dann", "weh", "den", "Ru\u00b7ten\u00b7f\u00fch\u00b7rern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Geh\u00f6rt vom Lindwurm habt ihr oft,", "tokens": ["Ge\u00b7h\u00f6rt", "vom", "Lind\u00b7wurm", "habt", "ihr", "oft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr meine Spielgesellen,", "tokens": ["Ihr", "mei\u00b7ne", "Spiel\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Nun wird es wahr, was ich gehofft,", "tokens": ["Nun", "wird", "es", "wahr", ",", "was", "ich", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$,", "PWS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Drachen werd ich f\u00e4llen.", "tokens": ["Den", "Dra\u00b7chen", "werd", "ich", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er liegt gekr\u00fcmmt am dunklen Ort", "tokens": ["Er", "liegt", "ge\u00b7kr\u00fcmmt", "am", "dunk\u00b7len", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "VVPP", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im kleinen Schrank am Spiegel dort,", "tokens": ["Im", "klei\u00b7nen", "Schrank", "am", "Spie\u00b7gel", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da hat er seine H\u00f6hle.", "tokens": ["Da", "hat", "er", "sei\u00b7ne", "H\u00f6h\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Ihr seid die beiden Doggen traut,", "tokens": ["Ihr", "seid", "die", "bei\u00b7den", "Dog\u00b7gen", "traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ich zum Kampfe brauche,", "tokens": ["Die", "ich", "zum", "Kamp\u00b7fe", "brau\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich treib euch an, ihr heulet laut", "tokens": ["Ich", "treib", "euch", "an", ",", "ihr", "heu\u00b7let", "laut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und packt ihn unterm Bauche.", "tokens": ["Und", "packt", "ihn", "un\u00b7term", "Bau\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich geh mit Schwert und Schild voran,", "tokens": ["Ich", "geh", "mit", "Schwert", "und", "Schild", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Helm und Panzer angetan,", "tokens": ["Mit", "Helm", "und", "Pan\u00b7zer", "an\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und schrei ihn aus dem Schlafe.", "tokens": ["Und", "schrei", "ihn", "aus", "dem", "Schla\u00b7fe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Hervor, hervor! du H\u00f6llenbrut!", "tokens": ["Her\u00b7vor", ",", "her\u00b7vor", "!", "du", "H\u00f6l\u00b7len\u00b7brut", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "PTKVZ", "$.", "PPER", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da, seht den grimmen Drachen!", "tokens": ["Da", ",", "seht", "den", "grim\u00b7men", "Dra\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Hu wie er Feuer speit und Blut", "tokens": ["Hu", "wie", "er", "Feu\u00b7er", "speit", "und", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PWAV", "PPER", "NN", "VVFIN", "KON", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Aus weit gesperrtem Rachen!", "tokens": ["Aus", "weit", "ge\u00b7sperr\u00b7tem", "Ra\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wir kamen unbedachtsam nicht", "tokens": ["Wir", "ka\u00b7men", "un\u00b7be\u00b7dacht\u00b7sam", "nicht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zu diesem Strau\u00df, tut eure Pflicht,", "tokens": ["Zu", "die\u00b7sem", "Strau\u00df", ",", "tut", "eu\u00b7re", "Pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ihr meine guten Doggen.", "tokens": ["Ihr", "mei\u00b7ne", "gu\u00b7ten", "Dog\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Und schnappt er gierig erst nach mir,", "tokens": ["Und", "schnappt", "er", "gie\u00b7rig", "erst", "nach", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ich werd ihn listig fassen,", "tokens": ["Ich", "werd", "ihn", "lis\u00b7tig", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die aufgeh\u00e4uften B\u00fccher hier", "tokens": ["Die", "auf\u00b7ge\u00b7h\u00e4uf\u00b7ten", "B\u00fc\u00b7cher", "hier"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sind schwere Felsenmassen,", "tokens": ["Sind", "schwe\u00b7re", "Fel\u00b7sen\u00b7mas\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In seinen Rachen werf ich sie,", "tokens": ["In", "sei\u00b7nen", "Ra\u00b7chen", "werf", "ich", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du Untier, erst verschlucke die,", "tokens": ["Du", "Un\u00b7tier", ",", "erst", "ver\u00b7schlu\u00b7cke", "die", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "VVFIN", "ART", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bevor du mich kannst bei\u00dfen.", "tokens": ["Be\u00b7vor", "du", "mich", "kannst", "bei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Die Schlacht beginnt, wohl aufgepa\u00dft!", "tokens": ["Die", "Schlacht", "be\u00b7ginnt", ",", "wohl", "auf\u00b7ge\u00b7pa\u00dft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir wollen Gutes hoffen;", "tokens": ["Wir", "wol\u00b7len", "Gu\u00b7tes", "hof\u00b7fen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er denkt: er h\u00e4lt mich schon gefa\u00dft,", "tokens": ["Er", "denkt", ":", "er", "h\u00e4lt", "mich", "schon", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein weites Maul ist offen, \u2013", "tokens": ["Sein", "wei\u00b7tes", "Maul", "ist", "of\u00b7fen", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADJD", "$,", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der dicke Scheller fliegt hinein,", "tokens": ["Der", "di\u00b7cke", "Schel\u00b7ler", "fliegt", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die andern folgen, gro\u00df und klein,", "tokens": ["Die", "an\u00b7dern", "fol\u00b7gen", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Br\u00f6der und der Buttmann.", "tokens": ["Der", "Br\u00f6\u00b7der", "und", "der", "Butt\u00b7mann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "O Buttmann! o was tust du mir,", "tokens": ["O", "Butt\u00b7mann", "!", "o", "was", "tust", "du", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "FM", "PWS", "VVFIN", "PPER", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du dummer, zum Verderben?!", "tokens": ["Du", "dum\u00b7mer", ",", "zum", "Ver\u00b7der\u00b7ben", "?!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du triffst den Spiegel, nicht das Tier,", "tokens": ["Du", "triffst", "den", "Spie\u00b7gel", ",", "nicht", "das", "Tier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da liegen, ach, die Scherben!", "tokens": ["Da", "lie\u00b7gen", ",", "ach", ",", "die", "Scher\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ITJ", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der dumme Spiegel nur ist Schuld,", "tokens": ["Der", "dum\u00b7me", "Spie\u00b7gel", "nur", "ist", "Schuld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tragen soll ich in Geduld", "tokens": ["Und", "tra\u00b7gen", "soll", "ich", "in", "Ge\u00b7duld"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVINF", "VMFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Deshalb noch viele Schl\u00e4ge.", "tokens": ["Des\u00b7halb", "noch", "vie\u00b7le", "Schl\u00e4\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "PIAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Das Gl\u00fcck hat feindlich sich erprobt,", "tokens": ["Das", "Gl\u00fcck", "hat", "feind\u00b7lich", "sich", "er\u00b7probt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Getrost, ihr Spielgesellen!", "tokens": ["Ge\u00b7trost", ",", "ihr", "Spiel\u00b7ge\u00b7sel\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich werde, wenn der Meister tobt,", "tokens": ["Ich", "wer\u00b7de", ",", "wenn", "der", "Meis\u00b7ter", "tobt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mich selbst f\u00fcr alle stellen.", "tokens": ["Mich", "selbst", "f\u00fcr", "al\u00b7le", "stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er schlage mich nach Herzenslust,", "tokens": ["Er", "schla\u00b7ge", "mich", "nach", "Her\u00b7zens\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er es kann, ist mir bewu\u00dft,", "tokens": ["Da\u00df", "er", "es", "kann", ",", "ist", "mir", "be\u00b7wu\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VMFIN", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch wird es so nicht dauern.", "tokens": ["Doch", "wird", "es", "so", "nicht", "dau\u00b7ern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich bin auf immer nicht ein Kind,", "tokens": ["Ich", "bin", "auf", "im\u00b7mer", "nicht", "ein", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ADV", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es wird das Blatt sich wenden,", "tokens": ["Es", "wird", "das", "Blatt", "sich", "wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die durch die Rute m\u00e4chtig sind,", "tokens": ["Die", "durch", "die", "Ru\u00b7te", "m\u00e4ch\u00b7tig", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Ruten werden enden.", "tokens": ["Die", "Ru\u00b7ten", "wer\u00b7den", "en\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich hab als Kind den Schwur getan,", "tokens": ["Ich", "hab", "als", "Kind", "den", "Schwur", "ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOUS", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bin ich erst erwachsner Mann,", "tokens": ["Und", "bin", "ich", "erst", "er\u00b7wachs\u00b7ner", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann weh den Rutenf\u00fchrern!", "tokens": ["Dann", "weh", "den", "Ru\u00b7ten\u00b7f\u00fch\u00b7rern", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}