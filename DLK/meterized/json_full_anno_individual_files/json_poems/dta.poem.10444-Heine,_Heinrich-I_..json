{"dta.poem.10444": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "I   .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Strahlte einst ein s\u00fc\u00dfes Bild;", "tokens": ["Strahl\u00b7te", "einst", "ein", "s\u00fc\u00b7\u00dfes", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Nun das s\u00fc\u00dfe Bild erblichen,", "tokens": ["Nun", "das", "s\u00fc\u00b7\u00dfe", "Bild", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Bin ich g\u00e4nzlich nachtumh\u00fcllt.", "tokens": ["Bin", "ich", "g\u00e4nz\u00b7lich", "nach\u00b7tum\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wenn die Kinder sind im Dunkeln,", "tokens": ["Wenn", "die", "Kin\u00b7der", "sind", "im", "Dun\u00b7keln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "APPRART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wird beklommen ihr Gem\u00fcth,", "tokens": ["Wird", "be\u00b7klom\u00b7men", "ihr", "Ge\u00b7m\u00fcth", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und um ihre Angst zu bannen,", "tokens": ["Und", "um", "ih\u00b7re", "Angst", "zu", "ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Singen sie ein lautes Lied.", "tokens": ["Sin\u00b7gen", "sie", "ein", "lau\u00b7tes", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich, ein tolles Kind, ich singe", "tokens": ["Ich", ",", "ein", "tol\u00b7les", "Kind", ",", "ich", "sin\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jetzo in der Dunkelheit;", "tokens": ["Jet\u00b7zo", "in", "der", "Dun\u00b7kel\u00b7heit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ist das Lied auch nicht erg\u00f6tzlich,", "tokens": ["Ist", "das", "Lied", "auch", "nicht", "er\u00b7g\u00f6tz\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat's mich doch von Angst befreit.", "tokens": ["Hat's", "mich", "doch", "von", "Angst", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}