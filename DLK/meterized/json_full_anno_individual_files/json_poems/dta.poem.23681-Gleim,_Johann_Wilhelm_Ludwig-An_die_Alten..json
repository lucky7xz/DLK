{"dta.poem.23681": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "An die Alten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-200905197682", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "V&#228;ter, st\u00f6rt uns nicht im Tanze!", "tokens": ["V", "&#228;", "ter", ",", "st\u00f6rt", "uns", "nicht", "im", "Tan\u00b7ze", "!"], "token_info": ["word", "XML_entity", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "$(", "NE", "$,", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt, und mischt euch in die Reihen,", "tokens": ["Kommt", ",", "und", "mischt", "euch", "in", "die", "Rei\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn ihr gleich mit Kr\u00fckken tanzet!", "tokens": ["Wenn", "ihr", "gleich", "mit", "Kr\u00fck\u00b7ken", "tan\u00b7zet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Tanzt, ihr V\u00e4ter, mit den T\u00f6chtern,", "tokens": ["Tanzt", ",", "ihr", "V\u00e4\u00b7ter", ",", "mit", "den", "T\u00f6ch\u00b7tern", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Geht, ihr S\u00f6hne, holt die M\u00fctter,", "tokens": ["Geht", ",", "ihr", "S\u00f6h\u00b7ne", ",", "holt", "die", "M\u00fct\u00b7ter", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Tragt sie tanzend auf den Armen,", "tokens": ["Tragt", "sie", "tan\u00b7zend", "auf", "den", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Oder la\u00dft die alten R\u00fckken", "tokens": ["O\u00b7der", "la\u00dft", "die", "al\u00b7ten", "R\u00fck\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Auf den iungeu R\u00fckken tanzen!", "tokens": ["Auf", "den", "i\u00b7un\u00b7geu", "R\u00fck\u00b7ken", "tan\u00b7zen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sch\u00fcttelt V\u00e4ter, sch\u00fcttelt M\u00fctter,", "tokens": ["Sch\u00fct\u00b7telt", "V\u00e4\u00b7ter", ",", "sch\u00fct\u00b7telt", "M\u00fct\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Da\u00df das kalte Blut erw\u00e4rme,", "tokens": ["Da\u00df", "das", "kal\u00b7te", "Blut", "er\u00b7w\u00e4r\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Da\u00df das Feuer in den Adern,", "tokens": ["Da\u00df", "das", "Feu\u00b7er", "in", "den", "A\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Noch einmal f\u00fcr Wollust brenne,", "tokens": ["Noch", "ein\u00b7mal", "f\u00fcr", "Wol\u00b7lust", "bren\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.13": {"text": "Wie es in der Jugend brannte,", "tokens": ["Wie", "es", "in", "der", "Ju\u00b7gend", "brann\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Damals, als ihr S\u00f6hne wurdet!", "tokens": ["Da\u00b7mals", ",", "als", "ihr", "S\u00f6h\u00b7ne", "wur\u00b7det", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "V\u00e4ter, f\u00fchlt die Freude wieder,", "tokens": ["V\u00e4\u00b7ter", ",", "f\u00fchlt", "die", "Freu\u00b7de", "wie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die ihr in der Jugend f\u00fchltet,", "tokens": ["Die", "ihr", "in", "der", "Ju\u00b7gend", "f\u00fchl\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Nehmt die M\u00fctter bei den H\u00e4lsen\u2019,", "tokens": ["Nehmt", "die", "M\u00fct\u00b7ter", "bei", "den", "H\u00e4lsen'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "Herzt und k\u00fc\u00dft sie, bis sie lachen!", "tokens": ["Herzt", "und", "k\u00fc\u00dft", "sie", ",", "bis", "sie", "la\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "W\u00e4lzt die Falten von der Stirne,", "tokens": ["W\u00e4lzt", "die", "Fal\u00b7ten", "von", "der", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "La\u00dft die Jugend wieder bl\u00fchen!", "tokens": ["La\u00dft", "die", "Ju\u00b7gend", "wie\u00b7der", "bl\u00fc\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Was ist besser, als die Jugend?", "tokens": ["Was", "ist", "bes\u00b7ser", ",", "als", "die", "Ju\u00b7gend", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Was ist sch\u00f6ner, als der Fr\u00fcling?", "tokens": ["Was", "ist", "sch\u00f6\u00b7ner", ",", "als", "der", "Fr\u00fc\u00b7ling", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}