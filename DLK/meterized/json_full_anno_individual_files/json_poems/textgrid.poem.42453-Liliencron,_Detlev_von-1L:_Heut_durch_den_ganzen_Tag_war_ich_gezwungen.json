{"textgrid.poem.42453": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Heut durch den ganzen Tag war ich gezwungen", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut durch den ganzen Tag war ich gezwungen", "tokens": ["Heut", "durch", "den", "gan\u00b7zen", "Tag", "war", "ich", "ge\u00b7zwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An meine ferne Heimatwelt zu denken.", "tokens": ["An", "mei\u00b7ne", "fer\u00b7ne", "Hei\u00b7mat\u00b7welt", "zu", "den\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Weit liegt sie fort, weit fort.", "tokens": ["Weit", "liegt", "sie", "fort", ",", "weit", "fort", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Die sch\u00f6ne F\u00fcrstin", "tokens": ["Die", "sch\u00f6\u00b7ne", "F\u00fcrs\u00b7tin"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Durft' ich zu Tische f\u00fchren, wo sich lebhaft", "tokens": ["Durft'", "ich", "zu", "Ti\u00b7sche", "f\u00fch\u00b7ren", ",", "wo", "sich", "leb\u00b7haft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPR", "NN", "VVINF", "$,", "PWAV", "PRF", "ADJD"], "meter": "-+-+-+-++--", "measure": "unknown.measure.penta"}, "line.6": {"text": "Gespr\u00e4ch entwickelt, Geist an Geist entz\u00fcndet.", "tokens": ["Ge\u00b7spr\u00e4ch", "ent\u00b7wi\u00b7ckelt", ",", "Geist", "an", "Geist", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "$,", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Doch immer, wie gebannt, in Red' und Antwort", "tokens": ["Doch", "im\u00b7mer", ",", "wie", "ge\u00b7bannt", ",", "in", "Red'", "und", "Ant\u00b7wort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "VVPP", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Lag mir ein einsamstilles Feld im Sinn:", "tokens": ["Lag", "mir", "ein", "ein\u00b7sam\u00b7stil\u00b7les", "Feld", "im", "Sinn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Pfl\u00fcger zieht dort Furche hinter Furche,", "tokens": ["Der", "Pfl\u00fc\u00b7ger", "zieht", "dort", "Fur\u00b7che", "hin\u00b7ter", "Fur\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Von Kr\u00e4hen nah begleitet, die ganz scheulos", "tokens": ["Von", "Kr\u00e4\u00b7hen", "nah", "be\u00b7glei\u00b7tet", ",", "die", "ganz", "scheu\u00b7los"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVPP", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Mit emsigen Schn\u00e4beln Engerlinge suchen.", "tokens": ["Mit", "em\u00b7si\u00b7gen", "Schn\u00e4\u00b7beln", "En\u00b7ger\u00b7lin\u00b7ge", "su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Der Fr\u00fchlingshimmel, wolkenlos, w\u00e4rmt schon", "tokens": ["Der", "Fr\u00fch\u00b7lings\u00b7him\u00b7mel", ",", "wol\u00b7ken\u00b7los", ",", "w\u00e4rmt", "schon"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "VAFIN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "So stark, da\u00df sich der junge Bauer bald", "tokens": ["So", "stark", ",", "da\u00df", "sich", "der", "jun\u00b7ge", "Bau\u00b7er", "bald"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PRF", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Die Jacke abgezogen hat. Und nun,", "tokens": ["Die", "Ja\u00b7cke", "ab\u00b7ge\u00b7zo\u00b7gen", "hat", ".", "Und", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "VAFIN", "$.", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Leine um die Schulter, schneidet er,", "tokens": ["Die", "Lei\u00b7ne", "um", "die", "Schul\u00b7ter", ",", "schnei\u00b7det", "er", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Den widerspenstigen Sterz fausthart umfassend,", "tokens": ["Den", "wi\u00b7der\u00b7spens\u00b7ti\u00b7gen", "Sterz", "faust\u00b7hart", "um\u00b7fas\u00b7send", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.17": {"text": "Durch's Herz das alte gute Mutterland.", "tokens": ["Durch's", "Herz", "das", "al\u00b7te", "gu\u00b7te", "Mut\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "\u00bbo nein, Sie m\u00fcssen wissen, gar nicht so,", "tokens": ["\u00bb", "o", "nein", ",", "Sie", "m\u00fcs\u00b7sen", "wis\u00b7sen", ",", "gar", "nicht", "so", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PTKANT", "$,", "PPER", "VMFIN", "VVINF", "$,", "ADV", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Wie wir uns einen Dichter vorgestellt ...\u00ab", "tokens": ["Wie", "wir", "uns", "ei\u00b7nen", "Dich\u00b7ter", "vor\u00b7ge\u00b7stellt", "...", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PPER", "PRF", "ART", "NN", "VVPP", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Was denn? Wer denn? Mir steht nur immerfort", "tokens": ["Was", "denn", "?", "Wer", "denn", "?", "Mir", "steht", "nur", "im\u00b7mer\u00b7fort"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "$.", "PWS", "ADV", "$.", "PPER", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Der junge Knecht, der brave Pflug vor Augen.", "tokens": ["Der", "jun\u00b7ge", "Knecht", ",", "der", "bra\u00b7ve", "Pflug", "vor", "Au\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "\u00bbden Landmann meinen Hoheit?\u00ab \u00bbAh, c'est dr\u00f4le ...\u00ab", "tokens": ["\u00bb", "den", "Land\u00b7mann", "mei\u00b7nen", "Ho\u00b7heit", "?", "\u00ab", "\u00bb", "Ah", ",", "c'\u00b7est", "dr\u00f4le", "...", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "PPOSAT", "NN", "$.", "$(", "$(", "NN", "$,", "FM", "FM", "$(", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Am Abend war mit lustigen K\u00fcnstlern ich", "tokens": ["Am", "A\u00b7bend", "war", "mit", "lus\u00b7ti\u00b7gen", "K\u00fcnst\u00b7lern", "ich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "APPR", "ADJA", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zusammen. L\u00e4rmend drang es in die Nacht", "tokens": ["Zu\u00b7sam\u00b7men", ".", "L\u00e4r\u00b7mend", "drang", "es", "in", "die", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Aus unsern Fenstern auf die stummen Stra\u00dfen.", "tokens": ["Aus", "un\u00b7sern", "Fens\u00b7tern", "auf", "die", "stum\u00b7men", "Stra\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ein muntres Weibsgesindel hielt mit uns.", "tokens": ["Ein", "mun\u00b7tres", "Weibs\u00b7ge\u00b7sin\u00b7del", "hielt", "mit", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir sa\u00df die schwarze Olga auf dem Scho\u00dfe.", "tokens": ["Mir", "sa\u00df", "die", "schwar\u00b7ze", "Ol\u00b7ga", "auf", "dem", "Scho\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sie f\u00e4llt mir um den Hals, sie tuschelt mir:", "tokens": ["Sie", "f\u00e4llt", "mir", "um", "den", "Hals", ",", "sie", "tu\u00b7schelt", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u00bbkomm nun, mach zu, komm, komm, wir wollen gehn ...\u00ab", "tokens": ["\u00bb", "komm", "nun", ",", "mach", "zu", ",", "komm", ",", "komm", ",", "wir", "wol\u00b7len", "gehn", "...", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "ADV", "$,", "VVFIN", "PTKVZ", "$,", "VVFIN", "$,", "VVFIN", "$,", "PPER", "VMFIN", "VVINF", "$(", "$("], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Ich aber schau' dem letzten blassen Mond,", "tokens": ["Ich", "a\u00b7ber", "schau'", "dem", "letz\u00b7ten", "blas\u00b7sen", "Mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "An ihr vor\u00fcber, in sein Traumgesicht \u2013", "tokens": ["An", "ihr", "vor\u00b7\u00fc\u00b7ber", ",", "in", "sein", "Traum\u00b7ge\u00b7sicht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Und vor mir wieder gl\u00e4nzt der Fr\u00fchlingstag:", "tokens": ["Und", "vor", "mir", "wie\u00b7der", "gl\u00e4nzt", "der", "Fr\u00fch\u00b7lings\u00b7tag", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Der Pflug, das Kr\u00e4henvolk, die schwitzenden Pferde;", "tokens": ["Der", "Pflug", ",", "das", "Kr\u00e4\u00b7hen\u00b7volk", ",", "die", "schwit\u00b7zen\u00b7den", "Pfer\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Die aufgeworf'ne Scholle, wie sie trieft!", "tokens": ["Die", "auf\u00b7ge\u00b7wor\u00b7f'\u00b7ne", "Schol\u00b7le", ",", "wie", "sie", "trieft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Abseits des Ackers liegt ein sandiger Weg,", "tokens": ["Ab\u00b7seits", "des", "A\u00b7ckers", "liegt", "ein", "san\u00b7di\u00b7ger", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+", "measure": "iambic.penta.invert"}, "line.14": {"text": "Von Knicks und W\u00e4llen rechts und links bes\u00e4umt.", "tokens": ["Von", "Knicks", "und", "W\u00e4l\u00b7len", "rechts", "und", "links", "be\u00b7s\u00e4umt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADV", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "An einer Weide dort, wo erste K\u00e4tzchen,", "tokens": ["An", "ei\u00b7ner", "Wei\u00b7de", "dort", ",", "wo", "ers\u00b7te", "K\u00e4tz\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wohl hundertb\u00fcschlig, sanft im Westwind schaukeln,", "tokens": ["Wohl", "hun\u00b7dert\u00b7b\u00fc\u00b7schlig", ",", "sanft", "im", "West\u00b7wind", "schau\u00b7keln", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Steh' in Gedanken ich, und meine Hand", "tokens": ["Steh'", "in", "Ge\u00b7dan\u00b7ken", "ich", ",", "und", "mei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "APPR", "NN", "PPER", "$,", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Greift in den Busch, da\u00df seine Bl\u00fcten st\u00e4uben.", "tokens": ["Greift", "in", "den", "Busch", ",", "da\u00df", "sei\u00b7ne", "Bl\u00fc\u00b7ten", "st\u00e4u\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "KOUS", "PPOSAT", "NN", "VVINF", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.19": {"text": "Und meine Sehnsucht dehnt die straffsten Fl\u00fcgel ...", "tokens": ["Und", "mei\u00b7ne", "Sehn\u00b7sucht", "dehnt", "die", "straffs\u00b7ten", "Fl\u00fc\u00b7gel", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Da biegst du, M\u00e4dchen pl\u00f6tzlich aus der Hecke,", "tokens": ["Da", "biegst", "du", ",", "M\u00e4d\u00b7chen", "pl\u00f6tz\u00b7lich", "aus", "der", "He\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Du, derer ich gedenke mondelang,", "tokens": ["Du", ",", "de\u00b7rer", "ich", "ge\u00b7den\u00b7ke", "mon\u00b7de\u00b7lang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.22": {"text": "Du, der ich meine ganze Seele hingab.", "tokens": ["Du", ",", "der", "ich", "mei\u00b7ne", "gan\u00b7ze", "See\u00b7le", "hin\u00b7gab", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.23": {"text": "Wie konnt' ich ahnen, hier dir zu begegnen,", "tokens": ["Wie", "konnt'", "ich", "ah\u00b7nen", ",", "hier", "dir", "zu", "be\u00b7geg\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$,", "ADV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "An dieser aller Welt entlegnen Stelle.", "tokens": ["An", "die\u00b7ser", "al\u00b7ler", "Welt", "ent\u00b7leg\u00b7nen", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Sie naht ... ich starre ... jetzt ... in gleicher H\u00f6he ...", "tokens": ["Sie", "naht", "...", "ich", "star\u00b7re", "...", "jetzt", "...", "in", "glei\u00b7cher", "H\u00f6\u00b7he", "..."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "$(", "ADV", "$(", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "\u00bbhalt an, bei Gott, halt an, ich liebe dich;", "tokens": ["\u00bb", "halt", "an", ",", "bei", "Gott", ",", "halt", "an", ",", "ich", "lie\u00b7be", "dich", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Ich wei\u00df, du bist des andern treue Braut.\u00ab", "tokens": ["Ich", "wei\u00df", ",", "du", "bist", "des", "an\u00b7dern", "treu\u00b7e", "Braut", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Und sie \u2013 h\u00e4lt an, und l\u00e4chelt: \u00bbDummer Junge,", "tokens": ["Und", "sie", "\u2013", "h\u00e4lt", "an", ",", "und", "l\u00e4\u00b7chelt", ":", "\u00bb", "Dum\u00b7mer", "Jun\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "VVFIN", "