{"textgrid.poem.57623": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Kleine Station", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Sonne br\u00fctet auf dem Stra\u00dfenstaube,", "tokens": ["Die", "Son\u00b7ne", "br\u00fc\u00b7tet", "auf", "dem", "Stra\u00b7\u00dfen\u00b7stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Sch\u00e4del br\u00fctet die Erinnerung,", "tokens": ["Im", "Sch\u00e4\u00b7del", "br\u00fc\u00b7tet", "die", "E\u00b7rin\u00b7ne\u00b7rung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Um mich die k\u00fchle Trauereschenlaube,", "tokens": ["Um", "mich", "die", "k\u00fch\u00b7le", "Trau\u00b7e\u00b7re\u00b7schen\u00b7lau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Wiesengraben t\u00f6nt es: \u00bbunk, unk, unk.\u00ab", "tokens": ["Vom", "Wie\u00b7sen\u00b7gra\u00b7ben", "t\u00f6nt", "es", ":", "\u00bb", "unk", ",", "unk", ",", "unk", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "$(", "ADJD", "$,", "ADJD", "$,", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Kellnerin entkorkt die lange Flasche,", "tokens": ["Die", "Kell\u00b7ne\u00b7rin", "ent\u00b7korkt", "die", "lan\u00b7ge", "F