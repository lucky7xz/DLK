{"textgrid.poem.60012": {"metadata": {"author": {"name": "Jacobi, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Muse, die zu blut'gen Leichen", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Muse, die zu blut'gen Leichen", "tokens": ["Die", "Mu\u00b7se", ",", "die", "zu", "blut'\u00b7gen", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Geist des Sophokles gef\u00fchrt;", "tokens": ["Den", "Geist", "des", "So\u00b7phok\u00b7les", "ge\u00b7f\u00fchrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Um ihre Stirn das Laub von Eichen,", "tokens": ["Um", "ih\u00b7re", "Stirn", "das", "Laub", "von", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Cedern, die der Blitz ger\u00fchrt;", "tokens": ["Von", "Ce\u00b7dern", ",", "die", "der", "Blitz", "ge\u00b7r\u00fchrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie zeigte Dir den nahen Ruhm", "tokens": ["Sie", "zeig\u00b7te", "Dir", "den", "na\u00b7hen", "Ruhm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit ewig dauerhaften Kr\u00e4nzen;", "tokens": ["Mit", "e\u00b7wig", "dau\u00b7er\u00b7haf\u00b7ten", "Kr\u00e4n\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du gingst, in Deinen ersten Lenzen,", "tokens": ["Du", "gingst", ",", "in", "Dei\u00b7nen", "ers\u00b7ten", "Len\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mit ihr vertraut ins Heiligthum:", "tokens": ["Mit", "ihr", "ver\u00b7traut", "ins", "Hei\u00b7lig\u00b7thum", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Da w\u00e4lzten Donnerwolken sich;", "tokens": ["Da", "w\u00e4lz\u00b7ten", "Don\u00b7ner\u00b7wol\u00b7ken", "sich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Du sahst den Dolch, der Bosheit R\u00e4cher,", "tokens": ["Du", "sahst", "den", "Dolch", ",", "der", "Bos\u00b7heit", "R\u00e4\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Du sahst den giftgef\u00fcllten Becher,", "tokens": ["Du", "sahst", "den", "gift\u00b7ge\u00b7f\u00fcll\u00b7ten", "Be\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Und Ketten rasselten um Dich;", "tokens": ["Und", "Ket\u00b7ten", "ras\u00b7sel\u00b7ten", "um", "Dich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.13": {"text": "Du sahst die bebende Natur,", "tokens": ["Du", "sahst", "die", "be\u00b7ben\u00b7de", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.14": {"text": "Voll Laster und voll Ungeheuer:", "tokens": ["Voll", "Las\u00b7ter", "und", "voll", "Un\u00b7ge\u00b7heu\u00b7er", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Mit nie gef\u00fchltem Schauder fuhr", "tokens": ["Mit", "nie", "ge\u00b7f\u00fchl\u00b7tem", "Schau\u00b7der", "fuhr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "In Dich ein allgewaltig Feuer,", "tokens": ["In", "Dich", "ein", "all\u00b7ge\u00b7wal\u00b7tig", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Das, von dem Himmel angefacht,", "tokens": ["Das", ",", "von", "dem", "Him\u00b7mel", "an\u00b7ge\u00b7facht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Zur G\u00f6ttin eine Clairon macht.", "tokens": ["Zur", "G\u00f6t\u00b7tin", "ei\u00b7ne", "Clai\u00b7ron", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Nun aber sank der finstre Schleyer,", "tokens": ["Nun", "a\u00b7ber", "sank", "der", "finst\u00b7re", "Schle\u00b7yer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+---", "measure": "unknown.measure.tri"}, "line.20": {"text": "Und Dir erschien Melpomene,", "tokens": ["Und", "Dir", "er\u00b7schien", "Mel\u00b7po\u00b7me\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Gleich einer hohen Grazie,", "tokens": ["Gleich", "ei\u00b7ner", "ho\u00b7hen", "Gra\u00b7zie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Mit jungem Lorberreis geschm\u00fcckt;", "tokens": ["Mit", "jun\u00b7gem", "Lor\u00b7berr\u00b7eis", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Kaum hatte sie Dich angeblickt,", "tokens": ["Kaum", "hat\u00b7te", "sie", "Dich", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "So lerntest Du die sch\u00f6nen Thr\u00e4nen,", "tokens": ["So", "lern\u00b7test", "Du", "die", "sch\u00f6\u00b7nen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Den s\u00fc\u00dfen Ton, das leise Sehnen", "tokens": ["Den", "s\u00fc\u00b7\u00dfen", "Ton", ",", "das", "lei\u00b7se", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Der Liebe, die voll Unschuld fleht;", "tokens": ["Der", "Lie\u00b7be", ",", "die", "voll", "Un\u00b7schuld", "fleht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und jene stille Majest\u00e4t,", "tokens": ["Und", "je\u00b7ne", "stil\u00b7le", "Ma\u00b7jes\u00b7t\u00e4t", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Womit am Throne der Tyrannen,", "tokens": ["Wo\u00b7mit", "am", "Thro\u00b7ne", "der", "Ty\u00b7ran\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Die das Verdienst in Kerker bannen,", "tokens": ["Die", "das", "Ver\u00b7dienst", "in", "Ker\u00b7ker", "ban\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.30": {"text": "Die unbesiegte Tugend steht.", "tokens": ["Die", "un\u00b7be\u00b7sieg\u00b7te", "Tu\u00b7gend", "steht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon leitet Dich, mit stolzen Schritten,", "tokens": ["Schon", "lei\u00b7tet", "Dich", ",", "mit", "stol\u00b7zen", "Schrit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unsterblichkeit an ihrer Hand.", "tokens": ["U\u00b7nsterb\u00b7lich\u00b7keit", "an", "ih\u00b7rer", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O wenn einst Oldfield", "tokens": ["O", "wenn", "einst", "Old\u00b7field"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KOUS", "ADV", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ihr Grabmaal bey Monarchen fand,", "tokens": ["Ihr", "Grab\u00b7maal", "bey", "Mon\u00b7ar\u00b7chen", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und unser kaltes Vaterland,", "tokens": ["Und", "un\u00b7ser", "kal\u00b7tes", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das im Palast ein Ordensband,", "tokens": ["Das", "im", "Pa\u00b7last", "ein", "Or\u00b7dens\u00b7band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mehr, als den gro\u00dfen Geist in H\u00fctten,", "tokens": ["Mehr", ",", "als", "den", "gro\u00b7\u00dfen", "Geist", "in", "H\u00fct\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mehr, als erhabne Werke, sch\u00e4tzt,", "tokens": ["Mehr", ",", "als", "er\u00b7hab\u00b7ne", "Wer\u00b7ke", ",", "sch\u00e4tzt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "$,", "KOUS", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht S\u00e4ulen Dir von Marmor setzt;", "tokens": ["Nicht", "S\u00e4u\u00b7len", "Dir", "von", "Mar\u00b7mor", "setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So denk: es lebet noch Dein Name,", "tokens": ["So", "denk", ":", "es", "le\u00b7bet", "noch", "Dein", "Na\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Wenn um die bald verge\u00dfne Dame,", "tokens": ["Wenn", "um", "die", "bald", "ver\u00b7ge\u00df\u00b7ne", "Da\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Die Dir ein gn\u00e4dig L\u00e4cheln giebt,", "tokens": ["Die", "Dir", "ein", "gn\u00e4\u00b7dig", "L\u00e4\u00b7cheln", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Kein sp\u00e4ter Enkel sich betr\u00fcbt.", "tokens": ["Kein", "sp\u00e4\u00b7ter", "En\u00b7kel", "sich", "be\u00b7tr\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ihr, die der Musen Chor geliebt,", "tokens": ["Ihr", ",", "die", "der", "Mu\u00b7sen", "Chor", "ge\u00b7liebt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr ruht in sch\u00f6nen Lorberhainen,", "tokens": ["Ihr", "ruht", "in", "sch\u00f6\u00b7nen", "Lor\u00b7ber\u00b7hai\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo G\u00f6tter euren Tod beweinen,", "tokens": ["Wo", "G\u00f6t\u00b7ter", "eu\u00b7ren", "Tod", "be\u00b7wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und heilig, wie ein Tempel, ist", "tokens": ["Und", "hei\u00b7lig", ",", "wie", "ein", "Tem\u00b7pel", ",", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "$,", "PWAV", "ART", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das stille Grab, das euch umschlie\u00dft.", "tokens": ["Das", "stil\u00b7le", "Grab", ",", "das", "euch", "um\u00b7schlie\u00dft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Muse, die zu blut'gen Leichen", "tokens": ["Die", "Mu\u00b7se", ",", "die", "zu", "blut'\u00b7gen", "Lei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Gei