{"dta.poem.8434": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Apoll und Dafne.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Dichter thun,", "tokens": ["Wie", "Dich\u00b7ter", "thun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Sah einst im Thal, wo Schatten k\u00fchlte,", "tokens": ["Sah", "einst", "im", "Thal", ",", "wo", "Schat\u00b7ten", "k\u00fchl\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPRART", "NN", "$,", "PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die Dafne ruhn.", "tokens": ["Die", "Daf\u00b7ne", "ruhn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NE", "VVINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Er nahte sich mit Stuzertritten,", "tokens": ["Er", "nah\u00b7te", "sich", "mit", "Stu\u00b7zer\u00b7trit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Ach und O,", "tokens": ["Mit", "Ach", "und", "O", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Als Dafne schnell mit Zefirschritten", "tokens": ["Als", "Daf\u00b7ne", "schnell", "mit", "Ze\u00b7firsc\u00b7hrit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "ADJD", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Gott entfloh.", "tokens": ["Dem", "Gott", "ent\u00b7floh", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Sie flog voran; Apollo keuchte", "tokens": ["Sie", "flog", "vo\u00b7ran", ";", "A\u00b7pol\u00b7lo", "keuch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "NE", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr hizig nach,", "tokens": ["Ihr", "hi\u00b7zig", "nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Bis er die Sch\u00f6ne fast erreichte", "tokens": ["Bis", "er", "die", "Sch\u00f6\u00b7ne", "fast", "er\u00b7reich\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Silberbach.", "tokens": ["Am", "Sil\u00b7ber\u00b7bach", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Da rief sie: Rettet mich, ihr G\u00f6tter!", "tokens": ["Da", "rief", "sie", ":", "Ret\u00b7tet", "mich", ",", "ihr", "G\u00f6t\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Th\u00f6rin die!", "tokens": ["Die", "Th\u00f6\u00b7rin", "die", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zeus winkt, und starre Lorberbl\u00e4tter", "tokens": ["Zeus", "winkt", ",", "und", "star\u00b7re", "Lor\u00b7ber\u00b7bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Umfliegen sie.", "tokens": ["Um\u00b7flie\u00b7gen", "sie", "."], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Ihr F\u00fcsschen, sonst so niedlich, wurzelt", "tokens": ["Ihr", "F\u00fcs\u00b7schen", ",", "sonst", "so", "nied\u00b7lich", ",", "wur\u00b7zelt"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADV", "ADJD", "$,", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Im Boden fest;", "tokens": ["Im", "Bo\u00b7den", "fest", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Apollo k\u00f6mmt herangepurzelt,", "tokens": ["A\u00b7pol\u00b7lo", "k\u00f6mmt", "her\u00b7an\u00b7ge\u00b7pur\u00b7zelt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schreiet: Pest!", "tokens": ["Und", "schrei\u00b7et", ":", "Pest", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Dann lehnt er seine feuchten Wangen", "tokens": ["Dann", "lehnt", "er", "sei\u00b7ne", "feuch\u00b7ten", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ans gr\u00fcne Holz:", "tokens": ["Ans", "gr\u00fc\u00b7ne", "Holz", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "J\u00fcngst eine Nimfe, sein Verlangen,", "tokens": ["J\u00fcngst", "ei\u00b7ne", "Nim\u00b7fe", ",", "sein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Nimfen Stolz!", "tokens": ["Der", "Nim\u00b7fen", "Stolz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.7": {"line.1": {"text": "Er girrt ein Weilchen, sinnt, und pfl\u00fccket", "tokens": ["Er", "girrt", "ein", "Weil\u00b7chen", ",", "sinnt", ",", "und", "pfl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sich einen Kranz\u201a", "tokens": ["Sich", "ei\u00b7nen", "Kranz", "\u201a"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Der seine blonde Scheitel schm\u00fccket", "tokens": ["Der", "sei\u00b7ne", "blon\u00b7de", "Schei\u00b7tel", "schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei Spiel und Tanz.", "tokens": ["Bei", "Spiel", "und", "Tanz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.8": {"line.1": {"text": "Du arme Dafne! Tausend pfl\u00fccken", "tokens": ["Du", "ar\u00b7me", "Daf\u00b7ne", "!", "Tau\u00b7send", "pfl\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADJA", "NE", "$.", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nun Kr\u00e4nze sich", "tokens": ["Nun", "Kr\u00e4n\u00b7ze", "sich"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "PRF"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Von deinen Haaren, sich zu schm\u00fccken!", "tokens": ["Von", "dei\u00b7nen", "Haa\u00b7ren", ",", "sich", "zu", "schm\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du daurest mich!", "tokens": ["Du", "dau\u00b7rest", "mich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Die Krieger und die Dichter zausen", "tokens": ["Die", "Krie\u00b7ger", "und", "die", "Dich\u00b7ter", "zau\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In deinem Haar,", "tokens": ["In", "dei\u00b7nem", "Haar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Wie St\u00fcrme, die den Wald durchbrausen!", "tokens": ["Wie", "St\u00fcr\u00b7me", ",", "die", "den", "Wald", "durch\u00b7brau\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die K\u00f6che gar!", "tokens": ["Die", "K\u00f6\u00b7che", "gar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Ja ja, die braunen K\u00f6che ziehen", "tokens": ["Ja", "ja", ",", "die", "brau\u00b7nen", "K\u00f6\u00b7che", "zie\u00b7hen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "PTKANT", "$,", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dir Locken aus,", "tokens": ["Dir", "Lo\u00b7cken", "aus", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Zum lieblichen Gew\u00fcrz der Br\u00fchen", "tokens": ["Zum", "lieb\u00b7li\u00b7chen", "Ge\u00b7w\u00fcrz", "der", "Br\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Beim Hochzeitsschmaus!", "tokens": ["Beim", "Hoch\u00b7zeits\u00b7schmaus", "!"], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.11": {"line.1": {"text": "Lasst, M\u00e4dchen, euch dies Beispiel r\u00fchren,", "tokens": ["Lasst", ",", "M\u00e4d\u00b7chen", ",", "euch", "dies", "Bei\u00b7spiel", "r\u00fch\u00b7ren", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "PPER", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Das Warnung spricht,", "tokens": ["Das", "War\u00b7nung", "spricht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und flieht, so lang' euch Reize zieren,", "tokens": ["Und", "flieht", ",", "so", "lang'", "euch", "Rei\u00b7ze", "zie\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADV", "ADV", "PPER", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Uns Dichter nicht!", "tokens": ["Uns", "Dich\u00b7ter", "nicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "NN", "PTKNEG", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}