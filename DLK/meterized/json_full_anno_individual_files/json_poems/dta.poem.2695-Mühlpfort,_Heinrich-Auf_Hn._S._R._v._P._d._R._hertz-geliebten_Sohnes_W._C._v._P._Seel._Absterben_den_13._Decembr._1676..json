{"dta.poem.2695": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. S. R. v. P. d. R. hertz-geliebten  \n Sohnes W. C. v. P. Seel. Absterben/ den  \n 13. Decembr. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Himmel eurer Eh' hat Finstern\u00fc\u00df bekleidet/", "tokens": ["Den", "Him\u00b7mel", "eu\u00b7rer", "Eh'", "hat", "Fins\u00b7ter\u00b7n\u00fc\u00df", "be\u00b7klei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAFIN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hochedle/ was euch jetzt bef\u00e4llt/ ist nichts als Nacht.", "tokens": ["Ho\u00b7ched\u00b7le", "/", "was", "euch", "jetzt", "be\u00b7f\u00e4llt", "/", "ist", "nichts", "als", "Nacht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "PWS", "PPER", "ADV", "VVFIN", "$(", "VAFIN", "PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach dem der liebste Sohn so fr\u00fch von hinnen scheidet/", "tokens": ["Nach", "dem", "der", "liebs\u00b7te", "Sohn", "so", "fr\u00fch", "von", "hin\u00b7nen", "schei\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "ADJA", "NN", "ADV", "ADJD", "APPR", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der sich zum Ebenbild der Ahnen schon gemacht.", "tokens": ["Der", "sich", "zum", "E\u00b7ben\u00b7bild", "der", "Ah\u00b7nen", "schon", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPRART", "NN", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn jetzt ein gantz Gestirn in Stralen-reichen Fackeln", "tokens": ["Wenn", "jetzt", "ein", "gantz", "Ge\u00b7stirn", "in", "Stra\u00b7len\u00b7rei\u00b7chen", "Fa\u00b7ckeln"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "ADV", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Schatten schwartzen Flor mit g\u00f6ldnen Funcken stickt/", "tokens": ["Der", "Schat\u00b7ten", "schwart\u00b7zen", "Flor", "mit", "g\u00f6ld\u00b7nen", "Fun\u00b7cken", "stickt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und nur ein eintzig Stern gemach f\u00e4ngt an zu wackeln/", "tokens": ["Und", "nur", "ein", "eint\u00b7zig", "Stern", "ge\u00b7mach", "f\u00e4ngt", "an", "zu", "wa\u00b7ckeln", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJD", "NN", "ADV", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bi\u00df den erkranckten Schein die Wolcke gantz erdr\u00fcckt/", "tokens": ["Bi\u00df", "den", "er\u00b7kranck\u00b7ten", "Schein", "die", "Wol\u00b7cke", "gantz", "er\u00b7dr\u00fcckt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So sehen wir betr\u00fcbt/ da\u00df so ein Liecht mu\u00df fallen/", "tokens": ["So", "se\u00b7hen", "wir", "be\u00b7tr\u00fcbt", "/", "da\u00df", "so", "ein", "Liecht", "mu\u00df", "fal\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "VVPP", "$(", "KOUS", "ADV", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Das vor der N\u00e4chte Zier und helle Leuchte war.", "tokens": ["Das", "vor", "der", "N\u00e4ch\u00b7te", "Zier", "und", "hel\u00b7le", "Leuch\u00b7te", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gar viel mehr klagen sie/ ", "tokens": ["Gar", "viel", "mehr", "kla\u00b7gen", "sie", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "VVFIN", "PPER", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Nun ihres Stammes Stern und Liecht liegt auf der Bahr.", "tokens": ["Nun", "ih\u00b7res", "Stam\u00b7mes", "Stern", "und", "Liecht", "liegt", "auf", "der", "Bahr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Denn wie die Sterne sonst den blauen Himmel zieren/", "tokens": ["Denn", "wie", "die", "Ster\u00b7ne", "sonst", "den", "blau\u00b7en", "Him\u00b7mel", "zie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So ziehrt gemeines Heil gewi\u00df der Adelstand.", "tokens": ["So", "ziehrt", "ge\u00b7mei\u00b7nes", "Heil", "ge\u00b7wi\u00df", "der", "A\u00b7del\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Ja ein Hochedles Blut pflegt Flammen bald zu sp\u00fcren", "tokens": ["Ja", "ein", "Ho\u00b7ched\u00b7les", "Blut", "pflegt", "Flam\u00b7men", "bald", "zu", "sp\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "ART", "ADJA", "NN", "VVFIN", "NN", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Die ihrem Ursprung nach dem Himmel sind verwand.", "tokens": ["Die", "ih\u00b7rem", "Ur\u00b7sprung", "nach", "dem", "Him\u00b7mel", "sind", "ver\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Drumb schien auch au\u00df dem Sohn und seiner ersten Wiegen/", "tokens": ["Drumb", "schien", "auch", "au\u00df", "dem", "Sohn", "und", "sei\u00b7ner", "ers\u00b7ten", "Wie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "APPR", "ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ein Sternen-heller Glantz/ der/ (wie der Ahnen Schild", "tokens": ["Ein", "Ster\u00b7nen\u00b7hel\u00b7ler", "Glantz", "/", "der", "/", "(", "wie", "der", "Ah\u00b7nen", "Schild"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$(", "ART", "$(", "$(", "KOKOM", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und Tapfferkeit gewohnt zu streiten und zu siegen/)", "tokens": ["Und", "Tapf\u00b7fer\u00b7keit", "ge\u00b7wohnt", "zu", "strei\u00b7ten", "und", "zu", "sie\u00b7gen", "/", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "NN", "VVPP", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$(", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Mit gleichem Helden-Muth der Eltern Wunsch erf\u00fcllt.", "tokens": ["Mit", "glei\u00b7chem", "Hel\u00b7den\u00b7Muth", "der", "El\u00b7tern", "Wunsch", "er\u00b7f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ach aber allzu fr\u00fch ist nur sein Schein vergangen/", "tokens": ["Ach", "a\u00b7ber", "all\u00b7zu", "fr\u00fch", "ist", "nur", "sein", "Schein", "ver\u00b7gan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "PTKA", "ADJD", "VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Das Hoffnungs-volle Feur wird in der Asche kalt:", "tokens": ["Das", "Hoff\u00b7nungs\u00b7vol\u00b7le", "Feur", "wird", "in", "der", "A\u00b7sche", "kalt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der ungest\u00fcme Tod bleicht seine Purpur-Wangen", "tokens": ["Der", "un\u00b7ge\u00b7st\u00fc\u00b7me", "Tod", "bleicht", "sei\u00b7ne", "Pur\u00b7pur\u00b7Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und zeigt sich/ wie er ist/ in he\u00dflicher ", "tokens": ["Und", "zeigt", "sich", "/", "wie", "er", "ist", "/", "in", "he\u00df\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "$(", "PWAV", "PPER", "VAFIN", "$(", "APPR", "ADJA"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.25": {"text": "Wer nimmt de\u00df Traurens Nacht/ de\u00df Kum\u0303ers schwartze Schatten", "tokens": ["Wer", "nimmt", "de\u00df", "Trau\u00b7rens", "Nacht", "/", "de\u00df", "Kum\u0303ers", "schwart\u00b7ze", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN", "$(", "ART", "NN", "VVFIN", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.26": {"text": "Hochadeliches Zwey/ von euren Hertzen weg?", "tokens": ["Hoc\u00b7ha\u00b7de\u00b7li\u00b7ches", "Zwey", "/", "von", "eu\u00b7ren", "Hert\u00b7zen", "weg", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Der mu\u00df eiu Felsen seyn/ der di\u00df nicht will verstatten/", "tokens": ["Der", "mu\u00df", "ei\u00b7u", "Fel\u00b7sen", "seyn", "/", "der", "di\u00df", "nicht", "will", "ver\u00b7stat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "NE", "NN", "VAINF", "$(", "ART", "PDS", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.28": {"text": "Da\u00df S\u00f6hne guter Art der Eltern Trost und Zweck.", "tokens": ["Da\u00df", "S\u00f6h\u00b7ne", "gu\u00b7ter", "Art", "der", "El\u00b7tern", "Trost", "und", "Zweck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJA", "NN", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und zwar wo allbereit ein solcher Ruhm erworben/", "tokens": ["Und", "zwar", "wo", "all\u00b7be\u00b7reit", "ein", "sol\u00b7cher", "Ruhm", "er\u00b7wor\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PWAV", "ADV", "ART", "PIAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der de\u00df ", "tokens": ["Der", "de\u00df"], "token_info": ["word", "word"], "pos": ["ART", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.31": {"text": "Vorfahren sind alsdann in Kindern nicht gestorben", "tokens": ["Vor\u00b7fah\u00b7ren", "sind", "als\u00b7dann", "in", "Kin\u00b7dern", "nicht", "ge\u00b7stor\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ADV", "APPR", "NN", "PTKNEG", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Wenn sie die Nachwelt auch von gleichem Wesen sch\u00e4tzt.", "tokens": ["Wenn", "sie", "die", "Nach\u00b7welt", "auch", "von", "glei\u00b7chem", "We\u00b7sen", "sch\u00e4tzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wolff Christoph sahe schon die Bahn von grossen Thaten", "tokens": ["Wolff", "Chris\u00b7toph", "sa\u00b7he", "schon", "die", "Bahn", "von", "gros\u00b7sen", "Tha\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Auff der die Vogtbarkeit der Jahre solte gehen:", "tokens": ["Auff", "der", "die", "Vogt\u00b7bar\u00b7keit", "der", "Jah\u00b7re", "sol\u00b7te", "ge\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Jhm kam im Spiegel f\u00fcr der Ahnen weises Rathen/", "tokens": ["Jhm", "kam", "im", "Spie\u00b7gel", "f\u00fcr", "der", "Ah\u00b7nen", "wei\u00b7ses", "Ra\u00b7then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Die K\u00e4yser Heinrich schon der Finckler wolt erh\u00f6h\u2019n.", "tokens": ["Die", "K\u00e4y\u00b7ser", "Hein\u00b7rich", "schon", "der", "Fin\u00b7ck\u00b7ler", "wolt", "er\u00b7h\u00f6h'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "NE", "ADV", "ART", "NN", "VMFIN", "VVFIN", "NE"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.37": {"text": "Solt\u2019 ich die Peiner sonst in langer Rey erzehlen/", "tokens": ["Solt'", "ich", "die", "Pei\u00b7ner", "sonst", "in", "lan\u00b7ger", "Rey", "er\u00b7zeh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So vor der F\u00fcrsten Heil mit h\u00f6chstem Ruhm gewacht/", "tokens": ["So", "vor", "der", "F\u00fcrs\u00b7ten", "Heil", "mit", "h\u00f6chs\u00b7tem", "Ruhm", "ge\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Es w\u00fcrde mir an Zeit nicht an Verdiensten fehlen/", "tokens": ["Es", "w\u00fcr\u00b7de", "mir", "an", "Zeit", "nicht", "an", "Ver\u00b7diens\u00b7ten", "feh\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Die sie bey aller Welt unsterblich l\u00e4ngst gemacht.", "tokens": ["Die", "sie", "bey", "al\u00b7ler", "Welt", "uns\u00b7terb\u00b7lich", "l\u00e4ngst", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PIAT", "NN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Denn wie\u00df der Mutter Blut ihm auch viel theure Helden", "tokens": ["Denn", "wie\u00df", "der", "Mut\u00b7ter", "Blut", "ihm", "auch", "viel", "theu\u00b7re", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NN", "PPER", "ADV", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "So selbst die Ewigkeit mit Lorber-Kronen ziehrt/", "tokens": ["So", "selbst", "die", "E\u00b7wig\u