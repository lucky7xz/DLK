{"dta.poem.4343": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Strand der Elbe im Winter  \n  1740 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was man erblickt', war alles wild,", "tokens": ["Was", "man", "er\u00b7blickt'", ",", "war", "al\u00b7les", "wild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VVFIN", "$,", "VAFIN", "PIS", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die ganze Landschaft war erf\u00fcllt", "tokens": ["Die", "gan\u00b7ze", "Land\u00b7schaft", "war", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Gegenw\u00fcrfen, voller Schrecken;", "tokens": ["Mit", "Ge\u00b7gen\u00b7w\u00fcr\u00b7fen", ",", "vol\u00b7ler", "Schre\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Doch konnte man darinn zugleich,", "tokens": ["Doch", "konn\u00b7te", "man", "da\u00b7rinn", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PAV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df sie von mancher Sch\u00f6nheit reich,", "tokens": ["Da\u00df", "sie", "von", "man\u00b7cher", "Sch\u00f6n\u00b7heit", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ja Farben, Glanz und Schmuck entdecken.", "tokens": ["Ja", "Far\u00b7ben", ",", "Glanz", "und", "Schmuck", "ent\u00b7de\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von schwehren Felsen, die zerst\u00fcckt,", "tokens": ["Von", "schweh\u00b7ren", "Fel\u00b7sen", ",", "die", "zer\u00b7st\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In nicht zu z\u00e4hlenden Gestalten,", "tokens": ["In", "nicht", "zu", "z\u00e4h\u00b7len\u00b7den", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PTKNEG", "PTKZU", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Zerdrengt, geborsten und gespalten,", "tokens": ["Zer\u00b7drengt", ",", "ge\u00b7bors\u00b7ten", "und", "ge\u00b7spal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ward der beschneite Strand gedr\u00fcckt.", "tokens": ["Ward", "der", "be\u00b7schnei\u00b7te", "Strand", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Hier drohet eine schwehre Last,", "tokens": ["Hier", "dro\u00b7het", "ei\u00b7ne", "schweh\u00b7re", "Last", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von ungeheuren Quader-St\u00fccken,", "tokens": ["Von", "un\u00b7ge\u00b7heu\u00b7ren", "Qua\u00b7der\u00b7St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So Schiff\u2019, als Bollwerk zu zerdr\u00fccken.", "tokens": ["So", "Schiff'", ",", "als", "Boll\u00b7werk", "zu", "zer\u00b7dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "KOUS", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die doch, nachdem sie einst in Rast,", "tokens": ["Die", "doch", ",", "nach\u00b7dem", "sie", "einst", "in", "Rast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "KOUS", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr mehren Drang und Anfall, ihnen", "tokens": ["F\u00fcr", "meh\u00b7ren", "Drang", "und", "An\u00b7fall", ",", "ih\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "PPER"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nunmehro selbst zur Mauer dienen.", "tokens": ["Nun\u00b7meh\u00b7ro", "selbst", "zur", "Mau\u00b7er", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier siehet alles \u00f6d\u2019 und wilde,", "tokens": ["Hier", "sie\u00b7het", "al\u00b7les", "\u00f6d'", "und", "wil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJD", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Landschaft starrt, wie Fels und Stein,", "tokens": ["Die", "Land\u00b7schaft", "starrt", ",", "wie", "Fels", "und", "Stein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da zeiget uns der Sonnenschein", "tokens": ["Da", "zei\u00b7get", "uns", "der", "Son\u00b7nen\u00b7schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein diamantenes Gefilde,", "tokens": ["Ein", "di\u00b7a\u00b7man\u00b7te\u00b7nes", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+--+---+-", "measure": "dactylic.di.plus"}, "line.5": {"text": "Wenn von den gl\u00e4nzenden Krystallen", "tokens": ["Wenn", "von", "den", "gl\u00e4n\u00b7zen\u00b7den", "Krys\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Die Blitze funkelnd r\u00fcckwerts prallen.", "tokens": ["Die", "Blit\u00b7ze", "fun\u00b7kelnd", "r\u00fcck\u00b7werts", "pral\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dort l\u00e4\u00dft, halb sch\u00f6n, halb f\u00fcrchterlich,", "tokens": ["Dort", "l\u00e4\u00dft", ",", "halb", "sch\u00f6n", ",", "halb", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "ADJD", "ADJD", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein hell-bestrahltes Eis-Feld sich,", "tokens": ["Ein", "hell\u00b7be\u00b7strahl\u00b7tes", "Eis\u00b7Feld", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "$,"], "meter": "-+-+-++-", "measure": "unknown.measure.tetra"}, "line.9": {"text": "In einem Strudel-reichen blauen", "tokens": ["In", "ei\u00b7nem", "Stru\u00b7del\u00b7rei\u00b7chen", "blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Beweglichen sapphirnen Strich,", "tokens": ["Be\u00b7weg\u00b7li\u00b7chen", "sap\u00b7phir\u00b7nen", "Strich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Und da von dunkel-blauer Fluht", "tokens": ["Und", "da", "von", "dun\u00b7kel\u00b7blau\u00b7er", "Fluht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein reger Strich, der nimmer ruht,", "tokens": ["Ein", "re\u00b7ger", "Strich", ",", "der", "nim\u00b7mer", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Jm Durchbruch weisser Berge, schauen.", "tokens": ["Jm", "Durch\u00b7bruch", "weis\u00b7ser", "Ber\u00b7ge", ",", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Hier str\u00f6hmten rege Felsen fort,", "tokens": ["Hier", "str\u00f6hm\u00b7ten", "re\u00b7ge", "Fel\u00b7sen", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erden Fl\u00e4che schien zu fliessen,", "tokens": ["Der", "Er\u00b7den", "Fl\u00e4\u00b7che", "schien", "zu", "flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erhabne schroffe Berge liessen,", "tokens": ["Er\u00b7hab\u00b7ne", "schrof\u00b7fe", "Ber\u00b7ge", "lies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als s\u00fcnken sie von ihrem Ort.", "tokens": ["Als", "s\u00fcn\u00b7ken", "sie", "von", "ih\u00b7rem", "Ort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Durchsicht\u2019ge H\u00fcgel von Krystallen", "tokens": ["Durch\u00b7sicht'\u00b7ge", "H\u00fc\u00b7gel", "von", "Krys\u00b7tal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sah man sanft hin und wieder wallen,", "tokens": ["Sah", "man", "sanft", "hin", "und", "wie\u00b7der", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADJD", "PTKVZ", "KON", "ADV", "VVINF", "$,"], "meter": "+-++-+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Und wenn darauf die Sonne schien,", "tokens": ["Und", "wenn", "da\u00b7rauf", "die", "Son\u00b7ne", "schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sah man dieselbe blitzen, gl\u00fchn,", "tokens": ["Sah", "man", "die\u00b7sel\u00b7be", "blit\u00b7zen", ",", "gl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PIS", "PDAT", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Jm bunt- gef\u00e4rbten Feur glimmen,", "tokens": ["Jm", "bunt", "ge\u00b7f\u00e4rb\u00b7ten", "Feur", "glim\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "TRUNC", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Und, zu des Blicks Erstaunen, schwimmen.", "tokens": ["Und", ",", "zu", "des", "Blicks", "Er\u00b7stau\u00b7nen", ",", "schwim\u00b7men", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Es war das Bruch- und Schiefer-Eis", "tokens": ["Es", "war", "das", "Bruch", "und", "Schie\u00b7fer\u00b7Eis"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "TRUNC", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "So gl\u00e4nzend-hell, so rein, so wei\u00df,", "tokens": ["So", "gl\u00e4n\u00b7zen\u00b7dhell", ",", "so", "rein", ",", "so", "wei\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Da\u00df es dem sch\u00f6nsten Silber gliche.", "tokens": ["Da\u00df", "es", "dem", "sch\u00f6ns\u00b7ten", "Sil\u00b7ber", "gli\u00b7che", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Wodurch es denn nicht anders lie\u00df,", "tokens": ["Wo\u00b7durch", "es", "denn", "nicht", "an\u00b7ders", "lie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Als wenn das, was vor\u00fcber schliche,", "tokens": ["Als", "wenn", "das", ",", "was", "vor\u00b7\u00fc\u00b7ber", "schli\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PDS", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Uns lauter fliessend Silber wies.", "tokens": ["Uns", "lau\u00b7ter", "flies\u00b7send", "Sil\u00b7ber", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Dort sieht man ungeheure Schollen,", "tokens": ["Dort", "sieht", "man", "un\u00b7ge\u00b7heu\u00b7re", "Schol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Wirbeln und in Strudeln, rollen,", "tokens": ["In", "Wir\u00b7beln", "und", "in", "Stru\u00b7deln", ",", "rol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wo sie, mit krachendem Get\u00f6n,", "tokens": ["Wo", "sie", ",", "mit", "kra\u00b7chen\u00b7dem", "Ge\u00b7t\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hier sinken und sich dort erh\u00f6hn.", "tokens": ["Hier", "sin\u00b7ken", "und", "sich", "dort", "er\u00b7h\u00f6hn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "PRF", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein recht gewaltig-schwehrer Drang", "tokens": ["Ein", "recht", "ge\u00b7wal\u00b7tig\u00b7schwehr\u00b7er", "Drang"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Pre\u00dft hier den sanft- und strengen Gang.", "tokens": ["Pre\u00dft", "hier", "den", "sanft", "und", "stren\u00b7gen", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "TRUNC", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein ernstes majest\u00e4tisch Regen", "tokens": ["Ein", "erns\u00b7tes", "ma\u00b7jes\u00b7t\u00e4\u00b7tisch", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Schien alles, was man fern und nah,", "tokens": ["Schien", "al\u00b7les", ",", "was", "man", "fern", "und", "nah", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PIS", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Theils angenehm, theils gre\u00dflich, sah,", "tokens": ["Theils", "an\u00b7ge\u00b7nehm", ",", "theils", "gre\u00df\u00b7lich", ",", "sah", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "ADJD", "$,", "ADV", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jm stillen Zuge, zu bewegen.", "tokens": ["Jm", "stil\u00b7len", "Zu\u00b7ge", ",", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es schwamm mein reger Blick, und glitt", "tokens": ["Es", "schwamm", "mein", "re\u00b7ger", "Blick", ",", "und", "glitt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In diesem strengen Zuge mit.", "tokens": ["In", "die\u00b7sem", "stren\u00b7gen", "Zu\u00b7ge", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Inzwischen dacht\u2019 ich der Gestalt", "tokens": ["I\u00b7nzwi\u00b7schen", "dacht'", "ich", "der", "Ge\u00b7stalt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Erden, wenn der Frost sie dr\u00fccket,", "tokens": ["Der", "Er\u00b7den", ",", "wenn", "der", "Frost", "sie", "dr\u00fc\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Bedachtsam nach, und sprach: Wie bald", "tokens": ["Be\u00b7dacht\u00b7sam", "nach", ",", "und", "sprach", ":", "Wie", "bald"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "PTKVZ", "$,", "KON", "VVFIN", "$.", "PWAV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird, was man sieht, nicht mehr erblicket!", "tokens": ["Wird", ",", "was", "man", "sieht", ",", "nicht", "mehr", "er\u00b7bli\u00b7cket", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PRELS", "PIS", "VVFIN", "$,", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie bald vergeht von der Natur", "tokens": ["Wie", "bald", "ver\u00b7geht", "von", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die itzt uns schreckende Figur!", "tokens": ["Die", "itzt", "uns", "schre\u00b7cken\u00b7de", "Fi\u00b7gur", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPER", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Sie schwindet, wie das Eis vergehet.", "tokens": ["Sie", "schwin\u00b7det", ",", "wie", "das", "Eis", "ver\u00b7ge\u00b7het", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Der Welt nat\u00fcrlich sch\u00f6nes Kleid", "tokens": ["Der", "Welt", "na\u00b7t\u00fcr\u00b7lich", "sch\u00f6\u00b7nes", "Kleid"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Zeigt bald die vor\u2019ge Lieblichkeit,", "tokens": ["Zeigt", "bald", "die", "vor'\u00b7ge", "Lieb\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "So bald ein lauer West-Wind wehet,", "tokens": ["So", "bald", "ein", "lau\u00b7er", "West\u00b7Wind", "we\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Und wenigstens zur Fr\u00fchlings-Zeit.", "tokens": ["Und", "we\u00b7nigs\u00b7tens", "zur", "Fr\u00fch\u00b7lings\u00b7Zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Inzwischen freuet sich mein Geist,", "tokens": ["I\u00b7nzwi\u00b7schen", "freu\u00b7et", "sich", "mein", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Da\u00df man auch, wenn es schneit und frieret,", "tokens": ["Da\u00df", "man", "auch", ",", "wenn", "es", "schneit", "und", "frie\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "$,", "KOUS", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch auch im Frost Vergn\u00fcgen sp\u00fchret,", "tokens": ["Doch", "auch", "im", "Frost", "Ver\u00b7gn\u00fc\u00b7gen", "sp\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mancher Winter-Lust geneu\u00dft.", "tokens": ["Und", "man\u00b7cher", "Win\u00b7ter\u00b7Lust", "ge\u00b7neu\u00dft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zumahl, wenn man dabey bedenket,", "tokens": ["Zu\u00b7mahl", ",", "wenn", "man", "da\u00b7bey", "be\u00b7den\u00b7ket", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df GOtt, auch in der Winters-Zeit,", "tokens": ["Da\u00df", "Gott", ",", "auch", "in", "der", "Win\u00b7ter\u00b7sZeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns mancherley Bequehmlichkeit,", "tokens": ["Uns", "man\u00b7cher\u00b7ley", "Be\u00b7quehm\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In Kleidern, Holz und Pelzwerk, schenket.", "tokens": ["In", "Klei\u00b7dern", ",", "Holz", "und", "Pelz\u00b7werk", ",", "schen\u00b7ket", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Doch la\u00dft uns nicht dabey vergessen", "tokens": ["Doch", "la\u00dft", "uns", "nicht", "da\u00b7bey", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "PPER", "PTKNEG", "PAV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die Noht, die manchen Armen dr\u00fcckt,", "tokens": ["Die", "Noht", ",", "die", "man\u00b7chen", "Ar\u00b7men", "dr\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", 