{"textgrid.poem.38671": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "L\u00e4ndliches Denkmal", "genre": "verse", "period": "N.A.", "pub_year": 1796, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kreuze weihte die Armuth hier zum stillen", "tokens": ["Kreu\u00b7ze", "weih\u00b7te", "die", "Ar\u00b7muth", "hier", "zum", "stil\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "ADV", "APPRART", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Denkmal h\u00e4uslicher Tugend, werth des Marmors", "tokens": ["Denk\u00b7mal", "h\u00e4us\u00b7li\u00b7cher", "Tu\u00b7gend", ",", "werth", "des", "Mar\u00b7mors"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "ADJD", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Der Triumfe verherrlicht, werth der sch\u00f6nern", "tokens": ["Der", "Tri\u00b7um\u00b7fe", "ver\u00b7herr\u00b7licht", ",", "werth", "der", "sch\u00f6\u00b7nern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "$,", "ADJD", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Z\u00e4hre der Nachwelt.", "tokens": ["Z\u00e4h\u00b7re", "der", "Nach\u00b7welt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "An der Linde des Kirchhofs winkt ein Grabstein.", "tokens": ["An", "der", "Lin\u00b7de", "des", "Kirch\u00b7hofs", "winkt", "ein", "Grab\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "ART",