{"textgrid.poem.55738": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du mu\u00dft in mir versinken,", "tokens": ["Du", "mu\u00dft", "in", "mir", "ver\u00b7sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir ganz zu eigen sein,", "tokens": ["Mir", "ganz", "zu", "ei\u00b7gen", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will Deine Seele trinken", "tokens": ["Ich", "will", "Dei\u00b7ne", "See\u00b7le", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie feuerfl\u00fcssigen Wein;", "tokens": ["Wie", "feu\u00b7er\u00b7fl\u00fcs\u00b7si\u00b7gen", "Wein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Und wenn sich zur Liebesekstase", "tokens": ["Und", "wenn", "sich", "zur", "Lie\u00b7be\u00b7seks\u00b7ta\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PRF", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Die sch\u00e4umende Wollust gesellt,", "tokens": ["Die", "sch\u00e4u\u00b7men\u00b7de", "Wol\u00b7lust", "ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.