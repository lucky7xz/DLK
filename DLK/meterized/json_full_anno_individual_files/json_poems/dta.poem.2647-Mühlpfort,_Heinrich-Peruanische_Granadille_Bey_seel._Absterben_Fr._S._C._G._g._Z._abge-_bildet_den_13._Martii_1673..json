{"dta.poem.2647": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Peruanische Granadille/  \n Bey seel. Absterben Fr. S. C. G. g. Z. abge-  \n bildet/ den 13. Martii 1673.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es scheinet ungereimt/ da\u00df ich will Blumen briugen/", "tokens": ["Es", "schei\u00b7net", "un\u00b7ge\u00b7reimt", "/", "da\u00df", "ich", "will", "Blu\u00b7men", "briu\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$(", "KOUS", "PPER", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ihr mit Asch und Staub/ ", "tokens": ["Da", "ihr", "mit", "Asch", "und", "Staub", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein Hau\u00df/ in dem sonst nichts als Todten-Glocken", "tokens": ["Ein", "Hau\u00df", "/", "in", "dem", "sonst", "nichts", "als", "Tod\u00b7ten\u00b7Glo\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "APPR", "ART", "ADV", "PIS", "KOKOM", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wird von de\u00df Fr\u00fchlings-Schmuck und Bildern nicht", "tokens": ["Wird", "von", "de\u00df", "Fr\u00fch\u00b7lings\u00b7Schmuck", "und", "Bil\u00b7dern", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "KON", "NN", "PTKNEG"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Alleine diese Blum hat di\u00df zum Eigenthume/", "tokens": ["Al\u00b7lei\u00b7ne", "die\u00b7se", "Blum", "hat", "di\u00df", "zum", "Ei\u00b7gen\u00b7thu\u00b7me", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NE", "VAFIN", "PDS", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie de\u00df ", "tokens": ["Da\u00df", "sie", "de\u00df"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "ART"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Blat/ St\u00e4ngel/ Bl\u00fcth/ und Cron zeigt was zu seinem Ruhme/", "tokens": ["Blat", "/", "St\u00e4n\u00b7gel", "/", "Bl\u00fcth", "/", "und", "Cron", "zeigt", "was", "zu", "sei\u00b7nem", "Ruh\u00b7me", "/"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$(", "KON", "NN", "VVFIN", "PIS", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "In dem es Striem und Blut zu bilden sich befleist.", "tokens": ["In", "dem", "es", "Striem", "und", "Blut", "zu", "bil\u00b7den", "sich", "be\u00b7fleist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "NN", "KON", "NN", "PTKZU", "VVINF", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Drumb last nur/ ", "tokens": ["Drumb", "last", "nur", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "$("], "meter": "+--", "measure": "dactylic.init"}, "line.10": {"text": "Weil des ", "tokens": ["Weil", "des"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Ein solches Vorbild seyn/ das eure Wunden stille/", "tokens": ["Ein", "sol\u00b7ches", "Vor\u00b7bild", "seyn", "/", "das", "eu\u00b7re", "Wun\u00b7den", "stil\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VAINF", "$(", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Ach und Seufftzen wehrt/ den bittren Thr\u00e4nen steurt.", "tokens": ["Das", "Ach", "und", "Seufft\u00b7zen", "wehrt", "/", "den", "bit\u00b7tren", "Thr\u00e4\u00b7nen", "steurt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$(", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Jhr unbeflecktes Kleid bl\u00fcht als die weissen Rosen/", "tokens": ["Ihr", "un\u00b7be\u00b7fleck\u00b7tes", "Kleid", "bl\u00fcht", "als", "die", "weis\u00b7sen", "Ro\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die h\u00f6chste Reinigkeit damit zu stellen f\u00fcr/", "tokens": ["Die", "h\u00f6chs\u00b7te", "Rei\u00b7nig\u00b7keit", "da\u00b7mit", "zu", "stel\u00b7len", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PAV", "PTKZU", "VVINF", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Weil der ", "tokens": ["Weil", "der"], "token_info": ["word", "word"], "pos": ["KOUS", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Und ein Vertilger seyn der s\u00fcndlichen Begier.", "tokens": ["Und", "ein", "Ver\u00b7til\u00b7ger", "seyn", "der", "s\u00fcnd\u00b7li\u00b7chen", "Be\u00b7gier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die in dem Umbkrei\u00df stehn mit Blut besprengte Bl\u00e4tter/", "tokens": ["Die", "in", "dem", "U\u00b7mbkrei\u00df", "stehn", "mit", "Blut", "be\u00b7spreng\u00b7te", "Bl\u00e4t\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Sind Zeugen/ da\u00df der Leib de\u00df Heylands Blut geschwitzt/", "tokens": ["Sind", "Zeu\u00b7gen", "/", "da\u00df", "der", "Leib", "de\u00df", "Hey\u00b7lands", "Blut", "ge\u00b7schwitzt", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "$(", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Als unsrer Sterbligkeit h\u00f6chst-heiliger Erretter/", "tokens": ["Als", "uns\u00b7rer", "Ster\u00b7blig\u00b7keit", "h\u00f6chst\u00b7hei\u00b7li\u00b7ger", "Er\u00b7ret\u00b7ter", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Dort am Oliven-Berg der Menschen Heil besch\u00fctzt.", "tokens": ["Dort", "am", "O\u00b7li\u00b7ven\u00b7Berg", "der", "Men\u00b7schen", "Heil", "be\u00b7sch\u00fctzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "Die drauf erwach\u00dfne Seul in der ber\u00fchmten Pflantze/", "tokens": ["Die", "drauf", "er\u00b7wach\u00df\u00b7ne", "Seul", "in", "der", "be\u00b7r\u00fchm\u00b7ten", "Pflant\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Mahlt uns die Seulen ab/ wo er gegeisselt ward/", "tokens": ["Mahlt", "uns", "die", "Seu\u00b7len", "ab", "/", "wo", "er", "ge\u00b7geis\u00b7selt", "ward", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$(", "PWAV", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Drey Aestlein so entstehn au\u00df dem geflochtnen Krantze/", "tokens": ["Drey", "A\u00b7est\u00b7lein", "so", "ent\u00b7stehn", "au\u00df", "dem", "ge\u00b7flocht\u00b7nen", "Krant\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.24": {"text": "Sind der drey N\u00e4gel Bild/ der Dornen Kronen Art.", "tokens": ["Sind", "der", "drey", "N\u00e4\u00b7gel", "Bild", "/", "der", "Dor\u00b7nen", "Kro\u00b7nen", "Art", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "CARD", "NN", "NN", "$(", "ART", "NN", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die zarten F\u00e4serlein so umb die Bl\u00e4tter spielen/", "tokens": ["Die", "zar\u00b7ten", "F\u00e4\u00b7ser\u00b7lein", "so", "umb", "die", "Bl\u00e4t\u00b7ter", "spie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die Geisseln/ so den Leib de\u00df ", "tokens": ["Die", "Geis\u00b7seln", "/", "so", "den", "Leib", "de\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "ART", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "K\u00f6nt auch was herrlichers wol die Natur erzielen/", "tokens": ["K\u00f6nt", "auch", "was", "herr\u00b7li\u00b7chers", "wol", "die", "Na\u00b7tur", "er\u00b7zie\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PWS", "ADV", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Als da\u00df sie ihre Blum mit solchem Anblick f\u00e4rbt?", "tokens": ["Als", "da\u00df", "sie", "ih\u00b7re", "Blum", "mit", "sol\u00b7chem", "An\u00b7blick", "f\u00e4rbt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "So gar mu\u00df die Natur ein Bild der Wunder werden/", "tokens": ["So", "gar", "mu\u00df", "die", "Na\u00b7tur", "ein", "Bild", "der", "Wun\u00b7der", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "ART", "NN", "ART", "NN", "ART", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Das gro\u00df Erl\u00f6sungs-Werck damit zu deuten an:", "tokens": ["Das", "gro\u00df", "Er\u00b7l\u00f6\u00b7sungs\u00b7\u00b7Werck", "da\u00b7mit", "zu", "deu\u00b7ten", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PAV", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Wie sich der Himmel hat verbunden mit der Erden/", "tokens": ["Wie", "sich", "der", "Him\u00b7mel", "hat", "ver\u00b7bun\u00b7den", "mit", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ART", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und der befreyte Mensch vor GOtt bestehen kan.", "tokens": ["Und", "der", "be\u00b7frey\u00b7te", "Mensch", "vor", "Gott", "be\u00b7ste\u00b7hen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ach solte nicht ein Christ entz\u00fcckt in Andachts-Flammen", "tokens": ["Ach", "sol\u00b7te", "nicht", "ein", "Christ", "ent\u00b7z\u00fcckt", "in", "An\u00b7dachts\u00b7Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "PTKNEG", "ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Verlangen/ da\u00df die Blumin seinem Hertzen steh\u2019/", "tokens": ["Ver\u00b7lan\u00b7gen", "/", "da\u00df", "die", "Blu\u00b7min", "sei\u00b7nem", "Hert\u00b7zen", "steh'", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "KOUS", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Da\u00df Hoffnung/ Lieb und Glaubals Bl\u00e4tter sich zusam