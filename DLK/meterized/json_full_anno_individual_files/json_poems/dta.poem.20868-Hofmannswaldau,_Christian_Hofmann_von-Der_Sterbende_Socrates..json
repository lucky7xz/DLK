{"dta.poem.20868": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Und wann sie durch den Todt sich von dem Leib erhebet/", "tokens": ["Und", "wann", "sie", "durch", "den", "Todt", "sich", "von", "dem", "Leib", "er\u00b7he\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "APPR", "ART", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So merckt man wie um sie viel Dunst der Reden klebet/", "tokens": ["So", "merckt", "man", "wie", "um", "sie", "viel", "Dunst", "der", "Re\u00b7den", "kle\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "KOKOM", "APPR", "PPER", "PIAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und in den Ort nicht kan/ der Gottes Wohnung heist/", "tokens": ["Und", "in", "den", "Ort", "nicht", "kan", "/", "der", "Got\u00b7tes", "Woh\u00b7nung", "heist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PTKNEG", "VMFIN", "$(", "ART", "NN", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Man schaut den Matten Flug hier um die Erde schweben/", "tokens": ["Man", "schaut", "den", "Mat\u00b7ten", "Flug", "hier", "um", "die", "Er\u00b7de", "schwe\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es sucht manch faules Grab ein solcher Flattergeist/", "tokens": ["Es", "sucht", "manch", "fau\u00b7les", "Grab", "ein", "sol\u00b7cher", "Flat\u00b7ter\u00b7geist", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und w\u00fcnscht den alten Leib aufs neue zu beleben.", "tokens": ["Und", "w\u00fcnscht", "den", "al\u00b7ten", "Leib", "aufs", "neu\u00b7e", "zu", "be\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wann nun der Jahre keins die letzte Zeit geb\u00fchrt/", "tokens": ["Wann", "nun", "der", "Jah\u00b7re", "keins", "die", "letz\u00b7te", "Zeit", "ge\u00b7b\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "PIAT", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das in ein neues Hau\u00df sie wieder wird gef\u00fchrt/", "tokens": ["Das", "in", "ein", "neu\u00b7es", "Hau\u00df", "sie", "wie\u00b7der", "wird", "ge\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "ADJA", "NN", "PPER", "ADV", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So mu\u00df sich ihrer Art auch ihre Wohnung gleichen.", "tokens": ["So", "mu\u00df", "sich", "ih\u00b7rer", "Art", "auch", "ih\u00b7re", "Woh\u00b7nung", "glei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PRF", "PPOSAT", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gewisser Thiere Leib mu\u00df ihre Wohnstadt seyn/", "tokens": ["Ge\u00b7wis\u00b7ser", "Thie\u00b7re", "Leib", "mu\u00df", "ih\u00b7re", "Wohn\u00b7stadt", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "VMFIN", "PPOSAT", "NN", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die grobe schauet man in einem Esel schleichen/", "tokens": ["Die", "gro\u00b7be", "schau\u00b7et", "man", "in", "ei\u00b7nem", "E\u00b7sel", "schlei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PIS", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und kommet in den Tag zu leiden Angst und Pein.", "tokens": ["Und", "kom\u00b7met", "in", "den", "Tag", "zu", "lei\u00b7den", "Angst", "und", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die so sich stets gezanckt/ und Rauberey geliebet/", "tokens": ["Die", "so", "sich", "stets", "ge\u00b7zanckt", "/", "und", "Rau\u00b7be\u00b7rey", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "ADV", "VVPP", "$(", "KON", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit falschem Hertz und Hand den Nechsten hat betr\u00fcbet/", "tokens": ["Mit", "fal\u00b7schem", "Hertz", "und", "Hand", "den", "Nechs\u00b7ten", "hat", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NN", "ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die zeigt sich wiederum in Habichts Raubgestalt/", "tokens": ["Die", "zeigt", "sich", "wie\u00b7de\u00b7rum", "in", "Ha\u00b7bichts", "Raub\u00b7ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "APPR", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie rauschet in der Lufft/ und suchet was zu fassen/", "tokens": ["Sie", "rau\u00b7schet", "in", "der", "Lufft", "/", "und", "su\u00b7chet", "was", "zu", "fas\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "KON", "VVFIN", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie l\u00e4chst nach Todten-Aa\u00df/ durch Berge/ Thal und Wald/", "tokens": ["Sie", "l\u00e4chst", "nach", "Tod\u00b7ten\u00b7Aa\u00df", "/", "durch", "Ber\u00b7ge", "/", "Thal", "und", "Wald", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$(", "APPR", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und suchet was die Hand des Schinders hat verlassen.", "tokens": ["Und", "su\u00b7chet", "was", "die", "Hand", "des", "Schin\u00b7ders", "hat", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PWS", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die sonsten nichts geliebt als Sch\u00fcssel/ Gla\u00df und Wein/", "tokens": ["Die", "sons\u00b7ten", "nichts", "ge\u00b7liebt", "als", "Sch\u00fcs\u00b7sel", "/", "Gla\u00df", "und", "Wein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIS", "VVPP", "KOKOM", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die krieget f\u00fcr ihr Hau\u00df ein Unflat-volles Schwein/", "tokens": ["Die", "krie\u00b7get", "f\u00fcr", "ihr", "Hau\u00df", "ein", "Un\u00b7flat\u00b7vol\u00b7les", "Schwein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und liebet auch allhier was sie zuvor geliebet/", "tokens": ["Und", "lie\u00b7bet", "auch", "all\u00b7hier", "was", "sie", "zu\u00b7vor", "ge\u00b7lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "PWS", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie lernet nimmer mehr was Kummer in sich h\u00e4lt/", "tokens": ["Sie", "ler\u00b7net", "nim\u00b7mer", "mehr", "was", "Kum\u00b7mer", "in", "sich", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PWS", "NN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Sie \u00fcbet itzt nichts mehr/ als was sie vor ge\u00fcbet/", "tokens": ["Sie", "\u00fc\u00b7bet", "itzt", "nichts", "mehr", "/", "als", "was", "sie", "vor", "ge\u00b7\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIS", "ADV", "$(", "KOUS", "PRELS", "PPER", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ein Abtritt lockt sie mehr als ein begr\u00fcntes Feld.", "tokens": ["Ein", "Ab\u00b7tritt", "lockt", "sie", "mehr", "als", "ein", "be\u00b7gr\u00fcn\u00b7tes", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}