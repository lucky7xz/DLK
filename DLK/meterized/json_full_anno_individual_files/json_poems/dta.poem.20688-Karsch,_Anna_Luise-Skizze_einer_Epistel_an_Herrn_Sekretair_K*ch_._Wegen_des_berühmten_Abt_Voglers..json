{"dta.poem.20688": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Skizze einer Epistel  \n an  \n  Herrn Sekretair K*ch .  Wegen des ber\u00fchmten Abt Voglers.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geheimer Sekret\u00e4r, beim hochgeliebten Bruder               ", "tokens": ["Ge\u00b7hei\u00b7mer", "Se\u00b7kre\u00b7t\u00e4r", ",", "beim", "hoch\u00b7ge\u00b7lieb\u00b7ten", "Bru\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des F\u00fcrsten, der durch Huld sich als ein F\u00fcrst beweist:", "tokens": ["Des", "F\u00fcrs\u00b7ten", ",", "der", "durch", "Huld", "sich", "als", "ein", "F\u00fcrst", "be\u00b7weist", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PRF", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich bin erfreut, da\u00df Charons Ruder", "tokens": ["Ich", "bin", "er\u00b7freut", ",", "da\u00df", "Cha\u00b7rons", "Ru\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "NE", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich nur geschreckt, da\u00df ich nicht aus der Welt gereist,", "tokens": ["Mich", "nur", "ge\u00b7schreckt", ",", "da\u00df", "ich", "nicht", "aus", "der", "Welt", "ge\u00b7reist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Eh ", "tokens": ["Eh"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Und f\u00fcr sein wundervolles Spiel", "tokens": ["Und", "f\u00fcr", "sein", "wun\u00b7der\u00b7vol\u00b7les", "Spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Das Lob der M\u00e4nner eingenommen,", "tokens": ["Das", "Lob", "der", "M\u00e4n\u00b7ner", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und Weiberk\u00fcsse viel, sehr viel!", "tokens": ["Und", "Wei\u00b7ber\u00b7k\u00fcs\u00b7se", "viel", ",", "sehr", "viel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich selber gab auf beyde Wangen", "tokens": ["Ich", "sel\u00b7ber", "gab", "auf", "bey\u00b7de", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Ihm Einen Musenku\u00df daf\u00fcr,", "tokens": ["Ihm", "Ei\u00b7nen", "Mu\u00b7sen\u00b7ku\u00df", "da\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da\u00df Er im Tempel ist gegangen,", "tokens": ["Da\u00df", "Er", "im", "Tem\u00b7pel", "ist", "ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Wo der Soldat und Officier", "tokens": ["Wo", "der", "Sol\u00b7dat", "und", "Of\u00b7fi\u00b7cier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Auf Kanzelrede merken m\u00fcssen,", "tokens": ["Auf", "Kan\u00b7zel\u00b7re\u00b7de", "mer\u00b7ken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Da hat Dein Vogler so gespielt,", "tokens": ["Da", "hat", "Dein", "Vog\u00b7ler", "so", "ge\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Da\u00df alle Seelen hingerissen", "tokens": ["Da\u00df", "al\u00b7le", "See\u00b7len", "hin\u00b7ge\u00b7ris\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Der Orgel Majest\u00e4t gef\u00fchlt \u2014", "tokens": ["Der", "Or\u00b7gel", "Ma\u00b7jes\u00b7t\u00e4t", "ge\u00b7f\u00fchlt"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Und droben unterm Himmels-Chore", "tokens": ["Und", "dro\u00b7ben", "un\u00b7term", "Him\u00b7mels\u00b7Cho\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Hat selbst C\u00e4cilia sich halb herabgeneigt,", "tokens": ["Hat", "selbst", "C\u00e4\u00b7ci\u00b7lia", "sich", "halb", "her\u00b7ab\u00b7ge\u00b7neigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NE", "PRF", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.19": {"text": "Und Ihn behorcht mit Einem Ohre,", "tokens": ["Und", "Ihn", "be\u00b7horcht", "mit", "Ei\u00b7nem", "Oh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Und ihren Schwestern angezeigt,", "tokens": ["Und", "ih\u00b7ren", "Schwes\u00b7tern", "an\u00b7ge\u00b7zeigt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Da\u00df Vogler doppelt Ruhm verdiente;", "tokens": ["Da\u00df", "Vog\u00b7ler", "dop\u00b7pelt", "Ruhm", "ver\u00b7dien\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Zuerst von wegen seiner Kunst,", "tokens": ["Zu\u00b7erst", "von", "we\u00b7gen", "sei\u00b7ner", "Kunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die sich des h\u00f6chsten Flugs erk\u00fchnte;", "tokens": ["Die", "sich", "des", "h\u00f6chs\u00b7ten", "Flugs", "er\u00b7k\u00fchn\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Dann wegen seiner frommen Busenbrunst,", "tokens": ["Dann", "we\u00b7gen", "sei\u00b7ner", "from\u00b7men", "Bu\u00b7sen\u00b7brunst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Ein armes V\u00f6lklein zu erquicken,", "tokens": ["Ein", "ar\u00b7mes", "V\u00f6l\u00b7klein", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Das bei der Wassersnoth verarmt", "tokens": ["Das", "bei", "der", "Was\u00b7sers\u00b7noth", "ver\u00b7armt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Und sterben mu\u00df vom Hungerdr\u00fccken,", "tokens": ["Und", "ster\u00b7ben", "mu\u00df", "vom", "Hun\u00b7ger\u00b7dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wenn sich das Mitleid nicht erbarmt.", "tokens": ["Wenn", "sich", "das", "Mit\u00b7leid", "nicht", "er\u00b7barmt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Das Mitleid ", "tokens": ["Das", "Mit\u00b7leid"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.30": {"text": "Im Tempel unsrer Garnison,", "tokens": ["Im", "Tem\u00b7pel", "uns\u00b7rer", "Gar\u00b7ni\u00b7son", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Da ward er laut gelobt von Vielen;", "tokens": ["Da", "ward", "er", "laut", "ge\u00b7lobt", "von", "Vie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "VVPP", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Doch seiner M\u00fche sch\u00f6nster Lohn", "tokens": ["Doch", "sei\u00b7ner", "M\u00fc\u00b7he", "sch\u00f6ns\u00b7ter", "Lohn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.33": {"text": "War\u2019s Opfer f\u00fcr die Mangelklager,", "tokens": ["Wa\u00b7r's", "Op\u00b7fer", "f\u00fcr", "die", "Man\u00b7gel\u00b7kla\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.34": {"text": "War aber nicht, wie er\u2019s gehofft,", "tokens": ["War", "a\u00b7ber", "nicht", ",", "wie", "er's", "ge\u00b7hofft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PTKNEG", "$,", "PWAV", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "War seinem Wunsche viel zu mager \u2014", "tokens": ["War", "sei\u00b7nem", "Wun\u00b7sche", "viel", "zu", "ma\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.36": {"text": "Das Gabenheischen k\u00f6mmt zu oft.", "tokens": ["Das", "Ga\u00b7ben\u00b7hei\u00b7schen", "k\u00f6mmt", "zu", "oft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Selbst F\u00fcrstent\u00f6chter \u00fcbertrafen", "tokens": ["Selbst", "F\u00fcrs\u00b7ten\u00b7t\u00f6ch\u00b7ter", "\u00fc\u00b7bert\u00b7ra\u00b7fen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Die b\u00fcrgerlichen T\u00f6chter nicht,", "tokens": ["Die", "b\u00fcr\u00b7ger\u00b7li\u00b7chen", "T\u00f6ch\u00b7ter", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Die unter niederm Dache schlafen \u2014", "tokens": ["Die", "un\u00b7ter", "nie\u00b7derm", "Da\u00b7che", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Zwar Voglers sch\u00f6ne Menschenpflicht", "tokens": ["Zwar", "Vog\u00b7lers", "sch\u00f6\u00b7ne", "Men\u00b7schen\u00b7pflicht"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "War ganz erf\u00fcllt, war sehr zu preisen,", "tokens": ["War", "ganz", "er\u00b7f\u00fcllt", ",", "war", "sehr", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "VVPP", "$,", "VAFIN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Und eigenm\u00fcndig wird er\u2019s Dir", "tokens": ["Und", "ei\u00b7gen\u00b7m\u00fcn\u00b7dig", "wird", "er's", "Dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VAFIN", "PIS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Erz\u00e4hlen, wenn er seine Reisen", "tokens": ["Er\u00b7z\u00e4h\u00b7len", ",", "wenn", "er", "sei\u00b7ne", "Rei\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.44": {"text": "Beschreibt, und Du voll Neubegier", "tokens": ["Be\u00b7schreibt", ",", "und", "Du", "voll", "Neu\u00b7be\u00b7gier"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "KON", "PPER", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Ihm gegen\u00fcber, ihm zur Seite", "tokens": ["Ihm", "ge\u00b7gen\u00b7\u00fc\u00b7ber", ",", "ihm", "zur", "Sei\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKVZ", "$,", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Die Rede seines Mundes trinkst,", "tokens": ["Die", "Re\u00b7de", "sei\u00b7nes", "Mun\u00b7des", "trinkst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Und oft, nach Art verliebter Leute,", "tokens": ["Und", "oft", ",", "nach", "Art", "ver\u00b7lieb\u00b7ter", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Mit Funkelaugen Beifall blinkst.", "tokens": ["Mit", "Fun\u00b7ke\u00b7lau\u00b7gen", "Bei\u00b7fall", "blinkst", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Dann la\u00df Dir auch von mir noch sagen:", "tokens": ["Dann", "la\u00df", "Dir", "auch", "von", "mir", "noch", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "APPR", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "Da\u00df ich, ein Weib mit deutschem Sinn,", "tokens": ["Da\u00df", "ich", ",", "ein", "Weib", "mit", "deut\u00b7schem", "Sinn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Troz meinen altgewordnen Tagen,", "tokens": ["Troz", "mei\u00b7nen", "alt\u00b7ge\u00b7word\u00b7nen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.52": {"text": "Noch munter und gar freudig bin.", "tokens": ["Noch", "mun\u00b7ter", "und", "gar", "freu\u00b7dig", "bin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}