{"dta.poem.19200": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "47.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Siehst du, wie der Planet sich um sich selber dreht,", "tokens": ["Siehst", "du", ",", "wie", "der", "Pla\u00b7net", "sich", "um", "sich", "sel\u00b7ber", "dreht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "PRF", "APPR", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Und still dabei im Kreis um seine Sonne geht?", "tokens": ["Und", "still", "da\u00b7bei", "im", "Kreis", "um", "sei\u00b7ne", "Son\u00b7ne", "geht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PAV", "APPRART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was um die Sonn' ihn zwingt, und was um sich ihn schwingt,", "tokens": ["Was", "um", "die", "Sonn'", "ihn", "zwingt", ",", "und", "was", "um", "sich", "ihn", "schwingt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "PWS", "APPR", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ist nicht verschieden, eins durchs andre ist bedingt.", "tokens": ["Ist", "nicht", "ver\u00b7schie\u00b7den", ",", "eins", "durchs", "and\u00b7re", "ist", "be\u00b7dingt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "VVINF", "$,", "PIS", "APPRART", "PIS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das ist des Mannes Muth, der auf der Liebe ruht,", "tokens": ["Das", "ist", "des", "Man\u00b7nes", "Muth", ",", "der", "auf", "der", "Lie\u00b7be", "ruht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der selbst sich thut den Dienst, den er dem andern thut.", "tokens": ["Der", "selbst", "sich", "thut", "den", "Dienst", ",", "den", "er", "dem", "an\u00b7dern", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PRF", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dagegen ein Trabant ist jener Stern genant,", "tokens": ["Da\u00b7ge\u00b7gen", "ein", "Tra\u00b7bant", "ist", "je\u00b7ner", "Stern", "ge\u00b7nant", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VAFIN", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der seinem Hauptstern sich zuwendet unverwandt.", "tokens": ["Der", "sei\u00b7nem", "Haupts\u00b7tern", "sich", "zu\u00b7wen\u00b7det", "un\u00b7ver\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PRF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Er kehrt in Dienstespflicht ihm zu sein Angesicht,", "tokens": ["Er", "kehrt", "in", "Diens\u00b7tes\u00b7pflicht", "ihm", "zu", "sein", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dreht sich so um ihn, doch um sich selber nicht.", "tokens": ["Und", "dreht", "sich", "so", "um", "ihn", ",", "doch", "um", "sich", "sel\u00b7ber", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "APPR", "PPER", "$,", "ADV", "KOUI", "PRF", "ADV", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}