{"textgrid.poem.59678": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Meine Grablegung", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbwann ich gestorben, schlagt den schwarzen Mantel", "tokens": ["\u00bb", "wann", "ich", "ge\u00b7stor\u00b7ben", ",", "schlagt", "den", "schwar\u00b7zen", "Man\u00b7tel"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PWAV", "PPER", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Um meinen morschen Leib, wie er verschlissen.", "tokens": ["Um", "mei\u00b7nen", "mor\u00b7schen", "Leib", ",", "wie", "er", "ver\u00b7schlis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "$,", "PWAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ihr wi\u00dft, warum: die S\u00fcnde, die Tarantel,", "tokens": ["Ihr", "wi\u00dft", ",", "wa\u00b7rum", ":", "die", "S\u00fcn\u00b7de", ",", "die", "Ta\u00b7ran\u00b7tel", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "$.", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hat mich in gr\u00fcner Jugend scharf gebissen.", "tokens": ["Hat", "mich", "in", "gr\u00fc\u00b7ner", "Ju\u00b7gend", "scharf", "ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Drum mu\u00dft' ich taumelnd in dem tollen Tanze,", "tokens": ["Drum", "mu\u00dft'", "ich", "tau\u00b7melnd", "in", "dem", "tol\u00b7len", "Tan\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Leben hei\u00dft, durch b\u00f6se Irren schweifen,", "tokens": ["Der", "Le\u00b7ben", "hei\u00dft", ",", "durch", "b\u00f6\u00b7se", "Ir\u00b7ren", "schwei\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Am Becher wilder Lust, am bunten Kranze", "tokens": ["Am", "Be\u00b7cher", "wil\u00b7der", "Lust", ",", "am", "bun\u00b7ten", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Torheit wie an Blumen mich vergreifen.", "tokens": ["Der", "Tor\u00b7heit", "wie", "an", "Blu\u00b7men", "mich", "ver\u00b7grei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "APPR", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie sollt' ich anders denn vor Gott erscheinen", "tokens": ["Wie", "sollt'", "ich", "an\u00b7ders", "denn", "vor", "Gott", "er\u00b7schei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am J\u00fcngsten Tag, als trauernd und zerrissen?", "tokens": ["Am", "J\u00fcng\u00b7sten", "Tag", ",", "als", "trau\u00b7ernd", "und", "zer\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "KOUS", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach! Mein Gefolg', mein Engel, der wird weinen", "tokens": ["Ach", "!", "Mein", "Ge\u00b7fol\u00b7g'", ",", "mein", "En\u00b7gel", ",", "der", "wird", "wei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "VVINF"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und mein Vertrauter zagen, mein Gewissen.\u00ab", "tokens": ["Und", "mein", "Ver\u00b7trau\u00b7ter", "za\u00b7gen", ",", "mein", "Ge\u00b7wis\u00b7sen", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So sprach ich. Und mein T\u00f6chterlein, das feine,", "tokens": ["So", "sprach", "ich", ".", "Und", "mein", "T\u00f6ch\u00b7ter\u00b7lein", ",", "das", "fei\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "KON", "PPOSAT", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wischt' aus den Augen sich die hellen Z\u00e4hren:", "tokens": ["Wischt'", "aus", "den", "Au\u00b7gen", "sich", "die", "hel\u00b7len", "Z\u00e4h\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "\u00bbo Vater, diese Farben sind nicht deine;", "tokens": ["\u00bb", "o", "Va\u00b7ter", ",", "die\u00b7se", "Far\u00b7ben", "sind", "nicht", "dei\u00b7ne", ";"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "NN", "$,", "PDAT", "NN", "VAFIN", "PTKNEG", "PPOSAT", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie kommst du auf die alten Heidenm\u00e4ren?", "tokens": ["Wie", "kommst", "du", "auf", "die", "al\u00b7ten", "Hei\u00b7den\u00b7m\u00e4\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich wei\u00df es besser, wie wir dann dich kleiden:", "tokens": ["Ich", "wei\u00df", "es", "bes\u00b7ser", ",", "wie", "wir", "dann", "dich", "klei\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "PPER", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dein Leichentuch mu\u00df gr\u00fcn sein, und ein rotes", "tokens": ["Dein", "Lei\u00b7chen\u00b7tuch", "mu\u00df", "gr\u00fcn", "sein", ",", "und", "ein", "ro\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "ADJD", "VAINF", "$,", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herz auf dein Herz gen\u00e4ht; denn diese beiden,", "tokens": ["Herz", "auf", "dein", "Herz", "ge\u00b7n\u00e4ht", ";", "denn", "die\u00b7se", "bei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "VVPP", "$.", "KON", "PDAT", "PIAT", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das Gr\u00fcn und Rot, verk\u00fcnden nichts Gedrohtes.", "tokens": ["Das", "Gr\u00fcn", "und", "Rot", ",", "ver\u00b7k\u00fcn\u00b7den", "nichts", "Ge\u00b7droh\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "$,", "VVFIN", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Die frohen Christenfarben sollst du nehmen", "tokens": ["Die", "fro\u00b7hen", "Chris\u00b7ten\u00b7far\u00b7ben", "sollst", "du", "neh\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit gr\u00fcnem Christenglauben in die Erde.", "tokens": ["Mit", "gr\u00fc\u00b7nem", "Chris\u00b7ten\u00b7glau\u00b7ben", "in", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was spielst du so mit w\u00fcsten Heidenschemen,", "tokens": ["Was", "spielst", "du", "so", "mit", "w\u00fcs\u00b7ten", "Hei\u00b7den\u00b7sche\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verzerrt durch Graun der d\u00fcstern Nachtgeb\u00e4rde?\u00ab", "tokens": ["Ver\u00b7zerrt", "durch", "Graun", "der", "d\u00fcs\u00b7tern", "Nacht\u00b7ge\u00b7b\u00e4r\u00b7de", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, 