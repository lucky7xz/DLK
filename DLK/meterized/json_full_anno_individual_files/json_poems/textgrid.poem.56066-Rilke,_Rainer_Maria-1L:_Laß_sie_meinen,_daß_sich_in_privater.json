{"textgrid.poem.56066": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: La\u00df sie meinen, da\u00df sich in privater", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df sie meinen, da\u00df sich in privater", "tokens": ["La\u00df", "sie", "mei\u00b7nen", ",", "da\u00df", "sich", "in", "pri\u00b7va\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "$,", "KOUS", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wehmut l\u00f6st, was einer dort bestritt.", "tokens": ["Weh\u00b7mut", "l\u00f6st", ",", "was", "ei\u00b7ner", "dort", "be\u00b7stritt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "ART", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Nirgend sonst als da ist ein Theater;", "tokens": ["Nir\u00b7gend", "sonst", "als", "da", "ist", "ein", "The\u00b7a\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "KOKOM", "ADV", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "rei\u00df den hohen Vorhang fort \u2013: da tritt", "tokens": ["rei\u00df", "den", "ho\u00b7hen", "Vor\u00b7hang", "fort", "\u2013", ":", "da", "tritt"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$(", "$.", "ADV", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "vor den Chor der N\u00e4chte, der begann", "tokens": ["vor", "den", "Chor", "der", "N\u00e4ch\u00b7te", ",", "der", "be\u00b7gann"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "ein unendlich breites Lied zu sagen,", "tokens": ["ein", "un\u00b7end\u00b7lich", "brei\u00b7tes", "Lied", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "++--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "jene Stunde auf, bei der sie lagen,", "tokens": ["je\u00b7ne", "Stun\u00b7de", "auf", ",", "bei", "der", "sie", "la\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "und zerrei\u00dft ihr Kleid und klagt sich an,", "tokens": ["und", "zer\u00b7rei\u00dft", "ihr", "Kleid", "und", "klagt", "sich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "um der andern, um der Stunde willen,", "tokens": ["um", "der", "an\u00b7dern", ",", "um", "der", "Stun\u00b7de", "wil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "KOUI", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "die sich wehrt und w\u00e4lzt im Hintergrunde;", "tokens": ["die", "sich", "wehrt", "und", "w\u00e4lzt", "im", "Hin\u00b7ter\u00b7grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PRF", "VVFIN", "KON", "VVFIN", "APPRART", "NN", "$."], "m