{"dta.poem.6752": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um ohne Furcht zu seyn, da\u00df wir uns selbst nicht heucheln,", "tokens": ["Um", "oh\u00b7ne", "Furcht", "zu", "seyn", ",", "da\u00df", "wir", "uns", "selbst", "nicht", "heu\u00b7cheln", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPR", "NN", "PTKZU", "VAINF", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch mit dem Lehr-Geb\u00e4u uns selber schmeicheln.", "tokens": ["Noch", "mit", "dem", "Lehr\u00b7Ge\u00b7b\u00e4u", "uns", "sel\u00b7ber", "schmei\u00b7cheln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn sich ein menschlicher Verstand", "tokens": ["Wenn", "sich", "ein", "menschli\u00b7cher", "Ver\u00b7stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "ART", "ADJA", "NN"], "meter": "---+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Bestrebt, das Werck von GOTTES Wunder-Hand,", "tokens": ["Be\u00b7strebt", ",", "das", "Werck", "von", "GoT\u00b7TES", "Wun\u00b7der\u00b7Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ART", "NN", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Jm Form von einem Aug\u2019 uns k\u00fcnstlich nachzumachen;", "tokens": ["Jm", "Form", "von", "ei\u00b7nem", "Aug'", "uns", "k\u00fcnst\u00b7lich", "nach\u00b7zu\u00b7ma\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So stellet ein geschliffen Glas", "tokens": ["So", "stel\u00b7let", "ein", "ge\u00b7schlif\u00b7fen", "Glas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Uns alle W\u00fcrckungen und alle Sachen", "tokens": ["Uns", "al\u00b7le", "W\u00fcr\u00b7ckun\u00b7gen", "und", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "Von dem crystallnem Na\u00df", "tokens": ["Von", "dem", "crys\u00b7tall\u00b7nem", "Na\u00df"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Recht augenscheinlich dar; zumahlen", "tokens": ["Recht", "au\u00b7gen\u00b7schein\u00b7lich", "dar", ";", "zu\u00b7mah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NN", "ADJD", "PTKVZ", "$.", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Wenn man an der Retinen statt", "tokens": ["Wenn", "man", "an", "der", "Re\u00b7ti\u00b7nen", "statt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ART", "NN", "APPR"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.11": {"text": "Ein Pergament-Blat stellt, wo die vereinten Strahlen", "tokens": ["Ein", "Per\u00b7ga\u00b7ment\u00b7Blat", "stellt", ",", "wo", "die", "ver\u00b7ein\u00b7ten", "Strah\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Sich hemmen und zusammen halten;", "tokens": ["Sich", "hem\u00b7men", "und", "zu\u00b7sam\u00b7men", "hal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "KON", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "So stellt sich, von den \u00e4useren Gestalten,", "tokens": ["So", "stellt", "sich", ",", "von", "den", "\u00e4u\u00b7se\u00b7ren", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Das Mahlwerck deutlich dar.", "tokens": ["Das", "Mahl\u00b7werck", "deut\u00b7lich", "dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.15": {"text": "Das Pergament empf\u00e4ngt die Bilder hell und klar;", "tokens": ["Das", "Per\u00b7ga\u00b7ment", "emp\u00b7f\u00e4ngt", "die", "Bil\u00b7der", "hell", "und", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So wie in unser Aug\u2019 ein jeder Vorwurf f\u00e4llt,", "tokens": ["So", "wie", "in", "un\u00b7ser", "Aug'", "ein", "je\u00b7der", "Vor\u00b7wurf", "f\u00e4llt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "APPR", "PPOSAT", "NN", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und sich in selbiges mit Z\u00fcg\u2019 und Farben stellt.", "tokens": ["Und", "sich", "in", "sel\u00b7bi\u00b7ges", "mit", "Z\u00fcg'", "und", "Far\u00b7ben", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "APPR", "ADJA", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}