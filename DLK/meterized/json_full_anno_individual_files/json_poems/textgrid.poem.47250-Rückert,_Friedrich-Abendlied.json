, "measure": "iambic.tri"}, "line.2": {"text": "Ruht nun darin sich aus;", "tokens": ["Ruht", "nun", "da\u00b7rin", "sich", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PAV", "PRF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wen die Fremde trennet,", "tokens": ["Und", "wen", "die", "Frem\u00b7de", "tren\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den tr\u00e4gt ein Traum nach Haus.", "tokens": ["Den", "tr\u00e4gt", "ein", "Traum", "nach", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Mich fasset ein Verlangen,", "tokens": ["Mich", "fas\u00b7set", "ein", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df ich zu dieser Frist", "tokens": ["Da\u00df", "ich", "zu", "die\u00b7ser", "Frist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Hinauf nicht kann gelangen,", "tokens": ["Hin\u00b7auf", "nicht", "kann", "ge\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo meine Heimat ist.", "tokens": ["Wo", "mei\u00b7ne", "Hei\u00b7mat", "ist", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}