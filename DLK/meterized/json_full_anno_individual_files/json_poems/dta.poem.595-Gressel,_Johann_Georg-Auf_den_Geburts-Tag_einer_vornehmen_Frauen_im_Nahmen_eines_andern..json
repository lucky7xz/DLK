{"dta.poem.595": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Geburts-Tag einer vornehmen  \n Frauen/ im Nahmen eines andern.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ermunter dich mein Geist/ und r\u00fchm die holden Stralen/", "tokens": ["Er\u00b7mun\u00b7ter", "dich", "mein", "Geist", "/", "und", "r\u00fchm", "die", "hol\u00b7den", "Stra\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPOSAT", "NN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit sich dieser Tag vor andern kostbahr macht/", "tokens": ["Wo\u00b7mit", "sich", "die\u00b7ser", "Tag", "vor", "an\u00b7dern", "kost\u00b7bahr", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "PDAT", "NN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Schau ", "tokens": ["Schau"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Er doppelt seinen Glantz mit ungemeiner Pracht.", "tokens": ["Er", "dop\u00b7pelt", "sei\u00b7nen", "Glantz", "mit", "un\u00b7ge\u00b7mei\u00b7ner", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Erde fecuet sich/ sie will sich mit erheben/", "tokens": ["Die", "Er\u00b7de", "fe\u00b7cu\u00b7et", "sich", "/", "sie", "will", "sich", "mit", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "$(", "PPER", "VMFIN", "PRF", "APPR", "VVINF", "$("], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Und die Natur vermehrt was sie schon offt gethan.", "tokens": ["Und", "die", "Na\u00b7tur", "ver\u00b7mehrt", "was", "sie", "schon", "offt", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJD", "PWS", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Deutung wil sich selbst in diesen Worten geben:", "tokens": ["Die", "Deu\u00b7tung", "wil", "sich", "selbst", "in", "die\u00b7sen", "Wor\u00b7ten", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PRF", "ADV", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Es zeigt was grosses an.", "tokens": ["Es", "zeigt", "was", "gros\u00b7ses", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Es zeigt was grosses an/ und macht durchaus begl\u00fccket", "tokens": ["Es", "zeigt", "was", "gros\u00b7ses", "an", "/", "und", "macht", "durc\u00b7haus", "be\u00b7gl\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADJA", "PTKVZ", "$(", "KON", "VVFIN", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn eine edle Seel den Tag offt wieder schaut/", "tokens": ["Wenn", "ei\u00b7ne", "ed\u00b7le", "Seel", "den", "Tag", "offt", "wie\u00b7der", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ART", "NN", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "An welchem sie zu erst die Erde angeblicket/", "tokens": ["An", "wel\u00b7chem", "sie", "zu", "erst", "die", "Er\u00b7de", "an\u00b7ge\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "APPR", "ADV", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "An dem des Himmels Gunst den Wohlfahrts-Grund erbaut:", "tokens": ["An", "dem", "des", "Him\u00b7mels", "Gunst", "den", "Wohl\u00b7fahrts\u00b7Grund", "er\u00b7baut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Sie kan Hoch-Edle Frau auch dieses Gl\u00fccke sehen/", "tokens": ["Sie", "kan", "Hoch\u00b7Ed\u00b7le", "Frau", "auch", "die\u00b7ses", "Gl\u00fc\u00b7cke", "se\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "ADV", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Denn dieses ist der Tag/ der ihr das Leben gab/", "tokens": ["Denn", "die\u00b7ses", "ist", "der", "Tag", "/", "der", "ihr", "das", "Le\u00b7ben", "gab", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ART", "NN", "$(", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Sie ist ein fruchtbahr Baum darbey die Worte stehen?", "tokens": ["Sie", "ist", "ein", "frucht\u00b7bahr", "Baum", "dar\u00b7bey", "die", "Wor\u00b7te", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So gr\u00fcnte Arons Stab.", "tokens": ["So", "gr\u00fcn\u00b7te", "A\u00b7rons", "Stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "So gr\u00fcnet Arons Stab/ und brachte s\u00fcsse Fr\u00fcchte/", "tokens": ["So", "gr\u00fc\u00b7net", "A\u00b7rons", "Stab", "/", "und", "brach\u00b7te", "s\u00fcs\u00b7se", "Fr\u00fcch\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "NN", "$(", "KON", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und mir wil ihre Gunst ein gleiches Wunder seyn/", "tokens": ["Und", "mir", "wil", "ih\u00b7re", "Gunst", "ein", "glei\u00b7ches", "Wun\u00b7der", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VAINF", "$("], "meter": "-++--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.19": {"text": "Wenn ich mein Sehnen hin zu ihrer G\u00fcte richte/", "tokens": ["Wenn", "ich", "mein", "Seh\u00b7nen", "hin", "zu", "ih\u00b7rer", "G\u00fc\u00b7te", "rich\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "So trifft mir also fort/ was ich gew\u00fcnschet/ ein.", "tokens": ["So", "trifft", "mir", "al\u00b7so", "fort", "/", "was", "ich", "ge\u00b7w\u00fcn\u00b7schet", "/", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$(", "PWS", "PPER", "VVPP", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mein Himmel lasse sie in stetem Gl\u00fccke gr\u00fcnen/", "tokens": ["Mein", "Him\u00b7mel", "las\u00b7se", "sie", "in", "ste\u00b7tem", "Gl\u00fc\u00b7cke", "gr\u00fc\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sch\u00fctz diesen edlen Baum mit deiner starcken Hand/", "tokens": ["Sch\u00fctz", "die\u00b7sen", "ed\u00b7len", "Baum", "mit", "dei\u00b7ner", "star\u00b7cken", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Denn wird die Uberschrifft auch mir zum Vortheil dienen/", "tokens": ["Denn", "wird", "die", "U\u00b7ber\u00b7schrifft", "auch", "mir", "zum", "Vor\u00b7theil", "die\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "PPER", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Gerecht zum Widerstand.", "tokens": ["Ge\u00b7recht", "zum", "Wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Gerecht zum Widerstand ist sie mir stets gewesen/", "tokens": ["Ge\u00b7recht", "zum", "Wi\u00b7der\u00b7stand", "ist", "sie", "mir", "stets", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VAFIN", "PPER", "PPER", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wenn mir ein Ungl\u00fccks-Sturm nicht allzu ferne war/", "tokens": ["Wenn", "mir", "ein", "Un\u00b7gl\u00fccks\u00b7Sturm", "nicht", "all\u00b7zu", "fer\u00b7ne", "war", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKNEG", "PTKA", "ADV", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Man kan die G\u00fctigkeit an ihrer Stirnen lesen/", "tokens": ["Man", "kan", "die", "G\u00fc\u00b7tig\u00b7keit", "an", "ih\u00b7rer", "Stir\u00b7nen", "le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Wenn mich ein Mangel dr\u00fcckt so wird sie offenbahr/", "tokens": ["Wenn", "mich", "ein", "Man\u00b7gel", "dr\u00fcckt", "so", "wird", "sie", "of\u00b7fen\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "ADV", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Ist Indien befugt den Wunder-Baum zu ehren/", "tokens": ["Ist", "In\u00b7di\u00b7en", "be\u00b7fugt", "den", "Wun\u00b7der\u00b7Baum", "zu", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Weil es von dessen Stamm fast alle Sachen hat/", "tokens": ["Weil", "es", "von", "des\u00b7sen", "Stamm", "fast", "al\u00b7le", "Sa\u00b7chen", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PRELAT", "NN", "ADV", "PIAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "So la\u00df ich mich von ihr mit diesem Denck-Spruch h\u00f6ren/", "tokens": ["So", "la\u00df", "ich", "mich", "von", "ihr", "mit", "die\u00b7sem", "Den\u00b7ck\u00b7Spruch", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "PRF", "APPR", "PPER", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.32": {"text": "In Worten und der That.", "tokens": ["In", "Wor\u00b7ten", "und", "der", "That", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "In Worten und der That versp\u00fchr ich ihre Liebe/", "tokens": ["In", "Wor\u00b7ten", "und", "der", "That", "ver\u00b7sp\u00fchr", "ich", "ih\u00b7re", "Lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ART", "NN", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Mein Himmel zahle doch vor mich die Schulden ab/", "tokens": ["Mein", "Him\u00b7mel", "zah\u00b7le", "doch", "vor", "mich", "die", "Schul\u00b7den", "ab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "APPR", "PPER", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Kein Unfall mache nie den Freuden Himmel tr\u00fcbe/", "tokens": ["Kein", "Un\u00b7fall", "ma\u00b7che", "nie", "den", "Freu\u00b7den", "Him\u00b7mel", "tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADV", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Sie seh offt diesen Tag entfernt von Noht und Grab/", "tokens": ["Sie", "seh", "offt", "die\u00b7sen", "Tag", "ent\u00b7fernt", "von", "Noht", "und", "Grab", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PDAT", "NN", "ADJD", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Mein Unverm\u00f6gen wei\u00df nichts bessers ihr zu schencken/", "tokens": ["Mein", "Un\u00b7ver\u00b7m\u00f6\u00b7gen", "wei\u00df", "nichts", "bes\u00b7sers", "ihr", "zu", "schen\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PIS", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Als da\u00df der grosse GOtt selbst der Vergelter sey/", "tokens": ["Als", "da\u00df", "der", "gros\u00b7se", "Gott", "selbst", "der", "Ver\u00b7gel\u00b7ter", "sey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "ADV", "ART", "NN", "VAFIN", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.39": {"text": "Und bey die Ehren-Kron will ich die Umschrifft dencken/", "tokens": ["Und", "bey", "die", "Eh\u00b7ren\u00b7Kron", "will", "ich", "die", "Um\u00b7schrifft", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VMFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Der Lohn wird immer neu.", "tokens": ["Der", "Lohn", "wird", "im\u00b7mer", "neu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Der Lohn wird immer neu/ den dieser F\u00fcrste giebet/", "tokens": ["Der", "Lohn", "wird", "im\u00b7mer", "neu", "/", "den", "die\u00b7ser", "F\u00fcrs\u00b7te", "gie\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$(", "ART", "PDAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Und in der Ewigkeit nimmt er noch t\u00e4glich zu/", "tokens": ["Und", "in", "der", "E\u00b7wig\u00b7keit", "nimmt", "er", "noch", "t\u00e4g\u00b7lich", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.43": {"text": "Sie wird in seinem Schutz von keiner Noht betr\u00fcbet/", "tokens": ["Sie", "wird", "in", "sei\u00b7nem", "Schutz", "von", "kei\u00b7ner", "Noht", "be\u00b7tr\u00fc\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFI