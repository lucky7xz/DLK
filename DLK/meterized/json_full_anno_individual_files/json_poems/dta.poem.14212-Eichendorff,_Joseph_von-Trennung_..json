{"dta.poem.14212": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Trennung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Du gehst nun fort, sprach sie, ich bleib' alleine;", "tokens": ["Du", "gehst", "nun", "fort", ",", "sprach", "sie", ",", "ich", "bleib'", "al\u00b7lei\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach! d\u00fcrft' ich alles lassen, still und heiter", "tokens": ["Ach", "!", "d\u00fcrft'", "ich", "al\u00b7les", "las\u00b7sen", ",", "still", "und", "hei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "PIS", "VVINF", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit Dir so zieh'n hinab und immer weiter \u2014", "tokens": ["Mit", "Dir", "so", "zieh'n", "hin\u00b7ab", "und", "im\u00b7mer", "wei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "VVFIN", "PTKVZ", "KON", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich sah Dich an \u2014 es spielten bleiche Scheine", "tokens": ["Ich", "sah", "Dich", "an", "es", "spiel\u00b7ten", "blei\u00b7che", "Schei\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$(", "PPER", "VVFIN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "So wunderbar um Locken Dir und Glieder;", "tokens": ["So", "wun\u00b7der\u00b7bar", "um", "Lo\u00b7cken", "Dir", "und", "Glie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "NN", "PPER", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "So ruhig, fremd warst Du mir nie erschienen,", "tokens": ["So", "ru\u00b7hig", ",", "fremd", "warst", "Du", "mir", "nie", "er\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADJD", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es war, als sagten die versteinten Mienen,", "tokens": ["Es", "war", ",", "als", "sag\u00b7ten", "die", "ver\u00b7stein\u00b7ten", "Mie\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Was Du verschwiegst: Wir seh'n uns niemals wieder!", "tokens": ["Was", "Du", "ver\u00b7schwiegst", ":", "Wir", "seh'n", "uns", "nie\u00b7mals", "wie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}