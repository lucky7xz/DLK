{"dta.poem.13004": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Leanders antwort.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn meine verse nicht so s\u00fc\u00df als deine klingen,", "tokens": ["Wenn", "mei\u00b7ne", "ver\u00b7se", "nicht", "so", "s\u00fc\u00df", "als", "dei\u00b7ne", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "KOKOM", "PPOSAT", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So wisse, da\u00df ich sie in meinem bette schrieb.", "tokens": ["So", "wis\u00b7se", ",", "da\u00df", "ich", "sie", "in", "mei\u00b7nem", "bet\u00b7te", "schrieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "PPER", "APPR", "PPOSAT", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man h\u00f6rt die nachtigall in keiner kammer singen,", "tokens": ["Man", "h\u00f6rt", "die", "nach\u00b7ti\u00b7gall", "in", "kei\u00b7ner", "kam\u00b7mer", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "NN", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Sie hat nur gr\u00fcne g\u00e4rt\u2019 und rosenth\u00e4ler lieb.", "tokens": ["Sie", "hat", "nur", "gr\u00fc\u00b7ne", "g\u00e4rt'", "und", "ro\u00b7sent\u00b7h\u00e4\u00b7ler", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "VVFIN", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich schaue nichts hiervon. Doch schaut\u2019 ich nur Floretten,", "tokens": ["Ich", "schau\u00b7e", "nichts", "hier\u00b7von", ".", "Doch", "schaut'", "ich", "nur", "Flo\u00b7ret\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PAV", "$.", "KON", "VVFIN", "PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dich, mein Seladsn! so schaut\u2019 ich schon genung.", "tokens": ["Und", "dich", ",", "mein", "Se\u00b7ladsn", "!", "so", "schaut'", "ich", "schon", "ge\u00b7nung", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.7": {"text": "Ich tr\u00fcge voller lust die schweren ungl\u00fccks-ketten,", "tokens": ["Ich", "tr\u00fc\u00b7ge", "vol\u00b7ler", "lust", "die", "schwe\u00b7ren", "un\u00b7gl\u00fccks\u00b7ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und sp\u00fcrte zweiffels-frey weit mehr beruhigung.", "tokens": ["Und", "sp\u00fcr\u00b7te", "zweif\u00b7fels\u00b7frey", "weit", "mehr", "be\u00b7ru\u00b7hi\u00b7gung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "CARD", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Indessen tr\u00f6stet mich doch euer angedencken.", "tokens": ["In\u00b7des\u00b7sen", "tr\u00f6s\u00b7tet", "mich", "doch", "eu\u00b7er", "an\u00b7ge\u00b7den\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Es hindert Seladon viel unmuth und verdru\u00df.", "tokens": ["Es", "hin\u00b7dert", "Se\u00b7la\u00b7don", "viel", "un\u00b7muth", "und", "ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Will das verh\u00e4ngni\u00df mich mit gall und myrrhen tr\u00e4ncken,", "tokens": ["Will", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "mich", "mit", "gall", "und", "myr\u00b7rhen", "tr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "VVFIN", "PRF", "APPR", "NE", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So geb ich in der angst Floretten einen ku\u00df.", "tokens": ["So", "geb", "ich", "in", "der", "angst", "Flo\u00b7ret\u00b7ten", "ei\u00b7nen", "ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Wie sollt\u2019 ich armer sonst die bitterkeit vers\u00fc\u00dfen?", "tokens": ["Wie", "sollt'", "ich", "ar\u00b7mer", "sonst", "die", "bit\u00b7ter\u00b7keit", "ver\u00b7s\u00fc\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADJA", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Ich wei\u00df, Florette selbst sieht meiner k\u00fchnheit nach.", "tokens": ["Ich", "wei\u00df", ",", "Flo\u00b7ret\u00b7te", "selbst", "sieht", "mei\u00b7ner", "k\u00fchn\u00b7heit", "nach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Zum minsten heist kein recht vor die gedancken b\u00fcssen.", "tokens": ["Zum", "mins\u00b7ten", "heist", "kein", "recht", "vor", "die", "ge\u00b7dan\u00b7cken", "b\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PIAT", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Weil auch wohl Joseph sich nicht ihrer gantz entbrach.", "tokens": ["Weil", "auch", "wohl", "Jo\u00b7se\u00b7ph", "sich", "nicht", "ih\u00b7rer", "gantz", "ent\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "NE", "PRF", "PTKNEG", "PPOSAT", "ADV", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "So unterhalt ich mich mein Seladon im geiste.", "tokens": ["So", "un\u00b7ter\u00b7halt", "ich", "mich", "mein", "Se\u00b7la\u00b7don", "im", "geis\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "PPOSAT", "NN", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Drum frage ja nicht mehr: Gedenckst du auch an mich?", "tokens": ["Drum", "fra\u00b7ge", "ja", "nicht", "mehr", ":", "Ge\u00b7denckst", "du", "auch", "an", "mich", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "PTKNEG", "ADV", "$.", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ich w\u00fcste keinen tag, da ich nicht zu dir reiste.", "tokens": ["Ich", "w\u00fcs\u00b7te", "kei\u00b7nen", "tag", ",", "da", "ich", "nicht", "zu", "dir", "reis\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Wer liebt, und reiset nicht? ich aber liebe dich.", "tokens": ["Wer", "liebt", ",", "und", "rei\u00b7set", "nicht", "?", "ich", "a\u00b7ber", "lie\u00b7be", "dich", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KON", "VVFIN", "PTKNEG", "$.", "PPER", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Ich liebe dich gewi\u00df, und will dich ewig lieben,", "tokens": ["Ich", "lie\u00b7be", "dich", "ge\u00b7wi\u00df", ",", "und", "will", "dich", "e\u00b7wig", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "KON", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Denn diese liebe soll mit mir zu grabe gehn.", "tokens": ["Denn", "die\u00b7se", "lie\u00b7be", "soll", "mit", "mir", "zu", "gra\u00b7be", "gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "VMFIN", "APPR", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Das gl\u00fccke mag an mir die sch\u00e4rffsten waffen \u00fcben,", "tokens": ["Das", "gl\u00fc\u00b7cke", "mag", "an", "mir", "die", "sch\u00e4rffs\u00b7ten", "waf\u00b7fen", "\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "VMFIN", "APPR", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Kan ich nur allezeit in deiner freundschafft stehn.", "tokens": ["Kan", "ich", "nur", "al\u00b7le\u00b7zeit", "in", "dei\u00b7ner", "freund\u00b7schafft", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Du schreibst: ich schaue nichts als blumen, gras und b\u00e4ume.", "tokens": ["Du", "schreibst", ":", "ich", "schau\u00b7e", "nichts", "als", "blu\u00b7men", ",", "gras", "und", "b\u00e4u\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "PIS", "KOKOM", "VVFIN", "$,", "NN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Ach freund! ich schaue noch kein angenehmes feld.", "tokens": ["Ach", "freund", "!", "ich", "schau\u00b7e", "noch", "kein", "an\u00b7ge\u00b7neh\u00b7mes", "feld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "PPER", "VVFIN", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Indessen f\u00fchret mich dein vers auf s\u00fc\u00dfe tr\u00e4ume,", "tokens": ["In\u00b7des\u00b7sen", "f\u00fch\u00b7ret", "mich", "dein", "vers", "auf", "s\u00fc\u00b7\u00dfe", "tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ob das verh\u00e4ngni\u00df mir gleich deinen wunsch verg\u00e4llt.", "tokens": ["Ob", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "mir", "gleich", "dei\u00b7nen", "wunsch", "ver\u00b7g\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PPER", "ADV", "PPOSAT", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Und also siehest du, was du vorl\u00e4ngst gelesen:", "tokens": ["Und", "al\u00b7so", "sie\u00b7hest", "du", ",", "was", "du", "vor\u00b7l\u00e4ngst", "ge\u00b7le\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Leanders unstern will ein fix-gestirne seyn.", "tokens": ["Le\u00b7an\u00b7ders", "uns\u00b7tern", "will", "ein", "fix\u00b7ge\u00b7stir\u00b7ne", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVINF", "VMFIN", "ART", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Doch wie die hoffnung noch niemals mein artzt gewesen,", "tokens": ["Doch", "wie", "die", "hoff\u00b7nung", "noch", "nie\u00b7mals", "mein", "artzt", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "ADV", "ADV", "PPOSAT", "VVPP", "VAPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.32": {"text": "So geht mir dieser satz nicht eben bitter ein.", "tokens": ["So", "geht", "mir", "die\u00b7ser", "satz", "nicht", "e\u00b7ben", "bit\u00b7ter", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PDAT", "NN", "PTKNEG", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Ich bin es schon gewohnt den wermuth-safft zu schmecken;", "tokens": ["Ich", "bin", "es", "schon", "ge\u00b7wohnt", "den", "wer\u00b7muth\u00b7\u00b7safft", "zu", "schme\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Vielleichte dient er mir mehr als der honigseim.", "tokens": ["Viel\u00b7leich\u00b7te", "dient", "er", "mir", "mehr", "als", "der", "ho\u00b7ni\u00b7gseim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PIAT", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Die wohllust m\u00f6chte zwar sehr gerne zucker lecken,", "tokens": ["Die", "wohl\u00b7lust", "m\u00f6ch\u00b7te", "zwar", "sehr", "ger\u00b7ne", "zu\u00b7cker", "le\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "ADV", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Doch f\u00e4llt sie mit der zeit dem tod und grab anheim.", "tokens": ["Doch", "f\u00e4llt", "sie", "mit", "der", "zeit", "dem", "tod", "und", "grab", "an\u00b7heim", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ART", "NN", "ART", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Das gl\u00fccke l\u00e4st ohndem sich nicht durch murren zwingen,", "tokens": ["Das", "gl\u00fc\u00b7cke", "l\u00e4st", "ohn\u00b7dem", "sich", "nicht", "durch", "mur\u00b7ren", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "KOUS", "PRF", "PTKNEG", "APPR", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Drum trag ich mit geduld, was ich nicht \u00e4ndern kan.", "tokens": ["Drum", "trag", "ich", "mit", "ge\u00b7duld", ",", "was", "ich", "nicht", "\u00e4n\u00b7dern", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "APPR", "NN", "$,", "PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Und will mein dornenpusch mir keine rosen bringen,", "tokens": ["Und", "will", "mein", "dor\u00b7nen\u00b7pusch", "mir", "kei\u00b7ne", "ro\u00b7sen", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "PPER", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So schau ich sie gleichwol bey meinen freunden an.", "tokens": ["So", "schau", "ich", "sie", "gleich\u00b7wol", "bey", "mei\u00b7nen", "freun\u00b7den", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "APPR", "PPOSAT", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ich will, wie Socrates, aus schmertzen wohllust saugen.", "tokens": ["Ich", "will", ",", "wie", "So\u00b7cra\u00b7tes", ",", "aus", "schmert\u00b7zen", "wohl\u00b7lust", "sau\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "NE", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.42": {"text": "Ein andrer hoff\u2019 und schrey, bi\u00df er im grabe liegt.", "tokens": ["Ein", "an\u00b7drer", "hoff'", "und", "schrey", ",", "bi\u00df", "er", "im", "gra\u00b7be", "liegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "KON", "ADJD", "$,", "KOUS", "PPER", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wenn meine verse nur der sch\u00f6nen schwester taugen,", "tokens": ["Wenn", "mei\u00b7ne", "ver\u00b7se", "nur", "der", "sch\u00f6\u00b7nen", "schwes\u00b7ter", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ART", "ADJA", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Und Seladon mich liebt; so bin ich schon vergn\u00fcgt.", "tokens": ["Und", "Se\u00b7la\u00b7don", "mich", "liebt", ";", "so", "bin", "ich", "schon", "ver\u00b7gn\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": 