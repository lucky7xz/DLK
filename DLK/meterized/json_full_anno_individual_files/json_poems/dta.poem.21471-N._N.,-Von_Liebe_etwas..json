{"dta.poem.21471": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Von Liebe etwas.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.42", "af:0.28", "et:0.14", "fr:0.14"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Viele tausend Male sei du mir gesegnet,", "tokens": ["Vie\u00b7le", "tau\u00b7send", "Ma\u00b7le", "sei", "du", "mir", "ge\u00b7seg\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "CARD", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Rosenbl\u00fcthenmund,", "tokens": ["Ro\u00b7sen\u00b7bl\u00fc\u00b7then\u00b7mund", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dem noch nicht der letzte Tropfen ist begegnet", "tokens": ["Dem", "noch", "nicht", "der", "letz\u00b7te", "Trop\u00b7fen", "ist", "be\u00b7geg\u00b7net"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PTKNEG", "ART", "ADJA", "NN", "VAFIN", "VVPP"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Auf des Bechers Grund.", "tokens": ["Auf", "des", "Be\u00b7chers", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Dich auch will ich segnen, dich auch selig preisen,", "tokens": ["Dich", "auch", "will", "ich", "seg\u00b7nen", ",", "dich", "auch", "se\u00b7lig", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "PPER", "VVINF", "$,", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Ohrenmuschelpaar,", "tokens": ["Oh\u00b7ren\u00b7mu\u00b7schel\u00b7paar", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Das noch kindlich staunend h\u00f6ret auf den leisen", "tokens": ["Das", "noch", "kind\u00b7lich", "stau\u00b7nend", "h\u00f6\u00b7ret", "auf", "den", "lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "ADJD", "ADJD", "VVFIN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Sang der Sternenschaar.", "tokens": ["Sang", "der", "Ster\u00b7nen\u00b7schaar", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Rosenbl\u00fcthenlippen, Ohrenmuschelp\u00e4rchen", "tokens": ["Ro\u00b7sen\u00b7bl\u00fc\u00b7then\u00b7lip\u00b7pen", ",", "Oh\u00b7ren\u00b7mu\u00b7schel\u00b7p\u00e4r\u00b7chen"], "token_info": ["word", "punct", "word"], "pos": ["NN", "$,", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Werden arg befehden", "tokens": ["Wer\u00b7den", "arg", "be\u00b7feh\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "ADJD", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Deine Veilchenaugen; wirst nach einem J\u00e4hrchen", "tokens": ["Dei\u00b7ne", "Veil\u00b7chen\u00b7au\u00b7gen", ";", "wirst", "nach", "ei\u00b7nem", "J\u00e4hr\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "VAFIN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Schon von Liebe reden.", "tokens": ["Schon", "von", "Lie\u00b7be", "re\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Ach von jener Liebe, die nur in Gedichten", "tokens": ["Ach", "von", "je\u00b7ner", "Lie\u00b7be", ",", "die", "nur", "in", "Ge\u00b7dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PDAT", "NN", "$,", "PRELS", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Noch so reizvoll gl\u00fcht,", "tokens": ["Noch", "so", "reiz\u00b7voll", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Die wie leis verscholl\u2019ne M\u00e4hrchentraumgeschichten", "tokens": ["Die", "wie", "leis", "ver\u00b7scholl'\u00b7ne", "M\u00e4hr\u00b7chen\u00b7traum\u00b7ge\u00b7schich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "L\u00e4ngst schon ist verbl\u00fcht, verbl\u00fcht!", "tokens": ["L\u00e4ngst", "schon", "ist", "ver\u00b7bl\u00fcht", ",", "ver\u00b7bl\u00fcht", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "VVPP", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}