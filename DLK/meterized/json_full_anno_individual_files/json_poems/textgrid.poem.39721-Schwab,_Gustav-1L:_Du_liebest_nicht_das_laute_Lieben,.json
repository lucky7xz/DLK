{"textgrid.poem.39721": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du liebest nicht das laute Lieben,", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du liebest nicht das laute Lieben,", "tokens": ["Du", "lie\u00b7best", "nicht", "das", "lau\u00b7te", "Lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "PDS", "VVFIN", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und r\u00fchmt' ich dich vor aller Welt,", "tokens": ["Und", "r\u00fchmt'", "ich", "dich", "vor", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df, du hie\u00dfest's \u00fcbertrieben,", "tokens": ["Ich", "wei\u00df", ",", "du", "hie\u00dfest's", "\u00fc\u00b7bert\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "ADJD", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wie Vieles, was dir nicht gef\u00e4llt.", "tokens": ["Wie", "Vie\u00b7les", ",", "was", "dir", "nicht", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "$,", "PWS", "PPER", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch brauch' ich ja dich nicht zu nennen,", "tokens": ["Auch", "brauch'", "ich", "ja", "dich", "nicht", "zu", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Was ich dir danke, sag' ich nur,", "tokens": ["Was", "ich", "dir", "dan\u00b7ke", ",", "sag'", "ich", "nur", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Mancher wird dich drin erkennen,", "tokens": ["Und", "Man\u00b7cher", "wird", "dich", "drin", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der deines Vorbilds Kraft erfuhr.", "tokens": ["Der", "dei\u00b7nes", "Vor\u00b7bilds", "Kraft", "er\u00b7fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Da\u00df ich geforschet im Gem\u00fcte,", "tokens": ["Da\u00df", "ich", "ge\u00b7for\u00b7schet", "im", "Ge\u00b7m\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und nicht zum Worte Wort gereimt,", "tokens": ["Und", "nicht", "zum", "Wor\u00b7te", "Wort", "ge\u00b7reimt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPRART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df ich erstrebte keine Bl\u00fcte,", "tokens": ["Da\u00df", "ich", "er\u00b7streb\u00b7te", "kei\u00b7ne", "Bl\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die aus der Wurzel nicht gekeimt;", "tokens": ["Die", "aus", "der", "Wur\u00b7zel", "nicht", "ge\u00b7keimt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Da\u00df ich, was schlicht ist, was gedrungen,", "tokens": ["Da\u00df", "ich", ",", "was", "schlicht", "ist", ",", "was", "ge\u00b7drun\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "ADJD", "VAFIN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gew\u00e4hlt, oft gegen eignen Sinn,", "tokens": ["Ge\u00b7w\u00e4hlt", ",", "oft", "ge\u00b7gen", "eig\u00b7nen", "Sinn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und w\u00e4r' es mir nur halb gelungen \u2013", "tokens": ["Und", "w\u00e4r'", "es", "mir", "nur", "halb", "ge\u00b7lun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dein, dein ist meines Lieds Gewinn! \u2013", "tokens": ["Dein", ",", "dein", "ist", "mei\u00b7nes", "Lieds", "Ge\u00b7winn", "!", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "$,", "PPOSAT", "VAFIN", "PPOSAT", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Es klaget Deutschland, weil zu fr\u00fche", "tokens": ["Es", "kla\u00b7g