{"dta.poem.9905": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts ist auf der welt zu finden/ was ich nicht bezwingen kan/", "tokens": ["Nichts", "ist", "auf", "der", "welt", "zu", "fin\u00b7den", "/", "was", "ich", "nicht", "be\u00b7zwin\u00b7gen", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$(", "PWS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sieger die sind meine sclaven/ k\u00f6nige mir unterthan.", "tokens": ["Sie\u00b7ger", "die", "sind", "mei\u00b7ne", "scla\u00b7ven", "/", "k\u00f6\u00b7ni\u00b7ge", "mir", "un\u00b7ter\u00b7than", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "VAFIN", "PPOSAT", "NN", "$(", "VMFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Die ich unter mich gebracht/ pfleg ich meistens zu betr\u00fcben/", "tokens": ["Die", "ich", "un\u00b7ter", "mich", "ge\u00b7bracht", "/", "pfleg", "ich", "meis\u00b7tens", "zu", "be\u00b7tr\u00fc\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVPP", "$(", "VVIMP", "PPER", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Und am \u00e4rgsten qu\u00e4l ich die/ die mich mehr als andre lieben.", "tokens": ["Und", "am", "\u00e4rgs\u00b7ten", "qu\u00e4l", "ich", "die", "/", "die", "mich", "mehr", "als", "and\u00b7re", "lie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "ADJA", "NN", "PPER", "ART", "$(", "PRELS", "PPER", "ADV", "KOUS", "PIS", "VVINF", "$."], "meter": "--+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}}}}}