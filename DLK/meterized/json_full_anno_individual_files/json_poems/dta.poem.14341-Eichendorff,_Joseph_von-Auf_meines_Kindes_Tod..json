{"dta.poem.14341": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Auf meines Kindes Tod.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Das ist's, was mich ganz verst\u00f6ret:", "tokens": ["Das", "ist's", ",", "was", "mich", "ganz", "ver\u00b7st\u00f6\u00b7ret", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Da\u00df die Nacht nicht Ruhe h\u00e4lt,", "tokens": ["Da\u00df", "die", "Nacht", "nicht", "Ru\u00b7he", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PTKNEG", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn zu athmen aufgeh\u00f6ret", "tokens": ["Wenn", "zu", "ath\u00b7men", "auf\u00b7ge\u00b7h\u00f6\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PTKZU", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lange schon die m\u00fcde Welt.", "tokens": ["Lan\u00b7ge", "schon", "die", "m\u00fc\u00b7de", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}