{"dta.poem.8425": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Lied eines M\u00e4dchens auf den  \n Tod ihrer Gespielin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Seit ich getrauert habe;", "tokens": ["Seit", "ich", "ge\u00b7trau\u00b7ert", "ha\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der falbe Wermut gr\u00fcnet schon", "tokens": ["Der", "fal\u00b7be", "Wer\u00b7mut", "gr\u00fc\u00b7net", "schon"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auf meiner Freundin Grabe.", "tokens": ["Auf", "mei\u00b7ner", "Freun\u00b7din", "Gra\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da horch' ich oft im Mondenglanz", "tokens": ["Da", "horch'", "ich", "oft", "im", "Mon\u00b7den\u00b7glanz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Grillen Nachtgesange,", "tokens": ["Der", "Gril\u00b7len", "Nacht\u00b7ge\u00b7san\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und lehn' an ihren Todtenkranz", "tokens": ["Und", "lehn'", "an", "ih\u00b7ren", "Tod\u00b7ten\u00b7kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die bleichgeh\u00e4rmte Wange.", "tokens": ["Die", "bleich\u00b7ge\u00b7h\u00e4rm\u00b7te", "Wan\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da siz' ich armes armes Kind", "tokens": ["Da", "si\u00b7z'", "ich", "ar\u00b7mes", "ar\u00b7mes", "Kind"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Im kalten Abendhauche;", "tokens": ["Im", "kal\u00b7ten", "A\u00b7bend\u00b7hau\u00b7che", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und manche Sehnsuchtsthr\u00e4ne rinnt", "tokens": ["Und", "man\u00b7che", "Sehn\u00b7suchtst\u00b7hr\u00e4\u00b7ne", "rinnt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Am falben Wermutstrauche.", "tokens": ["Am", "fal\u00b7ben", "Wer\u00b7muts\u00b7trau\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Flieder und die Linde wehn", "tokens": ["Der", "Flie\u00b7der", "und", "die", "Lin\u00b7de", "wehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mir bange Seelenschauer,", "tokens": ["Mir", "ban\u00b7ge", "See\u00b7len\u00b7schau\u00b7er", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und hohe d\u00fcstre Schatten gehn", "tokens": ["Und", "ho\u00b7he", "d\u00fcst\u00b7re", "Schat\u00b7ten", "gehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "VVFIN", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Rings an der Kirchhofmauer,", "tokens": ["Rings", "an", "der", "Kirch\u00b7hof\u00b7mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Die Kirchenfenster regen sich,", "tokens": ["Die", "Kir\u00b7chen\u00b7fens\u00b7ter", "re\u00b7gen", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "PRF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es regen sich die Klocken.", "tokens": ["Es", "re\u00b7gen", "sich", "die", "Klo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es gl\u00e4nzt! es gl\u00e4nzt! Ach! seh' ich dich", "tokens": ["Es", "gl\u00e4nzt", "!", "es", "gl\u00e4nzt", "!", "Ach", "!", "seh'", "ich", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$.", "ITJ", "$.", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deinen hellen Locken?", "tokens": ["Mit", "dei\u00b7nen", "hel\u00b7len", "Lo\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Der Mond ists, so der Wolk' entrollt,", "tokens": ["Der", "Mond", "ists", ",", "so", "der", "Wol\u00b7k'", "ent\u00b7rollt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Ins Kirchenfenster schimmert,", "tokens": ["Ins", "Kir\u00b7chen\u00b7fens\u00b7ter", "schim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Am rothen Band', am Flittergold", "tokens": ["Am", "ro\u00b7then", "Band'", ",", "am", "Flit\u00b7ter\u00b7gold"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Todtenkr\u00e4nze flimmert!", "tokens": ["Der", "Tod\u00b7ten\u00b7kr\u00e4n\u00b7ze", "flim\u00b7mert", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O komm zur\u00fcck! o komm zur\u00fcck", "tokens": ["O", "komm", "zu\u00b7r\u00fcck", "!", "o", "komm", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "FM", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deines Gottes Throne!", "tokens": ["Von", "dei\u00b7nes", "Got\u00b7tes", "Thro\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O komm auf einen Augenblick", "tokens": ["O", "komm", "auf", "ei\u00b7nen", "Au\u00b7gen\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In deiner Siegerkrone!", "tokens": ["In", "dei\u00b7ner", "Sie\u00b7ger\u00b7kro\u00b7ne", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "In deinem neuen Engelreiz", "tokens": ["In", "dei\u00b7nem", "neu\u00b7en", "En\u00b7gel\u00b7reiz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erscheine mir, erscheine,", "tokens": ["Er\u00b7schei\u00b7ne", "mir", ",", "er\u00b7schei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die ich, gelehnt ans schwarze Kreuz,", "tokens": ["Die", "ich", ",", "ge\u00b7lehnt", "ans", "schwar\u00b7ze", "Kreuz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "$,", "VVPP", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf deinem Grabe weine!", "tokens": ["Auf", "dei\u00b7nem", "Gra\u00b7be", "wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}