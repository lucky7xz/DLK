{"textgrid.poem.33223": {"metadata": {"author": {"name": "Zinzendorf, Nikolaus Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "2323.", "genre": "verse", "period": "N.A.", "pub_year": 1730, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir seligen leutlein, wir sitzen im Seitlein, im Seitlein vom L\u00e4mmlein, das herz ist voll fl\u00e4mmlein.", "tokens": ["Wir", "se\u00b7li\u00b7gen", "leut\u00b7lein", ",", "wir", "sit\u00b7zen", "im", "Seit\u00b7lein", ",", "im", "Seit\u00b7lein", "vom", "L\u00e4mm\u00b7lein", ",", "das", "herz", "ist", "voll", "fl\u00e4mm\u00b7lein", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,", "APPRART", "PPOSAT", "APPRART", "NN", "$,", "ART", "NN", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+--+--+--+--+--+--+-+-+", "measure": "amphibrach.penta.plus"}}, "stanza.2": {"line.1": {"text": "Voll fl\u00e4mmlein ists herze, voll liebe und schmerze, voll schmerze und liebe, voll blutiger triebe.", "tokens": ["Voll", "fl\u00e4mm\u00b7lein", "ists", "her\u00b7ze", ",", "voll", "lie\u00b7be", "und", "schmer\u00b7ze", ",", "voll", "schmer\u00b7ze", "und", "lie\u00b7be", ",", "voll", "blu\u00b7ti\u00b7ger", "trie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$,", "ADJD", "VVFIN", "KON", "VVFIN", "$,", "ADJD", "ADJA", "KON", "VVFIN", "$,", "ADJD", "ADJA", "VVFIN", "$."], "meter": "+---+--+--+--+--+--+--+-", "measure": "trochaic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Voll triebe zum lendgen, zun f\u00fc\u00dfgen und h\u00e4ndgen, zun h\u00e4ndgen und f\u00fc\u00