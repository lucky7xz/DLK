"word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADV", "$,", "PRELS", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "ADV", "PIS", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+--+--+--+--+--+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.6": {"line.1": {"text": "Mit schalle der freuden kans kirchlein da weiden, ja weide du kirchlein auf diesen frohn-f\u00fcrchlein.", "tokens": ["Mit", "schal\u00b7le", "der", "freu\u00b7den", "kans", "kirch\u00b7lein", "da", "wei\u00b7den", ",", "ja", "wei\u00b7de", "du", "kirch\u00b7lein", "auf", "die\u00b7sen", "frohn\u00b7f\u00fcrch\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "ART", "ADJA", "ADJA", "NN", "ADV", "VVINF", "$,", "ADV", "VVFIN", "PPER", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+--+-+-+-+--+-++--+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "Die h\u00f6hlen und f\u00e4cher, die blutgen schwei\u00df-l\u00f6cher, der angst-dunst so blutig, die machen uns muthig.", "tokens": ["Die", "h\u00f6h\u00b7len", "und", "f\u00e4\u00b7cher", ",", "die", "blut\u00b7gen", "schwei\u00df\u00b7l\u00f6\u00b7cher", ",", "der", "angst\u00b7dunst", "so", "blu\u00b7tig", ",", "die", "ma\u00b7chen", "uns", "mut\u00b7hig", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJD", "$,", "ART", "ADJA", "ADJA", "$,", "ART", "NN", "ADV", "ADJD", "$,", "PRELS", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+--+--+--+--++-+--+--+-", "measure": "amphibrach.penta.plus"}}, "stanza.8": {"line.1": {"text": "So muthig vor gnade, die wunden-", "tokens": ["So", "mut\u00b7hig", "vor", "gna\u00b7de", ",", "die", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", 