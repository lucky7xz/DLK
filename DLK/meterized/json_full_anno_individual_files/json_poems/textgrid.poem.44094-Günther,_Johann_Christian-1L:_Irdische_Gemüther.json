 "+-+-+-", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Giebt es was zu streiten,", "tokens": ["Giebt", "es", "was", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Will die Hofnung gleiten", "tokens": ["Will", "die", "Hof\u00b7nung", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Muth vergehn,", "tokens": ["Und", "der", "Muth", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Herr, du und mein Glaube", "tokens": ["Herr", ",", "du", "und", "mein", "Glau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "L\u00e4st mich nicht im Raube", "tokens": ["L\u00e4st", "mich", "nicht", "im", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Noch im Ungl\u00fcck stehn.", "tokens": ["Noch", "im", "Un\u00b7gl\u00fcck", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mir verspricht", "tokens": ["Mir", "ver\u00b7spricht"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Die Zuversicht,", "tokens": ["Die", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.