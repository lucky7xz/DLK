chmach dabey", "tokens": ["Und", "Schmach", "da\u00b7bey"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "PAV"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Folgt bey Sodoms b\u00f6sen Sch\u00e4zen", "tokens": ["Folgt", "bey", "So\u00b7doms", "b\u00f6\u00b7sen", "Sch\u00e4\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "NE", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Auf ein kurz Erg\u00f6zen.", "tokens": ["Auf", "ein", "kurz", "Er\u00b7g\u00f6\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Eine Lust auf Erden", "tokens": ["Ei\u00b7ne", "Lust", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Macht mir die Beschwerden", "tokens": ["Macht", "mir", "die", "Be\u00b7schwer\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Alles Kummers leicht,", "tokens": ["Al\u00b7les", "Kum\u00b7mers", "leicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADJD", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn ich nehmlich dencke,", "tokens": ["Wenn", "ich", "nehm\u00b7lich", "den\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Was mir vor Geschencke", "tokens": ["Was", "mir", "vor", "Ge\u00b7schen\u00b7cke"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Gottes Liebe reicht;", "tokens": ["Got\u00b7tes", "Lie\u00b7be", "reicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Seine Treu", "tokens": ["Sei\u00b7ne", "Treu"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Wird t\u00e4glich neu,", "tokens": ["Wird", "t\u00e4g\u00b7lich", "neu", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Und die Wollust seiner G\u00fcte", "tokens": ["Und", "die", "Wol\u00b7lust", "sei\u00b7ner", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "St\u00e4rckt mein schwach Gem\u00fcthe", "tokens": ["St\u00e4rckt", "mein", "schwach", "Ge\u00b7m\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dies ist meine Freude,", "tokens": ["Dies", "ist", "mei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Die in allem Leide", "tokens": ["Die", "in", "al\u00b7lem", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIS", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Furcht und Trauren stillt.", "tokens": ["Furcht", "und", "Trau\u00b7ren", "stillt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Wenn die S\u00fcnde naget,", "tokens": ["Wenn", "die", "S\u00fcn\u00b7de", "na\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Wenn das Hauscreuz plaget,", "tokens": ["Wenn", "das", "Haus\u00b7creuz", "pla\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Wenn die Misgunst billt,", "tokens": ["Wenn", "die", "Mis\u00b7gunst", "billt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Wenn kein Freund", "tokens": ["Wenn", "kein", "Freund"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Es redlich meint,", "tokens": ["Es", "red\u00b7lich", "meint", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So zerst\u00f6rt doch ihr Betriegen", "tokens": ["So", "zer\u00b7st\u00f6rt", "doch", "ihr", "Be\u00b7trie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Niemahls mein Vergn\u00fcgen.", "tokens": ["Nie\u00b7mahls", "mein", "Ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Giebt es was zu streiten,", "tokens": ["Giebt", "es", "was", "zu", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Will die Hofnung gleiten", "tokens": ["Will", "die", "Hof\u00b7nung", "glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und der Muth vergehn,", "tokens": ["Und", "der", "Muth", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Herr, du und mein Glaube", "tokens": ["Herr", ",", "du", "und", "mein", "Glau\u00b7be"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPER", "KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "L\u00e4st mich nicht im Raube", "tokens": ["L\u00e4st", "mich", "nicht", "im", "Rau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Noch im Ungl\u00fcck stehn.", "tokens": ["Noch", "im", "Un\u00b7gl\u00fcck", "stehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Mir verspricht", "tokens": ["Mir", "ver\u00b7spricht"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Die Zuversicht,", "tokens": ["Die", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "So ich auf den H\u00f6chsten seze,", "tokens": ["So", "ich", "auf", "den", "H\u00f6chs\u00b7ten", "se\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.10": {"text": "Reiche Seelensch\u00e4ze.", "tokens": ["Rei\u00b7che", "See\u00b7len\u00b7sch\u00e4\u00b7ze", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Sterben ist Gewinnen,", "tokens": ["Ster\u00b7ben", "ist", "Ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Es rei\u00dft Geist und Sinnen", "tokens": ["Es", "rei\u00dft", "Geist", "und", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Aus der ganzen Noth.", "tokens": ["Aus", "der", "gan\u00b7zen", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Ist gleich Fleisch und Blute", "tokens": ["Ist", "gleich", "Fleisch", "und", "Blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "NN", "KON", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Schlecht dabey zu Muthe?", "tokens": ["Schlecht", "da\u00b7bey", "zu", "Mu\u00b7the", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PAV", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Nimmermehr, o Tod!", "tokens": ["Nim\u00b7mer\u00b7mehr", ",", "o", "Tod", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "FM", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.7": {"text": "Denn dein Krieg", "tokens": ["Denn", "dein", "Krieg"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Verliert den Sieg,", "tokens": ["Ver\u00b7liert", "den", "Sieg", ","], "token_i