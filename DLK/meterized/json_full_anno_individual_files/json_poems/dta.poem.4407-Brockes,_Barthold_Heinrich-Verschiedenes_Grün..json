{"dta.poem.4407": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Verschiedenes Gr\u00fcn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1727", "urn": "urn:nbn:de:kobv:b4-200905198599", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach GOtt, Du Urqvell aller Lust,", "tokens": ["Ach", "Gott", ",", "Du", "Urq\u00b7vell", "al\u00b7ler", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach GOtt, Du Geber aller Gaben,", "tokens": ["Ach", "Gott", ",", "Du", "Ge\u00b7ber", "al\u00b7ler", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$,", "PPER", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ach la\u00df an Dir mich Sel\u2019 und Brust", "tokens": ["Ach", "la\u00df", "an", "Dir", "mich", "Sel'", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "APPR", "PPER", "PRF", "NE", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Ehrfurchts-voller Freude laben!", "tokens": ["Mit", "Ehr\u00b7furchts\u00b7vol\u00b7ler", "Freu\u00b7de", "la\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gib, da\u00df, so oft ich\u2019s Gr\u00fcne sehe,", "tokens": ["Gib", ",", "da\u00df", ",", "so", "oft", "ich's", "Gr\u00fc\u00b7ne", "se\u00b7he", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "$,", "ADV", "ADV", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es Dir zur Ehr\u2019, in meiner Lust, geschehe!", "tokens": ["Es", "Dir", "zur", "Ehr'", ",", "in", "mei\u00b7ner", "Lust", ",", "ge\u00b7sche\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPER", "APPRART", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}