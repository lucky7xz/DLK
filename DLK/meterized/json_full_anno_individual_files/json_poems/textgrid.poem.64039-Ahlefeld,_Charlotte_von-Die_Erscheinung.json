{"textgrid.poem.64039": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Die Erscheinung", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist's Dein Schatten, der mit lindem Wehen", "tokens": ["Ist's", "Dein", "Schat\u00b7ten", ",", "der", "mit", "lin\u00b7dem", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leise oft, und fl\u00fcsternd um mich schwebt,", "tokens": ["Lei\u00b7se", "oft", ",", "und", "fl\u00fcs\u00b7ternd", "um", "mich", "schwebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dass mir ahnungsvoll das Herz erbebt", "tokens": ["Dass", "mir", "ah\u00b7nungs\u00b7voll", "das", "Herz", "er\u00b7bebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir Thr\u00e4nen in den Augen stehen?", "tokens": ["Und", "mir", "Thr\u00e4\u00b7nen", "in", "den", "Au\u00b7gen", "ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Deinen K\u00f6rper birgt das dunkle Grab;", "tokens": ["Dei\u00b7nen", "K\u00f6r\u00b7per", "birgt", "das", "dunk\u00b7le", "Grab", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Doch in lichte, h\u00f6h're Regionen,", "tokens": ["Doch", "in", "lich\u00b7te", ",", "h\u00f6h'\u00b7re", "Re\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Unter Engeln schwesterlich zu wohnen,", "tokens": ["Un\u00b7ter", "En\u00b7geln", "schwes\u00b7ter\u00b7lich", "zu", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Schwang der Geist sich, welchen Gott Dir gab.", "tokens": ["Schwang", "der", "Geist", "sich", ",", "wel\u00b7chen", "Gott", "Dir", "gab", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PRF", "$,", "PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Sollt' er liebevoll mir wiederkehren,", "tokens": ["Sollt'", "er", "lie\u00b7be\u00b7voll", "mir", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Weil er weiss, wie bang ich Dich entbehrt?", "tokens": ["Weil", "er", "weiss", ",", "wie", "bang", "ich", "Dich", "ent\u00b7behrt", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Weil mein Herz, von Sehnsucht still verzehrt,", "tokens": ["Weil", "mein", "Herz", ",", "von", "Sehn\u00b7sucht", "still", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "APPR", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sich des bittern Grams nicht kann erwehren?", "tokens": ["Sich", "des", "bit\u00b7tern", "Grams", "nicht", "kann", "er\u00b7weh\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "ADJA", "NN", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "O gieb Antwort mir auf diese Frage,", "tokens": ["O", "gieb", "Ant\u00b7wort", "mir", "auf", "die\u00b7se", "Fra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVIMP", "NN", "PPER", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Denn Dein nachtumh\u00fclltes Schattenbild", "tokens": ["Denn", "Dein", "nach\u00b7tum\u00b7h\u00fcll\u00b7tes", "Schat\u00b7ten\u00b7bild"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ohne Deine Rede, sanft und mild,", "tokens": ["Oh\u00b7ne", "Dei\u00b7ne", "Re\u00b7de", ",", "sanft", "und", "mild", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Weckt nur inniger der Wehmuth Klage.", "tokens": ["Weckt", "nur", "in\u00b7ni\u00b7ger", "der", "Weh\u00b7muth", "Kla\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "ART", "NN", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Sprich wie sonst, mit freundlichem Vertrauen,", "tokens": ["Sprich", "wie", "sonst", ",", "mit", "freund\u00b7li\u00b7chem", "Ver\u00b7trau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "KOKOM", "ADV", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das Dich wiederum mir n\u00e4her bringt,", "tokens": ["Das", "Dich", "wie\u00b7de\u00b7rum", "mir", "n\u00e4\u00b7her", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Ach der Schmerz, der jetzt mich tief durchdringt,", "tokens": ["Ach", "der", "Schmerz", ",", "der", "jetzt", "mich", "tief", "durch\u00b7dringt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ART", "NN", "$,", "PRELS", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "L\u00f6st sich sonst in schauerliches Grauen.", "tokens": ["L\u00f6st", "sich", "sonst", "in", "schau\u00b7er\u00b7li\u00b7ches", "Grau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "\u00bbfasse Muth, und hebe ohne Thr\u00e4nen", "tokens": ["\u00bb", "fas\u00b7se", "Muth", ",", "und", "he\u00b7be", "oh\u00b7ne", "Thr\u00e4\u00b7nen"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Deine Blicke liebend zu mir auf.", "tokens": ["Dei\u00b7ne", "Bli\u00b7cke", "lie\u00b7bend", "zu", "mir", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Um zu lindern Dein unendlich Sehnen", "tokens": ["Um", "zu", "lin\u00b7dern", "Dein", "un\u00b7end\u00b7lich", "Seh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PTKZU", "VVINF", "PPOSAT", "ADJD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Stieg ich aus der Schattenwelt herauf.", "tokens": ["Stieg", "ich", "aus", "der", "Schat\u00b7ten\u00b7welt", "her\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Sieh, ich ", "tokens": ["Sieh", ",", "ich"], "token_info": ["word", "punct", "word"], "pos": ["NE", "$,", "PPER"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Was in reiner Unschuld einst gelebt,", "tokens": ["Was", "in", "rei\u00b7ner", "Un\u00b7schuld", "einst", "ge\u00b7lebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und gestillt in wonnevollen Wehen", "tokens": ["Und", "ge\u00b7stillt", "in", "won\u00b7ne\u00b7vol\u00b7len", "We\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVPP", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wird der Schmerz, der irdisch uns durchbebt.", "tokens": ["Wird", "der", "Schmerz", ",", "der", "ir\u00b7disch", "uns", "durch\u00b7bebt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Warum klagst Du, dass ich fr\u00fch gesunken,", "tokens": ["Wa\u00b7rum", "klagst", "Du", ",", "dass", "ich", "fr\u00fch", "ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "In der Erde k\u00fchlen Mutterschooss?", "tokens": ["In", "der", "Er\u00b7de", "k\u00fch\u00b7len", "Mut\u00b7ter\u00b7scho\u00b7oss", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Vom Entz\u00fccken h\u00f6h'rer Sph\u00e4ren trunken,", "tokens": ["Vom", "Ent\u00b7z\u00fc\u00b7cken", "h\u00f6h'\u00b7rer", "Sph\u00e4\u00b7ren", "trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJA", "NN", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ist der Himmel Wonne nun mein Loos.", "tokens": ["Ist", "der", "Him\u00b7mel", "Won\u00b7ne", "nun", "mein", "Loos", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Denn in Staub zerfallen ist die H\u00fclle,", "tokens": ["Denn", "in", "Staub", "zer\u00b7fal\u00b7len", "ist", "die", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Die so schmerzvoll meinen Geist umwand;", "tokens": ["Die", "so", "schmerz\u00b7voll", "mei\u00b7nen", "Geist", "um\u00b7wand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Jetzt mit ew'ger Ruh' ein bessres Land.", "tokens": ["Jetzt", "mit", "ew'\u00b7ger", "Ruh'", "ein", "bess\u00b7res", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Dass ich noch Dir tr\u00f6stend wiederkehre,", "tokens": ["Dass", "ich", "noch", "Dir", "tr\u00f6s\u00b7tend", "wie\u00b7der\u00b7keh\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ist der Freundschaft Werk, die fest und rein", "tokens": ["Ist", "der", "Freund\u00b7schaft", "Werk", ",", "die", "fest", "und", "rein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Uns vereinigte, und sieh, ich ehre", "tokens": ["Uns", "ver\u00b7ei\u00b7nig\u00b7te", ",", "und", "sieh", ",", "ich", "eh\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "VVFIN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ihr Gebot auch noch im bessern Seyn.", "tokens": ["Ihr", "Ge\u00b7bot", "auch", "noch", "im", "bes\u00b7sern", "Seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Scheiden muss ich, aber stillen Frieden", "tokens": ["Schei\u00b7den", "muss", "ich", ",", "a\u00b7ber", "stil\u00b7len", "Frie\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VMFIN", "PPER", "$,", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Statt der bangen Sehnsucht nimm von mir.", "tokens": ["Statt", "der", "ban\u00b7gen", "Sehn\u00b7sucht", "nimm", "von", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wiedersehen ist uns einst beschieden,", "tokens": ["Wie\u00b7der\u00b7se\u00b7hen", "ist", "uns", "einst", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Denn des Lebens Fackel l\u00f6scht auch Dir.", "tokens": ["Denn", "des", "Le\u00b7bens", "Fa\u00b7ckel", "l\u00f6scht", "auch", "Dir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "ADV", "PPER", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Ruhig sieh zu meiner Gruft hinab,", "tokens": ["Ru\u00b7hig", "sieh", "zu", "mei\u00b7ner", "Gruft", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Denn der Menschheit edelste Gef\u00fchle", "tokens": ["Denn", "der", "Menschheit", "e\u00b7dels\u00b7te", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Werden nicht zu Staub im tiefen Grab \u2013", "tokens": ["Wer\u00b7den", "nicht", "zu", "Staub", "im", "tie\u00b7fen", "Grab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PTKNEG", "APPR", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Fest bestehn sie noch am letzten Ziele.\u00ab", "tokens": ["Fest", "be\u00b7stehn", "sie", "noch", "am", "letz\u00b7ten", "Zie\u00b7le", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Ist's Dein Schatten, der mit lindem Wehen", "tokens": ["Ist's", "Dein", "Schat\u00b7ten", ",", "der", "mit", "lin\u00b7dem", "We\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Leise oft, und fl\u00fcsternd um mich schwebt,", "tokens": ["Lei\u00b7se", "oft", ",", "und", "fl\u00fcs\u00b7ternd", "um", "mich", "schwebt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KON", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Dass mir ahnungsvoll das Herz erbebt", "tokens": ["Dass", "mir", "ah\u00b7nungs\u00b7voll", "das", "Herz", "er\u00b7bebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "ART"