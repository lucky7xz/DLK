{"dta.poem.16219": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "273.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht leicht ein Sch\u00f6nes wird, ein Gutes seyn, wovon", "tokens": ["Nicht", "leicht", "ein", "Sch\u00f6\u00b7nes", "wird", ",", "ein", "Gu\u00b7tes", "seyn", ",", "wo\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PTKNEG", "ADJD", "ART", "NN", "VAFIN", "$,", "ART", "NN", "VAINF", "$,", "PWAV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nicht gesagt ein Wort, gesungen einen Ton.", "tokens": ["Ich", "nicht", "ge\u00b7sagt", "ein", "Wort", ",", "ge\u00b7sun\u00b7gen", "ei\u00b7nen", "Ton", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "VVPP", "ART", "NN", "$,", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Drum kann ich wohlgemuth gehn durch die Einsamkeiten,", "tokens": ["Drum", "kann", "ich", "wohl\u00b7ge\u00b7muth", "gehn", "durch", "die", "Ein\u00b7sam\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo solche Ch\u00f6re mich von Genien begleiten.", "tokens": ["Wo", "sol\u00b7che", "Ch\u00f6\u00b7re", "mich", "von", "Ge\u00b7ni\u00b7en", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "PRF", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Aufsprosset sanft und mild mir hier und dort ein Bild,", "tokens": ["Auf\u00b7spros\u00b7set", "sanft", "und", "mild", "mir", "hier", "und", "dort", "ein", "Bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "PPER", "ADV", "KON", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und schm\u00fcckt mit Fr\u00fchlingstraum das winternde Gefild.", "tokens": ["Und", "schm\u00fcckt", "mit", "Fr\u00fch\u00b7lings\u00b7traum", "das", "win\u00b7tern\u00b7de", "Ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}