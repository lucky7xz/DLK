{"dta.poem.8865": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Und sie konnten kaum empfinden, was dem Busen kaum", "tokens": ["Und", "sie", "konn\u00b7ten", "kaum", "emp\u00b7fin\u00b7den", ",", "was", "dem", "Bu\u00b7sen", "kaum"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADV", "VVINF", "$,", "PRELS", "ART", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Nicht den Geist, der scharf und sicher in des Lebens", "tokens": ["Nicht", "den", "Geist", ",", "der", "scharf", "und", "si\u00b7cher", "in", "des", "Le\u00b7bens"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ART", "NN", "$,", "PRELS", "ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Nicht die zarten Klagelaute jener Seele voll Gesang!", "tokens": ["Nicht", "die", "zar\u00b7ten", "Kla\u00b7ge\u00b7lau\u00b7te", "je\u00b7ner", "See\u00b7le", "voll", "Ge\u00b7sang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "ADJA", "NN", "PDAT", "NN", "ADJD", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Kalt und ahnungslos und schweigend, ja mit Hohn", "tokens": ["Kalt", "und", "ah\u00b7nungs\u00b7los", "und", "schwei\u00b7gend", ",", "ja", "mit", "Hohn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "KON", "ADJD", "$,", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.5": {"text": "W\u00e4hrend sie um niedre Stirnen ihre schn\u00f6den Zweige", "tokens": ["W\u00e4h\u00b7rend", "sie", "um", "nied\u00b7re", "Stir\u00b7nen", "ih\u00b7re", "schn\u00f6\u00b7den", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Mir indessen, dem's im Busen thatenschwanger w\u00fchlte,", "tokens": ["Mir", "in\u00b7des\u00b7sen", ",", "dem's", "im", "Bu\u00b7sen", "tha\u00b7ten\u00b7schwan\u00b7ger", "w\u00fchl\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PRELS", "APPRART", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.7": {"text": "Diente selbst der Scherz als Maske, wenn ich tiefe", "tokens": ["Dien\u00b7te", "selbst", "der", "Scherz", "als", "Mas\u00b7ke", ",", "wenn", "ich", "tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "KOUS", "NN", "$,", "KOUS", "PPER", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Doch getrost! Vielleicht nach Jahren, wenn den K\u00f6rper", "tokens": ["Doch", "ge\u00b7trost", "!", "Viel\u00b7leicht", "nach", "Jah\u00b7ren", ",", "wenn", "den", "K\u00f6r\u00b7per"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVPP", "$.", "ADV", "APPR", "NN", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Wird mein Schatte gl\u00e4nzend wandeln dieses deutsche", "tokens": ["Wird", "mein", "Schat\u00b7te", "gl\u00e4n\u00b7zend", "wan\u00b7deln", "die\u00b7ses", "deut\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "VVFIN", "PDAT", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}}}}