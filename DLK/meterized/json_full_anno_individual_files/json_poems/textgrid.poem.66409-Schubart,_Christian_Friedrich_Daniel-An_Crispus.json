{"textgrid.poem.66409": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Crispus", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Crispus, der berauscht von Gl\u00fcck,", "tokens": ["Herr", "Cris\u00b7pus", ",", "der", "be\u00b7rauscht", "von", "Gl\u00fcck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Recht gro\u00dfe Augen drehet,", "tokens": ["Recht", "gro\u00b7\u00dfe", "Au\u00b7gen", "dre\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und immer mit dem Falkenblick", "tokens": ["Und", "im\u00b7mer", "mit", "dem", "Fal\u00b7ken\u00b7blick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des N\u00e4chsten Fehler sp\u00e4het;", "tokens": ["Des", "N\u00e4chs\u00b7ten", "Feh\u00b7ler", "sp\u00e4\u00b7het", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "O werde in dem neuen Jahr", "tokens": ["O", "wer\u00b7de", "in", "dem", "neu\u00b7en", "Jahr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch blinder, als Tobias war,", "tokens": ["Noch", "blin\u00b7der", ",", "als", "To\u00b7bias", "war", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": [