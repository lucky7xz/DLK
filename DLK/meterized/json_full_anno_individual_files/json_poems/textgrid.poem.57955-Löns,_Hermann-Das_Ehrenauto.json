{"textgrid.poem.57955": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Ehrenauto", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu diesem deinem Ehrentage", "tokens": ["Zu", "die\u00b7sem", "dei\u00b7nem", "Eh\u00b7ren\u00b7ta\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nah'n wir uns mit Ergebenheit", "tokens": ["Nah'n", "wir", "uns", "mit", "Er\u00b7ge\u00b7ben\u00b7heit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "APPR", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.3": {"text": "Mit einem kleinen Angebinde,", "tokens": ["Mit", "ei\u00b7nem", "klei\u00b7nen", "An\u00b7ge\u00b7bin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind gl\u00fccklich, wenn es dich erfreut.", "tokens": ["Sind", "gl\u00fcck\u00b7lich", ",", "wenn", "es", "dich", "er\u00b7freut", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "KOUS", "PPER", "PRF", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nur f\u00fcnfunddrei\u00dfig Pferdekr\u00e4fte", "tokens": ["Nur", "f\u00fcn\u00b7fund\u00b7drei\u00b7\u00dfig", "Pfer\u00b7de\u00b7kr\u00e4f\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADV", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hat leider es, der reine Quark,", "tokens": ["Hat", "lei\u00b7der", "es", ",", "der", "rei\u00b7ne", "Quark", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "w