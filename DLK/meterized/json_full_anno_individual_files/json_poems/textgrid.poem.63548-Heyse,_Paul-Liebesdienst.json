{"textgrid.poem.63548": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Liebesdienst", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn das Haus im W\u00fcsten liegt,", "tokens": ["Wenn", "das", "Haus", "im", "W\u00fcs\u00b7ten", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wem gefielen G\u00e4ste?", "tokens": ["Wem", "ge\u00b7fie\u00b7len", "G\u00e4s\u00b7te", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Staub, der aus den Winkeln fliegt,", "tokens": ["Staub", ",", "der", "aus", "den", "Win\u00b7keln", "fliegt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Kehrt man vor dem Feste.", "tokens": ["Kehrt", "man", "vor", "dem", "Fes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Als ich just im Herzen tief", "tokens": ["Als", "ich", "just", "im", "Her\u00b7zen", "tief"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ordnung schaffen wollte,", "tokens": ["Ord\u00b7nung", "schaf\u00b7fen", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00f6rt' ich, wie ein Stimmchen rief,", "tokens": ["H\u00f6rt'", "ich", ",", "wie", "ein", "Stimm\u00b7chen", "rief", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich \u00f6ffnen sollte.", "tokens": ["Da\u00df", "ich", "\u00f6ff\u00b7nen", "soll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVINF", "VMFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Ach, die sch\u00f6ne Liebe stand", "tokens": ["Ach", ",", "die", "sch\u00f6\u00b7ne", "Lie\u00b7be", "stand"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Bittend an der Schwelle;", "tokens": ["Bit\u00b7tend", "an", "der", "Schwel\u00b7le", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df sie es im Argen fand,", "tokens": ["Da\u00df", "sie", "es", "im", "Ar\u00b7gen", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Klagt' ich ihr zur Stelle.", "tokens": ["Klagt'", "ich", "ihr", "zur", "Stel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Doch sie lacht' mir ins Gesicht,", "tokens": ["Doch", "sie", "lacht'", "mir", "ins", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Sprang ins Haus behende,", "tokens": ["Sprang", "ins", "Haus", "be\u00b7hen\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADJA", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Und wie l\u00e4ngst gewohnt der Pflicht,", "tokens": ["Und", "wie", "l\u00e4ngst", "ge\u00b7wohnt", "der", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "VVPP", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "R\u00fchrte sie die H\u00e4nde.", "tokens": ["R\u00fchr\u00b7te", "sie", "die", "H\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Staunend sag' ich, wenn ihr fragt:", "tokens": ["Stau\u00b7nend", "sag'", "ich", ",", "wenn", "ihr", "fragt", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Welch ein Glanz tiefinnen?", "tokens": ["Welch", "ein", "Glanz", "tie\u00b7fin\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbdie das Haus gefegt als Magd,", "tokens": ["\u00bb", "die", "das", "Haus", "ge\u00b7fegt", "als", "Magd", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PRELS", "ART", "NN", "VVPP", "KOKOM", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohnt als F\u00fcrstin drinnen.\u00ab"