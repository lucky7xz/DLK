{"dta.poem.8698": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Die Heideschenke .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Ich zog durch's weite Ungerland;", "tokens": ["Ich", "zog", "durch's", "wei\u00b7te", "Un\u00b7ger\u00b7land", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Herz fand seine Freude,", "tokens": ["Mein", "Herz", "fand", "sei\u00b7ne", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Als Dorf und Busch und Baum verschwand", "tokens": ["Als", "Dorf", "und", "Busch", "und", "Baum", "ver\u00b7schwand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "KON", "NE", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf einer stillen Heide.", "tokens": ["Auf", "ei\u00b7ner", "stil\u00b7len", "Hei\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Heide war so still, so leer,", "tokens": ["Die", "Hei\u00b7de", "war", "so", "still", ",", "so", "leer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abendhimmel zogen", "tokens": ["Am", "A\u00b7bend\u00b7him\u00b7mel", "zo\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wolken hin, gewitterschwer,", "tokens": ["Die", "Wol\u00b7ken", "hin", ",", "ge\u00b7wit\u00b7ter\u00b7schwer", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und leise Blitze flogen.", "tokens": ["Und", "lei\u00b7se", "Blit\u00b7ze", "flo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da h\u00f6rt' ich in der Ferne was,", "tokens": ["Da", "h\u00f6rt'", "ich", "in", "der", "Fer\u00b7ne", "was", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PWS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dunkler, meilenweiter;", "tokens": ["In", "dunk\u00b7ler", ",", "mei\u00b7len\u00b7wei\u00b7ter", ";"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich legte's Ohr an's knappe Gras,", "tokens": ["Ich", "leg\u00b7te's", "Ohr", "an's", "knap\u00b7pe", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir war als k\u00e4men Reiter.", "tokens": ["Mir", "war", "als", "k\u00e4\u00b7men", "Rei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KOKOM", "VVFIN", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als sie kamen n\u00e4herw\u00e4rts,", "tokens": ["Und", "als", "sie", "ka\u00b7men", "n\u00e4\u00b7her\u00b7w\u00e4rts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Begann der Grund zu zittern,", "tokens": ["Be\u00b7gann", "der", "Grund", "zu", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Stets b\u00e4nger, wie ein zages Herz", "tokens": ["Stets", "b\u00e4n\u00b7ger", ",", "wie", "ein", "za\u00b7ges", "Herz"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei nahenden Gewittern.", "tokens": ["Bei", "na\u00b7hen\u00b7den", "Ge\u00b7wit\u00b7tern", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}