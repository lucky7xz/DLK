{"textgrid.poem.55354": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Auf den Kauf", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist einer, der sich qu\u00e4let", "tokens": ["Wo", "ist", "ei\u00b7ner", ",", "der", "sich", "qu\u00e4\u00b7let"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ART", "$,", "PRELS", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit der Last, die wir getragen?", "tokens": ["Mit", "der", "Last", ",", "die", "wir", "ge\u00b7tra\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn es an Gestalten fehlet,", "tokens": ["Wenn", "es", "an", "Ge\u00b7stal\u00b7ten", "feh\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist ein Kreuz geschwind geschlagen.", "tokens": ["Ist", "ein", "Kreuz", "ge\u00b7schwind", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Pfaffenhelden singen sie,", "tokens": ["Pfaf\u00b7fen\u00b7hel\u00b7den", "sin\u00b7gen", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Frauen wohl empfohlen,", "tokens": ["Frau\u00b7en", "wohl", "emp\u00b7foh\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Oberleder bringen sie,", "tokens": ["O\u00b7berl\u00b7e\u00b7der", "brin\u00b7gen", "sie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber keine Sohlen.", "tokens": ["A\u00b7ber", "kei\u00b7ne", "Soh\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Jung' und Alte, gro\u00df und klein,", "tokens": ["Jung'", "und", "Al\u00b7te", ",", "gro\u00df", "und", "klein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Gr\u00e4\u00dfliches Gelichter!", "tokens": ["Gr\u00e4\u00df\u00b7li\u00b7ches", "Ge\u00b7lich\u00b7ter", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Niemand will ein Schuster sein,", "tokens": ["Nie\u00b7mand", "will", "ein", "Schus\u00b7ter", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedermann ein Dichter.", "tokens": ["Je\u00b7der\u00b7mann", "ein", "Dich\u00b7ter."], "token_info": ["word", "word", "abbreviation"], "pos": ["PIS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Alle kommen sie gerennt,", "tokens": ["Al\u00b7le", "kom\u00b7men", "sie", "ge\u00b7rennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"t