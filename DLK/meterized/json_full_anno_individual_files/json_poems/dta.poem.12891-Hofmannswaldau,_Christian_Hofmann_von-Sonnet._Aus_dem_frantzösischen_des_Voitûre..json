{"dta.poem.12891": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Aus dem frantz\u00f6sischen des   Voit\u00fbre.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es lie\u00df sich Florida, der sch\u00f6nheit ebenbild,", "tokens": ["Es", "lie\u00df", "sich", "Flo\u00b7ri\u00b7da", ",", "der", "sch\u00f6n\u00b7heit", "e\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "NE", "$,", "PRELS", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bey k\u00fchler abend-zeit in rosen-tracht erblicken,", "tokens": ["Bey", "k\u00fch\u00b7ler", "a\u00b7ben\u00b7dzeit", "in", "ro\u00b7sen\u00b7tracht", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie wu\u00dfte sich so licht und gl\u00e4ntzend auszuschm\u00fccken,", "tokens": ["Sie", "wu\u00df\u00b7te", "sich", "so", "licht", "und", "gl\u00e4nt\u00b7zend", "aus\u00b7zu\u00b7schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "ADJD", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sie die gantze welt vor die Aurora hielt.", "tokens": ["Da\u00df", "sie", "die", "gant\u00b7ze", "welt", "vor", "die", "Au\u00b7ro\u00b7ra", "hielt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "ART", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es war die linde lufft mit s\u00fc\u00dfem schall erf\u00fcllt,", "tokens": ["Es", "war", "die", "lin\u00b7de", "lufft", "mit", "s\u00fc\u00b7\u00dfem", "schall", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die erde gr\u00fc\u00dfte sie mit tausend blumen-st\u00fccken,", "tokens": ["Die", "er\u00b7de", "gr\u00fc\u00df\u00b7te", "sie", "mit", "tau\u00b7send", "blu\u00b7men\u00b7st\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Man sah\u2019 die sternen sich auf ihren abschied schicken,", "tokens": ["Man", "sah'", "die", "ster\u00b7nen", "sich", "auf", "ih\u00b7ren", "ab\u00b7schied", "schi\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie, wenn Aurorens thau der muscheln sehnsucht stillt.", "tokens": ["Wie", ",", "wenn", "Au\u00b7ro\u00b7rens", "thau", "der", "mu\u00b7scheln", "sehn\u00b7sucht", "stillt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "KOUS", "NE", "NE", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die sonne lie\u00df die schoos der holden Thetis fahren,", "tokens": ["Die", "son\u00b7ne", "lie\u00df", "die", "schoos", "der", "hol\u00b7den", "The\u00b7tis", "fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Blie\u00df ihre flammen auf, die schon verloschen waren,", "tokens": ["Blie\u00df", "ih\u00b7re", "flam\u00b7men", "auf", ",", "die", "schon", "ver\u00b7lo\u00b7schen", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "VVFIN", "PTKVZ", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und lie\u00df die deichsel sich zu unsrer Sch\u00f6nen drehn.", "tokens": ["Und", "lie\u00df", "die", "deich\u00b7sel", "sich", "zu", "uns\u00b7rer", "Sch\u00f6\u00b7nen", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Die Thetis hatte selbst der vorzug eingenommen;", "tokens": ["Die", "The\u00b7tis", "hat\u00b7te", "selbst", "der", "vor\u00b7zug", "ein\u00b7ge\u00b7nom\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Doch war ihr Florida kaum unter augen kommen,", "tokens": ["Doch", "war", "ihr", "Flo\u00b7ri\u00b7da", "kaum", "un\u00b7ter", "au\u00b7gen", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "So kroch sie in das meer und lie\u00df sich nicht mehr sehn.", "tokens": ["So", "kroch", "sie", "in", "das", "meer", "und", "lie\u00df", "sich", "nicht", "mehr", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "KON", "VVFIN", "PRF", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}