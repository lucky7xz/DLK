{"textgrid.poem.56700": {"metadata": {"author": {"name": "Sack, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchlingsmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Von diesen fr\u00fchlingskitschigen Vergn\u00fcgen", "tokens": ["Von", "die\u00b7sen", "fr\u00fch\u00b7lings\u00b7kit\u00b7schi\u00b7gen", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "mag mir der reine Himmel nur gen\u00fcgen \u2013", "tokens": ["mag", "mir", "der", "rei\u00b7ne", "Him\u00b7mel", "nur", "ge\u00b7n\u00fc\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "ADV", "VVINF", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "wie liegt er keusch und ewig unbewegt,", "tokens": ["wie", "liegt", "er", "keusch", "und", "e\u00b7wig", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "der mich aus dieser geilen Zeugungskraft", "tokens": ["der", "mich", "aus", "die\u00b7ser", "gei\u00b7len", "Zeu\u00b7gungs\u00b7kraft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PRF", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "hoch in die quellenfrische K\u00fchle rafft", "tokens": ["hoch", "in", "die", "quel\u00b7len\u00b7fri\u00b7sche", "K\u00fch\u00b7le", "rafft"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+