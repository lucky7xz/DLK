{"dta.poem.11830": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn etwas scheinet mehr als einen Grund zu haben,", "tokens": ["Wenn", "et\u00b7was", "schei\u00b7net", "mehr", "als", "ei\u00b7nen", "Grund", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PIS", "KOKOM", "ART", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So denke nur, du hast noch recht nicht nachgegraben.", "tokens": ["So", "den\u00b7ke", "nur", ",", "du", "hast", "noch", "recht", "nicht", "nach\u00b7ge\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "ADV", "ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn du recht auf den Grund nachgrubest, wird dir kund:", "tokens": ["Wenn", "du", "recht", "auf", "den", "Grund", "nach\u00b7gru\u00b7best", ",", "wird", "dir", "kund", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht viele Gr\u00fcnde sinds, es ist ein einz'ger Grund.", "tokens": ["Nicht", "vie\u00b7le", "Gr\u00fcn\u00b7de", "sinds", ",", "es", "ist", "ein", "einz'\u00b7ger", "Grund", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "po