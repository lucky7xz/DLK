{"textgrid.poem.42131": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "An Madame de Warens", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm dieses Bild, mit ihm die alte Treue,", "tokens": ["Nimm", "die\u00b7ses", "Bild", ",", "mit", "ihm", "die", "al\u00b7te", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$,", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das reine Herz, das einst sich dir geweiht.", "tokens": ["Das", "rei\u00b7ne", "Herz", ",", "das", "einst", "sich", "dir", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADV", "PRF", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Vertrauensvoll erfleht es sich aufs neue", "tokens": ["Ver\u00b7trau\u00b7ens\u00b7voll", "er\u00b7fleht", "es", "sich", "aufs", "neu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur einen Funken deiner G\u00f6ttlichkeit.", "tokens": ["Nur", "ei\u00b7nen", "Fun\u00b7ken", "dei\u00b7ner", "G\u00f6tt\u00b7lich\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Noch ist der zarte Flor ja nicht zerrissen,", "tokens": ["Noch", "ist", "der", "zar\u00b7te", "Flor", "ja", "nicht", "zer\u00b7ris\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit dem du mich in sch\u00f6ner Zeit umwobst,", "tokens": ["Mit", "dem", "du", "mich", "in", "sch\u00f6\u00b7ner", "Zeit", "um\u00b7wobst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Darin du mich empor aus Finsternissen", "tokens": ["Da\u00b7rin", "du", "mich", "em\u00b7por", "aus", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "PPER", "PRF", "PTKVZ", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zum blauen \u00c4ther deiner Liebe hobst.", "tokens": ["Zum", "blau\u00b7en", "\u00c4\u00b7ther", "dei\u00b7ner", "Lie\u00b7be", "hobst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nun m\u00f6cht an deiner Brust es wiedrum rasten", "tokens": ["Nun", "m\u00f6cht", "an", "dei\u00b7ner", "Brust", "es", "wie\u00b7drum", "ras\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und lauschen deiner Stimme weichem Klang.", "tokens": ["Und", "lau\u00b7schen", "dei\u00b7ner", "Stim\u00b7me", "wei\u00b7chem", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die Melodien, die es dort erfa\u00dften,", "tokens": ["Die", "Me\u00b7lo\u00b7dien", ",", "die", "es", "dort", "er\u00b7fa\u00df\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.12": {"text": "Sie hallen fort noch manchen Sommer lang.", "tokens": ["Sie", "hal\u00b7len", "fort", "noch", "man\u00b7chen", "Som\u00b7mer", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "ADV", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Welt ist \u00fcberreich an Gl\u00fcck und Freuden,", "tokens": ["Die", "Welt", "ist", "\u00fc\u00b7berr\u00b7eich", "an", "Gl\u00fcck", "und", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch reicher, hohe K\u00f6nigin, bist du.", "tokens": ["Doch", "rei\u00b7cher", ",", "ho\u00b7he", "K\u00f6\u00b7ni\u00b7gin", ",", "bist", "du", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "ADJA", "NN", "$,", "VAFIN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du wagst die Sch\u00e4tze sorglos zu vergeuden,", "tokens": ["Du", "wagst", "die", "Sch\u00e4t\u00b7ze", "sorg\u00b7los", "zu", "ver\u00b7geu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die andre h\u00fcten in besorgter Ruh.", "tokens": ["Die", "and\u00b7re", "h\u00fc\u00b7ten", "in", "be\u00b7sorg\u00b7ter", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und stets von neuem hast du reich zu geben", "tokens": ["Und", "stets", "von", "neu\u00b7em", "hast", "du", "reich", "zu", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ADJA", "VAFIN", "PPER", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Des Golds, das deiner Seele Tiefen f\u00fcllt.", "tokens": ["Des", "Golds", ",", "das", "dei\u00b7ner", "See\u00b7le", "Tie\u00b7fen", "f\u00fcllt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie manchen Schmerz in deiner N\u00e4chsten Leben", "tokens": ["Wie", "man\u00b7chen", "Schmerz", "in", "dei\u00b7ner", "N\u00e4chs\u00b7ten", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hast du mit mildem Himmelstrost gestillt.", "tokens": ["Hast", "du", "mit", "mil\u00b7dem", "Him\u00b7mel\u00b7strost", "ge\u00b7stillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Der Mensch verzweifelt unter schweren Qualen,", "tokens": ["Der", "Mensch", "ver\u00b7zwei\u00b7felt", "un\u00b7ter", "schwe\u00b7ren", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Siecht hin und altert in Entmutigung,", "tokens": ["Siecht", "hin", "und", "al\u00b7tert", "in", "Ent\u00b7mu\u00b7ti\u00b7gung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da leuchten deines Auges warme Strahlen", "tokens": ["Da", "leuch\u00b7ten", "dei\u00b7nes", "Au\u00b7ges", "war\u00b7me", "Strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und der gebeugte Geist ist wieder jung.", "tokens": ["Und", "der", "ge\u00b7beug\u00b7te", "Geist", "ist", "wie\u00b7der", "jung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Verla\u00df mich nicht; ich habe dir zu danken,", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", ";", "ich", "ha\u00b7be", "dir", "zu", "dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "PPER", "VAFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was Sch\u00f6nes jetzt in meinem Herzen ruht.", "tokens": ["Was", "Sch\u00f6\u00b7nes", "jetzt", "in", "mei\u00b7nem", "Her\u00b7zen", "ruht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Flammenbecher, den vereint wir tranken,", "tokens": ["Der", "Flam\u00b7men\u00b7be\u00b7cher", ",", "den", "ver\u00b7eint", "wir", "tran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Go\u00df lautres Feuer in mein junges Blut.", "tokens": ["Go\u00df", "laut\u00b7res", "Feu\u00b7er", "in", "mein", "jun\u00b7ges", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Verla\u00df mich nicht; mir lacht aus deinen Z\u00fcgen", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", ";", "mir", "lacht", "aus", "dei\u00b7nen", "Z\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mein Himmel, wenn du mir zur Seite stehst;", "tokens": ["Mein", "Him\u00b7mel", ",", "wenn", "du", "mir", "zur", "Sei\u00b7te", "stehst", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KOUS", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Verla\u00df mich nicht, du w\u00fcrdest mich betr\u00fcgen", "tokens": ["Ver\u00b7la\u00df", "mich", "nicht", ",", "du", "w\u00fcr\u00b7dest", "mich", "be\u00b7tr\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Um meinen Himmel, wenn du von mir gehst.", "tokens": ["Um", "mei\u00b7nen", "Him\u00b7mel", ",", "wenn", "du", "von", "mir", "gehst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Ich wei\u00df nicht, was mir noch auf Erden bliebe;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "mir", "noch", "auf", "Er\u00b7den", "blie\u00b7be", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mein Leben str\u00f6mt aus deinem Augenlicht.", "tokens": ["Mein", "Le\u00b7ben", "str\u00f6mt", "aus", "dei\u00b7nem", "Au\u00b7gen\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ich m\u00fc\u00dfte sterben ohne deine Liebe,", "tokens": ["Ich", "m\u00fc\u00df\u00b7te", "ster\u00b7ben", "oh\u00b7ne", "dei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Du Himmelsk\u00f6nigin, verla\u00df mich nicht!", "tokens": ["Du", "Him\u00b7mels\u00b7k\u00f6\u00b7ni\u00b7gin", ",", "ver\u00b7la\u00df", "mich", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVIMP", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nimm dieses Bild, mit ihm die alte Treue,", "tokens": ["Nimm", "die\u00b7ses", "Bild", ",", "mit", "ihm", "die", "al\u00b7te", "Treu\u00b7e", ","], "token_info": ["word", 