{"dta.poem.1773": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein tugendhaffter Sinn/ ein k\u00fchner Helden-Mutt/", "tokens": ["Ein", "tu\u00b7gend\u00b7haff\u00b7ter", "Sinn", "/", "ein", "k\u00fch\u00b7ner", "Hel\u00b7den\u00b7Mutt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Fall er Ehr und Ruhm durch dieses kan erwerben/", "tokens": ["Im", "Fall", "er", "Ehr", "und", "Ruhm", "durch", "die\u00b7ses", "kan", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "NN", "KON", "NN", "APPR", "PDS", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Will lieber alsobald verlieren Gutt und Blutt/", "tokens": ["Will", "lie\u00b7ber", "al\u00b7so\u00b7bald", "ver\u00b7lie\u00b7ren", "Gutt", "und", "Blutt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "ADJA", "NN", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als langsam und beschimpfft in grauen Haaren sterben.", "tokens": ["Als", "lang\u00b7sam", "und", "be\u00b7schimpfft", "in", "grau\u00b7en", "Haa\u00b7ren", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "VVFIN", "APPR", "ADJA", "NN"