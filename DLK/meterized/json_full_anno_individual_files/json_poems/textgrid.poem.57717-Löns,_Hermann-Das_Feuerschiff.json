{"textgrid.poem.57717": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Das Feuerschiff", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf glatter, grauer Woge ruht das feuerrote Feuerschiff,", "tokens": ["Auf", "glat\u00b7ter", ",", "grau\u00b7er", "Wo\u00b7ge", "ruht", "das", "feu\u00b7er\u00b7ro\u00b7te", "Feu\u00b7er\u00b7schiff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Es r\u00fchrt sich nicht; der Anker h\u00e4lt es fest im Meeressande;", "tokens": ["Es", "r\u00fchrt", "sich", "nicht", ";", "der", "An\u00b7ker", "h\u00e4lt", "es", "fest", "im", "Mee\u00b7res\u00b7san\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Fahrwasser weist sein Feuerschein durch Sandbank und durch Felsenriff", "tokens": ["Fahr\u00b7was\u00b7ser", "weist", "sein", "Feu\u00b7er\u00b7schein", "durch", "Sand\u00b7bank", "und", "durch", "Fel\u00b7sen\u00b7riff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Den Fischern in der Nebelnacht und leitet sie zum Strande.", "tokens": ["Den", "Fi\u00b7schern", "in", "der", "Ne\u00b7bel\u00b7nacht", "und", "lei\u00b7tet", "sie", "zum", "Stran\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Matrosen, grau und lendenlahm, Erleber mancher wilden Fahrt,", "tokens": ["Mat\u00b7ro\u00b7sen", ",", "grau", "und", "len\u00b7den\u00b7lahm", ",", "Er\u00b7le\u00b7ber", "man\u00b7cher", "wil\u00b7den", "Fahrt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "KON", "VVFIN", "$,", "NN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die halten hier das Feuer wach bei Nacht in Wind und Wetter;", "tokens": ["Die", "hal\u00b7ten", "hier", "das", "Feu\u00b7er", "wach", "bei", "Nacht", "in", "Wind", "und", "Wet\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Dem wurde grau in Tropenglut das Blondhaar und der rote Bart,", "tokens": ["Dem", "wur\u00b7de", "grau", "in", "Tro\u00b7pen\u00b7glut", "das", "Blond\u00b7haar", "und", "der", "ro\u00b7te", "Bart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "APPR", "NN", "ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+---+-+", "measure": "unknown.measure.septa"}, "line.4": {"text": "Den trugen bis nach Gr\u00f6nland hin die stahlbeschlagenen Bretter.", "tokens": ["Den", "tru\u00b7gen", "bis", "nach", "Gr\u00f6n\u00b7land", "hin", "die", "stahl\u00b7be\u00b7schla\u00b7ge\u00b7nen", "Bret\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der wusch im Fieber gelbes Gold aus Kaliforniens rotem Sand,", "tokens": ["Der", "wusch", "im", "Fie\u00b7ber", "gel\u00b7bes", "Gold", "aus", "Ka\u00b7li\u00b7for\u00b7ni\u00b7ens", "ro\u00b7tem", "Sand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "ADJA", "NN", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Der hat auf jedem Fleck der Welt das Handelsschiff gelandet,", "tokens": ["Der", "hat", "auf", "je\u00b7dem", "Fleck", "der", "Welt", "das", "Han\u00b7dels\u00b7schiff", "ge\u00b7lan\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PIAT", "NN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Der k\u00fc\u00dfte eines H\u00e4uptlings Weib an einem wei\u00dfen Palmenstrand;", "tokens": ["Der", "k\u00fc\u00df\u00b7te", "ei\u00b7nes", "H\u00e4upt\u00b7lings", "Weib", "an", "ei\u00b7nem", "wei\u00b7\u00dfen", "Pal\u00b7mens\u00b7trand", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Sie alle sind nach Schlu\u00df der Fahrt auf diesem Schiff gestrandet.", "tokens": ["Sie", "al\u00b7le", "sind", "nach", "Schlu\u00df", "der", "Fahrt", "auf", "die\u00b7sem", "Schiff", "ge\u00b7stran\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "VAFIN", "APPR", "NN", "ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Von Ehrgeiz, Habsucht, Liebe, Ha\u00df, von Hoffnung und von Furcht ist leer", "tokens": ["Von", "Ehr\u00b7geiz", ",", "Hab\u00b7sucht", ",", "Lie\u00b7be", ",", "Ha\u00df", ",", "von", "Hoff\u00b7nung", "und", "von", "Furcht", "ist", "leer"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Die Brust, das Wrack liegt morsch und m\u00fcrb, verfaulend in dem Hafen;", "tokens": ["Die", "Brust", ",", "das", "Wrack", "liegt", "morsch", "und", "m\u00fcrb", ",", "ver\u00b7fau\u00b7lend", "in", "dem", "Ha\u00b7fen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Und was sie f\u00fcnfzig Jahre hetzte, spornte, peitschte hin und her,", "tokens": ["Und", "was", "sie", "f\u00fcnf\u00b7zig", "Jah\u00b7re", "hetz\u00b7te", ",", "sporn\u00b7te", ",", "peitschte", "hin", "und", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "CARD", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.4": {"text": "Ist stumm, verloschen, ausgebrannt und endlich eingeschlafen.", "tokens": ["Ist", "stumm", ",", "ver\u00b7lo\u00b7schen", ",", "aus\u00b7ge\u00b7brannt", "und", "end\u00b7lich", "ein\u00b7ge\u00b7schla\u00b7fen", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "VVFIN", "$,", "VVPP", "KON", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Und ist das Herz auch kalt und tot; sie starren gierig in die Flut", "tokens": ["Und", "ist", "das", "Herz", "auch", "kalt", "und", "tot", ";", "sie", "star\u00b7ren", "gie\u00b7rig", "in", "die", "Flut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Schon stundenlang; kein Fischlein will an ihren Angeln bei\u00dfen;", "tokens": ["Schon", "stun\u00b7den\u00b7lang", ";", "kein", "Fisc\u00b7hlein", "will", "an", "ih\u00b7ren", "An\u00b7geln", "bei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$.", "PIAT", "NN", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Manch halbverge\u00dfner Fluch erschallt voll Ungeduld und wilder Wut,", "tokens": ["Manch", "halb\u00b7ver\u00b7ge\u00df\u00b7ner", "Fluch", "er\u00b7schallt", "voll", "Un\u00b7ge\u00b7duld", "und", "wil\u00b7der", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "VVFIN", "ADJD", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Das Meer ist geizig und es l\u00e4\u00dft sich heute nichts entrei\u00dfen.", "tokens": ["Das", "Meer", "ist", "gei\u00b7zig", "und", "es", "l\u00e4\u00dft", "sich", "heu\u00b7te", "nichts", "ent\u00b7rei\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "KON", "PPER", "VVFIN", "PRF", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Und ist auch lange abger\u00e4umt des Lebens reichgedeckter Tisch,", "tokens": ["Und", "ist", "auch", "lan\u00b7ge", "ab\u00b7ge\u00b7r\u00e4umt", "des", "Le\u00b7bens", "reich\u00b7ge\u00b7deck\u00b7ter", "Tisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "VVPP", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Und kalt das Herz, dem Freuden, Schmerzen, Angst und Hoffnung mangeln,", "tokens": ["Und", "kalt", "das", "Herz", ",", "dem", "Freu\u00b7den", ",", "Schmer\u00b7zen", ",", "Angst", "und", "Hoff\u00b7nung", "man\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "$,", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "So bleibt als hei\u00df ersehntes Ziel ein spannenlanger Fisch,", "tokens": ["So", "bleibt", "als", "hei\u00df", "er\u00b7sehn\u00b7tes", "Ziel", "ein", "span\u00b7nen\u00b7lan\u00b7ger", "Fisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KOKOM", "ADJD", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Nach dem sie stieren Auges t\u00e4glich angeln, angeln, angeln.", "tokens": ["Nach", "dem", "sie", "stie\u00b7ren", "Au\u00b7ges", "t\u00e4g\u00b7lich", "an\u00b7geln", ",", "an\u00b7geln", ",", "an\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJA", "NN", "ADJD", "VVINF", "$,", "VVINF", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Auf glatter, grauer Woge ruht das feuerrote Feuerschiff,", "tokens": ["Auf", "glat\u00b7ter", ",", "grau\u00b7er", "Wo\u00b7ge", "ruht", "das", "feu\u00b7er\u00b7ro\u00b7te", "Feu\u00b7er\u00b7schiff", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.2": {"text": "Es r\u00fchrt sich nicht; der Anker h\u00e4lt es fest im Meeressande;", "tokens": ["Es", "r\u00fchrt", "sich", "nicht", ";", "der", "An\u00b7ker", "h\u00e4lt", "es", "fest", "im", "Mee\u00b7res\u00b7san\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PTKNEG", "$.", "ART", "NN", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Fahrwasser weist sein Feuerschein durch Sandbank und durch Felsenriff", "tokens": ["Fahr\u00b7was\u00b7ser", "weist", "sein", "Feu\u00b7er\u00b7schein", "durch", "Sand\u00b7bank", "und", "durch", "Fel\u00b7sen\u00b7riff"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Den Fischern in der Nebelnacht und leitet sie zum S