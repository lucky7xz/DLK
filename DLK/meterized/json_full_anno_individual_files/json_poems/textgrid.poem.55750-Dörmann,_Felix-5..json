"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das faltige Schleiergewand,", "tokens": ["Das", "fal\u00b7ti\u00b7ge", "Schlei\u00b7er\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es ringt mein ganzes Gel\u00fcsten", "tokens": ["Es", "ringt", "mein", "gan\u00b7zes", "Ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Nach keuschen, ungek\u00fc\u00dften,", "tokens": ["Nach", "keu\u00b7schen", ",", "un\u00b7ge\u00b7k\u00fc\u00df\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hinweg, hinweg Deine Hand!", "tokens": ["Hin\u00b7weg", ",", "hin\u00b7weg", "Dei\u00b7ne", "Hand", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Ich f\u00fchle mein Aug' sich verglasen,", "tokens": ["Ich", "f\u00fch\u00b7le", "mein", "Aug'", "sich", "ver\u00b7gla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PRF", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Mein Leib verkohlt, verbrennt,", "tokens": ["Mein", "Leib", "ver\u00b7kohlt", ",", "ver\u00b7brennt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt mu\u00dft Du mit mir rasen, \u2013", "tokens": ["Jetzt", "mu\u00dft", "Du", "mit", "mir", "ra\u00b7sen", ",", "\u2013