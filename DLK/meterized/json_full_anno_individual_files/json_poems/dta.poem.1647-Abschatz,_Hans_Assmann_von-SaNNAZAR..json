{"dta.poem.1647": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "SaNNAZAR.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Neptun sah in der Flutt Venedigs edle Stadt/", "tokens": ["Nep\u00b7tun", "sah", "in", "der", "Flutt", "Ve\u00b7ne\u00b7digs", "ed\u00b7le", "Stadt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.2": {"text": "Die das gez\u00e4hmte Meer zu ihrem Willen hat/", "tokens": ["Die", "das", "ge\u00b7z\u00e4hm\u00b7te", "Meer", "zu", "ih\u00b7rem", "Wil\u00b7len", "hat", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VAFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er sprach: La\u00df Jupitern Tarpejus Schl\u00f6sser preisen/", "tokens": ["Er", "sprach", ":", "La\u00df", "Ju\u00b7pi\u00b7tern", "Tar\u00b7pe\u00b7jus", "Schl\u00f6s\u00b7ser", "prei\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "VVIMP", "NN", "NE", "NN", "VVFIN", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Gradivens k\u00fchnen Bau und freche Mauren weisen:", "tokens": ["Gra\u00b7di\u00b7vens", "k\u00fch\u00b7nen", "Bau", "und", "fre\u00b7che", "Mau\u00b7ren", "wei\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Sagt/ wenn ihr in der See Venedigs Wunder schaut/", "tokens": ["Sagt", "/", "wenn", "ihr", "in", "der", "See", "Ve\u00b7ne\u00b7digs", "Wun\u00b7der", "schaut", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "KOUS", "PPER", "APPR", "ART", "NN", "NE", "NN", "VVFIN", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Ob Menschen/ G\u00f6tter nicht? di\u00df edle Werck gebaut?", "tokens": ["Ob", "Men\u00b7schen", "/", "G\u00f6t\u00b7ter", "nicht", "?", "di\u00df", "ed\u00b7le", "Werck", "ge\u00b7baut", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$(", "NN", "PTKNEG", "$.", "PDS", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}