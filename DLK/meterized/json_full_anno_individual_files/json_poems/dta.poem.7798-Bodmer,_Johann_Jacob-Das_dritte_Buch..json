{"dta.poem.7798": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Das dritte Buch.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indessen n\u00e4hert sich das Volck in sch\u00f6ner Reihe,", "tokens": ["In\u00b7des\u00b7sen", "n\u00e4\u00b7hert", "sich", "das", "Volck", "in", "sch\u00f6\u00b7ner", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Man siehet mehr und mehr wie sich der Zug hindreihe", "tokens": ["Man", "sie\u00b7het", "mehr", "und", "mehr", "wie", "sich", "der", "Zug", "hind\u00b7rei\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ADV", "KON", "ADV", "KOKOM", "PRF", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nach ihren Mauren zu. Die Priester, Aarons S\u00f6hn,", "tokens": ["Nach", "ih\u00b7ren", "Mau\u00b7ren", "zu", ".", "Die", "Pries\u00b7ter", ",", "Aa\u00b7rons", "S\u00f6hn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$.", "ART", "NN", "$,", "NE", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erschienen da zuerst mit ihrem Siegget\u00f6n;", "tokens": ["Er\u00b7schie\u00b7nen", "da", "zu\u00b7erst", "mit", "ih\u00b7rem", "Sieg\u00b7ge\u00b7t\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nach ihnen kam der Stamm von Juda aufgezogen", "tokens": ["Nach", "ih\u00b7nen", "kam", "der", "Stamm", "von", "Ju\u00b7da", "auf\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "APPR", "NE", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Pracht und grossem Sieg, die alle wohl erwogen", "tokens": ["Mit", "Pracht", "und", "gros\u00b7sem", "Sieg", ",", "die", "al\u00b7le", "wohl", "er\u00b7wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "$,", "PRELS", "PIS", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was Gott bey sie gethan. Drauf folgte Jsaschar", "tokens": ["Was", "Gott", "bey", "sie", "ge\u00b7than", ".", "Drauf", "folg\u00b7te", "Jsasc\u00b7har"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "NN", "APPR", "PPER", "VVPP", "$.", "PAV", "VVFIN", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und was von Sebulon auch mit zu Felde war.", "tokens": ["Und", "was", "von", "Se\u00b7bu\u00b7lon", "auch", "mit", "zu", "Fel\u00b7de", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NE", "ADV", "APPR", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hie folgete die Beut auf tausend Kriegeswagen", "tokens": ["Hie", "fol\u00b7ge\u00b7te", "die", "Beut", "auf", "tau\u00b7send", "Krie\u00b7ges\u00b7wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die man im Lager theils, theils auch in dem Nachjagen", "tokens": ["Die", "man", "im", "La\u00b7ger", "theils", ",", "theils", "auch", "in", "dem", "Nach\u00b7ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "APPRART", "NN", "ADV", "$,", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-++-+-+", "measure": "unknown.measure.septa"}, "line.11": {"text": "Aus der Philister Land hat reichlich mitgebracht.", "tokens": ["Aus", "der", "Phi\u00b7lis\u00b7ter", "Land", "hat", "reich\u00b7lich", "mit\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Worauf sich sehen lie\u00df der Rubeniter Macht,", "tokens": ["Wo\u00b7rauf", "sich", "se\u00b7hen", "lie\u00df", "der", "Ru\u00b7be\u00b7ni\u00b7ter", "Macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PRF", "VVINF", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit Simeon und Gath; all auserle\u00dfne Leute.", "tokens": ["Mit", "Si\u00b7me\u00b7on", "und", "Gath", ";", "all", "au\u00b7ser\u00b7le\u00df\u00b7ne", "Leu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$.", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Nach diesen kam ein Theil hinwieder von der Beute.", "tokens": ["Nach", "die\u00b7sen", "kam", "ein", "Theil", "hin\u00b7wie\u00b7der", "von", "der", "Beu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Die da von Ephraim, die folgten nach der Hand.", "tokens": ["Die", "da", "von", "Eph\u00b7raim", ",", "die", "folg\u00b7ten", "nach", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NE", "$,", "PRELS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.16": {"text": "Nach ihnen Benjamin, allwo sich macht bekannt", "tokens": ["Nach", "ih\u00b7nen", "Ben\u00b7ja\u00b7min", ",", "all\u00b7wo", "sich", "macht", "be\u00b7kannt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "NE", "$,", "PWAV", "PRF", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der Kern von Jsrael, der K\u00f6nig und die Helden,", "tokens": ["Der", "Kern", "von", "Js\u00b7rael", ",", "der", "K\u00f6\u00b7nig", "und", "die", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Es konnt ihr Glantz und Schein gleich wer sie waren melden.", "tokens": ["Es", "konnt", "ihr", "Glantz", "und", "Schein", "gleich", "wer", "sie", "wa\u00b7ren", "mel\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "KON", "NN", "ADV", "PWS", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Thalmais treues Aug, der Michal Liebesflamm,", "tokens": ["Thal\u00b7mais", "treu\u00b7es", "Aug", ",", "der", "Mic\u00b7hal", "Lie\u00b7bes\u00b7flamm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.20": {"text": "Ergriff im Augenblick was da her\u00fcber kam.", "tokens": ["Er\u00b7griff", "im", "Au\u00b7gen\u00b7blick", "was", "da", "her\u00b7\u00fc\u00b7ber", "kam", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "PWS", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Es ritte Jonathan, der Benjamiter Ehre,", "tokens": ["Es", "rit\u00b7te", "Jo\u00b7na\u00b7than", ",", "der", "Ben\u00b7ja\u00b7mi\u00b7ter", "Eh\u00b7re", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die Kron des Haufes Saul, zuforderst f\u00fcr dem Heere", "tokens": ["Die", "Kron", "des", "Hau\u00b7fes", "Saul", ",", "zu\u00b7for\u00b7derst", "f\u00fcr", "dem", "Hee\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "NE", "$,", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Auf einem sch\u00f6nen Hengst, ihm folgte eine Schaar", "tokens": ["Auf", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Hengst", ",", "ihm", "folg\u00b7te", "ei\u00b7ne", "Schaar"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Von tausend Mann die stets zu seinen Diensten war.", "tokens": ["Von", "tau\u00b7send", "Mann", "die", "stets", "zu", "sei\u00b7nen", "Diens\u00b7ten", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "ART", "ADV", "APPR", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Die R\u00fcstung Silber war, die dieser F\u00fcrst heut f\u00fchrte,", "tokens": ["Die", "R\u00fcs\u00b7tung", "Sil\u00b7ber", "war", ",", "die", "die\u00b7ser", "F\u00fcrst", "heut", "f\u00fchr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$,", "PRELS", "PDAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wes Schein Thalmais Aug und Hertze hefftig r\u00fchrte.", "tokens": ["Wes", "Schein", "Thal\u00b7mais", "Aug", "und", "Hert\u00b7ze", "heff\u00b7tig", "r\u00fchr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "NN", "KON", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.27": {"text": "Sein Hauptschmuck war beziert mit Perlen und Saphir,", "tokens": ["Sein", "Haupt\u00b7schmuck", "war", "be\u00b7ziert", "mit", "Per\u00b7len", "und", "Sa\u00b7phir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "APPR", "NN", "KON", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Ein blaugewebte Bind schien an dem Arm herf\u00fcr,", "tokens": ["Ein", "blau\u00b7ge\u00b7web\u00b7te", "Bind", "schien", "an", "dem", "Arm", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die ihme sein Gemahl beym Abzug umgewunden.", "tokens": ["Die", "ih\u00b7me", "sein", "Ge\u00b7mahl", "beym", "Ab\u00b7zug", "um\u00b7ge\u00b7wun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPOSAT", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wie ward ihr Hertz, als sie die\u00df noch bey ihm gefunden?", "tokens": ["Wie", "ward", "ihr", "Hertz", ",", "als", "sie", "die\u00df", "noch", "bey", "ihm", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PDS", "ADV", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Es \u00fcbergosse sich gleich wie das hohe Meer,", "tokens": ["Es", "\u00fc\u00b7ber\u00b7gos\u00b7se", "sich", "gleich", "wie", "das", "ho\u00b7he", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADV", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und quolle Haufenwei\u00df aus ihren Augen her", "tokens": ["Und", "quol\u00b7le", "Hau\u00b7fen\u00b7wei\u00df", "aus", "ih\u00b7ren", "Au\u00b7gen", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "APZR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Die eingenommne Freud. Der Michal Rosenwangen", "tokens": ["Die", "ein\u00b7ge\u00b7nomm\u00b7ne", "Freud", ".", "Der", "Mic\u00b7hal", "Ro\u00b7sen\u00b7wan\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Erzeigten ebenfalls ein hertzliches Verlangen", "tokens": ["Er\u00b7zeig\u00b7ten", "e\u00b7ben\u00b7falls", "ein", "hertz\u00b7li\u00b7ches", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sowohl nach Jonathan, als nach dem andern Held,", "tokens": ["So\u00b7wohl", "nach", "Jo\u00b7na\u00b7than", ",", "als", "nach", "dem", "an\u00b7dern", "Held", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "$,", "KOUS", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Der sich mehr als zu sch\u00f6n f\u00fcr ihre Augen stellt.", "tokens": ["Der", "sich", "mehr", "als", "zu", "sch\u00f6n", "f\u00fcr", "ih\u00b7re", "Au\u00b7gen", "stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADV", "KOUS", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Sie kannte ihn zur Stund, weil da des grossen Riesen", "tokens": ["Sie", "kann\u00b7te", "ihn", "zur", "Stund", ",", "weil", "da", "des", "gros\u00b7sen", "Rie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Sein abgeschlagnes Haupt ward f\u00fcr ihm her gewiesen", "tokens": ["Sein", "ab\u00b7ge\u00b7schlag\u00b7nes", "Haupt", "ward", "f\u00fcr", "ihm", "her", "ge\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "APPR", "PPER", "APZR", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Auf einem langen Speer; das ja gnung zeigte dar,", "tokens": ["Auf", "ei\u00b7nem", "lan\u00b7gen", "Speer", ";", "das", "ja", "gnung", "zeig\u00b7te", "dar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "PDS", "ADV", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Da\u00df der der diesen folgt der edle David war.", "tokens": ["Da\u00df", "der", "der", "die\u00b7sen", "folgt", "der", "ed\u00b7le", "Da\u00b7vid", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "PDS", "VVFIN", "ART", "ADJA", "NE", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wie herrlich und wie sch\u00f6n derselbe anzusehen,", "tokens": ["Wie", "herr\u00b7lich", "und", "wie", "sch\u00f6n", "der\u00b7sel\u00b7be", "an\u00b7zu\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "KON", "PWAV", "ADJD", "PDAT", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Must Michal nicht allein mit ihrer Schaar gestehen,", "tokens": ["Must", "Mic\u00b7hal", "nicht", "al\u00b7lein", "mit", "ih\u00b7rer", "Schaar", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.he