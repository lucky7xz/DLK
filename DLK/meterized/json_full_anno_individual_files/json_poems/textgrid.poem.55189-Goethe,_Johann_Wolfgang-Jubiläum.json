{"textgrid.poem.55189": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Jubil\u00e4um", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hat der Tag sich kaum erneuet,", "tokens": ["Hat", "der", "Tag", "sich", "kaum", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo uns Winterfreude bl\u00fchet,", "tokens": ["Wo", "uns", "Win\u00b7ter\u00b7freu\u00b7de", "bl\u00fc\u00b7het", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Jedermann sich w\u00fcnschend freuet,", "tokens": ["Je\u00b7der\u00b7mann", "sich", "w\u00fcn\u00b7schend", "freu\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wenn er Freund und G\u00f6nner siehet.", "tokens": ["Wenn", "er", "Freund", "und", "G\u00f6n\u00b7ner", "sie\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sagt, wie schon am zweiten Tage", "tokens": ["Sagt", ",", "wie", "schon", "am", "zwei\u00b7ten", "Ta\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "PWAV", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich ein zweites Fest entz\u00fcndet?", "tokens": ["Sich", "ein", "zwei\u00b7tes", "Fest", "ent\u00b7z\u00fc