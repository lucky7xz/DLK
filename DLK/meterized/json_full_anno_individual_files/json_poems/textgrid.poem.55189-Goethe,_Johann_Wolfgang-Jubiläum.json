 "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht nun F\u00fcrst und Volk geborgen,", "tokens": ["Sieht", "nun", "F\u00fcrst", "und", "Volk", "ge\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dem er Geist und Kraft gewidmet.", "tokens": ["Dem", "er", "Geist", "und", "Kraft", "ge\u00b7wid\u00b7met", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Die Gemahlin, l\u00e4ngst verbunden", "tokens": ["Die", "Ge\u00b7mah\u00b7lin", ",", "l\u00e4ngst", "ver\u00b7bun\u00b7den"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm als treulichstes Geleite,", "tokens": ["Ihm", "als", "treu\u00b7lichs\u00b7tes", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieht er auch, der tausend Stunden", "tokens": ["Sieht", "er", "auch", ",", "der", "tau\u00b7send", "Stun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PRELS", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh gedenk, an seiner Seite.", "tokens": ["Froh", "ge\u00b7denk", ",", "an", "sei\u00b7ner", "Sei\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "Leb er so, mit J\u00fcnglingskr\u00e4ften", "tokens": ["Leb", "er", "so", ",", "mit", "J\u00fcng\u00b7lings\u00b7kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Immer herrlich und verm\u00f6gsam,", "tokens": ["Im\u00b7mer", "herr\u00b7lich", "und", "ver\u00b7m\u00f6g\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In den wichtigsten Gesch\u00e4ften", "tokens": ["In", "den", "wich\u00b7tigs\u00b7ten", "Ge\u00b7sch\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Heiter klug und weise regsam", "tokens": ["Hei\u00b7ter", "klug", "und", "wei\u00b7se", "reg\u00b7sam"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "KON", "VVFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Und in seiner Trauten Kreise", "tokens": ["Und", "in", "sei\u00b7ner", "Trau\u00b7ten", "Krei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sorgenfrei und unterhaltend,", "tokens": ["Sor\u00b7gen\u00b7frei", "und", "un\u00b7ter\u00b7hal\u00b7tend", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine Welt nach seiner Weise", "tokens": ["Ei\u00b7ne", "Welt", "nach", "sei\u00b7ner", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nah und fern umher gestaltend.", "tokens": ["Nah", "und", "fern", "um\u00b7her", "ge\u00b7stal\u00b7tend", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}