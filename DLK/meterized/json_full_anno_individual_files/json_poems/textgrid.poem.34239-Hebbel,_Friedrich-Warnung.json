{"textgrid.poem.34239": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Warnung", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reizt den Dichter nicht! Er kann sich f\u00fcrchterlich r\u00e4chen,", "tokens": ["Reizt", "den", "Dich\u00b7ter", "nicht", "!", "Er", "kann", "sich", "f\u00fcrch\u00b7ter\u00b7lich", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$.", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und es entzieht ihm den Feind Keiner, nicht einmal der Tod!", "tokens": ["Und", "es", "ent\u00b7zieht", "ihm", "den", "Feind", "Kei\u00b7ner", ",", "nicht", "ein\u00b7mal", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "ART", "NN", "PIS", "$,", "PTKNEG", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.3": {"text": "Denn, so wie sein Ku\u00df dem Freund unsterbliche Ehre", "tokens": ["Denn", ",", "so", "wie", "sein", "Ku\u00df", "dem", "Freund", "uns\u00b7terb\u00b7li\u00b7che", "Eh\u00b7re"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "ADV", "KOKOM", "PPOSAT", "NN", "ART", "NN", "ADJA", "NN"], "meter": "--+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sichert, so sichert sein Tritt diesem unsterbliche Schmach.", "tokens": ["Si\u00b7chert", ",", "so", "si\u00b7chert", "sein", "Tritt", "die\u00b7sem", "uns\u00b7terb\u00b7li\u00b7che", "Schmach", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADV", "VVFIN", "PPOSAT", "NN", "PDAT", "ADJA", "NN", "$."], "meter": "+---+-+---+--+", "measure": "iambic.penta.chol"}, "line.5": {"text": "Denkt an G\u00f6zze! Er stach nach Lessing und wollte ihn t\u00f6dten,", "tokens": ["Denkt", "an", "G\u00f6z\u00b7ze", "!", "Er", "stach", "nach", "Les\u00b7sing", "und", "woll\u00b7te", "ihn", "t\u00f6d\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$.", "PPER", "VVFIN", "APPR", "NE", "KON", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Lessing r\u00e4cht sich, er l\u00e4\u00dft ewig ihn leben, den Wicht!", "tokens": ["Les\u00b7sing", "r\u00e4cht", "sich", ",", "er", "l\u00e4\u00dft", "e\u00b7wig", "ihn", "le\u00b7ben", ",", "den", "Wicht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "ADJD", "PPER", "VVINF", "$,", "ART", "NN", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Ja, und h\u00e4tte er selbst den leuchtenden Kerker des Nathan,", "tokens": ["Ja", ",", "und", "h\u00e4t\u00b7te", "er", "selbst", "den", "leuch\u00b7ten\u00b7den", "Ker\u00b7ker", "des", "Na\u00b7than", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KON", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+--+-+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Der ihm den Pfaffen verwahrt, sp\u00e4ter ger\u00fchrt und vers\u00f6hnt,", "tokens": ["Der", "ihm", "den", "Pfaf\u00b7fen", "ver\u00b7wahrt", ",", "sp\u00e4\u00b7ter", "ge\u00b7r\u00fchrt", "und", "ver\u00b7s\u00f6hnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VVPP", "$,", "ADJD", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Oeffnen wollen, er h\u00e4tte den Schl\u00fcssel nimmer gefunden,", "tokens": ["Oeff\u00b7nen", "wol\u00b7len", ",", "er", "h\u00e4t\u00b7te", "den", "Schl\u00fcs\u00b7sel", "nim\u00b7mer", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Denn wir sperren nur ein, aber wir lassen nicht aus!", "tokens": ["Denn", "wir", "sper\u00b7ren", "nur", "ein", ",", "a\u00b7ber", "wir", "las\u00b7sen", "nicht", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "--+---+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Reizt den Dichter nicht! Er kann sich f\u00fcrchterlich r\u00e4chen,", "tokens": ["Reizt", "den", "Dich\u00b7ter", "nicht", "!", "Er", "kann", "sich", "f\u00fcrch\u00b7ter\u00b7lich", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PTKNEG", "$.", "PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Und es entzieht ihm den Feind Keiner, nicht einmal der Tod!", "tokens": ["Und", "es", "ent\u00b7zieht", "ihm", "den", "Feind", "Kei\u00b7ner", ",", "nicht", "ein\u00b7mal", "der", "Tod", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"],