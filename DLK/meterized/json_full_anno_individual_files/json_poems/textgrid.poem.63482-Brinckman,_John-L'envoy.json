{"textgrid.poem.63482": {"metadata": {"author": {"name": "Brinckman, John", "birth": "N.A.", "death": "N.A."}, "title": "L'envoy", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dor t\u00fcht een d\u00fcster W\u00e4der up,", "tokens": ["Dor", "t\u00fcht", "e\u00b7en", "d\u00fcs\u00b7ter", "W\u00e4\u00b7der", "up", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "s\u00fcdwest de H\u00e4ben glimmt!", "tokens": ["s\u00fcd\u00b7west", "de", "H\u00e4\u00b7ben", "glimmt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dat l\u00fccht't un leit un dunnert dor,", "tokens": ["Dat", "l\u00fccht't", "un", "leit", "un", "dun\u00b7nert", "dor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "wenn dat man up nich k\u00fcmmt!", "tokens": ["wenn", "dat", "man", "up", "nich", "k\u00fcmmt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PIS", "NE", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dor's Hagel in vun eegen Ort,", "tokens": ["Dor's", "Ha\u00b7gel", "in", "vun", "e\u00b7e\u00b7gen", "Ort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "de's blag un swor as Bli \u2013", "tokens": ["de's", "blag", "un", "swor", "as", "Bli", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "un wenn't so neg herannerfohrt,", "tokens": ["un", "wenn't", "so", "neg", "her\u00b7an\u00b7ner\u00b7fohrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "denn k\u00fcmmt dat up, ihr een sick wohrt,", "tokens": ["denn", "k\u00fcmmt", "dat", "up", ",", "ihr", "e\u00b7en", "sick", "wohrt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NE", "$,", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "+----+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "denn treckt't nich mihr v\u00f6rbi.", "tokens": ["denn", "treckt't", "nich", "mihr", "v\u00f6r\u00b7bi", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Hell klingt dat vun dor \u00fcnnen her", "tokens": ["Hell", "klingt", "dat", "vun", "dor", "\u00fcn\u00b7nen", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "as Trummel un Trumpet", "tokens": ["as", "Trum\u00b7mel", "un", "Trum\u00b7pet"], "token_info": ["word", "word", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "un dr\u0153hnt, grad as Kanonen swer,", "tokens": ["un", "dr\u0153hnt", ",", "grad", "as", "Ka\u00b7no\u00b7nen", "swer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "$,", "ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "as B\u00fc\u00df un as Musket.", "tokens": ["as", "B\u00fc\u00df", "un", "as", "Mus\u00b7ket", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Na, man nich bang! Wat k\u00fcmmt, dat gellt!", "tokens": ["Na", ",", "man", "nich", "bang", "!", "Wat", "k\u00fcmmt", ",", "dat", "gellt", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PIS", "PTKNEG", "ADJD", "$.", "NN", "VVFIN", "$,", "PRELS", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00dcns' oll Land M\u00e4kelb\u00f6rg", "tokens": ["\u00dcns'", "oll", "Land", "M\u00e4\u00b7kel\u00b7b\u00f6rg"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ADJD", "NN", "NE"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.7": {"text": "geiht mi di, Landsmann, ok in't Feld", "tokens": ["geiht", "mi", "di", ",", "Lands\u00b7mann", ",", "ok", "in't", "Feld"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "NE", "$,", "NN", "$,", "FM", "FM", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "un hett to rechte Tit sick stellt", "tokens": ["un", "hett", "to", "rech\u00b7te", "Tit", "sick", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "un sleiht mit Gott sick d\u00f6rch.", "tokens": ["un", "sleiht", "mit", "Gott", "sick", "d\u00f6rch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "VVFIN", "APPR", "NN", "VVIMP", "PTKVZ", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "In Rostock vor de Breede Strat,", "tokens": ["In", "Ros\u00b7tock", "vor", "de", "Bree\u00b7de", "Strat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dor steiht'n hogen Steen,", "tokens": ["dor", "steiht'n", "ho\u00b7gen", "Steen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "un up den Steen steiht een Soldat,", "tokens": ["un", "up", "den", "Steen", "steiht", "e\u00b7en", "Sol\u00b7dat", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "un den kennt jidwereen;", "tokens": ["un", "den", "kennt", "jid\u00b7we\u00b7reen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM", "ART", "VVFIN", "VVINF", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "de was echt m\u00e4kelb\u00f6rger Blot,", "tokens": ["de", "was", "echt", "m\u00e4\u00b7kel\u00b7b\u00f6r\u00b7ger", "Blot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWS", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un de verst\u00fcnn den Kram,", "tokens": ["un", "de", "ver\u00b7st\u00fcnn", "den", "Kram", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "de h\u00fclp \u00fcns eens ut all \u00fcns' Not,", "tokens": ["de", "h\u00fclp", "\u00fcns", "e\u00b7ens", "ut", "all", "\u00fcns'", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "dat's ewig schad, dat de is dot,", "tokens": ["dat's", "e\u00b7wig", "schad", ",", "dat", "de", "is", "dot", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "$,", "FM.nl", "FM.nl", "FM.nl", "FM.nl", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.9": {"text": "un Bl\u00fccher is sin Nam.", "tokens": ["un", "Bl\u00fc\u00b7cher", "is", "sin", "Nam", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "De hadd sick Anno d\u00fcrrteihn lihrt,", "tokens": ["De", "hadd", "sick", "An\u00b7no", "d\u00fcrr\u00b7teihn", "lihrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "as grad sonn W\u00e4der wir,", "tokens": ["as", "grad", "sonn", "W\u00e4\u00b7der", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "KON", "NN", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "een Wurt, dat's dusend Daler wirt,", "tokens": ["e\u00b7en", "Wurt", ",", "dat's", "du\u00b7send", "Da\u00b7ler", "wirt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "nee, mihr as Geld, v\u00e4l mihr!", "tokens": ["nee", ",", "mihr", "as", "Geld", ",", "v\u00e4l", "mihr", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKANT", "$,", "FM", "FM", "NN", "$,", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Dat Wurt, dat dunn he spraken hett,", "tokens": ["Dat", "Wurt", ",", "dat", "dunn", "he", "spra\u00b7ken", "hett", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "ART", "ADJA", "NN", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "drew ball dat W\u00e4der furt \u2013", "tokens": ["drew", "ball", "dat", "W\u00e4\u00b7der", "furt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ART", "NN", "VVFIN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "een Hundsfott, wen dat je vergett", "tokens": ["e\u00b7en", "Hunds\u00b7fott", ",", "wen", "dat", "je", "ver\u00b7gett"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWS", "ART", "ADV", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "un wen sick dor nich up verlett,", "tokens": ["un", "wen", "sick", "dor", "nich", "up", "ver\u00b7lett", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+---+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "un Fluschen heet dat Wurt!", "tokens": ["un", "Flu\u00b7schen", "heet", "dat", "Wurt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Denn schenkt de leew Gott sacht noch mal", "tokens": ["Denn", "schenkt", "de", "leew", "Gott", "sacht", "noch", "mal"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NE", "NE", "NN", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "dat, wat \u00fcns n\u00f6dig deiht,", "tokens": ["dat", ",", "wat", "\u00fcns", "n\u00f6\u00b7dig", "deiht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "sonn rechten echten General,", "tokens": ["sonn", "rech\u00b7ten", "ech\u00b7ten", "Ge\u00b7ne\u00b7ral", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "de ok sin'n Kram versteiht.", "tokens": ["de", "ok", "sin'n", "Kram", "ver\u00b7steiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn snall wi \u00fcns den Sabel an", "tokens": ["Denn", "snall", "wi", "\u00fcns", "den", "Sa\u00b7bel", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "FM", "FM", "FM", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "un str\u00f6p \u00fcns up de Brok,", "tokens": ["un", "str\u00f6p", "\u00fcns", "up", "de", "Brok", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.7": {"text": "all wat de Plemp man b\u00f6hren kann,", "tokens": ["all", "wat", "de", "Plemp", "man", "b\u00f6h\u00b7ren", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "FM", "FM", "FM", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "un k\u