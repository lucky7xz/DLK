{"textgrid.poem.42417": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Preist Ihr den Heldenlauf der Sieger, schm\u00fcckt", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Preist Ihr den Heldenlauf der Sieger, schm\u00fcckt", "tokens": ["Preist", "Ihr", "den", "Hel\u00b7den\u00b7lauf", "der", "Sie\u00b7ger", ",", "schm\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie mit dem Ruhmeskranz, Euch dran zu weiden \u2013", "tokens": ["Sie", "mit", "dem", "Ruh\u00b7mes\u00b7kranz", ",", "Euch", "dran", "zu", "wei\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "$,", "PPER", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich will indessen, in den Staub geb\u00fcckt,", "tokens": ["Ich", "will", "in\u00b7des\u00b7sen", ",", "in", "den", "Staub", "ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "$,", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Erniedrigung mit den Besiegten leiden.", "tokens": ["Er\u00b7nied\u00b7ri\u00b7gung", "mit", "den", "Be\u00b7sieg\u00b7ten", "lei\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-++--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Geringstes Volk! verp\u00f6nt, geschm\u00e4ht, verheert", "tokens": ["Ge\u00b7rings\u00b7tes", "Volk", "!", "ver\u00b7p\u00f6nt", ",", "ge\u00b7schm\u00e4ht", ",", "ver\u00b7heert"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$.", "VVFIN", "$,", "VVPP", "$,", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und bis zur Knechtschaft in die Knie gezwungen \u2013", "tokens": ["Und", "bis", "zur", "Knecht\u00b7schaft", "in", "die", "Knie", "ge\u00b7zwun\u00b7gen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du bist vor jedem stolzeren mir wert,", "tokens": ["Du", "bist", "vor", "je\u00b7dem", "stol\u00b7ze\u00b7ren", "mir", "wert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PIS", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Als w\u00e4r' mit dir ich einem Stamm entsprungen!", "tokens": ["Als", "w\u00e4r'", "mit", "dir", "ich", "ei\u00b7nem", "Stamm", "ent\u00b7sprun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPR", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Heiss brennt mich Scham, wenn das Triumphgebraus", "tokens": ["Heiss", "brennt", "mich", "Scham", ",", "wenn", "das", "Tri\u00b7umph\u00b7ge\u00b7braus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dem Feinde Fall und Untergang verk\u00fcndet,", "tokens": ["Dem", "Fein\u00b7de", "Fall", "und", "Un\u00b7ter\u00b7gang", "ver\u00b7k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn \u00fcber der Zerst\u00f6rung tost Applaus", "tokens": ["Wenn", "\u00fc\u00b7ber", "der", "Zer\u00b7st\u00f6\u00b7rung", "tost", "Ap\u00b7plaus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wilder noch die Machtgier sich entz\u00fcndet.", "tokens": ["Und", "wil\u00b7der", "noch", "die", "Macht\u00b7gier", "sich", "ent\u00b7z\u00fcn\u00b7det", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "ART", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weit lieber doch besiegt sein, als verf\u00fchrt", "tokens": ["Weit", "lie\u00b7ber", "doch", "be\u00b7siegt", "sein", ",", "als", "ver\u00b7f\u00fchrt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ADV", "ADV", "VVPP", "VAINF", "$,", "KOUS", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Von eitlem Glanz \u2013 und wenn auch am Verschmachten,", "tokens": ["Von", "eit\u00b7lem", "Glanz", "\u2013", "und", "wenn", "auch", "am", "Ver\u00b7schmach\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "KOUS", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und ob man gleich den Fuss im Nacken sp\u00fcrt \u2013", "tokens": ["Und", "ob", "man", "gleich", "den", "Fuss", "im", "Na\u00b7cken", "sp\u00fcrt", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "p