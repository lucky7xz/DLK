{"dta.poem.12099": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "21.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Pf\u00e4hle sah ich stehn, der eine wei\u00df und blau,", "tokens": ["Zwei", "Pf\u00e4h\u00b7le", "sah", "ich", "stehn", ",", "der", "ei\u00b7ne", "wei\u00df", "und", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "VVINF", "$,", "PRELS", "PIS", "VVFIN", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der andre gelb und schwarz, unlieblich war die Schau.", "tokens": ["Der", "and\u00b7re", "gelb", "und", "schwarz", ",", "un\u00b7lieb\u00b7lich", "war", "die", "Schau", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "KON", "ADJD", "$,", "ADJD", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die beiden sagen an, da\u00df hier Landgrenze sei;", "tokens": ["Die", "bei\u00b7den", "sa\u00b7gen", "an", ",", "da\u00df", "hier", "Land\u00b7gren\u00b7ze", "sei", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "VVFIN", "PTKVZ", "$,", "KOUS", "ADV", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sagten sie es nicht, so fiel' es mir nicht bei.", "tokens": ["Und", "sag\u00b7ten", "sie", "es", "nicht", ",", "so", "fiel'", "es", "mir", "nicht", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Denn unver\u00e4ndert ganz von Ansehn und Geberde", "tokens": ["Denn", "un\u00b7ver\u00b7\u00e4n\u00b7dert", "ganz", "von", "An\u00b7sehn", "und", "Ge\u00b7ber\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-----", "measure": "unknown.measure.tetra"}, "line.2": {"text": "H\u00fcben und dr\u00fcben ist der Himmel wie die Erde.", "tokens": ["H\u00fc\u00b7ben", "und", "dr\u00fc\u00b7ben", "ist", "der", "Him\u00b7mel", "wie", "die", "Er\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADV", "VAFIN", "ART", "NN", "KOKOM", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.4": {"line.1": {"text": "Die Berge laufen im ununterbrochnen Zug,", "tokens": ["Die", "Ber\u00b7ge", "lau\u00b7fen", "im", "un\u00b7un\u00b7ter\u00b7broch\u00b7nen", "Zug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und seine Wellen schl\u00e4gt der Flu\u00df, wie er sie schlug.", "tokens": ["Und", "sei\u00b7ne", "Wel\u00b7len", "schl\u00e4gt", "der", "Flu\u00df", ",", "wie", "er", "sie", "schlug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Hin \u00fcber'n Schlagbaum ziehn die Wolken nach Gefallen,", "tokens": ["Hin", "\u00fc\u00b7ber'n", "Schlag\u00b7baum", "ziehn", "die", "Wol\u00b7ken", "nach", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die V\u00f6gel d\u00fcrfen auch nach Lust dar\u00fcber wallen;", "tokens": ["Die", "V\u00f6\u00b7gel", "d\u00fcr\u00b7fen", "auch", "nach", "Lust", "da\u00b7r\u00fc\u00b7ber", "wal\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die h\u00fcben Nester baun, und dr\u00fcben, wenn sie wollen,", "tokens": ["Die", "h\u00fc\u00b7ben", "Nes\u00b7ter", "baun", ",", "und", "dr\u00fc\u00b7ben", ",", "wenn", "sie", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$,", "KON", "ADV", "$,", "KOUS", "PPER", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr Futter holen, ohn' es irgend zu verzollen.", "tokens": ["Ihr", "Fut\u00b7ter", "ho\u00b7len", ",", "ohn'", "es", "ir\u00b7gend", "zu", "ver\u00b7zol\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$,", "KOUI", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Nur Menschen trifft der Plack, da\u00df sie nicht nach Geschmack", "tokens": ["Nur", "Men\u00b7schen", "trifft", "der", "Plack", ",", "da\u00df", "sie", "nicht", "nach", "Ge\u00b7schmack"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "APPR", "NN"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Einf\u00fchren d\u00fcrfen Wein von hier, von dort Taback.", "tokens": ["Ein\u00b7f\u00fch\u00b7ren", "d\u00fcr\u00b7fen", "Wein", "von", "hier", ",", "von", "dort", "Ta\u00b7back", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "NN", "APPR", "ADV", "$,", "APPR", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}