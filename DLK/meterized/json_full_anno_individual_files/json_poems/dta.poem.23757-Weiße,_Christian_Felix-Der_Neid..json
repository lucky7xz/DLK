{"dta.poem.23757": {"metadata": {"author": {"name": "Wei\u00dfe, Christian Felix", "birth": "N.A.", "death": "N.A."}, "title": "Der Neid.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20643-5", "language": ["de:0.99"], "booktitle": "Wei\u00dfe, Christian Felix: Lieder f\u00fcr Kinder. Leipzig, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Man lobt den kleinen Fritzen sehr,", "tokens": ["Man", "lobt", "den", "klei\u00b7nen", "Frit\u00b7zen", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er sey gehorsam und bescheiden,", "tokens": ["Er", "sey", "ge\u00b7hor\u00b7sam", "und", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Verst\u00e4ndig, flei\u00dfig, lerne mehr,", "tokens": ["Ver\u00b7st\u00e4n\u00b7dig", ",", "flei\u00b7\u00dfig", ",", "ler\u00b7ne", "mehr", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als ich? ihn sollt ich wohl beneiden.", "tokens": ["Als", "ich", "?", "ihn", "sollt", "ich", "wohl", "be\u00b7nei\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Allein was hilft mein Neid mir nun?", "tokens": ["Al\u00b7lein", "was", "hilft", "mein", "Neid", "mir", "nun", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PPOSAT", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wird er dann weniger erhoben?", "tokens": ["Wird", "er", "dann", "we\u00b7ni\u00b7ger", "er\u00b7ho\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weit besser, es ihm vorzuthun!", "tokens": ["Weit", "bes\u00b7ser", ",", "es", "ihm", "vor\u00b7zu\u00b7thun", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mu\u00df man mich noch weit mehr loben.", "tokens": ["So", "mu\u00df", "man", "mich", "noch", "weit", "mehr", "lo\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "PRF", "ADV", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}