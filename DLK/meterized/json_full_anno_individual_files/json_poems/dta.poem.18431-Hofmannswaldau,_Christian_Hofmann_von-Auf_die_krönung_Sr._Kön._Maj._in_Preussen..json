{"dta.poem.18431": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf die kr\u00f6nung Sr. K\u00f6n. Maj.  \n in Preussen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.57", "nl:0.42"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was C&#339;sar abgezielt/ ward vom August vollzogen;", "tokens": ["Was", "C", "&#339;", "sar", "ab\u00b7ge\u00b7zielt", "/", "ward", "vom", "Au\u00b7gust", "voll\u00b7zo\u00b7gen", ";"], "token_info": ["word", "word", "XML_entity", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "$(", "ADJD", "VVPP", "$(", "VAFIN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Was Friedrich Wilhelm w\u00fcnscht/ hat Friederich gethan.", "tokens": ["Was", "Fried\u00b7rich", "Wil\u00b7helm", "w\u00fcnscht", "/", "hat", "Frie\u00b7de\u00b7rich", "ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "NE", "VVFIN", "$(", "VAFIN", "NE", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er legt ein neues reich/ wie dort Augustus/ an.", "tokens": ["Er", "legt", "ein", "neu\u00b7es", "reich", "/", "wie", "dort", "Au\u00b7gus\u00b7tus", "/", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJD", "$(", "KOKOM", "ADV", "NE", "$(", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Doch hierinn hat er noch den R\u00f6mer \u00fcberwogen;", "tokens": ["Doch", "hie\u00b7rinn", "hat", "er", "noch", "den", "R\u00f6\u00b7mer", "\u00fc\u00b7ber\u00b7wo\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df er in ruh betritt/ was jener blutig schaute/", "tokens": ["Da\u00df", "er", "in", "ruh", "be\u00b7tritt", "/", "was", "je\u00b7ner", "blu\u00b7tig", "schau\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "$(", "PWS", "PDAT", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df er dem sohne pflantzt/ was jener fremden baute.", "tokens": ["Da\u00df", "er", "dem", "soh\u00b7ne", "pflantzt", "/", "was", "je\u00b7ner", "frem\u00b7den", "bau\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$(", "PWS", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}