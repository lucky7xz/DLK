{"textgrid.poem.42176": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Eroberung", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, sie strampelt mit den F\u00fc\u00dfen,", "tokens": ["Ach", ",", "sie", "stram\u00b7pelt", "mit", "den", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, sie l\u00e4\u00dft es nicht geschehn,", "tokens": ["Ach", ",", "sie", "l\u00e4\u00dft", "es", "nicht", "ge\u00b7schehn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach, noch kann ich ihren s\u00fc\u00dfen", "tokens": ["Ach", ",", "noch", "kann", "ich", "ih\u00b7ren", "s\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VMFIN", "PPER", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6rper nur zur H\u00e4lfte sehn;", "tokens": ["K\u00f6r\u00b7per", "nur", "zur", "H\u00e4lf\u00b7te", "sehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Um die H\u00fcfte weht der Schleier,", "tokens": ["Um", "die", "H\u00fcf\u00b7te", "weht", "der", "Schlei\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Um den Schleier irrt mein Blick,", "tokens": ["Um", "den", "Schlei\u00b7er", "irrt", "mein", "Blick", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Immer wilder loht mein Feuer,", "tokens": ["Im\u00b7mer", "wil\u00b7der", "loht", "mein", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ach, sie dr\u00e4ngt mich scheu zur\u00fcck!", "tokens": ["Ach", ",", "sie", "dr\u00e4ngt", "mich", "scheu", "zu\u00b7r\u00fcck", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "M\u00e4dchen, ich will nichts erzwingen;", "tokens": ["M\u00e4d\u00b7chen", ",", "ich", "will", "nichts", "er\u00b7zwin\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VMFIN", "PIS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "M\u00e4dchen, gib mir einen Ku\u00df;", "tokens": ["M\u00e4d\u00b7chen", ",", "gib", "mir", "ei\u00b7nen", "Ku\u00df", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sieh, dich tragen eigne Schwingen", "tokens": ["Sieh", ",", "dich", "tra\u00b7gen", "eig\u00b7ne", "Schwin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch Begierde zum Genu\u00df.", "tokens": ["Durch"