{"dta.poem.17490": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "111.  Ausser GOtt leben ist Todt seyn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Mensch glaube di\u00df gewi\u00df: Wo du nicht lebst in GOtt/", "tokens": ["Mensch", "glau\u00b7be", "di\u00df", "ge\u00b7wi\u00df", ":", "Wo", "du", "nicht", "lebst", "in", "Gott", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PDS", "ADV", "$.", "PWAV", "PPER", "PTKNEG", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Lebstu gleich tausend Jahr/ du bist so lange todt.", "tokens": ["Lebs\u00b7tu", "gleich", "tau\u00b7send", "Jahr", "/", "du", "bist", "so", "lan\u00b7ge", "todt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "CARD", "NN", "$(", "PPER", "VAFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}