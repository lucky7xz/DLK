{"textgrid.poem.58576": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "8.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohl f\u00e4llt es hart und schwer, den Zorn des Herrn zu tragen", "tokens": ["Wohl", "f\u00e4llt", "es", "hart", "und", "schwer", ",", "den", "Zorn", "des", "Herrn", "zu", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In gro\u00dfen Proben; ach, wer will ihm danken dann!", "tokens": ["In", "gro\u00b7\u00dfen", "Pro\u00b7ben", ";", "ach", ",", "wer", "will", "ihm", "dan\u00b7ken", "dann", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "XY", "$,", "PWS", "VMFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer durchpassieret ist, der wei\u00df von Dank zu sagen,", "tokens": ["Wer", "durch\u00b7pas\u00b7sie\u00b7ret", "ist", ",", "der", "wei\u00df", "von", "Dank", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "VAFIN", "$,", "PRELS", "VVFIN", "APPR", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df sein getroster Geist von Herzen singen kann:", "tokens": ["Da\u00df", "sein", "ge\u00b7tros\u00b7ter", "Geist", "von", "Her\u00b7zen", "sin\u00b7gen", "kann", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Gott ist mein Heil allein, ich mu\u00dft' zur H\u00f6lle sinken,", "tokens": ["Gott", "ist", "mein", "Heil", "al\u00b7lein", ",", "ich", "mu\u00dft'", "zur", "H\u00f6l\u00b7le", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "ADV", "$,", "PPER", "VMFIN", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der Herr ist meine Kraft, es war schon mit mir aus;", "tokens": ["Der", "Herr", "ist", "mei\u00b7ne", "Kraft", ",", "es", "war", "schon", "mit", "mir", "aus", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Zwar find' ich nichts in mir, doch kann ich freudig trinken,", "tokens": ["Zwar", "find'", "ich", "nichts", "in", "mir", ",", "doch", "kann", "ich", "freu\u00b7dig", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PIS", "APPR", "PPER", "$,", "ADV", "VMFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da ich die Quell' des Heils, den Heiland, hab' im Haus.", "tokens": ["Da", "ich", "die", "Quell'", "des", "Heils", ",", "den", "Hei\u00b7land", ",", "hab'", "im", "Haus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART",