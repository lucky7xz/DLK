{"textgrid.poem.65845": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "119", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was f\u00fcr Sirenentr\u00e4nen, abgezogen", "tokens": ["Was", "f\u00fcr", "Si\u00b7re\u00b7nen\u00b7tr\u00e4\u00b7nen", ",", "ab\u00b7ge\u00b7zo\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PWS", "APPR", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Auf h\u00f6llischen Retorten sog ich ein!", "tokens": ["Auf", "h\u00f6l\u00b7li\u00b7schen", "Re\u00b7tor\u00b7ten", "sog", "ich", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie zwischen Furcht und Hoffnung gleich betrogen,", "tokens": ["Wie", "zwi\u00b7schen", "Furcht", "und", "Hoff\u00b7nung", "gleich", "be\u00b7tro\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erwarb ich statt Zufriedenheit mir Pein!", "tokens": ["Er\u00b7warb", "ich", "statt", "Zu\u00b7frie\u00b7den\u00b7heit", "mir", "Pein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wie frevelte das Herz in seinem Wahn,", "tokens": ["Wie", "fre\u00b7vel\u00b7te", "das", "Herz", "in", "sei\u00b7nem", "Wahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.6": {"text": "Als wenn es reich und selig w\u00e4r wie nie:", "tokens": ["Als", "wenn", "es", "reich", "und", "se\u00b7lig", "w\u00e4r", "wie", "nie", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "ADJD", "KON", "ADJD", "VAFIN", "KOKOM", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie rollte wild das Aug' aus seiner Bahn,", "tokens": ["Wie", "roll\u00b7te", "wild", "das", "Aug'", "aus", "sei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADJD", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "In jener w\u00fcsten Fieberphantasie! \u2013", "tokens": ["In", "je\u00b7ner", "w\u00fcs\u00b7ten", "Fie\u00b7ber\u00b7phan\u00b7ta\u00b7sie", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "O Arzenei des Schlimmen! Dies bew\u00e4hrt:", "tokens": ["O", "Ar\u00b7ze\u00b7nei", "des", "Schlim\u00b7men", "!", "Dies", "be\u00b7w\u00e4hrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$.", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Da\u00df \u00dcbel Bess'res gut und besser macht;", "tokens": ["Da\u00df", "\u00dc\u00b7bel", "Bess'\u00b7res", "gut", "und", "bes\u00b7ser", "macht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADJD", "KON", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Und da\u00df erloschne Liebe, frisch gen\u00e4hrt,", "tokens": [