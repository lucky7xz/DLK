{"textgrid.poem.41290": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Phanias", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es schreibt, mit leichter Hand, der leere Phanias,", "tokens": ["Es", "schreibt", ",", "mit", "leich\u00b7ter", "Hand", ",", "der", "lee\u00b7re", "Pha\u00b7ni\u00b7as", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei ungeduld'gem M\u00fc\u00dfiggang,", "tokens": ["Bei", "un\u00b7ge\u00b7duld'\u00b7gem", "M\u00fc\u00b7\u00dfig\u00b7gang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn' Achtsamkeit, Beruf und Zwang,", "tokens": ["Ohn'", "Acht\u00b7sam\u00b7keit", ",", "Be\u00b7ruf", "und", "Zwang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn' Ordnung und Zusammenhang,", "tokens": ["Ohn'", "Ord\u00b7nung", "und", "Zu\u00b7sam\u00b7men\u00b7hang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "In eines Buchs Gestalt, geschwind ich wei\u00df nicht was.", "tokens": ["In", "ei\u00b7nes", "Buchs", "Ge\u00b7stalt", ",", "ge\u00b7schwind", "ich", "wei\u00df", "nicht", "was", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "KON", "PPER", "VVFIN", "PTKNEG", "PIS", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ist dies nicht stets erlaubt gewesen?", "tokens": ["Ist", "dies", "nicht", "stets", "er\u00b7laubt", "ge\u00b7we\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "PTKNEG", "ADV", "VVPP", "VAPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Er schreibt ja, wie die meisten lesen.", "tokens": ["Er", "schreibt", "ja", ",", "wie", "die", "meis\u00b7ten", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "ART", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es schreibt, mit leichter Hand, der leere Phanias,", "tokens": ["Es", "schreibt", ",", "mit", "leich\u00b7ter", "Hand", ",", "der", "lee\u00b7re", "Pha\u00b7ni\u00b7as", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bei ungeduld'gem M\u00fc\u00dfiggang,", "tokens": ["Bei", "un\u00b7ge\u00b7duld'\u00b7gem", "M\u00fc\u00b7\u00dfig\u00b7gang", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ohn' Achtsamkeit, Beruf und Zwang,", "tokens": ["Ohn'", "Acht\u00b7sam\u00b7keit", ",", "Be\u00b7ruf", "und", "Zwang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ohn' Ordnung und Zusammenhang,", "tokens": ["Ohn'", "Ord\u00b7nung", "und", "Zu\u00b7sam