{"textgrid.poem.40372": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Das gro\u00dfe Karussell", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Himmel ist ein Karussell,", "tokens": ["Im", "Him\u00b7mel", "ist", "ein", "Ka\u00b7rus\u00b7sell", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "das dreht sich Tag und Nacht.", "tokens": ["das", "dreht", "sich", "Tag", "und", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es dreht sich wie im Traum so schnell,", "tokens": ["Es", "dreht", "sich", "wie", "im", "Traum", "so", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "KOKOM", "APPRART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "wir sehn es nicht, es ist zu hell", "tokens": ["wir", "sehn", "es", "nicht", ",", "es", "ist", "zu", "hell"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "PTKA", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "aus lauter Licht gemacht;", "tokens": ["aus", "lau\u00b7ter", "Licht", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "still, mein Wildfang, gib Acht!", "tokens": ["still", ",", "mein", "Wild\u00b7fang", ",", "gib", "Acht", "!"