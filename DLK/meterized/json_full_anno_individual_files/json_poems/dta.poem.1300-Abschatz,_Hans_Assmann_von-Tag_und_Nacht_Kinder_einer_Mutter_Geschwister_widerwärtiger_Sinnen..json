{"dta.poem.1300": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tag und Nacht/ Kinder einer Mutter/  \n Geschwister widerw\u00e4rtiger Sinnen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fl\u00fcgel-schnelle Zeit/ die F\u00fcrstin aller Sachen/", "tokens": ["Die", "Fl\u00fc\u00b7gel\u00b7schnel\u00b7le", "Zeit", "/", "die", "F\u00fcrs\u00b7tin", "al\u00b7ler", "Sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von welcher/ was nicht ist/ noch immer war entsteht/", "tokens": ["Von", "wel\u00b7cher", "/", "was", "nicht", "ist", "/", "noch", "im\u00b7mer", "war", "ent\u00b7steht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$(", "PWS", "PTKNEG", "VAFIN", "$(", "ADV", "ADV", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die alle Dinge gro\u00df und klein gewohnt zu machen/", "tokens": ["Die", "al\u00b7le", "Din\u00b7ge", "gro\u00df", "und", "klein", "ge\u00b7wohnt", "zu", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADJD", "KON", "ADJD", "VVPP", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit welcher/ was da ist/ und nicht stets war/ vergeht/", "tokens": ["Mit", "wel\u00b7cher", "/", "was", "da", "ist", "/", "und", "nicht", "stets", "war", "/", "ver\u00b7geht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELS", "$(", "PWS", "ADV", "VAFIN", "$(", "KON", "PTKNEG", "ADV", "VAFIN", "$(", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zu deren Diensten sich die Erde mu\u00df verpachen/", "tokens": ["Zu", "de\u00b7ren", "Diens\u00b7ten", "sich", "die", "Er\u00b7de", "mu\u00df", "ver\u00b7pa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "PRF", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der unterworffen ist was niedrig/ was erh\u00f6ht/", "tokens": ["Der", "un\u00b7ter\u00b7worf\u00b7fen", "ist", "was", "nied\u00b7rig", "/", "was", "er\u00b7h\u00f6ht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "VVPP", "VAFIN", "PIS", "ADJD", "$(", "PWS", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sucht das geraume Ziel der ungeme\u00dfnen Grantzen", "tokens": ["Sucht", "das", "ge\u00b7rau\u00b7me", "Ziel", "der", "un\u00b7ge\u00b7me\u00df\u00b7nen", "Grant\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Durch ein getreues Paar der Erben fortzupflantzen.", "tokens": ["Durch", "ein", "ge\u00b7treu\u00b7es", "Paar", "der", "Er\u00b7ben", "fort\u00b7zu\u00b7pflant\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es f\u00fchren diese Zwey noch bey der Mutter Leben", "tokens": ["Es", "f\u00fch\u00b7ren", "die\u00b7se", "Zwey", "noch", "bey", "der", "Mut\u00b7ter", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "CARD", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den hohen K\u00f6nigs-Stab in freygewohnter Hand/", "tokens": ["Den", "ho\u00b7hen", "K\u00f6\u00b7nigs\u00b7Stab", "in", "frey\u00b7ge\u00b7wohn\u00b7ter", "Hand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stadthalter m\u00fcssen sie in allen L\u00e4ndern geben/", "tokens": ["Stadt\u00b7hal\u00b7ter", "m\u00fcs\u00b7sen", "sie", "in", "al\u00b7len", "L\u00e4n\u00b7dern", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo dieser K\u00f6nigin Regierung ist bekandt/", "tokens": ["Wo", "die\u00b7ser", "K\u00f6\u00b7ni\u00b7gin", "Re\u00b7gie\u00b7rung", "ist", "be\u00b7kandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "NN", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch will sich mancher Streit bey solchem Paar erheben/", "tokens": ["Doch", "will", "sich", "man\u00b7cher", "Streit", "bey", "sol\u00b7chem", "Paar", "er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PRF", "PIAT", "NN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wer billich haben soll den allerersten Stand/", "tokens": ["Wer", "bil\u00b7lich", "ha\u00b7ben", "soll", "den", "al\u00b7le\u00b7rers\u00b7ten", "Stand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAINF", "VMFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wem wohl der gr\u00f6ste Staat zu halten will geb\u00fchren/", "tokens": ["Wem", "wohl", "der", "gr\u00f6s\u00b7te", "Staat", "zu", "hal\u00b7ten", "will", "ge\u00b7b\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "PTKZU", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wer k\u00fcnfftig mit der Zeit soll Kron und Scepter f\u00fchren?", "tokens": ["Wer", "k\u00fcnff\u00b7tig", "mit", "der", "Zeit", "soll", "Kron", "und", "Scep\u00b7ter", "f\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "ART", "NN", "VMFIN", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dem einen pflichten bey die meisten Reichs-Gesetze/", "tokens": ["Dem", "ei\u00b7nen", "pflich\u00b7ten", "bey", "die", "meis\u00b7ten", "Reichs\u00b7Ge\u00b7set\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ART", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der ander gr\u00fcndet sich auff mancher V\u00f6lcker Recht/", "tokens": ["Der", "an\u00b7der", "gr\u00fcn\u00b7det", "sich", "auff", "man\u00b7cher", "V\u00f6l\u00b7cker", "Recht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PRF", "APPR", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+---+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Der eine macht sich gro\u00df durch eingetragne Sch\u00e4tze/", "tokens": ["Der", "ei\u00b7ne", "macht", "sich", "gro\u00df", "durch", "ein\u00b7ge\u00b7trag\u00b7ne", "Sch\u00e4t\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "ADJD", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Damit er auff den Fall kan werben manchen Knecht/", "tokens": ["Da\u00b7mit", "er", "auff", "den", "Fall", "kan", "wer\u00b7ben", "man\u00b7chen", "Knecht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VMFIN", "VVINF", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Durch Freyheit/ da\u00df man sich in sanffter Ruh erg\u00f6tze/", "tokens": ["Durch", "Frey\u00b7heit", "/", "da\u00df", "man", "sich", "in", "sanff\u00b7ter", "Ruh", "er\u00b7g\u00f6t\u00b7ze", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "KOUS", "PIS", "PRF", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird von dem andern Theil des ersten Heer geschw\u00e4cht/", "tokens": ["Wird", "von", "dem", "an\u00b7dern", "Theil", "des", "ers\u00b7ten", "Heer", "ge\u00b7schw\u00e4cht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der eine suchet Gunst durch M\u00fchsamkeit bey allen/", "tokens": ["Der", "ei\u00b7ne", "su\u00b7chet", "Gunst", "durch", "M\u00fch\u00b7sam\u00b7keit", "bey", "al\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "NN", "APPR", "NN", "APPR", "PIAT", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der ander will der Welt durchs Widerspiel gefallen.", "tokens": ["Der", "an\u00b7der", "will", "der", "Welt", "durchs", "Wi\u00b7der\u00b7spiel", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VMFIN", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem einen ist das Haubt von denen Hofe-N\u00e4then/", "tokens": ["Dem", "ei\u00b7nen", "ist", "das", "Haubt", "von", "de\u00b7nen", "Ho\u00b7fe\u00b7N\u00e4then", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VAFIN", "ART", "NN", "APPR", "PRELS", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die seine Mutter h\u00e4lt/ zu Diensten beygethan;", "tokens": ["Die", "sei\u00b7ne", "Mut\u00b7ter", "h\u00e4lt", "/", "zu", "Diens\u00b7ten", "bey\u00b7ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVFIN", "$(", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die andern sechse sind zum Gegentheil getreten/", "tokens": ["Die", "an\u00b7dern", "sech\u00b7se", "sind", "zum", "Ge\u00b7gen\u00b7theil", "ge\u00b7tre\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil sich ihr Kopff zu ihm am besten schicken kan/", "tokens": ["Weil", "sich", "ihr", "Kopff", "zu", "ihm", "am", "bes\u00b7ten", "schi\u00b7cken", "kan", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPER", "PTKA", "ADJD", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der eine lebet mehr zu Land/ als in den St\u00e4dten/", "tokens": ["Der", "ei\u00b7ne", "le\u00b7bet", "mehr", "zu", "Land", "/", "als", "in", "den", "St\u00e4d\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "APPR", "NN", "$(", "KOKOM", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Der ander sezt die Stadt dem Dorffe weit voran/", "tokens": ["Der", "an\u00b7der", "sezt", "die", "Stadt", "dem", "Dorf\u00b7fe", "weit", "vo\u00b7ran", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der eine f\u00fchrt nicht viel/ doch guttes Volck zur Seiten/", "tokens": ["Der", "ei\u00b7ne", "f\u00fchrt", "nicht", "viel", "/", "doch", "gut\u00b7tes", "Volck", "zur", "Sei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PTKNEG", "ADV", "$(", "ADV", "ADJA", "NN", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Den andern aber pflegt die Menge zu begleiten.", "tokens": ["Den", "an\u00b7dern", "a\u00b7ber", "pflegt", "die", "Men\u00b7ge", "zu", "be\u00b7glei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der eine l\u00e4sset sich viel kluge K\u00fcnste lehren/", "tokens": ["Der", "ei\u00b7ne", "l\u00e4s\u00b7set", "sich", "viel", "klu\u00b7ge", "K\u00fcns\u00b7te", "leh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PRF", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Um Wissenschafften bleibt der ander unbem\u00fcht/", "tokens": ["Um", "Wis\u00b7sen\u00b7schaff\u00b7ten", "bleibt", "der", "an\u00b7der", "un\u00b7be\u00b7m\u00fcht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "VVFIN", "ART", "ADJD", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der eine l\u00e4st von sich viel Wort und Reden h\u00f6ren/", "tokens": ["Der", "ei\u00b7ne", "l\u00e4st", "von", "sich", "viel", "Wort", "und", "Re\u00b7den", "h\u00f6\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "PRF", "PIAT", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn stille Tr\u00e4umerey des andern Kopff durchzieht/", "tokens": ["Wenn", "stil\u00b7le", "Tr\u00e4u\u00b7me\u00b7rey", "des", "an\u00b7dern", "Kopff", "durch\u00b7zieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der eine pflegt allein die Sonne zu verehren/", "tokens": ["Der", "ei\u00b7ne", "pflegt", "al\u00b7lein", "die", "Son\u00b7ne", "zu", "ver\u00b7eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ART", "NN", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wenn jener nach dem Mond und tausend Sternen sieht/", "tokens": ["Wenn", "je\u00b7ner", "nach", "dem", "Mond", "und", "tau\u00b7send", "Ster\u00b7nen", "sieht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "ART", "NN", "KON", "CARD", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den einen k\u00f6nnen Schlaff und Liebe nicht bezwingen/", "tokens": ["Den", "ei\u00b7nen", "k\u00f6n\u00b7nen", "Schlaff", "und", "Lie\u00b7be", "nicht", "be\u00b7zwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dem andern m\u00fcssen sie die gr\u00f6ste Freude bringen.", "tokens": ["Dem", "an\u00b7dern", "m\u00fcs\u00b7sen", "sie", "die", "gr\u00f6s\u00b7te", "Freu\u00b7de", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der eine weiset gern dem Lichte seine Thaten/", "tokens": ["Der", "ei\u00b7ne", "wei\u00b7set", "gern", "dem", "Lich\u00b7