{"dta.poem.19601": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der gleitende Purpur.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eeia Weihnacht! Eia Weihnacht!\u201c", "tokens": ["\u201e", "ei\u00b7a", "Weih\u00b7nacht", "!", "Ei\u00b7a", "Weih\u00b7nacht", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "NE", "NN", "$.", "$("], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Schallt im M\u00fcnsterchor der Psalm der Knaben.", "tokens": ["Schallt", "im", "M\u00fcns\u00b7ter\u00b7chor", "der", "Psalm", "der", "Kna\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kaiser Otto lauscht der Mette,", "tokens": ["Kai\u00b7ser", "Ot\u00b7to", "lauscht", "der", "Met\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Diener hinter sich mit Spend' und Gaben.", "tokens": ["Die\u00b7ner", "hin\u00b7ter", "sich", "mit", "Spend'", "und", "Ga\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PRF", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Eia Weihnacht! Eia Weihnacht!", "tokens": ["Ei\u00b7a", "Weih\u00b7nacht", "!", "Ei\u00b7a", "Weih\u00b7nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heute, da die Himmel niederschweben,", "tokens": ["Heu\u00b7te", ",", "da", "die", "Him\u00b7mel", "nie\u00b7der\u00b7schwe\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wird dem Elend und der Bl\u00f6\u00dfe", "tokens": ["Wird", "dem", "E\u00b7lend", "und", "der", "Bl\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00e4ntel er und warme R\u00f6cke geben.", "tokens": ["M\u00e4n\u00b7tel", "er", "und", "war\u00b7me", "R\u00f6\u00b7cke", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Hundert Bettler stehn erwartend \u2014", "tokens": ["Hun\u00b7dert", "Bett\u00b7ler", "stehn", "er\u00b7war\u00b7tend"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVINF", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer h\u00e4lt des Kaisers Knie umfangen", "tokens": ["Ei\u00b7ner", "h\u00e4lt", "des", "Kai\u00b7sers", "Knie", "um\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit den wundgeriebnen Armen,", "tokens": ["Mit", "den", "wund\u00b7ge\u00b7rieb\u00b7nen", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dran zerrissner Fesseln Enden hangen.", "tokens": ["Dran", "zer\u00b7riss\u00b7ner", "Fes\u00b7seln", "En\u00b7den", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u2014 \u201eSchalk! Was zerrst du mir den Purpur?", "tokens": ["\u201e", "Schalk", "!", "Was", "zerrst", "du", "mir", "den", "Pur\u00b7pur", "?"], "token_info": ["punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "$(", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Harr und beite! Kennst du mich als Kargen?\u201c", "tokens": ["Harr", "und", "bei\u00b7te", "!", "Kennst", "du", "mich", "als", "Kar\u00b7gen", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "VVFIN", "$.", "VVFIN", "PPER", "PRF", "KOUS", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Doch der Bettler h\u00e4lt den Mantel", "tokens": ["Doch", "der", "Bett\u00b7ler", "h\u00e4lt", "den", "Man\u00b7tel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fest und jammert: \u201eKennst Du mich, den Argen?", "tokens": ["Fest", "und", "jam\u00b7mert", ":", "\u201e", "Kennst", "Du", "mich", ",", "den", "Ar\u00b7gen", "?"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "$.", "$(", "VVFIN", "PPER", "PRF", "$,", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Du Gesalbter und Erlauchter!", "tokens": ["Du", "Ge\u00b7salb\u00b7ter", "und", "Er\u00b7lauch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennst Du mich? ... Du hast mit mir gelegen,", "tokens": ["Kennst", "Du", "mich", "?", "...", "Du", "hast", "mit", "mir", "ge\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "$.", "$(", "PPER", "VAFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Mit dem Siechen, mit dem Wunden,", "tokens": ["Mit", "dem", "Sie\u00b7chen", ",", "mit", "dem", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unter Eines Mutterherzens Schl\u00e4gen.", "tokens": ["Un\u00b7ter", "Ei\u00b7nes", "Mut\u00b7ter\u00b7her\u00b7zens", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aus demselben Wollentuche", "tokens": ["Aus", "dem\u00b7sel\u00b7ben", "Wol\u00b7len\u00b7tu\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnitt man uns die Kappen und die Kleider!", "tokens": ["Schnitt", "man", "uns", "die", "Kap\u00b7pen", "und", "die", "Klei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PRF", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus demselben Psalmenbuche", "tokens": ["Aus", "dem\u00b7sel\u00b7ben", "Psal\u00b7men\u00b7bu\u00b7che"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sang das frische Jugendantlitz Beider!", "tokens": ["Sang", "das", "fri\u00b7sche", "Ju\u00b7gen\u00b7dant\u00b7litz", "Bei\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Heinz wo bist du? Heinz wo bleibst du?", "tokens": ["Heinz", "wo", "bist", "du", "?", "Heinz", "wo", "bleibst", "du", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "VAFIN", "PPER", "$.", "NN", "PWAV", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hast zum Spiele Du mich oft gerufen", "tokens": ["Hast", "zum", "Spie\u00b7le", "Du", "mich", "oft", "ge\u00b7ru\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPRART", "NN", "PPER", "PRF", "ADV", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Durch die S\u00e4le, durch die G\u00e4nge,", "tokens": ["Durch", "die", "S\u00e4\u00b7le", ",", "durch", "die", "G\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf und ab der Wendeltreppe Stufen ...", "tokens": ["Auf", "und", "ab", "der", "Wen\u00b7del\u00b7trep\u00b7pe", "Stu\u00b7fen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "ART", "NN", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Dann als einen falschen Bruder", "tokens": ["Dann", "als", "ei\u00b7nen", "fal\u00b7schen", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Verr\u00e4ther hast Du mich erfunden!", "tokens": ["Und", "Ver\u00b7r\u00e4\u00b7ther", "hast", "Du", "mich", "er\u00b7fun\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Du ergrimmtest und Du warfest", "tokens": ["Du", "er\u00b7grimm\u00b7test", "und", "Du", "war\u00b7fest"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "PPER", "VAFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "In die Kerkertiefe mich gebunden ...", "tokens": ["In", "die", "Ker\u00b7ker\u00b7tie\u00b7fe", "mich", "ge\u00b7bun\u00b7den", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "In der Tiefe meines Kerkers", "tokens": ["In", "der", "Tie\u00b7fe", "mei\u00b7nes", "Ker\u00b7kers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hab' ich ohne Mantel heut gefroren ...", "tokens": ["Hab'", "ich", "oh\u00b7ne", "Man\u00b7tel", "heut", "ge\u00b7fro\u00b7ren", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Eia Weihnacht! Eia Weihnacht!", "tokens": ["Ei\u00b7a", "Weih\u00b7nacht", "!", "Ei\u00b7a", "Weih\u00b7nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heute wird der Welt das Heil geboren!\u201c", "tokens": ["Heu\u00b7te", "wird", "der", "Welt", "das", "Heil", "ge\u00b7bo\u00b7ren", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "\u201eeia Weihnacht! Eia Weihnacht!\u201c", "tokens": ["\u201e", "ei\u00b7a", "Weih\u00b7nacht", "!", "Ei\u00b7a", "Weih\u00b7nacht", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "ADJA", "NN", "$.", "NE", "NN", "$.", "$("], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "Hundert Bettler strecken jetzt die H\u00e4nde:", "tokens": ["Hun\u00b7dert", "Bett\u00b7ler", "stre\u00b7cken", "jetzt", "die", "H\u00e4n\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u201egieb uns M\u00e4ntel! Gieb uns R\u00f6cke!", "tokens": ["\u201e", "gieb", "uns", "M\u00e4n\u00b7tel", "!", "Gieb", "uns", "R\u00f6\u00b7cke", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVIMP", "PPER", "NN", "$.", "VVIMP", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sei barmherzig! Gieb uns Deine Spende!\u201c", "tokens": ["Sei", "barm\u00b7her\u00b7zig", "!", "Gieb", "uns", "Dei\u00b7ne", "Spen\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAIMP", "ADJD", "$.", "VVIMP", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Eine Spange l\u00f6st der Kaiser", "tokens": ["Ei\u00b7ne", "Span\u00b7ge", "l\u00f6st", "der", "Kai\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sacht. Sein Purpur gleitet, gleitet, gleitet", "tokens": ["Sacht", ".", "Sein", "Pur\u00b7pur", "glei\u00b7tet", ",", "glei\u00b7tet", ",", "glei\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["NN", "$.", "PPOSAT", "NN", "VVFIN", "$,", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Ueber seinen s\u00fcnd'gen Bruder", "tokens": ["Ue\u00b7ber", "sei\u00b7nen", "s\u00fcn\u00b7d'\u00b7gen", "Bru\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Und der erste Bettler steht bekleidet ...", "tokens": ["Und", "der", "ers\u00b7te", "Bett\u00b7ler", "steht", "be\u00b7klei\u00b7det", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Eia Weihnacht! Eia Weihnacht!", "tokens": ["Ei\u00b7a", "Weih\u00b7nacht", "!", "Ei\u00b7a", "Weih\u00b7nacht", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "NE", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jubelt Erd' und Himmelreich mit Schallen.", "tokens": ["Ju\u00b7belt", "Erd'", "und", "Him\u00b7mel\u00b7reich", "mit", "Schal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Glorie! Glorie! Friede! Freude!", "tokens": ["Glo\u00b7rie", "!", "Glo\u00b7rie", "!", "Frie\u00b7de", "!", "Freu\u00b7de", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "NN", "$.", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und am Menschenkind ein Wohlgefallen!", "tokens": ["Und", "am", "Men\u00b7schen\u00b7kind", "ein", "Wohl\u00b7ge\u00b7fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}