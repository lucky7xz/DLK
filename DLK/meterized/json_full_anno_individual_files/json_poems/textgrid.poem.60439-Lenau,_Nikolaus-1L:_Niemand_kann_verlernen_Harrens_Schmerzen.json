{"textgrid.poem.60439": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Niemand kann verlernen Harrens Schmerzen", "genre": "verse", "period": "N.A.", "pub_year": 1826, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Niemand kann verlernen Harrens Schmerzen", "tokens": ["Nie\u00b7mand", "kann", "ver\u00b7ler\u00b7nen", "Har\u00b7rens", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einem sehnsuchtsvollen Frauenherzen", "tokens": ["Ei\u00b7nem", "sehn\u00b7suchts\u00b7vol\u00b7len", "Frau\u00b7en\u00b7her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Je vergelten, niemand ihr verg\u00fcten,", "tokens": ["Je", "ver\u00b7gel\u00b7ten", ",", "nie\u00b7mand", "ihr", "ver\u00b7g\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was in solchen unerme\u00dfnen Stunden", "tokens": ["Was", "in", "sol\u00b7chen", "un\u00b7er\u00b7me\u00df\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Still der Wurm genagt von ihren Bl\u00fcten,", "tokens": ["Still", "der", "Wurm", "ge\u00b7nagt", "von", "ih\u00b7ren", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "VVPP", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wenn er dann auch st\u00fcrzt zu ihren F\u00fc\u00dfen,", "tokens": ["Wenn", "er", "dann", "auch", "st\u00fcrzt", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Wenn er unter Tr\u00e4nen, tausend K\u00fcssen", "tokens": ["Wenn", "er", "un\u00b7ter", "Tr\u00e4\u00b7nen", ",", "tau\u00b7send", "K\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN", "$,", "CARD", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Leiden und vers\u00e4umtes Gl\u00fcck beklagt;", "tokens": ["Lei\u00b7den", "und", "ver\u00b7s\u00e4um\u00b7tes", "Gl\u00fcck", "be\u00b7klagt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Schmerz hat weh getan, der Wurm genagt.", "tokens": ["Schmerz", "hat", "weh", "ge\u00b7tan", ",", "der", "Wurm", "ge\u00b7nagt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "VVPP", "$,", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Aber mancher kehret nie mehr wieder,", "tokens": ["A\u00b7ber", "man\u00b7cher", "keh\u00b7ret", "nie", "mehr", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Dr\u00fcckt er auch ein Herz zum Grabe nieder.", "tokens": ["Dr\u00fcckt", "er", "auch", "ein", "Herz", "zum", "Gra\u00b7be", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Mira! herrliches Zigeunerkind!", "tokens": ["Mi\u00b7ra", "!", "herr\u00b7li\u00b7ches", "Zi\u00b7geu\u00b7ner\u00b7kind", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schnell hast du geliebt und welkst geschwind.", "tokens": ["Schnell", "hast", "du", "ge\u00b7liebt", "und", "welkst", "ge\u00b7schwind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "VVPP", "KON", "ADV", "ADJD", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Er verriet, verlie\u00df dich feigen Mutes,", "tokens": ["Er", "ver\u00b7riet", ",", "ver\u00b7lie\u00df", "dich", "fei\u00b7gen", "Mu\u00b7tes", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Weil die Liebe, die sein Herz versch\u00f6nt,", "tokens": ["Weil", "die", "Lie\u00b7be", ",", "die", "sein", "Herz", "ver\u00b7sch\u00f6nt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PRELS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ward in einer Schilderei verh\u00f6hnt", "tokens": ["Ward", "in", "ei\u00b7ner", "Schil\u00b7de\u00b7rei", "ver\u00b7h\u00f6hnt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Von den Adeligen seines Blutes.", "tokens": ["Von", "den", "A\u00b7de\u00b7li\u00b7gen", "sei\u00b7nes", "Blu\u00b7tes", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Eines Morgens kam in goldnem Rahmen", "tokens": ["Ei\u00b7nes", "Mor\u00b7gens", "kam", "in", "gold\u00b7nem", "Rah\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Ihm ein Bild, und das entrei\u00dft dir ihn,", "tokens": ["Ihm", "ein", "Bild", ",", "und", "das", "ent\u00b7rei\u00dft", "dir", "ihn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "KON", "PDS", "VVFIN", "PPER", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Weils dich schm\u00e4ht; auch hat er schon dahin", "tokens": ["Weils", "dich", "schm\u00e4ht", ";", "auch", "hat", "er", "schon", "da\u00b7hin"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "$.", "ADV", "VAFIN", "PPER", "ADV", "PAV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.10": {"text": "Schnellgesprochner Liebe s\u00fc\u00dfes Amen.", "tokens": ["Schnell\u00b7ge\u00b7sproch\u00b7ner", "Lie\u00b7be", "s\u00fc\u00b7\u00dfes", "A\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Stattlich zeigt das Bild auf breitem Raum", "tokens": ["Statt\u00b7lich", "zeigt", "das", "Bild", "auf", "brei\u00b7tem", "Raum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Seinen altber\u00fchmten Wappenbaum,", "tokens": ["Sei\u00b7nen", "alt\u00b7be\u00b7r\u00fchm\u00b7ten", "Wap\u00b7pen\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wie der Stamm sich spreitet, herrlich ragend,", "tokens": ["Wie", "der", "Stamm", "sich", "sprei\u00b7tet", ",", "herr\u00b7lich", "ra\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ruhm und Glanz auf jedem Zweige tragend.", "tokens": ["Ruhm", "und", "Glanz", "auf", "je\u00b7dem", "Zwei\u00b7ge", "tra\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Neben solchem Baume, hehr und stolz,", "tokens": ["Ne\u00b7ben", "sol\u00b7chem", "Bau\u00b7me", ",", "hehr", "und", "stolz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Steht ein schlechtes, d\u00fcrres Galgenholz,", "tokens": ["Steht", "ein", "schlech\u00b7tes", ",", "d\u00fcr\u00b7res", "Gal\u00b7gen\u00b7holz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Galgen hinter Galgen ist zu schauen,", "tokens": ["Gal\u00b7gen", "hin\u00b7ter", "Gal\u00b7gen", "ist", "zu", "schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Nach des Bildes Tiefe immer kleiner,", "tokens": ["Nach", "des", "Bil\u00b7des", "Tie\u00b7fe", "im\u00b7mer", "klei\u00b7ner", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Gleichsam schindend in der Vorzeit Grauen,", "tokens": ["Gleich\u00b7sam", "schin\u00b7dend", "in", "der", "Vor\u00b7zeit", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Und an jedem hangend ein Zigeuner;", "tokens": ["Und", "an", "je\u00b7dem", "han\u00b7gend", "ein", "Zi\u00b7geu\u00b7ner", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Und zerstreut im grausen d\u00fcrren Walde", "tokens": ["Und", "zer\u00b7streut", "im", "grau\u00b7sen", "d\u00fcr\u00b7ren", "Wal\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Sind viel schwarze Raben als Heralde;", "tokens": ["Sind", "viel", "schwar\u00b7ze", "Ra\u00b7ben", "als", "Her\u00b7al\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "KOUS", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Andre, auf dem Stammbaum, breit sich setzend,", "tokens": ["And\u00b7re", ",", "auf", "dem", "Stamm\u00b7baum", ",", "breit", "sich", "set\u00b7zend", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "$,", "APPR", "ART", "NN", "$,", "ADJD", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "An den Wappen sich den Schnabel wetzend.", "tokens": ["An", "den", "Wap\u00b7pen", "sich", "den", "Schna\u00b7bel", "wet\u00b7zend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PRF", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Niemand kann verlernen Harrens Schmerzen", "tokens": ["Nie\u00b7mand", "kann", "ver\u00b7ler\u00b7nen", "Har\u00b7rens", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VMFIN", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einem sehnsuchtsvollen Frauenherzen", "tokens": ["Ei\u00b7nem", "sehn\u00b7suchts\u00b7vol\u00b7len", "Frau\u00b7en\u00b7her\u00b7zen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Je vergelten, niemand ihr verg\u00fcten,", "tokens": ["Je", "ver\u00b7gel\u00b7ten", ",", "nie\u00b7mand", "ihr", "ver\u00b7g\u00fc\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "$,", "PIS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Was in solchen unerme\u00dfnen Stunden", "tokens": ["Was", "in", "sol\u00b7chen", "un\u00b7er\u00b7me\u00df\u00b7nen", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Still der Wurm genagt von ihren Bl\u00fcten,", "tokens": ["Still", "der", "Wurm", "ge\u00b7nagt", "von", "ih\u00b7ren", "Bl\u00fc\u00b7