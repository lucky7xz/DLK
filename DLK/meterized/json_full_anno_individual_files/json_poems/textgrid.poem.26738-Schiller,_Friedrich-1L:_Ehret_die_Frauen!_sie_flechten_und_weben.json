{"textgrid.poem.26738": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ehret die Frauen! sie flechten und weben", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ehret die Frauen! sie flechten und weben", "tokens": ["Eh\u00b7ret", "die", "Frau\u00b7en", "!", "sie", "flech\u00b7ten", "und", "we\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "PPER", "VVINF", "KON", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Himmlische Rosen ins irdische Leben,", "tokens": ["Himm\u00b7li\u00b7sche", "Ro\u00b7sen", "ins", "ir\u00b7di\u00b7sche", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Flechten der Liebe begl\u00fcckendes Band,", "tokens": ["Flech\u00b7ten", "der", "Lie\u00b7be", "be\u00b7gl\u00fc\u00b7cken\u00b7des", "Band", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Und in der Grazie z\u00fcchtigem Schleier", "tokens": ["Und", "in", "der", "Gra\u00b7zie", "z\u00fcch\u00b7ti\u00b7gem", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "N\u00e4hren sie wachsam das ewige Feuer", "tokens": ["N\u00e4h\u00b7ren", "sie", "wach\u00b7sam", "das", "e\u00b7wi\u00b7ge", "Feu\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "ART", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Sch\u00f6ner Gef\u00fchle mit heiliger Hand.", "tokens": ["Sch\u00f6\u00b7ner", "Ge\u00b7f\u00fch\u00b7le", "mit", "hei\u00b7li\u00b7ger", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Ewig aus der Wahrheit Schranken", "tokens": ["E\u00b7wig", "aus", "der", "Wahr\u00b7heit", "Schran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweift des Mannes wilde Kraft,", "tokens": ["Schweift", "des", "Man\u00b7nes", "wil\u00b7de", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unstet treiben die Gedanken", "tokens": ["Un\u00b7stet", "trei\u00b7ben", "die", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Auf dem Meer der Leidenschaft.", "tokens": ["Auf", "dem", "Meer", "der", "Lei\u00b7den\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gierig greift er in die Ferne,", "tokens": ["Gie\u00b7rig", "greift", "er", "in", "die", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer wird sein Herz gestillt,", "tokens": ["Nim\u00b7mer", "wird", "sein", "Herz", "ge\u00b7stillt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Rastlos durch entlegne Sterne", "tokens": ["Rast\u00b7los", "durch", "ent\u00b7leg\u00b7ne", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Jagt er seines Traumes Bild.", "tokens": ["Jagt", "er", "sei\u00b7nes", "Trau\u00b7mes", "Bild", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber mit zauberisch fesselndem Blicke", "tokens": ["A\u00b7ber", "mit", "zau\u00b7be\u00b7risch", "fes\u00b7seln\u00b7dem", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJD", "ADJA", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Winken die Frauen den Fl\u00fcchtling zur\u00fccke,", "tokens": ["Win\u00b7ken", "die", "Frau\u00b7en", "den", "Fl\u00fccht\u00b7ling", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Warnend zur\u00fcck in der Gegenwart Spur.", "tokens": ["War\u00b7nend", "zu\u00b7r\u00fcck", "in", "der", "Ge\u00b7gen\u00b7wart", "Spur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "APPR", "ART", "NN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "In der Mutter bescheidener H\u00fctte", "tokens": ["In", "der", "Mut\u00b7ter", "be\u00b7schei\u00b7de\u00b7ner", "H\u00fct\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Sind sie geblieben mit schamhafter Sitte,", "tokens": ["Sind", "sie", "ge\u00b7blie\u00b7ben", "mit", "scham\u00b7haf\u00b7ter", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Treue T\u00f6chter der frommen Natur.", "tokens": ["Treu\u00b7e", "T\u00f6ch\u00b7ter", "der", "from\u00b7men", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Feindlich ist des Mannes Streben,", "tokens": ["Feind\u00b7lich", "ist", "des", "Man\u00b7nes", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit zermalmender Gewalt", "tokens": ["Mit", "zer\u00b7mal\u00b7men\u00b7der", "Ge\u00b7walt"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Geht der wilde durch das Leben,", "tokens": ["Geht", "der", "wil\u00b7de", "durch", "das", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Rast und Aufenthalt.", "tokens": ["Oh\u00b7ne", "Rast", "und", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was er schuf, zerst\u00f6rt er wieder,", "tokens": ["Was", "er", "schuf", ",", "zer\u00b7st\u00f6rt", "er", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nimmer ruht der W\u00fcnsche Streit,", "tokens": ["Nim\u00b7mer", "ruht", "der", "W\u00fcn\u00b7sche", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Nimmer, wie das Haupt der Hyder", "tokens": ["Nim\u00b7mer", ",", "wie", "das", "Haupt", "der", "Hy\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ewig f\u00e4llt und sich erneut.", "tokens": ["E\u00b7wig", "f\u00e4llt", "und", "sich", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Aber, zufrieden mit stillerem Ruhme,", "tokens": ["A\u00b7ber", ",", "zu\u00b7frie\u00b7den", "mit", "stil\u00b7le\u00b7rem", "Ruh\u00b7me", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Brechen die Frauen des Augenblicks Blume,", "tokens": ["Bre\u00b7chen", "die", "Frau\u00b7en", "des", "Au\u00b7gen\u00b7blicks", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ART", "NN", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "N\u00e4hren sie sorgsam mit liebendem Flei\u00df,", "tokens": ["N\u00e4h\u00b7ren", "sie", "sorg\u00b7sam", "mit", "lie\u00b7ben\u00b7dem", "Flei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Freier in ihrem gebundenen Wirken,", "tokens": ["Frei\u00b7er", "in", "ih\u00b7rem", "ge\u00b7bun\u00b7de\u00b7nen", "Wir\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Reicher als er in des Wissens Bezirken", "tokens": ["Rei\u00b7cher", "als", "er", "in", "des", "Wis\u00b7sens", "Be\u00b7zir\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KOUS", "PPER", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Und in der Dichtung unendlichem Kreis.", "tokens": ["Und", "in", "der", "Dich\u00b7tung", "un\u00b7end\u00b7li\u00b7chem", "Kreis", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.6": {"line.1": {"text": "Streng und stolz sich selbst gen\u00fcgend,", "tokens": ["Streng", "und", "stolz", "sich", "selbst", "ge\u00b7n\u00fc\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "ADJD", "PRF", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kennt des Mannes kalte Brust,", "tokens": ["Kennt", "des", "Man\u00b7nes", "kal\u00b7te", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Herzlich an ein Herz sich schmiegend,", "tokens": ["Herz\u00b7lich", "an", "ein", "Herz", "sich", "schmie\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PRF", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nicht der Liebe G\u00f6tterlust,", "tokens": ["Nicht", "der", "Lie\u00b7be", "G\u00f6t\u00b7ter\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kennet nicht den Tausch der Seelen,", "tokens": ["Ken\u00b7net", "nicht", "den", "Tausch", "der", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nicht in Tr\u00e4nen schmilzt er hin,", "tokens": ["Nicht", "in", "Tr\u00e4\u00b7nen", "schmilzt", "er", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Selbst des Lebens K\u00e4mpfe st\u00e4hlen", "tokens": ["Selbst", "des", "Le\u00b7bens", "K\u00e4mp\u00b7fe", "st\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "H\u00e4rter seinen harten Sinn.", "tokens": ["H\u00e4r\u00b7ter", "sei\u00b7nen", "har\u00b7ten", "Sinn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Aber, wie leise vom Zephir ersch\u00fcttert", "tokens": ["A\u00b7ber", ",", "wie", "lei\u00b7se", "vom", "Ze\u00b7phir", "er\u00b7sch\u00fct\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "$,", "PWAV", "ADJD", "APPRART", "NN", "VVPP"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schnell die \u00e4olische Harfe erzittert,", "tokens": ["Schnell", "die", "\u00e4\u00b7o\u00b7li\u00b7sche", "Har\u00b7fe", "er\u00b7zit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Also die f\u00fchlende Seele der Frau.", "tokens": ["Al\u00b7so", "die", "f\u00fch\u00b7len\u00b7de", "See\u00b7le", "der", "Frau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.4": {"text": "Z\u00e4rtlich ge\u00e4ngstigt vom Bilde der Qualen,", "tokens": ["Z\u00e4rt\u00b7lich", "ge\u00b7\u00e4ngs\u00b7tigt", "vom", "Bil\u00b7de", "der", "Qua\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Wallet der liebende Busen, es strahlen", "tokens": ["Wal\u00b7let", "der", "lie\u00b7ben\u00b7de", "Bu\u00b7sen", ",", "es", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "PPER", "VVFIN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.6": {"text": "Perlend die Augen von himmlischem Tau.", "tokens": ["Per\u00b7lend", "die", "Au\u00b7gen", "von", "himm\u00b7li\u00b7schem", "Tau", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.8": {"line.1": {"text": "In der M\u00e4nner Herrschgebiete", "tokens": ["In", "der", "M\u00e4n\u00b