{"dta.poem.8122": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sie find\u2019s, ach! die wehdroh\u2019nd der Aufruf schreckt,", "tokens": ["Sie", "fin\u00b7d's", ",", "ach", "!", "die", "weh\u00b7droh'nd", "der", "Auf\u00b7ruf", "schreckt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ITJ", "$.", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie stehn auch von dem Tod\u2019 auf! O verschl\u00f6\u00df Nacht stets", "tokens": ["Sie", "stehn", "auch", "von", "dem", "Tod'", "auf", "!", "O", "ver\u00b7schl\u00f6\u00df", "Nacht", "stets"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ", "$.", "NE", "VVFIN", "NN", "ADV"], "meter": "-+-+-+-+-++-", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Und das Graunthal der Verwesung", "tokens": ["Und", "das", "Graunt\u00b7hal", "der", "Ver\u00b7we\u00b7sung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die des Throns Ausspruch in den Abgrund st\u00fcrzt!", "tokens": ["Die", "des", "Throns", "Aus\u00b7spruch", "in", "den", "Ab\u00b7grund", "st\u00fcrzt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}