{"dta.poem.3786": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1892", "urn": "urn:nbn:de:kobv:b4-200905191524", "language": ["de:0.99"], "booktitle": "George, Stefan: Algabal. Paris u. a., 1892."}, "poem": {"stanza.1": {"line.1": {"text": "Wisch die zeichen ihrer hiebe", "tokens": ["Wisch", "die", "zei\u00b7chen", "ih\u00b7rer", "hie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PPOSAT", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kind erkoren von den Hulden", "tokens": ["Kind", "er\u00b7ko\u00b7ren", "von", "den", "Hul\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Zu der V\u00f6lker heil und liebe", "tokens": ["Zu", "der", "V\u00f6l\u00b7ker", "heil", "und", "lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJD", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Heimgekehrter sieger rotte", "tokens": ["Heim\u00b7ge\u00b7kehr\u00b7ter", "sie\u00b7ger", "rot\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Beugte sich vor deiner sch\u00f6ne", "tokens": ["Beug\u00b7te", "sich", "vor", "dei\u00b7ner", "sch\u00f6\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihrem jugendlichen gotte", "tokens": ["Ih\u00b7rem", "ju\u00b7gend\u00b7li\u00b7chen", "got\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jubelten die erdens\u00f6hne", "tokens": ["Ju\u00b7bel\u00b7ten", "die", "er\u00b7den\u00b7s\u00f6h\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Die der ehre dank erwiesen", "tokens": ["Die", "der", "eh\u00b7re", "dank", "er\u00b7wie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "VVFIN", "APPR", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Neben solchem hort zu wohnen", "tokens": ["Ne\u00b7ben", "sol\u00b7chem", "hort", "zu", "woh\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wenn du auf den jaspis-fliesen", "tokens": ["Wenn", "du", "auf", "den", "jas\u00b7pis\u00b7flie\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weihtest vor bekr\u00e4nzten thronen", "tokens": ["Weih\u00b7test", "vor", "be\u00b7kr\u00e4nz\u00b7ten", "thro\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "M\u00e4nner weinten frauen st\u00f6hnten", "tokens": ["M\u00e4n\u00b7ner", "wein\u00b7ten", "frau\u00b7en", "st\u00f6hn\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "VVINF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter deines tempels th\u00fcre", "tokens": ["Un\u00b7ter", "dei\u00b7nes", "tem\u00b7pels", "th\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.3": {"text": "Gl\u00fchend baten die geh\u00f6hnten", "tokens": ["Gl\u00fc\u00b7hend", "ba\u00b7ten", "die", "ge\u00b7h\u00f6hn\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Dass dein kleid ihr haar ber\u00fchre \u2014", "tokens": ["Dass", "dein", "kleid", "ihr", "haar", "be\u00b7r\u00fch\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Eh dein gr\u00f6sster ruhm ersterbe", "tokens": ["Eh", "dein", "gr\u00f6s\u00b7ster", "ruhm", "ers\u00b7ter\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "---+-+--", "measure": "unknown.measure.di"}, "line.2": {"text": "Schm\u00fccke dich im weissen bade", "tokens": ["Schm\u00fc\u00b7cke", "dich", "im", "weis\u00b7sen", "ba\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Dass er noch zum wettbewerbe", "tokens": ["Dass", "er", "noch", "zum", "wett\u00b7be\u00b7wer\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alle hermen vor sich lade", "tokens": ["Al\u00b7le", "her\u00b7men", "vor", "sich", "la\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "PRF", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}