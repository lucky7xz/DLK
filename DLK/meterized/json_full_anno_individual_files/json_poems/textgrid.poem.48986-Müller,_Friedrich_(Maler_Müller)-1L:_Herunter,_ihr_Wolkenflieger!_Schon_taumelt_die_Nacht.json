{"textgrid.poem.48986": {"metadata": {"author": {"name": "M\u00fcller, Friedrich (Maler M\u00fcller)", "birth": "N.A.", "death": "N.A."}, "title": "1L: Herunter, ihr Wolkenflieger! Schon taumelt die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herunter, ihr Wolkenflieger! Schon taumelt die Nacht", "tokens": ["Her\u00b7un\u00b7ter", ",", "ihr", "Wol\u00b7ken\u00b7flie\u00b7ger", "!", "Schon", "tau\u00b7melt", "die", "Nacht"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "ART", "NN"], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit den hellleuchtenden Gestirnen, ihren Kindern.", "tokens": ["Mit", "den", "hell\u00b7leuch\u00b7ten\u00b7den", "Ge\u00b7stir\u00b7nen", ",", "ih\u00b7ren", "Kin\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+++-+-+-+-+-", "measure": "unknown.measure.septa"}, "line.3": {"text": "Was eilt ihr auf purpurnem Tau?", "tokens": ["Was", "eilt", "ihr", "auf", "pur\u00b7pur\u00b7nem", "Tau", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herunter, herunter, die Lanz' ist geschliffen!", "tokens": ["Her\u00b7un\u00b7ter", ",", "her\u00b7un\u00b7ter", ",", "die", "Lanz'", "ist", "ge\u00b7schlif\u00b7fen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.5": {"text": "Blutschwelger, h\u00f6rt ihr nicht", "tokens": ["Blut\u00b7schwel\u00b7ger", ",", "h\u00f6rt", "ihr", "nicht"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "PPER", "PTKNEG"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Schlachth\u00f6rner Schall \u2013 Tal hinab?", "tokens": ["Der", "Schlacht\u00b7h\u00f6r\u00b7ner", "Schall", "\u2013", "Tal", "hin\u00b7ab", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "NN", "PTKVZ", "$."], "meter": "-+--++-+", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Herunter, herunter, das Schwert ist gezogen!", "tokens": ["Her\u00b7un\u00b7ter", ",", "her\u00b7un\u00b7ter", ",", "das", "Schwert", "ist", "ge\u00b7zo\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Ihr h\u00f6rt das W\u00fcrglied \u2013 Wonneton", "tokens": ["Ihr", "h\u00f6rt", "das", "W\u00fcrg\u00b7lied", "\u2013", "Won\u00b7ne\u00b7ton"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Eurem Ohr \u2013 das euch festliche Tische bereit'", "tokens": ["Eu\u00b7rem", "Ohr", "\u2013", "das", "euch", "fest\u00b7li\u00b7che", "Ti\u00b7sche", "be\u00b7reit'"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$(", "PRELS", "PPER", "ADJA", "NN", "VVFIN"], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Und den roten Purpurkelch f\u00fcllt.", "tokens": ["Und", "den", "ro\u00b7ten", "Pur\u00b7pur\u00b7kelch", "f\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.11": {"text": "Wir sind die am H\u00fcgel \u2013 schwarz", "tokens": ["Wir", "sind", "die", "am", "H\u00fc\u00b7gel", "\u2013", "schwarz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "APPRART", "NN", "$(", "ADJD"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.12": {"text": "Wie des Donners Fittig,", "tokens": ["Wie", "des", "Don\u00b7ners", "Fit\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.13": {"text": "Rot geflammten Blicks \u2013", "tokens": ["Rot", "ge\u00b7flamm\u00b7ten", "Blicks", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.14": {"text": "Zu mir, zu mir braust der Fl\u00fcgel Get\u00f6n!", "tokens": ["Zu", "mir", ",", "zu", "mir", "braust", "der", "Fl\u00fc\u00b7gel", "Ge\u00b7t\u00f6n", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "APPR", "PPER", "VVFIN", "ART", "NN", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Ihr mit Eisen gepanzerte,", "tokens": ["Ihr", "mit", "Ei\u00b7sen", "ge\u00b7pan\u00b7zer\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.16": {"text": "Verk\u00fcndigt ihr Wodans Rache?", "tokens": ["Ver\u00b7k\u00fcn\u00b7digt", "ihr", "Wo\u00b7dans", "Ra\u00b7che", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Wie griff er die Lose \u2013 wie t\u00f6nte", "tokens": ["Wie", "griff", "er", "die", "Lo\u00b7se", "\u2013", "wie", "t\u00f6n\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "ART", "NN", "$(", "PWAV", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.18": {"text": "Der Urne Klang ... Schwebet, entschwindet!", "tokens": ["Der", "Ur\u00b7ne", "Klang", "...", "Schwe\u00b7bet", ",", "ent\u00b7schwin\u00b7det", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "$(", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.19": {"text": "Die Morgensonne gl\u00fcht am H\u00fcgel herauf", "tokens": ["Die", "Mor\u00b7gen\u00b7son\u00b7ne", "gl\u00fcht", "am", "H\u00fc\u00b7gel", "her\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "PTKVZ"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Und r\u00f6tet eure Fl\u00fcgel.", "tokens": ["Und", "r\u00f6\u00b7tet", "eu\u00b7re", "Fl\u00fc\u00b7gel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Ha, Wonne, ha Wonne! Ergreift das Schwert,", "tokens": ["Ha", ",", "Won\u00b7ne", ",", "ha", "Won\u00b7ne", "!", "Er\u00b7greift", "das", "Schwert", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "NE", "NN", "$.", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.22": {"text": "Cherusker!", "tokens": ["Che\u00b7rus\u00b7ker", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "---", "measure": "unknown.measure.zero"}, "line.23": {"text": "Ergreif das Schwert,", "tokens": ["Er\u00b7greif", "das", "Schwert", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.24": {"text": "Abk\u00f6mmling von Mana:", "tokens": ["Ab\u00b7k\u00f6mm\u00b7ling", "von", "Ma\u00b7na", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NE", "$."], "meter": "+---+-", "measure": "dactylic.init"}, "line.25": {"text": "Seht die Sklaven des Varus,", "tokens": ["Seht", "die", "Skla\u00b7ven", "des", "Va\u00b7rus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NE", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.26": {"text": "Wie sie vom Adlerklang geschreckt", "tokens": ["Wie", "sie", "vom", "Ad\u00b7ler\u00b7klang", "ge\u00b7schreckt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Die dunklen T\u00e4lern durchziehen!", "tokens": ["Die", "dunk\u00b7len", "T\u00e4\u00b7lern", "durch\u00b7zie\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Auf, auf \u2013 unser ist Sieg!", "tokens": ["Auf", ",", "auf", "\u2013", "un\u00b7ser", "ist", "Sieg", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "APPR", "$(", "ADJD", "VAFIN", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.29": {"text": "Ha, Wonne \u2013 t\u00f6nt H\u00f6rner!", "tokens": ["Ha", ",", "Won\u00b7ne", "\u2013", "t\u00f6nt", "H\u00f6r\u00b7ner", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWAV", "$(", "VVFIN", "NE", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.30": {"text": "Tal hinab \u2013 Kriegsgesang!", "tokens": ["Tal", "hin\u00b7ab", "\u2013", "Kriegs\u00b7ge\u00b7sang", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$(", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.31": {"text": "Hinab \u2013 hinab! \u2013", "tokens": ["Hin\u00b7ab", "\u2013", "hin\u00b7ab", "!", "\u2013"], "token_info": ["word", "punct", "word", "punct", "punct"], "pos": ["ADV", "$(", "PTKVZ", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Sie stehn, sie stehn im Blute!", "tokens": ["Sie", "stehn", ",", "sie", "stehn", "im", "Blu\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Singet, ihr M\u00e4dchen!", "tokens": ["Sin\u00b7get", ",", "ihr", "M\u00e4d\u00b7chen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.34": {"text": "Sie stehn im Blute, eure J\u00fcnglinge,", "tokens": ["Sie", "stehn", "im", "Blu\u00b7te", ",", "eu\u00b7re", "J\u00fcng\u00b7lin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-++-", "measure": "unknown.measure.penta"}, "line.35": {"text": "Holde Br\u00e4ute! \u2013", "tokens": ["Hol\u00b7de", "Br\u00e4u\u00b7te", "!", "\u2013"], "token_info": ["word", "word", "punct", "punct"], "pos": ["ADJA", "NN", "$.", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.36": {"text": "Pfl\u00fccket die Blumen und schm\u00fccket", "tokens": ["Pfl\u00fc\u00b7cket", "die", "Blu\u00b7men", "und", "schm\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.37": {"text": "Den Fu\u00df zum Tanz!", "tokens": ["Den", "Fu\u00df", "zum", "Tanz", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Sie fliehen, sie fliehen! Schon sinkt ein Turm!", "tokens": ["Sie", "flie\u00b7hen", ",", "sie", "flie\u00b7hen", "!", "Schon", "sinkt", "ein", "Turm", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF", "$.", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.39": {"text": "J\u00fcnglinge nach! Nach, Cherusker,", "tokens": ["J\u00fcng\u00b7lin\u00b7ge", "nach", "!", "Nach", ",", "Che\u00b7rus\u00b7ker", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "APPR", "$,", "NN", "$,"], "meter": "++-++-+-", "measure": "unknown.measure.penta"}, "line.40": {"text": "Bructerer \u2013 nach Ihr, Sonne der Edlen!", "tokens": ["Bruc\u00b7te\u00b7rer", "\u2013", "nach", "Ihr", ",", "Son\u00b7ne", "der", "Ed\u00b7len", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$(", "APPR", "PPER", "$,", "NN", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.41": {"text": "Sie fliehen, sie fliehen und schreien.", "tokens": ["Sie", "flie\u00b7hen", ",", "sie", "flie\u00b7hen", "und", "schrei\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PPER", "VVINF", "KON", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.42": {"text": "Wodan, Wodan brauset hinter ihnen her.", "tokens": ["Wo\u00b7dan", ",", "Wo\u00b7dan", "brau\u00b7set", "hin\u00b7ter", "ih\u00b7nen", "her", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "