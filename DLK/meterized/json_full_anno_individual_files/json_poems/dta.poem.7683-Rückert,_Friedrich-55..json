{"dta.poem.7683": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "55.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Vogel, der wie sonst sein Abendlied mir bringt,", "tokens": ["Der", "Vo\u00b7gel", ",", "der", "wie", "sonst", "sein", "A\u00b7ben\u00b7dlied", "mir", "bringt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "KOKOM", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O wie so eigen heut es mir zu Herzen klingt.", "tokens": ["O", "wie", "so", "ei\u00b7gen", "heut", "es", "mir", "zu", "Her\u00b7zen", "klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADV", "ADJD", "ADV", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ist es? er hat heut nicht einen von den Tagen,", "tokens": ["Was", "ist", "es", "?", "er", "hat", "heut", "nicht", "ei\u00b7nen", "von", "den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "ART", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den letzten Sommertag hat er zu Grab zu tragen.", "tokens": ["Den", "letz\u00b7ten", "Som\u00b7mer\u00b7tag", "hat", "er", "zu", "Grab", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die gute Nacht ist, die mir bietet sein Gesang,", "tokens": ["Die", "gu\u00b7te", "Nacht", "ist", ",", "die", "mir", "bie\u00b7tet", "sein", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auf keine kurze Nacht, auf einen Winter lang.", "tokens": ["Auf", "kei\u00b7ne", "kur\u00b7ze", "Nacht", ",", "auf", "ei\u00b7nen", "Win\u00b7ter", "lang", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}