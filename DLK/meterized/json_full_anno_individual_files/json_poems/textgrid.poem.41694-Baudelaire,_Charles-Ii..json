{"textgrid.poem.41694": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir ahmen Kreisel nach und Ball in ihrem Schwirren", "tokens": ["Wir", "ah\u00b7men", "Krei\u00b7sel", "nach", "und", "Ball", "in", "ih\u00b7rem", "Schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "KON", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihrem blinden Tanz; denn selbst im Schlummer nagt", "tokens": ["Und", "ih\u00b7rem", "blin\u00b7den", "Tanz", ";", "denn", "selbst", "im", "Schlum\u00b7mer", "nagt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$.", "KON", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Neugier uns das Herz und l\u00e4\u00dft uns weiter irren,", "tokens": ["Die", "Neu\u00b7gier", "uns", "das", "Herz", "und", "l\u00e4\u00dft", "uns", "wei\u00b7ter", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ART", "NN", "KON", "VVFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Grausamem Engel gleich, der Sonnen peitscht und jagt.", "tokens": ["Grau\u00b7sa\u00b7mem", "En\u00b7gel", "gleich", ",", "der", "Son\u00b7nen", "peitscht", "und", "jagt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "O sonderbares Gl\u00fcck, das stets verschiebt die Ziele,", "tokens": ["O", "son\u00b7der\u00b7ba\u00b7res", "Gl\u00fcck", ",", "das", "stets", "ver\u00b7schiebt", "die", "Zie\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PRELS", "ADV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das, weil es nirgends ist, uns \u00fcberall erscheint!", "tokens": ["Das", ",", "weil", "es", "nir\u00b7gends", "ist", ",", "uns", "\u00fc\u00b7be\u00b7rall", "er\u00b7scheint", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADV", "VAFIN", "$,", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So da\u00df der Mensch, der nie satt wird am tollen Spiele,", "tokens": ["So", "da\u00df", "der", "Mensch", ",", "der", "nie", "satt", "wird", "am", "tol\u00b7len", "Spie\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In ruhelosem Lauf Ruhm zu erjagen meint!", "tokens": ["In", "ru\u00b7he\u00b7lo\u00b7sem", "Lauf", "Ruhm", "zu", "er\u00b7ja\u00b7gen", "meint", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ein Fahrzeug ist der Geist, das dreigemastet steuert", "tokens": ["Ein", "Fahr\u00b7zeug", "ist", "der", "Geist", ",", "das", "drei\u00b7ge\u00b7mas\u00b7tet", "steu\u00b7ert"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Lande seines Gl\u00fccks. \u2013 Schau auf! t\u00f6nt's l\u00e4ngs dem Schiff;", "tokens": ["Zum", "Lan\u00b7de", "sei\u00b7nes", "Gl\u00fccks", ".", "\u2013", "Schau", "auf", "!", "t\u00f6nt's", "l\u00e4ngs", "dem", "Schiff", ";"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "$.", "$(", "NN", "PTKVZ", "$.", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-++-+--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "Vom Mastkorb hallt ein Ruf, von Wahnsinn angefeuert:", "tokens": ["Vom", "Mast\u00b7korb", "hallt", "ein", "Ruf", ",", "von", "Wahn\u00b7sinn", "an\u00b7ge\u00b7feu\u00b7ert", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Gl\u00fcck ... Liebe ... Ruhm! O Fluch! Er ist ein Felsenriff.", "tokens": ["Gl\u00fcck", "...", "Lie\u00b7be", "...", "Ruhm", "!", "O", "Fluch", "!", "Er", "ist", "ein", "Fel\u00b7sen\u00b7riff", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NN", "$(", "NN", "$.", "NE", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ein jedes Eiland, das der Mann auf Wache k\u00fcndet,", "tokens": ["Ein", "je\u00b7des", "Ei\u00b7land", ",", "das", "der", "Mann", "auf", "Wa\u00b7che", "k\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "PRELS", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erscheint ein Eden uns, das das Geschick verhie\u00df,", "tokens": ["Er\u00b7scheint", "ein", "E\u00b7den", "uns", ",", "das", "das", "Ge\u00b7schick", "ver\u00b7hie\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und unsre Phantasie schaut dort ihr Reich begr\u00fcndet,", "tokens": ["Und", "uns\u00b7re", "Phan\u00b7ta\u00b7sie", "schaut", "dort", "ihr", "Reich", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Bis eine Klippe nur im Morgengraun sich wies.", "tokens": ["Bis", "ei\u00b7ne", "Klip\u00b7pe", "nur", "im", "Mor\u00b7gen\u00b7graun", "sich", "wies", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "APPRART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ihn, dessen W\u00fcnsche nur ertr\u00e4umten Landen gelten,", "tokens": ["Ihn", ",", "des\u00b7sen", "W\u00fcn\u00b7sche", "nur", "er\u00b7tr\u00e4um\u00b7ten", "Lan\u00b7den", "gel\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELAT", "NN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sprecht, soll man fesseln ihn, ihn werfen in die See?", "tokens": ["Sprecht", ",", "soll", "man", "fes\u00b7seln", "ihn", ",", "ihn", "wer\u00b7fen", "in", "die", "See", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VMFIN", "PIS", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den trunknen Seemann, den Entdecker neuer Welten,", "tokens": ["Den", "trunk\u00b7nen", "See\u00b7mann", ",", "den", "Ent\u00b7de\u00b7cker", "neu\u00b7er", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die spiegelnd in der Flut versch\u00e4rfen unser Weh?", "tokens": ["Die", "spie\u00b7gelnd", "in", "der", "Flut", "ver\u00b7sch\u00e4r\u00b7fen", "un\u00b7ser", "Weh", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Gleichwie ein Vagabund durch Schmutz und Dunkel hinkend,", "tokens": ["Gleich\u00b7wie", "ein", "Va\u00b7ga\u00b7bund", "durch", "Schmutz", "und", "Dun\u00b7kel", "hin\u00b7kend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Nase in der Luft, sich Paradiese malt;", "tokens": ["Die", "Na\u00b7se", "in", "der", "Luft", ",", "sich", "Pa\u00b7ra\u00b7die\u00b7se", "malt", ";