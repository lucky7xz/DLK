{"textgrid.poem.53274": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dv siehest Mensch wie fort vnd fort", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dv siehest Mensch wie fort vnd fort", "tokens": ["Dv", "sie\u00b7hest", "Mensch", "wie", "fort", "vnd", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "KOKOM", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine hie, der andre dort", "tokens": ["Der", "ei\u00b7ne", "hie", ",", "der", "and\u00b7re", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "$,", "PRELS", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vns gute Nacht mu\u00df geben:", "tokens": ["Vns", "gu\u00b7te", "Nacht", "mu\u00df", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Todt helt keinen andern Lauff,", "tokens": ["Der", "Todt", "helt", "kei\u00b7nen", "an\u00b7dern", "Lauff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er sagt zuletzt die Wohnung auff", "tokens": ["Er", "sagt", "zu\u00b7letzt", "die", "Woh\u00b7nung", "auff"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vns allen, die wir leben.", "tokens": ["Vns", "al\u00b7len", ",", "die", "wir", "le\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "$,", "PRELS", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Bedenck' es weislich, in der Zeit,", "tokens": ["Be\u00b7denck", "es", "weis\u00b7lich", ",", "in", "der", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd fleuch den Schlaff der Sicherheit,", "tokens": ["Vnd", "fleuch", "den", "Schlaff", "der", "Si\u00b7cher\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sey augenblicklich wacker!", "tokens": ["Sey", "au\u00b7gen\u00b7blick\u00b7lich", "wa\u00b7cker", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn, wiss', es bleibet dabey nicht,", "tokens": ["Denn", ",", "wiss'", ",", "es", "blei\u00b7bet", "da\u00b7bey", "nicht", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "$,", "PPER", "VVFIN", "PAV", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df man dich hin au\u00df diesem Liecht", "tokens": ["Da\u00df", "man", "dich", "hin", "au\u00df", "die\u00b7sem", "Liecht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Tr\u00e4gt auff den Gottes-Acker.", "tokens": ["Tr\u00e4gt", "auff", "den", "Got\u00b7tes\u00b7A\u00b7cker", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Wir werden au\u00df den Gr\u00e4bern gehn", "tokens": ["Wir", "wer\u00b7den", "au\u00df", "den", "Gr\u00e4\u00b7bern", "gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd alle vor der Bancke stehn,", "tokens": ["Vnd", "al\u00b7le", "vor", "der", "Ban\u00b7cke", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Christus selbst wird hegen,", "tokens": ["Die", "Chris\u00b7tus", "selbst", "wird", "he\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADV", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wenn auff der Engel Feld-geschrey", "tokens": ["Wenn", "auff", "der", "En\u00b7gel", "Feld\u00b7ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Glut das grosse Welt-Gebew", "tokens": ["Die", "Glut", "das", "gros\u00b7se", "Welt\u00b7Ge\u00b7bew"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wird in die Asche legen.", "tokens": ["Wird", "in", "die", "A\u00b7sche", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Alsdenn wird erstlich aller Welt", "tokens": ["Als\u00b7denn", "wird", "erst\u00b7lich", "al\u00b7ler", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Belohnung werden zugestellt:", "tokens": ["Be\u00b7loh\u00b7nung", "wer\u00b7den", "zu\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die S\u00fcnder sollen b\u00fcssen,", "tokens": ["Die", "S\u00fcn\u00b7der", "sol\u00b7len", "b\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd Ihnen ohn Betrug vnd Schein", "tokens": ["Vnd", "Ih\u00b7nen", "ohn", "Be\u00b7trug", "vnd", "Schein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Selbst Kl\u00e4ger, Richter, Hencker seyn,", "tokens": ["Selbst", "Kl\u00e4\u00b7ger", ",", "Rich\u00b7ter", ",", "Hen\u00b7cker", "seyn", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "NN", "$,", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verdampt durch jhr Gewissen.", "tokens": ["Ver\u00b7dampt", "durch", "jhr", "Ge\u00b7wis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ach Gott! k\u00f6mpt mir die\u00df Vrtheil vor,", "tokens": ["Ach", "Gott", "!", "k\u00f6mpt", "mir", "die\u00df", "Vrtheil", "vor", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "VVFIN", "PPER", "PDS", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "So steigen mir die Haar empor,", "tokens": ["So", "stei\u00b7gen", "mir", "die", "Haar", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein Hertz f\u00fchlt Angst vnd Schrecken!", "tokens": ["Mein", "Hertz", "f\u00fchlt", "Angst", "vnd", "Schre\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ihr hohen H\u00fcgel, heb' ich an,", "tokens": ["Ihr", "ho\u00b7hen", "H\u00fc\u00b7gel", ",", "heb'", "ich", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr Berg', vnd was sich st\u00fcrzen kan,", "tokens": ["Ihr", "Ber\u00b7g'", ",", "vnd", "was", "sich", "st\u00fcr\u00b7zen", "kan", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "KON", "PWS", "PRF", "VVINF", "VMFIN", "$,"], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Fallt her, mich zu bedecken!", "tokens": ["Fallt", "her", ",", "mich", "zu", "be\u00b7de\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Herr Jesu, meine Zuversicht,", "tokens": ["Herr", "Je\u00b7su", ",", "mei\u00b7ne", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ach lass dein strenges Zorn-Gericht,", "tokens": ["Ach", "lass", "dein", "stren\u00b7ges", "Zorn\u00b7Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach lass es mir nicht schaden!", "tokens": ["Ach", "lass", "es", "mir", "nicht", "scha\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beut' an dem Vater den Vertrag,", "tokens": ["Beut'", "an", "dem", "Va\u00b7ter", "den", "Ver\u00b7trag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Damit ich k\u00fcnfftig h\u00f6ren mag", "tokens": ["Da\u00b7mit", "ich", "k\u00fcnff\u00b7tig", "h\u00f6\u00b7ren", "mag"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den s\u00fcssen Spruch der Gnaden!", "tokens": ["Den", "s\u00fcs\u00b7sen", "Spruch", "der", "Gna\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Gib da\u00df ich mich bey gutem Sinn", "tokens": ["Gib", "da\u00df", "ich", "mich", "bey", "gu\u00b7tem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vnd weil ich noch bey Kr\u00e4fften bin", "tokens": ["Vnd", "weil", "ich", "noch", "bey", "Kr\u00e4ff\u00b7ten", "bin"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum sterben fertig halte,", "tokens": ["Zum", "ster\u00b7ben", "fer\u00b7tig", "hal\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "VVFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Vnd nicht, o Jesu, meine Lust,", "tokens": ["Vnd", "nicht", ",", "o", "Je\u00b7su", ",", "mei\u00b7ne", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Begriffen in der S\u00fcnden Wust", "tokens": ["Be\u00b7grif\u00b7fen", "in", "der", "S\u00fcn\u00b7den", "Wust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Zum ewign Todt erkalte!", "tokens": ["Zum", "e\u00b7wign", "Todt", "er\u00b7kal\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Dv siehest Mensch wie fort vnd fort", "tokens": ["Dv", "sie\u00b7hest", "Mensch", "wie", "fort", "vnd", "fort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "KOKOM", "PTKVZ", "KON", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der eine hie, der andre dort", "tokens": ["Der", "ei\u00b7ne", "hie", ",", "der", "and\u00b7re", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ART", "ADV", "$,", "PRELS", "PIS", "ADV"], "meter": "-+-+-+-+", "measure": "iam