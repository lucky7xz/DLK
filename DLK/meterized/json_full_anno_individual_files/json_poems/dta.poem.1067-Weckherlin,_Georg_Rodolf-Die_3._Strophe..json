{"dta.poem.1067": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 3. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.71", "id:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fang ich dan an ein lobgesang", "tokens": ["Fang", "ich", "dan", "an", "ein", "lob\u00b7ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Von deinem blut auf meinen saiten/", "tokens": ["Von", "dei\u00b7nem", "blut", "auf", "mei\u00b7nen", "sai\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Will die Tugent/ das ich anfang", "tokens": ["Will", "die", "Tu\u00b7gent", "/", "das", "ich", "an\u00b7fang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ART", "NN", "$(", "PRELS", "PPER", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "An jhr dein lob recht au\u00dfzuspraiten;", "tokens": ["An", "jhr", "dein", "lob", "recht", "au\u00df\u00b7zu\u00b7sprai\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PPOSAT", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Als welche allein ohn betrug", "tokens": ["Als", "wel\u00b7che", "al\u00b7lein", "ohn", "be\u00b7trug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PRELS", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die sterbliche mit gotheit zieret/", "tokens": ["Die", "sterb\u00b7li\u00b7che", "mit", "got\u00b7heit", "zie\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Vnd sie mit schnell-geradem flug", "tokens": ["Vnd", "sie", "mit", "schnell\u00b7ge\u00b7ra\u00b7dem", "flug"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In der G\u00f6tter gemeinschaft f\u00fchret.", "tokens": ["In", "der", "G\u00f6t\u00b7ter", "ge\u00b7mein\u00b7schaft", "f\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.9": {"text": "Hercules durch jhren beystand", "tokens": ["Her\u00b7cu\u00b7les", "durch", "jhren", "beys\u00b7tand"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Er\u00f6fnete sein Vatterland;", "tokens": ["Er\u00b7\u00f6f\u00b7ne\u00b7te", "sein", "Vat\u00b7ter\u00b7land", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die zwilling auf dem M\u00f6hrgeehret", "tokens": ["Die", "zwil\u00b7ling", "auf", "dem", "M\u00f6hr\u00b7geeh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Haben durch sie die stern gemehret.", "tokens": ["Ha\u00b7ben", "durch", "sie", "die", "stern", "ge\u00b7meh\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ART", "ADJA", "VVPP", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}}}}