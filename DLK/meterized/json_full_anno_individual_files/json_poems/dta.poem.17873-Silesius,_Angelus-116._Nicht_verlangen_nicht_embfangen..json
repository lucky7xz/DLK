{"dta.poem.17873": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "116.  Nicht verlangen nicht embfangen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Wer GOttes angesicht hier nicht sieht mit begier/", "tokens": ["Wer", "Got\u00b7tes", "an\u00b7ge\u00b7sicht", "hier", "nicht", "sieht", "mit", "be\u00b7gier", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VVPP", "ADV", "PTKNEG", "VVFIN", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der komt in ewigkeit darnach nicht bey ihm f\u00fcr.", "tokens": ["Der", "komt", "in", "e\u00b7wig\u00b7keit", "dar\u00b7nach", "nicht", "bey", "ihm", "f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "PAV", "PTKNEG", "APPR", "PPER", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}