{"textgrid.poem.55219": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Krittler", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein unversch\u00e4mter Naseweis,", "tokens": ["Ein", "un\u00b7ver\u00b7sch\u00e4m\u00b7ter", "Na\u00b7se\u00b7weis", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der, was er durch Stahlarbeitersflei\u00df", "tokens": ["Der", ",", "was", "er", "durch", "Stahl\u00b7ar\u00b7bei\u00b7ters\u00b7flei\u00df"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWS", "PPER", "APPR", "NN"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Auf dem Laden k\u00fcnstlich liegen sah,", "tokens": ["Auf", "dem", "La\u00b7den", "k\u00fcnst\u00b7lich", "lie\u00b7gen", "sah", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dacht, es w\u00e4r f\u00fcr ihn alleine da:", "tokens": ["Dacht", ",", "es", "w\u00e4r", "f\u00fcr", "ihn", "al\u00b7lei\u00b7ne", "da", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "APPR", "PPER", "ADV", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "So tatscht' er dem geduldigen Mann", "tokens": ["So", "tatscht'", "er", "dem", "ge\u00b7dul\u00b7di\u00b7gen", "Mann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die blanken Waren s\u00e4mtlich an", "tokens": ["Die", "blan\u00b7ken", "Wa\u00b7ren", "s\u00e4mt\u00b7lich", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und sch\u00e4tzte sie nach D\u00fcnkelsrecht,", "tokens": ["Und", "sch\u00e4tz\u00b7te", "sie", "nach", "D\u00fcn\u00b7kels\u00b7recht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das Schlechte hoch, das Gute schlecht,", "tokens": ["Das", "Schlech\u00b7te", "hoch", ",", "das", "Gu\u00b7te", "schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Getrost, zufriednen Angesichts;", "tokens": ["Ge\u00b7trost", ",", "zu\u00b7fried\u00b7nen", "An\u00b7ge\u00b7sichts", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Dann ging er weg und kaufte nichts.", "tokens": ["Dann", "ging", "er", "weg", "und", "kauf\u00b7te", "nichts", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "PIS", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Den Kramer das zuletzt verdro\u00df,", "tokens": ["Den", "Kra\u00b7mer", "das", "zu\u00b7letzt", "ver\u00b7dro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und macht ein st\u00e4hlern k\u00fcnstlich Schlo\u00df", "tokens": ["Und", "macht", "ein", "st\u00e4h\u00b7lern", "k\u00fcnst\