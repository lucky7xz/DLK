{"textgrid.poem.61420": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Helle N\u00e4chte", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schweifst du noch immer dort oben,", "tokens": ["Schweifst", "du", "noch", "im\u00b7mer", "dort", "o\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADV", "ADV", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Du von den T\u00f6chtern des Himmels", "tokens": ["Du", "von", "den", "T\u00f6ch\u00b7tern", "des", "Him\u00b7mels"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Mir die freundlichste, Abendr\u00f6te?", "tokens": ["Mir", "die", "freund\u00b7lichs\u00b7te", ",", "A\u00b7bend\u00b7r\u00f6\u00b7te", "?"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "$,", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Oder naht schon von ferne", "tokens": ["O\u00b7der", "naht", "schon", "von", "fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Tagverk\u00fcndend", "tokens": ["Tag\u00b7ver\u00b7k\u00fcn\u00b7dend"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Die prangende Schwester,", "tokens": ["Die", "pran\u00b7gen\u00b7de", "Schwes\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Die mit den Rosenfingern", "to