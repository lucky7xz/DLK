{"dta.poem.21258": {"metadata": {"author": {"name": "Ramler, Karl Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Amynt und Chloe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1767", "urn": "urn:nbn:de:kobv:b4-20476-3", "language": ["de:0.99"], "booktitle": "Ramler, Karl Wilhelm: Oden. Berlin, 1767."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bins, o Chloe! fleuch nicht mit\nnacketem Fuss", "tokens": ["Ich", "bins", ",", "o", "Chloe", "!", "fleuch", "nicht", "mit", "na\u00b7cke\u00b7tem", "Fuss"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "$,", "FM", "NE", "$.", "VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Durch diese Dornen! fleuch nicht den", "tokens": ["Durch", "die\u00b7se", "Dor\u00b7nen", "!", "fleuch", "nicht", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$.", "VVFIN", "PTKNEG", "ART"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Hier ist dein Kranz, hier ist dein G\u00fcrtel!", "tokens": ["Hier", "ist", "dein", "Kranz", ",", "hier", "ist", "dein", "G\u00fcr\u00b7tel", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$,", "ADV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm, bade sicher, ich st\u00f6re dich", "tokens": ["Komm", ",", "ba\u00b7de", "si\u00b7cher", ",", "ich", "st\u00f6\u00b7re", "dich"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "ADJD", "$,", "PPER", "VVFIN", "PPER"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Sieh her! ich eile zur\u00fcck, und h\u00e4nge", "tokens": ["Sieh", "her", "!", "ich", "ei\u00b7le", "zu\u00b7r\u00fcck", ",", "und", "h\u00e4n\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "An diesen Weydenbaum auf. \u2012 \u2012 Ach!", "tokens": ["An", "die\u00b7sen", "Wey\u00b7den\u00b7baum", "auf", ".", "\u2012", "\u2012", "Ach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKVZ", "$.", "$(", "$(", "ITJ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Es folgt dir ja kein wilder Satyr,", "tokens": ["Es", "folgt", "dir", "ja", "kein", "wil\u00b7der", "Sa\u00b7tyr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.8": {"text": "Kein ungez\u00e4hmter Cyklope dir nach. \u2014", "tokens": ["Kein", "un\u00b7ge\u00b7z\u00e4hm\u00b7ter", "Cyk\u00b7lo\u00b7pe", "dir", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Dich, schlankes, fl\u00fcchtiges Reh, dich hab\u2019", "tokens": ["Dich", ",", "schlan\u00b7kes", ",", "fl\u00fcch\u00b7ti\u00b7ges", "Reh", ",", "dich", "hab'"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "$,", "ADJA", "$,", "ADJA", "NN", "$,", "PPER", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "Nun widerstrebe nicht mehr! Nimm G\u00fcrtel", "tokens": ["Nun", "wi\u00b7der\u00b7stre\u00b7be", "nicht", "mehr", "!", "Nimm", "G\u00fcr\u00b7tel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PTKNEG", "ADV", "$.", "NE", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und weihe sie der strengen G\u00f6ttinn,", "tokens": ["Und", "wei\u00b7he", "sie", "der", "stren\u00b7gen", "G\u00f6t\u00b7tinn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "An deren \u00f6dem Altare du dienst.", "tokens": ["An", "de\u00b7ren", "\u00f6\u00b7dem", "Al\u00b7ta\u00b7re", "du", "dienst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELAT", "ADJA", "NN", "PPER", "ADV", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}}}}