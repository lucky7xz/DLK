h gefallen,", "tokens": ["In", "Schuld", "und", "Schmach", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bin elend grenzenlos;", "tokens": ["Bin", "e\u00b7lend", "gren\u00b7zen\u00b7los", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich bete: \u2013 weh mir! \u2013 mache,", "tokens": ["Ich", "be\u00b7te", ":", "\u2013", "weh", "mir", "!", "\u2013", "ma\u00b7che", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "VVIMP", "PPER", "$.", "$(", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Aus Mitleid oder Rache,", "tokens": ["Aus", "Mit\u00b7leid", "o\u00b7der", "Ra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Unfruchtbar meinen Scho\u00df.", "tokens": ["Un\u00b7frucht\u00b7bar", "mei\u00b7nen", "Scho\u00df", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Die, deren Scho\u00df geboren,", "tokens": ["Die", ",", "de\u00b7ren", "Scho\u00df", "ge\u00b7bo\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PRELAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Wonn und Lust verloren,", "tokens": ["In", "Wonn", "und", "Lust", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.