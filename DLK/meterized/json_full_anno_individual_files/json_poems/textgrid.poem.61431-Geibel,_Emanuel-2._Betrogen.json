{"textgrid.poem.61431": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "2. Betrogen", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf Fl\u00fcgeln saust der Wind daher,", "tokens": ["Auf", "Fl\u00fc\u00b7geln", "saust", "der", "Wind", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Es rinnen und rauschen die Quellen.", "tokens": ["Es", "rin\u00b7nen", "und", "rau\u00b7schen", "die", "Quel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Du hast mich geliebt, doch du liebst mich nicht mehr", "tokens": ["Du", "hast", "mich", "ge\u00b7liebt", ",", "doch", "du", "liebst", "mich", "nicht", "mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VVPP", "$,", "KON", "PPER", "VVFIN", "PPER", "PTKNEG", "ADV"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Und \u00e4ugelst nach andern Gesellen.", "tokens": ["Und", "\u00e4u\u00b7gelst", "nach", "an\u00b7dern", "Ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Was soll mir dein schwankender, wankender Sinn?", "tokens": ["Was", "soll", "mir", "dein", "schwan\u00b7ken\u00b7der", ",", "wan\u00b7ken\u00b7der", "Sinn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Fahr hin, fahr hin,", "tokens": ["Fahr", "hin", ",", "fahr", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Fahr hin mit den Winden und Wellen!", "tokens": ["Fahr", "hin", "mit", "den", "Win\u00b7den", "und", "Wel\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "KON", "NN", "$."], "meter": "----+--+-", "measure": "iambic.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Ach, was ist so flatternd als Weibertreu!", "tokens": ["Ach", ",", "was", "ist", "so", "flat\u00b7ternd", "als", "Wei\u00b7ber\u00b7treu", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Du kannst sie nicht halten noch binden.", "tokens": ["Du", "kannst", "sie", "nicht", "hal\u00b7ten", "noch", "bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ach, was ist so bitter als Liebesreu,", "tokens": ["Ach", ",", "was", "ist", "so", "bit\u00b7ter", "als", "Lie\u00b7bes\u00b7reu", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wenn die goldenen Schl\u00f6sser verschwinden!", "tokens": ["Wenn", "die", "gol\u00b7de\u00b7nen", "Schl\u00f6s\u00b7s