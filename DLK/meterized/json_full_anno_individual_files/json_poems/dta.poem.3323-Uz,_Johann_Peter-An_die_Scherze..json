{"dta.poem.3323": {"metadata": {"author": {"name": "Uz, Johann Peter", "birth": "N.A.", "death": "N.A."}, "title": "An die Scherze.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1755", "urn": "urn:nbn:de:kobv:b4-20614-6", "language": ["de:0.99"], "booktitle": "Uz, Johann Peter: Lyrische und andere Gedichte. 2. Aufl. Ansbach, 1755."}, "poem": {"stanza.1": {"line.1": {"text": "Wo seyd ihr hin, ihr schlauen Scherze?", "tokens": ["Wo", "seyd", "ihr", "hin", ",", "ihr", "schlau\u00b7en", "Scher\u00b7ze", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vermi\u00df ich euch mit fr\u00fchem Schmerze,", "tokens": ["Ver\u00b7mi\u00df", "ich", "euch", "mit", "fr\u00fc\u00b7hem", "Schmer\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Noch ehe mich die Jugend flieht?", "tokens": ["Noch", "e\u00b7he", "mich", "die", "Ju\u00b7gend", "flieht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die ihr muthwillig um mich schwebtet,", "tokens": ["Die", "ihr", "mut\u00b7hwil\u00b7lig", "um", "mich", "schweb\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und oft mein leichtgefl\u00fcgelt Lied", "tokens": ["Und", "oft", "mein", "leicht\u00b7ge\u00b7fl\u00fc\u00b7gelt", "Lied"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PPOSAT", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit schalkhaftmunterm Witz belebtet!", "tokens": ["Mit", "schalk\u00b7haft\u00b7mun\u00b7term", "Witz", "be\u00b7leb\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht hier die vollen Gl\u00e4ser blinken!", "tokens": ["Seht", "hier", "die", "vol\u00b7len", "Gl\u00e4\u00b7ser", "blin\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie? meine Muse sieht mich trinken,", "tokens": ["Wie", "?", "mei\u00b7ne", "Mu\u00b7se", "sieht", "mich", "trin\u00b7ken", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "PPOSAT", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schlummert unermuntert ein?", "tokens": ["Und", "schlum\u00b7mert", "un\u00b7er\u00b7mun\u00b7tert", "ein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Winkt Bacchus euerm stolzen Schwarme", "tokens": ["Winkt", "Bac\u00b7chus", "eu\u00b7erm", "stol\u00b7zen", "Schwar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NE", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Umsonst mit feuervollem Wein", "tokens": ["Um\u00b7sonst", "mit", "feu\u00b7er\u00b7vol\u00b7lem", "Wein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und in der Freundschaft holdem Arme?", "tokens": ["Und", "in", "der", "Freund\u00b7schaft", "hol\u00b7dem", "Ar\u00b7me", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Umsonst! wenn Amor euch verlanget,", "tokens": ["Um\u00b7sonst", "!", "wenn", "A\u00b7mor", "euch", "ver\u00b7lan\u00b7get", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "KOUS", "NE", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der immer an Cytheren hanget!", "tokens": ["Der", "im\u00b7mer", "an", "Cy\u00b7the\u00b7ren", "han\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Seyd ihr auf ieden Wink bereit:", "tokens": ["Seyd", "ihr", "auf", "ie\u00b7den", "Wink", "be\u00b7reit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "PPER", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und alle Grazien begleiten", "tokens": ["Und", "al\u00b7le", "Gra\u00b7zi\u00b7en", "be\u00b7glei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Den Gott begl\u00fcckter Z\u00e4rtlichkeit,", "tokens": ["Den", "Gott", "be\u00b7gl\u00fcck\u00b7ter", "Z\u00e4rt\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Freude flattert ihm zur Seiten.", "tokens": ["Und", "Freu\u00b7de", "flat\u00b7tert", "ihm", "zur", "Sei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bey mit wird iede Mufe wilde:", "tokens": ["Bey", "mit", "wird", "ie\u00b7de", "Mu\u00b7fe", "wil\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "VAFIN", "PIAT", "NN", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wir irren einsam durch Gefilde,", "tokens": ["Wir", "ir\u00b7ren", "ein\u00b7sam", "durch", "Ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Durch W\u00e4lder, die der Herbst entlaubt;", "tokens": ["Durch", "W\u00e4l\u00b7der", ",", "die", "der", "Herbst", "ent\u00b7laubt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und scheinen, wenn durch \u00f6de Gr\u00fcnde", "tokens": ["Und", "schei\u00b7nen", ",", "wenn", "durch", "\u00f6\u00b7de", "Gr\u00fcn\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der greise Nord verheerend schnaubt,", "tokens": ["Der", "grei\u00b7se", "Nord", "ver\u00b7hee\u00b7rend", "schnaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Noch rauher, als die rauhen Winde.", "tokens": ["Noch", "rau\u00b7her", ",", "als", "die", "rau\u00b7hen", "Win\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da preis' ich ruhiges Ergetzen:", "tokens": ["Da", "preis'", "ich", "ru\u00b7hi\u00b7ges", "Er\u00b7get\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+--+-+--", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Kein Wunsch nach aufgeh\u00e4uften Sch\u00e4tzen", "tokens": ["Kein", "Wunsch", "nach", "auf\u00b7ge\u00b7h\u00e4uf\u00b7ten", "Sch\u00e4t\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erm\u00fcde, sing ich, meine Nacht!", "tokens": ["Er\u00b7m\u00fc\u00b7de", ",", "sing", "ich", ",", "mei\u00b7ne", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein freyes Herz trotz\u2019 unbesieget", "tokens": ["Mein", "frey\u00b7es", "Herz", "trotz'", "un\u00b7be\u00b7sie\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dem Ehrgeiz, der nur Sklaven macht,", "tokens": ["Dem", "Ehr\u00b7geiz", ",", "der", "nur", "Skla\u00b7ven", "macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und seine Sklaven stets betr\u00fcget!", "tokens": ["Und", "sei\u00b7ne", "Skla\u00b7ven", "stets", "be\u00b7tr\u00fc\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "O m\u00f6chte zwischen Wald und Str\u00e4uchen", "tokens": ["O", "m\u00f6ch\u00b7te", "zwi\u00b7schen", "Wald", "und", "Str\u00e4u\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Leben still vor\u00fcber schleichen,", "tokens": ["Mein", "Le\u00b7ben", "still", "vor\u00b7\u00fc\u00b7ber", "schlei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie jener Bach geruhig fleusst!", "tokens": ["Wie", "je\u00b7ner", "Bach", "ge\u00b7ru\u00b7hig", "fleusst", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo in den Th\u00e4lern, in den Triften", "tokens": ["Wo", "in", "den", "Th\u00e4\u00b7lern", ",", "in", "den", "Trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Sich seine milde Fluth ergeusst,", "tokens": ["Sich", "sei\u00b7ne", "mil\u00b7de", "Fluth", "er\u00b7geusst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Lacht fetter Klee und Bluhmen d\u00fcften.", "tokens": ["Lacht", "fet\u00b7ter", "Klee", "und", "Bluh\u00b7men", "d\u00fcf\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Verfliesst, ihr Tage meines Lebens,", "tokens": ["Ver\u00b7fliesst", ",", "ihr", "Ta\u00b7ge", "mei\u00b7nes", "Le\u00b7bens", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zwar unbemerkt, nur nicht vergebens", "tokens": ["Zwar", "un\u00b7be\u00b7merkt", ",", "nur", "nicht", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "$,", "ADV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "F\u00fcr meiner Mitgesch\u00f6pfe Gl\u00fcck!", "tokens": ["F\u00fcr", "mei\u00b7ner", "Mit\u00b7ge\u00b7sch\u00f6p\u00b7fe", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So mag von mir die Nachwelt schweigen!", "tokens": ["So", "mag", "von", "mir", "die", "Nach\u00b7welt", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "APPR", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "So sey ein gl\u00e4nzendes Geschick", "tokens": ["So", "sey", "ein", "gl\u00e4n\u00b7zen\u00b7des", "Ge\u00b7schick"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dem gl\u00fccklichk\u00fchnen Laster eigen!", "tokens": ["Dem", "gl\u00fcck\u00b7lich\u00b7k\u00fch\u00b7nen", "Las\u00b7ter", "ei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}