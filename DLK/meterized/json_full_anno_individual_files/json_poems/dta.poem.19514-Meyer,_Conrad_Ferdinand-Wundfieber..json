{"dta.poem.19514": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Wundfieber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eberggeist, ich h\u00f6re deine Str\u00f6me rauschen \u2014", "tokens": ["\u201e", "berg\u00b7geist", ",", "ich", "h\u00f6\u00b7re", "dei\u00b7ne", "Str\u00f6\u00b7me", "rau\u00b7schen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gieb mir Geh\u00f6r! Wir wollen Rede tauschen!", "tokens": ["Gieb", "mir", "Ge\u00b7h\u00f6r", "!", "Wir", "wol\u00b7len", "Re\u00b7de", "tau\u00b7schen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "NN", "$.", "PPER", "VMFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du von der Firne mondenhellen H\u00e4ngen,", "tokens": ["Du", "von", "der", "Fir\u00b7ne", "mon\u00b7den\u00b7hel\u00b7len", "H\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ich aus der Krankenkammer schw\u00fclen Engen!", "tokens": ["Ich", "aus", "der", "Kran\u00b7ken\u00b7kam\u00b7mer", "schw\u00fc\u00b7len", "En\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Denn wisse, Geist, ich liege hier gefangen", "tokens": ["Denn", "wis\u00b7se", ",", "Geist", ",", "ich", "lie\u00b7ge", "hier", "ge\u00b7fan\u00b7gen"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und lasse den geknickten Fl\u00fcgel hangen.", "tokens": ["Und", "las\u00b7se", "den", "ge\u00b7knick\u00b7ten", "Fl\u00fc\u00b7gel", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ich \u00e4chz' und st\u00f6hne, den gel\u00e4hmten, wunden,", "tokens": ["Ich", "\u00e4ch\u00b7z'", "und", "st\u00f6h\u00b7ne", ",", "den", "ge\u00b7l\u00e4hm\u00b7ten", ",", "wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "Gebrochnen Arm dicht an den Leib gebunden.", "tokens": ["Ge\u00b7broch\u00b7nen", "Arm", "dicht", "an", "den", "Leib", "ge\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Zwei kurzer Wandertage s\u00fc\u00dfes Tr\u00e4umen \u2014", "tokens": ["Zwei", "kur\u00b7zer", "Wan\u00b7der\u00b7ta\u00b7ge", "s\u00fc\u00b7\u00dfes", "Tr\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "ADJA", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Und dich verdro\u00df ein Gast in deinen R\u00e4umen.", "tokens": ["Und", "dich", "ver\u00b7dro\u00df", "ein", "Gast", "in", "dei\u00b7nen", "R\u00e4u\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Vom Tische stie\u00dfest du den freud'gen Zecher,", "tokens": ["Vom", "Ti\u00b7sche", "stie\u00b7\u00dfest", "du", "den", "freu\u00b7d'\u00b7gen", "Ze\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Entrissest mir den eisgew\u00fcrzten Becher", "tokens": ["Ent\u00b7ris\u00b7sest", "mir", "den", "eis\u00b7ge\u00b7w\u00fcrz\u00b7ten", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Und rolltest mich hohnlachend durch die Kl\u00fcfte", "tokens": ["Und", "roll\u00b7test", "mich", "hohn\u00b7la\u00b7chend", "durch", "die", "Kl\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Hinunter in des Fieberlagers Gr\u00fcfte.", "tokens": ["Hin\u00b7un\u00b7ter", "in", "des", "Fie\u00b7ber\u00b7la\u00b7gers", "Gr\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Verr\u00e4ther, schm\u00e4hlich hast du mich betrogen!", "tokens": ["Ver\u00b7r\u00e4\u00b7ther", ",", "schm\u00e4h\u00b7lich", "hast", "du", "mich", "be\u00b7tro\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "VAFIN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Hast du mich leise rufend nicht gezogen?", "tokens": ["Hast", "du", "mich", "lei\u00b7se", "ru\u00b7fend", "nicht", "ge\u00b7zo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "ADJD", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Warst du mir lange Jahre nicht gewogen?", "tokens": ["Warst", "du", "mir", "lan\u00b7ge", "Jah\u00b7re", "nicht", "ge\u00b7wo\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und wann in deinem Reich ich mich verirrte,", "tokens": ["Und", "wann", "in", "dei\u00b7nem", "Reich", "ich", "mich", "ver\u00b7irr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Schritt nicht, wie Zufall, mir voran ein Hirte", "tokens": ["Schritt", "nicht", ",", "wie", "Zu\u00b7fall", ",", "mir", "vo\u00b7ran", "ein", "Hir\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "PWAV", "NN", "$,", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Und lie\u00df in seine sichern Stapfen treten", "tokens": ["Und", "lie\u00df", "in", "sei\u00b7ne", "si\u00b7chern", "Stap\u00b7fen", "tre\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.21": {"text": "Bergab mich \u2014 ungerufen, ungebeten?", "tokens": ["Berg\u00b7ab", "mich", "un\u00b7ge\u00b7ru\u00b7fen", ",", "un\u00b7ge\u00b7be\u00b7ten", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "ADJD", "$,", "ADJA", "$."], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.22": {"text": "Du bist mir gram geworden? La\u00df dich fragen!", "tokens": ["Du", "bist", "mir", "gram", "ge\u00b7wor\u00b7den", "?", "La\u00df", "dich", "fra\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$.", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Mu\u00df ich der f\u00fchrerlosen Fahrt entsagen?", "tokens": ["Mu\u00df", "ich", "der", "f\u00fch\u00b7rer\u00b7lo\u00b7sen", "Fahrt", "ent\u00b7sa\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Des hohen Irreganges mich entw\u00f6hnen?\u201c", "tokens": ["Des", "ho\u00b7hen", "Ir\u00b7re\u00b7gan\u00b7ges", "mich", "ent\u00b7w\u00f6h\u00b7nen", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Mir gab Bescheid der Geist mit tiefen T\u00f6nen", "tokens": ["Mir", "gab", "Be\u00b7scheid", "der", "Geist", "mit", "tie\u00b7fen", "T\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Im Flutensturz und in der Laue Dr\u00f6hnen,", "tokens": ["Im", "Flu\u00b7ten\u00b7sturz", "und", "in", "der", "Lau\u00b7e", "Dr\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Es klang wie Droh'n und wieder klang's wie H\u00f6hnen:", "tokens": ["Es", "klang", "wie", "Droh'n", "und", "wie\u00b7der", "klang's", "wie", "H\u00f6h\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "KON", "ADV", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "\u201eein junger Wand'rer kam zu mir gefahren", "tokens": ["\u201e", "ein", "jun\u00b7ger", "Wan\u00b7d'\u00b7rer", "kam", "zu", "mir", "ge\u00b7fah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Mit hast'gen Schritten und mit weh'nden Haaren.", "tokens": ["Mit", "hast'\u00b7gen", "Schrit\u00b7ten", "und", "mit", "weh'n\u00b7den", "Haa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "Ein bleiches Bild, so ist er ohne Bangen", "tokens": ["Ein", "blei\u00b7ches", "Bild", ",", "so", "ist", "er", "oh\u00b7ne", "Ban\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Auf meinen schmalen Gr\u00e4ten umgegangen,", "tokens": ["Auf", "mei\u00b7nen", "schma\u00b7len", "Gr\u00e4\u00b7ten", "um\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "Und \u00fcber Kl\u00fcften, schwindelnd abgrundtiefen,", "tokens": ["Und", "\u00fc\u00b7ber", "Kl\u00fcf\u00b7ten", ",", "schwin\u00b7delnd", "ab\u00b7grund\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.33": {"text": "Aus welchen jubelnd ihn die Wogen riefen,", "tokens": ["Aus", "wel\u00b7chen", "ju\u00b7belnd", "ihn", "die", "Wo\u00b7gen", "rie\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VVPP", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Ist er gewandelt auf gest\u00fcrzten F\u00f6hren", "tokens": ["Ist", "er", "ge\u00b7wan\u00b7delt", "auf", "ge\u00b7st\u00fcrz\u00b7ten", "F\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und schien in meine Wildni\u00df zu geh\u00f6ren,", "tokens": ["Und", "schien", "in", "mei\u00b7ne", "Wild\u00b7ni\u00df", "zu", "ge\u00b7h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Ein dumpfer Ton in meinen dumpfen Ch\u00f6ren \u2014", "tokens": ["Ein", "dum\u00b7pfer", "Ton", "in", "mei\u00b7nen", "dum\u00b7pfen", "Ch\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.37": {"text": "Unkundig der Gefahr, in wachem Traume,", "tokens": ["Un\u00b7kun\u00b7dig", "der", "Ge\u00b7fahr", ",", "in", "wa\u00b7chem", "Trau\u00b7me", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Doch mir gefiel der K\u00fchne und der Blinde,", "tokens": ["Doch", "mir", "ge\u00b7fiel", "der", "K\u00fch\u00b7ne", "und", "der", "Blin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "Und Sorge trug ich dir als einem Kinde \u2014", "tokens": ["Und", "Sor\u00b7ge", "trug", "ich", "dir", "als", "ei\u00b7nem", "Kin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "PPER", "KOUS", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.40": {"text": "Jetzt, lieber Herr, bist leidlich du vern\u00fcnftig,", "tokens": ["Jetzt", ",", "lie\u00b7ber", "Herr", ",", "bist", "leid\u00b7lich", "du", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "NN", "$,", "VAFIN", "ADJD", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.41": {"text": "Hast Weib und Hof, bist in der Gilde z\u00fcnftig,", "tokens": ["Hast", "Weib", "und", "Hof", ",", "bist", "in", "der", "Gil\u00b7de", "z\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$,", "VAFIN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Verla\u00df dich nicht auf meine Fl\u00fcgel k\u00fcnftig!\u201c", "tokens": ["Ver\u00b7la\u00df", "dich", "nicht", "auf", "mei\u00b7ne", "Fl\u00fc\u00b7gel", "k\u00fcnf\u00b7tig", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "PTKNEG", "APPR", "PPOSAT", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}