{"textgrid.poem.59356": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Treue", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie dem Wanderer in Tr\u00e4umen,", "tokens": ["Wie", "dem", "Wan\u00b7de\u00b7rer", "in", "Tr\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df er still im Schlafe weint,", "tokens": ["Da\u00df", "er", "still", "im", "Schla\u00b7fe", "weint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwischen goldnen Wolkens\u00e4umen", "tokens": ["Zwi\u00b7schen", "gold\u00b7nen", "Wol\u00b7ken\u00b7s\u00e4u\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seine Heimat wohl erscheint:", "tokens": ["Sei\u00b7ne", "Hei\u00b7mat", "wohl", "er\u00b7scheint", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "So durch dieses Fr\u00fchlings Bl\u00fchen", "tokens": ["So", "durch", "die\u00b7ses", "Fr\u00fch\u00b7lings", "Bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "\u00dcber Berg' und T\u00e4ler tief,", "tokens": ["\u00dc\u00b7ber", "Ber\u00b7g'", "und", "T\u00e4\u00b7ler", "tief", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APP