{"dta.poem.13120": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Achtes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem sie mit dem Rath von Kinder-Liebe redte,", "tokens": ["In\u00b7dem", "sie", "mit", "dem", "Rath", "von", "Kin\u00b7der\u00b7Lie\u00b7be", "red\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "550H\u00f6rt' ich, da\u00df sie das Amt der Kinder-Zucht vertrete.", "tokens": ["'", "ich", ",", "da\u00df", "sie", "das", "Amt", "der", "Kin\u00b7der\u00b7Zucht", "ver\u00b7tre\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "KOUS", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr Aufzug schimmerte von eingewircktem Gold,", "tokens": ["Ihr", "Auf\u00b7zug", "schim\u00b7mer\u00b7te", "von", "ein\u00b7ge\u00b7wirck\u00b7tem", "Gold", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und ihrem Vortrag nach war sie nur Kindern hold.", "tokens": ["Und", "ih\u00b7rem", "Vor\u00b7trag", "nach", "war", "sie", "nur", "Kin\u00b7dern", "hold", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "VAFIN", "PPER", "ADV", "NN", "ADJD", "$."], "meter": "-+-+-++--+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Sie bog, indem sie sprach, ein gr\u00fcnes Rei\u00df zusammen,", "tokens": ["Sie", "bog", ",", "in\u00b7dem", "sie", "sprach", ",", "ein", "gr\u00fc\u00b7nes", "Rei\u00df", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vielleicht vergliche sie die Kinder jungen Stammen.", "tokens": ["Viel\u00b7leicht", "ver\u00b7gli\u00b7che", "sie", "die", "Kin\u00b7der", "jun\u00b7gen", "Stam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "555Jhr aufgeraumter Blick wies Ernst und Freundlichkeit;", "tokens": ["auf\u00b7ge\u00b7raum\u00b7ter", "Blick", "wies", "Ernst", "und", "Freund\u00b7lich\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "NE", "KON", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.8": {"text": "Jhr Sprechen Wiz, Verstand; ihr Regen Sittsamkeit.", "tokens": ["Ihr", "Spre\u00b7chen", "Wiz", ",", "Ver\u00b7stand", ";", "ihr", "Re\u00b7gen", "Sitt\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "$,", "NN", "$.", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ich wei\u00df nicht, was das Rei\u00df ihr vor Vergn\u00fcgen brachte,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "was", "das", "Rei\u00df", "ihr", "vor", "Ver\u00b7gn\u00fc\u00b7gen", "brach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PRELS", "ART", "NN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Weil sie dasselbe stets bald krumm bald grade machte,", "tokens": ["Weil", "sie", "das\u00b7sel\u00b7be", "stets", "bald", "krumm", "bald", "gra\u00b7de", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PDAT", "ADV", "ADV", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sie spielte stets damit. Nun wand sie sich zum Krei\u00df", "tokens": ["Sie", "spiel\u00b7te", "stets", "da\u00b7mit", ".", "Nun", "wand", "sie", "sich", "zum", "Krei\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "$.", "ADV", "VVFIN", "PPER", "PRF", "APPRART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "560Und sprach mit Freundlichkeit und Ernst auf diese Weis:", "tokens": ["sprach", "mit", "Freund\u00b7lich\u00b7keit", "und", "Ernst", "auf", "die\u00b7se", "Weis", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}