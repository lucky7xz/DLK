{"dta.poem.12692": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Venus wirthschafft-spiel  \n in Dre\u00dfden.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich gebe, sagte n\u00e4chst die Venus, etwas zu;", "tokens": ["Ich", "ge\u00b7be", ",", "sag\u00b7te", "n\u00e4chst", "die", "Ve\u00b7nus", ",", "et\u00b7was", "zu", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADV", "ART", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als nach dem Sachsen-fest schon alles war zur ruh,", "tokens": ["Als", "nach", "dem", "Sach\u00b7sen\u00b7fest", "schon", "al\u00b7les", "war", "zur", "ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ADV", "PIS", "VAFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Meyer soll sich bald mit einer Vogtin paaren,", "tokens": ["Ein", "Me\u00b7yer", "soll", "sich", "bald", "mit", "ei\u00b7ner", "Vog\u00b7tin", "paa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VMFIN", "PRF", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und meine wirthschaffts-lust zu tag und nacht erfahren.", "tokens": ["Und", "mei\u00b7ne", "wirth\u00b7schaffts\u00b7lust", "zu", "tag", "und", "nacht", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}