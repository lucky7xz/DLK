{"dta.poem.21589": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Komm Bruder Manfred \u2026", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Komm Bruder Manfred, reiche mir die Hand", "tokens": ["Komm", "Bru\u00b7der", "Man\u00b7fred", ",", "rei\u00b7che", "mir", "die", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "NE", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Her\u00fcber aus dem ungeheuren Nichts,", "tokens": ["Her\u00b7\u00fc\u00b7ber", "aus", "dem", "un\u00b7ge\u00b7heu\u00b7ren", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In dem Dein Geist ersehnte Ruhe fand \u2014", "tokens": ["In", "dem", "Dein", "Geist", "er\u00b7sehn\u00b7te", "Ru\u00b7he", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "PPOSAT", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich folge Dir, ach! l\u00e4chelnden Gesichts.", "tokens": ["Ich", "fol\u00b7ge", "Dir", ",", "ach", "!", "l\u00e4\u00b7cheln\u00b7den", "Ge\u00b7sichts", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "ITJ", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Auch meine Lampe brennt dem Ende zu,", "tokens": ["Auch", "mei\u00b7ne", "Lam\u00b7pe", "brennt", "dem", "En\u00b7de", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So oft gef\u00fcllt! ich la\u00df sie gern verrauchen.", "tokens": ["So", "oft", "ge\u00b7f\u00fcllt", "!", "ich", "la\u00df", "sie", "gern", "ver\u00b7rau\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$.", "PPER", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was ich ersann, erlebt, erlitt \u2014 wie Du!", "tokens": ["Was", "ich", "er\u00b7sann", ",", "er\u00b7lebt", ",", "er\u00b7litt", "wie", "Du", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVPP", "$,", "VVFIN", "$(", "PWAV", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "L\u00e4\u00dft mich mit Wonn\u2019 in\u2019s Dunkel niedertauchen.", "tokens": ["L\u00e4\u00dft", "mich", "mit", "Wonn'", "in's", "Dun\u00b7kel", "nie\u00b7der\u00b7tau\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}