{"dta.poem.15828": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Freunde haben mir den Becher \u00fcbersendet,", "tokens": ["Die", "Freun\u00b7de", "ha\u00b7ben", "mir", "den", "Be\u00b7cher", "\u00fc\u00b7ber\u00b7sen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der, au\u00dfen Silberschmuck und innen Gold, mich blendet.", "tokens": ["Der", ",", "au\u00b7\u00dfen", "Sil\u00b7ber\u00b7schmuck", "und", "in\u00b7nen", "Gold", ",", "mich", "blen\u00b7det", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "NN", "KON", "ADJA", "NN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er ist nur viel zu gro\u00df, ich kann daraus nicht trinken,", "tokens": ["Er", "ist", "nur", "viel", "zu", "gro\u00df", ",", "ich", "kann", "da\u00b7raus", "nicht", "trin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$,", "PPER", "VMFIN", "PAV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Arme w\u00fcrden mir mit dem gef\u00fcllten sinken.", "tokens": ["Die", "Ar\u00b7me", "w\u00fcr\u00b7den", "mir", "mit", "dem", "ge\u00b7f\u00fcll\u00b7ten", "sin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es ist ein sch\u00f6ner Schein, darum ward er gegeben", "tokens": ["Es", "ist", "ein", "sch\u00f6\u00b7ner", "Schein", ",", "da\u00b7rum", "ward", "er", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,", "PAV", "VAFIN", "PPER", "VVPP"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zum Lohn der Poesie, die auch nicht ist f\u00fcrs Leben.", "tokens": ["Zum", "Lohn", "der", "Poe\u00b7sie", ",", "die", "auch", "nicht", "ist", "f\u00fcrs", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "PRELS", "ADV", "PTKNEG", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}}}}