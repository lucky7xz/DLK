{"textgrid.poem.60285": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Schw\u00e4rmer", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diese Blumen ohne Duft und Farben", "tokens": ["Die\u00b7se", "Blu\u00b7men", "oh\u00b7ne", "Duft", "und", "Far\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und von ihr, an deren Brust sie starben,", "tokens": ["Und", "von", "ihr", ",", "an", "de\u00b7ren", "Brust", "sie", "star\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "$,", "APPR", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "In den Staub geworfen und vergessen,", "tokens": ["In", "den", "Staub", "ge\u00b7wor\u00b7fen", "und", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "KON", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Magst du sie noch an die Lippen pressen?", "tokens": ["Magst", "du", "sie", "noch", "an", "die", "Lip\u00b7pen", "pres\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Soll die Bl\u00fcte ihnen wiederkehren,", "tokens": ["Soll", "die", "Bl\u00fc\u00b7te", "ih\u00b7nen", "wie\u00b7der\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Da\u00df du sie betaust mit Liebesz\u00e4hren?", "tokens": ["Da\u00df", "du", "sie", "be\u00b7taust", "mit", "Lie\u00b7bes\u00b7z\u00e4h\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.7": {"text": "Schw\u00e4rmer, den ein welkes Blatt entz\u00fcckt,", "tokens": ["Schw\u00e4r\u00b7mer", ",", "den", "ein", "wel\u00b7kes", "Blatt", "ent\u00b7z\u00fcckt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.8": {"text": "Das im Spiel ein sch\u00f6nes Kind zerknickt!", "tokens": ["Das", "im", "Spiel", "ein", "sch\u00f6\u00b7nes", "Kind", "zer\u00b7knickt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "\u00bbschw\u00e4rmer! denkst du noch an jene Leiche?", "tokens": ["\u00bb", "schw\u00e4r\u00b7mer", "!", "denkst", "du", "noch", "an", "je\u00b7ne", "Lei\u00b7che", "?"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$.", "VVFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "O wie mochtest du die welke, bleiche", "tokens": ["O", "wie", "moch\u00b7test", "du", "die", "wel\u00b7ke", ",", "blei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PWAV", "VMFIN", "PPER", "ART", "NN", "$,", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "\u00dcberweinen und zur Lippe p