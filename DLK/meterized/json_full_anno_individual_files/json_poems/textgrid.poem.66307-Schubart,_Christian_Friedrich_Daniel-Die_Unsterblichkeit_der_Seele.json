{"textgrid.poem.66307": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "Die Unsterblichkeit der Seele", "genre": "verse", "period": "N.A.", "pub_year": 1767, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heut rei\u00dfe dich, o Seele! los", "tokens": ["Heut", "rei\u00b7\u00dfe", "dich", ",", "o", "See\u00b7le", "!", "los"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deiner Sklavenb\u00fcrde!", "tokens": ["Von", "dei\u00b7ner", "Skla\u00b7ven\u00b7b\u00fcr\u00b7de", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fleuch auf, Unsterbliche! sei gro\u00df,", "tokens": ["Fleuch", "auf", ",", "U\u00b7nsterb\u00b7li\u00b7che", "!", "sei", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$.", "VAFIN", "ADJD", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und singe deine W\u00fcrde,", "tokens": ["Und", "sin\u00b7ge", "dei\u00b7ne", "W\u00fcr\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Voll Majest\u00e4t, wie Orgelton,", "tokens": ["Voll", "Ma\u00b7jes\u00b7t\u00e4t", ",", "wie", "Or\u00b7gel\u00b7ton", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "NN", "$,", "PWAV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erhaben, wie Isais Sohn,", "tokens": ["Er\u00b7ha\u00b7ben", ",", "wie", "I\u00b7sais", "Sohn", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PWAV", "NE", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und hoch, wie Adler fliegen.", "tokens": ["Und", "hoch", ",", "wie", "Ad\u00b7ler", "flie\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "PWAV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie, die in unerflogner H\u00f6h'", "tokens": ["Sie", ",", "die", "in", "un\u00b7er\u00b7flog\u00b7ner", "H\u00f6h'"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Cherubsfl\u00fcgel schwinget,", "tokens": ["Die", "Che\u00b7rubs\u00b7fl\u00fc\u00b7gel", "schwin\u00b7get", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und tief ins Unerme\u00dfliche", "tokens": ["Und", "tief", "ins", "Un\u00b7er\u00b7me\u00df\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPRART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.4": {"text": "Mit edler K\u00fchnheit dringet,", "tokens": ["Mit", "ed\u00b7ler", "K\u00fchn\u00b7heit", "drin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die immer steigt, und niemals ruht,", "tokens": ["Die", "im\u00b7mer", "steigt", ",", "und", "nie\u00b7mals", "ruht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "KON", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Seele, diese Gottesgluth", "tokens": ["Die", "See\u00b7le", ",", "die\u00b7se", "Got\u00b7tes\u00b7gluth"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Soll einst verl\u00f6schen k\u00f6nnen?", "tokens": ["Soll", "einst", "ver\u00b7l\u00f6\u00b7schen", "k\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein Geist, der sich in Tiefen senkt", "tokens": ["Ein", "Geist", ",", "der", "sich", "in", "Tie\u00b7fen", "senkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und in die H\u00f6hen f\u00e4hret,", "tokens": ["Und", "in", "die", "H\u00f6\u00b7hen", "f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Wesen, das den Sch\u00f6pfer denkt,", "tokens": ["Ein", "We\u00b7sen", ",", "das", "den", "Sch\u00f6p\u00b7fer", "denkt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Wille, der ihn ehret,", "tokens": ["Ein", "Wil\u00b7le", ",", "der", "ihn", "eh\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Herz, das sich in Wahrheit \u00fcbt,", "tokens": ["Ein", "Herz", ",", "das", "sich", "in", "Wahr\u00b7heit", "\u00fcbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und dich, Unendlicher! ", "tokens": ["Und", "dich", ",", "Un\u00b7end\u00b7li\u00b7cher", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADJD", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.7": {"text": "Das soll der Tod zerst\u00f6ren? \u2013", "tokens": ["Das", "soll", "der", "Tod", "zer\u00b7st\u00f6\u00b7ren", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Soll denn dein Hauchen, Jehovah,", "tokens": ["Soll", "denn", "dein", "Hau\u00b7chen", ",", "Je\u00b7ho\u00b7vah", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So leicht, wie Wind, verwehen?", "tokens": ["So", "leicht", ",", "wie", "Wind", ",", "ver\u00b7we\u00b7hen", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "PWAV", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So hat umsonst der Golgatha", "tokens": ["So", "hat", "um\u00b7sonst", "der", "Gol\u00b7ga\u00b7tha"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Mittlers Blut gesehen?", "tokens": ["Des", "Mitt\u00b7lers", "Blut", "ge\u00b7se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So wallt vergeblich Gottes Geist,", "tokens": ["So", "wallt", "ver\u00b7geb\u00b7lich", "Got\u00b7tes", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der Kraft und Heiligung verhei\u00dft,", "tokens": ["Der", "Kraft", "und", "Hei\u00b7li\u00b7gung", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf uns im Bade nieder?", "tokens": ["Auf", "uns", "im", "Ba\u00b7de", "nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "So ist kein flammendes Gericht", "tokens": ["So", "ist", "kein", "flam\u00b7men\u00b7des", "Ge\u00b7richt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr S\u00fcnder, die ", "tokens": ["F\u00fcr", "S\u00fcn\u00b7der", ",", "die"], "token_info": ["word", "word", "punct", "word"], "pos": ["APPR", "NN", "$,", "PRELS"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "So sollen seine Donner nicht", "tokens": ["So", "sol\u00b7len", "sei\u00b7ne", "Don\u00b7ner", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des S\u00fcnders Scheitel fassen?", "tokens": ["Des", "S\u00fcn\u00b7ders", "Schei\u00b7tel", "fas\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So lebt der Thor im Ueberflu\u00df?", "tokens": ["So", "lebt", "der", "Thor", "im", "Ue\u00b7berf\u00b7lu\u00df", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Weisheit soll, wie Lazarus,", "tokens": ["Und", "Weis\u00b7heit", "soll", ",", "wie", "La\u00b7za\u00b7rus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "VMFIN", "$,", "PWAV", "NN", "$,"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.7": {"text": "In Bettlerslumpen sterben?", "tokens": ["In", "Bett\u00b7ler\u00b7slum\u00b7pen", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Entrei\u00dfe dich, verzagter Geist,", "tokens": ["Ent\u00b7rei\u00b7\u00dfe", "dich", ",", "ver\u00b7zag\u00b7ter", "Geist", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem bangen Todesschauer!", "tokens": ["Dem", "ban\u00b7gen", "To\u00b7des\u00b7schau\u00b7er", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Selbst deine Zweifelsucht beweist", "tokens": ["Selbst", "dei\u00b7ne", "Zwei\u00b7fel\u00b7sucht", "be\u00b7weist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Seele ew'ge Dauer;", "tokens": ["Der", "See\u00b7le", "ew'\u00b7ge", "Dau\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und jeder Kummer, der dich qu\u00e4lt,", "tokens": ["Und", "je\u00b7der", "Kum\u00b7mer", ",", "der", "dich", "qu\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und jedes Gl\u00fccke, das dir fehlt,", "tokens": ["Und", "je\u00b7des", "Gl\u00fc\u00b7cke", ",", "das", "dir", "fehlt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Spricht laut von deinem Adel.", "tokens": ["Spricht", "laut", "von", "dei\u00b7nem", "A\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Soll Gott, der jedes Wesen schafft,", "tokens": ["Soll", "Gott", ",", "der", "je\u00b7des", "We\u00b7sen", "schafft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$,", "PRELS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Sch\u00f6pfung Ruhm zu mehren,", "tokens": ["Der", "Sch\u00f6p\u00b7fung", "Ruhm", "zu", "meh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Geister, seine beste Kraft,", "tokens": ["Die", "Geis\u00b7ter", ",", "sei\u00b7ne", "bes\u00b7te", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein Meisterst\u00fcck, zerst\u00f6ren?", "tokens": ["Sein", "Meis\u00b7ter\u00b7st\u00fcck", ",", "zer\u00b7st\u00f6\u00b7ren", "?"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zwar fallen dich die Zweifel an:", "tokens": ["Zwar", "fal\u00b7len", "dich", "die", "Zwei\u00b7fel", "an", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.8": {"line.1": {"text": "Du Gott der Wahrheit! nur dein Wort", "tokens": ["Du", "Gott", "der", "Wahr\u00b7heit", "!", "nur", "dein", "Wort"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "NN", "ART", "NN", "$.", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kann bange Zweifel heben.", "tokens": ["Kann", "ban\u00b7ge", "Zwei\u00b7fel", "he\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ich traue dir! es sollen dort", "tokens": ["Ich", "trau\u00b7e", "dir", "!", "es", "sol\u00b7len", "dort"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VMFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Seelen ", "tokens": ["Die", "See\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Dort soll die Tugend gl\u00fccklich sein,", "tokens": ["Dort", "soll", "die", "Tu\u00b7gend", "gl\u00fcck\u00b7lich", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und Laster schluckt die H\u00f6lle ein \u2013", "tokens": ["Und", "Las\u00b7ter", "schluckt", "die", "H\u00f6l\u00b7le", "ein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So glaubt der Christ, und schweiget.", "tokens": ["So", "glaubt", "der", "Christ", ",", "und", "schwei\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Drum rei\u00dfe dich, o Seele! los", "tokens": ["Drum", "rei\u00b7\u00dfe", "dich", ",", "o", "See\u00b7le", "!", "los"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PAV", "VVFIN", "PPER", "$,", "FM", "NN", "$.", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von deiner Sklavenb\u00fcrde.", "tokens": ["Von", "dei\u00b7ner", "Skla\u00b7ven\u00b7b\u00fcr\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Fleuch auf, Unsterbliche! sei gro\u00df", "tokens": ["Fleuch", "auf", ",", "U\u00b7nsterb\u00b7li\u00b7che", "!", "sei", "gro\u00df"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "PTKVZ", "$,", "NN", "$.", "VAFIN", "ADJD"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und f\u00fchle deine W\u00fcrde!", "tokens": ["Und", "f\u00fch\u00b7le", "dei\u00b7ne", "W\u00fcr\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"