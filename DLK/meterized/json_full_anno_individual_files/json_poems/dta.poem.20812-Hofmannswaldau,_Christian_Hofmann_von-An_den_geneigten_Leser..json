{"dta.poem.20812": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An den geneigten  \n Leser.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.85", "nl:0.14"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Mars der rihset in den Lande/", "tokens": ["Herr", "Mars", "der", "rih\u00b7set", "in", "den", "Lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hat den werden GOt Amur", "tokens": ["Der", "hat", "den", "wer\u00b7den", "Got", "A\u00b7mur"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verhert mit R\u00f6be und \u00f6ch mit Brande/", "tokens": ["Ver\u00b7hert", "mit", "R\u00f6\u00b7be", "und", "\u00f6ch", "mit", "Bran\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "ADJD", "APPR", "NN", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "De\u00df sind di minne worden sur.", "tokens": ["De\u00df", "sind", "di", "min\u00b7ne", "wor\u00b7den", "sur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Herr Mars der reiset in dem Lande/", "tokens": ["Herr", "Mars", "der", "rei\u00b7set", "in", "dem", "Lan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der hat den werthen Buhlschaffts Gott", "tokens": ["Der", "hat", "den", "wert\u00b7hen", "Buhl\u00b7schaffts", "Gott"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Verhert mit Raub und auch mit Brande/", "tokens": ["Ver\u00b7hert", "mit", "Raub", "und", "auch", "mit", "Bran\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "KON", "ADV", "APPR", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dadurch die Liebe kam in Noth.", "tokens": ["Da\u00b7durch", "die", "Lie\u00b7be", "kam", "in", "Noth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}