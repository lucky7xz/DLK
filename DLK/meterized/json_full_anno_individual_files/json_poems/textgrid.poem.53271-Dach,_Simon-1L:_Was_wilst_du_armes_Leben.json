info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Christus mir kan geben", "tokens": ["Das", "Chris\u00b7tus", "mir", "kan", "ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VMFIN", "VVINF"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.6": {"text": "Durch seiner Vnschuld Blut.", "tokens": ["Durch", "sei\u00b7ner", "Vn\u00b7schuld", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Herr Jesu, Zwang der Hellen,", "tokens": ["Herr", "Je\u00b7su", ",", "Zwang", "der", "Hel\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Du vns Tausent stellen", "tokens": ["Der", "Du", "vns", "Tau\u00b7sent", "stel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Himmel auffger\u00e4umbt,", "tokens": ["Im", "Him\u00b7mel", "auff\u00b7ge\u00b7r\u00e4umbt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Nimm mich in Deine H\u00e4nde,", "tokens": ["Nimm", "mich", "in", "Dei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Weil meines Lebens Ende", "tokens": ["Weil", "mei\u00b7nes", "Le\u00b7bens", "En\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Sich nahet vnges\u00e4umbt!", "tokens": ["Sich", "na\u00b7het", "vn\u00b7ge\u00b7s\u00e4umbt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Eil' au\u00df den finstern H\u00f6len", "tokens": ["Eil'", "au\u00df", "den", "fins\u00b7tern", "H\u00f6\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit meiner armen Seelen,", "tokens": ["Mit", "mei\u00b7ner", "ar\u00b7men", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Vnd bring mich an das Licht,", "tokens": ["Vnd", "bring", "mich", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Da du selbst, Glantz vnd Sonne,", "tokens": ["Da", "du", "selbst", ",", "Glantz", "vnd", "Son\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Strahlen deiner Wonne", "tokens": ["Mit", "Strah\u00b7len", "dei\u00b7ner", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Verkl\u00e4hrst mein Angesicht!", "tokens": ["Ver\u00b7kl\u00e4hrst", "mein", "An\u00b7ge\u00b7sicht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "So werd' ich selbst anschawen,", "tokens": ["So", "werd'", "ich", "selbst", "an\u00b7scha\u00b7wen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Worauff wir hie nur bawen", "tokens": ["Wo\u00b7rauff", "wir", "hie", "nur", "ba\u00b7wen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Durch Glauben an dein Wort,", "tokens": ["Durch", "Glau\u00b7ben", "an", "dein", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Vnd mit der Schaar der Frommen", "tokens": ["Vnd", "mit", "der", "Schaar", "der", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Au\u00df Sturm vnd Wellen kommen", "tokens": ["Au\u00df", "Sturm", "vnd", "Wel\u00b7len", "kom\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu dem gew\u00fcnschten Port.", "tokens": ["Zu", "dem", "ge\u00b7w\u00fcnschten", "Port", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}