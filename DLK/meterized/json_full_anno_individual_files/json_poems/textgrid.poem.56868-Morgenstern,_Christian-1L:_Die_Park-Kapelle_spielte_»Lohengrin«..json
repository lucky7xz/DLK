{"textgrid.poem.56868": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Park-Kapelle spielte \u00bbLohengrin\u00ab.", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Park-Kapelle spielte \u00bbLohengrin\u00ab.", "tokens": ["Die", "Park\u00b7Ka\u00b7pel\u00b7le", "spiel\u00b7te", "\u00bb", "Lo\u00b7hen\u00b7grin", "\u00ab", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "NN", "$(", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da l\u00f6ste sich mein Blut zu j\u00e4hem Gang,", "tokens": ["Da", "l\u00f6s\u00b7te", "sich", "mein", "Blut", "zu", "j\u00e4\u00b7hem", "Gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "da\u00df hei\u00df und weh das Herz mir \u00fcberschwoll.", "tokens": ["da\u00df", "hei\u00df", "und", "weh", "das", "Herz", "mir", "\u00fc\u00b7bersc\u00b7hwoll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "KON", "ADV", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Auch Du hast jene T\u00f6ne ja geliebt", "tokens": ["Auch", "Du", "hast", "je\u00b7ne", "T\u00f6\u00b7ne", "ja", "ge\u00b7liebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "PDAT", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "und einst voll tiefen Dursts in dich getrunken,", "tokens": ["und", "einst", "voll", "tie\u00b7fen", "Dursts", "in", "dich", "ge\u00b7trun\u00b7ken"