{"textgrid.poem.24150": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch Li-yo-ing, wo aus morastiger Erde", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch Li-yo-ing, wo aus morastiger Erde", "tokens": ["Durch", "Li\u00b7yo\u00b7ing", ",", "wo", "aus", "mo\u00b7ras\u00b7ti\u00b7ger", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Riesenbambus aufschie\u00dft wie ein Wald,", "tokens": ["Der", "Rie\u00b7sen\u00b7bam\u00b7bus", "auf\u00b7schie\u00dft", "wie", "ein", "Wald", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ging, in Gedanken ganz verstrickt, der Weise,", "tokens": ["Ging", ",", "in", "Ge\u00b7dan\u00b7ken", "ganz", "ver\u00b7strickt", ",", "der", "Wei\u00b7se", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "ADV", "VVPP", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Dichter Lyng-Lun. K\u00fcmmerlich sein Leib;", "tokens": ["Der", "Dich\u00b7ter", "Lyng\u00b7Lun", ".", "K\u00fcm\u00b7mer\u00b7lich", "sein", "Leib", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$.", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Doch seine Seele, die war gottesstark.", "tokens": ["Doch", "sei\u00b7ne", "See\u00b7le", ",", "die", "war", "got\u00b7tes\u00b7stark", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In breiten St\u00f6\u00dfen, osther, rauschten an", "tokens": ["In", "brei\u00b7ten", "St\u00f6\u00b7\u00dfen", ",", "ost\u00b7her", ",", "rauschten", "an"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "$,", "VVFIN", "PTKVZ"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Die Winde voller Kraft und bliesen laut", "tokens": ["Die", "Win\u00b7de", "vol\u00b7ler", "Kraft", "und", "blie\u00b7sen", "laut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Durch dieses R\u00f6hrichts palmenhohe St\u00e4mme;", "tokens": ["Durch", "die\u00b7ses", "R\u00f6h\u00b7richts", "pal\u00b7men\u00b7ho\u00b7he", "St\u00e4m\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Und wundersame Weisen weckten sie", "tokens": ["Und", "wun\u00b7der\u00b7sa\u00b7me", "Wei\u00b7sen", "weck\u00b7ten", "sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Aus ihnen, da\u00df es wie vom Menschen klang.", "tokens": ["Aus", "ih\u00b7nen", ",", "da\u00df", "es", "wie", "vom", "Men\u00b7schen", "klang", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPER", "KOKOM", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Ein Singen, Jauchzen und ein Klagen wars.", "tokens": ["Ein", "Sin\u00b7gen", ",", "Jauch\u00b7zen", "und", "ein", "Kla\u00b7gen", "wars", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "KON", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Bis zu des Weisen F\u00fc\u00dfen beugten sich", "tokens": ["Bis", "zu", "des", "Wei\u00b7sen", "F\u00fc\u00b7\u00dfen", "beug\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die schlanken Sch\u00e4fte, seinen Wangen l\u00e4ngs", "tokens": ["Die", "schlan\u00b7ken", "Sch\u00e4f\u00b7te", ",", "sei\u00b7nen", "Wan\u00b7gen", "l\u00e4ngs"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schwebten wie Schmeichelh\u00e4nde ihre Bl\u00e4tter,", "tokens": ["Schweb\u00b7ten", "wie", "Schmei\u00b7chel\u00b7h\u00e4n\u00b7de", "ih\u00b7re", "Bl\u00e4t\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "NN", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Die langen, schmalen. Und es hielt Lyng-Lun", "tokens": ["Die", "lan\u00b7gen", ",", "schma\u00b7len", ".", "Und", "es", "hielt", "Lyng\u00b7Lun"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$.", "KON", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "In Schweigen sinnend einen Stamm zur\u00fcck.", "tokens": ["In", "Schwei\u00b7gen", "sin\u00b7nend", "ei\u00b7nen", "Stamm", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Und zwischen zweien Knorren schnitt er sich,", "tokens": ["Und", "zwi\u00b7schen", "zwei\u00b7en", "Knor\u00b7ren", "schnitt", "er", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "CARD", "NN", "VVFIN", "PPER", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Genau inmitten schnitt ein St\u00fcck er sich", "tokens": ["Ge\u00b7nau", "in\u00b7mit\u00b7ten", "schnitt", "ein", "St\u00fcck", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "VVFIN", "ART", "NN", "PPER", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Heraus, und sieh: Da seinen Atem er", "tokens": ["He\u00b7raus", ",", "und", "sieh", ":", "Da", "sei\u00b7nen", "A\u00b7tem", "er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KON", "VVIMP", "$.", "KOUS", "PPOSAT", "NN", "PPER"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.9": {"text": "Dem Rohre einblies, schwoll ihm sanft ein Klang", "tokens": ["Dem", "Roh\u00b7re", "ein\u00b7blies", ",", "schwoll", "ihm", "sanft", "ein", "Klang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sehr tief und voll aus diesem Rohr entgegen,", "tokens": ["Sehr", "tief", "und", "voll", "aus", "die\u00b7sem", "Rohr", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "PDAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ein Klang, lebendig wie die eigne Stimme.", "tokens": ["Ein", "Klang", ",", "le\u00b7ben\u00b7dig", "wie", "die", "eig\u00b7ne", "Stim\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wunderbar: als h\u00e4tte rings um ihn", "tokens": ["Und", "wun\u00b7der\u00b7bar", ":", "als", "h\u00e4t\u00b7te", "rings", "um", "ihn"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$.", "KOKOM", "VAFIN", "ADV", "APPR", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In Luft und Erden alles nur geschwiegen,", "tokens": ["In", "Luft", "und", "Er\u00b7den", "al\u00b7les", "nur", "ge\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PIS", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Bis da\u00df ihm Stimme gab sein Menschenmund,", "tokens": ["Bis", "da\u00df", "ihm", "Stim\u00b7me", "gab", "sein", "Men\u00b7schen\u00b7mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ward nun mit eins die ganze Welt Gesang.", "tokens": ["Ward", "nun", "mit", "eins", "die", "gan\u00b7ze", "Welt", "Ge\u00b7sang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPR", "PIS", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Hoangho, der seinen gr\u00fcnen Strom", "tokens": ["Der", "Ho\u00b7an\u00b7gho", ",", "der", "sei\u00b7nen", "gr\u00fc\u00b7nen", "Strom"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NE", "$,", "PRELS", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie eine Flut von Schlangen vor ihm w\u00e4lzte,", "tokens": ["Wie", "ei\u00b7ne", "Flut", "von", "Schlan\u00b7gen", "vor", "ihm", "w\u00e4lz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Er wieherte wie ein gepanzert Ro\u00df,", "tokens": ["Er", "wie\u00b7her\u00b7te", "wie", "ein", "ge\u00b7pan\u00b7zert", "Ro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "ART", "VVPP", "NN", "$,"], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.8": {"text": "Wenn es zum erstenmal im L\u00e4rm der Schlacht", "tokens": ["Wenn", "es", "zum", "ers\u00b7ten\u00b7mal", "im", "L\u00e4rm", "der", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADV", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Auf Schild und Br\u00fcnne Schwerter klirren h\u00f6rt.", "tokens": ["Auf", "Schild", "und", "Br\u00fcn\u00b7ne", "Schwer\u00b7ter", "klir\u00b7ren", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der Fung-hoan, der rote Zaubervogel,", "tokens": ["Der", "Fung\u00b7ho\u00b7an", ",", "der", "ro\u00b7te", "Zau\u00b7ber\u00b7vo\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Schwang sich mit seinem Weibchen auf den Ast,", "tokens": ["Schwang", "sich", "mit", "sei\u00b7nem", "Weib\u00b7chen", "auf", "den", "Ast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Und seiner Liebe s\u00fc\u00dfe Sehnsucht klang", "tokens": ["Und", "sei\u00b7ner", "Lie\u00b7be", "s\u00fc\u00b7\u00dfe", "Sehn\u00b7sucht", "klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Wie lebend Gold.", "tokens": ["Wie", "le\u00b7bend", "Gold", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Da rief Lyng-Lun, der Weise,", "tokens": ["Da", "rief", "Lyng\u00b7Lun", ",", "der", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Begeistert laut: Ah, huldreiche Natur,", "tokens": ["Be\u00b7geis\u00b7tert", "laut", ":", "Ah", ",", "huld\u00b7rei\u00b7che", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "$.", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ich h\u00f6re deine Stimme. Br\u00fclle, Strom!", "tokens": ["Ich", "h\u00f6\u00b7re", "dei\u00b7ne", "Stim\u00b7me", ".", "Br\u00fcl\u00b7le", ",", "Strom", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$.", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sing, singe, roter Vogel! Winde, braust", "tokens": ["Sing", ",", "sin\u00b7ge", ",", "ro\u00b7ter", "Vo\u00b7gel", "!", "Win\u00b7de", ",", "braust"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "punct", "word"], "pos": ["NE", "$,", "VVFIN", "$,", "ADJA", "NN", "$.", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und rauscht Akkorde durch das schwanke Rohr,", "tokens": ["Und", "rauscht", "Ak\u00b7kor\u00b7de", "durch", "das", "schwan\u00b7ke", "Rohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df ich erlausche deiner Stimme Klang", "tokens": ["Da\u00df", "ich", "er\u00b7lau\u00b7sche", "dei\u00b7ner", "Stim\u00b7me", "Klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und in mich berge, denn es wird fortan", "tokens": ["Und", "in", "mich", "ber\u00b7ge", ",", "denn", "es", "wird", "for\u00b7tan"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "VVFIN", "$,", "KON", "PPER", "VAFIN", "NE"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mitklingen und mitsingen die Natur,", "tokens": ["Mit\u00b7klin\u00b7gen", "und", "mit\u00b7sin\u00b7gen", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wo seiner Seele Tiefgef\u00fchl der Mensch,", "tokens": ["Wo", "sei\u00b7ner", "See\u00b7le", "Tief\u00b7ge\u00b7f\u00fchl", "der", "Mensch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der leidende, ausklagt, ausjauchzt, aussingt.", "tokens": ["Der", "lei\u00b7den\u00b7de", ",", "aus\u00b7klagt", ",", "aus\u00b7jauchzt", ",", "aus\u00b7singt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVPP", "$,", "VVFIN", "$,", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.11": {"text": "Und sorgsam lauschend schnitt er Rohr auf Rohr", "tokens": ["Und", "sorg\u00b7sam", "lau\u00b7schend", "schnitt", "er", "Rohr", "auf", "Rohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJD", "VVFIN", "PPER", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Sich aus dem gro\u00dfen, schwanken Bambuswald", "tokens": ["Sich", "aus", "dem", "gro\u00b7\u00dfen", ",", "schwan\u00b7ken", "Bam\u00b7bus\u00b7wald"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Und stimmte sie genau: sechs nach dem Rauschen", "tokens": ["Und", "stimm\u00b7te", "sie", "ge\u00b7nau", ":", "sechs", "nach", "dem", "Rau\u00b7schen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "CARD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Des Stroms, des R\u00f6hrichts und der wilden B\u00e4ume,", "tokens": ["Des", "Stroms", ",", "des", "R\u00f6h\u00b7richts", "und", "