{"dta.poem.2586": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Berg-Reyhen  \n  Auf Hn. C. W. u. F. A. S. V. g. H. Hoch-  \n zeit den 29. Apr. 1680.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Wei\u00df er hat auf seinen Zechen", "tokens": ["Herr", "Wei\u00df", "er", "hat", "auf", "sei\u00b7nen", "Ze\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So manchen guten Gux erlangt;", "tokens": ["So", "man\u00b7chen", "gu\u00b7ten", "Gux", "er\u00b7langt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "ADJA", "NE", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wei\u00df wie man das Ertz sol brechen", "tokens": ["Und", "wei\u00df", "wie", "man", "das", "Ertz", "sol", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "PIS", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das in der Menschen Augen prangt/", "tokens": ["Das", "in", "der", "Men\u00b7schen", "Au\u00b7gen", "prangt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "L\u00e4st er nun Fahrt und Stollen stehen/", "tokens": ["L\u00e4st", "er", "nun", "Fahrt", "und", "Stol\u00b7len", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "NN", "KON", "NN", "VVFIN", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Ver\u00e4ndert Vorsatz/ Hertz und Sinn/", "tokens": ["Ver\u00b7\u00e4n\u00b7dert", "Vor\u00b7satz", "/", "Hertz", "und", "Sinn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und wil in Schlesien gar gehen/", "tokens": ["Und", "wil", "in", "Schle\u00b7si\u00b7en", "gar", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "NE", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Was suchet er da vor Gewinn?", "tokens": ["Was", "su\u00b7chet", "er", "da", "vor", "Ge\u00b7winn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er ist ja nicht Bergs\u00fcchtig worden/", "tokens": ["Er", "ist", "ja", "nicht", "Berg\u00b7s\u00fcch\u00b7tig", "wor\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "VAPP", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Das zeigt die lebende Gestalt/", "tokens": ["Das", "zeigt", "die", "le\u00b7ben\u00b7de", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So thut ihm auch der kalte Norden", "tokens": ["So", "thut", "ihm", "auch", "der", "kal\u00b7te", "Nor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Durch die Gewonheit nicht Gewalt.", "tokens": ["Durch", "die", "Ge\u00b7won\u00b7heit", "nicht", "Ge\u00b7walt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKNEG", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Es m\u00fcssen andere Magneten", "tokens": ["Es", "m\u00fcs\u00b7sen", "an\u00b7de\u00b7re", "Mag\u00b7ne\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Jhm seine W\u00fcnschel-Ruthe ziehn/", "tokens": ["Jhm", "sei\u00b7ne", "W\u00fcn\u00b7schel\u00b7Ru\u00b7the", "ziehn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo er wil bauen/ schmeltzen/ l\u00f6then", "tokens": ["Wo", "er", "wil", "bau\u00b7en", "/", "schmelt\u00b7zen", "/", "l\u00f6\u00b7then"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["PWAV", "PPER", "VMFIN", "VVINF", "$(", "VVINF", "$(", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und in gew\u00fcnschter Nahrung bl\u00fchn.", "tokens": ["Und", "in", "ge\u00b7w\u00fcnschter", "Nah\u00b7rung", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "Wie/ oder wil er dem nachsetzen/", "tokens": ["Wie", "/", "o\u00b7der", "wil", "er", "dem", "nach\u00b7set\u00b7zen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$(", "KON", "VMFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie das gemeine Spr\u00fcchwort klang?", "tokens": ["Wie", "das", "ge\u00b7mei\u00b7ne", "Spr\u00fcch\u00b7wort", "klang", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nichts kan den Bergmann mehr ergetzen/", "tokens": ["Nichts", "kan", "den", "Berg\u00b7mann", "mehr", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ein sch\u00f6n Weib und ein sch\u00f6n Gang.", "tokens": ["Als", "ein", "sch\u00f6n", "Weib", "und", "ein", "sch\u00f6n", "Gang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "KON", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und sucht er Fletzen und Geschicke", "tokens": ["Und", "sucht", "er", "Flet\u00b7zen", "und", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bey seiner liebsten ", "tokens": ["Bey", "sei\u00b7ner", "liebs\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "So r\u00fchm ich billich das Gel\u00fccke", "tokens": ["So", "r\u00fchm", "ich", "bil\u00b7lich", "das", "Ge\u00b7l\u00fc\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das ihn gewiesen hat dahin.", "tokens": ["Das", "ihn", "ge\u00b7wie\u00b7sen", "hat", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "VVPP", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Berg-Mann kan nicht immer hauen/", "tokens": ["Ein", "Ber\u00b7gMann", "kan", "nicht", "im\u00b7mer", "hau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er hat auch seine Wechsel-Schicht;", "tokens": ["Er", "hat", "auch", "sei\u00b7ne", "Wech\u00b7sel\u00b7Schicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ist ihm auch vergunt zu schauen", "tokens": ["Es", "ist", "ihm", "auch", "ver\u00b7gunt", "zu", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nach einem klaren Angesicht.", "tokens": ["Nach", "ei\u00b7nem", "kla\u00b7ren", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wann ihn der Berg-M\u00fcnch hat erschrecket", "tokens": ["Wann", "ihn", "der", "Ber\u00b7gM\u00fcnch", "hat", "er\u00b7schre\u00b7cket"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ART", "NN", "VAFIN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und da\u00df er auffgefahren ist/", "tokens": ["Und", "da\u00df", "er", "auff\u00b7ge\u00b7fah\u00b7ren", "ist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So wird ihm neue Krafft erwecket/", "tokens": ["So", "wird", "ihm", "neu\u00b7e", "Krafft", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wenn ihn sein Liebes-Engel k\u00fcst.", "tokens": ["Wenn", "ihn", "sein", "Lie\u00b7bes\u00b7En\u00b7gel", "k\u00fcst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Sein Au\u00dfbeuth bleibt doch Ertz und Liebe", "tokens": ["Sein", "Au\u00df\u00b7beuth", "bleibt", "doch", "Ertz", "und", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mehrerin der gantzen Welt/", "tokens": ["Die", "Meh\u00b7re\u00b7rin", "der", "gant\u00b7zen", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu diesem herrlichen Geschiebe", "tokens": ["Zu", "die\u00b7sem", "herr\u00b7li\u00b7chen", "Ge\u00b7schie\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird aller Nachschlag angestellt.", "tokens": ["Wird", "al\u00b7ler", "Nach\u00b7schlag", "an\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wer seinen Gang bringt so ins streichen/", "tokens": ["Wer", "sei\u00b7nen", "Gang", "bringt", "so", "ins", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "ADV", "APPRART", "ADJA", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als wie ", "tokens": ["Als", "wie"], "token_info": ["word", "word"], "pos": ["KOUS", "KOKOM"], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Der kan die rechte Zeit erreichen/", "tokens": ["Der", "kan", "die", "rech\u00b7te", "Zeit", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und schaut vollkommen alles an.", "tokens": ["Und", "schaut", "voll\u00b7kom\u00b7men", "al\u00b7les", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er findt den Vogel in dem Neste/", "tokens": ["Er", "findt", "den", "Vo\u00b7gel", "in", "dem", "Nes\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Gang entbl\u00f6sset seinen Bauch;", "tokens": ["Der", "Gang", "ent\u00b7bl\u00f6s\u00b7set", "sei\u00b7nen", "Bauch", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Frau Braut der Knapp' ist wol der beste", "tokens": ["Frau", "Braut", "der", "Knapp'", "ist", "wol", "der", "bes\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "VAFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gruben-Compa\u00df braucht er auch/", "tokens": ["Den", "Gru\u00b7ben\u00b7Com\u00b7pa\u00df", "braucht", "er", "auch", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er wird bald muthen und drauff sch\u00fcrffen", "tokens": ["Er", "wird", "bald", "mu\u00b7then", "und", "drauff", "sch\u00fcrf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "VVINF", "KON", "PAV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Best\u00e4tigen und werffen Seil/", "tokens": ["Be\u00b7st\u00e4\u00b7ti\u00b7gen", "und", "werf\u00b7fen", "Seil", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bi\u00df er/ wird gar ansitzen durffen", "tokens": ["Bi\u00df", "er", "/", "wird", "gar", "an\u00b7sit\u00b7zen", "durf\u00b7fen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$(", "VAFIN", "ADV", "VVINF", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Fluth verschroten in der Eil.", "tokens": ["Die", "Fluth", "ver\u00b7schro\u00b7ten", "in", "der", "Eil", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}