{"textgrid.poem.55207": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rat", "genre": "verse", "period": "N.A.", "pub_year": 1774, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Geschieht wohl, da\u00df man einen Tag", "tokens": ["Ge\u00b7schieht", "wohl", ",", "da\u00df", "man", "ei\u00b7nen", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weder sich noch andre leiden mag,", "tokens": ["We\u00b7der", "sich", "noch", "and\u00b7re", "lei\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Will nichts dir nach dem Herzen ein;", "tokens": ["Will", "nichts", "dir", "nach", "dem", "Her\u00b7zen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sollt's in der Kunst wohl anders sein?", "tokens": ["Sollt's", "in", "der", "Kunst", "wohl", "an\u00b7ders", "sein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "ADV", "ADV", "VAINF", "$."], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Drum hetze dich nicht zur schlimmen Zeit,", "tokens": ["Drum", "het\u00b7ze", "dich", "nicht", "zur", "schlim\u00b7men", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PTKNEG", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Denn F\u00fcll und Kraft sind nimmer weit:", "tokens": ["Denn", "F\u00fcll", "und", "Kraft", "sind", "nim\u00b7mer", "weit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Hast in der b\u00f6sen Stund geruht,", "tokens": ["Hast", "in", "der", "b\u00f6\u00b7sen", "Stund", "ge\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ist dir die gute doppelt gut.", "tokens": ["Ist", "dir", "die", "gu\u00b7te", "dop\u00b7pelt", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geschieht wohl, da\u00df man einen Tag", "tokens": ["Ge\u00b7schieht", "wohl", ",", "da\u00df", "man", "ei\u00b7nen", "Tag"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weder sich noch andre leiden mag,", "tokens": ["We\u00b7der", "sich", "noch", "and\u00b7re", "lei\u00b7den", "mag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Will nichts dir nach dem Herzen ein;", "tokens": ["Will", "nichts", "dir", "nach", "dem", "Her\u00b7zen", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text"