word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der R\u00fcstung dich entkleiden", "tokens": ["Der", "R\u00fcs\u00b7tung", "dich", "ent\u00b7klei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bau'n dein frommes Feld.", "tokens": ["Und", "bau'n", "dein", "from\u00b7mes", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du wirkst im stillen Schaffen,", "tokens": ["Du", "wirkst", "im", "stil\u00b7len", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie in dem L\u00e4rm der Waffen,", "tokens": ["Wie", "in", "dem", "L\u00e4rm", "der", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr's Vaterland ein Held.", "tokens": ["F\u00fcr's", "Va\u00b7ter\u00b7land", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}