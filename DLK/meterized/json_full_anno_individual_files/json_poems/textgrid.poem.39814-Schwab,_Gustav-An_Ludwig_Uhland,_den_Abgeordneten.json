{"textgrid.poem.39814": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "An Ludwig Uhland, den Abgeordneten", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die grauen B\u00fccher melden", "tokens": ["Die", "grau\u00b7en", "B\u00fc\u00b7cher", "mel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von einem R\u00f6merhelden,", "tokens": ["Von", "ei\u00b7nem", "R\u00f6\u00b7mer\u00b7hel\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den man bei'm Pfluge fand;", "tokens": ["Den", "man", "bei'm", "Pflu\u00b7ge", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er hatte l\u00e4ngst vergessen,", "tokens": ["Er", "hat\u00b7te", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da\u00df er im Rath gesessen,", "tokens": ["Da\u00df", "er", "im", "Rath", "ge\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df vorn im Feld er stand.", "tokens": ["Da\u00df", "vorn", "im", "Feld", "er", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Den rief der B\u00fcrger Wille", "tokens": ["Den", "rief", "der", "B\u00fcr\u00b7ger", "Wil\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von seines Tagwerks Stille", "tokens": ["Von", "sei\u00b7nes", "Tag\u00b7werks", "Stil\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zur\u00fcck zu neuer That.", "tokens": ["Zu\u00b7r\u00fcck", "zu", "neu\u00b7er", "That", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der Wunsch ging ihm zu Herzen,", "tokens": ["Der", "Wunsch", "ging", "ihm", "zu", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er trennte sich mit Schmerzen", "tokens": ["Er", "trenn\u00b7te", "sich", "mit", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von seiner jungen Saat.", "tokens": ["Von", "sei\u00b7ner", "jun\u00b7gen", "Saat", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Doch auf der V\u00e4ter Sitze,", "tokens": ["Doch", "auf", "der", "V\u00e4\u00b7ter", "Sit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch bei der Schwerter Blitze", "tokens": ["Doch", "bei", "der", "Schwer\u00b7ter", "Blit\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Kam ihm der gute Mut;", "tokens": ["Kam", "ihm", "der", "gu\u00b7te", "Mut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Wo Freiheitskampf erschallte,", "tokens": ["Wo", "Frei\u00b7heits\u00b7kampf", "er\u00b7schall\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da war er ganz der Alte,", "tokens": ["Da", "war", "er", "ganz", "der", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das treue Heldenblut.", "tokens": ["Das", "treu\u00b7e", "Hel\u00b7den\u00b7blut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Auch du, erprobter Streiter,", "tokens": ["Auch", "du", ",", "er\u00b7prob\u00b7ter", "Strei\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du gingest still und heiter", "tokens": ["Du", "gin\u00b7gest", "still", "und", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Schon lange hinter'm Pflug,", "tokens": ["Schon", "lan\u00b7ge", "hin\u00b7ter'm", "Pflug", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du s\u00e4etest Gedichte,", "tokens": ["Du", "s\u00e4e\u00b7test", "Ge\u00b7dich\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Du bautest die Geschichte,", "tokens": ["Du", "bau\u00b7test", "die", "Ge\u00b7schich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Da\u00df frische Frucht sie trug.", "tokens": ["Da\u00df", "fri\u00b7sche", "Frucht", "sie", "trug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun wieder St\u00fcrme drohten,", "tokens": ["Nun", "wie\u00b7der", "St\u00fcr\u00b7me", "droh\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da schickte seine Boten", "tokens": ["Da", "schick\u00b7te", "sei\u00b7ne", "Bo\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu dir das Vaterland,", "tokens": ["Zu", "dir", "das", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ab rief es deine St\u00e4rke", "tokens": ["Ab", "rief", "es", "dei\u00b7ne", "St\u00e4r\u00b7ke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "VVFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Vom sanften Friedenswerke", "tokens": ["Vom", "sanf\u00b7ten", "Frie\u00b7dens\u00b7wer\u00b7ke"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und heischte deine Hand.", "tokens": ["Und", "heischte", "dei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.6": {"line.1": {"text": "Das tapfre Volk der Schwaben", "tokens": ["Das", "tapf\u00b7re", "Volk", "der", "Schwa\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kennt seiner Dichter Gaben:", "tokens": ["Kennt", "sei\u00b7ner", "Dich\u00b7ter", "Ga\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sind ja Tr\u00e4umer nicht!", "tokens": ["Sie", "sind", "ja", "Tr\u00e4u\u00b7mer", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Es griff ihr mutig Streben", "tokens": ["Es", "griff", "ihr", "mu\u00b7tig", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von jeher ein ins Leben,", "tokens": ["Von", "je\u00b7her", "ein", "ins", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ART", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wenn Leben ward Gedicht.", "tokens": ["Wenn", "Le\u00b7ben", "ward", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auch kommst du mit Genossen,", "tokens": ["Auch", "kommst", "du", "mit", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die tragen, k\u00fchn entsprossen,", "tokens": ["Die", "tra\u00b7gen", ",", "k\u00fchn", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den Lorbeer schon im Haar.", "tokens": ["Den", "Lor\u00b7beer", "schon", "im", "Haar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und mit des Liedes Meistern", "tokens": ["Und", "mit", "des", "Lie\u00b7des", "Meis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Eint sich von freien Geistern", "tokens": ["Eint", "sich", "von", "frei\u00b7en", "Geis\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.6": {"text": "Die dicht geschlossne Schaar.", "tokens": ["Die", "dicht", "ge\u00b7schloss\u00b7ne", "Schaar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du bringst uns gute Waffen,", "tokens": ["Du", "bringst", "uns", "gu\u00b7te", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Zum edlen Kampf geschaffen,", "tokens": ["Zum", "ed\u00b7len", "Kampf", "ge\u00b7schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Rechts, des Freimuts Hort.", "tokens": ["Des", "Rechts", ",", "des", "Frei\u00b7muts", "Hort", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und von dem Dienst der Musen", "tokens": ["Und", "von", "dem", "Dienst", "der", "Mu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Bringst du den warmen Busen,", "tokens": ["Bringst", "du", "den", "war\u00b7men", "Bu\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das w\u00fcrdevolle Wort.", "tokens": ["Das", "w\u00fcr\u00b7de\u00b7vol\u00b7le", "Wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Des Kampfes Ziel ist Frieden,", "tokens": ["Des", "Kamp\u00b7fes", "Ziel", "ist", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der Sieg, der uns beschieden,", "tokens": ["Der", "Sieg", ",", "der", "uns", "be\u00b7schie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sei Volks- und F\u00fcrsten-Heil.", "tokens": ["Sei", "Volks", "und", "F\u00fcrs\u00b7ten\u00b7Heil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du hast, wenn wir's erschwingen,", "tokens": ["Du", "hast", ",", "wenn", "wir's", "er\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit M\u00e4\u00dfigung erringen,", "tokens": ["Mit", "M\u00e4\u00b7\u00dfi\u00b7gung", "er\u00b7rin\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Daran dein gutes Theil.", "tokens": ["Da\u00b7ran", "dein", "gu\u00b7tes", "Theil", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Dann magst du wieder scheiden,", "tokens": ["Dann", "magst", "du", "wie\u00b7der", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der R\u00fcstung dich entkleiden", "tokens": ["Der", "R\u00fcs\u00b7tung", "dich", "ent\u00b7klei\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bau'n dein frommes Feld.", "tokens": ["Und", "bau'n", "dein", "from\u00b7mes", "Feld", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du wirkst im stillen Schaffen,", "tokens": ["Du", "wirkst", "im", "stil\u00b7len", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie in dem L\u00e4rm der Waffen,", "tokens": ["Wie", "in", "dem", "L\u00e4rm", "der", "Waf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr's Vaterland ein Held.", "tokens": ["F\u00fcr's", "Va\u00b7ter\u00b7land", "ein", "Held", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Die grauen B\u00fccher melden", "tokens": ["Die", "grau\u00b7en", "B\u00fc\u00b7cher", "mel\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Von einem R\u00f6merhelden,", "tokens": ["Von", "ei\u00b7nem", "R\u00f6\u00b7mer\u00b7hel\u00b7den", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Den man bei'm Pfluge fand;", "tokens": ["Den", "man", "bei'm", "Pflu\u00b7ge", "fand", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er hatte l\u00e4ngst vergessen,", "tokens": ["Er", "hat\u00b7te", "l\u00e4ngst", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tr