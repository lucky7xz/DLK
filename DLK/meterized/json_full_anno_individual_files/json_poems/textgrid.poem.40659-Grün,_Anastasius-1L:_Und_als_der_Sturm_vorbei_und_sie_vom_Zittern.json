{"textgrid.poem.40659": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Und als der Sturm vorbei und sie vom Zittern", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und als der Sturm vorbei und sie vom Zittern", "tokens": ["Und", "als", "der", "Sturm", "vor\u00b7bei", "und", "sie", "vom", "Zit\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PTKVZ", "KON", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Genesen, da erstarkten sie zum Schm\u00e4hen,", "tokens": ["Ge\u00b7ne\u00b7sen", ",", "da", "er\u00b7stark\u00b7ten", "sie", "zum", "Schm\u00e4\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADV", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und dich und uns, die ihn vorausgesehen,", "tokens": ["Und", "dich", "und", "uns", ",", "die", "ihn", "vor\u00b7aus\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KON", "PPER", "$,", "PRELS", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Urheber schalten sie von den Gewittern. \u2013", "tokens": ["Ur\u00b7he\u00b7ber", "schal\u00b7ten", "sie", "von", "den", "Ge\u00b7wit\u00b7tern", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}, "stanza.2": {"line.1": {"text": "Sturmvogel warnt bevor die Masten knittern;", "tokens": ["Sturm\u00b7vo\u00b7gel", "warnt", "be\u00b7vor", "die", "Mas\u00b7ten", "knit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Er wei\u00df: der Seemann wird den Ruf verstehen,", "tokens": ["Er", "wei\u00df", ":", "der", "See\u00b7mann", "wird", "den", "Ruf", "ver\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "ART", "NN", "VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sich r\u00fcsten, treu nach Tau und Segel sp\u00e4hen,", "tokens": ["Sich", "r\u00fcs\u00b7ten", ",", "treu", "nach", "Tau", "und", "Se\u00b7gel", "sp\u00e4\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "ADJD", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df der Orkan sein Schiff nicht schlag' zu Splittern.", "tokens": ["Da\u00df", "der", "Or\u00b7kan", "sein", "Schiff", "nicht", "schlag'", "zu", "Split\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPOSAT", "NN", "PTKNEG", "VVFIN", "APPR", "NN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Und wollt' ein Bube oder Fremdling wagen", "tokens": ["Und", "wollt'", "ein", "Bu\u00b7be", "o\u00b7der", "Fremd\u00b7ling", "wa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ART", "NN", "KON", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den Vogel mit dem Feuerrohr zu messen,", "tokens": ["Den", "Vo\u00b7gel", "mit", "dem", "Feu\u0