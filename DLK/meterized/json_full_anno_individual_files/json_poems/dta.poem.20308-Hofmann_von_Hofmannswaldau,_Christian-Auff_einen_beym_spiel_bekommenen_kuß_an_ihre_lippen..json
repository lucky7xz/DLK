{"dta.poem.20308": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Auff einen beym spiel bekommenen  \n ku\u00df an ihre lippen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Lebt wohl! der himmel will/ ihr lippen/ da\u00df ich scheide!", "tokens": ["Lebt", "wohl", "!", "der", "him\u00b7mel", "will", "/", "ihr", "lip\u00b7pen", "/", "da\u00df", "ich", "schei\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$.", "ART", "NN", "VMFIN", "$(", "PPOSAT", "NN", "$(", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ihr habt durch einen ku\u00df das hertze mir entf\u00fchrt/", "tokens": ["Ihr", "habt", "durch", "ei\u00b7nen", "ku\u00df", "das", "hert\u00b7ze", "mir", "ent\u00b7f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In meinen wangen wird kein leben mehr gesp\u00fcrt/", "tokens": ["In", "mei\u00b7nen", "wan\u00b7gen", "wird", "kein", "le\u00b7ben", "mehr", "ge\u00b7sp\u00fcrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PIAT", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mein mund steht gantz erbla\u00dft/ und weisser noch als kreide/", "tokens": ["Mein", "mund", "steht", "gantz", "er\u00b7bla\u00dft", "/", "und", "weis\u00b7ser", "noch", "als", "krei\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "VVPP", "$(", "KON", "ADJA", "ADV", "KOKOM", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die augen brechen mir/ sie kennen keine freude/", "tokens": ["Die", "au\u00b7gen", "bre\u00b7chen", "mir", "/", "sie", "ken\u00b7nen", "kei\u00b7ne", "freu\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil nichts als finsterni\u00df in ihnen sich gebiehrt;", "tokens": ["Weil", "nichts", "als", "fins\u00b7ter\u00b7ni\u00df", "in", "ih\u00b7nen", "sich", "ge\u00b7biehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOKOM", "VVFIN", "APPR", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die seele wird durch angst und bleiche pein ger\u00fchrt/", "tokens": ["Die", "see\u00b7le", "wird", "durch", "angst", "und", "blei\u00b7che", "pein", "ge\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und zeigt/ da\u00df ich den tod in allen gliedern leide.", "tokens": ["Und", "zeigt", "/", "da\u00df", "ich", "den", "tod", "in", "al\u00b7len", "glie\u00b7dern", "lei\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sie stirbet/ sie vergeht auff eures mundes pracht/", "tokens": ["Sie", "stir\u00b7bet", "/", "sie", "ver\u00b7geht", "auff", "eu\u00b7res", "mun\u00b7des", "pracht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sie liegt in letzter noth/ sie rufft schon: Gute nacht!", "tokens": ["Sie", "liegt", "in", "letz\u00b7ter", "noth", "/", "sie", "rufft", "schon", ":", "Gu\u00b7te", "nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$(", "PPER", "VVFIN", "ADV", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Hilff/ Venus/ wo du kanst/ doch ihre wahlstatt ehren/", "tokens": ["Hilff", "/", "Ve\u00b7nus", "/", "wo", "du", "kanst", "/", "doch", "ih\u00b7re", "wahl\u00b7statt", "eh\u00b7ren", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "NE", "$(", "PWAV", "PPER", "VMFIN", "$(", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "La\u00df um ihr s\u00fcsses grab stets frische rosen bl\u00fchn.", "tokens": ["La\u00df", "um", "ihr", "s\u00fcs\u00b7ses", "grab", "stets", "fri\u00b7sche", "ro\u00b7sen", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PPOSAT", "ADJA", "NN", "ADV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ach aber Solime! la\u00df sie doch nicht zu k\u00fchn", "tokens": ["Ach", "a\u00b7ber", "So\u00b7li\u00b7me", "!", "la\u00df", "sie", "doch", "nicht", "zu", "k\u00fchn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ADV", "NN", "$.", "VVIMP", "PPER", "ADV", "PTKNEG", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch einen fremden ku\u00df in ihrer liebe st\u00f6hren.", "tokens": ["Durch", "ei\u00b7nen", "frem\u00b7den", "ku\u00df", "in", "ih\u00b7rer", "lie\u00b7be", "st\u00f6h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}