{"dta.poem.23387": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Eine himmlische Etymologie.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eein gro\u00dfer Teufel ist schon Gog,", "tokens": ["\u201e", "ein", "gro\u00b7\u00dfer", "Teu\u00b7fel", "ist", "schon", "Gog", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VAFIN", "ADV", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ma-Gog ist ein viel gr\u00f6\u00dfrer noch.", "tokens": ["Ma\u00b7\u00b7Gog", "ist", "ein", "viel", "gr\u00f6\u00df\u00b7rer", "noch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "PIAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was aber ist der De-Ma-Gog,", "tokens": ["Was", "a\u00b7ber", "ist", "der", "De\u00b7Ma\u00b7\u00b7Gog", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das ist der allergr\u00f6\u00dfte doch.\u201c", "tokens": ["Das", "ist", "der", "al\u00b7ler\u00b7gr\u00f6\u00df\u00b7te", "doch", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VAFIN", "ART", "ADJA", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "So sprach dereinst der Engel Mund,", "tokens": ["So", "sprach", "de\u00b7reinst", "der", "En\u00b7gel", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und das vernahm der deutsche Bund,", "tokens": ["Und", "das", "ver\u00b7nahm", "der", "deut\u00b7sche", "Bund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der machte schnell den Engelsfund", "tokens": ["Der", "mach\u00b7te", "schnell", "den", "En\u00b7gels\u00b7fund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns armen, armen Teufeln kund.", "tokens": ["Uns", "ar\u00b7men", ",", "ar\u00b7men", "Teu\u00b7feln", "kund", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "$,", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}