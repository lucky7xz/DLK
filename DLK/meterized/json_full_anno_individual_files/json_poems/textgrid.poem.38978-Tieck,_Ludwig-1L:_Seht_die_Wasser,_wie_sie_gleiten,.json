", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Seine Wange abzuk\u00fchlen.", "tokens": ["Sei\u00b7ne", "Wan\u00b7ge", "ab\u00b7zu\u00b7k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Also spiegelt Liebestreue", "tokens": ["Al\u00b7so", "spie\u00b7gelt", "Lie\u00b7be\u00b7streu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im wechselnden Empfinden,", "tokens": ["Sich", "im", "wech\u00b7seln\u00b7den", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie Gef\u00fchle kommen, schwinden,", "tokens": ["Wie", "Ge\u00b7f\u00fch\u00b7le", "kom\u00b7men", ",", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Erinnern baden, neue", "tokens": ["Im", "E\u00b7rin\u00b7nern", "ba\u00b7den", ",", "neu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich vermischen in die Reihe,", "tokens": ["Sich", "ver\u00b7mi\u00b7schen", "in", "die", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandeln vor und gern zur\u00fcck,", "tokens": ["Wan\u00b7deln", "vor", "und", "gern", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch der innerlichste Blick", "tokens": ["Doch", "der", "in\u00b7ner\u00b7lichs\u00b7te", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht Gestalten fortgeschwommen,", "tokens": ["Sieht", "Ge\u00b7stal\u00b7ten", "fort\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die andern nahe kommen,", "tokens": ["Und", "die", "an\u00b7dern", "na\u00b7he", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und in allen nur Ein Gl\u00fcck.", "tokens": ["Und", "in", "al\u00b7len", "nur", "Ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Darum wechselt nur Gedanken,", "tokens": ["Da\u00b7rum", "wech\u00b7selt", "nur", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie ihr wandelt in Gestalten,", "tokens": ["Wie", "ihr", "wan\u00b7delt", "in", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df ich eins doch fest zu halten", "tokens": ["Wei\u00df", "ich", "eins", "doch", "fest", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Wandel, ohne Wanken.", "tokens": ["Oh\u00b7ne", "Wan\u00b7del", ",", "oh\u00b7ne", "Wan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn nie darf der Glaub' erkranken,", "tokens": ["Denn", "nie", "darf", "der", "Glaub'", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Glaube ist das Element,", "tokens": ["Glau\u00b7be", "ist", "das", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "In dem nur die Liebe brennt.", "tokens": ["In", "dem", "nur", "die", "Lie\u00b7be", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und des Herzens reinste Bl\u00e4ue", "tokens": ["Und", "des", "Her\u00b7zens", "reins\u00b7te", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kl\u00e4rt sich hell und heller, Treue", "tokens": ["Kl\u00e4rt", "sich", "hell", "und", "hel\u00b7ler", ",", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PRF", "ADJD", "KON", "ADJD", "$,", "NN"], "meter": "+-+-+