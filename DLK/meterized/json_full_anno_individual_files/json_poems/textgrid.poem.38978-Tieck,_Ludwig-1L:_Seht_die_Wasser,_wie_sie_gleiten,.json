{"textgrid.poem.38978": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Seht die Wasser, wie sie gleiten,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Seht die Wasser, wie sie gleiten,", "tokens": ["Seht", "die", "Was\u00b7ser", ",", "wie", "sie", "glei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sich in der Fluth die B\u00e4ume", "tokens": ["Und", "sich", "in", "der", "Fluth", "die", "B\u00e4u\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PRF", "APPR", "ART", "NN", "ART", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.3": {"text": "Still beschauen, goldne Tr\u00e4ume", "tokens": ["Still", "be\u00b7schau\u00b7en", ",", "gold\u00b7ne", "Tr\u00e4u\u00b7me"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVINF", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seh' ich durch die Wolken schreiten.", "tokens": ["Seh'", "ich", "durch", "die", "Wol\u00b7ken", "schrei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Wie die Wogen ringend streiten,", "tokens": ["Wie", "die", "Wo\u00b7gen", "rin\u00b7gend", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sich entfliehen und vereinen,", "tokens": ["Sich", "ent\u00b7flie\u00b7hen", "und", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "KON", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Spielen mit den Widerscheinen,", "tokens": ["Spie\u00b7len", "mit", "den", "Wi\u00b7der\u00b7schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Blumen roth und gold", "tokens": ["Und", "die", "Blu\u00b7men", "roth", "und", "gold"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.9": {"text": "Sich bespiegeln, und so hold", "tokens": ["Sich", "be\u00b7spie\u00b7geln", ",", "und", "so", "hold"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "VVINF", "$,", "KON", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Thau in diese Wellen weinen!", "tokens": ["Thau", "in", "die\u00b7se", "Wel\u00b7len", "wei\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Sieh, es ist ein Liebesringen,", "tokens": ["Sieh", ",", "es", "ist", "ein", "Lie\u00b7bes\u00b7rin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Welle hascht die fl\u00fccht'ge Welle", "tokens": ["Wel\u00b7le", "hascht", "die", "fl\u00fccht'\u00b7ge", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sie lacht so fr\u00f6hlich, helle,", "tokens": ["Und", "sie", "lacht", "so", "fr\u00f6h\u00b7lich", ",", "hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gl\u00e4nzend sie sich all verschlingen,", "tokens": ["Gl\u00e4n\u00b7zend", "sie", "sich", "all", "ver\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PRF", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Alle liebend sich durchdringen,", "tokens": ["Al\u00b7le", "lie\u00b7bend", "sich", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Im Erg\u00f6tzen lieblich spielen;", "tokens": ["Im", "Er\u00b7g\u00f6t\u00b7zen", "lieb\u00b7lich", "spie\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wie sie durch einander w\u00fchlen", "tokens": ["Wie", "sie", "durch", "ein\u00b7an\u00b7der", "w\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PRF", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Scheint der reine blaue Himmel", "tokens": ["Scheint", "der", "rei\u00b7ne", "blau\u00b7e", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "In das h\u00fcpfende Get\u00fcmmel,", "tokens": ["In", "das", "h\u00fcp\u00b7fen\u00b7de", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+---+-", "measure": "unknown.measure.tri"}, "line.10": {"text": "Seine Wange abzuk\u00fchlen.", "tokens": ["Sei\u00b7ne", "Wan\u00b7ge", "ab\u00b7zu\u00b7k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Also spiegelt Liebestreue", "tokens": ["Al\u00b7so", "spie\u00b7gelt", "Lie\u00b7be\u00b7streu\u00b7e"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich im wechselnden Empfinden,", "tokens": ["Sich", "im", "wech\u00b7seln\u00b7den", "Emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie Gef\u00fchle kommen, schwinden,", "tokens": ["Wie", "Ge\u00b7f\u00fch\u00b7le", "kom\u00b7men", ",", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "NN", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Im Erinnern baden, neue", "tokens": ["Im", "E\u00b7rin\u00b7nern", "ba\u00b7den", ",", "neu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPRART", "NN", "VVINF", "$,", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich vermischen in die Reihe,", "tokens": ["Sich", "ver\u00b7mi\u00b7schen", "in", "die", "Rei\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wandeln vor und gern zur\u00fcck,", "tokens": ["Wan\u00b7deln", "vor", "und", "gern", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Doch der innerlichste Blick", "tokens": ["Doch", "der", "in\u00b7ner\u00b7lichs\u00b7te", "Blick"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sieht Gestalten fortgeschwommen,", "tokens": ["Sieht", "Ge\u00b7stal\u00b7ten", "fort\u00b7ge\u00b7schwom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Und die andern nahe kommen,", "tokens": ["Und", "die", "an\u00b7dern", "na\u00b7he", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und in allen nur Ein Gl\u00fcck.", "tokens": ["Und", "in", "al\u00b7len", "nur", "Ein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Darum wechselt nur Gedanken,", "tokens": ["Da\u00b7rum", "wech\u00b7selt", "nur", "Ge\u00b7dan\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ADV", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Wie ihr wandelt in Gestalten,", "tokens": ["Wie", "ihr", "wan\u00b7delt", "in", "Ge\u00b7stal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wei\u00df ich eins doch fest zu halten", "tokens": ["Wei\u00df", "ich", "eins", "doch", "fest", "zu", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Wandel, ohne Wanken.", "tokens": ["Oh\u00b7ne", "Wan\u00b7del", ",", "oh\u00b7ne", "Wan\u00b7ken", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn nie darf der Glaub' erkranken,", "tokens": ["Denn", "nie", "darf", "der", "Glaub'", "er\u00b7kran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Glaube ist das Element,", "tokens": ["Glau\u00b7be", "ist", "das", "E\u00b7le\u00b7ment", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+---+", "measure": "unknown.measure.tri"}, "line.7": {"text": "In dem nur die Liebe brennt.", "tokens": ["In", "dem", "nur", "die", "Lie\u00b7be", "brennt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und des Herzens reinste Bl\u00e4ue", "tokens": ["Und", "des", "Her\u00b7zens", "reins\u00b7te", "Bl\u00e4u\u00b7e"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Kl\u00e4rt sich hell und heller, Treue", "tokens": ["Kl\u00e4rt", "sich", "hell", "und", "hel\u00b7ler", ",", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["NE", "PRF", "ADJD", "KON", "ADJD", "$,", "NN"], "meter": "+-+-+-+-", "measur