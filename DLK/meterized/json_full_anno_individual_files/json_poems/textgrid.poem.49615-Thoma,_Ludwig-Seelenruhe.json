{"textgrid.poem.49615": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Seelenruhe", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Fr\u00fchling stimmt mir heiter das Gem\u00fct;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "stimmt", "mir", "hei\u00b7ter", "das", "Ge\u00b7m\u00fct", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich schreite lustig in den frischen Morgen.", "tokens": ["Ich", "schrei\u00b7te", "lus\u00b7tig", "in", "den", "fri\u00b7schen", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht so wie sonst ist gallig mein Gebl\u00fct,", "tokens": ["Nicht", "so", "wie", "sonst", "ist", "gal\u00b7lig", "mein", "Ge\u00b7bl\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ADV", "VAFIN", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und wenig oder nichts erregt mir Sorgen.", "tokens": ["Und", "we\u00b7nig", "o\u00b7der", "nichts", "er\u00b7regt", "mir", "Sor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KON", "PIS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die bunten Blumen und der klare Flu\u00df,", "tokens": ["Die", "bun\u00b7ten", "Blu\u00b7men", "und", "der", "kla\u00b7re", "Flu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ich freu' mich an den sch\u00f6nen Dingen,", "tokens": ["Ich", "freu'", "mich", "an", "den", "sch\u00f6\u00b7nen", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als edler Mensch empfind' ich den Genu\u00df,", "tokens": ["Als", "ed\u00b7ler", "Mensch", "emp\u00b7find'", "ich", "den", "Ge\u00b7nu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Den h\u00fcbsch geleg'ne Orte mit sich bringen.", "tokens": ["Den", "h\u00fcbsch", "ge\u00b7leg'\u00b7ne", "Or\u00b7te", "mit", "sich", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wie kann ich schw\u00e4rmen f\u00fcr Natur und Kunst!", "tokens": ["Wie", "kann", "ich", "schw\u00e4r\u00b7men", "f\u00fcr", "Na\u00b7tur", "und", "Kunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es gibt nichts Besseres dahier auf Erden.", "tokens": ["Es", "gibt", "nichts", "Bes\u00b7se\u00b7res", "da\u00b7hier", "auf", "Er\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PIS", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was meinen Sie? Wem fehlt der blasse Dunst?", "tokens": ["Was", "mei\u00b7nen", "Sie", "?", "Wem", "fehlt", "der", "blas\u00b7se", "Dunst", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PWS", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie m\u00fcssen immer doch politisch werden!", "tokens": ["Sie", "m\u00fcs\u00b7sen", "im\u00b7mer", "doch", "po\u00b7li\u00b7tisch", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch ganz umsonst! Mich bringt heut nichts in Wut,", "tokens": ["Doch", "ganz", "um\u00b7sonst", "!", "Mich", "bringt", "heut", "nichts", "in", "Wut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$.", "PPER", "VVFIN", "ADV", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was auch ein bayrischer Minister sage!", "tokens": ["Was", "auch", "ein", "bay\u00b7ri\u00b7scher", "Mi\u00b7nis\u00b7ter", "sa\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Leutchen meinen es am Ende gut,", "tokens": ["Die", "Leut\u00b7chen", "mei\u00b7nen", "es", "am", "En\u00b7de", "gut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wie sie's versteh'n, ist eine and're Frage.", "tokens": ["Wie", "sie's", "ver\u00b7steh'n", ",", "ist", "ei\u00b7ne", "an\u00b7d'\u00b7re", "Fra\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVINF", "$,", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "Ich dacht' von ihnen stets nur so la la,", "tokens": ["Ich", "dacht'", "von", "ih\u00b7nen", "stets", "nur", "so", "la", "la", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "ADV", "ADV", "ADV", "FM", "FM", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch heute will ich \u2013 werden Sie's begreifen? \u2013", "tokens": ["Doch", "heu\u00b7te", "will", "ich", "\u2013", "wer\u00b7den", "Sie's", "be\u00b7grei\u00b7fen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "$(", "VAFIN", "PIS", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf Paragraph Einhundertvierundachtzig a", "tokens": ["Auf", "Pa\u00b7ra\u00b7gra\u00b7ph", "Ein\u00b7hun\u00b7dert\u00b7vie\u00b7run\u00b7dacht\u00b7zig", "a"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "NE"], "meter": "-+----+-+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "Und sonst noch wen und was vergn\u00fcglich pfeifen.", "tokens": ["Und", "sonst", "noch", "wen", "und", "was", "ver\u00b7gn\u00fcg\u00b7lich", "pfei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PWS", "KON", "PWS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Der Fr\u00fchling stimmt mir heiter das Gem\u00fct;", "tokens