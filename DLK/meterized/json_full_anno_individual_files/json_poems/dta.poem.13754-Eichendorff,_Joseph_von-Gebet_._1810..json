{"dta.poem.13754": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Gebet   .  \n 1810.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was soll ich, auf Gott nur bauend,", "tokens": ["Was", "soll", "ich", ",", "auf", "Gott", "nur", "bau\u00b7end", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "$,", "APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Schlechter sein, als all' die Andern,", "tokens": ["Schlech\u00b7ter", "sein", ",", "als", "all'", "die", "An\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAINF", "$,", "KOUS", "PIS", "ART", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die, so wohlbehaglich schauend,", "tokens": ["Die", ",", "so", "wohl\u00b7be\u00b7hag\u00b7lich", "schau\u00b7end", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Froh dem eigenen Nichts vertrauend,", "tokens": ["Froh", "dem", "ei\u00b7ge\u00b7nen", "Nichts", "ver\u00b7trau\u00b7end", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "Die gemeine Stra\u00dfe wandern?", "tokens": ["Die", "ge\u00b7mei\u00b7ne", "Stra\u00b7\u00dfe", "wan\u00b7dern", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}