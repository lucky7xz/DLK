{"textgrid.poem.55314": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Dauer im Wechsel", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hielte diesen fr\u00fchen Segen,", "tokens": ["Hiel\u00b7te", "die\u00b7sen", "fr\u00fc\u00b7hen", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ach, nur ", "tokens": ["Ach", ",", "nur"], "token_info": ["word", "punct", "word"], "pos": ["ITJ", "$,", "ADV"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Aber vollen Bl\u00fctenregen", "tokens": ["A\u00b7ber", "vol\u00b7len", "Bl\u00fc\u00b7ten\u00b7re\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00fcttelt schon der laue West.", "tokens": ["Sch\u00fct\u00b7telt", "schon", "der", "lau\u00b7e", "West", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Soll ich mich des Gr\u00fcnen freuen,", "tokens": ["Soll", "ich", "mich", "des", "Gr\u00fc\u00b7nen", "freu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dem ich Schatten erst verdankt?", "tokens": ["Dem", "ich", "Schat\u00b7ten", "erst", "ver\u00b7dankt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Bald wird Sturm auch das zerstreuen,", "tokens": ["Bald", "wird", "Sturm", "auch", "das", "zer\u00b7streu\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "PDS", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wenn es falb im Herbst geschwankt.", "tokens": ["Wenn", "es", "falb", "im", "Herbst", "ge\u00b7schwankt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Willst du nach den Fr\u00fcchten greifen,", "tokens": ["Willst", "du", "nach", "den", "Fr\u00fcch\u00b7ten", "grei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Eilig nimm dein Teil davon!", "tokens": ["Ei\u00b7lig", "nimm", "dein", "Teil", "da\u00b7von", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVIMP", "PPOSAT", "NN", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Diese fangen an zu reifen,", "tokens": ["Die\u00b7se", "fan\u00b7gen", "an", "zu", "rei\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und die andern keimen schon;", "tokens": ["Und", "die", "an\u00b7dern", "kei\u00b7men", "schon", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gleich mit jedem Regengusse", "tokens": ["Gleich", "mit", "je\u00b7dem", "Re\u00b7gen\u00b7gus\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00c4ndert sich dein holdes Tal,", "tokens": ["\u00c4n\u00b7dert", "sich", "dein", "hol\u00b7des", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Ach, und in demselben Flusse", "tokens": ["Ach", ",", "und", "in", "dem\u00b7sel\u00b7ben", "Flus\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KON", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schwimmst du nicht zum zweitenmal.", "tokens": ["Schwimmst", "du", "nicht", "zum", "zwei\u00b7ten\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "APPRART", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Du nun selbst! Was felsenfeste", "tokens": ["Du", "nun", "selbst", "!", "Was", "fel\u00b7sen\u00b7fes\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "ADV", "ADV", "$.", "PWS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich vor dir hervorgetan,", "tokens": ["Sich", "vor", "dir", "her\u00b7vor\u00b7ge\u00b7tan", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mauern siehst du, siehst Pal\u00e4ste", "tokens": ["Mau\u00b7ern", "siehst", "du", ",", "siehst", "Pa\u00b7l\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "$,", "VVFIN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stets mit andern Augen an.", "tokens": ["Stets", "mit", "an\u00b7dern", "Au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Weggeschwunden ist die Lippe,", "tokens": ["Weg\u00b7ge\u00b7schwun\u00b7den", "ist", "die", "Lip\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Die im Kusse sonst genas,", "tokens": ["Die", "im", "Kus\u00b7se", "sonst", "ge\u00b7nas", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Jener Fu\u00df, der an der Klippe", "tokens": ["Je\u00b7ner", "Fu\u00df", ",", "der", "an", "der", "Klip\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich mit Gemsenfreche ma\u00df,", "tokens": ["Sich", "mit", "Gem\u00b7sen\u00b7fre\u00b7che", "ma\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Jene Hand, die gern und milde", "tokens": ["Je\u00b7ne", "Hand", ",", "die", "gern", "und", "mil\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "$,", "PRELS", "ADV", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich bewegte, wohlzutun,", "tokens": ["Sich", "be\u00b7weg\u00b7te", ",", "wohl\u00b7zu\u00b7tun", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PRF", "VVFIN", "$,", "PWAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Das gegliederte Gebilde,", "tokens": ["Das", "ge\u00b7glie\u00b7der\u00b7te", "Ge\u00b7bil\u00b7de", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Alles ist ein andres nun.", "tokens": ["Al\u00b7les", "ist", "ein", "and\u00b7res", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ART", "PIS", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und was sich an jener Stelle", "tokens": ["Und", "was", "sich", "an", "je\u00b7ner", "Stel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "PRF", "APPR", "PDAT", "NN"], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.6": {"text": "Nun mit deinem Namen nennt,", "tokens": ["Nun", "mit", "dei\u00b7nem", "Na\u00b7men", "nennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kam herbei wie eine Welle,", "tokens": ["Kam", "her\u00b7bei", "wie", "ei\u00b7ne", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und so eilt's zum Element.", "tokens": ["Und", "so", "eilt's", "zum", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "La\u00df den Anfang mit dem Ende", "tokens": ["La\u00df", "den", "An\u00b7fang", "mit", "dem", "En\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich in ", "tokens": ["Sich", "in"], "token_info": ["word", "word"], "pos": ["PRF", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Schneller als die Gegenst\u00e4nde", "tokens": ["Schnel\u00b7ler", "als", "die", "Ge\u00b7gen\u00b7st\u00e4n\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selber dich vor\u00fcberfliehn.", "tokens": ["Sel\u00b7ber", "dich", "vor\u00b7\u00fc\u00b7berf\u00b7liehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Danke, da\u00df die Gunst der Musen", "tokens": ["Dan\u00b7ke", ",", "da\u00df", "die", "Gunst", "der", "Mu\u00b7sen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unverg\u00e4ngliches verhei\u00dft,", "tokens": ["Un\u00b7ver\u00b7g\u00e4ng\u00b7li\u00b7ches", "ver\u00b7hei\u00dft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Den Gehalt in deinem Busen", "tokens": ["Den", "Ge\u00b7halt", "in", "dei\u00b7nem", "Bu\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und die Form in deinem Geist.", "tokens": ["Und", "die", "Form", "in", "dei\u00b7nem", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hielte diesen fr\u00fchen Segen,", "tokens": ["Hiel\u00b7te", "die\u00b7sen", "fr\u00fc\u00b7hen