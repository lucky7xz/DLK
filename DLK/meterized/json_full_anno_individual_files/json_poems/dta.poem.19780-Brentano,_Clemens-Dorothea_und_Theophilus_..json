{"dta.poem.19780": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Dorothea und Theophilus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie ein fruchtbarer Regen               ", "tokens": ["Gleich", "wie", "ein", "frucht\u00b7ba\u00b7rer", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "ART", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Ist der Martyrer Blut,", "tokens": ["Ist", "der", "Mar\u00b7ty\u00b7rer", "Blut", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und Frucht durch Gottes Segen", "tokens": ["Und", "Frucht", "durch", "Got\u00b7tes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Reichlicher bringen thut.", "tokens": ["Reich\u00b7li\u00b7cher", "brin\u00b7gen", "thut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Durchs Kreutz die Kirche dringet", "tokens": ["Durchs", "Kreutz", "die", "Kir\u00b7che", "drin\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und w\u00e4chst ohn Unterla\u00df,", "tokens": ["Und", "w\u00e4chst", "ohn", "Un\u00b7ter\u00b7la\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Durch Tod zum Leben ringet,", "tokens": ["Durch", "Tod", "zum", "Le\u00b7ben", "rin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Wer herzlich glaubet das.", "tokens": ["Wer", "herz\u00b7lich", "glau\u00b7bet", "das", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VVFIN", "PDS", "$."], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.9": {"text": "Aus guter Zucht und Namen", "tokens": ["Aus", "gu\u00b7ter", "Zucht", "und", "Na\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Erschwingt sich gute Art,", "tokens": ["Er\u00b7schwingt", "sich", "gu\u00b7te", "Art", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Von Gott die Frommen kamen,", "tokens": ["Von", "Gott", "die", "From\u00b7men", "ka\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Der frommen Kinder wart't.", "tokens": ["Der", "from\u00b7men", "Kin\u00b7der", "wart'", "t."], "token_info": ["word", "word", "word", "word", "abbreviation"], "pos": ["ART", "ADJA", "NN", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Ist Dorothea geboren", "tokens": ["Ist", "Do\u00b7ro\u00b7thea", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "NE", "VVPP"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.14": {"text": "Von Aeltern keusch und rein,", "tokens": ["Von", "A\u00b7el\u00b7tern", "keusch", "und", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "So geht sie nicht verloren,", "tokens": ["So", "geht", "sie", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.16": {"text": "Und bleibt sie auch allein.", "tokens": ["Und", "bleibt", "sie", "auch", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Die Heyden wollten zwingen", "tokens": ["Die", "Hey\u00b7den", "woll\u00b7ten", "zwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Sie zur Abg\u00f6tterey,", "tokens": ["Sie", "zur", "Ab\u00b7g\u00f6t\u00b7te\u00b7rey", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.19": {"text": "Dem Feind wollts nicht gelingen,", "tokens": ["Dem", "Feind", "wollts", "nicht", "ge\u00b7lin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Christum bekannt sie frey,", "tokens": ["Chris\u00b7tum", "be\u00b7kannt", "sie", "frey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Ein Urtheil ward gef\u00e4llet", "tokens": ["Ein", "Ur\u00b7theil", "ward", "ge\u00b7f\u00e4l\u00b7let"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.22": {"text": "Verdient h\u00e4tt sie den Tod,", "tokens": ["Ver\u00b7di\u00b7ent", "h\u00e4tt", "sie", "den", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.23": {"text": "Ritterlich sie sich stellet,", "tokens": ["Rit\u00b7ter\u00b7lich", "sie", "sich", "stel\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-++-+-", "measure": "unknown.measure.tetra"}, "line.24": {"text": "Und schrie ernstlich zu Gott.", "tokens": ["Und", "schrie", "ernst\u00b7lich", "zu", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.25": {"text": "Und Theophil dem Kanzler", "tokens": ["Und", "Theo\u00b7phil", "dem", "Kanz\u00b7ler"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.26": {"text": "Dem jammert die Jungfrau sehr;", "tokens": ["Dem", "jam\u00b7mert", "die", "Jung\u00b7frau", "sehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ADV", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.27": {"text": "Er sprach: O schon dein Leben,", "tokens": ["Er", "sprach", ":", "O", "schon", "dein", "Le\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "NE", "ADV", "PPOSAT", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Verla\u00df die falsche Lehr,", "tokens": ["Ver\u00b7la\u00df", "die", "fal\u00b7sche", "Lehr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "Und frist dein junges Leben!", "tokens": ["Und", "frist", "dein", "jun\u00b7ges", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.30": {"text": "Drauf Dorothea spricht:", "tokens": ["Drauf", "Do\u00b7ro\u00b7thea", "spricht", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PAV", "NE", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.31": {"text": "\u201eein be\u00dfres wird er geben", "tokens": ["\u201e", "ein", "be\u00df\u00b7res", "wird", "er", "ge\u00b7ben"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ART", "ADJA", "VAFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.32": {"text": "\u201eund das vergehet nicht.", "tokens": ["\u201e", "und", "das", "ver\u00b7ge\u00b7het", "nicht", "."], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PDS", "VVFIN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "\u201ezum sch\u00f6nen Paradiese", "tokens": ["\u201e", "zum", "sch\u00f6\u00b7nen", "Pa\u00b7ra\u00b7die\u00b7se"], "token_info": ["punct", "word", "word", "word"], "pos": ["$(", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "\u201ekomm ich nach meinem Tod,", "tokens": ["\u201e", "komm", "ich", "nach", "mei\u00b7nem", "Tod", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "\u201eda\u00df sie sich Christum wiesen,", "tokens": ["\u201e", "da\u00df", "sie", "sich", "Chris\u00b7tum", "wie\u00b7sen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PRF", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.36": {"text": "\u201estehn da viel R\u00f6slein roth,", "tokens": ["\u201e", "stehn", "da", "viel", "R\u00f6s\u00b7lein", "roth", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ADV", "PIAT", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.37": {"text": "\u201edraus wird mir Christ, mein Herre", "tokens": ["\u201e", "draus", "wird", "mir", "Christ", ",", "mein", "Her\u00b7re"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "PAV", "VAFIN", "PPER", "NN", "$,", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.38": {"text": "\u201emachen ein Ehrenkranz,", "tokens": ["\u201e", "ma\u00b7chen", "ein", "Eh\u00b7ren\u00b7kranz", ","], "token_info": ["punct", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.39": {"text": "\u201eder Tod geliebt vielmehre,", "tokens": ["\u201e", "der", "Tod", "ge\u00b7liebt", "viel\u00b7meh\u00b7re", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "\u201eals so ich ging zum Tanz.\u201c", "tokens": ["\u201e", "als", "so", "ich", "ging", "zum", "Tanz", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KOUS", "ADV", "PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Doch Theophil die Rede", "tokens": ["Doch", "Theo\u00b7phil", "die", "Re\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.42": {"text": "Erkl\u00e4rt f\u00fcr lauter Spott,", "tokens": ["Er\u00b7kl\u00e4rt", "f\u00fcr", "lau\u00b7ter", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.43": {"text": "Sprach: Liebe Dorothea,", "tokens": ["Sprach", ":", "Lie\u00b7be", "Do\u00b7ro\u00b7thea", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "NN", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.44": {"text": "Wenn du bey deinem Gott", "tokens": ["Wenn", "du", "bey", "dei\u00b7nem", "Gott"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Schick mir auch Aepfel und Rosen", "tokens": ["Schick", "mir", "auch", "A\u00b7e\u00b7pfel", "und", "Ro\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "NN", "KON", "NN"], "meter": "---+---+-", "measure": "unknown.measure.di"}, "line.46": {"text": "Aus Christi Garten sch\u00f6n! \u2014", "tokens": ["Aus", "Chris\u00b7ti", "Gar\u00b7ten", "sch\u00f6n", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "NE", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.47": {"text": "\u201eja, sprach sie, heilge Rosen", "tokens": ["\u201e", "ja", ",", "sprach", "sie", ",", "heil\u00b7ge", "Ro\u00b7sen"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKANT", "$,", "VVFIN", "PPER", "$,", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.48": {"text": "\u201edie sollst du wahrlich sehn.\u201c", "tokens": ["\u201e", "die", "sollst", "du", "wahr\u00b7lich", "sehn", ".", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.49": {"text": "Das Fr\u00e4ulein war gerichtet,", "tokens": ["Das", "Fr\u00e4u\u00b7lein", "war", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Da klopft es an sein Haus,", "tokens": ["Da", "klopft", "es", "an", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.51": {"text": "Der helle Morgen lichtet,", "tokens": ["Der", "hel\u00b7le", "Mor\u00b7gen", "lich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.52": {"text": "Ein Kn\u00e4blein stehet draus,", "tokens": ["Ein", "Kn\u00e4\u00b7blein", "ste\u00b7het", "draus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.53": {"text": "Geschwingt mit goldnen Fl\u00fcgeln", "tokens": ["Ge\u00b7schwingt", "mit", "gold\u00b7nen", "Fl\u00fc\u00b7geln"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.54": {"text": "Reichts Rosenk\u00f6rbchen dar,", "tokens": ["Reichts", "Ro\u00b7sen\u00b7k\u00f6rb\u00b7chen", "dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.55": {"text": "Verschwindet auf den H\u00fcgeln,", "tokens": ["Ver\u00b7schwin\u00b7det", "auf", "den", "H\u00fc\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.56": {"text": "Von wo es kommen war.", "tokens": ["Von", "wo", "es", "kom\u00b7men", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAV", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.57": {"text": "Und auf den Rosenbl\u00e4ttern", "tokens": ["Und", "auf", "den", "Ro\u00b7sen\u00b7bl\u00e4t\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.58": {"text": "Da steht geschrieben klar:", "tokens": ["Da", "steht", "ge\u00b7schrie\u00b7ben", "klar", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.59": {"text": "\u201emein Christus ist mein Retter,", "tokens": ["\u201e", "mein", "Chris\u00b7tus", "ist", "mein", "Ret\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NE", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.60": {"text": "Und er mir gn\u00e4dig war,", "tokens": ["Und", "er", "mir", "gn\u00e4\u00b7dig", "war", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.61": {"text": "\u201eich leb in Freud und Wonne,", "tokens": ["\u201e", "ich", "leb", "in", "Freud", "und", "Won\u00b7ne", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.62": {"text": "\u201ein ewger Herrlichkeit! \u2014", "tokens": ["\u201e", "in", "ew\u00b7ger", "Herr\u00b7lich\u00b7keit", "!"], "token_info": ["punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.63": {"text": "\u201emein Irrthum ist zerronnen!\u201c", "tokens": ["\u201e", "mein", "Irr\u00b7thum", "ist", "zer\u00b7ron\u00b7nen", "!", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.64": {"text": "Theophilus sagt mit Freud,", "tokens": ["Theo\u00b7phi\u00b7lus", "sagt", "mit", "Freud", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.65": {"text": "Bald fing er an zu preisen", "tokens": ["Bald", "fing", "er", "an", "zu", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.66": {"text": "Dich Christus wahren Gott,", "tokens": ["Dich", "Chris\u00b7tus", "wah\u00b7ren", "Gott", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.67": {"text": "Und lie\u00df sich unterweisen", "tokens": ["Und", "lie\u00df", "sich", "un\u00b7ter\u00b7wei\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.68": {"text": "Wohl in des Herrn Gebot,", "tokens": ["Wohl", "in", "des", "Herrn", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.69": {"text": "Hat heilge Tauf empfangen", "tokens": ["Hat", "heil\u00b7ge", "Tauf", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.70": {"text": "Und Christum frey bekennt,", "tokens": ["Und", "Chris\u00b7tum", "frey", "be\u00b7kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.71": {"text": "Zur Marter ist gegangen", "tokens": ["Zur", "Mar\u00b7ter", "ist", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.72": {"text": "Und mit der Ros verbrennt.", "tokens": ["Und", "mit", "der", "Ros", "ver\u00b7brennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}