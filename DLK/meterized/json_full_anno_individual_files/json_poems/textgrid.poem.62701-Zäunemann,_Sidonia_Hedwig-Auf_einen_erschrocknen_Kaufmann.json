{"textgrid.poem.62701": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen erschrocknen Kaufmann", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "C\u00f6linus sucht jetzt einen neuen Orden,", "tokens": ["C\u00f6\u00b7li\u00b7nus", "sucht", "jetzt", "ei\u00b7nen", "neu\u00b7en", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Denn er ist Commendant zu Bettstadt worden.", "tokens": ["Denn", "er", "ist", "Com\u00b7men\u00b7dant", "zu", "Bett\u00b7stadt", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "APPR", "NN", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was mu\u00df denn wohl die Ursach seyn?", "tokens": ["Was", "mu\u00df", "denn", "wohl", "die", "Ur\u00b7sach", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Creditores wachen auf,", "tokens": ["Die", "Cre\u00b7di\u00b7to\u00b7res", "wa\u00b7chen", "auf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der eine droht ihm mit dem Wechsel-Recht,", "tokens": ["Der", "ei\u00b7ne", "droht", "ihm", "mit", "dem", "Wech\u00b7sel\u00b7Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Der andre aber schw\u00e4cht", "tokens": ["Der", "and\u00b7re", "a\u00b7ber", "schw\u00e4cht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "ADV", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Den vorgenommnen Tausch und Kauf.", "tokens": ["Den", "vor\u00b7ge\u00b7nomm\u00b7nen", "Tausch", "und", "Kauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Drum wei\u00df er auch vor Angst nicht aus und ein,", "tokens": ["Drum", "wei\u00df", "er", "auch", "vor", "Angst", "nicht", "aus", "und", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPR", "NN", "PTKNEG", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "F\u00e4llt auf das Bett, und kriegt den Stein.", "tokens": ["F\u00e4llt", "auf", "das", "Bett", ",", "und", "kriegt", "den", "Stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "C\u00f6linus sucht jetzt einen neuen Orden,", "tokens": ["C\u00f6\u00b7li\u00b7nus", "sucht", "jetzt", "ei\u00b7nen", "neu\u00b7en", "Or\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Denn er ist Commendant zu Bettstadt worden.", "tokens": ["Denn", "er", "ist", "Com\u00b7men\u00b7dant", "zu", "Bett\u00b7stadt", "wor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word"