{"textgrid.poem.65069": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "In den Bergen", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom Gew\u00fchle weg die Schritte lenkend", "tokens": ["Vom", "Ge\u00b7w\u00fch\u00b7le", "weg", "die", "Schrit\u00b7te", "len\u00b7kend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hin zu dem Palast der Alpenfee,", "tokens": ["Hin", "zu", "dem", "Pa\u00b7last", "der", "Al\u00b7pen\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Standen wir auf sonnigstiller H\u00f6h'", "tokens": ["Stan\u00b7den", "wir", "auf", "son\u00b7nigs\u00b7til\u00b7ler", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In den Abgrund uns're Blicke senkend.", "tokens": ["In", "den", "Ab\u00b7grund", "un\u00b7s'\u00b7re", "Bli\u00b7cke", "sen\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Durch die F\u00f6hren ging des Windes Rauschen,", "tokens": ["Durch", "die", "F\u00f6h\u00b7ren", "ging", "des", "Win\u00b7des", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und des Gie\u00dfbach's wildes Brausen drang", "tokens": ["Und", "des", "Gie\u00df\u00b7bach's", "wil\u00b7des", "Brau\u00b7sen", "drang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Mir an's Ohr, gleich einem Geistersang,", "tokens": ["Mir", "an's", "Ohr", ",", "gleich", "ei\u00b7nem", "Geis\u00b7ter\u00b7sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Dem ich todesfreudig mu\u00dfte lauschen.", "tokens": ["Dem", "ich", "to\u00b7des\u00b7freu\u00b7dig", "mu\u00df\u00b7te", "lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Todes", "tokens": ["To\u00b7des"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Denn das Sterben schien mir einzig nur", "tokens": ["Denn", "das", "Ster\u00b7ben", "schien", "mir", "ein\u00b7zig", "nur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VVFIN", "PPER", "ADJD", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Heimkehr, in die Arme der Natur,", "tokens": ["Heim\u00b7kehr", ",", "in", "die", "Ar\u00b7me", "der", "Na\u00b7tur", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Froh Vergeh'n an ihrem Mutterherzen;", "tokens": ["Froh", "Ver\u00b7geh'n", "an", "ih\u00b7rem", "Mut\u00b7ter\u00b7her\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ein Zersprengen nied'rer Sinnesketten", "tokens": ["Ein", "Zer\u00b7spren\u00b7gen", "nie\u00b7d'\u00b7rer", "Sin\u00b7nes\u00b7ket\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.2": {"text": "Und des Geistes eig'ne, freie Wahl", "tokens": ["Und", "des", "Geis\u00b7tes", "eig'\u00b7ne", ",", "frei\u00b7e", "Wahl"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "$,", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Aus des Einzellebens dumpfer Qual", "tokens": ["Aus", "des", "Ein\u00b7zel\u00b7le\u00b7bens", "dum\u00b7pfer", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In das All, das sel'ge, sich zu retten!", "tokens": ["In", "das", "All", ",", "das", "sel'\u00b7ge", ",", "sich", "zu", "ret\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "ADJA", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Bl\u00fch'nder Rosen Wonne zu empfinden,", "tokens": ["Bl\u00fch'n\u00b7der", "Ro\u00b7sen", "Won\u00b7ne", "zu", "emp\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wenn, berauscht von lauer Fr\u00fchlingsluft,", "tokens": ["Wenn", ",", "be\u00b7rauscht", "von", "lau\u00b7er", "Fr\u00fch\u00b7lings\u00b7luft", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Liebestrunken sie den w\u00fcrz'gen Duft", "tokens": ["Lie\u00b7be\u00b7strun\u00b7ken", "sie", "den", "w\u00fcrz'\u00b7gen", "Duft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Ihres Kelchs verstreu'n nach allen Winden;", "tokens": ["Ih\u00b7res", "Kelchs", "ver\u00b7streu'n", "nach", "al\u00b7len", "Win\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Theilzunehmen an dem Freudensegen,", "tokens": ["Theil\u00b7zu\u00b7neh\u00b7men", "an", "dem", "Freu\u00b7den\u00b7se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der sich ahnend durch den Baum ergie\u00dft,", "tokens": ["Der", "sich", "ah\u00b7nend", "durch", "den", "Baum", "er\u00b7gie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "ADJD", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn im Bl\u00fcthenflor, der ihm entsprie\u00dft,", "tokens": ["Wenn", "im", "Bl\u00fc\u00b7then\u00b7flor", ",", "der", "ihm", "ent\u00b7sprie\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "APPRART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Sich des Werdens heil'ge Kr\u00e4fte regen;", "tokens": ["Sich", "des", "Wer\u00b7dens", "heil'\u00b7ge", "Kr\u00e4f\u00b7te", "re\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ART", "NN", "ADJA", "NN", "ADJA", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Einen Ton zum Liede mitzubringen,", "tokens": ["Ei\u00b7nen", "Ton", "zum", "Lie\u00b7de", "mit\u00b7zu\u00b7brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das sich aufschwingt voll verkl\u00e4rter Pracht,", "tokens": ["Das", "sich", "auf\u00b7schwingt", "voll", "ver\u00b7kl\u00e4r\u00b7ter", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVFIN", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wenn die V\u00f6gel in der Waldesnacht", "tokens": ["Wenn", "die", "V\u00f6\u00b7gel", "in", "der", "Wal\u00b7des\u00b7nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Fr\u00fch und Abends ihr Te Deum singen;", "tokens": ["Fr\u00fch", "und", "A\u00b7bends", "ihr", "Te", "Deum", "sin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "NN", "PPOSAT", "NN", "NE", "VVFIN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Zu vereinen sich mit ", "tokens": ["Zu", "ver\u00b7ei\u00b7nen", "sich", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PRF", "APPR"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Festzuwurzeln in dem Stamm des Seins", "tokens": ["Fest\u00b7zu\u00b7wur\u00b7zeln", "in", "dem", "Stamm", "des", "Seins"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Und nicht mehr, der fl\u00fccht'gen Bl\u00e4tter ein's,", "tokens": ["Und", "nicht", "mehr", ",", "der", "fl\u00fccht'\u00b7gen", "Bl\u00e4t\u00b7ter", "ein's", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "$,", "ART", "ADJA", "NN", "NE", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "In dem Hauch' des Sturmes hinzubeben! \u2013", "tokens": ["In", "dem", "Hauch'", "des", "Stur\u00b7mes", "hin\u00b7zu\u00b7be\u00b7ben", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Also dacht' ich. Wunderbare Lieder", "tokens": ["Al\u00b7so", "dacht'", "ich", ".", "Wun\u00b7der\u00b7ba\u00b7re", "Lie\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Stiegen aus der finstern Schlucht empor,", "tokens": ["Stie\u00b7gen", "aus", "der", "fins\u00b7tern", "Schlucht", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wirrer Stimmen s\u00fc\u00dfverschmolz'ner Chor \u2013", "tokens": ["Wir\u00b7rer", "Stim\u00b7men", "s\u00fc\u00df\u00b7ver\u00b7schmolz'\u00b7ner", "Chor", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "tiefer beugt' ich mich zum Abgrund nieder.", "tokens": ["tie\u00b7fer", "beugt'", "ich", "mich", "zum", "Ab\u00b7grund", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Ja, ich stand, wo sich die Wege spalten,", "tokens": ["Ja", ",", "ich", "stand", ",", "wo", "sich", "die", "We\u00b7ge", "spal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVFIN", "$,", "PWAV", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Auf der Schwelle zwischen Dort und Hier,", "tokens": ["Auf", "der", "Schwel\u00b7le", "zwi\u00b7schen", "Dort", "und", "Hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "KON", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Doch umschlungen f\u00fchlt' ich mich von dir,", "tokens": ["Doch", "um\u00b7schlun\u00b7gen", "f\u00fchlt'", "ich", "mich", "von", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und durch dich vom Sturz zur\u00fcckgehalten.", "tokens": ["Und", "durch", "dich", "vom", "Sturz", "zu\u00b7r\u00fcck\u00b7ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.11": {"line.1": {"text": "Licht und klar durchdrang es da mein Wesen:", "tokens": ["Licht", "und", "klar", "durch\u00b7drang", "es", "da", "mein", "We\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Aufgeh'n in dem All ist dein Begehr? \u2013", "tokens": ["Auf\u00b7geh'n", "in", "dem", "All", "ist", "dein", "Be\u00b7gehr", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$.", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Tauche unter in der Liebe Meer,", "tokens": ["Tau\u00b7che", "un\u00b7ter", "in", "der", "Lie\u00b7be", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und du wirst von deinem Ich genesen!", "tokens": ["Und", "du", "wirst", "von", "dei\u00b7nem", "Ich", "ge\u00b7ne\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "PPOSAT", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.12": {"line.1": {"text": "Deiner Selbstheit Schranken werden sinken,", "tokens": ["Dei\u00b7ner", "Selbst\u00b7heit", "Schran\u00b7ken", "wer\u00b7den", "sin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fchlen wirst du dich im gro\u00dfen All,", "tokens": ["F\u00fch\u00b7len", "wirst", "du", "dich", "im", "gro\u00b7\u00dfen", "All", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ew'ger Strahl im ew'gen Sonnenball", "tokens": ["Ew'\u00b7ger", "Strahl", "im", "ew'\u00b7gen", "Son\u00b7nen\u00b7ball"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wird dein Geist die Luft der Heimath trinken.", "tokens": ["Wird", "dein", "Geist", "die", "Luft", "der", "Hei\u00b7math", "trin\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.13": {"line.1": {"text": "Wenn im Tod' die Wangen sich entf\u00e4rben", "tokens": ["Wenn", "im", "Tod'", "die", "Wan\u00b7gen", "sich", "ent\u00b7f\u00e4r\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "PRF", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Meinst du, da\u00df nur da Vollendung sei? \u2013", "tokens": ["Meinst", "du", ",", "da\u00df", "nur", "da", "Vol\u00b7len\u00b7dung", "sei", "?", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADV", "KOUS", "NN", "VAFIN", "$.", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "And'rer Weg zur Gottheit steht dir frei", "tokens": ["An\u00b7d'\u00b7rer", "Weg", "zur", "Got\u00b7theit", "steht", "dir", "frei"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPRART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und die Liebe ist das sch\u00f6nste Sterben.", "tokens": ["Und", "die", "Lie\u00b7be", "ist", "das", "sch\u00f6ns\u00b7te", "Ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.14": {"line.1": {"text": "Vom Gew\u00fchle weg die Schritte lenkend", "tokens": ["Vom", "Ge\u00b7w\u00fch\u00b7le", "weg", "die", "Schrit\u00b7te", "len\u00b7kend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hin zu dem Palast der Alpenfee,", "tokens": ["Hin", "zu", "dem", "Pa\u00b7last", "der", "Al\u00b7pen\u00b7fee", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Standen wir auf sonnigstiller H\u00f6h'", "tokens": ["Stan\u00b7den", "wir", "auf", "son\u00b7nigs\u00b7til\u00b7ler", "H\u00f6h'"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "In den Abgrund uns're Blicke senkend.", "tokens": ["In", "den", "Ab\u00b7grund", "un\u00b7s'\u00b7re", "Bli\u00b7cke", "sen\u00b7kend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "--+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.15": {"line.1": {"text": "Durch die F\u00f6hren ging des Windes Rauschen,", "tokens": ["Durch", "die", "F\u00f6h\u00b7ren", "ging", "des", "Win\u00b7des", "Rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und des Gie\u00