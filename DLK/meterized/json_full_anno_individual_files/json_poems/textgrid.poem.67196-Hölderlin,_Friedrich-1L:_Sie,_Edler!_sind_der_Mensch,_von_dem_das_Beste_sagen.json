{"textgrid.poem.67196": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie, Edler! sind der Mensch, von dem das Beste sagen", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie, Edler! sind der Mensch, von dem das Beste sagen", "tokens": ["Sie", ",", "Ed\u00b7ler", "!", "sind", "der", "Mensch", ",", "von", "dem", "das", "Bes\u00b7te", "sa\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NE", "$.", "VAFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nicht f\u00e4lschlich ist, da jeder Mensch es kennet,", "tokens": ["Nicht", "f\u00e4lschlich", "ist", ",", "da", "je\u00b7der", "Mensch", "es", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VAFIN", "$,", "KOUS", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch die Vollkommenheit enth\u00e4lt verschiedne Fragen,", "tokens": ["Doch", "die", "Voll\u00b7kom\u00b7men\u00b7heit", "ent\u00b7h\u00e4lt", "ver\u00b7schied\u00b7ne", "Fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wenn schon der Mensch es leicht bezeuget nennet.", "tokens": ["Wenn", "schon", "der", "Mensch", "es", "leicht", "be\u00b7zeu\u00b7get", "nen\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PPER", "ADJD", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie aber haben dies in recht gewohntem Leben,", "tokens": ["Sie", "a\u00b7ber", "ha\u00b7ben", "dies", "in", "recht", "ge\u00b7wohn\u00b7tem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "PDS", "APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In der Gewogenheit, von der sich Menschen ehren,", "tokens": ["In", "der", "Ge\u00b7wo\u00b7gen\u00b7heit", ",", "von", "der", "sich", "Men\u00b7schen", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "PRELS", "PRF", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das ist den W\u00fcrdigern als wie ein Gut gegeben,", "tokens": ["Das", "ist", "den", "W\u00fcr\u00b7di\u00b7gern", "als", "wie", "ein", "Gut", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "KOUS", "KOKOM", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da viele sich in Not und Gram verzehren.", "tokens": ["Da", "vie\u00b7le", "sich", "in", "Not", "und", "Gram", "ver\u00b7zeh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "APPR", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So unverlierbar dies, so geht es, hoch zu gelten,", "tokens": ["So", "un\u00b7ver\u00b7lier\u00b7bar", "dies", ",", "so", "geht", "es", ",", "hoch", "zu", "gel\u00b7ten", ","], "token_info": ["word", "word", "word", "punc