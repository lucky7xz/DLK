{"dta.poem.8261": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "f\u00fcnffter  A ufftritt.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Sieger wird besieget/", "tokens": ["Ein", "Sie\u00b7ger", "wird", "be\u00b7sie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wann Liebreitz gegen ihn", "tokens": ["Wann", "Lieb\u00b7reitz", "ge\u00b7gen", "ihn"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ger\u00fcst zu Felde lieget/", "tokens": ["Ge\u00b7r\u00fcst", "zu", "Fel\u00b7de", "lie\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er kan ihn nach sich ziehn.", "tokens": ["Er", "kan", "ihn", "nach", "sich", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Selbst Mars der wird bestricket/", "tokens": ["Selbst", "Mars", "der", "wird", "be\u00b7stri\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "VAFIN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wann Venus Garne legt:", "tokens": ["Wann", "Ve\u00b7nus", "Gar\u00b7ne", "legt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df sie ins Netz ihn r&#252;cket/", "tokens": ["Da\u00df", "sie", "ins", "Netz", "ihn", "r", "&#252;", "cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PPER", "NE", "$(", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und seine Freyheit schl&#228;gt.", "tokens": ["Und", "sei\u00b7ne", "Frey\u00b7heit", "schl", "&#228;", "gt", "."], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}