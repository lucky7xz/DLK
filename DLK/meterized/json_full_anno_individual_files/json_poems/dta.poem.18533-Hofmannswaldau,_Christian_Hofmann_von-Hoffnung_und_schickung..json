{"dta.poem.18533": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Hoffnung und schickung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der himmel wirckt mir lauter ungel\u00fccke/", "tokens": ["Der", "him\u00b7mel", "wirckt", "mir", "lau\u00b7ter", "un\u00b7ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein hoffen ist das lust-spiel seiner macht/", "tokens": ["Mein", "hof\u00b7fen", "ist", "das", "lust\u00b7spiel", "sei\u00b7ner", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Den vorsatz treibt offt nur sein schlu\u00df zur\u00fccke/", "tokens": ["Den", "vor\u00b7satz", "treibt", "offt", "nur", "sein", "schlu\u00df", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "PTKVZ", "$("], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Den hoffnung schon in guten stand gebracht.", "tokens": ["Den", "hoff\u00b7nung", "schon", "in", "gu\u00b7ten", "stand", "ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "ADJA", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die schickung schl\u00e4gt mein schiffchen bald zur\u00fccke/", "tokens": ["Die", "schi\u00b7ckung", "schl\u00e4gt", "mein", "schiff\u00b7chen", "bald", "zu\u00b7r\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "ADJA", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das hoffnung doch blie\u00df in die hohe see:", "tokens": ["Das", "hoff\u00b7nung", "doch", "blie\u00df", "in", "die", "ho\u00b7he", "see", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.7": {"text": "Die hoffnung gab die w\u00e4rmsten sonnen-blicke/", "tokens": ["Die", "hoff\u00b7nung", "gab", "die", "w\u00e4rms\u00b7ten", "son\u00b7nen\u00b7bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und schickung macht aus w\u00e4rme k\u00e4lt und schnee.", "tokens": ["Und", "schi\u00b7ckung", "macht", "aus", "w\u00e4r\u00b7me", "k\u00e4lt", "und", "schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "ADJA", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Die schickung machts/ da\u00df ich die seegel streiche/", "tokens": ["Die", "schi\u00b7ckung", "machts", "/", "da\u00df", "ich", "die", "see\u00b7gel", "strei\u00b7che", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die hoffnung erst mir spannte gl\u00fccklich auf.", "tokens": ["Die", "hoff\u00b7nung", "erst", "mir", "spann\u00b7te", "gl\u00fcck\u00b7lich", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Die schickung f\u00fcgts/ da\u00df ich lavirend weiche/", "tokens": ["Die", "schi\u00b7ckung", "f\u00fcgts", "/", "da\u00df", "ich", "la\u00b7vi\u00b7rend", "wei\u00b7che", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KOUS", "PPER", "ADJD", "ADJA", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da hoffnung schon mir gab geraden lauf.", "tokens": ["Da", "hoff\u00b7nung", "schon", "mir", "gab", "ge\u00b7ra\u00b7den", "lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "In hoffnung lief mein schifchen sonder stranden/", "tokens": ["In", "hoff\u00b7nung", "lief", "mein", "schifc\u00b7hen", "son\u00b7der", "stran\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPOSAT", "ADJA", "KON", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Und schickung wirffts auf ki\u00df und ungl\u00fccks-sand/", "tokens": ["Und", "schi\u00b7ckung", "wirffts", "auf", "ki\u00df", "und", "un\u00b7gl\u00fccks\u00b7sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "In hoffnung war ich ausser kett und banden/", "tokens": ["In", "hoff\u00b7nung", "war", "ich", "aus\u00b7ser", "kett", "und", "ban\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Und schickung legt mich unter feindes hand.", "tokens": ["Und", "schi\u00b7ckung", "legt", "mich", "un\u00b7ter", "fein\u00b7des", "hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "In hoffnung mu\u00df mir ieder caper weichen/", "tokens": ["In", "hoff\u00b7nung", "mu\u00df", "mir", "ie\u00b7der", "ca\u00b7per", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Die schickung f\u00fcgts/ da\u00df dieser kommt empor/", "tokens": ["Die", "schi\u00b7ckung", "f\u00fcgts", "/", "da\u00df", "die\u00b7ser", "kommt", "em\u00b7por", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "KOUS", "PDS", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Die schickung weist mir der Helenen zeichen;", "tokens": ["Die", "schi\u00b7ckung", "weist", "mir", "der", "He\u00b7le\u00b7nen", "zei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "In hoffnung schien mir Pollux und Castor.", "tokens": ["In", "hoff\u00b7nung", "schien", "mir", "Pol\u00b7lux", "und", "Cas\u00b7tor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.21": {"text": "Die schickung blitzt mit nichts/ als ungewittern/", "tokens": ["Die", "schi\u00b7ckung", "blitzt", "mit", "nichts", "/", "als", "un\u00b7ge\u00b7wit\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PIS", "$(", "KOUS", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Und hoffnung sprach/ da\u00df alles helle sey;", "tokens": ["Und", "hoff\u00b7nung", "sprach", "/", "da\u00df", "al\u00b7les", "hel\u00b7le", "sey", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$(", "KOUS", "PIS", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Durch schickung mu\u00df mein fl\u00fcchtig schiff zersplittern/", "tokens": ["Durch", "schi\u00b7ckung", "mu\u00df", "mein", "fl\u00fcch\u00b7tig", "schiff", "zer\u00b7split\u00b7tern", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPOSAT", "ADJD", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "In hoffnung gieng ich klipp und fel\u00df vorbey.", "tokens": ["In", "hoff\u00b7nung", "gieng", "ich", "klipp", "und", "fel\u00df", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ADJD", "KON", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Die hoffnung reitzt mich freudig anzulenden/", "tokens": ["Die", "hoff\u00b7nung", "reitzt", "mich", "freu\u00b7dig", "an\u00b7zu\u00b7len\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "VVIZU", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Die schickung will da\u00df ich zur\u00fccke mu\u00df.", "tokens": ["Die", "schi\u00b7ckung", "will", "da\u00df", "ich", "zu\u00b7r\u00fc\u00b7cke", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KOUS", "PPER", "VVFIN", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Kurtz: schickung mu\u00df der hoffnung h\u00fclffe senden/", "tokens": ["Kurtz", ":", "schi\u00b7ckung", "mu\u00df", "der", "hoff\u00b7nung", "h\u00fclf\u00b7fe", "sen\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "NN", "VMFIN", "ART", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Sonst wird zuletzt auch hoffnung zum verdru\u00df.", "tokens": ["Sonst", "wird", "zu\u00b7letzt", "auch", "hoff\u00b7nung", "zum", "ver\u00b7dru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ADV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}