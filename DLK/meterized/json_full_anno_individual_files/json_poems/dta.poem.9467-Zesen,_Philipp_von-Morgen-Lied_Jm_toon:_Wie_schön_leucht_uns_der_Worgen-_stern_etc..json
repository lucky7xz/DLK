{"dta.poem.9467": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Morgen-Lied/  \n Jm toon:  \n  Wie sch\u00f6n leucht uns der Worgen-  \n stern/ etc.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Nacht das schrecken-volle Kind/", "tokens": ["Die", "Nacht", "das", "schre\u00b7cken\u00b7vol\u00b7le", "Kind", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die schwartze Demmerung zerrinnt/", "tokens": ["Die", "schwart\u00b7ze", "Dem\u00b7me\u00b7rung", "zer\u00b7rinnt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der Fr\u00fche-tag sich zeiget/", "tokens": ["Der", "Fr\u00fc\u00b7he\u00b7tag", "sich", "zei\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die g\u00fcldne Morgenr\u00f6th entspringt/", "tokens": ["Die", "g\u00fcld\u00b7ne", "Mor\u00b7gen\u00b7r\u00f6th", "ent\u00b7springt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Sonnen-glantz hernacher dringe", "tokens": ["Der", "Son\u00b7nen\u00b7glantz", "her\u00b7na\u00b7cher", "drin\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "und auf gen Himmel steiget:", "tokens": ["und", "auf", "gen", "Him\u00b7mel", "stei\u00b7get", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Singet/ klinget/", "tokens": ["Sin\u00b7get", "/", "klin\u00b7get", "/"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Freuden-Lieder soll ein jeder", "tokens": ["Freu\u00b7den\u00b7Lie\u00b7der", "soll", "ein", "je\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gott zu ehren/", "tokens": ["Gott", "zu", "eh\u00b7ren", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Aus dem Munde la\u00dfen h\u00f6ren.", "tokens": ["Aus", "dem", "Mun\u00b7de", "la\u00b7\u00dfen", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wohlan! mein Hertz erhebe dich", "tokens": ["Wo\u00b7hlan", "!", "mein", "Hertz", "er\u00b7he\u00b7be", "dich"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$.", "PPOSAT", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und dencke wie der H\u00f6chste sich", "tokens": ["und", "den\u00b7cke", "wie", "der", "H\u00f6chs\u00b7te", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bem\u00fcht vor dich zu wachen.", "tokens": ["Be\u00b7m\u00fcht", "vor", "dich", "zu", "wa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er hart beh\u00fctet diese Nacht", "tokens": ["Er", "hart", "be\u00b7h\u00fc\u00b7tet", "die\u00b7se", "Nacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr aller Angst und Krieges-macht", "tokens": ["F\u00fcr", "al\u00b7ler", "Angst", "und", "Krie\u00b7ge\u00b7smacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "F\u00fcr Noth und Todt uns Schwachen:", "tokens": ["F\u00fcr", "Noth", "und", "Todt", "uns", "Schwa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PPER", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Singet/ klinget", "tokens": ["Sin\u00b7get", "/", "klin\u00b7get"], "token_info": ["word", "punct", "word"], "pos": ["VVFIN", "$(", "VVFIN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Freuden-Lieder soll ein jeder", "tokens": ["Freu\u00b7den\u00b7Lie\u00b7der", "soll", "ein", "je\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "PIAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Gott zu dan cken", "tokens": ["Gott", "zu", "dan", "cken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADV", "VVINF"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "Sch\u00fctten aus des Hertzeng-schrancken.", "tokens": ["Sch\u00fct\u00b7ten", "aus", "des", "Hert\u00b7zeng\u00b7schran\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Brich an du Sonne meiner Seel", "tokens": ["Brich", "an", "du", "Son\u00b7ne", "mei\u00b7ner", "Seel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "PPER", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In meines f\u00fcnstern Hertzens-h\u00f6l/", "tokens": ["In", "mei\u00b7nes", "f\u00fcns\u00b7tern", "Hert\u00b7zens\u00b7h\u00f6l", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du g\u00fcldner Glantz der freuden/", "tokens": ["Du", "g\u00fcld\u00b7ner", "Glantz", "der", "freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erquicke mich mit deinem Geist", "tokens": ["Er\u00b7qui\u00b7cke", "mich", "mit", "dei\u00b7nem", "Geist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und mir gew\u00fcndschten Beystand leist", "tokens": ["und", "mir", "ge\u00b7w\u00fcnd\u00b7schten", "Beys\u00b7tand", "leist"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In meinem Thun und Leiden/", "tokens": ["In", "mei\u00b7nem", "Thun", "und", "Lei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus Noth und Todt", "tokens": ["Aus", "Noth", "und", "Todt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Hilff mir Armen mit erbarmen/", "tokens": ["Hilff", "mir", "Ar\u00b7men", "mit", "er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPR", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mein studieren/", "tokens": ["Mein", "stu\u00b7die\u00b7ren", "/"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "La\u00df mich vollend wohl ausf\u00fchren.", "tokens": ["La\u00df", "mich", "vol\u00b7lend", "wohl", "aus\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach brich herf\u00fcr du Trost und Licht/", "tokens": ["Ach", "brich", "her\u00b7f\u00fcr", "du", "Trost", "und", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "ADV", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn mich der S\u00fcnden-Worm anficht/", "tokens": ["Wenn", "mich", "der", "S\u00fcn\u00b7den\u00b7Worm", "an\u00b7ficht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "und mein Gewissen naget;", "tokens": ["und", "mein", "Ge\u00b7wis\u00b7sen", "na\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nim weg die schwere S\u00fcnden-last/", "tokens": ["Nim", "weg", "die", "schwe\u00b7re", "S\u00fcn\u00b7den\u00b7last", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dieweil Du sie geb\u00fc\u00dfet hast", "tokens": ["Die\u00b7weil", "Du", "sie", "ge\u00b7b\u00fc\u00b7\u00dfet", "hast"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und Dich vor mich gewaget.", "tokens": ["und", "Dich", "vor", "mich", "ge\u00b7wa\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Aus Noth und Todt", "tokens": ["Aus", "Noth", "und", "Todt"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Hilff mir Armen mit erbarmen/", "tokens": ["Hilff", "mir", "Ar\u00b7men", "mit", "er\u00b7bar\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "APPR", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Mein studieren", "tokens": ["Mein", "stu\u00b7die\u00b7ren"], "token_info": ["word", "word"], "pos": ["PPOSAT", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.10": {"text": "La\u00df mich vollend wohl ausf\u00fchren!", "tokens": ["La\u00df", "mich", "vol\u00b7lend", "wohl", "aus\u00b7f\u00fch\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVPP", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}