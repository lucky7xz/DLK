{"dta.poem.1856": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein guter Nahm ist keiner Zeiten Raub/", "tokens": ["Ein", "gu\u00b7ter", "Nahm", "ist", "kei\u00b7ner", "Zei\u00b7ten", "Raub", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PIAT", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Ehre folgt uns wenn wir Asch und Staub.", "tokens": ["Die", "Eh\u00b7re", "folgt", "uns", "wenn", "wir", "Asch", "und", "Staub", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KOUS", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was Tugend hat zu ihrer Zeit gethan/", "tokens": ["Was", "Tu\u00b7gend", "hat", "zu", "ih\u00b7rer", "Zeit", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Schreibt ihr zu Ruhm die sp\u00e4te Nachwelt an.", "tokens": ["Schreibt", "ihr", "zu", "Ruhm", "die", "sp\u00e4\u00b7te", "Nach\u00b7welt", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}