{"dta.poem.7612": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "27.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie Wasser von der Erd' ein Sonnenstral aufzieht,", "tokens": ["Wie", "Was\u00b7ser", "von", "der", "Erd'", "ein", "Son\u00b7nen\u00b7stral", "auf\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So h\u00f6hres Licht den Geist, wenn er dem Leib entflieht.", "tokens": ["So", "h\u00f6h\u00b7res", "Licht", "den", "Geist", ",", "wenn", "er", "dem", "Leib", "ent\u00b7flieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Doch wie zur Erde neu die schwerern D\u00fcnste fallen,", "tokens": ["Doch", "wie", "zur", "Er\u00b7de", "neu", "die", "schwe\u00b7rern", "D\u00fcns\u00b7te", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADJD", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer wei\u00df, ob Geister so ins Leben wieder wallen?", "tokens": ["Wer", "wei\u00df", ",", "ob", "Geis\u00b7ter", "so", "ins", "Le\u00b7ben", "wie\u00b7der", "wal\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "KOUS", "NN", "ADV", "APPRART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und wie zum Aether nur die feinsten D\u00fcfte steigen,", "tokens": ["Und", "wie", "zum", "A\u00b7e\u00b7ther", "nur", "die", "feins\u00b7ten", "D\u00fcf\u00b7te", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So ein \u00e4therischer Geist zum h\u00f6chsten Geisterreigen!", "tokens": ["So", "ein", "\u00e4t\u00b7he\u00b7ri\u00b7scher", "Geist", "zum", "h\u00f6chs\u00b7ten", "Geis\u00b7ter\u00b7rei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+-+-+-+-", "measure": "anapaest.init"}}}}}