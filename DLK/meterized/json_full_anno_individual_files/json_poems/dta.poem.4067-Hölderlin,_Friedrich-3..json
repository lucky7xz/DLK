{"dta.poem.4067": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Aber damit uns nicht, gleich Allzuklugen, entfliehe", "tokens": ["A\u00b7ber", "da\u00b7mit", "uns", "nicht", ",", "gleich", "A\u00b7llzu\u00b7klu\u00b7gen", ",", "ent\u00b7flie\u00b7he"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "KOUS", "PPER", "PTKNEG", "$,", "ADV", "NN", "$,", "VVFIN"], "meter": "+--+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Diese neigende Zeit, komm' ich entgegen sogleich,", "tokens": ["Die\u00b7se", "nei\u00b7gen\u00b7de", "Zeit", ",", "komm'", "ich", "ent\u00b7ge\u00b7gen", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$,", "VVFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Bis an die Grenze des Lands, wo mir den lieben", "tokens": ["Bis", "an", "die", "Gren\u00b7ze", "des", "Lands", ",", "wo", "mir", "den", "lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "ART", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Geburtsort", "tokens": ["Ge\u00b7burts\u00b7ort"], "token_info": ["word"], "pos": ["NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Und die Insel des Stroms blaues Gew\u00e4sser", "tokens": ["Und", "die", "In\u00b7sel", "des", "Stroms", "blau\u00b7es", "Ge\u00b7w\u00e4s\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "NN", "ADJA", "NN"], "meter": "---+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "umflie\u00dft.", "tokens": ["um\u00b7flie\u00dft", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.7": {"text": "Heilig ist mir der Ort, an beiden Ufern, der Fels", "tokens": ["Hei\u00b7lig", "ist", "mir", "der", "Ort", ",", "an", "bei\u00b7den", "U\u00b7fern", ",", "der", "Fels"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJA", "VAFIN", "PPER", "ART", "NN", "$,", "APPR", "PIAT", "NN", "$,", "ART", "NN"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "auch,", "tokens": ["auch", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Der mit Garten und Hausgr\u00fcn aus den Wellen", "tokens": ["Der", "mit", "Gar\u00b7ten", "und", "Haus\u00b7gr\u00fcn", "aus", "den", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "NN", "APPR", "ART", "NN"], "meter": "--+--++--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "sich hebt.", "tokens": ["sich", "hebt", "."], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Dort begegnen wir uns, o g\u00fctiges Licht! wo zuerst", "tokens": ["Dort", "be\u00b7geg\u00b7nen", "wir", "uns", ",", "o", "g\u00fc\u00b7ti\u00b7ges", "Licht", "!", "wo", "zu\u00b7erst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "FM", "ADJA", "NN", "$.", "PWAV", "ADV"], "meter": "--+--+-+--+--+", "measure": "anapaest.di.plus"}, "line.12": {"text": "mich,", "tokens": ["mich", ","], "token_info": ["word", "punct"], "pos": ["PPER", "$,"], "meter": "-", "measure": "single.down"}, "line.13": {"text": "Deiner gef\u00fchlteren Stralen mich einer betraf.", "tokens": ["Dei\u00b7ner", "ge\u00b7f\u00fchl\u00b7te\u00b7ren", "Stra\u00b7len", "mich", "ei\u00b7ner", "be\u00b7traf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PPER", "PIS", "VVFIN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.14": {"text": "Dort begann und beginnt das liebe Leben von", "tokens": ["Dort", "be\u00b7gann", "und", "be\u00b7ginnt", "das", "lie\u00b7be", "Le\u00b7ben", "von"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ART", "ADJA", "NN", "APPR"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Neuem,", "tokens": ["Neu\u00b7em", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.16": {"text": "Aber des Vaters Grab seh' ich, und weine dir", "tokens": ["A\u00b7ber", "des", "Va\u00b7ters", "Grab", "seh'", "ich", ",", "und", "wei\u00b7ne", "dir"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN", "PPER"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.17": {"text": "schon?", "tokens": ["schon", "?"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Wein' und halt' und habe den Freund und h\u00f6re", "tokens": ["Wein'", "und", "halt'", "und", "ha\u00b7be", "den", "Freund", "und", "h\u00f6\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "KON", "VVFIN", "KON", "VAFIN", "ART", "NN", "KON", "VVFIN"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.19": {"text": "das Wort, das", "tokens": ["das", "Wort", ",", "das"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$,", "PRELS"], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Einst mir in himmlischer Kunst Leiden der Liebe", "tokens": ["Einst", "mir", "in", "himm\u00b7li\u00b7scher", "Kunst", "Lei\u00b7den", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "APPR", "ADJA", "NN", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "geheilt.", "tokens": ["ge\u00b7heilt", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "-+", "measure": "iambic.single"}, "line.22": {"text": "Andres erwacht! Ich mu\u00df die Landesheroen ihm", "tokens": ["And\u00b7res", "er\u00b7wacht", "!", "Ich", "mu\u00df", "die", "Lan\u00b7des\u00b7he\u00b7roen", "ihm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVPP", "$.", "PPER", "VMFIN", "ART", "NN", "PPER"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.23": {"text": "nennen!", "tokens": ["nen\u00b7nen", "!"], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.24": {"text": "Barbarossa! dich auch, g\u00fctiger Christoph, und", "tokens": ["Bar\u00b7ba\u00b7ros\u00b7sa", "!", "dich", "auch", ",", "g\u00fc\u00b7ti\u00b7ger", "Chris\u00b7toph", ",", "und"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NE", "$.", "PPER", "ADV", "$,", "ADJA", "NN", "$,", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.25": {"text": "dich", "tokens": ["dich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.26": {"text": "Konradin! wie du fielst, so fallen Starke, der", "tokens": ["Kon\u00b7ra\u00b7din", "!", "wie", "du", "fielst", ",", "so", "fal\u00b7len", "Star\u00b7ke", ",", "der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$.", "PWAV", "PPER", "VVFIN", "$,", "ADV", "ADJA", "NN", "$,", "PRELS"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Epheu", "tokens": ["E\u00b7pheu"], "token_info": ["word"], "pos": ["NN"], "meter": "--", "measure": "unknown.measure.zero"}, "line.28": {"text": "Gr\u00fcnt am Fels, und die Burg deckt das bac-", "tokens": ["Gr\u00fcnt", "am", "Fels", ",", "und", "die", "Burg", "deckt", "das", "bac"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "ART", "TRUNC"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.29": {"text": "chantische Laub,", "tokens": ["chan\u00b7ti\u00b7sche", "Laub", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.30": {"text": "Doch Vergangenes ist, wie K\u00fcnftiges, heilig den", "tokens": ["Doch", "Ver\u00b7gan\u00b7ge\u00b7nes", "ist", ",", "wie", "K\u00fcnf\u00b7ti\u00b7ges", ",", "hei\u00b7lig", "den"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "NN", "VAFIN", "$,", "PWAV", "NN", "$,", "ADJD", "ART"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.31": {"text": "S\u00e4ngern,", "tokens": ["S\u00e4n\u00b7gern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Und in Tagen des Herbsts s\u00fchnen die Schatten", "tokens": ["Und", "in", "Ta\u00b7gen", "des", "Herbsts", "s\u00fch\u00b7nen", "die", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "--+---+--+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "wir aus.", "tokens": ["wir", "aus", "."], "token_info": ["word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}}}}