{"dta.poem.7734": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Im Kampf ist Welt und Ich, und nur in Gott ist Frieden,", "tokens": ["Im", "Kampf", "ist", "Welt", "und", "Ich", ",", "und", "nur", "in", "Gott", "ist", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "NN", "KON", "PPER", "$,", "KON", "ADV", "APPR", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil Welt und Ich in Gott nicht weiter sind geschieden.", "tokens": ["Weil", "Welt", "und", "Ich", "in", "Gott", "nicht", "wei\u00b7ter", "sind", "ge\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "PPER", "APPR", "NN", "PTKNEG", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Den Acker friedigst du von au\u00dfen ein vorm Wild,", "tokens": ["Den", "A\u00b7cker", "frie\u00b7digst", "du", "von", "au\u00b7\u00dfen", "ein", "vorm", "Wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch unbefriedet bleibt im Innern dein Gefild.", "tokens": ["Doch", "un\u00b7be\u00b7frie\u00b7det", "bleibt", "im", "In\u00b7nern", "dein", "Ge\u00b7fild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Nicht durch Befriedigung befriedigst du die Triebe;", "tokens": ["Nicht", "durch", "Be\u00b7frie\u00b7di\u00b7gung", "be\u00b7frie\u00b7digst", "du", "die", "Trie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zufriedenheit gibt nur die Friedlichkeit der Liebe.", "tokens": ["Zu\u00b7frie\u00b7den\u00b7heit", "gibt", "nur", "die", "Fried\u00b7lich\u00b7keit", "der", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.4": {"line.1": {"text": "Ihr habet oft den Witz misbraucht zu Krieg und Hader;", "tokens": ["Ihr", "ha\u00b7bet", "oft", "den", "Witz", "mis\u00b7braucht", "zu", "Krieg", "und", "Ha\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch seht, es hat der Witz auch eine Friedensader.", "tokens": ["Doch", "seht", ",", "es", "hat", "der", "Witz", "auch", "ei\u00b7ne", "Frie\u00b7den\u00b7sa\u00b7der", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}