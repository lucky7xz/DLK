{"dta.poem.16118": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "172.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn Seuche herrscht und selbst die Luft ist Krankheitszunder,", "tokens": ["Wenn", "Seu\u00b7che", "herrscht", "und", "selbst", "die", "Luft", "ist", "Krank\u00b7heits\u00b7zun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "KON", "ADV", "ART", "NN", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bleibt davon einer unergriffen, ists ein Wunder.", "tokens": ["Bleibt", "da\u00b7von", "ei\u00b7ner", "un\u00b7er\u00b7grif\u00b7fen", ",", "ists", "ein", "Wun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "ADJA", "$,", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein solches Wunder ists, wenn in der Zeit, befleckt", "tokens": ["Ein", "sol\u00b7ches", "Wun\u00b7der", "ists", ",", "wenn", "in", "der", "Zeit", ",", "be\u00b7fleckt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "PIAT", "NN", "VAFIN", "$,", "KOUS", "APPR", "ART", "NN", "$,", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von soviel B\u00f6sem, bleibt ein Herz unangesteckt.", "tokens": ["Von", "so\u00b7viel", "B\u00f6\u00b7sem", ",", "bleibt", "ein", "Herz", "un\u00b7an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "VVFIN", "ART", "NN", "ADJD", "$