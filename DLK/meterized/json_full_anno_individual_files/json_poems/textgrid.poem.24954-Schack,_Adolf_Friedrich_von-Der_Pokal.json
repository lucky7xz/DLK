{"textgrid.poem.24954": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Der Pokal", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "W\u00e4r' ich noch der alte Lacher,", "tokens": ["W\u00e4r'", "ich", "noch", "der", "al\u00b7te", "La\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ich war in jener Zeit,", "tokens": ["Der", "ich", "war", "in", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da das Gl\u00fcck zu hundertfacher", "tokens": ["Da", "das", "Gl\u00fcck", "zu", "hun\u00b7dert\u00b7fa\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lust uns jeden Tag geweiht;", "tokens": ["Lust", "uns", "je\u00b7den", "Tag", "ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "W\u00e4r' ich, wie in jenen Bonner", "tokens": ["W\u00e4r'", "ich", ",", "wie", "in", "je\u00b7nen", "Bon\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jahren noch des Frohsinns voll,", "tokens": ["Jah\u00b7ren", "noch", "des", "Froh\u00b7sinns", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da bald s\u00e4uselnd, bald wie Donner", "tokens": ["Da", "bald", "s\u00e4u\u00b7selnd", ",", "bald", "wie", "Don\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "KOKOM", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Unser Rundgesang erscholl:", "tokens": ["Un\u00b7ser", "Rund\u00b7ge\u00b7sang", "er\u00b7scholl", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sicher h\u00e4tt' ich mit dem Danke,", "tokens": ["Si\u00b7cher", "h\u00e4tt'", "ich", "mit", "dem", "Dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "PTKANT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teure Freunde, nicht ges\u00e4umt,", "tokens": ["Teu\u00b7re", "Freun\u00b7de", ",", "nicht", "ge\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr den Becher samt dem Tranke,", "tokens": ["F\u00fcr", "den", "Be\u00b7cher", "samt", "dem", "Tran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der in seinem Kelche sch\u00e4umt!", "tokens": ["Der", "in", "sei\u00b7nem", "Kel\u00b7che", "sch\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mich vergangner Lust zu mahnen,", "tokens": ["Mich", "ver\u00b7gang\u00b7ner", "Lust", "zu", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schickt ihr diesen Festpokal,", "tokens": ["Schickt", "ihr", "die\u00b7sen", "Fest\u00b7po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenen gleich, daraus die Ahnen", "tokens": ["Je\u00b7nen", "gleich", ",", "da\u00b7raus", "die", "Ah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "$,", "PAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich gelabt beim Freudenmahl.", "tokens": ["Sich", "ge\u00b7labt", "beim", "Freu\u00b7den\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "O f\u00fcrwahr, der alten Zecher", "tokens": ["O", "f\u00fcr\u00b7wahr", ",", "der", "al\u00b7ten", "Ze\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der m\u00e4chtig gro\u00dfe wert;", "tokens": ["Ist", "der", "m\u00e4ch\u00b7tig", "gro\u00b7\u00dfe", "wert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frundsberg h\u00e4tte solchen Becher", "tokens": ["Frunds\u00b7berg", "h\u00e4t\u00b7te", "sol\u00b7chen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl auf ", "tokens": ["Wohl", "auf"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "G\u00f6tz auch, dem der Wein nicht k\u00e4rger", "tokens": ["G\u00f6tz", "auch", ",", "dem", "der", "Wein", "nicht", "k\u00e4r\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PRELS", "ART", "NN", "PTKNEG", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flo\u00df nach Fehde und Gefecht,", "tokens": ["Flo\u00df", "nach", "Feh\u00b7de", "und", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat vielleicht im Heidelberger", "tokens": ["Hat", "viel\u00b7leicht", "im", "Hei\u00b7del\u00b7ber\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "NN"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.4": {"text": "Hirsch aus solchem Ma\u00df gezecht.", "tokens": ["Hirsch", "aus", "sol\u00b7chem", "Ma\u00df", "ge\u00b7zecht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Doch, Geliebte, draus zu nippen", "tokens": ["Doch", ",", "Ge\u00b7lieb\u00b7te", ",", "draus", "zu", "nip\u00b7pen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "NN", "$,", "PAV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df man froh wie jene sein;", "tokens": ["Mu\u00df", "man", "froh", "wie", "je\u00b7ne", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "KOKOM", "PDS", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ich mit meinen blassen Lippen", "tokens": ["Ich", "mit", "mei\u00b7nen", "blas\u00b7sen", "Lip\u00b7pen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "W\u00fcrde diesen Kelch entweihn.", "tokens": ["W\u00fcr\u00b7de", "die\u00b7sen", "Kelch", "ent\u00b7weihn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Nicht f\u00fcr mich der Kreis der Trinker,", "tokens": ["Nicht", "f\u00fcr", "mich", "der", "Kreis", "der", "Trin\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn ums Haupt der Kranz sich schlingt", "tokens": ["Wenn", "ums", "Haupt", "der", "Kranz", "sich", "schlingt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "ART", "NN", "PRF", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zu Rechter und zu Linker", "tokens": ["Und", "zu", "Rech\u00b7ter", "und", "zu", "Lin\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "KON", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Becher an den Becher klingt!", "tokens": ["Be\u00b7cher", "an", "den", "Be\u00b7cher", "klingt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Leert' ich doch die letzte Hefe", "tokens": ["Leert'", "ich", "doch", "die", "letz\u00b7te", "He\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In dem Wermutkelch des Seins;", "tokens": ["In", "dem", "Wer\u00b7mut\u00b7kelch", "des", "Seins", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O, wie kr\u00f6nt' ich noch die Schl\u00e4fe", "tokens": ["O", ",", "wie", "kr\u00f6nt'", "ich", "noch", "die", "Schl\u00e4\u00b7fe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "VVFIN", "PPER", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem frischen Gr\u00fcn des Hains?", "tokens": ["Mit", "dem", "fri\u00b7schen", "Gr\u00fcn", "des", "Hains", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Die nicht, die aus gr\u00fcnem Moose,", "tokens": ["Die", "nicht", ",", "die", "aus", "gr\u00fc\u00b7nem", "Moo\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "$,", "PRELS", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus der Bl\u00e4tter F\u00fclle gl\u00e4nzt,", "tokens": ["Aus", "der", "Bl\u00e4t\u00b7ter", "F\u00fcl\u00b7le", "gl\u00e4nzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mir geziemt die wei\u00dfe Rose,", "tokens": ["Mir", "ge\u00b7ziemt", "die", "wei\u00b7\u00dfe", "Ro\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df sie meine Stirn bekr\u00e4nzt.", "tokens": ["Da\u00df", "sie", "mei\u00b7ne", "Stirn", "be\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Und so mahn' ich, liebe Geber,", "tokens": ["Und", "so", "mahn'", "ich", ",", "lie\u00b7be", "Ge\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Euch in diesem tr\u00fcben Dank", "tokens": ["Euch", "in", "die\u00b7sem", "tr\u00fc\u00b7ben", "Dank"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "An die Alten, die auf Gr\u00e4ber", "tokens": ["An", "die", "Al\u00b7ten", ",", "die", "auf", "Gr\u00e4\u00b7ber"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Gossen einen Opfertrank.", "tokens": ["Gos\u00b7sen", "ei\u00b7nen", "Op\u00b7fer\u00b7trank", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Bald an meinem ernsten Male", "tokens": ["Bald", "an", "mei\u00b7nem", "erns\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00fcrmt der Herbst sein welkes Laub;", "tokens": ["T\u00fcrmt", "der", "Herbst", "sein", "wel\u00b7kes", "Laub", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gie\u00dft mir dann aus dem Pokale", "tokens": ["Gie\u00dft", "mir", "dann", "aus", "dem", "Po\u00b7ka\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Spende in den Staub!", "tokens": ["Ei\u00b7ne", "Spen\u00b7de", "in", "den", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "W\u00e4r' ich noch der alte Lacher,", "tokens": ["W\u00e4r'", "ich", "noch", "der", "al\u00b7te", "La\u00b7cher", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der ich war in jener Zeit,", "tokens": ["Der", "ich", "war", "in", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VAFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da das Gl\u00fcck zu hundertfacher", "tokens": ["Da", "das", "Gl\u00fcck", "zu", "hun\u00b7dert\u00b7fa\u00b7cher"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "APPR", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lust uns jeden Tag geweiht;", "tokens": ["Lust", "uns", "je\u00b7den", "Tag", "ge\u00b7weiht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "W\u00e4r' ich, wie in jenen Bonner", "tokens": ["W\u00e4r'", "ich", ",", "wie", "in", "je\u00b7nen", "Bon\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "$,", "PWAV", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jahren noch des Frohsinns voll,", "tokens": ["Jah\u00b7ren", "noch", "des", "Froh\u00b7sinns", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da bald s\u00e4uselnd, bald wie Donner", "tokens": ["Da", "bald", "s\u00e4u\u00b7selnd", ",", "bald", "wie", "Don\u00b7ner"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "ADJD", "$,", "ADV", "KOKOM", "NN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Unser Rundgesang erscholl:", "tokens": ["Un\u00b7ser", "Rund\u00b7ge\u00b7sang", "er\u00b7scholl", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Sicher h\u00e4tt' ich mit dem Danke,", "tokens": ["Si\u00b7cher", "h\u00e4tt'", "ich", "mit", "dem", "Dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "ART", "PTKANT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Teure Freunde, nicht ges\u00e4umt,", "tokens": ["Teu\u00b7re", "Freun\u00b7de", ",", "nicht", "ge\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PTKNEG", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "F\u00fcr den Becher samt dem Tranke,", "tokens": ["F\u00fcr", "den", "Be\u00b7cher", "samt", "dem", "Tran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der in seinem Kelche sch\u00e4umt!", "tokens": ["Der", "in", "sei\u00b7nem", "Kel\u00b7che", "sch\u00e4umt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Mich vergangner Lust zu mahnen,", "tokens": ["Mich", "ver\u00b7gang\u00b7ner", "Lust", "zu", "mah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schickt ihr diesen Festpokal,", "tokens": ["Schickt", "ihr", "die\u00b7sen", "Fest\u00b7po\u00b7kal", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jenen gleich, daraus die Ahnen", "tokens": ["Je\u00b7nen", "gleich", ",", "da\u00b7raus", "die", "Ah\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "ADV", "$,", "PAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sich gelabt beim Freudenmahl.", "tokens": ["Sich", "ge\u00b7labt", "beim", "Freu\u00b7den\u00b7mahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"line.1": {"text": "O f\u00fcrwahr, der alten Zecher", "tokens": ["O", "f\u00fcr\u00b7wahr", ",", "der", "al\u00b7ten", "Ze\u00b7cher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist der m\u00e4chtig gro\u00dfe wert;", "tokens": ["Ist", "der", "m\u00e4ch\u00b7tig", "gro\u00b7\u00dfe", "wert", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJD", "ADJA", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Frundsberg h\u00e4tte solchen Becher", "tokens": ["Frunds\u00b7berg", "h\u00e4t\u00b7te", "sol\u00b7chen", "Be\u00b7cher"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wohl auf ", "tokens": ["Wohl", "auf"], "token_info": ["word", "word"], "pos": ["ADV", "APPR"], "meter": "-+", "measure": "iambic.single"}}, "stanza.18": {"line.1": {"text": "G\u00f6tz auch, dem der Wein nicht k\u00e4rger", "tokens": ["G\u00f6tz", "auch", ",", "dem", "der", "Wein", "nicht", "k\u00e4r\u00b7ger"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "$,", "PRELS", "ART", "NN", "PTKNEG", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Flo\u00df nach Fehde und Gefecht,", "tokens": ["Flo\u00df", "nach", "Feh\u00b7de", "und", "Ge\u00b7fecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hat vielleicht im Heidelberger", "tokens": ["Hat", "viel\u00b7leicht", "im", "Hei\u00b7del\u00b7ber\u00b7ger"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "APPRART", "NN"], "meter": "--+-+---", "measure": "anapaest.init"}, "line.4": {"text": "Hirsch aus solchem Ma\u00df gezecht.", "tokens": ["Hirsch", "aus", "sol\u00b7chem", "Ma\u00df", "ge\u00b7ze