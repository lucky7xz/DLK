{"textgrid.poem.55991": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Tr\u00f6stung des Elia", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Er hatte das getan und dies, den Bund", "tokens": ["Er", "hat\u00b7te", "das", "ge\u00b7tan", "und", "dies", ",", "den", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PDS", "VVPP", "KON", "PDS", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "wie jenen Altar wieder aufzubauen,", "tokens": ["wie", "je\u00b7nen", "Al\u00b7tar", "wie\u00b7der", "auf\u00b7zu\u00b7bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDAT", "NN", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "zu dem sein weitgeschleudertes Vertrauen", "tokens": ["zu", "dem", "sein", "weit\u00b7ge\u00b7schleu\u00b7der\u00b7tes", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "zur\u00fcck als Feuer fiel von ferne, und", "tokens": ["zu\u00b7r\u00fcck", "als", "Feu\u00b7er", "fiel", "von", "fer\u00b7ne", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKVZ", "KOKOM", "NN", "VVFIN", "APPR", "ADJA", "$,", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "hatte er dann nicht Hunderte zerhauen,", "tokens": ["hat\u00b7te", "er", "dann", "nicht", "Hun\u00b7der\u00b7te", "zer\u00b7hau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "NN", "VVINF", "$,"], "meter": "+--+-+-