{"dta.poem.9097": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "In meinen gr\u00fcnen Steinwald", "tokens": ["In", "mei\u00b7nen", "gr\u00fc\u00b7nen", "Stein\u00b7wald"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "scheint der Mond.", "tokens": ["scheint", "der", "Mond", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "In seinem Licht", "tokens": ["In", "sei\u00b7nem", "Licht"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "sitzt ein blasses Weib und singt.", "tokens": ["sitzt", "ein", "blas\u00b7ses", "Weib", "und", "singt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Von einem Sonnensee,", "tokens": ["Von", "ei\u00b7nem", "Son\u00b7nen\u00b7see", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "von blauen Blumen,", "tokens": ["von", "blau\u00b7en", "Blu\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "von einem Kind, das Mutter ruft.", "tokens": ["von", "ei\u00b7nem", "Kind", ",", "das", "Mut\u00b7ter", "ruft", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "M\u00fcde", "tokens": ["M\u00fc\u00b7de"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "f\u00e4llt die Hand ihr \u00fcbers Knie,", "tokens": ["f\u00e4llt", "die", "Hand", "ihr", "\u00fc\u00b7bers", "Knie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "in ihrer stummen Harfe", "tokens": ["in", "ih\u00b7rer", "stum\u00b7men", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "gl\u00e4nzt der Mond.", "tokens": ["gl\u00e4nzt", "der", "Mond", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}