{"textgrid.poem.50750": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "23", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es ist nicht Selbstsucht und nicht Eitelkeit,", "tokens": ["Es", "ist", "nicht", "Selbst\u00b7sucht", "und", "nicht", "Ei\u00b7tel\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "KON", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Was sehnend mir das Herz grab\u00fcber tr\u00e4gt;", "tokens": ["Was", "seh\u00b7nend", "mir", "das", "Herz", "grab\u00b7\u00fc\u00b7ber", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "PPER", "ART", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich glaub, was mir die sch\u00f6ne Br\u00fccke schl\u00e4gt,", "tokens": ["Ich", "glaub", ",", "was", "mir", "die", "sch\u00f6\u00b7ne", "Br\u00fc\u00b7cke", "schl\u00e4gt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist wohl der Stolz, der mich vom Staub befreit.", "tokens": ["Ist", "wohl", "der", "Stolz", ",", "der", "mich", "vom", "Staub", "be\u00b7freit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie ist so kurz, die gr\u00fcne Erdenzeit,", "tokens": ["Sie", "ist", "so", "kurz", 