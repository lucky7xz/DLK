{"textgrid.poem.41935": {"metadata": {"author": {"name": "Schulze, Ernst", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hier ruh' ich weich, vom Laubgeweb' umstrickt,", "genre": "verse", "period": "N.A.", "pub_year": 1803, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ruh' ich weich, vom Laubgeweb' umstrickt,", "tokens": ["Hier", "ruh'", "ich", "weich", ",", "vom", "Laub\u00b7ge\u00b7web'", "um\u00b7strickt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Am leisen Spiel der duftig frischen Quellen,", "tokens": ["Am", "lei\u00b7sen", "Spiel", "der", "duf\u00b7tig", "fri\u00b7schen", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und seh' hinab, wie zu den glatten Wellen", "tokens": ["Und", "seh'", "hin\u00b7ab", ",", "wie", "zu", "den", "glat\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "$,", "PWAV", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das stolze Schlo\u00df so freundlich niederblickt.", "tokens": ["Das", "stol\u00b7ze", "Schlo\u00df", "so", "freund\u00b7lich", "nie\u00b7der\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Mit Anmuth hat die W\u00fcrde sich geschm\u00fcckt,", "tokens": ["Mit", "An\u00b7muth", "hat", "die", "W\u00fcr\u00b7de", "si