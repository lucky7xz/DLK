{"textgrid.poem.54357": {"metadata": {"author": {"name": "Ziegler, Christiana Mariana von", "birth": "N.A.", "death": "N.A."}, "title": "3. Sch\u00e4ferlied", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Komm Sch\u00e4fer, komm zu Scherz und Lust,", "tokens": ["Komm", "Sch\u00e4\u00b7fer", ",", "komm", "zu", "Scherz", "und", "Lust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Fr\u00fchling labt die frohe Brust.", "tokens": ["Der", "Fr\u00fch\u00b7ling", "labt", "die", "fro\u00b7he", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ruft Schaf und Rinder in das Feld,", "tokens": ["Ruft", "Schaf", "und", "Rin\u00b7der", "in", "das", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir sind vor sie zur Huth bestellt.", "tokens": ["Wir", "sind", "vor", "sie", "zur", "Huth", "be\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Es z\u00e4ckt sich alles in der Luft,", "tokens": ["Es", "z\u00e4ckt", "sich", "al\u00b7les", "in", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", 