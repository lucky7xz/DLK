{"textgrid.poem.56054": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Don Juans Kindheit", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In seiner Schlankheit war, schon fast entscheidend,", "tokens": ["In", "sei\u00b7ner", "Schlank\u00b7heit", "war", ",", "schon", "fast", "ent\u00b7schei\u00b7dend", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "$,", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "der Bogen, der an Frauen nicht zerbricht;", "tokens": ["der", "Bo\u00b7gen", ",", "der", "an", "Frau\u00b7en", "nicht", "zer\u00b7bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "und manchmal, seine Stirne nicht mehr meidend,", "tokens": ["und", "manch\u00b7mal", ",", "sei\u00b7ne", "Stir\u00b7ne", "nicht", "mehr", "mei\u00b7dend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "PTKNEG", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "ging eine Neigung durch sein Angesicht", "tokens": ["ging", "ei\u00b7ne", "Nei\u00b7gung", "durch", "sein", "An\u00b7ge\u00b7sicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "zu einer die vor\u00fcberkam, zu einer", "tokens": ["zu", "ei\u00b7ner", "die", "vor\u00b7\u00fc\u00b7ber\u00b7kam", ",", "zu", "ei\u00b7ner"], "token_info