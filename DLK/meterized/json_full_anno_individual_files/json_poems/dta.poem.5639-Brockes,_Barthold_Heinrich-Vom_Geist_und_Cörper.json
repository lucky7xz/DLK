{"dta.poem.5639": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seele f\u00e4ngt also Jhr Seyn zu kennen an.", "tokens": ["Die", "See\u00b7le", "f\u00e4ngt", "al\u00b7so", "Ihr", "Seyn", "zu", "ken\u00b7nen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hernach betrachtet sie, was \u00e4usserlich,", "tokens": ["Her\u00b7nach", "be\u00b7trach\u00b7tet", "sie", ",", "was", "\u00e4us\u00b7ser\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und findet von den Gegenw\u00fcrffen sich,", "tokens": ["Und", "fin\u00b7det", "von", "den", "Ge\u00b7gen\u00b7w\u00fcrf\u00b7fen", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Durch deren Gegenwart, zu aller Zeit, bewoget,", "tokens": ["Durch", "de\u00b7ren", "Ge\u00b7gen\u00b7wart", ",", "zu", "al\u00b7ler", "Zeit", ",", "be\u00b7wo\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PRELAT", "NN", "$,", "APPR", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch, da\u00df, wenn die nicht da; sich die Bewegung leget.", "tokens": ["Auch", ",", "da\u00df", ",", "wenn", "die", "nicht", "da", ";", "sich", "die", "Be\u00b7we\u00b7gung", "le\u00b7get", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "$,", "KOUS", "ART", "PTKNEG", "ADV", "$.", "PRF", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Wodurch wir denn erkennen k\u00f6nnen", "tokens": ["Wo\u00b7durch", "wir", "denn", "er\u00b7ken\u00b7nen", "k\u00f6n\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Dinge, die wir C\u00f6rper nennen.", "tokens": ["Die", "Din\u00b7ge", ",", "die", "wir", "C\u00f6r\u00b7per", "nen\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ist unsre Seele nun durch einen Druck ger\u00fchrt,", "tokens": ["Ist", "uns\u00b7re", "See\u00b7le", "nun", "durch", "ei\u00b7nen", "Druck", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Den unsrer Sinne Werckzeug sp\u00fcrt;", "tokens": ["Den", "uns\u00b7rer", "Sin\u00b7ne", "Wer\u00b7ck\u00b7zeug", "sp\u00fcrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "So schliesset Sie, indem sie \u00fcberleget,", "tokens": ["So", "schlies\u00b7set", "Sie", ",", "in\u00b7dem", "sie", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die W\u00fcrckung, die den Sinnen eingepr\u00e4get,", "tokens": ["Die", "W\u00fcr\u00b7ckung", ",", "die", "den", "Sin\u00b7nen", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Da\u00df selbige nothwendig m\u00fcss\u2019 entspringen", "tokens": ["Da\u00df", "sel\u00b7bi\u00b7ge", "noth\u00b7wen\u00b7dig", "m\u00fcss'", "ent\u00b7sprin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "ADJD", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Vou fremden c\u00f6rperlichen Dingen.", "tokens": ["Vou", "frem\u00b7den", "c\u00f6r\u00b7per\u00b7li\u00b7chen", "Din\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}}}}}