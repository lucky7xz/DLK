{"dta.poem.12554": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "LxIV.  \n  Eichhorn.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1843", "urn": "urn:nbn:de:kobv:b4-200905192440", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Viel N\u00fcsse knackt es schwerlich,", "tokens": ["Viel", "N\u00fcs\u00b7se", "knackt", "es", "schwer\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein Maul ist alt und steif,", "tokens": ["Sein", "Maul", "ist", "alt", "und", "steif", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Kopf gar ungef\u00e4hrlich,", "tokens": ["Sein", "Kopf", "gar", "un\u00b7ge\u00b7f\u00e4hr\u00b7lich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Doch riesenhaft \u2014 sein Schweif.", "tokens": ["Doch", "rie\u00b7sen\u00b7haft", "sein", "Schweif", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$(", "PPOSAT", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}