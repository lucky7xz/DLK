{"textgrid.poem.41148": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dies Gespenst ist f\u00fcrchterlich!", "tokens": ["Dies", "Ge\u00b7spenst", "ist", "f\u00fcrch\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Mitternachts erhebt es sich", "tokens": ["Mit\u00b7ter\u00b7nachts", "er\u00b7hebt", "es", "sich"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus des Herrn Baronen Gruft.", "tokens": ["Aus", "des", "Herrn", "Ba\u00b7ro\u00b7nen", "Gruft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.4": {"text": "Dann, wenn's einen Bauern sieht,", "tokens": ["Dann", ",", "wenn's", "ei\u00b7nen", "Bau\u00b7ern", "sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "St\u00fcrzt es auf ihn aus der Luft,", "tokens": ["St\u00fcrzt", "es", "auf", "ihn", "aus", "der", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "H\u00e4ngt sich an sein Herz und zieht", "tokens": ["H\u00e4ngt", "sich", "an", "sein", "Herz", "und", "zieht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Alles Blut aus solchem schier.", "tokens": ["Al\u00b7les", "Blut", "aus", "sol\u00b7chem", "schier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dies Gespenst hei\u00dft man ", "tokens": ["Dies", "Ge\u00b7spenst", "hei\u00dft", "man"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "NN", "VVFIN", "PIS"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Ob das der Baron einst war,", "tokens": ["Ob", "das", "der", "Ba\u00b7ron", "einst", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "ART", "NN", "ADV", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Will und kann ich glauben nicht,", "tokens": ["Will", "und", "kann", "ich", "glau\u00b7ben", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "KON", "VMFIN", "PPER", "VVFIN", "PTKNEG", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Das w\u00e4r' gar zu arg f\u00fcrwahr!", "tokens": ["Das", "w\u00e4r'", "gar", "zu", "arg", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKA", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "Fragt man, leis der Bauer spricht:", "tokens": ["Fragt", "man", ",", "leis", "der", "Bau\u00b7er", "spricht", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bb's war des Herrn Baron sein alter", "tokens": ["\u00bb", "'