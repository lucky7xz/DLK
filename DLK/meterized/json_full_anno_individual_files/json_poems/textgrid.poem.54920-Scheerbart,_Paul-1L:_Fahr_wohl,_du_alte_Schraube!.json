{"textgrid.poem.54920": {"metadata": {"author": {"name": "Scheerbart, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fahr wohl, du alte Schraube!", "genre": "verse", "period": "N.A.", "pub_year": 1889, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahr wohl, du alte Schraube!", "tokens": ["Fahr", "wohl", ",", "du", "al\u00b7te", "Schrau\u00b7be", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir warst du sehr egal.", "tokens": ["Mir", "warst", "du", "sehr", "e\u00b7gal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir schmeckt die Lebenstraube,", "tokens": ["Mir", "schmeckt", "die", "Le\u00b7ben\u00b7strau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und dir ist alles Qual!", "tokens": ["Und", "dir", "ist", "al\u00b7les", "Qual", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Tu immer, was du wolltest;", "tokens": ["Tu", "im\u00b7mer", ",", "was", "du", "woll\u00b7test", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ich st\u00f6r dich nicht dabei.", "tokens": ["Ich", "st\u00f6r", "dich", "nicht", "da\u00b7bei", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PAV", "$."], "meter": "-+-+-+", "me