{"dta.poem.18483": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines fr\u00fch-kindes.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich eilte in die welt/ ich eilte aus der welt/", "tokens": ["Ich", "eil\u00b7te", "in", "die", "welt", "/", "ich", "eil\u00b7te", "aus", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$(", "PPER", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durchs erste hab ich leid/ durchs letzte freud\u2019 erwecket/", "tokens": ["Durchs", "ers\u00b7te", "hab", "ich", "leid", "/", "durchs", "letz\u00b7te", "freud'", "er\u00b7we\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VAFIN", "PPER", "ADJD", "$(", "APPRART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man h\u00e4tte lieber mich gesehen noch verstecket/", "tokens": ["Man", "h\u00e4t\u00b7te", "lie\u00b7ber", "mich", "ge\u00b7se\u00b7hen", "noch", "ver\u00b7ste\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "PPER", "VVPP", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da fr\u00fches wachsthum sonst fast jedermann gef\u00e4llt.", "tokens": ["Da", "fr\u00fc\u00b7hes", "wach\u00b7sthum", "sonst", "fast", "je\u00b7der\u00b7mann", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN"