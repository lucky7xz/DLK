{"textgrid.poem.38861": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du sahst, wie tief mich beugte sein Entfernen,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du sahst, wie tief mich beugte sein Entfernen,", "tokens": ["Du", "sahst", ",", "wie", "tief", "mich", "beug\u00b7te", "sein", "Ent\u00b7fer\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "ADJD", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da kam mir freundlich deine Lieb' entgegen,", "tokens": ["Da", "kam", "mir", "freund\u00b7lich", "dei\u00b7ne", "Lieb'", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da fiel in's d\u00fcrre Herz der frische Regen,", "tokens": ["Da", "fiel", "in's", "d\u00fcr\u00b7re", "Herz", "der", "fri\u00b7sche", "Re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Himmel gl\u00fcht' mit neuen Liebessternen.", "tokens": ["Der", "Him\u00b7mel", "gl\u00fcht'", "mit", "neu\u00b7en", "Lie\u00b7bess\u00b7ter\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sehr ich zagte, mu\u00dft ich wieder lernen,", "tokens": ["Wie", "sehr", "ich", "zag\u00b7te", ",", "mu\u00dft", "ich", "wie\u00b7der", "ler\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "VVFIN", "$,", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Seelen-Eintracht kann das Herz bewegen,", "tokens": ["Wie", "See\u00b7len\u00b7Ein\u00b7tracht", "kann", "das", "Herz", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trotz St\u00fcrmen mu\u00dften sich die Wogen legen", "tokens": ["Trotz", "St\u00fcr\u00b7men", "mu\u00df\u00b7ten", "sich", "die", "Wo\u00b7gen", "le\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und goldne Zukunft winkt' aus frohen Fernen.", "tokens": ["Und", "gold\u00b7ne", "Zu\u00b7kunft", "winkt'", "aus", "fro\u00b7hen", "Fer\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du gabst mir Trost, ich gab dir Muth zum Leben.", "tokens": ["Du", "gabst", "mir", "Trost", ",", "ich", "gab", "dir", "Muth", "zum", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "$,", "PPER", "VVFIN", "PPER", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wir sprachen: nie soll Leid uns niederdr\u00fccken!", "tokens": ["Wir", "spra\u00b7chen", ":", "nie", "soll", "Leid", "u