{"dta.poem.8816": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "4.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4ngst verlernt zu k\u00e4mpfen h\u00e4tt' ich deinetwegen manchen", "tokens": ["L\u00e4ngst", "ver\u00b7lernt", "zu", "k\u00e4mp\u00b7fen", "h\u00e4tt'", "ich", "dei\u00b7net\u00b7we\u00b7gen", "man\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "PTKZU", "VVINF", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "W\u00e4ren deine Blicke k\u00e4lter, deine Locken minder kraus;", "tokens": ["W\u00e4\u00b7ren", "dei\u00b7ne", "Bli\u00b7cke", "k\u00e4l\u00b7ter", ",", "dei\u00b7ne", "Lo\u00b7cken", "min\u00b7der", "kraus", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,", "PPOSAT", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Doch du bist ein Bild im Wasser, ohne Wesen und", "tokens": ["Doch", "du", "bist", "ein", "Bild", "im", "Was\u00b7ser", ",", "oh\u00b7ne", "We\u00b7sen", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "KOUI", "NN", "KON"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Wenn du auch dem Auge schmeichelst, weichst du doch", "tokens": ["Wenn", "du", "auch", "dem", "Au\u00b7ge", "schmei\u00b7chelst", ",", "weichst", "du", "doch"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "A