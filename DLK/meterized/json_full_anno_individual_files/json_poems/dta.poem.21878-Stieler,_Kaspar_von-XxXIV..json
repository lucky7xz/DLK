{"dta.poem.21878": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "XxXIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bahte sie auff Brodt und Wein", "tokens": ["Ich", "bah\u00b7te", "sie", "auff", "Brodt", "und", "Wein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und Rosilis verschwur zu kommen/", "tokens": ["und", "Ro\u00b7si\u00b7lis", "ver\u00b7schwur", "zu", "kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie hat den Grauen angenommen/", "tokens": ["Sie", "hat", "den", "Grau\u00b7en", "an\u00b7ge\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "da wir uns doch so freundlich hatten/", "tokens": ["da", "wir", "uns", "doch", "so", "freund\u00b7lich", "hat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADJD", "VAFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "als Luna spielte mit dem Schatten.", "tokens": ["als", "Lu\u00b7na", "spiel\u00b7te", "mit", "dem", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nu F\u00f6bus mahlt der L\u00fcffte Tohr/", "tokens": ["Nu", "F\u00f6\u00b7bus", "mahlt", "der", "L\u00fcff\u00b7te", "Tohr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich lob\u2019 es/ wo durch scheinsams hassen", "tokens": ["Ich", "lob'", "es", "/", "wo", "durch", "schein\u00b7sams", "has\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "man nur nicht Argwohn m\u00f6chte fassen.", "tokens": ["man", "nur", "nicht", "Arg\u00b7wohn", "m\u00f6ch\u00b7te", "fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADV", "PTKNEG", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}