["VVFIN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Gents, die ihre Wagen lenken \u2013", "tokens": ["Gents", ",", "die", "ih\u00b7re", "Wa\u00b7gen", "len\u00b7ken", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "M\u00e4dchen, die den Beutel schwenken \u2013", "tokens": ["M\u00e4d\u00b7chen", ",", "die", "den", "Beu\u00b7tel", "schwen\u00b7ken", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "wo das deutsche Leben zieht-", "tokens": ["wo", "das", "deut\u00b7sche", "Le\u00b7ben", "zieht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "TRUNC"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "singen wir das kleine Lied:", "tokens": ["sin\u00b7gen", "wir", "das", "klei\u00b7ne", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbwer bezahlt denn meine Steuern?", "tokens": ["\u00bb", "wer", "be\u00b7zahlt", "denn", "mei\u00b7ne", "Steu\u00b7ern", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VVPP", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bezahl sie ", "tokens": ["Be\u00b7zahl", "sie"], "token_info": ["word", "word"], "pos": ["KOUS", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Bei den Zeiten, bei den teuern \u2013", "tokens": ["Bei", "den", "Zei\u00b7ten", ",", "bei", "den", "teu\u00b7ern", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da mu\u00df man froh sein, wenns f\u00fcrn kleinen Kognak reicht!\u00ab", "tokens": ["da", "mu\u00df", "man", "froh", "sein", ",", "wenns", "f\u00fcrn", "klei\u00b7nen", "Ko\u00b7gnak", "reicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADJD", "VAINF", "$,", "KOUS", "ADJA", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "W\u00e4lder bla\u00df erdunkeln.", "tokens": ["W\u00e4l\u00b7der", "bla\u00df", "er\u00b7dun\u00b7keln", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Silberteiche funkeln.", "tokens": ["Sil\u00b7ber\u00b7tei\u00b7che", "fun\u00b7keln", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Schwer empor steigt Nickelmann,", "tokens": ["Schwer", "em\u00b7por", "steigt", "Ni\u00b7ckel\u00b7mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "VVFIN", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "f\u00e4ngt ein bi\u00dfchen mit den Elfen an.", "tokens": ["f\u00e4ngt", "ein", "bi\u00df\u00b7chen", "mit", "den", "El\u00b7fen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIS", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Waldesgeister weben.", "tokens": ["Wal\u00b7des\u00b7geis\u00b7ter", "we\u00b7ben", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Elfen nebbich schweben \u2013", "tokens": ["El\u00b7fen", "neb\u00b7bich", "schwe\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "auf dem Pfad, wo Mondschein geht \u2013", "tokens": ["auf", "dem", "Pfad", ",", "wo", "Mond\u00b7schein", "geht", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "weil das so bei Richard Wagner steht . . .", "tokens": ["weil", "das", "so", "bei", "Ric\u00b7hard", "Wag\u00b7ner", "steht", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["KOUS", "PDS", "ADV", "APPR", "NE", "NE", "VVFIN", "$.", "$.", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Und w\u00e4hrend Poesie die Luft durchzieht,", "tokens": ["Und", "w\u00e4h\u00b7rend", "Poe\u00b7sie", "die", "Luft", "durch\u00b7zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.10": {"text": "singt die kleine Elfe leis ihr Lied:", "tokens": ["singt", "die", "klei\u00b7ne", "El\u00b7fe", "leis", "ihr", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ADJD", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "\u00bbwer bezahlt denn meine Steuern?", "tokens": ["\u00bb", "wer", "be\u00b7zahlt", "denn", "mei\u00b7ne", "Steu\u00b7ern", "?"], "token_info": ["punct", "word", "wo