{"textgrid.poem.60499": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Protest", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich verachte heimliches Verschw\u00f6ren,", "tokens": ["Wenn", "ich", "ver\u00b7ach\u00b7te", "heim\u00b7li\u00b7ches", "Ver\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn ich hasse Meuchelm\u00f6rderhand,", "tokens": ["Und", "wenn", "ich", "has\u00b7se", "Meu\u00b7chel\u00b7m\u00f6r\u00b7der\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wenn in des Volkserretters Ruhmgewand", "tokens": ["Wenn", "in", "des", "Volk\u00b7ser\u00b7ret\u00b7ters", "Ruhm\u00b7ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verh\u00fcllte Schufte meinen Groll emp\u00f6ren,", "tokens": ["Ver\u00b7h\u00fcll\u00b7te", "Schuf\u00b7te", "mei\u00b7nen", "Groll", "em\u00b7p\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Reih ich das K\u00f6nigstum den Himmelsgaben,", "tokens": ["Reih", "ich", "das", "K\u00f6\u00b7nigs\u00b7tum", "den", "Him\u00b7mels\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verla\u00dfner V\u00f6lker Vaterhaus und Hort.", "tokens": ["Ver\u00b7la\u00df\u00b7ner", "V\u00f6l\u00b7ker", "Va\u00b7ter\u00b7haus", "und", "Hort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "O glaubet nicht, ich liebe drum sofort,", "tokens": ["O", "glau\u00b7bet", "nicht", ",", "ich", "lie\u00b7be", "drum", "so\u00b7fort", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PAV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was jetzt und hier an K\u00f6nigen wir haben.", "tokens": ["Was", "jetzt", "und", "hier", "an", "K\u00f6\u00b7ni\u00b7gen", "wir", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "KON", "ADV", "APPR", "NN", "PPER", "VAFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O glaubet nicht, ich f\u00fchre keinen Zunder", "tokens": ["O", "glau\u00b7bet", "nicht", ",", "ich", "f\u00fch\u00b7re", "kei\u00b7nen", "Zun\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Herzen f\u00fcr des Zornes edle Glut,", "tokens": ["Im", "Her\u00b7zen", "f\u00fcr", "des", "Zor\u00b7nes", "ed\u00b7le", "Glut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Tritt wo ein F\u00fcrst sein Volk im \u00dcbermut,", "tokens": ["Tritt", "wo", "ein", "F\u00fcrst", "sein", "Volk", "im", "\u00dc\u00b7ber\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PWAV", "ART", "NN", "PPOSAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch da\u00df ich ehren kann gekr\u00f6nten Plunder.", "tokens": ["Noch", "da\u00df", "ich", "eh\u00b7ren", "kann", "ge\u00b7kr\u00f6n\u00b7ten", "Plun\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "VMFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Nie wird mein Fl\u00fcgelro\u00df zum Schindergaule", "tokens": ["Nie", "wird", "mein", "Fl\u00fc\u00b7gel\u00b7ro\u00df", "zum", "Schin\u00b7der\u00b7gau\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcr meine Ehre, und mich strafe Gott,", "tokens": ["F\u00fcr", "mei\u00b7ne", "Eh\u00b7re", ",", "und", "mich", "stra\u00b7fe", "Gott", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sing ich ein F\u00fcrstenlied, da\u00df mir, zum Spott,", "tokens": ["Sing", "ich", "ein", "F\u00fcrs\u00b7ten\u00b7lied", ",", "da\u00df", "mir", ",", "zum", "Spott", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "KOUS", "PPER", "$,", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Hand vom Saitenspiel herunterfaule.", "tokens": ["Die", "Hand", "vom", "Sai\u00b7ten\u00b7spiel", "her\u00b7un\u00b7ter\