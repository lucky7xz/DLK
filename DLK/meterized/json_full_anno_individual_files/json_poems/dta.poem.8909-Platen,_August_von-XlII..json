{"dta.poem.8909": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "XlII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Ich mich betrogen wei\u00df und hintergangen,", "tokens": ["Ich", "mich", "be\u00b7tro\u00b7gen", "wei\u00df", "und", "hin\u00b7ter\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "VVPP", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du f\u00fcllst mein Herz mit brennendem Verlangen,", "tokens": ["Du", "f\u00fcllst", "mein", "Herz", "mit", "bren\u00b7nen\u00b7dem", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meinen Gaumen an mit Bitterkeiten.", "tokens": ["Und", "mei\u00b7nen", "Gau\u00b7men", "an", "mit", "Bit\u00b7ter\u00b7kei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Was nur dem Feinde mag der Feind bereiten,", "tokens": ["Was", "nur", "dem", "Fein\u00b7de", "mag", "der", "Feind", "be\u00b7rei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hab' ich von dir als Freundeslohn empfangen,", "tokens": ["Hab'", "ich", "von", "dir", "als", "Freun\u00b7des\u00b7lohn", "emp\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PPER", "KOUS", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich aber lasse deinen Namen prangen,", "tokens": ["Ich", "a\u00b7ber", "las\u00b7se", "dei\u00b7nen", "Na\u00b7men", "pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und \u00fcberlief're dich dem Lob der Zeiten.", "tokens": ["Und", "\u00fc\u00b7ber\u00b7lie\u00b7f'\u00b7re", "dich", "dem", "Lob", "der", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Bey diesem Thau, der mir im Auge flimmert,", "tokens": ["Bey", "die\u00b7sem", "Thau", ",", "der", "mir", "im", "Au\u00b7ge", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Noch geb' ich deine Liebe nicht verloren,", "tokens": ["Noch", "geb'", "ich", "dei\u00b7ne", "Lie\u00b7be", "nicht", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sehr dein Herz sich gegen mich verschlimmert!", "tokens": ["Wie", "sehr", "dein", "Herz", "sich", "ge\u00b7gen", "mich", "ver\u00b7schlim\u00b7mert", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPOSAT", "NN", "PRF", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Dich hat zum Spiegel sich der Lenz erkoren,", "tokens": ["Dich", "hat", "zum", "Spie\u00b7gel", "sich", "der", "Lenz", "er\u00b7ko\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PRF", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Jugend lacht auf deiner Stirn und schimmert", "tokens": ["Die", "Ju\u00b7gend", "lacht", "auf", "dei\u00b7ner", "Stirn", "und", "schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie ein Gemisch von Sonnen und Auroren!", "tokens": ["Wie", "ein", "Ge\u00b7misch", "von", "Son\u00b7nen", "und", "Au\u00b7ro\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}