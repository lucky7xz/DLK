{"dta.poem.21141": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An die Nachtigall .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du S\u00e4ngerin der N\u00e4chte,", "tokens": ["Du", "S\u00e4n\u00b7ge\u00b7rin", "der", "N\u00e4ch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du liebe Philomele,", "tokens": ["Du", "lie\u00b7be", "Phi\u00b7lo\u00b7me\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Du singest ja so kl\u00e4glich.", "tokens": ["Du", "sin\u00b7gest", "ja", "so", "kl\u00e4g\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was ist dir wiederfahren?", "tokens": ["Was", "ist", "dir", "wie\u00b7der\u00b7fah\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ich glaube, da\u00df du liebest.", "tokens": ["Ich", "glau\u00b7be", ",", "da\u00df", "du", "lie\u00b7best", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ach! lieber kleiner Vogel,", "tokens": ["Ach", "!", "lie\u00b7ber", "klei\u00b7ner", "Vo\u00b7gel", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich lieb auch, wie du liebest,", "tokens": ["Ich", "lieb", "auch", ",", "wie", "du", "lie\u00b7best", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bin der Stadt entflohen,", "tokens": ["Und", "bin", "der", "Stadt", "ent\u00b7flo\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und bin hieher gekommen,", "tokens": ["Und", "bin", "hie\u00b7her", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PAV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Einmal recht auszuweinen.", "tokens": ["Ein\u00b7mal", "recht", "aus\u00b7zu\u00b7wei\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Dort in den gro\u00dfen H\u00e4usern,", "tokens": ["Dort", "in", "den", "gro\u00b7\u00dfen", "H\u00e4u\u00b7sern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da ist man immer lustig;", "tokens": ["Da", "ist", "man", "im\u00b7mer", "lus\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da will man immer lachen;", "tokens": ["Da", "will", "man", "im\u00b7mer", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da sollt ich auch mit lachen;", "tokens": ["Da", "sollt", "ich", "auch", "mit", "la\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "APPR", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da bin ich weggelaufen.", "tokens": ["Da", "bin", "ich", "weg\u00b7ge\u00b7lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Komm, ich will mit dir klagen.", "tokens": ["Komm", ",", "ich", "will", "mit", "dir", "kla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPER", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wie z\u00e4rtlich kanst du klagen!", "tokens": ["Wie", "z\u00e4rt\u00b7lich", "kanst", "du", "kla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mich r\u00fchren deine Seufzer;", "tokens": ["Mich", "r\u00fch\u00b7ren", "dei\u00b7ne", "Seuf\u00b7zer", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du suchst wohl die Geliebte,", "tokens": ["Du", "suchst", "wohl", "die", "Ge\u00b7lieb\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die man von dir getrennet.", "tokens": ["Die", "man", "von", "dir", "ge\u00b7tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Mich hat von meinem M\u00e4dchen", "tokens": ["Mich", "hat", "von", "mei\u00b7nem", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das Schicksal auch getrennet.", "tokens": ["Das", "Schick\u00b7sal", "auch", "ge\u00b7tren\u00b7net", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Doch, Vogel, du bist gl\u00fccklich!", "tokens": ["Doch", ",", "Vo\u00b7gel", ",", "du", "bist", "gl\u00fcck\u00b7lich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "NE", "$,", "PPER", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sieh nur, du hast ja Fl\u00fcgel,", "tokens": ["Sieh", "nur", ",", "du", "hast", "ja", "Fl\u00fc\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Du kanst ja zu ihr fliegen.", "tokens": ["Du", "kanst", "ja", "zu", "ihr", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Ich wollte hier nicht sitzen,", "tokens": ["Ich", "woll\u00b7te", "hier", "nicht", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und um mein armes M\u00e4dchen", "tokens": ["Und", "um", "mein", "ar\u00b7mes", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "An diesen Linden weinen.", "tokens": ["An", "die\u00b7sen", "Lin\u00b7den", "wei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "H\u00e4tt ich nur deine Fl\u00fcgel;", "tokens": ["H\u00e4tt", "ich", "nur", "dei\u00b7ne", "Fl\u00fc\u00b7gel", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wie wollt ich zu ihr fliegen!", "tokens": ["Wie", "wollt", "ich", "zu", "ihr", "flie\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}