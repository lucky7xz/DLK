{"dta.poem.20431": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Wettstreit der tulpen/ nelcke/ rose/  \n lilie/ cypre\u00df/ narci\u00df und  \n hyacinth.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich bunte tulipan/ an allen farben reich;", "tokens": ["Ich", "bun\u00b7te", "tu\u00b7li\u00b7pan", "/", "an", "al\u00b7len", "far\u00b7ben", "reich", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "$(", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der g\u00e4rten sch\u00f6nster schmuck/ das h\u00f6chste meisterst\u00fccke/", "tokens": ["Der", "g\u00e4r\u00b7ten", "sch\u00f6ns\u00b7ter", "schmuck", "/", "das", "h\u00f6chs\u00b7te", "meis\u00b7ter\u00b7st\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So die natur erdacht. Welch pinsel mahlt die blicke/", "tokens": ["So", "die", "na\u00b7tur", "er\u00b7dacht", ".", "Welch", "pin\u00b7sel", "mahlt", "die", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVPP", "$.", "PIAT", "NN", "VVFIN", "ART", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die meiner bl\u00e4tter rund/ dem plitz und flammen gleich/", "tokens": ["Die", "mei\u00b7ner", "bl\u00e4t\u00b7ter", "rund", "/", "dem", "plitz", "und", "flam\u00b7men", "gleich", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "$(", "ART", "NN", "KON", "VVFIN", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So wunderlich dich zeugt/ itzt bin ich roth itzt bleich;", "tokens": ["So", "wun\u00b7der\u00b7lich", "dich", "zeugt", "/", "itzt", "bin", "ich", "roth", "itzt", "bleich", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "VVFIN", "$(", "ADV", "VAFIN", "PPER", "ADJD", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr meinen kulmen geht das kl\u00e4rste gold zur\u00fccke:", "tokens": ["F\u00fcr", "mei\u00b7nen", "kul\u00b7men", "geht", "das", "kl\u00e4rs\u00b7te", "gold", "zu\u00b7r\u00fc\u00b7cke", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Kein purpur ist so hoch/ den ich nicht unterdr\u00fccke/", "tokens": ["Kein", "pur\u00b7pur", "ist", "so", "hoch", "/", "den", "ich", "nicht", "un\u00b7ter\u00b7dr\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJD", "VAFIN", "ADV", "ADJD", "$(", "PRELS", "PPER", "PTKNEG", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So offt ge\u00e4ndert und gestr\u00f6hmet ist kein teich;", "tokens": ["So", "offt", "ge\u00b7\u00e4n\u00b7dert", "und", "ge\u00b7str\u00f6h\u00b7met", "ist", "kein", "teich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "KON", "VVPP", "VAFIN", "PIAT", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das heiter himmel blau und frische winter-gr\u00fcn/", "tokens": ["Das", "hei\u00b7ter", "him\u00b7mel", "blau", "und", "fri\u00b7sche", "win\u00b7ter\u00b7gr\u00fcn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mu\u00df in den bl\u00e4tter tracht durch s\u00fcsse wechsel ziehn.", "tokens": ["Mu\u00df", "in", "den", "bl\u00e4t\u00b7ter", "tracht", "durch", "s\u00fcs\u00b7se", "wech\u00b7sel", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Ihr andern blumen starrt ob meinen lieblichkeiten/", "tokens": ["Ihr", "an\u00b7dern", "blu\u00b7men", "starrt", "ob", "mei\u00b7nen", "lieb\u00b7lich\u00b7kei\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "KOUS", "PPOSAT", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der rothen rosen farb der liljen weiser schein/", "tokens": ["Der", "ro\u00b7then", "ro\u00b7sen", "farb", "der", "lil\u00b7jen", "wei\u00b7ser", "schein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ART", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "L\u00e4st mir das vorzugs recht/ und geht es willig ein/", "tokens": ["L\u00e4st", "mir", "das", "vor\u00b7zugs", "recht", "/", "und", "geht", "es", "wil\u00b7lig", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDS", "ADV", "ADJD", "$(", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df ich mich immer mag vor k\u00f6nigin ausbreiten.", "tokens": ["Da\u00df", "ich", "mich", "im\u00b7mer", "mag", "vor", "k\u00f6\u00b7ni\u00b7gin", "aus\u00b7brei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}