, "sei", "denn", ",", "du", "wirst", "neu", "ge\u00b7bo\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADV", "$,", "PPER", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "So sinkst du hier und bist f\u00fcr dort", "tokens": ["So", "sinkst", "du", "hier", "und", "bist", "f\u00fcr", "dort"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "KON", "VAFIN", "APPR", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und f\u00fcr die Seligkeit verloren.\u00ab", "tokens": ["Und", "f\u00fcr", "die", "Se\u00b7lig\u00b7keit", "ver\u00b7lo\u00b7ren", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVPP", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Geh hin, und bitte Gott, den Herrn,", "tokens": ["Geh", "hin", ",", "und", "bit\u00b7te", "Gott", ",", "den", "Herrn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "KON", "ADV", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er wolle gn\u00e4dig dir verzeihen.", "tokens": ["Er", "wol\u00b7le", "gn\u00e4\u00b7dig", "dir", "ver\u00b7zei\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Er thut es; ja, er thut es gern,", "tokens": ["Er", "thut", "es", ";", "ja", ",", "er", "thut", "es", "gern", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PTKANT", "$,", "PPER