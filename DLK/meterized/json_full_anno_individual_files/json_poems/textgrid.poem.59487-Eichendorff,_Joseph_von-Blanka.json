{"textgrid.poem.59487": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Blanka", "genre": "verse", "period": "N.A.", "pub_year": 1839, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbblanker seid Ihr, meine Herrin,", "tokens": ["\u00bb", "blan\u00b7ker", "seid", "Ihr", ",", "mei\u00b7ne", "Her\u00b7rin", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Blanker, als der Sonne Strahl!", "tokens": ["Blan\u00b7ker", ",", "als", "der", "Son\u00b7ne", "Strahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal sorglos m\u00f6cht ich schlafen", "tokens": ["Ein\u00b7mal", "sorg\u00b7los", "m\u00f6cht", "ich", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Waffen diese Nacht,", "tokens": ["Oh\u00b7ne", "Waf\u00b7fen", "die\u00b7se", "Nacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PDAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn wohl sieben lange Jahre", "tokens": ["Denn", "wohl", "sie\u00b7ben", "lan\u00b7ge", "Jah\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "CARD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Legt ich nicht die R\u00fcstung ab,", "tokens": ["Legt", "ich", "nicht", "die", "R\u00fcs\u00b7tung", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dunkler schon als ru\u00df'ge Kohlen", "tokens": ["Dunk\u00b7ler", "schon", "als", "ru\u00df'\u00b7ge", "Koh\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "KOUS", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist mein junger Leib vom Stahl.\u00ab", "tokens": ["Ist", "mein", "jun\u00b7ger", "Leib", "vom", "Stahl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbruhet diese Nacht nur, Ritter,", "tokens": ["\u00bb", "ru\u00b7het", "die\u00b7se", "Nacht", "nur", ",", "Rit\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "VVFIN", "PDAT", "NN", "ADV", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schlaft entwaffnet ohne Arg,", "tokens": ["Schlaft", "ent\u00b7waff\u00b7net", "oh\u00b7ne", "Arg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn der Graf ist fern im Walde,", "tokens": ["Denn", "der", "Graf", "ist", "fern", "im", "Wal\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Jagend \u00fcber Berg und Tal.", "tokens": ["Ja\u00b7gend", "\u00fc\u00b7ber", "Berg", "und", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollt, der Sturm zerriss' die Hunde", "tokens": ["Wollt", ",", "der", "Sturm", "zer\u00b7riss'", "die", "Hun\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "$,", "ART", "NN", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Adler ihm den Falk,", "tokens": ["Und", "der", "Ad\u00b7ler", "ihm", "den", "Falk", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und die Berg, im Grunde wankend,", "tokens": ["Und", "die", "Berg", ",", "im", "Grun\u00b7de", "wan\u00b7kend", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "St\u00fcrzten ihn vom Fels herab!\u00ab", "tokens": ["St\u00fcrz\u00b7ten", "ihn", "vom", "Fels", "her\u00b7ab", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PPER", "APPRART", "NN", "ADV", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Drauf, heimkehrend aus dem Walde,", "tokens": ["Drauf", ",", "heim\u00b7keh\u00b7rend", "aus", "dem", "Wal\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "$,", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Trat ins Zimmer ihr Gemahl:", "tokens": ["Trat", "ins", "Zim\u00b7mer", "ihr", "Ge\u00b7mahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00bbwas hier einsam sinnt Ihr, Dame?", "tokens": ["\u00bb", "was", "hier", "ein\u00b7sam", "sinnt", "Ihr", ",", "Da\u00b7me", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PWS", "ADV", "ADJD", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Euer Stamm ist voll Verrat.\u00ab \u2013", "tokens": ["Eu\u00b7er", "Stamm", "ist", "voll", "Ver\u00b7rat", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00bbherr, ich k\u00e4mme meine Locken,", "tokens": ["\u00bb", "herr", ",", "ich", "k\u00e4m\u00b7me", "mei\u00b7ne", "Lo\u00b7cken", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "K\u00e4mme sie mit gro\u00dfem Gram,", "tokens": ["K\u00e4m\u00b7me", "sie", "mit", "gro\u00b7\u00dfem", "Gram", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Weil Ihr so allein mich lasset,", "tokens": ["Weil", "Ihr", "so", "al\u00b7lein", "mich", "las\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Drau\u00dfen schweifend auf der Jagd. \u2013\u00ab", "tokens": ["Drau\u00b7\u00dfen", "schwei\u00b7fend", "auf", "der", "Jagd", ".", "\u2013", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "\u00bbdiese Worte, sch\u00f6ne Blanka,", "tokens": ["\u00bb", "die\u00b7se", "Wor\u00b7te", ",", "sch\u00f6\u00b7ne", "Blan\u00b7ka", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "PDAT", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.10": {"text": "Haben einen falschen Klang,", "tokens": ["Ha\u00b7ben", "ei\u00b7nen", "fal\u00b7schen", "Klang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Wessen ist das Ro\u00df im Hofe,", "tokens": ["Wes\u00b7sen", "ist", "das", "Ro\u00df", "im", "Ho\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Dessen Wiehern dort erschallt?\u00ab \u2013", "tokens": ["Des\u00b7sen", "Wie\u00b7hern", "dort", "er\u00b7schallt", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDAT", "NN", "ADV", "VVPP", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "\u00bbmeines Vaters R\u00f6\u00dflein ist es,", "tokens": ["\u00bb", "mei\u00b7nes", "Va\u00b7ters", "R\u00f6\u00df\u00b7lein", "ist", "es", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "NN", "NN", "VAFIN", "PPER", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Das er Euch geschickt zur Jagd.\u00ab \u2013", "tokens": ["Das", "er", "Euch", "ge\u00b7schickt", "zur", "Jagd", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PDS", "PPER", "PPER", "VVPP", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "\u00bbwessen sind die blanken Waffen,", "tokens": ["\u00bb", "wes\u00b7sen", "sind", "die", "blan\u00b7ken", "Waf\u00b7fen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die ich leuchten sah im Gang?\u00ab \u2013", "tokens": ["Die", "ich", "leuch\u00b7ten", "sah", "im", "Gang", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "PPER", "VVINF", "VVFIN", "APPRART", "NN", "$.", "$(", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "\u00bbherr, 's sind meines Bruders Waffen,", "tokens": ["\u00bb", "herr", ",", "'s", "sind", "mei\u00b7nes", "Bru\u00b7ders", "Waf\u00b7fen", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VAFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Euch hat er sie heut gesandt.\u00ab \u2013", "tokens": ["Euch", "hat", "er", "sie", "heut", "ge\u00b7sandt", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PPER", "ADV", "VVPP", "$.", "$(", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.19": {"text": "\u00bbwessen ist die fremde Lanze,", "tokens": ["\u00bb", "wes\u00b7sen", "ist", "die", "frem\u00b7de", "Lan\u00b7ze", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Die dort herblinkt von der Wand?\u00ab \u2013", "tokens": ["Die", "dort", "her\u00b7blinkt", "von", "der", "Wand", "?", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.21": {"text": "\u00bbnehmt sie rasch und sto\u00dft mich nieder,", "tokens": ["\u00bb", "nehmt", "sie", "rasch", "und", "sto\u00dft", "mich", "nie\u00b7der", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Das verdien ich, guter Graf!\u00ab", "tokens": ["Das", "ver\u00b7di\u00b7en", "ich", ",", "gu\u00b7ter", "Graf", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "\u00bbblanker seid Ihr, meine Herrin,", "tokens": ["\u00bb", "blan\u00b7ker", "seid", "Ihr", ",", "mei\u00b7ne", "Her\u00b7rin", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADJD", "VAFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Blanker, als der Sonne Strahl!", "tokens": ["Blan\u00b7ker", ",", "als", "der", "Son\u00b7ne", "Strahl", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einmal sorglos m\u00f6cht ich schlafen", "tokens": ["Ein\u00b7mal", "sorg\u00b7los", "m\u00f6cht", "ich", "schla\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VMFIN", "PPER", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ohne Waffen diese Nacht,", "tok