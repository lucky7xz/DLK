{"textgrid.poem.24732": {"metadata": {"author": {"name": "Hofmannsthal, Hugo von", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1901, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich will den Schatten einziger Geschicke", "tokens": ["Ich", "will", "den", "Schat\u00b7ten", "ein\u00b7zi\u00b7ger", "Ge\u00b7schi\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Gro\u00df an den Boden der Gedichte legen,", "tokens": ["Gro\u00df", "an", "den", "Bo\u00b7den", "der", "Ge\u00b7dich\u00b7te", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der jungen Helden ungeheure Blicke", "tokens": ["Der", "jun\u00b7gen", "Hel\u00b7den", "un\u00b7ge\u00b7heu\u00b7re", "Bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und andre G\u00f6tter, die den Sinn bewegen:", "tokens": ["Und", "and\u00b7re", "G\u00f6t\u00b7ter", ",", "die", "den", "Sinn", "be\u00b7we\u00b7gen", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,", "PRELS", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dann sollst du \u00fcber ihren Rand dich neigen", "tokens": ["Dann", "sollst", "du", "\u00fc\u00b7ber", "ih\u00b7ren", "Rand", "dich", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "