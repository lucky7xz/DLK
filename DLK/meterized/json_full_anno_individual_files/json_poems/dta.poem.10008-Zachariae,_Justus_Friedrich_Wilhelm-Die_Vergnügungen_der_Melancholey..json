{"dta.poem.10008": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn die Erde der blendende Stral des Mittags", "tokens": ["Wenn", "die", "Er\u00b7de", "der", "blen\u00b7den\u00b7de", "Stral", "des", "Mit\u00b7tags"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ART", "ADJA", "NN", "ART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Und in der hellen s\u00fcdlichen Laube des goldenen Tages", "tokens": ["Und", "in", "der", "hel\u00b7len", "s\u00fcd\u00b7li\u00b7chen", "Lau\u00b7be", "des", "gol\u00b7de\u00b7nen", "Ta\u00b7ges"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "---+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "G\u00fctger Regent sich freut, und alles unter ihm lachet:", "tokens": ["G\u00fct\u00b7ger", "Re\u00b7gent", "sich", "freut", ",", "und", "al\u00b7les", "un\u00b7ter", "ihm", "la\u00b7chet", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PRF", "VVFIN", "$,", "KON", "PIS", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.4": {"text": "Wie hat dann mein Wunsch nicht der Nacht Zur\u00fcck-", "tokens": ["Wie", "hat", "dann", "mein", "Wunsch", "nicht", "der", "Nacht", "Zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "PPOSAT", "NN", "PTKNEG", "ART", "NN", "TRUNC"], "meter": "-+-++--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die zum melancholischen Gem\u00fcth viel gleicher ge-", "tokens": ["Die", "zum", "me\u00b7lan\u00b7cho\u00b7li\u00b7schen", "Ge\u00b7m\u00fcth", "viel", "glei\u00b7cher", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPRART", "ADJA", "NN", "PIAT", "ADJA", "TRUNC"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Sey mir willkommen, o heilige Nacht! mein einsa-", "tokens": ["Sey", "mir", "will\u00b7kom\u00b7men", ",", "o", "hei\u00b7li\u00b7ge", "Nacht", "!", "mein", "ein\u00b7sa"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "FM", "ADJA", "NN", "$.", "PPOSAT", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.7": {"text": "Dir auch geweyht! o Schwester der herrschenden He-", "tokens": ["Dir", "auch", "ge\u00b7weyht", "!", "o", "Schwes\u00b7ter", "der", "herr\u00b7schen\u00b7den", "He"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVPP", "$.", "FM", "NN", "ART", "ADJA", "TRUNC"], "meter": "+--+-+--+--+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Heil dir! wenn du entweder, im dicken Dunkel ver-", "tokens": ["Heil", "dir", "!", "wenn", "du", "ent\u00b7we\u00b7der", ",", "im", "di\u00b7cken", "Dun\u00b7kel", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "KOUS", "PPER", "PTKVZ", "$,", "APPRART", "ADJA", "NN", "TRUNC"], "meter": "+--+-+--+-+-+", "measure": "iambic.hexa.invert"}, "line.9": {"text": "Deinen Wagen, verh\u00fcllt in schwangeren Wolken, da-", "tokens": ["Dei\u00b7nen", "Wa\u00b7gen", ",", "ver\u00b7h\u00fcllt", "in", "schwan\u00b7ge\u00b7ren", "Wol\u00b7ken", ",", "da"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,", "TRUNC"], "meter": "+-+--+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "Oder dein leuchtendes Haupt mit der silbernen Krone", "tokens": ["O\u00b7der", "dein", "leuch\u00b7ten\u00b7des", "Haupt", "mit", "der", "sil\u00b7ber\u00b7nen", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-", "measure": "dactylic.penta"}, "line.11": {"text": "Obgleich in der Finsterni\u00df Schutz der Zauberer Schaaren", "tokens": ["Ob\u00b7gleich", "in", "der", "Fins\u00b7ter\u00b7ni\u00df", "Schutz", "der", "Zau\u00b7be\u00b7rer", "Schaa\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "ART", "NN", "NN"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Oft in schrecklichen H\u00f6len von Lapplands beschneyten", "tokens": ["Oft", "in", "schreck\u00b7li\u00b7chen", "H\u00f6\u00b7len", "von", "Lap\u00b7plands", "be\u00b7schney\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "APPR", "NE", "VVFIN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.13": {"text": "Mit verworrenen Reimen den blutigen Kessel besprechen;", "tokens": ["Mit", "ver\u00b7wor\u00b7re\u00b7nen", "Rei\u00b7men", "den", "blu\u00b7ti\u00b7gen", "Kes\u00b7sel", "be\u00b7spre\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+--+--+--+--+-", "measure": "hexameter"}, "line.14": {"text": "Ob die Mordsucht gleich oft in deinen beschirmenden", "tokens": ["Ob", "die", "Mord\u00b7sucht", "gleich", "oft", "in", "dei\u00b7nen", "be\u00b7schir\u00b7men\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "ADJA"], "meter": "--+--+-+--+--", "measure": "anapaest.di.plus"}, "line.15": {"text": "Jhre Verehrer zusammenberuft, ein heimliches Blutbad", "tokens": ["Ih\u00b7re", "Ver\u00b7eh\u00b7rer", "zu\u00b7sam\u00b7men\u00b7be\u00b7ruft", ",", "ein", "heim\u00b7li\u00b7ches", "Blut\u00b7bad"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.16": {"text": "Auszudenken, indem bey blauer sterbender Lampe", "tokens": ["Aus\u00b7zu\u00b7den\u00b7ken", ",", "in\u00b7dem", "bey", "blau\u00b7er", "ster\u00b7ben\u00b7der", "Lam\u00b7pe"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "APPR", "ADJA", "ADJA", "NN"], "meter": "+-+-+-++-+--+-", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "In dem scheu\u00dflichen Rathe vereint, die horchende Bande", "tokens": ["In", "dem", "scheu\u00df\u00b7li\u00b7chen", "Ra\u00b7the", "ver\u00b7eint", ",", "die", "hor\u00b7chen\u00b7de", "Ban\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.18": {"text": "Sitzt; bey jedem s\u00e4uselnden Wind, bey jedem Ger\u00e4usche", "tokens": ["Sitzt", ";", "bey", "je\u00b7dem", "s\u00e4u\u00b7seln\u00b7den", "Wind", ",", "bey", "je\u00b7dem", "Ge\u00b7r\u00e4u\u00b7sche"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPR", "PIAT", "ADJA", "NN", "$,", "APPR", "PIAT", "NN"], "meter": "+-+-+--+-+--+-", "measure": "hexameter"}, "line.19": {"text": "Auff\u00e4hrt, und mit wilden und starrenden Augen um-", "tokens": ["Auf\u00b7f\u00e4hrt", ",", "und", "mit", "wil\u00b7den", "und", "star\u00b7ren\u00b7den", "Au\u00b7gen", "um"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KON", "APPR", "ADJA", "KON", "ADJA", "NN", "TRUNC"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.20": {"text": "Obgleich deinen entsetzlichen Pfad der Wandrer ver-", "tokens": ["Ob\u00b7gleich", "dei\u00b7nen", "ent\u00b7setz\u00b7li\u00b7chen", "Pfad", "der", "Wand\u00b7rer", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ART", "NN", "TRUNC"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Wenn er, v\u00f6llig verirrt in weiten Arabischen W\u00fcsten,", "tokens": ["Wenn", "er", ",", "v\u00f6l\u00b7lig", "ver\u00b7irrt", "in", "wei\u00b7ten", "A\u00b7ra\u00b7bi\u00b7schen", "W\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "VVPP", "APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+--+-+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.22": {"text": "Rings um sich her das wilde Geheul blutd\u00fcrstiger Thiere", "tokens": ["Rings", "um", "sich", "her", "das", "wil\u00b7de", "Ge\u00b7heul", "blut\u00b7d\u00fcrs\u00b7ti\u00b7ger", "Thie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "PRF", "APZR", "ART", "ADJA", "NN", "ADJA", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.23": {"text": "Durch die Wildni\u00df vernimmt, indem der schw\u00e4rzeste", "tokens": ["Durch", "die", "Wild\u00b7ni\u00df", "ver\u00b7nimmt", ",", "in\u00b7dem", "der", "schw\u00e4r\u00b7zes\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,", "KOUS", "ART", "ADJA"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.24": {"text": "Unaufh\u00f6rlich verfolgt: so ist doch deine Zur\u00fcckkunft", "tokens": ["Un\u00b7auf\u00b7h\u00f6r\u00b7lich", "ver\u00b7folgt", ":", "so", "ist", "doch", "dei\u00b7ne", "Zu\u00b7r\u00fcck\u00b7kunft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVPP", "$.", "ADV", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.25": {"text": "Angenehmer dem stillen Gem\u00fcth, als die Ankunft des", "tokens": ["An\u00b7ge\u00b7neh\u00b7mer", "dem", "stil\u00b7len", "Ge\u00b7m\u00fcth", ",", "als", "die", "An\u00b7kunft", "des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KOUS", "ART", "NN", "ART"], "meter": "+-+--+--+--+--", "measure": "trochaic.penta.relaxed"}, "line.26": {"text": "Wenn er auch jugendlich stolz im May frischbl\u00fchende", "tokens": ["Wenn", "er", "auch", "ju\u00b7gend\u00b7lich", "stolz", "im", "May", "frischbl\u00fc\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADJD", "APPRART", "NN", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.27": {"text": "Und ambrosischen Thau, von den Pforten des purpur-", "tokens": ["Und", "am\u00b7bro\u00b7si\u00b7schen", "Thau", ",", "von", "den", "Pfor\u00b7ten", "des", "pur\u00b7pur"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "ART", "NN", "ART", "TRUNC"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.28": {"text": "Auf die Gefilde verstreut. \u2014 Doch ist die Ankunft", "tokens": ["Auf", "die", "Ge\u00b7fil\u00b7de", "ver\u00b7streut", ".", "Doch", "ist", "die", "An\u00b7kunft"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$.", "$(", "KON", "VAFIN", "ART", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.29": {"text": "Angenehm, wenn er, verh\u00fcllt in tr\u00f6pfelnde Wolken, er-", "tokens": ["An\u00b7ge\u00b7nehm", ",", "wenn", "er", ",", "ver\u00b7h\u00fcllt", "in", "tr\u00f6p\u00b7feln\u00b7de", "Wol\u00b7ken", ",", "er"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "KOUS", "PPER", "$,", "VVFIN", "APPR", "ADJA", "NN", "$,", "TRUNC"], "meter": "+-+-+-+-+--+-+", "measure": "trochaic.septa.relaxed"}, "line.30": {"text": "Wenn in finsterer Luft der tr\u00fcbe S\u00fcdwind einherbraust,", "tokens": ["Wenn", "in", "fins\u00b7te\u00b7rer", "Luft", "der", "tr\u00fc\u00b7be", "S\u00fcd\u00b7wind", "ein\u00b7her\u00b7braust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-+-+", "measure": "anapaest.di.plus"}, "line.31": {"text": "Und die traurige Landfchaft schw\u00e4rzt, da\u00df W\u00e4lder und", "tokens": ["Und", "die", "trau\u00b7ri\u00b7ge", "Land\u00b7fchaft", "schw\u00e4rzt", ",", "da\u00df", "W\u00e4l\u00b7der", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,", "KOUS", "NN", "KON"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.32": {"text": "Sich, in einander vermengt, in dicken Nebeln verlieren.", "tokens": ["Sich", ",", "in", "ein\u00b7an\u00b7der", "ver\u00b7mengt", ",", "in", "di\u00b7cken", "Ne\u00b7beln", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "$,", "APPR", "PRF", "VVFIN", "$,", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.33": {"text": "K\u00fcmmerlich sitzen alsdann die S\u00e4nger des traurenden", "tokens": ["K\u00fcm\u00b7mer\u00b7lich", "sit\u00b7zen", "als\u00b7dann", "die", "S\u00e4n\u00b7ger", "des", "trau\u00b7ren\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "+--+-+--+--+--", "measure": "iambic.penta.invert"}, "line.34": {"text": "Und begr\u00fcssen die Dunkelheit nicht; die rauschenden", "tokens": ["Und", "be\u00b7gr\u00fcs\u00b7sen", "die", "Dun\u00b7kel\u00b7heit", "nicht", ";", "die", "rau\u00b7schen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKNEG", "$.", "ART", "NN"], "meter": "--+--+-++-+--", "measure": "anapaest.di.plus"}, "line.35": {"text": "Die mit majest\u00e4tischem Haupt in waldichten Reihen", "tokens": ["Die", "mit", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7schem", "Haupt", "in", "wal\u00b7dich\u00b7ten", "Rei\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.36": {"text": "Etwan ein Landhaus umringen, sind stumm; und schal-", "tokens": ["Et\u00b7wan", "ein", "Land\u00b7haus", "um\u00b7rin\u00b7gen", ",", "sind", "stumm", ";", "und", "scha\u00b7l"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ART", "NN", "VVINF", "$,", "VAFIN", "ADJD", "$.", "KON", "TRUNC"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.37": {"text": "Von der Dohlen verwirrten Geschrey, da, triefend,", "tokens": ["Von", "der", "Doh\u00b7len", "ver\u00b7wirr\u00b7ten", "Ge\u00b7schrey", ",", "da", ",", "trie\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "KOUS", "$,", "VVPP", "$,"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.38": {"text": "Sich das Federvieh macht; in Sicherheit h\u00e4nget der", "tokens": ["Sich", "das", "Fe\u00b7der\u00b7vieh", "macht", ";", "in", "Si\u00b7cher\u00b7heit", "h\u00e4n\u00b7get", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PRF", "ART", "NN", "VVFIN", "$.", "APPR", "NN", "VVFIN", "ART"], "meter": "--+-++-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Ueber dem prasselnden Feuer, und wagt aus der ruhi-", "tokens": ["Ue\u00b7ber", "dem", "pras\u00b7seln\u00b7den", "Feu\u00b7er", ",", "und", "wagt", "aus", "der", "ru\u00b7hi"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.40": {"text": "Nicht sich hinaus in den Sturm. In unvollendeter", "tokens": ["Nicht", "sich", "hin\u00b7aus", "in", "den", "Sturm", ".", "In", "un\u00b7voll\u00b7en\u00b7de\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "PRF", "APZR", "APPR", "ART", "NN", "$.", "APPR", "ADJA"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.41": {"text": "Feyert der Pflug; vom Get\u00f6ne des Horns, und dem", "tokens": ["Fe\u00b7yert", "der", "Pflug", ";", "vom", "Ge\u00b7t\u00f6\u00b7ne", "des", "Horns", ",", "und", "dem"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "APPRART", "NN", "ART", "NN", "$,", "KON", "ART"], "meter": "+--+--+--+--", "measure": "dactylic.tetra"}, "line.42": {"text": "Schallet der Forst nicht; in trauriger Stille liegt alles", "tokens"