{"textgrid.poem.47428": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[die Flamme hier, die wilde, zu verhehlen]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Flamme hier, die wilde, zu verhehlen,", "tokens": ["Die", "Flam\u00b7me", "hier", ",", "die", "wil\u00b7de", ",", "zu", "ver\u00b7heh\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "ART", "ADJA", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Schmerzen alle, welche mich zerqu\u00e4len,", "tokens": ["Die", "Schmer\u00b7zen", "al\u00b7le", ",", "wel\u00b7che", "mich", "zer\u00b7qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIS", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vermag ich es, da alle Winde ringsum", "tokens": ["Ver\u00b7mag", "ich", "es", ",", "da", "al\u00b7le", "Win\u00b7de", "ring\u00b7sum"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "$,", "KOUS", "PIAT", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Gr\u00fcnde meiner Traurigkeit erz\u00e4hlen?", "tokens": ["Die", "Gr\u00fcn\u00b7de", "mei\u00b7ner", "Trau\u00b7rig\u00b7keit", "er\u00b7z\u00e4h\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da\u00df ich ein St\u00e4ubchen deines Weges st\u00e4ube,", "tokens": ["Da\u00df", "ich", "ein", "St\u00e4ub\u00b7chen", "dei\u00b