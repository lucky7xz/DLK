{"textgrid.poem.63216": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Der erste Schnee", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der weisse Schnee befl\u00fcgelt mein Gehirn.", "tokens": ["Der", "weis\u00b7se", "Schnee", "be\u00b7fl\u00fc\u00b7gelt", "mein", "Ge\u00b7hirn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Tannen auch erscheinen sch\u00f6n besternt.", "tokens": ["Die", "Tan\u00b7nen", "auch", "er\u00b7schei\u00b7nen", "sch\u00f6n", "bes\u00b7ternt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJA", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So seien nun die Sonnen und die d\u00fcrrn", "tokens": ["So", "sei\u00b7en", "nun", "die", "Son\u00b7nen", "und", "die", "d\u00fcrrn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Oktoberzweige aus dem Blick entfernt.", "tokens": ["Ok\u00b7to\u00b7ber\u00b7zwei\u00b7ge", "aus", "dem", "Blick", "ent\u00b7fernt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wenn dieses Gl\u00fcck uns auch nicht w\u00e4rmer macht,", "tokens": ["Wenn", "die\u00b7ses", "Gl\u00fcck", "uns", "auch", "nicht", "w\u00e4r\u00b7mer", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "NN", "P