{"dta.poem.18064": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "XxI.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198462", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Weidmann fieng ein V\u00f6gellein,", "tokens": ["Ein", "Weid\u00b7mann", "fi\u00b7eng", "ein", "V\u00f6\u00b7gel\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das war voll Symphonie, doch klein;", "tokens": ["Das", "war", "voll", "Sym\u00b7pho\u00b7nie", ",", "doch", "klein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADJD", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es ward die Nachtigall genannt.", "tokens": ["Es", "ward", "die", "Nach\u00b7ti\u00b7gall", "ge\u00b7nannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Vogler nahm es in die Hand", "tokens": ["Der", "Vog\u00b7ler", "nahm", "es", "in", "die", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wollt ihm gleich den Hals umdrehn,", "tokens": ["Und", "wollt", "ihm", "gleich", "den", "Hals", "um\u00b7drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als es ihm flehte: La\u00df mich gehn,", "tokens": ["Als", "es", "ihm", "fleh\u00b7te", ":", "La\u00df", "mich", "gehn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$.", "VVIMP", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Du \u00e4ssest dich nicht satt von mir; ", "tokens": ["Du", "\u00e4s\u00b7sest", "dich", "nicht", "satt", "von", "mir", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "F\u00fcr selche Wohlthat geb ich dir", "tokens": ["F\u00fcr", "sel\u00b7che", "Wohlt\u00b7hat", "geb", "ich", "dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Drey Lehren, die dir n\u00fczlich seyn,", "tokens": ["Drey", "Leh\u00b7ren", ",", "die", "dir", "n\u00fcz\u00b7lich", "seyn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "$,", "PRELS", "PPER", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Pr\u00e4gst du sie im Ged\u00e4chtni\u00df ein.", "tokens": ["Pr\u00e4gst", "du", "sie", "im", "Ge\u00b7d\u00e4cht\u00b7ni\u00df", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Er sprach, sag an: was mag das seyn?", "tokens": ["Er", "sprach", ",", "sag", "an", ":", "was", "mag", "das", "seyn", "?"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVIMP", "PTKVZ", "$.", "PWS", "VMFIN", "PDS", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da sprach das kleine V\u00f6gellein:", "tokens": ["Da", "sprach", "das", "klei\u00b7ne", "V\u00f6\u00b7gel\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Die erste Lehre: Glaube nicht", "tokens": ["Die", "ers\u00b7te", "Leh\u00b7re", ":", "Glau\u00b7be", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das, was sich selber widerspricht.", "tokens": ["Das", ",", "was", "sich", "sel\u00b7ber", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Die andre: Gieb in deinem Hertzen", "tokens": ["Die", "and\u00b7re", ":", "Gieb", "in", "dei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PIS", "$.", "VVIMP", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Nicht Platz dem unbedachten Schmertzen", "tokens": ["Nicht", "Platz", "dem", "un\u00b7be\u00b7dach\u00b7ten", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.17": {"text": "Um was, das nicht kan widerkommen,", "tokens": ["Um", "was", ",", "das", "nicht", "kan", "wi\u00b7der\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "$,", "PRELS", "PTKNEG", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.18": {"text": "Dergleichen Leid mag niemand frommen.", "tokens": ["Derg\u00b7lei\u00b7chen", "Leid", "mag", "nie\u00b7mand", "from\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "Die dritt und letzte sch\u00e4rfft dir ein,", "tokens": ["Die", "dritt", "und", "letz\u00b7te", "sch\u00e4rfft", "dir", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJA", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Du sollest nicht bem\u00fchet seyn", "tokens": ["Du", "sol\u00b7lest", "nicht", "be\u00b7m\u00fc\u00b7het", "seyn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Um das, was dir nicht werden mag;", "tokens": ["Um", "das", ",", "was", "dir", "nicht", "wer\u00b7den", "mag", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PDS", "$,", "PWS", "PPER", "PTKNEG", "VAINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Der f\u00fchrt auf seinen Kopf den Schlag, ", "tokens": ["Der", "f\u00fchrt", "auf", "sei\u00b7nen", "Kopf", "den", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Der \u00fcbersiehet die\u00df Gebot,", "tokens": ["Der", "\u00fc\u00b7ber\u00b7sie\u00b7het", "die\u00df", "Ge\u00b7bot", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PDS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Und macht sich zu der Leute Spott.", "tokens": ["Und", "macht", "sich", "zu", "der", "Leu\u00b7te", "Spott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Beh\u00e4ltst du diese Lehren wohl,", "tokens": ["Be\u00b7h\u00e4ltst", "du", "die\u00b7se", "Leh\u00b7ren", "wohl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PDAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "So bist du aller Wei\u00dfheit voll.", "tokens": ["So", "bist", "du", "al\u00b7ler", "Wei\u00df\u00b7heit", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PIAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der Mann hatt an der Lehr Begn\u00fcgen", "tokens": ["Der", "Mann", "hatt", "an", "der", "Lehr", "Be\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Und lie\u00df den kleinen Vogel fliegen.", "tokens": ["Und", "lie\u00df", "den", "klei\u00b7nen", "Vo\u00b7gel", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Er flog auf einen Baum hinauf,", "tokens": ["Er", "flog", "auf", "ei\u00b7nen", "Baum", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Mit leichtem Muth, und sprach darauf:", "tokens": ["Mit", "leich\u00b7tem", "Muth", ",", "und", "sprach", "da\u00b7rauf", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "KON", "VVFIN", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Du solltest dich wohl selber hassen,", "tokens": ["Du", "soll\u00b7test", "dich", "wohl", "sel\u00b7ber", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Thor, da\u00df du mich hast fliegen lassen.", "tokens": ["Thor", ",", "da\u00df", "du", "mich", "hast", "flie\u00b7gen", "las\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "PPER", "VAFIN", "VVINF", "VVINF", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.33": {"text": "Das mu\u00df dir Schad und Ungl\u00fcck seyn. ", "tokens": ["Das", "mu\u00df", "dir", "Schad", "und", "Un\u00b7gl\u00fcck", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPER", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Denn ich trag einen edeln Stein", "tokens": ["Denn", "ich", "trag", "ei\u00b7nen", "e\u00b7deln", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Jm Leibe, wer ihn hat, wird gro\u00df,", "tokens": ["Jm", "Lei\u00b7be", ",", "wer", "ihn", "hat", ",", "wird", "gro\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWS", "PPER", "VAFIN", "$,", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Jm Felde nimmer sattellos.", "tokens": ["Jm", "Fel\u00b7de", "nim\u00b7mer", "sat\u00b7tel\u00b7los", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.37": {"text": "Er d\u00e4mpft und t\u00f6det allen Gift;", "tokens": ["Er", "d\u00e4mpft", "und", "t\u00f6\u00b7det", "al\u00b7len", "Gift", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "So gro\u00df, da\u00df er ein Strau\u00dfey \u00fcbertrift.", "tokens": ["So", "gro\u00df", ",", "da\u00df", "er", "ein", "Strau\u00b7\u00dfey", "\u00fc\u00b7bert\u00b7rift", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.39": {"text": "Du siehst, so viel hast du verlohren.", "tokens": ["Du", "siehst", ",", "so", "viel", "hast", "du", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "ADV", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Der Weidmann h\u00e4tte nun geschworen,", "tokens": ["Der", "Weid\u00b7mann", "h\u00e4t\u00b7te", "nun", "ge\u00b7schwo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.41": {"text": "Das alles w\u00e4re kurtzum wahr.", "tokens": ["Das", "al\u00b7les", "w\u00e4\u00b7re", "kurt\u00b7zum", "wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "VAFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "Aus dummem Sinn verga\u00df er gar", "tokens": ["Aus", "dum\u00b7mem", "Sinn", "ver\u00b7ga\u00df", "er", "gar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Der Lehren, die ihm erst der Vogel selbst gegeben.", "tokens": ["Der", "Leh\u00b7ren", ",", "die", "ihm", "erst", "der", "Vo\u00b7gel", "selbst", "ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Er ward betr\u00fcbt, als um sein Leben;", "tokens": ["Er", "ward", "be\u00b7tr\u00fcbt", ",", "als", "um", "sein", "Le\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.45": {"text": "Er glaubte, was sich widerspricht;", "tokens": ["Er", "glaub\u00b7te", ",", "was", "sich", "wi\u00b7der\u00b7spricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "PRF", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.46": {"text": "Er schonte seines Fleisses nicht", "tokens": ["Er", "schon\u00b7te", "sei\u00b7nes", "Fleis\u00b7ses", "nicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.47": {"text": "Den Vogel, der ihm so entgangen,", "tokens": ["Den", "Vo\u00b7gel", ",", "der", "ihm", "so", "ent\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.48": {"text": "Und sein Gel\u00fcck damit, zum andern mahl zu fangen.", "tokens": ["Und", "sein", "Ge\u00b7l\u00fcck", "da\u00b7mit", ",", "zum", "an\u00b7dern", "mahl", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PAV", "$,", "APPRART", "ADJA", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "Der weise Vogel redt ihm ein:", "tokens": ["Der", "wei\u00b7se", "Vo\u00b7gel", "redt", "ihm", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.50": {"text": "Willst du allzeit ein Narre seyn,", "tokens": ["Willst", "du", "all\u00b7zeit", "ein", "Nar\u00b7re", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.51": {"text": "Und denckst an meine Lehren nicht;", "tokens": ["Und", "denckst", "an", "mei\u00b7ne", "Leh\u00b7ren", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.52": {"text": "Da\u00df man nicht glauben soll, was selbst sich widerspricht?", "tokens": ["Da\u00df", "man", "nicht", "glau\u00b7ben", "soll", ",", "was", "selbst", "sich", "wi\u00b7der\u00b7spricht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PTKNEG", "VVINF", "VMFIN", "$,", "PRELS", "ADV", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.53": {"text": "In mir dergleichen grossen Stein", "tokens": ["In", "mir", "derg\u00b7lei\u00b7chen", "gros\u00b7sen", "Stein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "PIS", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.54": {"text": "Zu tragen, bin ich allzuklein.", "tokens": ["Zu", "tra\u00b7gen", ",", "bin", "ich", "all\u00b7zu\u00b7klein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "VAFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.55": {"text": "Daneben hast du Leid und Schmertzen", "tokens": ["Da\u00b7ne\u00b7ben", "hast", "du", "Leid", "und", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.56": {"text": "Um mich in deinem eiteln Hertzen,", "tokens": ["Um", "mich", "in", "dei\u00b7nem", "ei\u00b7teln", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.57": {"text": "Dieweil ich deiner Hand entgangen.", "tokens": ["Die\u00b7weil", "ich", "dei\u00b7ner", "Hand", "ent\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.58": {"text": "Zuletzt bist du bem\u00fcht, mich wiederum zu fangen.", "tokens": ["Zu\u00b7letzt", "bist", "du", "be\u00b7m\u00fcht", ",", "mich", "wie\u00b7de\u00b7rum", "zu", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "$,", "PPER", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.59": {"text": "Mein Weg und deiner sind nicht gleich,", "tokens": ["Mein", "Weg", "und", "dei\u00b7ner", "sind", "nicht", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "VAFIN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.60": {"text": "Du wirst an Weisheit nimmer reich.", "tokens": ["Du", "wirst", "an", "Weis\u00b7heit", "nim\u00b7mer", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}