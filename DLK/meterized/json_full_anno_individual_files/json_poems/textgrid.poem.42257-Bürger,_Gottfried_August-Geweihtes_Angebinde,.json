{"textgrid.poem.42257": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "Geweihtes Angebinde,", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kann denn nur der Vater Pabst allein", "tokens": ["Kann", "denn", "nur", "der", "Va\u00b7ter", "Pabst", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schwerter, Kerzen, Amulett' und Ringe", "tokens": ["Schwer\u00b7ter", ",", "Ker\u00b7zen", ",", "A\u00b7mu\u00b7lett'", "und", "Rin\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr die Frommen seiner Kirche weih'n,", "tokens": ["F\u00fcr", "die", "From\u00b7men", "sei\u00b7ner", "Kir\u00b7che", "weih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df kein Leid und Unheil an sie dringe? \u2013", "tokens": ["Da\u00df", "kein", "Leid", "und", "Un\u00b7heil", "an", "sie", "drin\u00b7ge", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIAT", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Freilich r\u00fchmt er sich mit stolzem Sinn", "tokens": ["Frei\u00b7lich", "r\u00fchmt", "er", "sich", "mit", "stol\u00b7zem", "Sinn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gottes h\u00f6chsten Priester auf der Erde;", "tokens": ["Got\u00b7tes", "h\u00f6chs\u00b7ten", "Pries\u00b7ter", "auf", "der", "Er\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aber ich, auch ich wei\u00df, was ich bin,", "tokens": ["A\u00b7ber", "ich", ",", "auch", "ich", "wei\u00df", ",", "was", "ich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "ADV", "PPER", "VVFIN", "$,", "PWS", "PPER", "VAFIN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wei\u00df, da\u00df ich ihm nimmer weichen werde.", "tokens": ["Wei\u00df", ",", "da\u00df", "ich", "ihm", "nim\u00b7mer", "wei\u00b7chen", "wer\u00b7de", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "KOUS", "PPER", "PPER", "ADV", "VVINF", "VAFIN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Denn ich bin zu hoher Priesterschaft,", "tokens": ["Denn", "ich", "bin", "zu", "ho\u00b7her", "Pries\u00b7ter\u00b7schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht, wie er, von Menschen auserkoren,", "tokens": ["Nicht", ",", "wie", "er", ",", "von", "Men\u00b7schen", "au\u00b7ser\u00b7ko\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PWAV", "PPER", "$,", "APPR", "NN", "VVIZU", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Bin dazu empfangen und geboren", "tokens": ["Bin", "da\u00b7zu", "emp\u00b7fan\u00b7gen", "und", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PAV", "VVPP", "KON", "VVPP"], "meter": "----+-+-+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Und emporgespro\u00dft durch Gottes Kraft!", "tokens": ["Und", "em\u00b7por\u00b7ge\u00b7spro\u00dft", "durch", "Got\u00b7tes", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Bin geweiht zum Priester des Apoll", "tokens": ["Bin", "ge\u00b7weiht", "zum", "Pries\u00b7ter", "des", "A\u00b7poll"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "VVPP", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Mit des Gottes Kranz und goldnem Stabe!", "tokens": ["Mit", "des", "Got\u00b7tes", "Kranz", "und", "gold\u00b7nem", "Sta\u00b7be", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Seines Geistes bin ich froh und voll;", "tokens": ["Sei\u00b7nes", "Geis\u00b7tes", "bin", "ich", "froh", "und", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Warum nicht auch frommer Wundergabe? \u2013", "tokens": ["Wa\u00b7rum", "nicht", "auch", "from\u00b7mer", "Wun\u00b7der\u00b7ga\u00b7be", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "PTKNEG", "ADV", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Ja, ich bin's! So weih' ich betend dann", "tokens": ["Ja", ",", "ich", "bin's", "!", "So", "weih'", "ich", "be\u00b7tend", "dann"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "$.", "ADV", "VVFIN", "PPER", "VVPP", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dieses Band mit Wunderkraft und Segen,", "tokens": ["Die\u00b7ses", "Band", "mit", "Wun\u00b7der\u00b7kraft", "und", "Se\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Da\u00df ich's an ", "tokens": ["Da\u00df", "ich's", "an"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PIS", "PTKVZ"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Und damit Ihr Herz begl\u00fccken kann;", "tokens": ["Und", "da\u00b7mit", "Ihr", "Herz", "be\u00b7gl\u00fc\u00b7cken", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPOSAT", "NN", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "O ein Herz, des besten Gl\u00fcckes wert!", "tokens": ["O", "ein", "Herz", ",", "des", "bes\u00b7ten", "Gl\u00fc\u00b7ckes", "wert", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Das ich nie zu r\u00fchmen mich bestrebe,", "tokens": ["Das", "ich", "nie", "zu", "r\u00fch\u00b7men", "mich", "be\u00b7stre\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "PTKZU", "VVINF", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Weil der sch\u00f6nste Name, den ich gebe,", "tokens": ["Weil", "der", "sch\u00f6ns\u00b7te", "Na\u00b7me", ",", "den", "ich", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Doch dies Herz noch nicht genugsam ehrt. \u2013", "tokens": ["Doch", "dies", "Herz", "noch", "nicht", "ge\u00b7nug\u00b7sam", "ehrt", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PDS", "NN", "ADV", "PTKNEG", "ADJD", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Band, ich segne dich mit Freud' und Lust,", "tokens": ["Band", ",", "ich", "seg\u00b7ne", "dich", "mit", "Freud'", "und", "Lust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "F\u00fcr das l\u00e4ngste Leben, sonder Gr\u00e4men;", "tokens": ["F\u00fcr", "das", "l\u00e4ngs\u00b7te", "Le\u00b7ben", ",", "son\u00b7der", "Gr\u00e4\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Diesen Segen sollst du in die Brust", "tokens": ["Die\u00b7sen", "Se\u00b7gen", "sollst", "du", "in", "die", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "APPR", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Meiner ", "tokens": ["Mei\u00b7ner"], "token_info": ["word"], "pos": ["PPOSAT"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Freud' und Lust an ", "tokens": ["Freud'", "und", "Lust", "an"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "APPR"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ein Jahrhundert, oder nicht viel minder,", "tokens": ["Ein", "Jahr\u00b7hun\u00b7dert", ",", "o\u00b7der", "nicht", "viel", "min\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "PTKNEG", "ADV", "ADV", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Freud' und Lust an allem ab und an,", "tokens": ["Freud'", "und", "Lust", "an", "al\u00b7lem", "ab", "und", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "PIS", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "An und ab dem Kleeblatt holder Kinder;", "tokens": ["An", "und", "ab", "dem", "Klee\u00b7blatt", "hol\u00b7der", "Kin\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KON", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Freud' und Lust, von keinem Harm verg\u00e4llt,", "tokens": ["Freud'", "und", "Lust", ",", "von", "kei\u00b7nem", "Harm", "ver\u00b7g\u00e4llt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "APPR", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Sei durch dich ", "tokens": ["Sei", "durch", "dich"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "APPR", "PPER"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Freud' an Gottes ganzer weiter Welt,", "tokens": ["Freud'", "an", "Got\u00b7tes", "gan\u00b7zer", "wei\u00b7ter", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Mich, den Priester, auch mit eingeschlossen!", "tokens": ["Mich", ",", "den", "Pries\u00b7ter", ",", "auch", "mit", "ein\u00b7ge\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "ADV", "APPR", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.10": {"line.1": {"text": "Kann denn nur der Vater Pabst allein", "tokens": ["Kann", "denn", "nur", "der", "Va\u00b7ter", "Pabst", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "ART", "NN", "NN", "ADV"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Schwerter, Kerzen, Amulett' und Ringe", "tokens": ["Schwer\u00b7ter", ",", "Ker\u00b7zen", ",", "A\u00b7mu\u00b7lett'", "und", "Rin\u00b7ge"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "NE", "KON", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr die Frommen seiner Kirche weih'n,", "tokens": ["F\u00fcr", "die", "From\u00b7men", "sei\u00b7ner", "Kir\u00b7che", "weih'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Da\u00df kein Leid und Unheil an sie dringe? \u2013", "tokens": ["Da\u00df", "kein", "Leid", "und", "Un\u00b7heil", "an", "sie", "drin\u00b7ge", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"