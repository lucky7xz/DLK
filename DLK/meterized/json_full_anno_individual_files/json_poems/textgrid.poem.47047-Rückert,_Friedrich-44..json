{"textgrid.poem.47047": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "44.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hab' es wohl gef\u00fchlt, da\u00df eine Binde", "tokens": ["Ich", "hab'", "es", "wohl", "ge\u00b7f\u00fchlt", ",", "da\u00df", "ei\u00b7ne", "Bin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von Amors Zaubern um mein Antlitz hange;", "tokens": ["Von", "A\u00b7mors", "Zau\u00b7bern", "um", "mein", "Ant\u00b7litz", "han\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich hab' es wohl gemerkt, da\u00df eine Spange", "tokens": ["Ich", "hab'", "es", "wohl", "ge\u00b7merkt", ",", "da\u00df", "ei\u00b7ne", "Span\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Von seinen T\u00e4uschungen den Geist umwinde.", "tokens": ["Von", "sei\u00b7nen", "T\u00e4u\u00b7schun\u00b7gen", "den", "Geist", "um\u00b7win\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Ich aber wollte selber meine blinde", "tokens": ["Ich", "a\u00b7ber", "woll\u00b7te", "sel\u00b7ber", "mei\u00b7ne", "blin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gl\u00fcckseligkeit nicht st\u00f6ren in dem Gange;", "tokens": ["Gl\u00fcck\u00b7se\u00b7lig\u00b7keit", "nicht", "st\u00f6\u00b7ren", "in", "dem", "Gan\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ach, dem Geschick w\u00e4hrt bald ein Gl\u00fcck zu lange,", "tokens": ["Ach", ",", "dem", "Ge\u00b7schick", "w\u00e4hrt", "bald", "ein", "Gl\u00fcck", "zu", "lan\u00b7ge", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "NN", "VVFIN", "ADV", "ART", "NN", "APPR", "ADV", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und weise ruft es meiner Thorheit: Schwinde!", "tokens": ["Und", "wei\u00b7se", "ruft", "es", "mei\u00b7ner", "Thor\u00b7heit", ":", "Schwin\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich hab' es ja gewu\u00dft, da\u00df ich getr\u00e4umet,", "tokens": ["Ich", "hab'", "es", "ja", "ge\u00b7wu\u00dft", ",", "da\u00df", "ich", "ge\u00b7tr\u00e4u\u00b7met", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch wollt' ich selbst nicht meinen Traum zerschlagen,", "tokens": ["Doch", "wollt'", "ich", "selbst", "nicht", "mei\u00b7nen", "Traum", "zer\u00b7schla\u00b