{"dta.poem.10185": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Einige Natur-Kr\u00e4ffte, Gesetze und Ei-  \n genschaften, zu Ehren ihres allm\u00e4chtigen Be-  \n herrschers, bey dem Jahrs-Wechsel des  \n 1731. Jahrs betrachtet.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "HeRR der Jahre, Tag' und Zeiten!", "tokens": ["HeRR", "der", "Jah\u00b7re", ",", "Tag'", "und", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Krafft der Kr\u00e4ffte! Born des Lichts!", "tokens": ["Krafft", "der", "Kr\u00e4ff\u00b7te", "!", "Born", "des", "Lichts", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$.", "NE", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ursprungs-Meer der Ewigkeiten!", "tokens": ["Ur\u00b7sprungs\u00b7Meer", "der", "E\u00b7wig\u00b7kei\u00b7ten", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Blo\u00df durch Deine Huld geschichts,", "tokens": ["Blo\u00df", "durch", "Dei\u00b7ne", "Huld", "ge\u00b7schichts", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df wir nunmehr abermahl", "tokens": ["Da\u00df", "wir", "nun\u00b7mehr", "a\u00b7ber\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Zu der Sonnen warmen Strahl,", "tokens": ["Zu", "der", "Son\u00b7nen", "war\u00b7men", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit der regen Last der Erden,", "tokens": ["Mit", "der", "re\u00b7gen", "Last", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Wieder\u00fcm gef\u00fchret werden.", "tokens": ["Wie\u00b7de\u00b7r\u00fcm", "ge\u00b7f\u00fch\u00b7ret", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "La\u00df uns daf\u00fcr Dir allein,", "tokens": ["La\u00df", "uns", "da\u00b7f\u00fcr", "Dir", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PAV", "PPER", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Als dem Sch\u00f6pfer, danckbar seyn!", "tokens": ["Als", "dem", "Sch\u00f6p\u00b7fer", ",", "dan\u00b7ck\u00b7bar", "seyn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADJD", "VAINF", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.2": {"line.1": {"text": "Entferne dich, Seele, von eitelen Dingen!", "tokens": ["Ent\u00b7fer\u00b7ne", "dich", ",", "See\u00b7le", ",", "von", "ei\u00b7te\u00b7len", "Din\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Ermuntre dich! rege die geistigen Schwingen,", "tokens": ["Er\u00b7munt\u00b7re", "dich", "!", "re\u00b7ge", "die", "geis\u00b7ti\u00b7gen", "Schwin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und fache, voll Andacht und Ehr-Furcht, die Flammen", "tokens": ["Und", "fa\u00b7che", ",", "voll", "An\u00b7dacht", "und", "Ehr\u00b7Furcht", ",", "die", "Flam\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "ADJA", "$,", "ADJD", "NN", "KON", "NN", "$,", "ART", "NN"], "meter": "-+--++-++-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Inbr\u00fcnstiger Danckbarkeit hefftiger an!", "tokens": ["In\u00b7br\u00fcns\u00b7ti\u00b7ger", "Dan\u00b7ck\u00b7bar\u00b7keit", "heff\u00b7ti\u00b7ger", "an", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+--+-+-+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Entz\u00fcnde dich! ziehe die Kr\u00e4ffte zusammen!", "tokens": ["Ent\u00b7z\u00fcn\u00b7de", "dich", "!", "zie\u00b7he", "die", "Kr\u00e4ff\u00b7te", "zu\u00b7sam\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.6": {"text": "Vereine sie, desto geschickter zu werden,", "tokens": ["Ver\u00b7ei\u00b7ne", "sie", ",", "des\u00b7to", "ge\u00b7schick\u00b7ter", "zu", "wer\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "ADJD", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Des herrlichen Sch\u00f6pfers der Himmel, der Erden", "tokens": ["Des", "herr\u00b7li\u00b7chen", "Sch\u00f6p\u00b7fers", "der", "Him\u00b7mel", ",", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.8": {"text": "Lieb\u2019, Allmacht und Weisheit stets mehr zu bemercken!", "tokens": ["Lieb'", ",", "All\u00b7macht", "und", "Weis\u00b7heit", "stets", "mehr", "zu", "be\u00b7mer\u00b7cken", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADV", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.9": {"text": "Sey rege durch feuriger Danckbarkeit Triebe,", "tokens": ["Sey", "re\u00b7ge", "durch", "feu\u00b7ri\u00b7ger", "Dan\u00b7ck\u00b7bar\u00b7keit", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJA", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "+---+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "Erkenntlicher Sehnsucht und flammender Liebe,", "tokens": ["Er\u00b7kennt\u00b7li\u00b7cher", "Sehn\u00b7sucht", "und", "flam\u00b7men\u00b7der", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.11": {"text": "F\u00fcr alle vom Sch\u00f6pfer erhaltene G\u00fcte", "tokens": ["F\u00fcr", "al\u00b7le", "vom", "Sch\u00f6p\u00b7fer", "er\u00b7hal\u00b7te\u00b7ne", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "APPRART", "NN", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.12": {"text": "Mit br\u00fcnstig- und innig-ger\u00fchrtem Gem\u00fcthe,", "tokens": ["Mit", "br\u00fcns\u00b7tig", "und", "in\u00b7nig\u00b7ge\u00b7r\u00fchr\u00b7tem", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "ADJA", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Ein frohes Erkenntlichkeits-Opfer zu bringen,", "tokens": ["Ein", "fro\u00b7hes", "Er\u00b7kennt\u00b7lich\u00b7keits\u00b7Op\u00b7fer", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.14": {"text": "Und G\u00f6ttlicher Allmacht ein Lob-Lied zu singen!", "tokens": ["Und", "G\u00f6tt\u00b7li\u00b7cher", "All\u00b7macht", "ein", "Lob\u00b7Lied", "zu", "sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+--++-++-+-", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Danck, das Lob nun, welches wir,", "tokens": ["Der", "Danck", ",", "das", "Lob", "nun", ",", "wel\u00b7ches", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "ADV", "$,", "PRELS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "O wunderth\u00e4tger Sch\u00f6pfer, Dir,", "tokens": ["O", "wun\u00b7der\u00b7th\u00e4t\u00b7ger", "Sch\u00f6p\u00b7fer", ",", "Dir", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bey dieser Wechsel-Zeit, da sich das Jahr erneuet,", "tokens": ["Bey", "die\u00b7ser", "Wech\u00b7sel\u00b7Zeit", ",", "da", "sich", "das", "Jahr", "er\u00b7neu\u00b7et", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und da die neue Nachbarschafft", "tokens": ["Und", "da", "die", "neu\u00b7e", "Nach\u00b7bar\u00b7schafft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der, blo\u00df allein durch Deines Willens Krafft", "tokens": ["Der", ",", "blo\u00df", "al\u00b7lein", "durch", "Dei\u00b7nes", "Wil\u00b7lens", "Krafft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Gewordnen Sonnen, uns mit W\u00e4rm\u2019 und Licht erfreuet,", "tokens": ["Ge\u00b7word\u00b7nen", "Son\u00b7nen", ",", "uns", "mit", "W\u00e4rm'", "und", "Licht", "er\u00b7freu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00fcr so viel Gnad und Huld zu geben,", "tokens": ["F\u00fcr", "so", "viel", "Gnad", "und", "Huld", "zu", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und darzulegen uns bestreben;", "tokens": ["Und", "dar\u00b7zu\u00b7le\u00b7gen", "uns", "be\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Soll dieses mahl darin bestehn:", "tokens": ["Soll", "die\u00b7ses", "mahl", "da\u00b7rin", "be\u00b7stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "ADV", "PAV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jm Reiche der Natur verschiednes anzusehn,", "tokens": ["Jm", "Rei\u00b7che", "der", "Na\u00b7tur", "ver\u00b7schied\u00b7nes", "an\u00b7zu\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NE", "ART", "NN", "ADJA", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wodurch, in Lufft und Fluth und auf der Erden,", "tokens": ["Wo\u00b7durch", ",", "in", "Lufft", "und", "Fluth", "und", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "NN", "KON", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Erstaunens-w\u00fcrdige Bewegungen geschehn,", "tokens": ["Er\u00b7stau\u00b7nens\u00b7w\u00fcr\u00b7di\u00b7ge", "Be\u00b7we\u00b7gun\u00b7gen", "ge\u00b7schehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+---+", "measure": "unknown.measure.penta"}, "line.13": {"text": "Und alle, Dir zum Ruhm, o HERR! verrichtet werden.", "tokens": ["Und", "al\u00b7le", ",", "Dir", "zum", "Ruhm", ",", "o", "HeRR", "!", "ver\u00b7rich\u00b7tet", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "PPER", "APPRART", "NN", "$,", "FM", "NN", "$.", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da alle Regungen nur blo\u00df allein,", "tokens": ["Da", "al\u00b7le", "Re\u00b7gun\u00b7gen", "nur", "blo\u00df", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "O aller Kr\u00e4ffte Krafft, durch Dich verrichtet seyn;", "tokens": ["O", "al\u00b7ler", "Kr\u00e4ff\u00b7te", "Krafft", ",", "durch", "Dich", "ver\u00b7rich\u00b7tet", "seyn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "NN", "$,", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was k\u00f6nnen denn der Menschen Seelen", "tokens": ["Was", "k\u00f6n\u00b7nen", "denn", "der", "Men\u00b7schen", "See\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "ADV", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr einen edlern Endzweck wehlen?", "tokens": ["F\u00fcr", "ei\u00b7nen", "ed\u00b7lern", "End\u00b7zweck", "weh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie k\u00f6nnen wir doch des Verstandes Kr\u00e4ffte,", "tokens": ["Wie", "k\u00f6n\u00b7nen", "wir", "doch", "des", "Ver\u00b7stan\u00b7des", "Kr\u00e4ff\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Womit der Sch\u00f6pfer sie beschenckt,", "tokens": ["Wo\u00b7mit", "der", "Sch\u00f6p\u00b7fer", "sie", "be\u00b7schenckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und die er nicht von ungefehr gegeben,", "tokens": ["Und", "die", "er", "nicht", "von", "un\u00b7ge\u00b7fehr", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELS", "PPER", "PTKNEG", "APPR", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu einem GOTT-gef\u00e4lligern Gesch\u00e4ffte", "tokens": ["Zu", "ei\u00b7nem", "GoT\u00b7T\u00b7ge\u00b7f\u00e4l\u00b7li\u00b7gern", "Ge\u00b7sch\u00e4ff\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Hier anzuwenden uns bestreben;", "tokens": ["Hier", "an\u00b7zu\u00b7wen\u00b7den", "uns", "be\u00b7stre\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIZU", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als wenn man Geist und Witz in Seine Wercke sencket,", "tokens": ["Als", "wenn", "man", "Geist", "und", "Witz", "in", "Sei\u00b7ne", "Wer\u00b7cke", "sen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PIS", "NN", "KON", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Die Ordnungen, wodurch Er alles lencket,", "tokens": ["Die", "Ord\u00b7nun\u00b7gen", ",", "wo\u00b7durch", "Er", "al\u00b7les", "len\u00b7cket", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und der Natur Bewegungen, bedencket;", "tokens": ["Und", "der", "Na\u00b7tur", "Be\u00b7we\u00b7gun\u00b7gen", ",", "be\u00b7den\u00b7cket", ";"], "token_info": ["word", 