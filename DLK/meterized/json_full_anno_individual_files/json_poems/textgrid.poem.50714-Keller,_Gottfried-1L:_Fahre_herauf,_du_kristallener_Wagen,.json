{"textgrid.poem.50714": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Fahre herauf, du kristallener Wagen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Fahre herauf, du kristallener Wagen,", "tokens": ["Fah\u00b7re", "her\u00b7auf", ",", "du", "kris\u00b7tal\u00b7le\u00b7ner", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Klingender Morgen, so frisch und so klar!", "tokens": ["Klin\u00b7gen\u00b7der", "Mor\u00b7gen", ",", "so", "frisch", "und", "so", "klar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Seidene Wimpel, vom Ostwind getragen,", "tokens": ["Sei\u00b7de\u00b7ne", "Wim\u00b7pel", ",", "vom", "Ost\u00b7wind", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Flattre, du rosige W\u00f6lkleinschar!", "tokens": ["Flatt\u00b7re", ",", "du", "ro\u00b7si\u00b7ge", "W\u00f6lk\u00b7lein\u00b7schar", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Siehe die Meere, sie schaukeln und branden,", "tokens": ["Sie\u00b7he", "die", "Mee\u00b7re", ",", "sie", "schau\u00b7keln", "und", "bran\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ART", "NN", "$,", "PPER", "VVINF", "KON", "VVFIN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Fr\u00f6hlich die Brise vom Morgenland weht,", "tokens": ["Fr\u00f6h\u00b7lich", "die", "Bri\u00b7se", "vom", "Mor\u00b7gen\u00b7land", "weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "S\u00fchnend erfunkelt der Tau auf den Landen,", "tokens": ["S\u00fch\u00b7nend", "er\u00b7fun\u00b7kelt", "der", "Tau", "auf", "den", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Weihbrunn zum heiligen Sonnengebet.", "tokens": ["Weih\u00b7brunn", "zum", "hei\u00b7li\u00b7gen", "Son\u00b7nen\u00b7ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Tausendfach wollen die Blumen entriegeln", "tokens": ["Tau\u00b7send\u00b7fach", "wol\u00b7len", "die", "Blu\u00b7men", "ent\u00b7rie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Aus ihrer Brust den gefangenen Gott;", "tokens": ["Aus", "ih\u00b7rer", "Brust", "den", "ge\u00b7fan\u00b7ge\u00b7nen", "Gott", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Doch die vergoldeten Kreuze bespiegeln", "tokens": ["Doch", "die", "ver\u00b7gol\u00b7de\u00b7ten", "Kreu\u00b7ze", "be\u00b7spie\u00b7geln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sich auf den Domen mit glei\u00dfendem Spott!", "tokens": ["Sich", "auf", "den", "Do\u00b7men", "mit", "glei\u00b7\u00dfen\u00b7dem", "Spott", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Singen nicht Lerchen dort hoch in den L\u00fcften,", "tokens": ["Sin\u00b7gen", "nicht", "Ler\u00b7chen", "dort", "hoch", "in", "den", "L\u00fcf\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "NN", "ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Schwenkend und ziehend im freudigen Zug?", "tokens": ["Schwen\u00b7kend", "und", "zie\u00b7hend", "im", "freu\u00b7di\u00b7gen", "Zug", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Nein, aber aufw\u00e4rtsgeschwungen aus Gr\u00fcften,", "tokens": ["Nein", ",", "a\u00b7ber", "auf\u00b7w\u00e4rts\u00b7ge\u00b7schwun\u00b7gen", "aus", "Gr\u00fcf\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sonnt sich ein kreischender Rabenflug!", "tokens": ["Sonnt", "sich", "ein", "krei\u00b7schen\u00b7der", "Ra\u00b7ben\u00b7flug", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Springt nicht ein Fischlein aus silberner Welle,", "tokens": ["Springt", "nicht", "ein", "Fisc\u00b7hlein", "aus", "sil\u00b7ber\u00b7ner", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das sich am lieblichen Lichte erfreut?", "tokens": ["Das", "sich", "am", "lieb\u00b7li\u00b7chen", "Lich\u00b7te", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ja, 's ist ein Hecht, der mit t\u00fcckischer Schnelle", "tokens": ["Ja", ",", "'s", "ist", "ein", "Hecht", ",", "der", "mit", "t\u00fc\u00b7cki\u00b7scher", "Schnel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Seinen t\u00e4glichen Raub nur erneut!", "tokens": ["Sei\u00b7nen", "t\u00e4g\u00b7li\u00b7chen", "Raub", "nur", "er\u00b7neut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Fahre hin\u00fcber auf klingenden Speichen,", "tokens": ["Fah\u00b7re", "hin\u00b7\u00fc\u00b7ber", "auf", "klin\u00b7gen\u00b7den", "Spei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Gl\u00e4nzender Morgen! noch ist es nicht Zeit;", "tokens": ["Gl\u00e4n\u00b7zen\u00b7der", "Mor\u00b7gen", "!", "noch", "ist", "es", "nicht", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Rosige Wimpel, und ihr m\u00f6gt erbleichen!", "tokens": ["Ro\u00b7si\u00b7ge", "Wim\u00b7pel", ",", "und", "ihr", "m\u00f6gt", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Weh mir! schon weht ihr so bla\u00df und so weit!", "tokens": ["Weh", "mir", "!", "schon", "weht", "ihr", "so", "bla\u00df", "und", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.7": {"line.1": {"text": "Fahre! \u2013 es tr\u00e4umet ein Riese auf Erden,", "tokens": ["Fah\u00b7re", "!", "\u2013", "es", "tr\u00e4u\u00b7met", "ein", "Rie\u00b7se", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Dem es gar ahnend im Ohre erklingt.", "tokens": ["Dem", "es", "gar", "ah\u00b7nend", "im", "Oh\u00b7re", "er\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, 