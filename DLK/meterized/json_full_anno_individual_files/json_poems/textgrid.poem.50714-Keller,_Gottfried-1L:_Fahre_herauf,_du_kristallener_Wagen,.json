er": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "Springt nicht ein Fischlein aus silberner Welle,", "tokens": ["Springt", "nicht", "ein", "Fisc\u00b7hlein", "aus", "sil\u00b7ber\u00b7ner", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das sich am lieblichen Lichte erfreut?", "tokens": ["Das", "sich", "am", "lieb\u00b7li\u00b7chen", "Lich\u00b7te", "er\u00b7freut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ja, 's ist ein Hecht, der mit t\u00fcckischer Schnelle", "tokens": ["Ja", ",", "'s", "ist", "ein", "Hecht", ",", "der", "mit", "t\u00fc\u00b7cki\u00b7scher", "Schnel\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "APPR", "ADJA", "NN"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Seinen t\u00e4glichen Raub nur erneut!", "tokens": ["Sei\u00b7nen", "t\u00e4g\u00b7li\u00b7chen", "Raub", "nur", "er\u00b7neut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}}, "stanza.14": {"line.1": {"text": "Fahre hin\u00fcber auf klingenden Speichen,", "tokens": ["Fah\u00b7re", "hin\u00b7\u00fc\u00b7ber", "auf", "klin\u00b7gen\u00b7den", "Spei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Gl\u00e4nzender Morgen! noch ist es nicht Zeit;", "tokens": ["Gl\u00e4n\u00b7zen\u00b7der", "Mor\u00b7gen", "!", "noch", "ist", "es", "nicht", "Zeit", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VAFIN", "PPER", "PTKNEG", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Rosige Wimpel, und ihr m\u00f6gt erbleichen!", "tokens": ["Ro\u00b7si\u00b7ge", "Wim\u00b7pel", ",", "und", "ihr", "m\u00f6gt", "er\u00b7blei\u00b7chen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "PPER", "VMFIN", "VVINF", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Weh mir! schon weht ihr so bla\u00df und so weit!", "tokens": ["Weh", "mir", "!", "schon", "weht", "ihr", "so", "bla\u00df", "und", "so", "weit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$.", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.15": {"line.1": {"text": "Fahre! \u2013 es tr\u00e4umet ein Riese auf Erden,", "tokens": ["Fah\u00b7re", "!", "\u2013", "es", "tr\u00e4u\u00b7met", "ein", "Rie\u00b7se", "auf", "Er\u00b7den", ","], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.2": {"text": "Dem es gar ahnend im Ohre erklingt.", "tokens": ["Dem", "es", "gar", "ah\u00b7nend", "im", "Oh\u00b7re", "er\u00b7klingt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Auf 