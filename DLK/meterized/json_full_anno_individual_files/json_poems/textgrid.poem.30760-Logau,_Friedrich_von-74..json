{"textgrid.poem.30760": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "74.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jungfern, solln sie Tichter nemen, wollen sie versichert seyn,", "tokens": ["Jung\u00b7fern", ",", "solln", "sie", "Tich\u00b7ter", "ne\u00b7men", ",", "wol\u00b7len", "sie", "ver\u00b7si\u00b7chert", "seyn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "NN", "VVINF", "$,", "VMFIN", "PPER", "VVPP", "VAINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df sie auch, wie in dem Deutschen, so sind fertig im Latein.", "tokens": ["Da\u00df", "sie", "auch", ",", "wie", "in", "dem", "Deut\u00b7schen", ",", "so", "sind", "fer\u00b7tig", "im", "La\u00b7tein."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "abbreviation"], "pos": ["KOUS", "PPER", "ADV", "$,", "PWAV", "APPR", "ART", "NN", "$,", "ADV", "VAFIN", "ADJD", "APPRART", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.3": {"text": "Sollen binden, sollen schrencken auff gut deutsch; doch gleichwol wissen", "tokens": ["Sol\u00b7len", "bin\u00b7den", ",", "sol\u00b7len", "schren\u00b7cken", "auff", "gut", "deutsch", ";", "doch", "gleich\u00b7wol", "wis\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$,", "VMFIN", "VVINF", "APPR", "ADJD", "ADJD", "$.", "ADV", "ADV", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Auff lateinisch, was geh\u00f6re, da\u00df die Verse gehn auff F\u00fcssen.", "tokens": ["Auff", "la\u00b7tei\u0