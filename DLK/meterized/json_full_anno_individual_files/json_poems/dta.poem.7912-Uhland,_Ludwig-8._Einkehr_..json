{"dta.poem.7912": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "8.  Einkehr .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Bei einem Wirthe, wundermild,", "tokens": ["Bei", "ei\u00b7nem", "Wirt\u00b7he", ",", "wun\u00b7der\u00b7mild", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da war ich j\u00fcngst zu Gaste;", "tokens": ["Da", "war", "ich", "j\u00fcngst", "zu", "Gas\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein goldner Apfel war sein Schild", "tokens": ["Ein", "gold\u00b7ner", "Ap\u00b7fel", "war", "sein", "Schild"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "An einem langen Aste.", "tokens": ["An", "ei\u00b7nem", "lan\u00b7gen", "As\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es war der gute Apfelbaum,", "tokens": ["Es", "war", "der", "gu\u00b7te", "Ap\u00b7fel\u00b7baum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei dem ich eingekehret;", "tokens": ["Bei", "dem", "ich", "ein\u00b7ge\u00b7keh\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit s\u00fc\u00dfer Kost und frischem Schaum", "tokens": ["Mit", "s\u00fc\u00b7\u00dfer", "Kost", "und", "fri\u00b7schem", "Schaum"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hat er mich wohl gen\u00e4hret.", "tokens": ["Hat", "er", "mich", "wohl", "ge\u00b7n\u00e4h\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es kamen in sein gr\u00fcnes Haus", "tokens": ["Es", "ka\u00b7men", "in", "sein", "gr\u00fc\u00b7nes", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Viel leichtbeschwingte G\u00e4ste;", "tokens": ["Viel", "leicht\u00b7be\u00b7schwing\u00b7te", "G\u00e4s\u00b7te", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie sprangen frei und hielten Schmaus", "tokens": ["Sie", "spran\u00b7gen", "frei", "und", "hiel\u00b7ten", "Schmaus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und sangen auf das Beste.", "tokens": ["Und", "san\u00b7gen", "auf", "das", "Bes\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ich fand ein Bett zu s\u00fc\u00dfer Ruh", "tokens": ["Ich", "fand", "ein", "Bett", "zu", "s\u00fc\u00b7\u00dfer", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auf weichen, gr\u00fcnen Matten;", "tokens": ["Auf", "wei\u00b7chen", ",", "gr\u00fc\u00b7nen", "Mat\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der Wirth, er deckte selbst mich zu", "tokens": ["Der", "Wirth", ",", "er", "deck\u00b7te", "selbst", "mich", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "ADV", "PPER", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit seinem k\u00fchlen Schatten.", "tokens": ["Mit", "sei\u00b7nem", "k\u00fch\u00b7len", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Nun fragt\u2019 ich nach der Schuldigkeit;", "tokens": ["Nun", "fragt'", "ich", "nach", "der", "Schul\u00b7dig\u00b7keit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sch\u00fcttelt\u2019 er den Wipfel.", "tokens": ["Da", "sch\u00fct\u00b7telt'", "er", "den", "Wip\u00b7fel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gesegnet sey er allezeit,", "tokens": ["Ge\u00b7seg\u00b7net", "sey", "er", "al\u00b7le\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Von der Wurzel bis zum Gipfel!", "tokens": ["Von", "der", "Wur\u00b7zel", "bis", "zum", "Gip\u00b7fel", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}