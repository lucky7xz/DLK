{"dta.poem.10420": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "XlIV.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Aus alten M\u00e4hrchen winkt es", "tokens": ["Aus", "al\u00b7ten", "M\u00e4hr\u00b7chen", "winkt", "es"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hervor mit wei\u00dfer Hand,", "tokens": ["Her\u00b7vor", "mit", "wei\u00b7\u00dfer", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da singt es und da klingt es", "tokens": ["Da", "singt", "es", "und", "da", "klingt", "es"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "KON", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von einem Zauberland';", "tokens": ["Von", "ei\u00b7nem", "Zau\u00b7ber\u00b7land'", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wo bunte Blumen bl\u00fchen", "tokens": ["Wo", "bun\u00b7te", "Blu\u00b7men", "bl\u00fc\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im goldnen Abendlicht',", "tokens": ["Im", "gold\u00b7nen", "A\u00b7bend\u00b7licht'", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lieblich duftend gl\u00fchen,", "tokens": ["Und", "lieb\u00b7lich", "duf\u00b7tend", "gl\u00fc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit br\u00e4utlichem Gesicht;", "tokens": ["Mit", "br\u00e4ut\u00b7li\u00b7chem", "Ge\u00b7sicht", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und gr\u00fcne B\u00e4ume singen", "tokens": ["Und", "gr\u00fc\u00b7ne", "B\u00e4u\u00b7me", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Uralte Melodein,", "tokens": ["Ur\u00b7al\u00b7te", "Me\u00b7lo\u00b7dein", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die L\u00fcfte heimlich klingen,", "tokens": ["Die", "L\u00fcf\u00b7te", "heim\u00b7lich", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und V\u00f6gel schmettern drein;", "tokens": ["Und", "V\u00f6\u00b7gel", "schmet\u00b7tern", "drein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Nebelbilder steigen", "tokens": ["Und", "Ne\u00b7bel\u00b7bil\u00b7der", "stei\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wohl aus der Erd' hervor,", "tokens": ["Wohl", "aus", "der", "Erd'", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und tanzen luft'gen Reigen,", "tokens": ["Und", "tan\u00b7zen", "luft'\u00b7gen", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im wunderlichen Chor;", "tokens": ["Im", "wun\u00b7der\u00b7li\u00b7chen", "Chor", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Und blaue Funken brennen", "tokens": ["Und", "blau\u00b7e", "Fun\u00b7ken", "bren\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An jedem Blatt und Reis,", "tokens": ["An", "je\u00b7dem", "Blatt", "und", "Reis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und rothe Lichter rennen", "tokens": ["Und", "ro\u00b7the", "Lich\u00b7ter", "ren\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Im irren, wirren Kreis;", "tokens": ["Im", "ir\u00b7ren", ",", "wir\u00b7ren", "Kreis", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "VVINF", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Und laute Quellen brechen", "tokens": ["Und", "lau\u00b7te", "Quel\u00b7len", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aus wildem Marmorstein,", "tokens": ["Aus", "wil\u00b7dem", "Mar\u00b7mor\u00b7stein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und seltsam in den B\u00e4chen", "tokens": ["Und", "selt\u00b7sam", "in", "den", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Strahlt fort der Wiederschein.", "tokens": ["Strahlt", "fort", "der", "Wie\u00b7der\u00b7schein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ach! k\u00f6nnt' ich dorthin kommen,", "tokens": ["Ach", "!", "k\u00f6nnt'", "ich", "dor\u00b7thin", "kom\u00b7men", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und dort mein Herz erfreu'n,", "tokens": ["Und", "dort", "mein", "Herz", "er\u00b7freu'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und aller Qual entnommen,", "tokens": ["Und", "al\u00b7ler", "Qual", "ent\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und frei und selig seyn!", "tokens": ["Und", "frei", "und", "se\u00b7lig", "seyn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Ach! jenes Land der Wonne,", "tokens": ["Ach", "!", "je\u00b7nes", "Land", "der", "Won\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PDAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das seh' ich oft im Traum,", "tokens": ["Das", "seh'", "ich", "oft", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Doch kommt die Morgensonne,", "tokens": ["Doch", "kommt", "die", "Mor\u00b7gen\u00b7son\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zerflie\u00dft's wie eitel Schaum.", "tokens": ["Zer\u00b7flie\u00dft's", "wie", "ei\u00b7tel", "Schaum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "KOKOM", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}