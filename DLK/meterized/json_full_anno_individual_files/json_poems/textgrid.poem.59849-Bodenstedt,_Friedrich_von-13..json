{"textgrid.poem.59849": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sollst dich in Andacht beugen", "tokens": ["Sollst", "dich", "in", "An\u00b7dacht", "beu\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vor jenem hohen Geist,", "tokens": ["Vor", "je\u00b7nem", "ho\u00b7hen", "Geist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von dem die Werke zeugen,", "tokens": ["Von", "dem", "die", "Wer\u00b7ke", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die er dich schaffen hei\u00dft.", "tokens": ["Die", "er", "dich", "schaf\u00b7fen", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PRF", "VVINF", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der, was du je vollbracht", "tokens": ["Der", ",", "was", "du", "je", "voll\u00b7bracht"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ART", "$,", "PWS", "PPER", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und was dir je gelungen,", "tokens": ["Und", "was", "dir", "je", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Urbildlich vorgedacht,", "toke