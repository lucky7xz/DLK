": ["PIAT", "NN", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd keine Plage kan dir zu:", "tokens": ["Vnd", "kei\u00b7ne", "Pla\u00b7ge", "kan", "dir", "zu", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "VMFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dieses arme Leben", "tokens": ["Die\u00b7ses", "ar\u00b7me", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Ist mit Angst vmbgeben,", "tokens": ["Ist", "mit", "Angst", "vmb\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Dort ist Herrlicheit,", "tokens": ["Dort", "ist", "Herr\u00b7li\u00b7cheit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Ist Gewinn ohn Schaden.", "tokens": ["Ist", "Ge\u00b7winn", "ohn", "Scha\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "APPR", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Wer ist, der in Gnaden", "tokens": ["Wer", "ist", ",", "der", "in", "Gna\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VAFIN", "$,", "PRELS", "APPR", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Vns auch bald befreyt?", "tokens": ["Vns", "auch", "bald", "be\u00b7freyt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Gott, die\u00df hast du in deinen H\u00e4nden,", "tokens": ["Gott", ",", "die\u00df", "hast", "du", "in", "dei\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PDS", "VAFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Du hast den Geist vns zugewandt,", "tokens": ["Du", "hast", "den", "Geist", "vns", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du hilffst jhm auch die\u00df Leben enden,", "tokens": ["Du", "hilffst", "jhm", "auch", "die\u00df", "Le\u00b7ben", "en\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PDS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vnd nimmst jhn in sein Vaterlandt,", "tokens": ["Vnd", "nimmst", "jhn", "in", "sein", "Va\u00b7ter\u00b7landt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach la\u00df vns von S\u00fcnden,", "tokens": ["Ach", "la\u00df", "vns", "von", "S\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVIMP", "PPER", "APPR", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die wir an vns finden,", "tokens": ["Die", "wir", "an", "vns", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPER", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Zeitig abgethan", "tokens": ["Zei\u00b7tig", "ab\u00b7ge\u00b7than"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.8": {"text": "Hier au\u00df diesen Thr\u00e4nen,", "tokens": ["Hier", "au\u00df", "die\u00b7sen", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Au\u00df Aegypten sehnen", "tokens": ["Au\u00df", "A\u00b7e\u00b7gyp\u00b7ten", "seh\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NE", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "In dein Canaan.", "tokens": ["In", "dein", "Ca\u00b7naan", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}