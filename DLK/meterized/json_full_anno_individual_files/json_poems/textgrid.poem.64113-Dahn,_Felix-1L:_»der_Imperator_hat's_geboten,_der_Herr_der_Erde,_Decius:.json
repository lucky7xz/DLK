{"textgrid.poem.64113": {"metadata": {"author": {"name": "Dahn, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: \u00bbder Imperator hat's geboten, der Herr der Erde, Decius:", "genre": "verse", "period": "N.A.", "pub_year": 1873, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbder Imperator hat's geboten, der Herr der Erde, Decius:", "tokens": ["\u00bb", "der", "Im\u00b7pe\u00b7ra\u00b7tor", "hat's", "ge\u00b7bo\u00b7ten", ",", "der", "Herr", "der", "Er\u00b7de", ",", "De\u00b7ci\u00b7us", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,", "FM.la", "$."], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr sollt zur\u00fcck, ihr kecken Goten, vom Ufer des Danubius.", "tokens": ["Ihr", "sollt", "zu\u00b7r\u00fcck", ",", "ihr", "ke\u00b7cken", "Go\u00b7ten", ",", "vom", "U\u00b7fer", "des", "Da\u00b7nu\u00b7bi\u00b7us", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,", "APPRART", "NN", "ART", "NE", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Am Purpur Romas, ihr Barbaren, habt ihr gezerrt zu lange schon,", "tokens": ["Am", "Pur\u00b7pur", "Ro\u00b7mas", ",", "ihr", "Bar\u00b7ba\u00b7ren", ",", "habt", "ihr", "ge\u00b7zerrt", "zu", "lan\u00b7ge", "schon", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PPER", "VVPP", "APPR", "ADV", "ADV", "$,"], "meter": "-+-+--+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Es k\u00f6mmt der Erbe der C\u00e4saren, es k\u00f6mmt der Decier gro\u00dfer Sohn.\u00ab", "tokens": ["Es", "k\u00f6mmt", "der", "Er\u00b7be", "der", "C\u00e4\u00b7sa\u00b7ren", ",", "es", "k\u00f6mmt", "der", "De\u00b7cier", "gro\u00b7\u00dfer", "Sohn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "\u2013 \u00bbEr komme nur, der Herr der Erde, wir harren sein an diesem Flu\u00df!\u00ab", "tokens": ["\u2013", "\u00bb", "Er", "kom\u00b7me", "nur", ",", "der", "Herr", "der", "Er\u00b7de", ",", "wir", "har\u00b7ren", "sein", "an", "die\u00b7sem", "Flu\u00df", "!", "\u00ab"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "$(", "PPER", "VVFIN", "ADV", "$,", "ART", "NN", "ART", "NN", "$,", "PPER", "VVFIN", "PPOSAT", "APPR", "PDAT", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Und siebzigtausend Gotenpferde durchschwammen den Danubius.", "tokens": ["Und", "sieb\u00b7zig\u00b7tau\u00b7send", "Go\u00b7ten\u00b7pfer\u00b7de", "durch\u00b7schwam\u00b7men", "den", "Da\u00b7nu\u00b7bi\u00b7us", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "VVFIN", "ART", "NE", "$."], "meter": "-+-+-+-+--+---+-+", "measure": "iambic.septa.relaxed"}}, "stanza.2": {"line.1": {"text": "Und als der Kaiser k\u00f6mmt gezogen, fr\u00e4gt er der Opferzeichen Spur:", "tokens": ["Und", "als", "der", "Kai\u00b7ser", "k\u00f6mmt", "ge\u00b7zo\u00b7gen", ",", "fr\u00e4gt", "er", "der", "Op\u00b7fer\u00b7zei\u00b7chen", "Spur", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "\u00bbwirfst du in dieses Flusses Wogen das Beste nicht\u00ab \u2013 spricht der Augur \u2013", "tokens": ["\u00bb", "wirfst", "du", "in", "die\u00b7ses", "Flus\u00b7ses", "Wo\u00b7gen", "das", "Bes\u00b7te", "nicht", "\u00ab", "\u2013", "spricht", "der", "Au\u00b7gur", "\u2013"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "PPER", "APPR", "PDAT", "NN", "NN", "ART", "NN", "PTKNEG", "$(", "$(", "VVFIN", "ART", "NN", "$("], "meter": "+--+-+-+--+--+---", "measure": "iambic.hexa.invert"}, "line.3": {"text": "\u00bbdas K\u00f6stlichste, was Rom zu eigen, so ist verloren Sieg und Gl\u00fcck.\u00ab", "tokens": ["\u00bb", "das", "K\u00f6st\u00b7lichs\u00b7te", ",", "was", "Rom", "zu", "ei\u00b7gen", ",", "so", "ist", "ver\u00b7lo\u00b7ren", "Sieg", "und", "Gl\u00fcck", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$,", "PRELS", "NE", "PTKA", "ADJD", "$,", "ADV", "VAFIN", "VVPP", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Der Kaiser h\u00f6rt ihn an mit Schweigen, er denkt an seinen Ahn zur\u00fcck;", "tokens": ["Der", "Kai\u00b7ser", "h\u00f6rt", "ihn", "an", "mit", "Schwei\u00b7gen", ",", "er", "denkt", "an", "sei\u00b7nen", "Ahn", "zu\u00b7r\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "APPR", "NN", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Und durch das Lager geht ein Ahnen: \u00bbDer Kaiser weihet sich dem Strom", "tokens": ["Und", "durch", "das", "La\u00b7ger", "geht", "ein", "Ah\u00b7nen", ":", "\u00bb", "Der", "Kai\u00b7ser", "wei\u00b7het", "sich", "dem", "Strom"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "$.", "$(", "ART", "NN", "VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Und von dem Abgrund der Germanen befreit er durch sein Opfer Rom!\u00ab", "tokens": ["Und", "von", "dem", "Ab\u00b7grund", "der", "Ger\u00b7ma\u00b7nen", "be\u00b7freit", "er", "durch", "sein", "Op\u00b7fer", "Rom", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "NE", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.3": {"line.1": {"text": "Und aus des R\u00f6merlagers Pforten, als nun der blut'ge Tag begann,", "tokens": ["Und", "aus", "des", "R\u00f6\u00b7mer\u00b7la\u00b7gers", "Pfor\u00b7ten", ",", "als", "nun", "der", "blut'\u00b7ge", "Tag", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "KOUS", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Schritt Decius den Schlachtkohorten im Kaiserschmuck zum Flu\u00df voran.", "tokens": ["Schritt", "De\u00b7ci\u00b7us", "den", "Schlacht\u00b7ko\u00b7hor\u00b7ten", "im", "Kai\u00b7ser\u00b7schmuck", "zum", "Flu\u00df", "vo\u00b7ran", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+--+-+-+-+", "measure": "iambic.octa.plus.invert"}}, "stanza.4": {"line.1": {"text": "Er ging mit langsam ernstem Schritte: wie eines Priesters war sein Gang", "tokens": ["Er", "ging", "mit", "lang\u00b7sam", "erns\u00b7tem", "Schrit\u00b7te", ":", "wie", "ei\u00b7nes", "Pries\u00b7ters", "war", "sein", "Gang"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN", "$.", "PWAV", "ART", "NN", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und also, in der Heere Mitte, sprach er vom steilen Uferhang:", "tokens": ["Und", "al\u00b7so", ",", "in", "der", "Hee\u00b7re", "Mit\u00b7te", ",", "sprach", "er", "vom", "stei\u00b7len", "U\u00b7fer\u00b7hang", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "APPR", "ART", "NN", "NN", "$,", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "\u00bbsein h\u00f6chstes Gut soll Rom versenken, geopfert, in den Donauflu\u00df,", "tokens": ["\u00bb", "sein", "h\u00f6chs\u00b7tes", "Gut", "soll", "Rom", "ver\u00b7sen\u00b7ken", ",", "ge\u00b7o\u00b7pfert", ",", "in", "den", "Don\u00b7auf\u00b7lu\u00df", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPOSAT", "ADJA", "NN", "VMFIN", "NE", "VVINF", "$,", "VVPP", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Damit uns Sieg die G\u00f6tter schenken: \u2013 wohlan, ich bin ein Decius!\u00ab", "tokens": ["Da\u00b7mit", "uns", "Sieg", "die", "G\u00f6t\u00b7ter", "schen\u00b7ken", ":", "\u2013", "wo\u00b7hlan", ",", "ich", "bin", "ein", "De\u00b7ci\u00b7us", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "NN", "ART", "NN", "VVINF", "$.", "$(", "ADV", "$,", "PPER", "VAFIN", "ART", "NE", "$.", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.5": {"line.1": {"text": "Und schon das Haupt geneigt zum Springen, schaut er noch einmal in die Flut. \u2013", "tokens": ["Und", "schon", "das", "Haupt", "ge\u00b7neigt", "zum", "Sprin\u00b7gen", ",", "schaut", "er", "noch", "ein\u00b7mal", "in", "die", "Flut", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "ART", "NN", "VVPP", "APPRART", "NN", "$,", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+--+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Da sieht er schwarz der Wellen Schlingen und sieht der Str\u00f6mung grimme Wut,", "tokens": ["Da", "sieht", "er", "schwarz", "der", "Wel\u00b7len", "Schlin\u00b7gen", "und", "sieht", "der", "Str\u00f6\u00b7mung", "grim\u00b7me", "Wut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NN", "NN", "KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Er f\u00fchlt sein Herz im Krampf ersticken, im Ohre rauscht's ihm grausenhaft:", "tokens": ["Er", "f\u00fchlt", "sein", "Herz", "im", "Krampf", "er\u00b7sti\u00b7cken", ",", "im", "Oh\u00b7re", "rauscht's", "ihm", "grau\u00b7sen\u00b7haft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "VVINF", "$,", "APPRART", "NN", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.4": {"text": "Da wird es Nacht vor seinen Blicken: \u2013 er wankt: \u2013 es sinkt ihm Mut und Kraft \u2013", "tokens": ["Da", "wird", "es", "Nacht", "vor", "sei\u00b7nen", "Bli\u00b7cken", ":", "\u2013", "er", "wankt", ":", "\u2013", "es", "sinkt", "ihm", "Mut", "und", "Kraft", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$.", "$(", "PPER", "VVFIN", "$.", "$(", "PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.5": {"text": "Er, der in zwanzig Perserschlachten dem Tod getrotzt hat k\u00fchn und stark,", "tokens": ["Er", ",", "der", "in", "zwan\u00b7zig", "Per\u00b7ser\u00b7schlach\u00b7ten", "dem", "Tod", "ge\u00b7trotzt", "hat", "k\u00fchn", "und", "stark", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPR", "CARD", "NN", "ART", "NN", "VVPP", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.6": {"text": "Der mit des Herzens edelm Trachten verj\u00fcngen wollte Romas Mark, \u2013", "tokens": ["Der", "mit", "des", "Her\u00b7zens", "e\u00b7delm", "Trach\u00b7ten", "ver\u00b7j\u00fcn\u00b7gen", "woll\u00b7te", "Ro\u00b7mas", "Mark", ",", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "VVINF", "VMFIN", "NE", "NE", "$,", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Er ", "tokens": ["Er"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.8": {"text": "Die G\u00f6tter sind mit den Germanen, das Schicksal will sein Opfer nicht!", "tokens": ["Die", "G\u00f6t\u00b7ter", "sind", "mit", "den", "Ger\u00b7ma\u00b7nen", ",", "das", "Schick\u00b7sal", "will", "sein", "Op\u00b7fer", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "ART", "NN", "$,", "ART", "NN", "VMFIN", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+--+--+--+-+-+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.6": {"line.1": {"text": "Er wendet sich, er flieht mit Grausen, fein Haupt verh\u00fcllt im Purpurkleid", "tokens": ["Er", "wen\u00b7det", "sich", ",", "er", "flieht", "mit", "Grau\u00b7sen", ",", "fein", "Haupt", "ver\u00b7h\u00fcllt", "im", "Pur\u00b7pur\u00b7kleid"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "$,", "PPER", "VVFIN", "APPR", "NN", "$,", "ADJD", "NN", "VVFIN", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "Und hinter ihm die Goten brausen mit Siegesjubel in den Streit.", "tokens": ["Und", "hin\u00b7ter", "ihm", "die", "Go\u00b7ten", "brau\u00b7sen", "mit", "Sie\u00b7ges\u00b7ju\u00b7bel", "in", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "VVFIN", "APPR", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.3": {"text": "Sie fielen all', die R\u00f6merscharen, auch Decius fiel an diesem Tag:", "tokens": ["Sie", "fie\u00b7len", "all'", ",", "die", "R\u00f6\u00b7mer\u00b7scha\u00b7ren", ",", "auch", "De\u00b7ci\u00b7us", "fiel", "an", "die\u00b7sem", "Tag", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "ART", "NN", "$,", "ADV", "NE", "VVFIN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.4": {"text": "Er war der erste der C\u00e4saren, der st\u00fcrzte von Germanenschlag.", "tokens": ["Er", "war", "der", "ers\u00b7te", "der", "C\u00e4\u00b7sa\u00b7ren", ",", "der", "st\u00fcrz\u00b7te", "von", "Ger\u00b7ma\u00b7nen\u00b7schlag", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ART", "NN", "$,", "PRELS", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.7": {"line.1": {"text": "\u00bbder Imperator hat's geboten, der Herr der Erde, Decius:", "tokens": ["\u00bb", "der", "Im\u00b7pe\u00b7ra\u00b7tor", "hat's", "ge\u00b7bo\u00b7ten", ",", "der", "Herr", "der", "Er\u00b7de", ",", "De\u00b7ci\u00b7us", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["$(", "ART", "NN", "VAFIN", "VVPP", "$,", "ART", "NN", "ART", "NN", "$,", "FM.la", "$."], "meter": "--+--+-+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ihr sollt zur\u00fcck, ihr kecken Goten, vom Ufer des Danubius.", "tokens": ["Ihr", "sollt", "zu\u00b7r\u00fcck", ",", "ihr", "ke\u00b7cken", "Go\u00b7ten", ",", "vom", "U\u00b7fer", "des", "D