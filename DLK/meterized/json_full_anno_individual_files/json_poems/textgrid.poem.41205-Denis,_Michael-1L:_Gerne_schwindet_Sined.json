{"textgrid.poem.41205": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gerne schwindet Sined", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gerne schwindet Sined", "tokens": ["Ger\u00b7ne", "schwin\u00b7det", "Si\u00b7ned"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VVFIN", "NE"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "In des Mondes k\u00fchlen Wandelstunden", "tokens": ["In", "des", "Mon\u00b7des", "k\u00fch\u00b7len", "Wan\u00b7del\u00b7stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Aus dem lauten Kreise seiner Freunde weg;", "tokens": ["Aus", "dem", "lau\u00b7ten", "Krei\u00b7se", "sei\u00b7ner", "Freun\u00b7de", "weg", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Nur gefolgt von seinen Liedern", "tokens": ["Nur", "ge\u00b7folgt", "von", "sei\u00b7nen", "Lie\u00b7dern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVPP", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sucht er stille Lindenwipfel", "tokens": ["Sucht", "er", "stil\u00b7le", "Lin\u00b7den\u00b7wip\u00b7fel"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Unter welchen seine Seele", "tokens": ["Un\u00b7ter", "wel\u00b7chen", "sei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PWAT", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit entferneten Geliebten", "tokens": ["Mit", "ent\u00b7fer\u00b7ne\u00b7ten", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich bis hin zur Mitternacht ergeht.", "tokens": ["Sich", "bis", "hin", "zur", "Mit\u00b7ter\u00b7nacht", "er\u00b7geht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.9": {"text": "Da l\u00e4chelt oft vom Ufer, wo die Rur", "tokens": ["Da", "l\u00e4\u00b7chelt", "oft", "vom", "U\u00b7fer", ",", "wo", "die", "Rur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Sich in die Maas verstr\u00f6mt, ein hohes Bild", "tokens": ["Sich", "in", "die", "Maas", "ver\u00b7str\u00f6mt", ",", "ein", "ho\u00b7hes", "Bild"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "NN", "VVPP", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Im Oberpriesterschmucke sanft auf ihn;", "tokens": ["Im", "O\u00b7ber\u00b7pries\u00b7ter\u00b7schmu\u00b7cke", "sanft", "auf", "ihn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Da kehren Tage, die zur Ewigkeit", "tokens": ["Da", "keh\u00b7ren", "Ta\u00b7ge", ",", "die", "zur", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "Hin\u00fcber sind, vor seinen Geist zur\u00fcck.", "tokens": ["Hin\u00b7\u00fc\u00b7ber", "sind", ",", "vor", "sei\u00b7nen", "Geist", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "$,", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Sein Busen schwillt empor. Er wendet sich", "tokens": ["Sein", "Bu\u00b7sen", "schwillt", "em\u00b7por", ".", "Er", "wen\u00b7det", "sich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Nach seinen Liedern um, und spricht entz\u00fcckt:", "tokens": ["Nach", "sei\u00b7nen", "Lie\u00b7dern", "um", ",", "und", "spricht", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dort, Kinder meines Herzens! dort seht ihr ihn", "tokens": ["Dort", ",", "Kin\u00b7der", "mei\u00b7nes", "Her\u00b7zens", "!", "dort", "seht", "ihr", "ihn"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "PPOSAT", "NN", "$.", "ADV", "VVFIN", "PPER", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den ehrenvollen Bardenbeg\u00fcnstiger,", "tokens": ["Den", "eh\u00b7ren\u00b7vol\u00b7len", "Bar\u00b7den\u00b7be\u00b7g\u00fcns\u00b7ti\u00b7ger", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Durch den ihr seyd, auf dessen Zuspruch", "tokens": ["Durch", "den", "ihr", "seyd", ",", "auf", "des\u00b7sen", "Zu\u00b7spruch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VAFIN", "$,", "APPR", "PRELAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Z\u00f6gernden Lippen ihr euch entrisset,", "tokens": ["Z\u00f6\u00b7gern\u00b7den", "Lip\u00b7pen", "ihr", "euch", "ent\u00b7ris\u00b7set", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Dem Veilchen \u00e4hnlich, welches in Erde schlief,", "tokens": ["Dem", "Veil\u00b7chen", "\u00e4hn\u00b7lich", ",", "wel\u00b7ches", "in", "Er\u00b7de", "schlief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und jetzt den Ruf des Flurenermunterers", "tokens": ["Und", "jetzt", "den", "Ruf", "des", "Flu\u00b7re\u00b7ner\u00b7mun\u00b7te\u00b7rers"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Lenzes h\u00f6ret, und den blauen", "tokens": ["Des", "Len\u00b7zes", "h\u00f6\u00b7ret", ",", "und", "den", "blau\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Busen der suchenden Jugend aufthut.", "tokens": ["Bu\u00b7sen", "der", "su\u00b7chen\u00b7den", "Ju\u00b7gend", "auf\u00b7thut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Hier unter diesen Linden erging er sich", "tokens": ["Hier", "un\u00b7ter", "die\u00b7sen", "Lin\u00b7den", "er\u00b7ging", "er", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NE", "VVFIN", "PPER", "PRF"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Mitte seiner Sorgen. Ein wichtiges", "tokens": ["In", "Mit\u00b7te", "sei\u00b7ner", "Sor\u00b7gen", ".", "Ein", "wich\u00b7ti\u00b7ges"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "PPOSAT", "NN", "$.", "ART", "ADJA"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Gesch\u00e4ft zum Wohl der Folgezeiten", "tokens": ["Ge\u00b7sch\u00e4ft", "zum", "Wohl", "der", "Fol\u00b7ge\u00b7zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hatte die F\u00fcrstin ihm aufgeladen,", "tokens": ["Hat\u00b7te", "die", "F\u00fcrs\u00b7tin", "ihm", "auf\u00b7ge\u00b7la\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.5": {"line.1": {"text": "Die Zucht der Heldenkinder. Noch war't ihr nicht,", "tokens": ["Die", "Zucht", "der", "Hel\u00b7den\u00b7kin\u00b7der", ".", "Noch", "wa\u00b7r't", "ihr", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$.", "ADV", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Noch scholl im Opferkreise der feiernden", "tokens": ["Noch", "scholl", "im", "Op\u00b7fer\u00b7krei\u00b7se", "der", "fei\u00b7ern\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "APPRART", "NN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Druiden nicht, und nicht im Mahle", "tokens": ["Dru\u00b7i\u00b7den", "nicht", ",", "und", "nicht", "im", "Mah\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PTKNEG", "$,", "KON", "PTKNEG", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Spiegelnder Helme der Name Sined.", "tokens": ["Spie\u00b7geln\u00b7der", "Hel\u00b7me", "der", "Na\u00b7me", "Si\u00b7ned", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ART", "NN", "NE", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Da sah er mir ins Auge. Dein Blick verr\u00e4th", "tokens": ["Da", "sah", "er", "mir", "ins", "Au\u00b7ge", ".", "Dein", "Blick", "ver\u00b7r\u00e4\u00b7th"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "NN", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In deinem Busen lodernde Bardenglut.", "tokens": ["In", "dei\u00b7nem", "Bu\u00b7sen", "lo\u00b7dern\u00b7de", "Bar\u00b7deng\u00b7lut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "So sprach er: Weile nicht in helle", "tokens": ["So", "sprach", "er", ":", "Wei\u00b7le", "nicht", "in", "hel\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "PTKNEG", "APPR", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Flammen die lodernde Glut zu fachen.", "tokens": ["Flam\u00b7men", "die", "lo\u00b7dern\u00b7de", "Glut", "zu", "fa\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Ein Loos von oben war mir des Weisen Rath.", "tokens": ["Ein", "Loos", "von", "o\u00b7ben", "war", "mir", "des", "Wei\u00b7sen", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich thats. Ihr wurdet, Lieder! da gaben mir", "tokens": ["Ich", "thats", ".", "Ihr", "wur\u00b7det", ",", "Lie\u00b7der", "!", "da", "ga\u00b7ben", "mir"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "$,", "NN", "$.", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die Barden Eichenkranz, und Rhingulph", "tokens": ["Die", "Bar\u00b7den", "Ei\u00b7chen\u00b7kranz", ",", "und", "Rhin\u00b7gulph"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Namen", "tokens": ["Na\u00b7men"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Und er der Weise freute der Wege sich,", "tokens": ["Und", "er", "der", "Wei\u00b7se", "freu\u00b7te", "der", "We\u00b7ge", "sich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "VVFIN", "ART", "NN", "PRF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die Sined ging zum Ruhme. Durch ihn erschwang", "tokens": ["Die", "Si\u00b7ned", "ging", "zum", "Ruh\u00b7me", ".", "Durch", "ihn", "er\u00b7schwang"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$.", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Sich mein Gesang und meiner Harfe", "tokens": ["Sich", "mein", "Ge\u00b7sang", "und", "mei\u00b7ner", "Har\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PRF", "PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Lispel zu Menschengebieterohren", "tokens": ["Lis\u00b7pel", "zu", "Men\u00b7schen\u00b7ge\u00b7bie\u00b7ter\u00b7oh\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["NE", "APPR", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Und nun geneu\u00dft er seiner Verdienste Lohn", "tokens": ["Und", "nun", "ge\u00b7neu\u00dft", "er", "sei\u00b7ner", "Ver\u00b7diens\u00b7te", "Lohn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In jenen Fernen; aber er freut sich noch", "tokens": ["In", "je\u00b7nen", "Fer\u00b7nen", ";", "a\u00b7ber", "er", "freut", "sich", "noch"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$.", "KON", "PPER", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Wege Sineds, liebt euch, Lieder!", "tokens": ["Der", "We\u00b7ge", "Si\u00b7neds", ",", "liebt", "euch", ",", "Lie\u00b7der", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "VVFIN", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn ihr sein hohes Gew\u00f6lb' besuchet.", "tokens": ["Wenn", "ihr", "sein", "ho\u00b7hes", "Ge\u00b7w\u00f6lb'", "be\u00b7su\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Ha! sind auch deiner Liebe, mein Vaterland!", "tokens": ["Ha", "!", "sind", "auch", "dei\u00b7ner", "Lie\u00b7be", ",", "mein", "Va\u00b7ter\u00b7land", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$.", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Die Lieder w\u00fcrdig, welche dir Sined singt,", "tokens": ["Die", "Lie\u00b7der", "w\u00fcr\u00b7dig", ",", "wel\u00b7che", "dir", "Si\u00b7ned", "singt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$,", "PRELS", "PPER", "NE", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O so vergi\u00df es nie: die Lieder", "tokens": ["O", "so", "ver\u00b7gi\u00df", "es", "nie", ":", "die", "Lie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "ADV", "VVIMP", "PPER", "ADV", "$.", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bist du dem Oberdruiden schuldig.", "tokens": ["Bist", "du", "dem", "O\u00b7ber\u00b7dru\u00b7i\u00b7den", "schul\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.