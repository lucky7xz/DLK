{"dta.poem.23360": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kuhschnappelsche Volksrepr\u00e4sentation.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ei, was soll noch Kunst und Witz?", "tokens": ["Ei", ",", "was", "soll", "noch", "Kunst", "und", "Witz", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VMFIN", "ADV", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Hier gilt nur der Grundbesitz.", "tokens": ["Hier", "gilt", "nur", "der", "Grund\u00b7be\u00b7sitz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "F\u00fcr den Landbau, f\u00fcr's Gewerbe", "tokens": ["F\u00fcr", "den", "Land\u00b7bau", ",", "f\u00fcr's", "Ge\u00b7wer\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schweigt kein Volksrepr\u00e4sentant;", "tokens": ["Schweigt", "kein", "Volks\u00b7re\u00b7pr\u00e4\u00b7sen\u00b7tant", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch des Geistes Gut und Erbe", "tokens": ["Doch", "des", "Geis\u00b7tes", "Gut", "und", "Er\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJD", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Legen sie in Gottes Hand.", "tokens": ["Le\u00b7gen", "sie", "in", "Got\u00b7tes", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie verlassen und verwaist,", "tokens": ["Wie", "ver\u00b7las\u00b7sen", "und", "ver\u00b7waist", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Armer, armer Menschengeist!", "tokens": ["Ar\u00b7mer", ",", "ar\u00b7mer", "Men\u00b7schen\u00b7geist", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wie der Vogel auf dem Dache", "tokens": ["Wie", "der", "Vo\u00b7gel", "auf", "dem", "Da\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hast auch du kein Vaterland,", "tokens": ["Hast", "auch", "du", "kein", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPER", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und der Menschheit heil'ge Sache", "tokens": ["Und", "der", "Menschheit", "heil'\u00b7ge", "Sa\u00b7che"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Gab dir Gott in deine Hand.", "tokens": ["Gab", "dir", "Gott", "in", "dei\u00b7ne", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}