{"textgrid.poem.41390": {"metadata": {"author": {"name": "Hagedorn, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Ja und Nein", "genre": "verse", "period": "N.A.", "pub_year": 1731, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Barde hie\u00df, aus frommer Pflicht,", "tokens": ["Ein", "Bar\u00b7de", "hie\u00df", ",", "aus", "from\u00b7mer", "Pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ganzes Heer von Silben ringen.", "tokens": ["Ein", "gan\u00b7zes", "Heer", "von", "Sil\u00b7ben", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will nur zwo zur Sprache zwingen,", "tokens": ["Ich", "will", "nur", "zwo", "zur", "Spra\u00b7che", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "CARD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil doch in Fabeln alles spricht.", "tokens": ["Weil", "doch", "in", "Fa\u00b7beln", "al\u00b7les", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sind die, so ich reden lasse,", "tokens": ["Es", "sind", "die", ",", "so", "ich", "re\u00b7den", "las\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Machtw\u00f6rter von der ersten Klasse,", "tokens": ["Macht\u00b7w\u00f6r\u00b7ter", "von", "der", "ers\u00b7ten", "Klas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die in der Welt was rechtes schrein,", "tokens": ["Die", "in", "der", "Welt", "was", "rech\u00b7tes", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die alten Feinde: ", "tokens": ["Die", "al\u00b7ten", "Fein\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Es r\u00fcsten beide sich zum Streit.", "tokens": ["Es", "r\u00fcs\u00b7ten", "bei\u00b7de", "sich", "zum", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wollen nun als Helden fechten,", "tokens": ["Sie", "wol\u00b7len", "nun", "als", "Hel\u00b7den", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht, wie kleine Hadrer, rechten.", "tokens": ["Und", "nicht", ",", "wie", "klei\u00b7ne", "Had\u00b7rer", ",", "rech\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kurz: sie bestimmen Ort und Zeit.", "tokens": ["Kurz", ":", "sie", "be\u00b7stim\u00b7men", "Ort", "und", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nein tobt, und treibet jeden Mann,", "tokens": ["Nein", "tobt", ",", "und", "trei\u00b7bet", "je\u00b7den", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stellt sich schnaubend an die Spitze;", "tokens": ["Und", "stellt", "sich", "schnau\u00b7bend", "an", "die", "Spit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Winkt erst dem Feind, und red't ihn an.", "tokens": ["Winkt", "erst", "dem", "Feind", ",", "und", "red't", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Halt! spricht er, ehe wir uns schlagen,", "tokens": ["Halt", "!", "spricht", "er", ",", "e\u00b7he", "wir", "uns", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hab' ich dir noch ein Wort zu sagen:", "tokens": ["Hab'", "ich", "dir", "noch", "ein", "Wort", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df jene Balger etwas ruhn.", "tokens": ["La\u00df", "je\u00b7ne", "Bal\u00b7ger", "et\u00b7was", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir m\u00fcssen selbst das Beste thun.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "selbst", "das", "Bes\u00b7te", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du Waghals, dessen Eigensinn", "tokens": ["Du", "Wag\u00b7hals", ",", "des\u00b7sen", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur selten oder sp\u00e4t zu brechen,", "tokens": ["Nur", "sel\u00b7ten", "o\u00b7der", "sp\u00e4t", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man sagt, dein Eifer l\u00e4\u00dft sich schw\u00e4chen;", "tokens": ["Man", "sagt", ",", "dein", "Ei\u00b7fer", "l\u00e4\u00dft", "sich", "schw\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich r\u00fchret Schmeicheln und Gewinn.", "tokens": ["Dich", "r\u00fch\u00b7ret", "Schmei\u00b7cheln", "und", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich hat die Heimat der Guineen", "tokens": ["Dich", "hat", "die", "Hei\u00b7mat", "der", "Gui\u00b7neen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Oft z\u00e4rtlich und gekirrt gesehen,", "tokens": ["Oft", "z\u00e4rt\u00b7lich", "und", "ge\u00b7kirrt", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo mancher Kitzel in der Hand", "tokens": ["Wo", "man\u00b7cher", "Kit\u00b7zel", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir deine freie Zunge band.", "tokens": ["Dir", "dei\u00b7ne", "frei\u00b7e", "Zun\u00b7ge", "band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zum \u00f6ftern pflegt ein doppelt ", "tokens": ["Zum", "\u00f6f\u00b7tern", "pflegt", "ein", "dop\u00b7pelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "VVFIN", "ART", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie sollten denn um Nebensachen", "tokens": ["Wie", "soll\u00b7ten", "denn", "um", "Ne\u00b7ben\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Blutsverwandten so entzwein!", "tokens": ["Sich", "Bluts\u00b7ver\u00b7wand\u00b7ten", "so", "ent\u00b7zwein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jeder kann das Seine prahlen.", "tokens": ["Ein", "je\u00b7der", "kann", "das", "Sei\u00b7ne", "prah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und Recht und Zeugen widerstehn.", "tokens": ["Und", "Recht", "und", "Zeu\u00b7gen", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Durch Z\u00f6gern seinen Feind erm\u00fcden.", "tokens": ["Durch", "Z\u00f6\u00b7gern", "sei\u00b7nen", "Feind", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der in der Eile siegen mu\u00df.", "tokens": ["Der", "in", "der", "Ei\u00b7le", "sie\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen, in gewissen F\u00e4llen,", "tokens": ["Wir", "wol\u00b7len", ",", "in", "ge\u00b7wis\u00b7sen", "F\u00e4l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns beide meisterlich verstellen.", "tokens": ["Uns", "bei\u00b7de", "meis\u00b7ter\u00b7lich", "ver\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Hofe soll das ", "tokens": ["Am", "Ho\u00b7fe", "soll", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.7": {"line.1": {"text": "Beschlo\u00df, von nun an leeren H\u00e4nden", "tokens": ["Be\u00b7schlo\u00df", ",", "von", "nun", "an", "lee\u00b7ren", "H\u00e4n\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "APPR", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den Beistand nimmer zu verpf\u00e4nden,", "tokens": ["Den", "Bei\u00b7stand", "nim\u00b7mer", "zu", "ver\u00b7pf\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und sprach zum ersten Male: ", "tokens": ["Und", "sprach", "zum", "ers\u00b7ten", "Ma\u00b7le", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die ganze Fehde ward geschlichtet,", "tokens": ["Die", "gan\u00b7ze", "Feh\u00b7de", "ward", "ge\u00b7schlich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Aus Eigennutz ein Bund errichtet,", "tokens": ["Aus", "Ei\u00b7gen\u00b7nutz", "ein", "Bund", "er\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und beide dienen jetzt der Welt,", "tokens": ["Und", "bei\u00b7de", "die\u00b7nen", "jetzt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nach Schweizerart, um baares Geld.", "tokens": ["Nach", "Schwei\u00b7zer\u00b7art", ",", "um", "baa\u00b7res", "Geld", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Ein Barde hie\u00df, aus frommer Pflicht,", "tokens": ["Ein", "Bar\u00b7de", "hie\u00df", ",", "aus", "from\u00b7mer", "Pflicht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein ganzes Heer von Silben ringen.", "tokens": ["Ein", "gan\u00b7zes", "Heer", "von", "Sil\u00b7ben", "rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will nur zwo zur Sprache zwingen,", "tokens": ["Ich", "will", "nur", "zwo", "zur", "Spra\u00b7che", "zwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "CARD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Weil doch in Fabeln alles spricht.", "tokens": ["Weil", "doch", "in", "Fa\u00b7beln", "al\u00b7les", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "APPR", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es sind die, so ich reden lasse,", "tokens": ["Es", "sind", "die", ",", "so", "ich", "re\u00b7den", "las\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "$,", "ADV", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Machtw\u00f6rter von der ersten Klasse,", "tokens": ["Macht\u00b7w\u00f6r\u00b7ter", "von", "der", "ers\u00b7ten", "Klas\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die in der Welt was rechtes schrein,", "tokens": ["Die", "in", "der", "Welt", "was", "rech\u00b7tes", "schrein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PWS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die alten Feinde: ", "tokens": ["Die", "al\u00b7ten", "Fein\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Es r\u00fcsten beide sich zum Streit.", "tokens": ["Es", "r\u00fcs\u00b7ten", "bei\u00b7de", "sich", "zum", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "PRF", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sie wollen nun als Helden fechten,", "tokens": ["Sie", "wol\u00b7len", "nun", "als", "Hel\u00b7den", "fech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KOUS", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und nicht, wie kleine Hadrer, rechten.", "tokens": ["Und", "nicht", ",", "wie", "klei\u00b7ne", "Had\u00b7rer", ",", "rech\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PTKNEG", "$,", "PWAV", "ADJA", "NN", "$,", "ADJA", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kurz: sie bestimmen Ort und Zeit.", "tokens": ["Kurz", ":", "sie", "be\u00b7stim\u00b7men", "Ort", "und", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nein tobt, und treibet jeden Mann,", "tokens": ["Nein", "tobt", ",", "und", "trei\u00b7bet", "je\u00b7den", "Mann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "VVFIN", "$,", "KON", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und stellt sich schnaubend an die Spitze;", "tokens": ["Und", "stellt", "sich", "schnau\u00b7bend", "an", "die", "Spit\u00b7ze", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Doch ", "tokens": ["Doch"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Winkt erst dem Feind, und red't ihn an.", "tokens": ["Winkt", "erst", "dem", "Feind", ",", "und", "red't", "ihn", "an", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Halt! spricht er, ehe wir uns schlagen,", "tokens": ["Halt", "!", "spricht", "er", ",", "e\u00b7he", "wir", "uns", "schla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVFIN", "PPER", "$,", "KOUS", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hab' ich dir noch ein Wort zu sagen:", "tokens": ["Hab'", "ich", "dir", "noch", "ein", "Wort", "zu", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df jene Balger etwas ruhn.", "tokens": ["La\u00df", "je\u00b7ne", "Bal\u00b7ger", "et\u00b7was", "ruhn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wir m\u00fcssen selbst das Beste thun.", "tokens": ["Wir", "m\u00fcs\u00b7sen", "selbst", "das", "Bes\u00b7te", "thun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Du Waghals, dessen Eigensinn", "tokens": ["Du", "Wag\u00b7hals", ",", "des\u00b7sen", "Ei\u00b7gen\u00b7sinn"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPER", "NN", "$,", "PRELAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur selten oder sp\u00e4t zu brechen,", "tokens": ["Nur", "sel\u00b7ten", "o\u00b7der", "sp\u00e4t", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Man sagt, dein Eifer l\u00e4\u00dft sich schw\u00e4chen;", "tokens": ["Man", "sagt", ",", "dein", "Ei\u00b7fer", "l\u00e4\u00dft", "sich", "schw\u00e4\u00b7chen", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PPOSAT", "NN", "VVFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dich r\u00fchret Schmeicheln und Gewinn.", "tokens": ["Dich", "r\u00fch\u00b7ret", "Schmei\u00b7cheln", "und", "Ge\u00b7winn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dich hat die Heimat der Guineen", "tokens": ["Dich", "hat", "die", "Hei\u00b7mat", "der", "Gui\u00b7neen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Oft z\u00e4rtlich und gekirrt gesehen,", "tokens": ["Oft", "z\u00e4rt\u00b7lich", "und", "ge\u00b7kirrt", "ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Wo mancher Kitzel in der Hand", "tokens": ["Wo", "man\u00b7cher", "Kit\u00b7zel", "in", "der", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dir deine freie Zunge band.", "tokens": ["Dir", "dei\u00b7ne", "frei\u00b7e", "Zun\u00b7ge", "band", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Zum \u00f6ftern pflegt ein doppelt ", "tokens": ["Zum", "\u00f6f\u00b7tern", "pflegt", "ein", "dop\u00b7pelt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADV", "VVFIN", "ART", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Wie sollten denn um Nebensachen", "tokens": ["Wie", "soll\u00b7ten", "denn", "um", "Ne\u00b7ben\u00b7sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Blutsverwandten so entzwein!", "tokens": ["Sich", "Bluts\u00b7ver\u00b7wand\u00b7ten", "so", "ent\u00b7zwein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein jeder kann das Seine prahlen.", "tokens": ["Ein", "je\u00b7der", "kann", "das", "Sei\u00b7ne", "prah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "PDS", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Das ", "tokens": ["Das"], "token_info": ["word"], "pos": ["PDS"], "meter": "-", "measure": "single.down"}, "line.7": {"text": "Und Recht und Zeugen widerstehn.", "tokens": ["Und", "Recht", "und", "Zeu\u00b7gen", "wi\u00b7der\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Durch Z\u00f6gern seinen Feind erm\u00fcden.", "tokens": ["Durch", "Z\u00f6\u00b7gern", "sei\u00b7nen", "Feind", "er\u00b7m\u00fc\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der in der Eile siegen mu\u00df.", "tokens": ["Der", "in", "der", "Ei\u00b7le", "sie\u00b7gen", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wir wollen, in gewissen F\u00e4llen,", "tokens": ["Wir", "wol\u00b7len", ",", "in", "ge\u00b7wis\u00b7sen", "F\u00e4l\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Uns beide meisterlich verstellen.", "tokens": ["Uns", "bei\u00b7de", "meis\u00b7ter\u00b7lich", "ver\u00b7stel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Am Hofe soll das ", "tokens": ["Am", "Ho\u00b7fe", "soll", "das"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "VMFIN", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}}, "stanza.14": {"line.1": {"text": "Beschlo\u00df, von nun an leeren H\u00e4nden", "tokens": ["Be\u00b7sch