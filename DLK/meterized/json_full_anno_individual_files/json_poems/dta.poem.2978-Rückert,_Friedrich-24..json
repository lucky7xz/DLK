{"dta.poem.2978": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zwei Kampfparteien stehn im Feld der Gegenwart,", "tokens": ["Zwei", "Kampf\u00b7par\u00b7tei\u00b7en", "stehn", "im", "Feld", "der", "Ge\u00b7gen\u00b7wart", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Gewaffnet jede mit besondrer Waffenart.", "tokens": ["Ge\u00b7waff\u00b7net", "je\u00b7de", "mit", "be\u00b7sond\u00b7rer", "Waf\u00b7fen\u00b7art", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie hei\u00dfen die Partein? und worum ist ihr Streit?", "tokens": ["Wie", "hei\u00b7\u00dfen", "die", "Par\u00b7tein", "?", "und", "wo\u00b7rum", "ist", "ihr", "Streit", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$.", "KON", "PWAV", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+---+-+-+-+", "measure": "dactylic.init"}, "line.2": {"text": "Die Zukunft hei\u00dfen sie und die Vergangenheit.", "tokens": ["Die", "Zu\u00b7kunft", "hei\u00b7\u00dfen", "sie", "und", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die k\u00e4mpfet f\u00fcrs Bestehn, und jene f\u00fcr das Werden;", "tokens": ["Die", "k\u00e4mp\u00b7fet", "f\u00fcrs", "Be\u00b7stehn", ",", "und", "je\u00b7ne", "f\u00fcr", "das", "Wer\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "KON", "PDS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wer profezeit, wie es mit ihnen wird auf Erden?", "tokens": ["Wer", "pro\u00b7fe\u00b7zeit", ",", "wie", "es", "mit", "ih\u00b7nen", "wird", "auf", "Er\u00b7den", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "PWAV", "PPER", "APPR", "PPER", "VAFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "In ihrem Namen ist der Ausgang profezeit:", "tokens": ["In", "ih\u00b7rem", "Na\u00b7men", "ist", "der", "Aus\u00b7gang", "pro\u00b7fe\u00b7zeit", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nie vor der Zukunft h\u00e4lt Stand die Vergangenheit.", "tokens": ["Nie", "vor", "der", "Zu\u00b7kunft", "h\u00e4lt", "Stand", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "NN", "ART", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}}}}