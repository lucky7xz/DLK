{"textgrid.poem.63839": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Wilhelm Hertz \u2020", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dumpf \u00fcber Berg und Tal dringt zu mir her", "tokens": ["Dumpf", "\u00fc\u00b7ber", "Berg", "und", "Tal", "dringt", "zu", "mir", "her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "KON", "NN", "VVFIN", "APPR", "PPER", "APZR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die bitterste der Kunden, tr\u00e4nenschwer:", "tokens": ["Die", "bit\u00b7ters\u00b7te", "der", "Kun\u00b7den", ",", "tr\u00e4\u00b7nen\u00b7schwer", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "ART", "NN", "$,", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Teure, den ich br\u00fcderlich geliebt", "tokens": ["Der", "Teu\u00b7re", ",", "den", "ich", "br\u00fc\u00b7der\u00b7lich", "ge\u00b7liebt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "In jung' und alter Treue, nie getr\u00fcbt,", "tokens": ["In", "jung'", "und", "al\u00b7ter", "Treu\u00b7e", ",", "nie", "ge\u00b7tr\u00fcbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "ADJA", "NN", "$,", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Lang vor der Zeit der hingewelkten Kraft", "tokens": ["Lang", "vor", "der", "Zeit", "der", "hin\u00b7ge\u00b7welk\u00b7ten", "Kraft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So unerbittlich j\u00e4h hinweggerafft,", "tokens": ["So", "un\u00b7er\u00b7bitt\u00b7lich", "j\u00e4h", "hin\u00b7weg\u00b7ge\u00b7rafft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Leib von grimmer Schmerzen Qual verzehrt,", "tokens": ["Der", "Leib", "von", "grim\u00b7mer", "Schmer\u00b7zen", "Qual", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der reiche Geist ins All zur\u00fcckgekehrt!", "tokens": ["Der", "rei\u00b7che", "Geist", "ins", "All", "zu\u00b7r\u00fcck\u00b7ge\u00b7kehrt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Und wir, da\u00df er uns fehlt, wir fassen's kaum,", "tokens": ["Und", "wir", ",", "da\u00df", "er", "uns", "fehlt", ",", "wir", "fas\u00b7sen's", "kaum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Als \u00e4ngstet' uns ein unbarmherz'ger Traum.", "tokens": ["Als", "\u00e4ngs\u00b7tet'", "uns", "ein", "un\u00b7barm\u00b7her\u00b7z'\u00b7ger", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "All was wir ihm verdankt, erst im Verlust", "tokens": ["All", "was", "wir", "ihm", "ver\u00b7dankt", ",", "erst", "im", "Ver\u00b7lust"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PWS", "PPER", "PPER", "VVFIN", "$,", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wird's der beraubten Seele voll bewu\u00dft:", "tokens": ["Wird's", "der", "be\u00b7raub\u00b7ten", "See\u00b7le", "voll", "be\u00b7wu\u00dft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die Welt von Wissen, die er in sich trug", "tokens": ["Die", "Welt", "von", "Wis\u00b7sen", ",", "die", "er", "in", "sich", "trug"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "PPER", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sie beherrscht' in freiem Geistesflug,", "tokens": ["Und", "sie", "be\u00b7herrscht'", "in", "frei\u00b7em", "Geis\u00b7tes\u00b7flug", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Die Welt von Sch\u00f6nheit, die von Jugend an", "tokens": ["Die", "Welt", "von", "Sch\u00f6n\u00b7heit", ",", "die", "von", "Ju\u00b7gend", "an"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PRELS", "APPR", "NN", "APPR"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erobernd er zu eigen sich gewann,", "tokens": ["Er\u00b7o\u00b7bernd", "er", "zu", "ei\u00b7gen", "sich", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PPER", "PTKA", "ADJD", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Am Quell der alten Dichtung fr\u00fch gen\u00e4hrt,", "tokens": ["Am", "Quell", "der", "al\u00b7ten", "Dich\u00b7tung", "fr\u00fch", "ge\u00b7n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Erbe Gottfried's, seines Ahnen wert,", "tokens": ["Der", "Er\u00b7be", "Gott\u00b7frie\u00b7d's", ",", "sei\u00b7nes", "Ah\u00b7nen", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.10": {"line.1": {"text": "So da\u00df, erwacht bei seiner Saiten Klang,", "tokens": ["So", "da\u00df", ",", "er\u00b7wacht", "bei", "sei\u00b7ner", "Sai\u00b7ten", "Klang", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verschollne Sage neu die Fl\u00fcgel schwang!", "tokens": ["Ver\u00b7scholl\u00b7ne", "Sa\u00b7ge", "neu", "die", "Fl\u00fc\u00b7gel", "schwang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJD", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.11": {"line.1": {"text": "Wohl lie\u00df er uns zu k\u00f6stlichem Gewinn", "tokens": ["Wohl", "lie\u00df", "er", "uns", "zu", "k\u00f6st\u00b7li\u00b7chem", "Ge\u00b7winn"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein Werk zur\u00fcck, \u2013 er selber ging dahin,", "tokens": ["Sein", "Werk", "zu\u00b7r\u00fcck", ",", "\u2013", "er", "sel\u00b7ber", "ging", "da\u00b7hin", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PTKVZ", "$,", "$(", "PPER", "ADV", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Und was er lebend war und gab und sprach,", "tokens": ["Und", "was", "er", "le\u00b7bend", "war", "und", "gab", "und", "sprach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VAFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "T\u00f6nt unersetzlich uns im Innern nach.", "tokens": ["T\u00f6nt", "un\u00b7er\u00b7setz\u00b7lich", "uns", "im", "In\u00b7nern", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.13": {"line.1": {"text": "Wie gern genie\u00dfend sa\u00df er jugendfrisch,", "tokens": ["Wie", "gern", "ge\u00b7nie\u00b7\u00dfend", "sa\u00df", "er", "ju\u00b7gend\u00b7frisch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVPP", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein stolzer Zecher, an des Lebens Tisch!", "tokens": ["Ein", "stol\u00b7zer", "Ze\u00b7cher", ",", "an", "des", "Le\u00b7bens", "Tisch", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Sein goldnes Lachen \u2013 niemand lachte so! \u2013", "tokens": ["Sein", "gold\u00b7nes", "La\u00b7chen", "\u2013", "nie\u00b7mand", "lach\u00b7te", "so", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$(", "PIS", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie macht' es Jeden in der Seele froh!", "tokens": ["Wie", "macht'", "es", "Je\u00b7den", "in", "der", "See\u00b7le", "froh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PIAT", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Sein milder Ernst, der Keinen je versehrt,", "tokens": ["Sein", "mil\u00b7der", "Ernst", ",", "der", "Kei\u00b7nen", "je", "ver\u00b7sehrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sein edler Zorn, wenn Niedres ihn emp\u00f6rt,", "tokens": ["Sein", "ed\u00b7ler", "Zorn", ",", "wenn", "Nie\u00b7dres", "ihn", "em\u00b7p\u00f6rt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "KOUS", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Ein zartes Mitgef\u00fchl in Leid und Lust,", "tokens": ["Ein", "zar\u00b7tes", "Mit\u00b7ge\u00b7f\u00fchl", "in", "Leid", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kindersinn in fester Mannesbrust,", "tokens": ["Ein", "Kin\u00b7der\u00b7sinn", "in", "fes\u00b7ter", "Man\u00b7nes\u00b7brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.17": {"line.1": {"text": "Treu seinen G\u00f6ttern dienend, immer fern", "tokens": ["Treu", "sei\u00b7nen", "G\u00f6t\u00b7tern", "die\u00b7nend", ",", "im\u00b7mer", "fern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Marktgew\u00fchl, vertrauend seinem Stern,", "tokens": ["Dem", "Markt\u00b7ge\u00b7w\u00fchl", ",", "ver\u00b7trau\u00b7end", "sei\u00b7nem", "Stern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVPP", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.18": {"line.1": {"text": "Und all das nun dahin, was uns begl\u00fcckt,", "tokens": ["Und", "all", "das", "nun", "da\u00b7hin", ",", "was", "uns", "be\u00b7gl\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PDS", "ADV", "PAV", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Kalt diese Hand, die unsre warm gedr\u00fcckt \u2013 \u2013", "tokens": ["Kalt", "die\u00b7se", "Hand", ",", "die", "uns\u00b7re", "warm", "ge\u00b7dr\u00fcckt", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "PDAT", "NN", "$,", "PRELS", "PPOSAT", "ADJD", "VVPP", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.19": {"line.1": {"text": "Ja, klagt um ihn! Doch unsre Klage stillt:", "tokens": ["Ja", ",", "klagt", "um", "ihn", "!", "Doch", "uns\u00b7re", "Kla\u00b7ge", "stillt", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "VVFIN", "APPR", "PPER", "$.", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns war geg\u00f6nnt solch seltnes Menschenbild.", "tokens": ["Uns", "war", "ge\u00b7g\u00f6nnt", "solch", "selt\u00b7nes", "Men\u00b7schen\u00b7bild", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.20": {"line.1": {"text": "Ihm sagen durften wir, bescheiden zwar,", "tokens": ["Ihm", "sa\u00b7gen", "durf\u00b7ten", "wir", ",", "be\u00b7schei\u00b7den", "zwar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "PPER", "$,", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem tiefbescheidnen Freund, was er uns war,", "tokens": ["Dem", "tief\u00b7be\u00b7scheid\u00b7nen", "Freund", ",", "was", "er", "uns", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWS", "PPER", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.21": {"line.1": {"text": "So da\u00df er, den die Mitwelt kaum erkannt,", "tokens": ["So", "da\u00df", "er", ",", "den", "die", "Mit\u00b7welt", "kaum", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "$,", "PRELS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Doch einen Schatz von Lieb' und Treue fand", "tokens": ["Doch", "ei\u00b7nen", "Schatz", "von", "Lieb'", "und", "Treu\u00b7e", "fand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.22": {"line.1": {"text": "Und nichts, bis ihn verschlang die letzte Nacht,", "tokens": ["Und", "nichts", ",", "bis", "ihn", "ver\u00b7schlang", "die", "letz\u00b7te", "Nacht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$,", "KOUS", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Entbehrte, was das Leben lieblich macht.", "tokens": ["Ent\u00b7behr\u00b7te", ",", "was", "das", "Le\u00b7ben", "lieb\u00b7lich", "macht", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.23": {"line.1": {"text": "Und so, ob du nun ruhst am dunklen Ort,", "tokens": ["Und", "so", ",", "ob", "du", "nun", "ruhst", "am", "dunk\u00b7len", "Ort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Du Vielgeliebter, lebst du mit uns fort,", "tokens": ["Du", "Viel\u00b7ge\u00b7lieb\u00b7ter", ",", "lebst", "du", "mit", "uns", "fort", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "VVFIN", "PPER", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.24": {"line.1": {"text": "Und Tod und Schicksal \u00fcberdauernd, zieht", "tokens": ["Und", "Tod", "und", "Schick\u00b7sal", "\u00f