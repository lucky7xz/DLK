{"textgrid.poem.47023": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Drum wenn du nun, wie du mit jedem Blicke,", "tokens": ["Drum", "wenn", "du", "nun", ",", "wie", "du", "mit", "je\u00b7dem", "Bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "ADV", "$,", "PWAV", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit jedem Laut es gibst mir zu erkennen,", "tokens": ["Mit", "je\u00b7dem", "Laut", "es", "gibst", "mir", "zu", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "APPR", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Gern dieses Handels F\u00e4den m\u00f6chtest trennen,", "tokens": ["Gern", "die\u00b7ses", "Han\u00b7dels", "F\u00e4\u00b7den", "m\u00f6ch\u00b7test", "tren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So thu's, du kannst es ja im Augenblicke.", "tokens": ["So", "thu's", ",", "du", "kannst", "es", "ja", "im", "Au\u00b7gen\u00b7bli\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "PPER", "VMFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sag' nur dem Aug' einmal, da\u00df sanft es blicke,", "tokens": ["Sag'", "nur", "dem", "Aug'", "ein\u00b7mal", ",", "da\u00df", "sanft", "es", "bli\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "NN", "ADV", "$,", "KOUS", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "La\u00df deinen Mund einmal nur sanft mich nennen,", "tokens": ["La\u00df", "dei\u00b7nen", "Mund", "ein\u00b7mal", "nur", "sanft", "mich", "nen\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "ADV", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Lippen Ku\u00df nur einmal sanft mir brennen,", "tokens": ["Der", "Lip\u00b7pen", "Ku\u00df", "nur", "ein\u00b7mal", "sanft", "mir", "bren\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADV", "ADV", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So f\u00e4llt das Band von selbst mir vom Genicke.", "tokens": ["So", "f\u00e4llt", "das", "Band", "von", "selbst", "mir", "vom", "Ge\u00b7ni\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Denn da die Zauber, die mich halten, Dorne", "tokens": ["Denn", "da", "die", "Zau\u00b7ber", ",", "die", "mich", "hal\u00b7ten", ",", "Dor\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nur sind des Stolzes und des Trotzes Nesseln;", 