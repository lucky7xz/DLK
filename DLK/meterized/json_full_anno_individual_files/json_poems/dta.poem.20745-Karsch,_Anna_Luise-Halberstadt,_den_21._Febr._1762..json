{"dta.poem.20745": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Halberstadt, den 21. Febr. 1762.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verw\u00fcnschte Heiligkeit der Ehe!                 ", "tokens": ["Ver\u00b7w\u00fcnschte", "Hei\u00b7lig\u00b7keit", "der", "E\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ich zittre, wenn ich noch im Geist zur\u00fccke sehe,", "tokens": ["Ich", "zitt\u00b7re", ",", "wenn", "ich", "noch", "im", "Geist", "zu\u00b7r\u00fc\u00b7cke", "se\u00b7he", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Abscheulich war der Sclavenstand,", "tokens": ["Ab\u00b7scheu\u00b7lich", "war", "der", "Scla\u00b7ven\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "++-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein nur mit Menschenhaut bezogner H\u00f6llenbrand", "tokens": ["Ein", "nur", "mit", "Men\u00b7schen\u00b7haut", "be\u00b7zog\u00b7ner", "H\u00f6l\u00b7len\u00b7brand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Trat herrisch vor mir hin und br\u00fcllte meine Klage", "tokens": ["Trat", "her\u00b7risch", "vor", "mir", "hin", "und", "br\u00fcll\u00b7te", "mei\u00b7ne", "Kla\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit bitterm Spotte nach, und war geborne Plage", "tokens": ["Mit", "bit\u00b7term", "Spot\u00b7te", "nach", ",", "und", "war", "ge\u00b7bor\u00b7ne", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$,", "KON", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "F\u00fcr mein so sanftes Herz; mein ewig Einerlei", "tokens": ["F\u00fcr", "mein", "so", "sanf\u00b7tes", "Herz", ";", "mein", "e\u00b7wig", "Ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADV", "ADJA", "NN", "$.", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Blieb er zehn volle Jahr, ri\u00df oft ein Blatt entzwei,", "tokens": ["Blieb", "er", "zehn", "vol\u00b7le", "Jahr", ",", "ri\u00df", "oft", "ein", "Blatt", "ent\u00b7zwei", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "CARD", "ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.9": {"text": "Ganz von Gedanken voll, denn dieser Mann, kein", "tokens": ["Ganz", "von", "Ge\u00b7dan\u00b7ken", "voll", ",", "denn", "die\u00b7ser", "Mann", ",", "kein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,", "KON", "PDAT", "NN", "$,", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Denker,", "tokens": ["Den\u00b7ker", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "War fehlbar durch den Rausch, war meines Lebens", "tokens": ["War", "fehl\u00b7bar", "durch", "den", "Rausch", ",", "war", "mei\u00b7nes", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "APPR", "ART", "NN", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Henker,", "tokens": ["Hen\u00b7ker", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Sein Gang, sein Wort, sein Blick, war alles meine", "tokens": ["Sein", "Gang", ",", "sein", "Wort", ",", "sein", "Blick", ",", "war", "al\u00b7les", "mei\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "VAFIN", "PIS", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Qual,", "tokens": ["Qual", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "O Gott! beh\u00fcte mich f\u00fcr eine Mannes-Wahl.", "tokens": ["O", "Gott", "!", "be\u00b7h\u00fc\u00b7te", "mich", "f\u00fcr", "ei\u00b7ne", "Man\u00b7nes\u00b7Wahl", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}