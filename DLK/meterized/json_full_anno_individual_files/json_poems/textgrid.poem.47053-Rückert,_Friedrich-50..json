{"textgrid.poem.47053": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "50.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du denkst vielleicht, ich habe dich vergessen,", "tokens": ["Du", "denkst", "viel\u00b7leicht", ",", "ich", "ha\u00b7be", "dich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil du nicht mehr mich siehest, da\u00df ich wanke", "tokens": ["Weil", "du", "nicht", "mehr", "mich", "sie\u00b7hest", ",", "da\u00df", "ich", "wan\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "ADV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hinaus nach dem von dir kredenzten Tranke", "tokens": ["Hin\u00b7aus", "nach", "dem", "von", "dir", "kre\u00b7denz\u00b7ten", "Tran\u00b7ke"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "APPR", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Liebesthorheit, wie ich's that vordessen.", "tokens": ["Der", "Lie\u00b7best\u00b7hor\u00b7heit", ",", "wie", "ich's", "that", "vor\u00b7des\u00b7sen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PIS", "VVFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Nicht denken w\u00fcrdest du es, wenn ermessen", "tokens": ["Nicht", "den\u00b7ken", "w\u00fcr\u00b7dest", "du", "es", ",", "wenn", "er\u00b7mes\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "PPER", "$,", "KOUS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du k\u00f6nntest, wie noch t\u00e4glich mein Gedanke", "tokens": ["Du", "k\u00f6nn\u00b7test", ",", "wie", "noch", "t\u00e4g\u00b7lich", "mein", "Ge\u00b7dan\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "$,", "PWAV", "ADV", "ADJD", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ausfliegend Kost mir holt aus deinem Schranke,", "tokens": ["Aus\u00b7flie\u00b7gend", "Kost", "mir", "holt", "aus", "dei\u00b7nem", "Schran\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie Raben einst dem Seher holten Essen.", "tokens": ["Wie", "Ra\u00b7ben", "einst", "dem", "Se\u00b7her", "hol\u00b7ten", "Es\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nicht denken w\u00fcrdest du es, wenn du w\u00fc\u00dftest,", "tokens": ["Nicht", "den\u00b7ken", "w\u00fcr\u00b7dest", "du", "es", ",", "wenn", "du", "w\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "VAFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie oft ich n\u00e4chtlich hinter deinem R\u00fccken", "tokens": ["Wie", "oft", "ich", "n\u00e4cht\u00b7lich", "hin\u00b7ter", "dei\u00b7nem", "R\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "ADJD", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Veranstaltet mit dir Zusammenk\u00fcnfte,", "tokens": ["Ver\u00b7an\u00b7stal\u00b7tet", "mit", "dir", "Zu\u00b7sam\u00b7men\u00b7k\u00fcnf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "NN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ja wie du eben jetzt hier einziehn m\u00fc\u00dftest,", "tokens": ["Ja", "wie", "du", "e\u00b7ben", "jetzt", "hier", "ein\u00b7ziehn", "m\u00fc\u00df\u00b7test", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWAV", "PPER", "ADV", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da ich, dich deinem Lager zu entr\u00fccken,", "tokens": ["Da", "ich", ",", "dich", "dei\u00b7nem", "La\u00b7ger", "zu", "ent\u00b7r\u00fc\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PPER", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Hab' ausgesendet meiner Geister Z\u00fcnfte.", "tokens": ["Hab'", "aus\u00b7ge\u00b7sen\u00b7det", "mei\u00b7ner", "Geis\u00b7ter", "Z\u00fcnf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du denkst vielleicht, ich habe dich vergessen,", "tokens": ["Du", "denkst", "viel\u00b7leicht", ",", "ich", "ha\u00b7be", "dich", "ver\u00b7ges\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["