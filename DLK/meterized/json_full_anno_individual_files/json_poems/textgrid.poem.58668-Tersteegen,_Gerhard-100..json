{"textgrid.poem.58668": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "100.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach, weide mich in dir mit dir, mein guter Hirte,", "tokens": ["Ach", ",", "wei\u00b7de", "mich", "in", "dir", "mit", "dir", ",", "mein", "gu\u00b7ter", "Hir\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "PRF", "APPR", "PPER", "APPR", "PPER", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Herz la\u00df meinen Stall und sanftes Lager sein;", "tokens": ["Dein", "Herz", "la\u00df", "mei\u00b7nen", "Stall", "und", "sanf\u00b7tes", "La\u00b7ger", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVIMP", "PPOSAT", "NN", "KON", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Such die verlorne Seel', bring wieder die verirrte,", "tokens": ["Such", "die", "ver\u00b7lor\u00b7ne", "Seel'", ",", "bring", "wie\u00b7der", "die", "ver\u00b7irr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "ADJA", "NN", "$,", "VVFIN", "ADV", "ART", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verbind und st\u00e4rk dein krank' und schwaches Sch\u00e4felein!", "tokens": ["Ver\u00b7bind", "und", "st\u00e4rk", "dein", "krank'", "und", "schwa\u00b7ches", "Sch\u00e4\u00b7fe\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein andrer w\u00e4hne frei, da\u00df er 'was sei und k\u00f6nne,", "tokens": ["Ein", "an\u00b7drer", "w\u00e4h\u00b7ne", "frei", ",", "da\u00df", "er", "'was", "sei", "und", "k\u00f6n\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADJD", "$,", "KOUS", "PPER", "ADV", "VAFIN", "KON", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Vertilge ganz in mir, den fett' und starken Sinn;", "tokens": ["Ver\u00b7til\u00b7ge", "ganz", "in", "mir", ",", "den", "fett'", "und", "star\u00b7ken", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "PPER", "$,", "ART", "ADJA", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Gib, da\u00df ich tief mein Nichts und deine Gnad' erkenne,", "tokens": ["Gib", ",", "da\u00df", "ich", "tief", "mein", "Nichts", "und", "dei\u00b7ne", "Gnad'", "er\u00b7ken\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "KOUS", "PPER", "ADJD", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df du mein guter Hirt und ich dein Sch\u00e4flein bin!", "tokens": ["Da\u00df", "du", "mein", "gu\u00b7ter", "Hirt", "und", "ich", "dein", "Sch\u00e4f\u00b7lein", "bin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "ADJA", "NN", "KON", "PPER", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hex