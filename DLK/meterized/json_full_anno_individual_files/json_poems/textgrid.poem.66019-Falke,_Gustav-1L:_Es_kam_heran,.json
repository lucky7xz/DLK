{"textgrid.poem.66019": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es kam heran,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.85", "nl:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kam heran,", "tokens": ["Es", "kam", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Glanzstrotzend kam's heran,", "tokens": ["Glanz\u00b7strot\u00b7zend", "kam's", "he\u00b7ran", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "++-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mit wei\u00dfen Hengsten, langsam, feierlich,", "tokens": ["Mit", "wei\u00b7\u00dfen", "Hengs\u00b7ten", ",", "lang\u00b7sam", ",", "fei\u00b7er\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Ruhms Gespann.", "tokens": ["Des", "Ruhms", "Ge\u00b7spann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Als blitzten hundert Sonnen", "tokens": ["Als", "blitz\u00b7ten", "hun\u00b7dert", "Son\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "CARD", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr Licht um ihn,", "tokens": ["Ihr", "Licht", "um", "ihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPER", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Erstrahlte seine Bahn,", "tokens": ["Er\u00b7strahl\u00b7te", "sei\u00b7ne", "Bahn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Der ganze Himmel schien", "tokens": ["Der", "gan\u00b7ze", "Him\u00b7mel", "schien"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Vom Leuchten \u00fcberronnen,", "tokens": ["Vom", "Leuch\u00b7ten", "\u00fc\u00b7ber\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "Das wie ein Herold lief und k\u00fcndete sein Nahn:", "tokens": ["Das", "wie", "ein", "He\u00b7rold", "lief", "und", "k\u00fcn\u00b7de\u00b7te", "sein", "Nahn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KOKOM", "ART", "NN", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Triumph! Triumph!", "tokens": ["Tri\u00b7umph", "!", "Tri\u00b7umph", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$."], "meter": "+---", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Er kam, kam k\u00f6niglich.", "tokens": ["Er", "kam", ",", "kam", "k\u00f6\u00b7nig\u00b7lich", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Ein sorglos L\u00e4cheln lag,", "tokens": ["Ein", "sorg\u00b7los", "L\u00e4\u00b7cheln", "lag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ein heitrer Mut auf seinem Angesicht,", "tokens": ["Ein", "hei\u00b7trer", "Mut", "auf", "sei\u00b7nem", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein heldisches \u00bbIch wag\u00ab,", "tokens": ["Ein", "hel\u00b7di\u00b7sches", "\u00bb", "Ich", "wag", "\u00ab", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "$(", "PPER", "VVFIN", "$(", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Das trunken Sterne sich zum Siegeskranze bricht.", "tokens": ["Das", "trun\u00b7ken", "Ster\u00b7ne", "sich", "zum", "Sie\u00b7ges\u00b7kran\u00b7ze", "bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sein gro\u00dfes Auge, spr\u00fchte", "tokens": ["Sein", "gro\u00b7\u00dfes", "Au\u00b7ge", ",", "spr\u00fch\u00b7te"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Den gro\u00dfen Friedrichsblick,", "tokens": ["Den", "gro\u00b7\u00dfen", "Fried\u00b7richs\u00b7blick", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Die sch\u00f6ne, volle Wange gl\u00fchte,", "tokens": ["Die", "sch\u00f6\u00b7ne", ",", "vol\u00b7le", "Wan\u00b7ge", "gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Stolz straffte das Genick,", "tokens": ["Stolz", "straff\u00b7te", "das", "Ge\u00b7nick", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Und ich war nichts dem Herrn.", "tokens": ["Und", "ich", "war", "nichts", "dem", "Herrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Am Wege stand ich da, bedr\u00e4ngt von Huf und Speichen,", "tokens": ["Am", "We\u00b7ge", "stand", "ich", "da", ",", "be\u00b7dr\u00e4ngt", "von", "Huf", "und", "Spei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Hart streifte mich der Nabe Rand,", "tokens": ["Hart", "streif\u00b7te", "mich", "der", "Na\u00b7be", "Rand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Triumphators flatterndes Gewand,", "tokens": ["Des", "Tri\u00b7um\u00b7pha\u00b7tors", "flat\u00b7tern\u00b7des", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Scharlachsaum, die Hand konnt' ihn erreichen.", "tokens": ["Sein", "Schar\u00b7lach\u00b7saum", ",", "die", "Hand", "konnt'", "ihn", "er\u00b7rei\u00b7chen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und langsam nur,", "tokens": ["Und", "lang\u00b7sam", "nur", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Ganz langsam mir vorbei sein goldner Wagen fuhr.", "tokens": ["Ganz", "lang\u00b7sam", "mir", "vor\u00b7bei", "sein", "gold\u00b7ner", "Wa\u00b7gen", "fuhr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPER", "ADV", "PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wen sah ich hinten drauf?", "tokens": ["Wen", "sah", "ich", "hin\u00b7ten", "drauf", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Wen lud der stolze Herr sich auf?", "tokens": ["Wen", "lud", "der", "stol\u00b7ze", "Herr", "sich", "auf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "PRF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wer hockte auf dem Ruhmeskarren?", "tokens": ["Wer", "hock\u00b7te", "auf", "dem", "Ruh\u00b7mes\u00b7kar\u00b7ren", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Der Tod eintr\u00e4chtig mit den Narren.", "tokens": ["Der", "Tod", "ein\u00b7tr\u00e4ch\u00b7tig", "mit", "den", "Nar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der eine sa\u00df zusammengekauert,", "tokens": ["Der", "ei\u00b7ne", "sa\u00df", "zu\u00b7sam\u00b7men\u00b7ge\u00b7kau\u00b7ert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Starr, unbewegt,", "tokens": ["Starr", ",", "un\u00b7be\u00b7wegt", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["NE", "$,", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Den Kiefer auf das spitze Kinn gelegt,", "tokens": ["Den", "Kie\u00b7fer", "auf", "das", "spit\u00b7ze", "Kinn", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Raubtier, das auf Beute lauert.", "tokens": ["Ein", "Raub\u00b7tier", ",", "das", "auf", "Beu\u00b7te", "lau\u00b7ert", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und unverwandt,", "tokens": ["Und", "un\u00b7ver\u00b7wandt", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Am plumpen Sensenschaft die Knochenhand,", "tokens": ["Am", "plum\u00b7pen", "Sen\u00b7sen\u00b7schaft", "die", "Kno\u00b7chen\u00b7hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umraschelt leis von welkem Lorbeerlaub,", "tokens": ["Um\u00b7ra\u00b7schelt", "leis", "von", "wel\u00b7kem", "Lor\u00b7beer\u00b7laub", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PWAT", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.8": {"text": "Dran noch die wei\u00dfe Atlasschleife baumelt,", "tokens": ["Dran", "noch", "die", "wei\u00b7\u00dfe", "At\u00b7las\u00b7schlei\u00b7fe", "bau\u00b7melt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "So stierte mich, der j\u00e4h zur\u00fcckgetaumelt", "tokens": ["So", "stier\u00b7te", "mich", ",", "der", "j\u00e4h", "zu\u00b7r\u00fcck\u00b7ge\u00b7tau\u00b7melt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PRELS", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Im Wegestaub,", "tokens": ["Im", "We\u00b7ge\u00b7staub", ","], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der Tod wie drohend an.", "tokens": ["Der", "Tod", "wie", "dro\u00b7hend", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Doch rechts mit Schalks- und Schelmenpossen,", "tokens": ["Doch", "rechts", "mit", "Schalks", "und", "Schel\u00b7men\u00b7pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Grinsegru\u00df und Freundschaftsnicken,", "tokens": ["Mit", "Grin\u00b7se\u00b7gru\u00df", "und", "Freund\u00b7schafts\u00b7ni\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Im Faschingkleid aus bunten Flicken", "tokens": ["Im", "Fa\u00b7sching\u00b7kleid", "aus", "bun\u00b7ten", "Fli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sa\u00df schellenklingelnd neben dem Genossen", "tokens": ["Sa\u00df", "schel\u00b7len\u00b7klin\u00b7gelnd", "ne\u00b7ben", "dem", "Ge\u00b7nos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Der Narr und lie\u00df die Beine pendeln.", "tokens": ["Der", "Narr", "und", "lie\u00df", "die", "Bei\u00b7ne", "pen\u00b7deln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Kinderspielzeug vor dem Mund,", "tokens": ["Ein", "Kin\u00b7der\u00b7spiel\u00b7zeug", "vor", "dem", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ein Blechtrompetchen winziger Gr\u00f6\u00dfe,", "tokens": ["Ein", "Blech\u00b7trom\u00b7pet\u00b7chen", "win\u00b7zi\u00b7ger", "Gr\u00f6\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Blies er die Backen kugelrund,", "tokens": ["Blies", "er", "die", "Ba\u00b7cken", "ku\u00b7gel\u00b7rund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Als g\u00e4lte es Posaunenst\u00f6\u00dfe.", "tokens": ["Als", "g\u00e4l\u00b7te", "es", "Po\u00b7sau\u00b7nen\u00b7st\u00f6\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Und wie vor seinem Jahrmarktszelt,", "tokens": ["Und", "wie", "vor", "sei\u00b7nem", "Jahr\u00b7markts\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Vor seiner Wunder-Plunderwelt,", "tokens": ["Vor", "sei\u00b7ner", "Wun\u00b7der\u00b7P\u00b7lun\u00b7der\u00b7welt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein Clown die bl\u00f6den Gaffer stellt,", "tokens": ["Ein", "Clown", "die", "bl\u00f6\u00b7den", "Gaf\u00b7fer", "stellt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Gewohnt mit jedem anzub\u00e4ndeln,", "tokens": ["Ge\u00b7wohnt", "mit", "je\u00b7dem", "an\u00b7zu\u00b7b\u00e4n\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PIS", "VVIZU", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Verh\u00f6hnte mich sein Pritschenwink:", "tokens": ["Ver\u00b7h\u00f6hn\u00b7te", "mich", "sein", "Prit\u00b7schen\u00b7wink", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Spring auf doch lieber Vetter, flink!", "tokens": ["Spring", "auf", "doch", "lie\u00b7ber", "Vet\u00b7ter", ",", "flink", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "APPR", "ADV", "ADV", "NE", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Gel\u00fcstet's dich nicht, mitzufahren?", "tokens": [