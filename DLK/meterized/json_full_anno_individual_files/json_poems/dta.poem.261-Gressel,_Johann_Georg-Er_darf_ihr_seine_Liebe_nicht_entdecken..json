{"dta.poem.261": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er darf ihr seine Liebe nicht entdecken.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Blondine deine Pracht", "tokens": ["Blon\u00b7di\u00b7ne", "dei\u00b7ne", "Pracht"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hat mich verliebt gemacht;", "tokens": ["Hat", "mich", "ver\u00b7liebt", "ge\u00b7macht", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mein Hertz mu\u00df dir zu einem Opffer brennen/", "tokens": ["Mein", "Hertz", "mu\u00df", "dir", "zu", "ei\u00b7nem", "Opf\u00b7fer", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "APPR", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch darf ich nicht die Seelen-Wunde nennen;", "tokens": ["Doch", "darf", "ich", "nicht", "die", "See\u00b7len\u00b7Wun\u00b7de", "nen\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ich soll dich nicht/", "tokens": ["Ich", "soll", "dich", "nicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Du sch\u00f6nstes Licht/", "tokens": ["Du", "sch\u00f6ns\u00b7tes", "Licht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Zu meinem Trost und froher H\u00fclffe kennen.", "tokens": ["Zu", "mei\u00b7nem", "Trost", "und", "fro\u00b7her", "H\u00fclf\u00b7fe", "ken\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allzeit bin ich betr\u00fcbt/", "tokens": ["All\u00b7zeit", "bin", "ich", "be\u00b7tr\u00fcbt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ohne Trost verliebt/", "tokens": ["Und", "oh\u00b7ne", "Trost", "ver\u00b7liebt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich darf dich nur/ mein Licht/ von ferne schauen/", "tokens": ["Ich", "darf", "dich", "nur", "/", "mein", "Licht", "/", "von", "fer\u00b7ne", "schau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "$(", "PPOSAT", "NN", "$(", "APPR", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nicht dem Artzt die Kranckheit anvertrauen/", "tokens": ["Und", "nicht", "dem", "Artzt", "die", "Kran\u00b7ck\u00b7heit", "an\u00b7ver\u00b7trau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Die innre Noht/", "tokens": ["Die", "inn\u00b7re", "Noht", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Der Seelen Tod/", "tokens": ["Der", "See\u00b7len", "Tod", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Will mir aus Gram schon mein Gr\u00e4bni\u00df bauen/", "tokens": ["Will", "mir", "aus", "Gram", "schon", "mein", "Gr\u00e4b\u00b7ni\u00df", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Wenn soll erbo\u00dftes Gl\u00fcck", "tokens": ["Wenn", "soll", "er\u00b7bo\u00df\u00b7tes", "Gl\u00fcck"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich \u00e4ndern dein Geschick?", "tokens": ["Sich", "\u00e4n\u00b7dern", "dein", "Ge\u00b7schick", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn wird vor Gram einst stoltze Freude bl\u00fchen/", "tokens": ["Wenn", "wird", "vor", "Gram", "einst", "stolt\u00b7ze", "Freu\u00b7de", "bl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "APPR", "NN", "ADV", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nach der Nacht der rohte Morgen gl\u00fchen?", "tokens": ["Und", "nach", "der", "Nacht", "der", "roh\u00b7te", "Mor\u00b7gen", "gl\u00fc\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da\u00df ich mit Lust", "tokens": ["Da\u00df", "ich", "mit", "Lust"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Kan aus der Brust", "tokens": ["Kan", "aus", "der", "Brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Den s\u00fcssen Safft der Zucker K\u00fcsse ziehen.", "tokens": ["Den", "s\u00fcs\u00b7sen", "Safft", "der", "Zu\u00b7cker", "K\u00fcs\u00b7se", "zie\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch stoltze Freude fort/", "tokens": ["Doch", "stolt\u00b7ze", "Freu\u00b7de", "fort", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Hier ist kein Anmuhts-Port/", "tokens": ["Hier", "ist", "kein", "Anm\u00b7uhts\u00b7Port", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich mu\u00df mein Licht nur in Gedancken k\u00fcssen/", "tokens": ["Ich", "mu\u00df", "mein", "Licht", "nur", "in", "Ge\u00b7dan\u00b7cken", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nichts von Lust und wahrer Freude wissen;", "tokens": ["Und", "nichts", "von", "Lust", "und", "wah\u00b7rer", "Freu\u00b7de", "wis\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "APPR", "NN", "KON", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Bis da\u00df der Schmertz", "tokens": ["Bis", "da\u00df", "der", "Schmertz"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Zerbricht mein Hertz/", "tokens": ["Zer\u00b7bricht", "mein", "Hertz", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Und nach der Pein mein Leben macht beschliessen.", "tokens": ["Und", "nach", "der", "Pein", "mein", "Le\u00b7ben", "macht", "be\u00b7schlies\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PPOSAT", "NN", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}