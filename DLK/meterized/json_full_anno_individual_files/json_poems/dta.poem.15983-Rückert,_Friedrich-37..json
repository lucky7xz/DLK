{"dta.poem.15983": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "37.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.71", "nl:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wol der Gedanke bringt die ganze Welt hervor,", "tokens": ["Wol", "der", "Ge\u00b7dan\u00b7ke", "bringt", "die", "gan\u00b7ze", "Welt", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Der, welchen Gott gedacht, nicht den du denkst, o Thor.", "tokens": ["Der", ",", "wel\u00b7chen", "Gott", "ge\u00b7dacht", ",", "nicht", "den", "du", "denkst", ",", "o", "Thor", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "$,", "PWAT", "NN", "VVPP", "$,", "PTKNEG", "ART", "PPER", "VVFIN", "$,", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du denkst sie, ohne da\u00df darum entsteht die Welt,", "tokens": ["Du", "denkst", "sie", ",", "oh\u00b7ne", "da\u00df", "da\u00b7rum", "ent\u00b7steht", "die", "Welt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "KOUI", "KOUS", "PAV", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ohne da\u00df, wenn du sie wegdenkst, sie wegf\u00e4llt.", "tokens": ["Und", "oh\u00b7ne", "da\u00df", ",", "wenn", "du", "sie", "weg\u00b7denkst", ",", "sie", "weg\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "APPR", "KOUS", "$,", "KOUS", "PPER", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Aus Geist entstand die Welt, und gehet auf in Geist,", "tokens": ["Aus", "Geist", "ent\u00b7stand", "die", "Welt", ",", "und", "ge\u00b7het", "auf", "in", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geist ist der Grund, aus dem, in den zur\u00fcck sie kreist.", "tokens": ["Geist", "ist", "der", "Grund", ",", "aus", "dem", ",", "in", "den", "zu\u00b7r\u00fcck", "sie", "kreist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "APPR", "ART", "$,", "APPR", "ART", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Der Geist ein Aetherduft hat sich in sich gedichtet,", "tokens": ["Der", "Geist", "ein", "A\u00b7e\u00b7ther\u00b7duft", "hat", "sich", "in", "sich", "ge\u00b7dich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PRF", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und Sternennebel hat zu Sonnen sich gelichtet.", "tokens": ["Und", "Ster\u00b7nen\u00b7ne\u00b7bel", "hat", "zu", "Son\u00b7nen", "sich", "ge\u00b7lich\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "APPR", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der Nebel hat in Luft und Wasser sich zersetzt,", "tokens": ["Der", "Ne\u00b7bel", "hat", "in", "Luft", "und", "Was\u00b7ser", "sich", "zer\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und Schlamm ward Erd' und Stein, und Pflanz' und Thier zuletzt,", "tokens": ["Und", "Schlamm", "ward", "Erd'", "und", "Stein", ",", "und", "Pflanz'", "und", "Thier", "zu\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "NN", "KON", "NN", "$,", "KON", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Und menschliche Gestalt, in der der Menschengeist", "tokens": ["Und", "menschli\u00b7che", "Ge\u00b7stalt", ",", "in", "der", "der", "Men\u00b7schen\u00b7geist"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Durch Gottes Hauch erwacht, und Ihn, den Urgeist, preist.", "tokens": ["Durch", "Got\u00b7tes", "Hauch", "er\u00b7wacht", ",", "und", "Ihn", ",", "den", "Ur\u00b7geist", ",", "preist", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "NN", "NN", "VVPP", "$,", "KON", "PPER", "$,", "ART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}