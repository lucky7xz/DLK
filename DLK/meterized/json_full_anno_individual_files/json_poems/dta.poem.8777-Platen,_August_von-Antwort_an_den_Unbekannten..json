{"dta.poem.8777": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Antwort an den Unbekannten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Gl\u00e4ttend jene Falten alle, welche meine Stirn verd\u00fcstern,", "tokens": ["Gl\u00e4t\u00b7tend", "je\u00b7ne", "Fal\u00b7ten", "al\u00b7le", ",", "wel\u00b7che", "mei\u00b7ne", "Stirn", "ver\u00b7d\u00fcs\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "PIS", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Zeigend, da\u00df ich nicht vergebens Nesseln schwang und", "tokens": ["Zei\u00b7gend", ",", "da\u00df", "ich", "nicht", "ver\u00b7ge\u00b7bens", "Nes\u00b7seln", "schwang", "und"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "NN", "VVFIN", "KON"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Nicht mit Danaideneimern aus des Lebens Brunnen", "tokens": ["Nicht", "mit", "Da\u00b7na\u00b7i\u00b7de\u00b7nei\u00b7mern", "aus", "des", "Le\u00b7bens", "Brun\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "APPR", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Meiner Widersacher Mi\u00dfmuth st\u00f6rt mich nicht in Roms", "tokens": ["Mei\u00b7ner", "Wi\u00b7der\u00b7sa\u00b7cher", "Mi\u00df\u00b7muth", "st\u00f6rt", "mich", "nicht", "in", "Roms"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "APPR", "NE"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.5": {"text": "Doch die Liebe, wie ein Pilger, \u00fcbersteigt die Apenninen.", "tokens": ["Doch", "die", "Lie\u00b7be", ",", "wie", "ein", "Pil\u00b7ger", ",", "\u00fc\u00b7bers\u00b7teigt", "die", "A\u00b7pen\u00b7ni\u00b7nen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Allen denen, die so gerne jede wahre Kraft verkennen", "tokens": ["Al\u00b7len", "de\u00b7nen", ",", "die", "so", "ger\u00b7ne", "je\u00b7de", "wah\u00b7re", "Kraft", "ver\u00b7ken\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "PDS", "$,", "PRELS", "ADV", "ADV", "PIAT", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Sey's gesagt, da\u00df nicht einmal ich ihren Namen h\u00f6re", "tokens": ["Sey's", "ge\u00b7sagt", ",", "da\u00df", "nicht", "ein\u00b7mal", "ich", "ih\u00b7ren", "Na\u00b7men", "h\u00f6\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVPP", "$,", "KOUS", "PTKNEG", "ADV", "PPER", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.8": {"text": "Doch von Andern h\u00f6r' ich, welche sonder Scheu vor Witzes-", "tokens": ["Doch", "von", "An\u00b7dern", "h\u00f6r'", "ich", ",", "wel\u00b7che", "son\u00b7der", "Scheu", "vor", "Wit\u00b7zes"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "VVFIN", "PPER", "$,", "PRELS", "APPR", "NN", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.9": {"text": "Loben mein Gedicht mit Einsicht und mit Einsicht auch", "tokens": ["Lo\u00b7ben", "mein", "Ge\u00b7dicht", "mit", "Ein\u00b7sicht", "und", "mit", "Ein\u00b7sicht", "auch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPR", "NN", "KON", "APPR", "NN", "ADV"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.10": {"text": "Diesen biet' ich aus der Ferne gern die Hand, und Dir", "tokens": ["Die\u00b7sen", "biet'", "ich", "aus", "der", "Fer\u00b7ne", "gern", "die", "Hand", ",", "und", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "ART", "NN", "$,", "KON", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Zwar Du lie\u00dfest nicht die Stimme kritischer Vernunft", "tokens": ["Zwar", "Du", "lie\u00b7\u00dfest", "nicht", "die", "Stim\u00b7me", "kri\u00b7ti\u00b7scher", "Ver\u00b7nunft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.12": {"text": "Aber nach dem Kapitole, dessen H\u00f6h'n ich jezt erklimme,", "tokens": ["A\u00b7ber", "nach", "dem", "Ka\u00b7pi\u00b7to\u00b7le", ",", "des\u00b7sen", "H\u00f6h'n", "ich", "jezt", "er\u00b7klim\u00b7me", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Liessest wehn Du mir Begeist'rung, jene reine Milder-", "tokens": ["Lies\u00b7sest", "wehn", "Du", "mir", "Be\u00b7geist'\u00b7rung", ",", "je\u00b7ne", "rei\u00b7ne", "Mil\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "PPER", "NN", "$,", "PDAT", "ADJA", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Die so glockenhell und herrlich von der Menschenlippe", "tokens": ["Die", "so", "glo\u00b7cken\u00b7hell", "und", "herr\u00b7lich", "von", "der", "Men\u00b7schen\u00b7lip\u00b7pe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "KON", "ADJD", "APPR", "ART", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Und elektrisch ihren sch\u00f6nen Liebesfunken weiter leitet.", "tokens": ["Und", "e\u00b7lekt\u00b7risch", "ih\u00b7ren", "sch\u00f6\u00b7nen", "Lie\u00b7be\u00b7sfun\u00b7ken", "wei\u00b7ter", "lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "Ja, es m\u00fcssen, wo dem Guten sie sich beigesellt, dem", "tokens": ["Ja", ",", "es", "m\u00fcs\u00b7sen", ",", "wo", "dem", "Gu\u00b7ten", "sie", "sich", "bei\u00b7ge\u00b7sellt", ",", "dem"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PTKANT", "$,", "PPER", "VMFIN", "$,", "PWAV", "ART", "NN", "PPER", "PRF", "VVPP", "$,", "PRELS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Aus der Seele Dithyramben, wie aus Wolken Blitze,", "tokens": ["Aus", "der", "See\u00b7le", "Dit\u00b7hy\u00b7ram\u00b7ben", ",", "wie", "aus", "Wol\u00b7ken", "Blit\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,", "PWAV", "APPR", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.18": {"text": "M\u00f6gen denn auch meine T\u00f6ne durch des Nordens St\u00fcrme", "tokens": ["M\u00f6\u00b7gen", "denn", "auch", "mei\u00b7ne", "T\u00f6\u00b7ne", "durch", "des", "Nor\u00b7dens", "St\u00fcr\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ADV", "PPOSAT", "NN", "APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Wie ein Weihgesang des Orpheus auf dem Schiff der", "tokens": ["Wie", "ein", "Weih\u00b7ge\u00b7sang", "des", "Or\u00b7pheus", "auf", "dem", "Schiff", "der"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NE", "APPR", "ART", "NN", "ART"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "Die den Pelz, den im Barbarenland sie sich mit M\u00fch'", "tokens": ["Die", "den", "Pelz", ",", "den", "im", "Bar\u00b7ba\u00b7ren\u00b7land", "sie", "sich", "mit", "M\u00fch'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "PPER", "PRF", "APPR", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.21": {"text": "F\u00fcr Apollo's Mantel halten, der in Tempe's L\u00fcften", "tokens": ["F\u00fcr", "A\u00b7pol\u00b7lo's", "Man\u00b7tel", "hal\u00b7ten", ",", "der", "in", "Tem\u00b7pe's", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "VVINF", "$,", "PRELS", "APPR", "NE", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "Rufe nicht, da mich das deutsche Chaos w\u00fcrde blos er-", "tokens": ["Ru\u00b7fe", "nicht", ",", "da", "mich", "das", "deut\u00b7sche", "Chaos", "w\u00fcr\u00b7de", "blos", "er"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PPER", "ART", "ADJA", "NN", "VAFIN", "ADV", "TRUNC"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Rufe nicht zur\u00fcck den Dichter aus dem vielgeliebten", "tokens": ["Ru\u00b7fe", "nicht", "zu\u00b7r\u00fcck", "den", "Dich\u00b7ter", "aus", "dem", "viel\u00b7ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "ADV", "ART", "NN", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Welcher, bis mich Frost und Alter l\u00fcstern macht nach", "tokens": ["Wel\u00b7cher", ",", "bis", "mich", "Frost", "und", "Al\u00b7ter", "l\u00fcs\u00b7tern", "macht", "nach"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAT", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VVINF", "VVFIN", "APPR"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Ueber jedes meiner Worte Str\u00f6me von Musik ergie\u00dfe.", "tokens": ["Ue\u00b7ber", "je\u00b7des", "mei\u00b7ner", "Wor\u00b7te", "Str\u00f6\u00b7me", "von", "Mu\u00b7sik", "er\u00b7gie\u00b7\u00dfe", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Immer mehr nach S\u00fcden la\u00df mich meines Auges W\u00fcnsche", "tokens": ["Im\u00b7mer", "mehr", "nach", "S\u00fc\u00b7den", "la\u00df", "mich", "mei\u00b7nes", "Au\u00b7ges", "W\u00fcn\u00b7sche"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "NN", "VVIMP", "PPER", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Und gen\u00e4hrt von Hyblahonig auf des Aetna Gipfel dichten!", "tokens": ["Und", "ge\u00b7n\u00e4hrt", "von", "Hyb\u00b7la\u00b7ho\u00b7nig", "auf", "des", "A\u00b7et\u00b7na", "Gip\u00b7fel", "dich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "APPR", "NE", "APPR", "ART", "NE", "NE", "ADJA", "$."], "meter": "+-+-+-+-+-+--+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.7": {"text": "La\u00df mich Odysseen erfinden, schweifend an Homers Ge-", "tokens": ["La\u00df", "mich", "O\u00b7dys\u00b7seen", "er\u00b7fin\u00b7den", ",", "schwei\u00b7fend", "an", "Ho\u00b7mers", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "NN", "VVINF", "$,", "ADJD", "APPR", "NE", "TRUNC"], "meter": "+-+-+-+-+--+--", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Bald, in voller Waffenr\u00fcstung, folgen ihnen Iliaden.", "tokens": ["Bald", ",", "in", "vol\u00b7ler", "Waf\u00b7fen\u00b7r\u00fcs\u00b7tung", ",", "fol\u00b7gen", "ih\u00b7nen", "I\u00b7lia\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "NE", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.9": {"text": "Ja, wenn ganz mit deutscher Seele griech'sche Kunst sich", "tokens": ["Ja", ",", "wenn", "ganz", "mit", "deut\u00b7scher", "See\u00b7le", "griech'\u00b7sche", "Kunst", "sich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "KOUS", "ADV", "APPR", "ADJA", "NN", "ADJA", "NN", "PRF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.10": {"text": "Sollst Du sehn, zu welchen Pfeilen greift Apoll, zu wel-", "tokens": ["Sollst", "Du", "sehn", ",", "zu", "wel\u00b7chen", "Pfei\u00b7len", "greift", "A\u00b7poll", ",", "zu", "wel"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "VVINF", "$,", "APPR", "PWAT", "NN", "VVFIN", "NE", "$,", "APPR", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.11": {"text": "Noch so lange, Freund, so lange la\u00df umher mich", "tokens": ["Noch", "so", "lan\u00b7ge", ",", "Freund", ",", "so", "lan\u00b7ge", "la\u00df", "um\u00b7her", "mich"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "NN", "$,", "ADV", "ADV", "VVIMP", "PTKVZ", "PPER"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.12": {"text": "Bis Thuiskons Volk und meine Wenigkeit zusammen", "tokens": ["Bis", "Thu\u00b7is\u00b7kons", "Volk", "und", "mei\u00b7ne", "We\u00b7nig\u00b7keit", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NN", "KON", "PPOSAT", "NN", "PTKVZ"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.13": {"text": "Bis wir Einer Lehre Sch\u00fcler, Br\u00fcder sind von Einem", "tokens": ["Bis", "wir", "Ei\u00b7ner", "Leh\u00b7re", "Sch\u00fc\u00b7ler", ",", "Br\u00fc\u00b7der", "sind", "von", "Ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "NN", "$,", "NN", "VAFIN", "APPR", "ART"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.14": {"text": "Beide dann einander w\u00fcrdig, und einander lieb ge-", "tokens": ["Bei\u00b7de", "dann", "ein\u00b7an\u00b7der", "w\u00fcr\u00b7dig", ",", "und", "ein\u00b7an\u00b7der", "lieb", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "ADV", "ADV", "ADJD", "$,", "KON", "ADV", "ADJD", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.15": {"text": "Wie die Lerche m\u00f6cht' ich kommen, wann die ersten", "tokens": ["Wie", "die", "Ler\u00b7che", "m\u00f6cht'", "ich", "kom\u00b7men", ",", "wann", "die", "ers\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PWAV", "ART", "ADJA"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.16": {"text": "Nicht wie euer Schneegest\u00f6ber wehn und endlich liegen", "tokens": ["Nicht", "wie", "eu\u00b7er", "Schnee\u00b7ge\u00b7st\u00f6\u00b7ber", "wehn", "und", "end\u00b7lich", "lie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "KOKOM", "PPOSAT", "NN", "VVINF", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.17": {"text": "Eher nicht an eure Herzen klopf' ich an, an eure Pforten,", "tokens": ["E\u00b7her", "nicht", "an", "eu\u00b7re", "Her\u00b7zen", "klopf'", "ich", "an", ",", "an", "eu\u00b7re", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Bis das Sch\u00f6nste nicht gethan ich, eine gro\u00dfe That in", "tokens": ["Bis", "das", "Sch\u00f6ns\u00b7te", "nicht", "ge\u00b7than", "ich", ",", "ei\u00b7ne", "gro\u00b7\u00dfe", "That", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PTKNEG", "VVPP", "PPER", "$,", "ART", "ADJA", "NN", "APPR"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.19": {"text": "Welche kalte Sinne gl\u00fchn macht, Lob erpre\u00dft von Syl-", "tokens": ["Wel\u00b7che", "kal\u00b7te", "Sin\u00b7ne", "gl\u00fchn", "macht", ",", "Lob", "er\u00b7pre\u00dft", "von", "Sy