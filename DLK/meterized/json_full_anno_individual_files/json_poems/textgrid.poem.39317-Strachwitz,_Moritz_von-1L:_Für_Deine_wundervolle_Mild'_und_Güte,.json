{"textgrid.poem.39317": {"metadata": {"author": {"name": "Strachwitz, Moritz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: F\u00fcr Deine wundervolle Mild' und G\u00fcte,", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "F\u00fcr Deine wundervolle Mild' und G\u00fcte,", "tokens": ["F\u00fcr", "Dei\u00b7ne", "wun\u00b7der\u00b7vol\u00b7le", "Mild'", "und", "G\u00fc\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie k\u00f6nnt' ich jemals hoffen, Dir zu danken!", "tokens": ["Wie", "k\u00f6nnt'", "ich", "je\u00b7mals", "hof\u00b7fen", ",", "Dir", "zu", "dan\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich kann nur beten, da\u00df Dich Gott beh\u00fcte!", "tokens": ["Ich", "kann", "nur", "be\u00b7ten", ",", "da\u00df", "Dich", "Gott", "be\u00b7h\u00fc\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "VVINF", "$,", "KOUS", "PPER", "NN", "VVFIN", "$."], "meter": "-+-+-+++-+-", "measure": "unknown.measure.hexa"}}, "stanza.2": {"line.1": {"text": "Du sa\u00dfest standhaft bei dem wilden Kranken", "tokens": ["Du", "sa\u00b7\u00dfest", "stand\u00b7haft", "bei", "dem", "wil\u00b7den", "Kran\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und h\u00f6rtest an und suchtest zu beschwichten", "tokens": ["Und", "h\u00f6r\u00b7test", "an", "und", "such\u00b7test", "zu", "be\u00b7schwich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KON", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Des wunden Herzens st\u00fcrmische Gedanken.", "tokens": ["Des", "wun\u00b7den", "Her\u00b7zens", "st\u00fcr\u00b7mi\u00b7sche", "Ge\u00b7dan\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "So kennst Du denn mein Denken und mein Dichten,", "tokens": ["So", "kennst", "Du", "denn", "mein", "Den\u00b7ken", "und", "mein", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich lie\u00df Dich schaun bis auf den Grund der Welle,", "tokens": ["Ich", "lie\u00df", "Dich", "schaun", "bis", "auf", "den", "Grund", "der", "Wel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVINF", "KON", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kennst mich ganz; Du wirst zu streng nicht richten!", "tokens": ["Du", "kennst", "mich", "ganz", ";", "Du", "wirst", "zu", "streng", "nicht", "rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PTKZU", "VVFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es rollt mein Blut in mehr als deutscher Schnelle,", "tokens": ["Es", "rollt", "mein", "Blut", "in", "mehr", "als", "deut\u00b7scher", "Schnel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "KOKOM", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und viel ges\u00fcndigt hat dies Blut zu Zeiten;", "tokens": ["Und", "viel", "ge\u00b7s\u00fcn\u00b7digt", "hat", "dies", "Blut", "zu", "Zei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAFIN", "PDS", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch bin ich sonst ein ehrlicher Geselle.", "tokens": ["Doch", "bin", "ich", "sonst", "ein", "ehr\u00b7li\u00b7cher", "Ge\u00b7sel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich konnte selten nur dies Blut bestreiten,", "tokens": ["Ich", "konn\u00b7te", "sel\u00b7ten", "nur", "dies", "Blut", "be\u00b7strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJD", "ADV", "PDS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Geschrieben steht in diesem Buch mit Flammen", "tokens": ["Ge\u00b7schrie\u00b7ben", "steht", "in", "die\u00b7sem", "Buch", "mit", "Flam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "PDAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die lange Kunde meiner Trunkenheiten.", "tokens": ["Die", "lan\u00b7ge", "Kun\u00b7de", "mei\u