{"dta.poem.11783": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "69.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Ewige, das ganz geno\u00dfen G\u00f6tters\u00f6hne,", "tokens": ["Das", "E\u00b7wi\u00b7ge", ",", "das", "ganz", "ge\u00b7no\u00b7\u00dfen", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "PRELS", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ward Menschen dreigetheilt das Wahre, Gute, Sch\u00f6ne.", "tokens": ["Ward", "Men\u00b7schen", "drei\u00b7ge\u00b7theilt", "das", "Wah\u00b7re", ",", "Gu\u00b7te", ",", "Sch\u00f6\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "NN", "VVFIN", "ART", "ADJA", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Denn k\u00e4m' es ungetheilt, des Menschen schwache Sinnen", "tokens": ["Denn", "k\u00e4m'", "es", "un\u00b7ge\u00b7theilt", ",", "des", "Men\u00b7schen", "schwa\u00b7che", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Riss' \u00fcberw\u00e4ltigend das Ew'ge ganz von hinnen.", "tokens": ["Riss'", "\u00fc\u00b7berw\u00b7\u00e4l\u00b7ti\u00b7gend", "das", "Ew'\u00b7ge", "ganz", "von", "hin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "ART", "ADJA", "ADV", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Drum hat es sich getheilt, nur in verschiedner Weise", "tokens": ["Drum", "hat", "es", "sich", "ge\u00b7theilt", ",", "nur", "in", "ver\u00b7schied\u00b7ner", "Wei\u00b7se"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "PPER", "PRF", "VVPP", "$,", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Sinn zum Ewigen vorzubereiten leise.", "tokens": ["Den", "Sinn", "zum", "E\u00b7wi\u00b7gen", "vor\u00b7zu\u00b7be\u00b7rei\u00b7ten", "lei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "VVFIN", "ADJD", "$."], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Das Wahre wird gewahrt vom geist'gen Sinn, dem Sinnen;", "tokens": ["Das", "Wah\u00b7re", "wird", "ge\u00b7wahrt", "vom", "geist'\u00b7gen", "Sinn", ",", "dem", "Sin\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gute wohnt verh\u00fcllt dem Sinn des Guten innen.", "tokens": ["Das", "Gu\u00b7te", "wohnt", "ver\u00b7h\u00fcllt", "dem", "Sinn", "des", "Gu\u00b7ten", "in\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVFIN", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nur zu erscheinen hat das Sch\u00f6ne sich getraut", "tokens": ["Nur", "zu", "er\u00b7schei\u00b7nen", "hat", "das", "Sch\u00f6\u00b7ne", "sich", "ge\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKZU", "VVINF", "VAFIN", "ART", "NN", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem \u00e4u\u00dfern Sinne selbst, das Sch\u00f6ne wird geschaut.", "tokens": ["Dem", "\u00e4u\u00b7\u00dfern", "Sin\u00b7ne", "selbst", ",", "das", "Sch\u00f6\u00b7ne", "wird", "ge\u00b7schaut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die beiden wollten auch durchs dritte sichtbar werden,", "tokens": ["Die", "bei\u00b7den", "woll\u00b7ten", "auch", "durchs", "drit\u00b7te", "sicht\u00b7bar", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VMFIN", "ADV", "APPRART", "ADJA", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum Sch\u00f6nen sprachen sie mit flehenden Geberden:", "tokens": ["Zum", "Sch\u00f6\u00b7nen", "spra\u00b7chen", "sie", "mit", "fle\u00b7hen\u00b7den", "Ge\u00b7ber\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Versprich uns, nie zu gehn ins Menschenaug' allein,", "tokens": ["Ver\u00b7sprich", "uns", ",", "nie", "zu", "gehn", "ins", "Men\u00b7sche\u00b7naug'", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "$,", "ADV", "PTKZU", "VVINF", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ohn' uns in Geist und Herz zu f\u00fchren mit hinein.", "tokens": ["Ohn'", "uns", "in", "Geist", "und", "Herz", "zu", "f\u00fch\u00b7ren", "mit", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "APPR", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Sonst wird der bl\u00f6de Geist das Wahre kaum gewahr,", "tokens": ["Sonst", "wird", "der", "bl\u00f6\u00b7de", "Geist", "das", "Wah\u00b7re", "kaum", "ge\u00b7wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nicht dem Herzen wird das Gute g\u00f6ttlich klar.", "tokens": ["Und", "nicht", "dem", "Her\u00b7zen", "wird", "das", "Gu\u00b7te", "g\u00f6tt\u00b7lich", "klar", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "VAFIN", "ART", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Du sollst das Wahre ihm bew\u00e4hren, ja gew\u00e4hren,", "tokens": ["Du", "sollst", "das", "Wah\u00b7re", "ihm", "be\u00b7w\u00e4h\u00b7ren", ",", "ja", "ge\u00b7w\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "PPER", "VVINF", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Gute sollst du ihm verkl\u00e4ren, ja verkl\u00e4ren.", "tokens": ["Das", "Gu\u00b7te", "sollst", "du", "ihm", "ver\u00b7kl\u00e4\u00b7ren", ",", "ja", "ver\u00b7kl\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und dir, o Sch\u00f6nes, ist der Vorzug mit geschenkt,", "tokens": ["Und", "dir", ",", "o", "Sch\u00f6\u00b7nes", ",", "ist", "der", "Vor\u00b7zug", "mit", "ge\u00b7schenkt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "FM", "NN", "$,", "VAFIN", "ART", "NN", "APPR", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df er als Gutes selbst dich f\u00fchlt, als Wahres denkt.", "tokens": ["Da\u00df", "er", "als", "Gu\u00b7tes", "selbst", "dich", "f\u00fchlt", ",", "als", "Wah\u00b7res", "denkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ADV", "PPER", "VVFIN", "$,", "KOUS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Nur wenn wir so in ihm erg\u00e4nzend uns vereinen,", "tokens": ["Nur", "wenn", "wir", "so", "in", "ihm", "er\u00b7g\u00e4n\u00b7zend", "uns", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVPP", "PPER", "VVINF", "$,"], "meter": "-+-+---+-+-+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Wird ganz das Ewige im Endlichen erscheinen.", "tokens": ["Wird", "ganz", "das", "E\u00b7wi\u00b7ge", "im", "End\u00b7li\u00b7chen", "er\u00b7schei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}