{"dta.poem.2735": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der lebendige Brunn Jsraels/  \n Bey Beerdigung Hn.   M. C. N. D.   zu   St. M. M.  \n  den 24. Junii 1679. entworffen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Brunn/ der aus Jsraels Heil-Quellen ist entsprossen/", "tokens": ["Brunn", "/", "der", "aus", "Js\u00b7raels", "Heil\u00b7Quel\u00b7len", "ist", "ent\u00b7spros\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "ART", "APPR", "NE", "NN", "VAFIN", "VVPP", "$("], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Der offt den Matten Durst der Sterblichkeit gelescht/", "tokens": ["Der", "offt", "den", "Mat\u00b7ten", "Durst", "der", "Sterb\u00b7lich\u00b7keit", "ge\u00b7lescht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der von dem Lebens-Fel\u00df Crystallen-klar geflossen/", "tokens": ["Der", "von", "dem", "Le\u00b7bens\u00b7Fel\u00df", "Cry\u00b7stal\u00b7len\u00b7klar", "ge\u00b7flos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und dessen Ufer nie bekleidet Schilf und J\u00e4scht/", "tokens": ["Und", "des\u00b7sen", "U\u00b7fer", "nie", "be\u00b7klei\u00b7det", "Schilf", "und", "J\u00e4scht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADV", "VVFIN", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So solstu wie der Krith in Augenblick versiegen?", "tokens": ["So", "sols\u00b7tu", "wie", "der", "Krith", "in", "Au\u00b7gen\u00b7blick", "ver\u00b7sie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "KOKOM", "ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Schluckt denn der Erden-Schlund jetzt deine Quellen ein?", "tokens": ["Schluckt", "denn", "der", "Er\u00b7den\u00b7Sch\u00b7lund", "jetzt", "dei\u00b7ne", "Quel\u00b7len", "ein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-++-+-+-+", "measure": "unknown.measure.septa"}, "line.7": {"text": "Soll kein bedr\u00e4ngtes Hertz sich mehr zu dir verf\u00fcgen/", "tokens": ["Soll", "kein", "be\u00b7dr\u00e4ng\u00b7tes", "Hertz", "sich", "mehr", "zu", "dir", "ver\u00b7f\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIAT", "ADJA", "NN", "PRF", "ADV", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das Labsal bey dir sucht/ und Lindrung seiner Pein?", "tokens": ["Das", "Lab\u00b7sal", "bey", "dir", "sucht", "/", "und", "Lind\u00b7rung", "sei\u00b7ner", "Pein", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VVFIN", "$(", "KON", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Was ist doch lieblichers auff aller Welt zu finden/", "tokens": ["Was", "ist", "doch", "lieb\u00b7li\u00b7chers", "auff", "al\u00b7ler", "Welt", "zu", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADV", "APPR", "PIAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn jetzt der Sonnen-Glut der Erden Brust verbrennt/", "tokens": ["Wenn", "jetzt", "der", "Son\u00b7nen\u00b7Glut", "der", "Er\u00b7den", "Brust", "ver\u00b7brennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "NN", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und alle Kr\u00e4fften uns/ ja Geist und Leben schwinden/", "tokens": ["Und", "al\u00b7le", "Kr\u00e4ff\u00b7ten", "uns", "/", "ja", "Geist", "und", "Le\u00b7ben", "schwin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "$(", "ADV", "NN", "KON", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Da\u00df man f\u00fcr Mattigkeit einander kaum fast kennt/", "tokens": ["Da\u00df", "man", "f\u00fcr", "Mat\u00b7tig\u00b7keit", "ein\u00b7an\u00b7der", "kaum", "fast", "kennt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PRF", "ADV", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Als wenn ein tunckler Wald und eine gr\u00fcne H\u00f6le", "tokens": ["Als", "wenn", "ein", "tunck\u00b7ler", "Wald", "und", "ei\u00b7ne", "gr\u00fc\u00b7ne", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Uns einen Brunn gew\u00e4hrt/ der aus den Adern quillt/", "tokens": ["Uns", "ei\u00b7nen", "Brunn", "ge\u00b7w\u00e4hrt", "/", "der", "aus", "den", "A\u00b7dern", "quillt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "VVPP", "$(", "ART", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mit lieblichem Ger\u00e4usch ergetzt die krancke Seele/", "tokens": ["Mit", "lieb\u00b7li\u00b7chem", "Ge\u00b7r\u00e4usch", "er\u00b7getzt", "die", "kran\u00b7cke", "See\u00b7le", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und durch sein k\u00fchles Na\u00df des Durstes Flammen stillt?", "tokens": ["Und", "durch", "sein", "k\u00fch\u00b7les", "Na\u00df", "des", "Durs\u00b7tes", "Flam\u00b7men", "stillt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier sucht der Wanders-Mann/ der Kraft-lo\u00df/ sein Vergn\u00fcgen/", "tokens": ["Hier", "sucht", "der", "Wan\u00b7der\u00b7sMann", "/", "der", "Kraft\u00b7lo\u00df", "/", "sein", "Ver\u00b7gn\u00fc\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "ART", "NN", "$(", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Der Schnitter eilet hier halb-schmachtende zur Ruh:", "tokens": ["Der", "Schnit\u00b7ter", "ei\u00b7let", "hier", "halb\u00b7schmach\u00b7ten\u00b7de", "zur", "Ruh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Der Bothe w\u00fcnscht ihm da auf kurtze Zeit zu ligen/", "tokens": ["Der", "Bo\u00b7the", "w\u00fcnscht", "ihm", "da", "auf", "kurt\u00b7ze", "Zeit", "zu", "li\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und wer f\u00fcr\u00fcber geht/ gesellet sich darzu.", "tokens": ["Und", "wer", "f\u00fc\u00b7r\u00fc\u00b7ber", "geht", "/", "ge\u00b7sel\u00b7let", "sich", "dar\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADV", "VVFIN", "$(", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "So viel Ergetzligkeit kan frisches Wasser geben!", "tokens": ["So", "viel", "Er\u00b7getz\u00b7lig\u00b7keit", "kan", "fri\u00b7sches", "Was\u00b7ser", "ge\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VMFIN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ach wie weit herrlicher mu\u00df dieser Brunn-Quell seyn?", "tokens": ["Ach", "wie", "weit", "herr\u00b7li\u00b7cher", "mu\u00df", "die\u00b7ser", "Brunn\u00b7Quell", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ADJD", "ADJA", "VMFIN", "PDAT", "NN", "VAINF", "$."], "meter": "+-++-+-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.23": {"text": "Der durch sein Wasser uns gibt Seele Geist und leben/", "tokens": ["Der", "durch", "sein", "Was\u00b7ser", "uns", "gibt", "See\u00b7le", "Geist", "und", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PPER", "VVFIN", "NN", "NN", "KON", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Da\u00df uns auf Ewigkeit kein Durst nicht nimmet ein!", "tokens": ["Da\u00df", "uns", "auf", "E\u00b7wig\u00b7keit", "kein", "Durst", "nicht", "nim\u00b7met", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "PIAT", "NN", "PTKNEG", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Zu diesem Lebens-Brunn hastu durch deine Lehren", "tokens": ["Zu", "die\u00b7sem", "Le\u00b7bens\u00b7Brunn", "has\u00b7tu", "durch", "dei\u00b7ne", "Leh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Bi\u00dfher/ ", "tokens": ["Bi\u00df\u00b7her", "/"], "token_info": ["word", "punct"], "pos": ["ADV", "$("], "meter": "--", "measure": "unknown.measure.zero"}, "line.27": {"text": "Dein Reden war ein Strom/ der stets mit vollen R\u00f6hren", "tokens": ["Dein", "Re\u00b7den", "war", "ein", "Strom", "/", "der", "stets", "mit", "vol\u00b7len", "R\u00f6h\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$(", "ART", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Auff der Zuh\u00f6rer Hertz lebendig Wasser geust.", "tokens": ["Auff", "der", "Zu\u00b7h\u00f6\u00b7rer", "Hertz", "le\u00b7ben\u00b7dig", "Was\u00b7ser", "geust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "ADJD", "NN", "VVPP", "$."], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.29": {"text": "Und wie hat nicht dein Mund von GOttes Lob gequollen/", "tokens": ["Und", "wie", "hat", "nicht", "dein", "Mund", "von", "Got\u00b7tes", "Lob", "ge\u00b7quol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VAFIN", "PTKNEG", "PPOSAT", "NN", "APPR", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Wenn seine Satzungen und G\u00fcte du gelehrt?", "tokens": ["Wenn", "sei\u00b7ne", "Sat\u00b7zun\u00b7gen", "und", "G\u00fc\u00b7te", "du", "ge\u00b7lehrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "KON", "NN", "PPER", "VVPP", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.31": {"text": "Uns trifft nur der Verlust/ da\u00df wir dich missen sollen/", "tokens": ["Uns", "trifft", "nur", "der", "Ver\u00b7lust", "/", "da\u00df", "wir", "dich", "mis\u00b7sen", "sol\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$(", "KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df man dich weiter nicht/ dich Kirchen-Engel/ h\u00f6rt.", "tokens": ["Da\u00df", "man", "dich", "wei\u00b7ter", "nicht", "/", "dich", "Kir\u00b7chen\u00b7En\u00b7gel", "/", "h\u00f6rt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADV", "PTKNEG", "$(", "PPER", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Es ist das Heidenthum mit Eifer zu verlachen/", "tokens": ["Es", "ist", "das", "Hei\u00b7den\u00b7thum", "mit", "Ei\u00b7fer", "zu", "ver\u00b7la\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Das seine Brunnen hat den Najaden geweyht/", "tokens": ["Das", "sei\u00b7ne", "Brun\u00b7nen", "hat", "den", "Na\u00b7ja\u00b7den", "ge\u00b7weyht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.35": {"text": "Und aus den Quellen selbst offt G\u00f6tter wollen machen", "tokens": ["Und", "aus", "den", "Quel\u00b7len", "selbst", "offt", "G\u00f6t\u00b7ter", "wol\u00b7len", "ma\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "ADV", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Mit Opffern sie verehrt/ mit Blumen auch bestreut.", "tokens": ["Mit", "Opf\u00b7fern", "sie", "ver\u00b7ehrt", "/", "mit", "Blu\u00b7men", "auch", "be\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "VVPP", "$(", "APPR", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Nein/ du bist deinem GOtt ein heilig Quell gewesen/", "tokens": ["Nein", "/", "du", "bist", "dei\u00b7nem", "Gott", "ein", "hei\u00b7lig", "Quell", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "ART", "ADJD", "NN", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Und ein versigelt Brunn durch seiner Allmacht Hand.", "tokens": ["Und", "ein", "ver\u00b7si\u00b7gelt", "Brunn", "durch", "sei\u00b7ner", "All\u00b7macht", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "VVFIN", "NE", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Er lie\u00df von erster Schuld dich durch sein Bad genesen/", "tokens": ["Er", "lie\u00df", "von", "ers\u00b7ter", "Schuld", "dich", "durch", "sein", "Bad", "ge\u00b7ne\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "PRF", "APPR", "PPOSAT", "NE", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und hat den Himmel dir zum Erbrecht zugewandt.", "tokens": ["Und", "hat", "den", "Him\u00b7mel", "dir", "zum", "Er\u00b7brecht", "zu\u00b7ge\u00b7wandt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Denn hat er seinen Geist sehr reich auff dich gegossen/", "tokens": ["Denn", "hat", "er", "sei\u00b7nen", "Geist", "sehr", "reich", "auff", "dich", "ge\u00b7gos\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-++--+-", "measure": "iambi