{"dta.poem.20143": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1666", "urn": "urn:nbn:de:kobv:b4-20424-1", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch-Historischer Lustgarten. Frankfurt (Main), 1666."}, "poem": {"stanza.1": {"line.1": {"text": "So schosst und sprosset einst die Gartenlust\nhervor/", "tokens": ["So", "schosst", "und", "spros\u00b7set", "einst", "die", "Gar\u00b7ten\u00b7lust", "her\u00b7vor", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mit der Bl\u00fcht zugleich begilbte Fr\u00fcchte", "tokens": ["So", "mit", "der", "Bl\u00fcht", "zu\u00b7gleich", "be\u00b7gilb\u00b7te", "Fr\u00fcch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kraft welcher Uns bekannt/ da\u00df/ der ihn ange-", "tokens": ["Kraft", "wel\u00b7cher", "Uns", "be\u00b7kannt", "/", "da\u00df", "/", "der", "ihn", "an\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NN", "PRELS", "PPER", "ADJD", "$(", "KOUS", "$(", "PRELS", "PPER", "TRUNC"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Schon albereit beflammt gl\u00fch\u2019 an Olympus", "tokens": ["Schon", "al\u00b7be\u00b7reit", "be\u00b7flammt", "gl\u00fch'", "an", "O\u00b7lym\u00b7pus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "ADJD", "APPR", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Man r\u00fchmet jenen Wald in welchem Or-", "tokens": ["Man", "r\u00fch\u00b7met", "je\u00b7nen", "Wald", "in", "wel\u00b7chem", "Or"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PDAT", "NN", "APPR", "PWAT", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "So lieblich da\u00df Er auch/ den harten Fel\u00df be-", "tokens": ["So", "lieb\u00b7lich", "da\u00df", "Er", "auch", "/", "den", "har\u00b7ten", "Fel\u00df", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KOUS", "PPER", "ADV", "$(", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Ich/ \u00e4dler Neuemark/ ich r\u00fchme deinen", "tokens": ["Ich", "/", "\u00e4d\u00b7ler", "Neu\u00b7e\u00b7mark", "/", "ich", "r\u00fch\u00b7me", "dei\u00b7nen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "$(", "ADJA", "NN", "$(", "PPER", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der nun im Garten klingt/ der Hertz und Sinne", "tokens": ["Der", "nun", "im", "Gar\u00b7ten", "klingt", "/", "der", "Hertz", "und", "Sin\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "APPRART", "NN", "VVFIN", "$(", "ART", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du gabst Uns einen Wald/ du giebst Uns einen", "tokens": ["Du", "gabst", "Uns", "ei\u00b7nen", "Wald", "/", "du", "giebst", "Uns", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$(", "PPER", "VVFIN", "PPER", "ART"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In welchem du besingst der schn\u00f6den Erden", "tokens": ["In", "wel\u00b7chem", "du", "be\u00b7singst", "der", "schn\u00f6\u00b7den", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es mu\u00df vor solchen Flei\u00df dir einsten Eden war-", "tokens": ["Es", "mu\u00df", "vor", "sol\u00b7chen", "Flei\u00df", "dir", "eins\u00b7ten", "E\u00b7den", "wa\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PIAT", "NN", "PPER", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Denn singe G\u00f6tterfreund/ denn letze deine", "tokens": ["Denn", "sin\u00b7ge", "G\u00f6t\u00b7ter\u00b7freund", "/", "denn", "let\u00b7ze", "dei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "$(", "KON", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}