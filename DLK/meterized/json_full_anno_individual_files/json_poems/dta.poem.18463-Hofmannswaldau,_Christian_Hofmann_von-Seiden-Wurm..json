{"dta.poem.18463": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Seiden-Wurm.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1703", "urn": "urn:nbn:de:kobv:b4-200905199360", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dieses was mir nahrung schafft/", "tokens": ["Die\u00b7ses", "was", "mir", "nah\u00b7rung", "schafft", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Ist was mich von hinnen rafft/", "tokens": ["Ist", "was", "mich", "von", "hin\u00b7nen", "rafft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PWS", "PPER", "APPR", "ADV", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wil ich andern reichthum geben/", "tokens": ["Wil", "ich", "an\u00b7dern", "reicht\u00b7hum", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bring ich mich selbst um das leben.", "tokens": ["Bring", "ich", "mich", "selbst", "um", "das", "le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PDS", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}