{"textgrid.poem.55015": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Abschied", "genre": "verse", "period": "N.A.", "pub_year": 1797, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu lieblich ist's, ein Wort zu brechen,", "tokens": ["Zu", "lieb\u00b7lich", "ist's", ",", "ein", "Wort", "zu", "bre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu schwer die wohlerkannte Pflicht,", "tokens": ["Zu", "schwer", "die", "woh\u00b7ler\u00b7kann\u00b7te", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und leider kann man nichts versprechen,", "tokens": ["Und", "lei\u00b7der", "kann", "man", "nichts", "ver\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PIS", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was unserm Herzen widerspricht.", "tokens": ["Was", "un\u00b7serm", "Her\u00b7zen", "wi\u00b7der\u00b7spricht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du \u00fcbst die alten Zauberlieder,", "tokens": ["Du", "\u00fcbst", "die", "al\u00b7ten", "Zau\u00b7berl\u00b7ie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du lockst ihn, der kaum ruhig war,", "tokens": ["Du", "lockst", "ihn", ",", "der", "kaum", "ru\u00b7hig", "war", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Schaukelkahn der s\u00fc\u00dfen Torheit wieder,", "tokens": ["Zum", "Schau\u00b7kel\u00b7kahn", "der", "s\u00fc\u00b7\u00dfen", "Tor\u00b7heit", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erneust, verdoppelst die Gefahr.", "tokens": ["Er\u00b7neust", ",", "ver\u00b7dop\u00b7pelst", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Was suchst du mir dich zu verstecken!", "tokens": ["Was", "suchst", "du", "mir", "dich", "zu", "ver\u00b7ste\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PPER", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sei offen, flieh nicht meinen Blick!", "tokens": ["Sei", "of\u00b7fen", ",", "flieh", "nicht", "mei\u00b7nen", "Blick", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VVFIN", "PTKNEG", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fr\u00fch oder sp\u00e4t mu\u00dft ich's entdecken,", "tokens": ["Fr\u00fch", "o\u00b7der", "sp\u00e4t", "mu\u00dft", "ich's", "ent\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VMFIN", "PIS", "VVINF", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Und hier hast du dein Wort zur\u00fcck.", "tokens": ["Und", "hier", "hast", "du", "dein", "Wort", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Was ich gesollt, hab ich vollendet;", "tokens": ["Was", "ich", "ge\u00b7sollt", ",", "hab", "ich", "voll\u00b7en\u00b7det", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch mich sei dir von nun an nichts verwehrt;", "tokens": ["Durch", "mich", "sei", "dir", "von", "nun", "an", "nichts", "ver\u00b7wehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPER", "APPR", "ADV", "APPR", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Allein verzeih dem Freund, der sich nun von dir wendet", "tokens": ["Al\u00b7lein", "ver\u00b7zeih", "dem", "Freund", ",", "der", "sich", "nun", "von", "dir", "wen\u00b7det"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "PRF", "ADV", "APPR", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und still in sich zur\u00fccke kehrt.", "tokens": ["Und", "still", "in", "sich", "zu\u00b7r\u00fc\u00b7cke", "kehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Zu liebli