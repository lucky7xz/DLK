{"textgrid.poem.42359": {"metadata": {"author": {"name": "Lachmann, Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Diesseits das Ufer und der Strom,", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Diesseits das Ufer und der Strom,", "tokens": ["Dies\u00b7seits", "das", "U\u00b7fer", "und", "der", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Lichtgr\u00fcn ein Rasen am klaren Strand.", "tokens": ["Licht\u00b7gr\u00fcn", "ein", "Ra\u00b7sen", "am", "kla\u00b7ren", "Strand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Der hohe, strahlende Himmelsdom", "tokens": ["Der", "ho\u00b7he", ",", "strah\u00b7len\u00b7de", "Him\u00b7mels\u00b7dom"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "\u00dcber H\u00fcgel und W\u00e4lder hingespannt.", "tokens": ["\u00dc\u00b7ber", "H\u00fc\u00b7gel", "und", "W\u00e4l\u00b7der", "hin\u00b7ge\u00b7spannt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "--+--+-+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Jenseits verhohlen und abgewandt,", "tokens": ["Jen\u00b7seits", "ver\u00b7hoh\u00b7len", "und", "ab\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVINF", "KON", "VVPP", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Wie die schattenfl\u00fcchtige Seele vom Leib,