ine.1": {"text": "Krieg denn, Krieg!", "tokens": ["Krieg", "denn", ",", "Krieg", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Roter Hahn, flieg!", "tokens": ["Ro\u00b7ter", "Hahn", ",", "flieg", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Flieg \u00fcber die Schl\u00f6sser all,", "tokens": ["Flieg", "\u00fc\u00b7ber", "die", "Schl\u00f6s\u00b7ser", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "PIAT", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Schwing die Fl\u00fcgel und kr\u00e4he!", "tokens": ["Schwing", "die", "Fl\u00fc\u00b7gel", "und", "kr\u00e4\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "KON", "VVFIN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Niemand ackre, niemand s\u00e4e,", "tokens": ["Nie\u00b7mand", "ac\u00b7kre", ",", "nie\u00b7mand", "s\u00e4e", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00d6d sei Scheuer, Hof und Stall.", "tokens": ["\u00d6d", "sei", "Scheu\u00b7er", ",", "Hof", "und", "Stall", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sengt und brennt,", "tokens": ["Sengt", "und", "brennt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Was ihr k\u00f6nnt!