{"dta.poem.16583": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "244.  Die Liebe ist der weisen Stein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Lied\u2019 ist der weisen Stein: sie scheidet Gold au\u00df koth/", "tokens": ["Lied'", "ist", "der", "wei\u00b7sen", "Stein", ":", "sie", "schei\u00b7det", "Gold", "au\u00df", "koth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie machet nichts zu ichts/ und wandelt mich in GOtt.", "tokens": ["Sie", "ma\u00b7chet", "nichts", "zu", "ichts", "/", "und", "wan\u00b7delt", "mich", "in", "Gott", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PIS", "$(", "KON", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}