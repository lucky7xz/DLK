{"dta.poem.17941": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "184.  Alles verdirbt und was wir nit seind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Christ werde was du suchst: wo du \u2019s nicht selber bist/", "tokens": ["Christ", "wer\u00b7de", "was", "du", "suchst", ":", "wo", "du", "'s", "nicht", "sel\u00b7ber", "bist", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PWS", "PPER", "VVFIN", "$.", "PWAV", "PPER", "PPER", "PTKNEG", "ADV", "VAFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "So komstu nie zur Ruh/ und wird dir alls zu Mist.", "tokens": ["So", "koms\u00b7tu", "nie", "zur", "Ruh", "/", "und", "wird", "dir", "alls", "zu", "Mist", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "APPRART", "NN", "$(", "KON", "VAFIN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}