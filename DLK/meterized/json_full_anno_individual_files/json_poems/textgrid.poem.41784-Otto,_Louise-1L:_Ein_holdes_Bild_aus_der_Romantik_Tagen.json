{"textgrid.poem.41784": {"metadata": {"author": {"name": "Otto, Louise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein holdes Bild aus der Romantik Tagen", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein holdes Bild aus der Romantik Tagen", "tokens": ["Ein", "hol\u00b7des", "Bild", "aus", "der", "Ro\u00b7man\u00b7tik", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will noch einmal sich uns verk\u00f6rpert zeigen,", "tokens": ["Will", "noch", "ein\u00b7mal", "sich", "uns", "ver\u00b7k\u00f6r\u00b7pert", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ADV", "PRF", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Aus stillen Wasser holde Bl\u00fcten steigen", "tokens": ["Aus", "stil\u00b7len", "Was\u00b7ser", "hol\u00b7de", "Bl\u00fc\u00b7ten", "stei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und schwimmen, von den Wellen sanft getragen.", "tokens": ["Und", "schwim\u00b7men", ",", "von", "den", "Wel\u00b7len", "sanft", "ge\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "APPR", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Inmitten sehen wir die Holde ragen,", "tokens": ["In\u00b7mit\u00b7ten", "se\u00b7hen", "wir", "die", "Hol\u00b7de", "ra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als K\u00f6nigin erkannt in diesen Reichen,", "tokens": ["Als", "K\u00f6\u00b7ni\u00b7gin", "er\u00b7kannt", "in", "die\u00b7sen", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor der sich freudig alle Nymphen neigen", "tokens": ["Vor", "der", "sich", "freu\u00b7dig", "al\u00b7le", "Nym\u00b7phen", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELS", "PRF", "ADJD", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wenn sie das Aug' zum Lichte aufgeschlagen.", "tokens": ["Wenn", "sie", "das", "Aug'", "zum", "Lich\u00b7te", "auf\u00b7ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "In blendend Wei\u00df mit rotem Saum gekleidet,", "tokens": ["In", "blen\u00b7dend", "Wei\u00df", "mit", "ro\u00b7tem", "Saum", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So wiegt sie sich auf ihrem gr\u00fcnen Throne,", "tokens": ["So", "wiegt", "sie", "sich", "auf", "ih\u00b7rem", "gr\u00fc\u00b7nen", "Thro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und goldne Fischlein spielend sie umwerben.", "tokens": ["Und", "gold\u00b7ne", "Fisc\u00b7hlein", "spie\u00b7lend", "sie", "um\u00b7wer\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ADJD", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch wenn sie wieder von dem Throne scheidet,", "tokens": ["Doch", "wenn", "sie", "wie\u00b7der", "von", "dem", "Thro\u00b7ne", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dann taucht sie nieder, wie das Haupt die Krone,", "tokens": ["Dann", "taucht", "sie", "nie\u00b7der", ",", "wie", "das", "Haupt", "die", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PWAV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Um unterm Bl\u00e4tterschilde still zu sterben.", "tokens": ["Um", "un\u00b7term", "Bl\u00e4t\u00b7ter\u00b7schil\u00b7de", "still", "zu", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "APPRART", "NN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ein holdes Bild aus der Romantik Tagen", "tokens": ["Ein", "hol\u00b7des", "Bild", "aus", "der", "Ro\u00b7man\u00b7tik", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Will noch einmal sich uns verk\u00f6rper