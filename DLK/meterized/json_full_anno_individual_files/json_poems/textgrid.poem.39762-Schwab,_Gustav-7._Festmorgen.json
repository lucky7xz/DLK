{"textgrid.poem.39762": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "7. Festmorgen", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Singen m\u00f6cht' ich Liederweisen,", "tokens": ["Sin\u00b7gen", "m\u00f6cht'", "ich", "Lie\u00b7der\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Herrgott m\u00f6cht' ich preisen,", "tokens": ["Mei\u00b7nen", "Herr\u00b7gott", "m\u00f6cht'", "ich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In dem Tempel m\u00f6cht' ich stehn.", "tokens": ["In", "dem", "Tem\u00b7pel", "m\u00f6cht'", "ich", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und doch l\u00e4\u00dft sich in die Runde,", "tokens": ["Und", "doch", "l\u00e4\u00dft", "sich", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf den Umkreis einer Stunde,", "tokens": ["Auf", "den", "Um\u00b7kreis", "ei\u00b7ner", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Nichts als diese Schenke sehn.", "tokens": ["Nichts", "als", "die\u00b7se", "Schen\u00b7ke", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "PDAT", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Werde sie mir denn zur Klause,", "tokens": ["Wer\u00b7de", "sie", "mir", "denn", "zur", "Klau\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Werde sie zum Gotteshause!", "tokens": ["Wer\u00b7de", "sie", "zum", "Got\u00b7tes\u00b7hau\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche Stelle predigt nicht?", "tokens": ["Wel\u00b7che", "Stel\u00b7le", "pre\u00b7digt", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wo sich ernster Sinn erweitert,", "tokens": ["Wo", "sich", "erns\u00b7ter", "Sinn", "er\u00b7wei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich mit Himmelslichte heitert,", "tokens": ["Sich", "mit", "Him\u00b7mels\u00b7lich\u00b7te", "hei\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fehlt Altar und Kanzel nicht.", "tokens": ["Fehlt", "Al\u00b7tar", "und", "Kan\u00b7zel", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "KON", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Warum sollt' ich mich besinnen \u2013", "tokens": ["Wa\u00b7rum", "sollt'", "ich", "mich", "be\u00b7sin\u00b7nen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Horch! wie l\u00e4rmt es schon da drinnen!", "tokens": ["Horch", "!", "wie", "l\u00e4rmt", "es", "schon", "da", "drin\u00b7nen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwarz von Bauern sitzt die Bank.", "tokens": ["Schwarz", "von", "Bau\u00b7ern", "sitzt", "die", "Bank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und was h\u00f6r' ich! sich zur Plage", "tokens": ["Und", "was", "h\u00f6r'", "ich", "!", "sich", "zur", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht dies Volk die Feiertage,", "tokens": ["Macht", "dies", "Volk", "die", "Fei\u00b7er\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Glaube wird ein Zank.", "tokens": ["Und", "der", "Glau\u00b7be", "wird", "ein", "Zank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Wie der Lutheraner m\u00e4chtig", "tokens": ["Wie", "der", "Lu\u00b7the\u00b7ra\u00b7ner", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Demonstrirt, wie er bed\u00e4chtig", "tokens": ["De\u00b7mons\u00b7trirt", ",", "wie", "er", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spruch um Spruch zu Schl\u00fcssel sucht!", "tokens": ["Spruch", "um", "Spruch", "zu", "Schl\u00fcs\u00b7sel", "sucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Katholik ihm knurrend", "tokens": ["Wie", "der", "Ka\u00b7tho\u00b7lik", "ihm", "knur\u00b7rend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "PPER", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ausweicht, und verdrie\u00dflich murrend", "tokens": ["Aus\u00b7weicht", ",", "und", "ver\u00b7drie\u00df\u00b7lich", "mur\u00b7rend"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "KON", "ADJD", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ketzer in die H\u00f6lle flucht!", "tokens": ["Ket\u00b7zer", "in", "die", "H\u00f6l\u00b7le", "flucht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nein! hier kann ich auch nicht beten,", "tokens": ["Nein", "!", "hier", "kann", "ich", "auch", "nicht", "be\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ADV", "VMFIN", "PPER", "ADV", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mu\u00df verstimmt bei Seite treten.", "tokens": ["Mu\u00df", "ver\u00b7stimmt", "bei", "Sei\u00b7te", "tre\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Den im Winkel sprech' ich an,", "tokens": ["Den", "im", "Win\u00b7kel", "sprech'", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Der vom ganzen Streit nichts h\u00f6rte,", "tokens": ["Der", "vom", "gan\u00b7zen", "Streit", "nichts", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der nur Augen, ungest\u00f6rte,", "tokens": ["Der", "nur", "Au\u00b7gen", ",", "un\u00b7ge\u00b7st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heftet auf den Korduan.", "tokens": ["Hef\u00b7tet", "auf", "den", "Kor\u00b7du\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Hand auf Schulter, bessern Mutes", "tokens": ["Hand", "auf", "Schul\u00b7ter", ",", "bes\u00b7sern", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprech' ich: \u00bbChrist, was liesest Gutes?\u00ab", "tokens": ["Sprech'", "ich", ":", "\u00bb", "Christ", ",", "was", "lie\u00b7sest", "Gu\u00b7tes", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$.", "$(", "NN", "$,", "PWS", "VVFIN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich schau' ihm in den Text. \u2013", "tokens": ["Und", "ich", "schau'", "ihm", "in", "den", "Text", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist dies Haus nicht europ\u00e4isch?", "tokens": ["Ist", "dies", "Haus", "nicht", "eu\u00b7ro\u00b7p\u00e4\u00b7isch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Welch ein D\u00e4mon hat Hebr\u00e4isch", "tokens": ["Welch", "ein", "D\u00e4\u00b7mon", "hat", "Heb\u00b7r\u00e4\u00b7isch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VAFIN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Auf das Psalmbuch hingehext?", "tokens": ["Auf", "das", "Psalm\u00b7buch", "hin\u00b7ge\u00b7hext", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bin ich im gelobten Lande? \u2013", "tokens": ["Bin", "ich", "im", "ge\u00b7lob\u00b7ten", "Lan\u00b7de", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, gesteh zu deiner Schande,", "tokens": ["Herz", ",", "ge\u00b7steh", "zu", "dei\u00b7ner", "Schan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem ", "tokens": ["Vor", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Hei\u00df' den Bruder doch willkommen,", "tokens": ["Hei\u00df'", "den", "Bru\u00b7der", "doch", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freue dich mit diesem Frommen,", "tokens": ["Freu\u00b7e", "dich", "mit", "die\u00b7sem", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halte mit ihm Sabbathsruh!", "tokens": ["Hal\u00b7te", "mit", "ihm", "Sab\u00b7baths\u00b7ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Singen m\u00f6cht' ich Liederweisen,", "tokens": ["Sin\u00b7gen", "m\u00f6cht'", "ich", "Lie\u00b7der\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Herrgott m\u00f6cht' ich preisen,", "tokens": ["Mei\u00b7nen", "Herr\u00b7gott", "m\u00f6cht'", "ich", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In d