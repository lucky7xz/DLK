 "punct"], "pos": ["ART", "APPRART", "ADJA", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Der nur Augen, ungest\u00f6rte,", "tokens": ["Der", "nur", "Au\u00b7gen", ",", "un\u00b7ge\u00b7st\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADV", "NN", "$,", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Heftet auf den Korduan.", "tokens": ["Hef\u00b7tet", "auf", "den", "Kor\u00b7du\u00b7an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Hand auf Schulter, bessern Mutes", "tokens": ["Hand", "auf", "Schul\u00b7ter", ",", "bes\u00b7sern", "Mu\u00b7tes"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "APPR", "NN", "$,", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprech' ich: \u00bbChrist, was liesest Gutes?\u00ab", "tokens": ["Sprech'", "ich", ":", "\u00bb", "Christ", ",", "was", "lie\u00b7sest", "Gu\u00b7tes", "?", "\u00ab"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVIMP", "PPER", "$.", "$(", "NN", "$,", "PWS", "VVFIN", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ich schau' ihm in den Text. \u2013", "tokens": ["Und", "ich", "schau'", "ihm", "in", "den", "Text", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist dies Haus nicht europ\u00e4isch?", "tokens": ["Ist", "dies", "Haus", "nicht", "eu\u00b7ro\u00b7p\u00e4\u00b7isch", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDS", "NN", "PTKNEG", "ADJD", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.5": {"text": "Welch ein D\u00e4mon hat Hebr\u00e4isch", "tokens": ["Welch", "ein", "D\u00e4\u00b7mon", "hat", "Heb\u00b7r\u00e4\u00b7isch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "NN", "VAFIN", "NE"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Auf das Psalmbuch hingehext?", "tokens": ["Auf", "das", "Psalm\u00b7buch", "hin\u00b7ge\u00b7hext", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Bin ich im gelobten Lande? \u2013", "tokens": ["Bin", "ich", "im", "ge\u00b7lob\u00b7ten", "Lan\u00b7de", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Herz, gesteh zu deiner Schande,", "tokens": ["Herz", ",", "ge\u00b7steh", "zu", "dei\u00b7ner", "Schan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vor dem ", "tokens": ["Vor", "dem"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "Hei\u00df' den Bruder doch willkommen,", "tokens": ["Hei\u00df'", "den", "Bru\u00b7der", "doch", "will\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Freue dich mit diesem Frommen,", "tokens": ["Freu\u00b7e", "dich", "mit", "die\u00b7sem", "From\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Halte mit ihm Sabbathsruh!", "tokens": ["Hal\u00b7te", "mit", "ihm", "Sab\u00b7baths\u00b7ruh", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}