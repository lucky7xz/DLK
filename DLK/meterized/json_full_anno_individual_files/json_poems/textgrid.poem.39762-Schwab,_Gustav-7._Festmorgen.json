-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und was h\u00f6r' ich! sich zur Plage", "tokens": ["Und", "was", "h\u00f6r'", "ich", "!", "sich", "zur", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PWS", "VVFIN", "PPER", "$.", "PRF", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Macht dies Volk die Feiertage,", "tokens": ["Macht", "dies", "Volk", "die", "Fei\u00b7er\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und der Glaube wird ein Zank.", "tokens": ["Und", "der", "Glau\u00b7be", "wird", "ein", "Zank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Wie der Lutheraner m\u00e4chtig", "tokens": ["Wie", "der", "Lu\u00b7the\u00b7ra\u00b7ner", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Demonstrirt, wie er bed\u00e4chtig", "tokens": ["De\u00b7mons\u00b7trirt", ",", "wie", "er", "be\u00b7d\u00e4ch\u00b7tig"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVPP", "$,", "PWAV", "PPER", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spruch um Spruch zu Schl\u00fcssel sucht!", "tokens": ["Spruch", "um", "Spruch", "zu", "Schl\u00fcs\u00b7sel", "sucht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Katholik ihm knurrend", "tokens": ["Wie", "der", "Ka\u00b7tho\u00b7lik", "ihm", "k