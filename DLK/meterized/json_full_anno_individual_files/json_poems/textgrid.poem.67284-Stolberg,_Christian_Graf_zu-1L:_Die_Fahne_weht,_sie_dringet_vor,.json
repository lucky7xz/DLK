{"textgrid.poem.67284": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Fahne weht, sie dringet vor,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Fahne weht, sie dringet vor,", "tokens": ["Die", "Fah\u00b7ne", "weht", ",", "sie", "drin\u00b7get", "vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entfaltend ihren Glanz,", "tokens": ["Ent\u00b7fal\u00b7tend", "ih\u00b7ren", "Glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Gru\u00df und Lied wallt ihr empor", "tokens": ["Und", "Gru\u00df", "und", "Lied", "wallt", "ihr", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei'm frohen Waffentanz.", "tokens": ["Bei'm", "fro\u00b7hen", "Waf\u00b7fen\u00b7tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sie strahlt, des freien Bund's Panier,", "tokens": ["Sie", "strahlt", ",", "des", "frei\u00b7en", "Bun\u00b7d's", "Pa\u00b7nier", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Im sch\u00f6nsten Morgenroth;", "tokens": ["Im", "sch\u00f6ns\u00b7ten", "Mor\u00b7gen\u00b7roth", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir schaun auf dich, wir folgen dir,", "tokens": ["Wir", "schaun", "auf", "dich", ",", "wir", "fol\u00b7gen", "dir", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Sieg, zu Heldentod!", "tokens": ["Zu", "Sieg", ",", "zu", "Hel\u00b7den\u00b7tod", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ein k\u00f6stlich Weihgeschenk, gewebt", "tokens": ["Ein", "k\u00f6st\u00b7lich", "Weih\u00b7ge\u00b7schenk", ",", "ge\u00b7webt"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "NN", "$,", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von edler Frauen Hand,", "tokens": ["Von", "ed\u00b7ler", "Frau\u00b7en", "Hand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von ihrer W\u00fcnsche Chor umschwebt,", "tokens": ["Von", "ih\u00b7rer", "W\u00fcn\u00b7sche", "Chor", "um\u00b7schwebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr's Deutsche Vaterland.", "tokens": ["F\u00fcr's", "Deut\u00b7sche", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Gehobnen Busens, Perl' im Blick,", "tokens": ["Ge\u00b7hob\u00b7nen", "Bu\u00b7sens", ",", "Perl'", "im", "Blick", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So trug die fromme Schaar,", "tokens": ["So", "trug", "die", "from\u00b7me", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df kr\u00f6ne Segen ihr Geschick,", "tokens": ["Da\u00df", "kr\u00f6\u00b7ne", "Se\u00b7gen", "ihr", "Ge\u00b7schick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie hin zum Hochaltar.", "tokens": ["Sie", "hin", "zum", "Hoc\u00b7hal\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Ein heil'ger Priester sprach ihn aus,", "tokens": ["Ein", "heil'\u00b7ger", "Pries\u00b7ter", "sprach", "ihn", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Weihspruch \u00fcber sie,", "tokens": ["Den", "Weih\u00b7spruch", "\u00fc\u00b7ber", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Amen! scholl das Gotteshaus", "tokens": ["Und", "A\u00b7men", "!", "scholl", "das", "Got\u00b7tes\u00b7haus"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "NN", "$.", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles st\u00fcrzt' auf's Knie.", "tokens": ["Und", "Al\u00b7les", "st\u00fcrzt'", "auf's", "Knie", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Der Reigen zog vor's Kirchenthor,", "tokens": ["Der", "Rei\u00b7gen", "zog", "vor's", "Kir\u00b7chen\u00b7thor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir standen harrend da,", "tokens": ["Wir", "stan\u00b7den", "har\u00b7rend", "da", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und flehend Aller Aug' empor", "tokens": ["Und", "fle\u00b7hend", "Al\u00b7ler", "Aug'", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu Fahn' und Himmel sah.", "tokens": ["Zu", "Fahn'", "und", "Him\u00b7mel", "sah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Ein holdes Weib, mit edler Schaam", "tokens": ["Ein", "hol\u00b7des", "Weib", ",", "mit", "ed\u00b7ler", "Schaam"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An Wang' und Stirn geschm\u00fcckt,", "tokens": ["An", "Wang'", "und", "Stirn", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Trat sittsam vor, das Wort sie nahm,", "tokens": ["Trat", "sitt\u00b7sam", "vor", ",", "das", "Wort", "sie", "nahm", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PTKVZ", "$,", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In Andachtsgluth entz\u00fcckt:", "tokens": ["In", "An\u00b7dachts\u00b7gluth", "ent\u00b7z\u00fcckt", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbdringt, Br\u00fcder, wo sie wallt, in's Herz", "tokens": ["\u00bb", "dringt", ",", "Br\u00fc\u00b7der", ",", "wo", "sie", "wallt", ",", "in's", "Herz"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "VVFIN", "$,", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dem dichten Feindesschwarm!", "tokens": ["Dem", "dich\u00b7ten", "Fein\u00b7des\u00b7schwarm", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wenn ernster Will' ihn hebt \u2013 ist Scherz", "tokens": ["Wenn", "erns\u00b7ter", "Will'", "ihn", "hebt", "\u2013", "ist", "Scherz"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVFIN", "$(", "VAFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Sieg dem deutschen Arm!", "tokens": ["Der", "Sieg", "dem", "deut\u00b7schen", "Arm", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Seyd L\u00f6wen in des Treffens Graun;", "tokens": ["Seyd", "L\u00f6\u00b7wen", "in", "des", "Tref\u00b7fens", "Graun", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "NN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch schweb' ein Bild euch vor", "tokens": ["Doch", "schweb'", "ein", "Bild", "euch", "vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "APPR"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Von Braut und Schwester, das euch raun'", "tokens": ["Von", "Braut", "und", "Schwes\u00b7ter", ",", "das", "euch", "raun'"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erweichten Sinn in's Ohr.", "tokens": ["Er\u00b7weich\u00b7ten", "Sinn", "in's", "Ohr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nun Gott mit dir, du hehre Schaar!", "tokens": ["Nun", "Gott", "mit", "dir", ",", "du", "heh\u00b7re", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr Weib und Kind und Heerd,", "tokens": ["F\u00fcr", "Weib", "und", "Kind", "und", "Heerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "F\u00fcr Recht, f\u00fcr Freiheit, f\u00fcr Altar", "tokens": ["F\u00fcr", "Recht", ",", "f\u00fcr", "Frei\u00b7heit", ",", "f\u00fcr", "Al\u00b7tar"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schwingst du das R\u00e4cherschwert!\u00ab", "tokens": ["Schwingst", "du", "das", "R\u00e4\u00b7cher\u00b7schwert", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sprach's mit verkl\u00e4rtem Aug'. \u2013 Es strebt", "tokens": ["Sprach's", "mit", "ver\u00b7kl\u00e4r\u00b7tem", "Aug'", ".", "\u2013", "Es", "strebt"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das Kleinod nun voran,", "tokens": ["Das", "Klei\u00b7nod", "nun", "vo\u00b7ran", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und felsenfester Glaub' erhebt", "tokens": ["Und", "fel\u00b7sen\u00b7fes\u00b7ter", "Glaub'", "er\u00b7hebt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Muth uns himmelan.", "tokens": ["Den", "Muth", "uns", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Die ernste Stunde schl\u00e4gt; wir gehn", "tokens": ["Die", "erns\u00b7te", "Stun\u00b7de", "schl\u00e4gt", ";", "wir", "gehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In Sieg, in Heldentod,", "tokens": ["In", "Sieg", ",", "in", "Hel\u00b7den\u00b7tod", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Laut schallt's im Thal und auf den H\u00f6h'n:", "tokens": ["Laut", "schallt's", "im", "Thal", "und", "auf", "den", "H\u00f6h'n", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPRART", "NN", "KON", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.13": {"line.1": {"text": "Die Fahne weht, sie dringet vor,", "tokens": ["Die", "Fah\u00b7ne", "weht", ",", "sie", "drin\u00b7get", "vor", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entfaltend ihren Glanz,", "tokens": ["Ent\u00b7fal\u00b7tend", "ih\u00b7ren", "Glanz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und Gru\u00df und Lied wallt ihr empor", "tokens": ["Und", "Gru\u00df", "und", "Lied", "wallt", "ihr", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bei'm frohen Waffentanz.", "tokens": ["Bei'm", "fro\u00b7hen", "Waf\u00b7fen\u00b7tanz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "Sie strahlt, des freien Bund's Panier,", "tokens": ["Sie", "strahlt", ",", "des", "frei\u00b7en", "Bun\u00b7d's", "P