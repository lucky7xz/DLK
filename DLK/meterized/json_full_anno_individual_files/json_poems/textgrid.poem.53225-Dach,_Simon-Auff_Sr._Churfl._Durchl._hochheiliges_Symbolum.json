asure": "trochaic.tri"}, "line.6": {"text": "Und gef\u00e4hrlich irre,", "tokens": ["Und", "ge\u00b7f\u00e4hr\u00b7lich", "ir\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie ein Schiff, das weder Raht,", "tokens": ["Wie", "ein", "Schiff", ",", "das", "we\u00b7der", "Raht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch Compas, noch Ruder hat,", "tokens": ["Noch", "Com\u00b7pas", ",", "noch", "Ru\u00b7der", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ADV", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.14": {"line.1": {"text": "Hie legt Zorn mir tausend Netze", "tokens": ["Hie", "legt", "Zorn", "mir", "tau\u00b7send", "Net\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da Gewalt und Eigen-Sinn,", "tokens": ["Da", "Ge\u00b7walt", "und", "Ei\u00b7gen\u00b7Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ihm selber stelt Gesetze,", "tokens": ["Der", "ihm", "sel\u00b7ber", "stelt", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wirfft deine Satzung hin;", "tokens": ["Und", "wirfft", "dei\u00b7ne", "Sat\u00b7zung", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da wil Wollust leiten", "tokens": ["Da", "wil", "Wol\u00b7lust", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mich auff b\u00f6se Seiten;", "tokens": ["Mich", "auff", "b\u00f6\u00b7se", "Sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und was t\u00fcckisch auff mich h\u00e4lt,", "tokens": ["Und", "was", "t\u00fc\u00b7ckisch", "auff", "mich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist vorau\u00df die b\u00f6se Welt.", "tokens": ["Ist", "vor\u00b7au\u00df", "die", "b\u00f6\u00b7se", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Aller Weg geht in die Helle,", "tokens": ["Al\u00b7ler", "Weg", "geht", "in", "die", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Gefahr und Todt bewacht:", "tokens": ["Den", "Ge\u00b7fahr", "und", "Todt", "be\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey mein trewer Spie\u00df-Geselle,", "tokens": ["Sey", "mein", "tre\u00b7wer", "Spie\u00df\u00b7Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchr mich durch die finstre Nacht,", "tokens": ["F\u00fchr", "mich", "durch", "die", "finst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich nichts bewegen", "tokens": ["La\u00df", "mich", "nichts", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIS", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Weder Sturm, noch Regen,", "tokens": ["We\u00b7der", "Sturm", ",", "noch", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sey mein Leit-Stern, sey mein Gang,", "tokens": ["Sey", "mein", "Leit\u00b7S\u00b7tern", ",", "sey", "mein", "Gang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Meiner Schritt und Tritte Zwang.", "tokens": ["Mei\u00b7ner", "Schritt", "und", "Trit\u00b7te", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.16": {"line.1": {"text": "Jesu! der du mich wol kennest,", "tokens": ["Je\u00b7su", "!", "der", "du", "mich", "wol", "ken\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dich selbst in deinem Wort", "tokens": ["Und", "dich", "selbst", "in", "dei\u00b7nem", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warheit, Weg und Leben nennest,", "tokens": ["War\u00b7heit", ",", "Weg", "und", "Le\u00b7ben", "nen\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilff mir armen Pilgrim fort,", "tokens": ["Hilff", "mir", "ar\u00b7men", "Pil\u00b7grim", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mach mein gantzes Leben", "tokens": ["Mach", "mein", "gant\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Deinem Wandel eben,", "tokens": ["Dei\u00b7nem", "Wan\u00b7del", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df ich bleibe f\u00fcr und f\u00fcr", "tokens": ["Da\u00df", "ich", "blei\u00b7be", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem rechten Wege, dir.", "tokens": ["In", "dem", "rech\u00b7ten", "We\u00b7ge", ",", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.17": {"lin