{"textgrid.poem.53225": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "Auff Sr. Churfl. Durchl. hochheiliges Symbolum", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr der liechten Seraphinen!", "tokens": ["Herr", "der", "liech\u00b7ten", "Se\u00b7ra\u00b7phi\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dem die Cronen aller Welt,", "tokens": ["Dem", "die", "Cro\u00b7nen", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Scepter m\u00fcssen dienen,", "tokens": ["Al\u00b7le", "Scep\u00b7ter", "m\u00fcs\u00b7sen", "die\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Deiner starcken Helden Held!", "tokens": ["Dei\u00b7ner", "star\u00b7cken", "Hel\u00b7den", "Held", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gn\u00e4dig, Ewig, pr\u00e4chtig,", "tokens": ["Gn\u00e4\u00b7dig", ",", "E\u00b7wig", ",", "pr\u00e4ch\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Allwei\u00df, heilig, m\u00e4chtig,", "tokens": ["All\u00b7wei\u00df", ",", "hei\u00b7lig", ",", "m\u00e4ch\u00b7tig", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Der Ihm stracks zu einem Heer", "tokens": ["Der", "Ihm", "stracks", "zu", "ei\u00b7nem", "Heer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Auffbringt Himmel, Erd' und Meer!", "tokens": ["Auff\u00b7bringt", "Him\u00b7mel", ",", "Erd'", "und", "Meer", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Was ich hab an Macht auff Erden,", "tokens": ["Was", "ich", "hab", "an", "Macht", "auff", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VAFIN", "APPR", "NN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gott, ist deine Gnad allein,", "tokens": ["Gott", ",", "ist", "dei\u00b7ne", "Gnad", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn du l\u00e4ssest deiner Herden", "tokens": ["Denn", "du", "l\u00e4s\u00b7sest", "dei\u00b7ner", "Her\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich nur einen Hirten seyn,", "tokens": ["Mich", "nur", "ei\u00b7nen", "Hir\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich bester massen", "tokens": ["La\u00df", "mich", "bes\u00b7ter", "mas\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Sie in Auffsicht fassen,", "tokens": ["Sie", "in", "Auff\u00b7sicht", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und in stets genawe Hutt", "tokens": ["Und", "in", "stets", "ge\u00b7na\u00b7we", "Hutt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADV", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Eines jeden Gutt und Blutt.", "tokens": ["Ei\u00b7nes", "je\u00b7den", "Gutt", "und", "Blutt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Thue mir kundt den Weg f\u00fcr allen,", "tokens": ["Thue", "mir", "kundt", "den", "Weg", "f\u00fcr", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "ART", "NN", "APPR", "PIAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den ich allzeit wandlen sol,", "tokens": ["Den", "ich", "all\u00b7zeit", "wand\u00b7len", "sol", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADV", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "La\u00df mein Leben dir gefallen,", "tokens": ["La\u00df", "mein", "Le\u00b7ben", "dir", "ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mach mich deines Geistes voll,", "tokens": ["Mach", "mich", "dei\u00b7nes", "Geis\u00b7tes", "voll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Leucht' in meinem Hertzen", "tokens": ["Leucht'", "in", "mei\u00b7nem", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Durch der Wei\u00dfheit Kertzen,", "tokens": ["Durch", "der", "Wei\u00df\u00b7heit", "Kert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Denn ohn deines Wordtes Licht", "tokens": ["Denn", "ohn", "dei\u00b7nes", "Word\u00b7tes", "Licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Find ich deinen Richt-steig nicht.", "tokens": ["Find", "ich", "dei\u00b7nen", "Richt\u00b7steig", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Sathan suchet mich zu blenden,", "tokens": ["Sa\u00b7than", "su\u00b7chet", "mich", "zu", "blen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meinen Sinn, Verstandt und Wahn", "tokens": ["Mei\u00b7nen", "Sinn", ",", "Ver\u00b7standt", "und", "Wahn"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Einig von dir abzuwenden,", "tokens": ["Ei\u00b7nig", "von", "dir", "ab\u00b7zu\u00b7wen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "VVIZU", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df ich fehle deiner Bahn,", "tokens": ["Da\u00df", "ich", "feh\u00b7le", "dei\u00b7ner", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mich in mich verwirre,", "tokens": ["Mich", "in", "mich", "ver\u00b7wir\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Und gef\u00e4hrlich irre,", "tokens": ["Und", "ge\u00b7f\u00e4hr\u00b7lich", "ir\u00b7re", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Wie ein Schiff, das weder Raht,", "tokens": ["Wie", "ein", "Schiff", ",", "das", "we\u00b7der", "Raht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Noch Compas, noch Ruder hat,", "tokens": ["Noch", "Com\u00b7pas", ",", "noch", "Ru\u00b7der", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NE", "$,", "ADV", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Hie legt Zorn mir tausend Netze", "tokens": ["Hie", "legt", "Zorn", "mir", "tau\u00b7send", "Net\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "PPER", "CARD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da Gewalt und Eigen-Sinn,", "tokens": ["Da", "Ge\u00b7walt", "und", "Ei\u00b7gen\u00b7Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der ihm selber stelt Gesetze,", "tokens": ["Der", "ihm", "sel\u00b7ber", "stelt", "Ge\u00b7set\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVFIN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und wirfft deine Satzung hin;", "tokens": ["Und", "wirfft", "dei\u00b7ne", "Sat\u00b7zung", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Da wil Wollust leiten", "tokens": ["Da", "wil", "Wol\u00b7lust", "lei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Mich auff b\u00f6se Seiten;", "tokens": ["Mich", "auff", "b\u00f6\u00b7se", "Sei\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Und was t\u00fcckisch auff mich h\u00e4lt,", "tokens": ["Und", "was", "t\u00fc\u00b7ckisch", "auff", "mich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ADJD", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Ist vorau\u00df die b\u00f6se Welt.", "tokens": ["Ist", "vor\u00b7au\u00df", "die", "b\u00f6\u00b7se", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Aller Weg geht in die Helle,", "tokens": ["Al\u00b7ler", "Weg", "geht", "in", "die", "Hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den Gefahr und Todt bewacht:", "tokens": ["Den", "Ge\u00b7fahr", "und", "Todt", "be\u00b7wacht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sey mein trewer Spie\u00df-Geselle,", "tokens": ["Sey", "mein", "tre\u00b7wer", "Spie\u00df\u00b7Ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "F\u00fchr mich durch die finstre Nacht,", "tokens": ["F\u00fchr", "mich", "durch", "die", "finst\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "La\u00df mich nichts bewegen", "tokens": ["La\u00df", "mich", "nichts", "be\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "PIS", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Weder Sturm, noch Regen,", "tokens": ["We\u00b7der", "Sturm", ",", "noch", "Re\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "ADV", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Sey mein Leit-Stern, sey mein Gang,", "tokens": ["Sey", "mein", "Leit\u00b7S\u00b7tern", ",", "sey", "mein", "Gang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Meiner Schritt und Tritte Zwang.", "tokens": ["Mei\u00b7ner", "Schritt", "und", "Trit\u00b7te", "Zwang", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Jesu! der du mich wol kennest,", "tokens": ["Je\u00b7su", "!", "der", "du", "mich", "wol", "ken\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ART", "PPER", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dich selbst in deinem Wort", "tokens": ["Und", "dich", "selbst", "in", "dei\u00b7nem", "Wort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Warheit, Weg und Leben nennest,", "tokens": ["War\u00b7heit", ",", "Weg", "und", "Le\u00b7ben", "nen\u00b7nest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hilff mir armen Pilgrim fort,", "tokens": ["Hilff", "mir", "ar\u00b7men", "Pil\u00b7grim", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mach mein gantzes Leben", "tokens": ["Mach", "mein", "gant\u00b7zes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Deinem Wandel eben,", "tokens": ["Dei\u00b7nem", "Wan\u00b7del", "e\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Da\u00df ich bleibe f\u00fcr und f\u00fcr", "tokens": ["Da\u00df", "ich", "blei\u00b7be", "f\u00fcr", "und", "f\u00fcr"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVFIN", "APPR", "KON", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "In dem rechten Wege, dir.", "tokens": ["In", "dem", "rech\u00b7ten", "We\u00b7ge", ",", "dir", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "La\u00df mich seyn, wie du, bescheiden,", "tokens": ["La\u00df", "mich", "seyn", ",", "wie", "du", ",", "be\u00b7schei\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "VAINF", "$,", "PWAV", "PPER", "$,", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Heilig, fromm, gerecht und still,", "tokens": ["Hei\u00b7lig", ",", "fromm", ",", "ge\u00b7recht", "und", "still", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Freudig Noht und Todt zu leiden,", "tokens": ["Freu\u00b7dig", "Noht", "und", "Todt", "zu", "lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wollen was dein Vater will,", "tokens": ["Wol\u00b7len", "was", "dein", "Va\u00b7ter", "will", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "PPOSAT