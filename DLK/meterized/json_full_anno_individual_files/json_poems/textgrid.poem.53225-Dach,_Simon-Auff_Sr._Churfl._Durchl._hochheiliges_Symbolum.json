", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PPER", "ADJD", "KOKOM", "ADV", "PPER", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und f\u00fcr Erd und Himmel Dich.", "tokens": ["Und", "f\u00fcr", "Erd", "und", "Him\u00b7mel", "Dich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.18": {"line.1": {"text": "Da\u00df man mich in dir stets sp\u00fcre", "tokens": ["Da\u00df", "man", "mich", "in", "dir", "stets", "sp\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "PRF", "APPR", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und ich meist ein Hertzog sey,", "tokens": ["Und", "ich", "meist", "ein", "Hert\u00b7zog", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "ART", "NE", "VAFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Der durch dich zum Leben f\u00fchre", "tokens": ["Der", "durch", "dich", "zum", "Le\u00b7ben", "f\u00fch\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die du trawest meiner Trew,", "tokens": ["Die", "du", "tra\u00b7west", "mei\u00b7ner", "Trew", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und mir jenes Leben", "tokens": ["Und", "mir", "je\u00b7nes", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPER", "PDAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Zeugn\u00fc\u00df k\u00f6nne 