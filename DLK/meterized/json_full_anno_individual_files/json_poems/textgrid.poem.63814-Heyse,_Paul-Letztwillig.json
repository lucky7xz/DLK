{"textgrid.poem.63814": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Letztwillig", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Und schlie\u00df' ich einst die Augen zu,", "tokens": ["Und", "schlie\u00df'", "ich", "einst", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie wieder aufzuwachen,", "tokens": ["Nie", "wie\u00b7der", "auf\u00b7zu\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gilt mir's gleich, wo man zur Ruh", "tokens": ["So", "gilt", "mir's", "gleich", ",", "wo", "man", "zur", "Ruh"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NE", "ADV", "$,", "PWAV", "PIS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mir wird das Bette machen.", "tokens": ["Mir", "wird", "das", "Bet\u00b7te", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch schl\u00fcge mein letztes St\u00fcndlein hier", "tokens": ["Doch", "schl\u00fc\u00b7ge", "mein", "letz\u00b7tes", "St\u00fcnd\u00b7lein", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An dieser Seeflut Borden,", "tokens": ["An", "die\u00b7ser", "See\u00b7flut", "Bor\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So w\u00fcnscht' ich, man erlie\u00dfe mir", "tokens": ["So", "w\u00fcnscht'", "ich", ",", "man", "er\u00b7lie\u00b7\u00dfe", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIS", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Brennerfahrt gen Norden.", "tokens": ["Die", "Bren\u00b7ner\u00b7fahrt", "gen", "Nor\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Man gr\u00fcbe mir ein stilles Grab", "tokens": ["Man", "gr\u00fc\u00b7be", "mir", "ein", "stil\u00b7les", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dort unter den Zypressen,", "tokens": ["Dort", "un\u00b7ter", "den", "Zyp\u00b7res\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.3": {"text": "Wo ich in wonnigen Tr\u00e4umen hab'", "tokens": ["Wo", "ich", "in", "won\u00b7ni\u00b7gen", "Tr\u00e4u\u00b7men", "hab'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "ADJA", "NN", "VAFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So manchesmal gesessen.", "tokens": ["So", "man\u00b7ches\u00b7mal", "ge\u00b7ses\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Mein Deutschland, immer liebt' ich dich", "tokens": ["Mein", "Deutschland", ",", "im\u00b7mer", "liebt'", "ich", "dich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PRF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Vor allen L\u00e4ndern der Erden,", "tokens": ["Vor", "al\u00b7len", "L\u00e4n\u00b7dern", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Doch nach Italien fl\u00fcchtet' ich", "tokens": ["Doch", "nach", "I\u00b7ta\u00b7li\u00b7en", "fl\u00fcch\u00b7tet'", "ich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "VVFIN", "PPER"], "meter": "+-+-++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Gar oft, um warm zu werden.", "tokens": ["Gar", "oft", ",", "um", "warm", "zu", "wer\u00b7den", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUI", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Es hei\u00dft, ein armer Toter soll", "tokens": ["Es", "hei\u00dft", ",", "ein", "ar\u00b7mer", "To\u00b7ter", "soll"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VMFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Manchmal vom Schlaf erstehen,", "tokens": ["Manch\u00b7mal", "vom", "Schlaf", "er\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Neugierig und gedankenvoll", "tokens": ["Neu\u00b7gie\u00b7rig", "und", "ge\u00b7dan\u00b7ken\u00b7voll"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein bi\u00dfchen spuken gehen.", "tokens": ["Ein", "bi\u00df\u00b7chen", "spu\u00b7ken", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Im Norden sind die N\u00e4chte rauh,", "tokens": ["Im", "Nor\u00b7den", "sind", "die", "N\u00e4ch\u00b7te", "rauh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da schlotterten mir die Gebeine.", "tokens": ["Da", "schlot\u00b7ter\u00b7ten", "mir", "die", "Ge\u00b7bei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Hier unten weht die Luft so lau", "tokens": ["Hier", "un\u00b7ten", "weht", "die", "Luft", "so", "lau"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "ART", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nachts im Zypressenhaine.", "tokens": ["Nachts", "im", "Zyp\u00b7res\u00b7sen\u00b7hai\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NE", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.7": {"line.1": {"text": "Da s\u00e4\u00df' ich nieder in guter Ruh", "tokens": ["Da", "s\u00e4\u00df'", "ich", "nie\u00b7der", "in", "gu\u00b7ter", "Ruh"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "An meines Sees Gestade", "tokens": ["An", "mei\u00b7nes", "Sees", "Ge\u00b7sta\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und h\u00f6rte dem Rauschen der Wellen zu,", "tokens": ["Und", "h\u00f6r\u00b7te", "dem", "Rau\u00b7schen", "der", "Wel\u00b7len", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Dem Zirpen der Zikade;", "tokens": ["Dem", "Zir\u00b7pen", "der", "Zi\u00b7ka\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Und gr\u00fc\u00dft' hin\u00fcber, wo d\u00e4mmrig gl\u00e4nzt", "tokens": ["Und", "gr\u00fc\u00dft'", "hin\u00b7\u00fc\u00b7ber", ",", "wo", "d\u00e4mm\u00b7rig", "gl\u00e4nzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "$,", "PWAV", "ADJD", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Das Berghaupt schlummertrunken", "tokens": ["Das", "Berg\u00b7haupt", "schlum\u00b7mer\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Des Monte Baldo, die Stirn umkr\u00e4nzt", "tokens": ["Des", "Mon\u00b7te", "Bal\u00b7do", ",", "die", "Stirn", "um\u00b7kr\u00e4nzt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "ART", "NN", "VVPP"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von kleinen Sternenfunken.", "tokens": ["Von", "klei\u00b7nen", "Ster\u00b7nen\u00b7fun\u00b7ken", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Italiens Himmel strahlte herab", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "Him\u00b7mel", "strahl\u00b7te", "her\u00b7ab"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "NN", "VVFIN", "ADV"], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Ins Tal der Tr\u00e4nen so heiter \u2013", "tokens": ["Ins", "Tal", "der", "Tr\u00e4\u00b7nen", "so", "hei\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "ADJD", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da schl\u00fcg' es Eins. Zur\u00fcck ins Grab", "tokens": ["Da", "schl\u00fcg'", "es", "Eins", ".", "Zu\u00b7r\u00fcck", "ins", "Grab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$.", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Schl\u00fcpft' ich und schliefe weiter.", "tokens": ["Schl\u00fcpft'", "ich", "und", "schlie\u00b7fe", "wei\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Und schlie\u00df' ich einst die Augen zu,", "tokens": ["Und", "schlie\u00df'", "ich", "einst", "die", "Au\u00b7gen", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nie wieder aufzuwachen,", "tokens": ["Nie", "wie\u00b7der", "auf\u00b7zu\u00b7wa\u00b7chen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVIZU", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "So gilt mir