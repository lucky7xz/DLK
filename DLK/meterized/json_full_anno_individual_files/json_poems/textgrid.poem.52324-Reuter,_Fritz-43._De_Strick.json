{"textgrid.poem.52324": {"metadata": {"author": {"name": "Reuter, Fritz", "birth": "N.A.", "death": "N.A."}, "title": "43. De Strick", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Herr Amtmann Schacht, de \u00fcmmer pr\u00fcgeln lett,", "tokens": ["Herr", "Amt\u00b7mann", "Schacht", ",", "de", "\u00fcm\u00b7mer", "pr\u00fc\u00b7geln", "lett", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NN", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "De f\u00fchrt mal mit sin Fru spazieren,", "tokens": ["De", "f\u00fchrt", "mal", "mit", "sin", "Fru", "spa\u00b7zie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Un as hei nu so ganz behaglich sitt,", "tokens": ["Un", "as", "hei", "nu", "so", "ganz", "be\u00b7hag\u00b7lich", "sitt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "ADV", "ADV", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dunn m\u00f6t em dat Mall\u00fcr passieren,", "tokens": ["Dunn", "m\u00f6t", "em", "dat", "Mal\u00b7l\u00fcr", "pas\u00b7sie\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dat em de B\u00e4nkenreimen ritt.", "tokens": ["Dat", "em", "de", "B\u00e4n\u00b7ken\u00b7rei\u00b7men", "ritt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Hei k\u00fcnn so'n d\u00f6rtig Liespund w\u00e4gen,", "tokens": ["Hei", "k\u00fcnn", "so'n", "d\u00f6r\u00b7tig", "Lies\u00b7pund", "w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Un so vel wull de Reimen nich verdr\u00e4gen.", "tokens": ["Un", "so", "vel", "wull", "de", "Rei\u00b7men", "nich", "ver\u00b7dr\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADJD", "VMFIN", "NE", "NN", "PTKNEG", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Dor lag hei nu un zappelt mit de Bein;", "tokens": ["Dor", "lag", "hei", "nu", "un", "zap\u00b7pelt", "mit", "de", "Bein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "FM", "FM", "FM", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Un sine Fru, de ok r\u00fcgg\u00e4wer sch\u00fctt,", "tokens": ["Un", "si\u00b7ne", "Fru", ",", "de", "ok", "r\u00fcg\u00b7g\u00e4\u00b7wer", "sch\u00fctt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "De zappelt mit ehr leiwen Beinings mit.", "tokens": ["De", "zap\u00b7pelt", "mit", "ehr", "lei\u00b7wen", "Bei\u00b7nings", "mit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "\u00bbdu brukst di gor nich \u00fcmtauseihn\u00ab,", "tokens": ["\u00bb", "du", "brukst", "di", "gor", "nich", "\u00fcm\u00b7tau\u00b7seihn", "\u00ab", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "NE", "NE", "PTKNEG", "VVINF", "$(", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Seggt tau den Kutscher Amtmann Schacht,", "tokens": ["Seggt", "tau", "den", "Kut\u00b7scher", "Amt\u00b7mann", "Schacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ART", "NN", "NE", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "\u00bbgiww leiwerst up de M\u00e4hren acht!", "tokens": ["\u00bb", "giww", "lei\u00b7werst", "up", "de", "M\u00e4h\u00b7ren", "acht", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "FM", "FM", "FM", "NN", "CARD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "De Sak, de ward sick liker reihn,", "tokens": ["De", "Sak", ",", "de", "ward", "sick", "li\u00b7ker", "reihn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "NE", "VAFIN", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.15": {"text": "Wenn wi man blot en Stricken'n f\u00fcnn'n,", "tokens": ["Wenn", "wi", "man", "blot", "en", "Stri\u00b7cken'n", "f\u00fcnn'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "FM", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Dat wi de B\u00e4nk uns wedder b\u00fcnn'n.\u00ab", "tokens": ["Dat", "wi", "de", "B\u00e4nk", "uns", "wed\u00b7der", "b\u00fcnn'", "n.", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "De Kutscher, de s\u00f6cht v\u00f6rn, de Amtmann hinn'n,", "tokens": ["De", "Kut\u00b7scher", ",", "de", "s\u00f6cht", "v\u00f6rn", ",", "de", "Amt\u00b7mann", "hinn'n", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM.fr", "FM.fr", "FM.fr", "$,", "NE", "NE", "PTKVZ", "$,"], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.18": {"text": "Doch nahrens wull en Strick sick finn'n.", "tokens": ["Doch", "nah\u00b7rens", "wull", "en", "Strick", "sick", "finn'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["KON", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Den Amtmann sine leiwe Fru,", "tokens": ["Den", "Amt\u00b7mann", "si\u00b7ne", "lei\u00b7we", "Fru", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "De m\u00fc\u00dft nah desen Trubel nu", "tokens": ["De", "m\u00fc\u00dft", "nah", "de\u00b7sen", "Tru\u00b7bel", "nu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VMFIN", "ADJD", "ART", "NN", "ADV"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.21": {"text": "Bi ehren Jochen sick platzieren;", "tokens": ["Bi", "eh\u00b7ren", "Jo\u00b7chen", "sick", "plat\u00b7zie\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "De Amtmann blew up sinen R\u00fcggen", "tokens": ["De", "Amt\u00b7mann", "blew", "up", "si\u00b7nen", "R\u00fcg\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "In des' Bequemlichkeit beliggen,", "tokens": ["In", "des'", "Be\u00b7quem\u00b7lich\u00b7keit", "be\u00b7lig\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Hei was kein grote Fr\u00fcnd von't R\u00fchren.", "tokens": ["Hei", "was", "kein", "gro\u00b7te", "Fr\u00fcnd", "von't", "R\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "ADJA", "NN", "APPR", "NN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.25": {"text": "So kam'n sei denn nah M\u00f6llenhagen.", "tokens": ["So", "kam'n", "sei", "denn", "nah", "M\u00f6l\u00b7len\u00b7ha\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VAFIN", "ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Bi'n Kraug, dor h\u00f6llt de Kutscher an", "tokens": ["Bi'n", "Kraug", ",", "dor", "h\u00f6llt", "de", "Kut\u00b7scher", "an"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "NE", "$,", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Un deiht de L\u00fcd un deiht den Kr\u00e4uger fragen,", "tokens": ["Un", "deiht", "de", "L\u00fcd", "un", "deiht", "den", "Kr\u00e4u\u00b7ger", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "VVFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.28": {"text": "Ob hei em nich en Strick verschaffen kann.", "tokens": ["Ob", "hei", "em", "nich", "en", "Strick", "ver\u00b7schaf\u00b7fen", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "FM", "FM", "FM", "FM", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Na, wil dat eben Kraugdag wir,", "tokens": ["Na", ",", "wil", "dat", "e\u00b7ben", "Kraug\u00b7dag", "wir", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VMFIN", "ART", "ADV", "NN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "So wiren all de Buren hir.", "tokens": ["So", "wi\u00b7ren", "all", "de", "Bu\u00b7ren", "hir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "De Schult, de trett ok an den Wagen,", "tokens": ["De", "Schult", ",", "de", "trett", "ok", "an", "den", "Wa\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "FM", "FM", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Un den'n ward ok de Amtmann fragen.", "tokens": ["Un", "den'n", "ward", "ok", "de", "Amt\u00b7mann", "fra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDS", "VAFIN", "NE", "NE", "NE", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "\u00bbja woll, Herr Amtmann, glik in'n Ogenblick!\u00ab", "tokens": ["\u00bb", "ja", "woll", ",", "Herr", "Amt\u00b7mann", ",", "glik", "in'n", "O\u00b7gen\u00b7blick", "!", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "p