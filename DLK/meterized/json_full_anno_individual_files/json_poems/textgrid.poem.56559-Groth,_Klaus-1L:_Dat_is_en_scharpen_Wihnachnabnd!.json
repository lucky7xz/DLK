{"textgrid.poem.56559": {"metadata": {"author": {"name": "Groth, Klaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dat is en scharpen Wihnachnabnd!", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dat is en scharpen Wihnachnabnd!", "tokens": ["Dat", "is", "en", "schar\u00b7pen", "Wih\u00b7nach\u00b7nabnd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Greetdort kik mal nan Kachelabnd!", "tokens": ["Greet\u00b7dort", "kik", "mal", "nan", "Ka\u00b7chel\u00b7abnd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Grotvader fr\u00fcsst uns sunst noch dot,", "tokens": ["Grot\u00b7va\u00b7der", "fr\u00fcsst", "uns", "sunst", "noch", "dot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Em ward v\u0153r K\u00fcll de N\u00e4s al roth.", "tokens": ["Em", "ward", "v\u0153r", "K\u00fcll", "de", "N\u00e4s", "al", "roth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "NE", "NE", "NE", "NE", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Och, lat He nu de Weeg man stan!", "tokens": ["Och", ",", "lat", "He", "nu", "de", "Weeg", "man", "stan", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "He schull man hier nan L\u0153hnstohl gan! \u2013", "tokens": ["He", "schull", "man", "hier", "nan", "L\u0153hn\u00b7stohl", "gan", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PIS", "ADV", "NE", "NE", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "S\u00fch so! nu is de Stuv al rein,", "tokens": ["S\u00fch", "so", "!", "nu", "is", "de", "Stuv", "al", "rein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$.", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Un f\u0119hlt der nix, as Sand to strein.", "tokens": ["Un", "f\u0119hlt", "der", "nix", ",", "as", "Sand", "to", "strein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NE", "$,", "FM.en", "FM.en", "FM.en", "FM.en", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "De Finstern tuckt un muckt sik ni,", "tokens": ["De", "Fins\u00b7tern", "tuckt", "un", "muckt", "sik", "ni", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "FM.fr", "$,"], "meter": "-+-+----", "measure": "unknown.measure.di"}, "line.2": {"text": "Wi m\u0153t noch rein mit't F\u00fcrfatt bi!", "tokens": ["Wi", "m\u0153t", "noch", "rein", "mit't", "F\u00fcr\u00b7fatt", "bi", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Wa knarrt de Snee! Wats dat v\u0153r Een?", "tokens": ["Wa", "knarrt", "de", "Snee", "!", "Wats", "dat", "v\u0153r", "E\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NE", "NE", "$.", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "De Frost makt idel flinke Been.", "tokens": ["De", "Frost", "makt", "i\u00b7del", "flin\u00b7ke", "Be\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dar kumt de S\u00fcnn! se's f\u00fcerroth!", "tokens": ["Dar", "kumt", "de", "S\u00fcnn", "!", "se's", "f\u00fcer\u00b7roth", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NE", "NE", "$.", "NE", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wenn de man h\u00f6lpt, so hett't keen Noth.", "tokens": ["Wenn", "de", "man", "h\u00f6lpt", ",", "so", "hett't", "ke\u00b7en", "Noth", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "PIS", "VVFIN", "$,", "ADV", "ADJD", "VVFIN", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "S\u00fch an! de Ecken schint al blank", "tokens": ["S\u00fch", "an", "!", "de", "E\u00b7cken", "schint", "al", "blank"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "PTKVZ", "$.", "NE", "NN", "VVFIN", "ADV", "PTKVZ"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Un drippelt oppe Finsterbank.", "tokens": ["Un", "drip\u00b7pelt", "op\u00b7pe", "Fins\u00b7ter\u00b7bank", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}}, "stanza.5": {"line.1": {"text": "De B\u00f6m hebbt all \u0119r Winterkleed,", "tokens": ["De", "B\u00f6m", "hebbt", "all", "\u0119r", "Win\u00b7ter\u00b7kleed", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dats witt, so wit de Ogen seht.", "tokens": ["Dats", "witt", ",", "so", "wit", "de", "O\u00b7gen", "seht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADV", "APPR", "NE", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man blot de B\u0119k int Wischenland", "tokens": ["Man", "blot", "de", "B\u0119k", "int", "Wi\u00b7schen\u00b7land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIS", "NE", "NE", "NE", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Is as en Spegel an de Wand.", "tokens": ["Is", "as", "en", "Spe\u00b7gel", "an", "de", "Wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "NN", "APPR", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "De Armn s\u00fcnd richti al to Gang!", "tokens": ["De", "Armn", "s\u00fcnd", "rich\u00b7ti", "al", "to", "Gang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De Nachts ni warm liggt, sl\u00f6ppt ni lang.", "tokens": ["De", "Nachts", "ni", "warm", "liggt", ",", "sl\u00f6ppt", "ni", "lang", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ADJD", "VVFIN", "$,", "FM.fr", "FM.fr", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "De l\u00fctten Dinger krupt so krumm", "tokens": ["De", "l\u00fct\u00b7ten", "Din\u00b7ger", "krupt", "so", "krumm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Hannschen an un D\u00f6ker um \u2013", "tokens": ["Mit", "Hann\u00b7schen", "an", "un", "D\u00f6\u00b7ker", "um", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "FM", "NN", "APPR", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Och, een l\u00fctt Seel fangt an to weenn,", "tokens": ["Och", ",", "e\u00b7en", "l\u00fctt", "Seel", "fangt", "an", "to", "weenn", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADJD", "NN", "VVFIN", "APPR", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Dats richti truri antosehn!", "tokens": ["Dats", "rich\u00b7ti", "tru\u00b7ri", "an\u00b7to\u00b7sehn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un so unsch\u00fclli un so smuck,", "tokens": ["Un", "so", "un\u00b7sch\u00fcl\u00b7li", "un", "so", "smuck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "FM", "FM", "ADV", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "V\u0153r Mitlidn ward dat Hart Een buck.", "tokens": ["V\u0153r", "Mit\u00b7lidn", "ward", "dat", "Hart", "E\u00b7en", "buck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VAFIN", "ART", "NN", "NE", "NE", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "De W\u00e4chter hett sin Stutenaarn \u2013", "tokens": ["De", "W\u00e4ch\u00b7ter", "hett", "sin", "Stu\u00b7te\u00b7naarn", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "De ward ok \u00f6ller mit de Jahrn.", "tokens": ["De", "ward", "ok", "\u00f6l\u00b7ler", "mit", "de", "Jahrn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "ADJD", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sin Festleed b\u0119vt de Strat hentlank,", "tokens": ["Sin", "Fest\u00b7leed", "b\u0119vt", "de", "Strat", "hent\u00b7lank", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "As sung he s\u00fclm sin Graffgesank.", "tokens": ["As", "sung", "he", "s\u00fclm", "sin", "Graff\u00b7ge\u00b7sank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "FM", "FM", "FM", "FM", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.9": {"line.1": {"text": "Wenn he hier rinkumt mit sin Korf,", "tokens": ["Wenn", "he", "hier", "rin\u00b7kumt", "mit", "sin", "Korf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ADV", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So fragt em mal na Holt un Torf,", "tokens": ["So", "fragt", "em", "mal", "na", "Holt", "un", "Torf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "FM", "FM", "FM", "FM", "FM", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Un g\u0119vt em man en Stuten mehr,", "tokens": ["Un", "g\u0119vt", "em", "man", "en", "Stu\u00b7ten", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn't wul de letzte Wihnacht weer!", "tokens": ["Wenn't", "wul", "de", "letz\u00b7te", "Wih\u00b7nacht", "weer", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "De Tid geit rascher as en Drom:", "tokens": ["De", "Tid", "geit", "ra\u00b7scher", "as", "en", "Drom", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.2": {"text": "Eerst krigt wi s\u00fclm en Wihnachtsbom,", "tokens": ["E\u00b7erst", "krigt", "wi", "s\u00fclm", "en", "Wih\u00b7nachts\u00b7bom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "FM.la", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Denn kamt uns Kinner an de Reeg,", "tokens": ["Denn", "kamt", "uns", "Kin\u00b7ner", "an", "de", "Reeg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "APPR", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn sitt Grotmoder bi de Weeg.", "tokens": ["Denn", "sitt", "Grot\u00b7mo\u00b7der", "bi", "de", "Weeg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "NE", "NE", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetr