{"textgrid.poem.31415": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich stehe Felsen- fest in meinem hohen hoffen.", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich stehe Felsen- fest in meinem hohen hoffen.", "tokens": ["Ich", "ste\u00b7he", "Fel\u00b7sen", "fest", "in", "mei\u00b7nem", "ho\u00b7hen", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "TRUNC", "ADJD", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die wellen prellen ab / an meinem steinern Haubt.", "tokens": ["Die", "wel\u00b7len", "prel\u00b7len", "ab", "/", "an", "mei\u00b7nem", "stei\u00b7nern", "Haubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$(", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ist dem Meere-Heer / zu st\u00fcrmen nicht erlaubt.", "tokens": ["So", "ist", "dem", "Mee\u00b7re\u00b7Heer", "/", "zu", "st\u00fcr\u00b7men", "nicht", "er\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$(", "PTKZU", "VVINF", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ihm schadt es nicht / ob schon die ungl\u00fcck Str\u00f6m es troffen.", "tokens": ["ihm", "schadt", "es", "nicht", "/", "ob", "schon", "die", "un\u00b7gl\u00fcck", "Str\u00f6m", "es", "trof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "