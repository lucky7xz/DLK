{"dta.poem.1009": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Entflieht auf leichten k&#228;hnen               ", "tokens": ["Ent\u00b7flieht", "auf", "leich\u00b7ten", "k", "&#228;", "hnen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$(", "VVINF"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Berauschten sonnenwelten               ", "tokens": ["Be\u00b7rauschten", "son\u00b7nen\u00b7wel\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Dass immer mildre thr&#228;nen               ", "tokens": ["Dass", "im\u00b7mer", "mild\u00b7re", "thr", "&#228;", "nen"], "token_info": ["word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "ADV", "ADJA", "NN", "$(", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Euch eure flucht entgelten             ", "tokens": ["Euch", "eu\u00b7re", "flucht", "ent\u00b7gel\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Seht diesen taumel blonder               ", "tokens": ["Seht", "die\u00b7sen", "tau\u00b7mel", "blon\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PDAT", "NN", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lichtblauer traumgewalten               ", "tokens": ["Licht\u00b7blau\u00b7er", "traum\u00b7ge\u00b7wal\u00b7ten"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und trunkner wonnen sonder               ", "tokens": ["Und", "trunk\u00b7ner", "won\u00b7nen", "son\u00b7der"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Verz&#252;ckung sich entfalten             ", "tokens": ["Verz", "&#252;", "ckung", "sich", "ent\u00b7fal\u00b7ten"], "token_info": ["word", "XML_entity", "word", "word", "word"], "pos": ["NN", "$(", "NN", "PRF", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Dass nicht der s&#252;sse schauer               ", "tokens": ["Dass", "nicht", "der", "s", "&#252;", "sse", "schau\u00b7er"], "token_info": ["word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In neues leid euch h&#252;lle &#8212;               ", "tokens": ["In", "neu\u00b7es", "leid", "euch", "h", "&#252;", "lle", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "XML_entity"], "pos": ["APPR", "ADJA", "ADJD", "PPER", "VVFIN", "$(", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es sei die stille trauer               ", "tokens": ["Es", "sei", "die", "stil\u00b7le", "trau\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die diesen fr&#252;hling f&#252;lle.             ", "tokens": ["Die", "die\u00b7sen", "fr", "&#252;", "hling", "f", "&#252;", "lle", "."], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "XML_entity", "word", "punct"], "pos": ["ART", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}