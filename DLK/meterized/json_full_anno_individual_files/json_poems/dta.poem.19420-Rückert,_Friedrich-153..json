{"dta.poem.19420": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "153.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Geliebt zu seyn, mein Sohn, ohn' auch zugleich geachtet,", "tokens": ["Ge\u00b7liebt", "zu", "seyn", ",", "mein", "Sohn", ",", "ohn'", "auch", "zu\u00b7gleich", "ge\u00b7ach\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "PTKZU", "VAINF", "$,", "PPOSAT", "NN", "$,", "KOUI", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nach diesem hab' ich nie getrachtet noch geschmachtet,", "tokens": ["Nach", "die\u00b7sem", "hab'", "ich", "nie", "ge\u00b7trach\u00b7tet", "noch", "ge\u00b7schmach\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VAFIN", "PPER", "ADV", "VVPP", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie's manche Leute dieser Zeit, nicht M\u00e4nner, giebt,", "tokens": ["Wie's", "man\u00b7che", "Leu\u00b7te", "die\u00b7ser", "Zeit", ",", "nicht", "M\u00e4n\u00b7ner", ",", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PIAT", "NN", "PDAT", "NN", "$,", "PTKNEG", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die nicht geachtet nicht geliebt sind, doch beliebt.", "tokens": ["Die", "nicht", "ge\u00b7ach\u00b7tet", "nicht", "ge\u00b7liebt", "sind", ",", "doch", "be\u00b7liebt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "po