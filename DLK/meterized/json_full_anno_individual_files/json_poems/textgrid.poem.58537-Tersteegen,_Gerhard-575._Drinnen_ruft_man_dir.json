{"textgrid.poem.58537": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "575. Drinnen ruft man dir", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hinein, hinein", "tokens": ["Hin\u00b7ein", ",", "hin\u00b7ein"], "token_info": ["word", "punct", "word"], "pos": ["ADV", "$,", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Der Seele \u00c4ugelein,", "tokens": ["Der", "See\u00b7le", "\u00c4u\u00b7ge\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hemme sanft den Trieb der Sinnen,", "tokens": ["Und", "hem\u00b7me", "sanft", "den", "Trieb", "der", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der liebste Freund ist bei dir drinnen,", "tokens": ["Der", "liebs\u00b7te", "Freund", "ist", "bei", "dir", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Schau doch, er will dich gar!", "tokens": ["Schau", "doch", ",", "er", "will", "dich", "gar", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PPER", "VMFIN", "PPER", "ADV", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Er sammelt Herz und Sinn und was zerstreuet war;", "tokens": ["Er", "sam\u00b7melt", "Herz", "und", "Sinn", "und", "was", "zer\u00b7streu\u00b7et", "war", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "KON", "PWS", "VVFIN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wer sollt' nicht innigst sinken", "tokens": ["Wer", "sollt'", "nicht", "in\u00b7nigst", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PTKNEG", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Und \u00fcberlassen sich der Liebe Winken?", "tokens": ["Und", "\u00fc\u00b7ber\u00b7las\u00b7sen", "sich", "der", "Lie\u00b7be", "Win\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Ganz still und stumm,", "tokens": ["Ganz", "still", "und", "stumm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Der Herr ist da in seinem Heiligtum!", "tokens": ["Der", "Herr", "ist", "da", "in", "sei\u00b7nem", "Hei\u00b7lig\u00b7tum", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Beim Umgang und bei \u00e4u\u00dfern Werken", "tokens": ["Beim", "Um\u00b7gang", "und", "bei", "\u00e4u\u00b7\u00dfern", "Wer\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "KON", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "La\u00df diesen Blick hinein, den stillen Sinn sich st\u00e4rken;", "tokens": ["La\u00df", "die\u00b7sen", "Blick", "hin\u00b7ein", ",", "den", "stil\u00b7len", "Sinn", "sich", "st\u00e4r\u00b7ken", ";"], "token_info": ["word", "word", "word", 