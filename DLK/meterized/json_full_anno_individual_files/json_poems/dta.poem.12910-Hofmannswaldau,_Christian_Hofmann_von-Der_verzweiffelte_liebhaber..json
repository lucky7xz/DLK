{"dta.poem.12910": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der verzweiffelte liebhaber.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! bricht dein grimm den porcellan", "tokens": ["Ach", "!", "bricht", "dein", "grimm", "den", "por\u00b7cel\u00b7lan"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "VVFIN", "PPOSAT", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der s\u00fc\u00dfen hoffnung gantz in dr\u00fcmmer?", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "hoff\u00b7nung", "gantz", "in", "dr\u00fcm\u00b7mer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "APPR", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Du blickest meiner flammen schimmer,", "tokens": ["Du", "bli\u00b7ckest", "mei\u00b7ner", "flam\u00b7men", "schim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als ein geborgtes wesen an.", "tokens": ["Als", "ein", "ge\u00b7borg\u00b7tes", "we\u00b7sen", "an", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und spiel\u2019 ich gleich betr\u00fcbte lieder;", "tokens": ["Und", "spiel'", "ich", "gleich", "be\u00b7tr\u00fcb\u00b7te", "lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So schl\u00e4gt dein kalter sinn doch ihre kr\u00e4ffte nieder.", "tokens": ["So", "schl\u00e4gt", "dein", "kal\u00b7ter", "sinn", "doch", "ih\u00b7re", "kr\u00e4ff\u00b7te", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADV", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein geist verachtet meine treu:", "tokens": ["Dein", "geist", "ver\u00b7ach\u00b7tet", "mei\u00b7ne", "treu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du fragest nichts nach angst und sehnen,", "tokens": ["Du", "fra\u00b7gest", "nichts", "nach", "angst", "und", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und stellest die verliebten thr\u00e4nen", "tokens": ["Und", "stel\u00b7lest", "die", "ver\u00b7lieb\u00b7ten", "thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In der verlohrnen perlen reih.", "tokens": ["In", "der", "ver\u00b7lohr\u00b7nen", "per\u00b7len", "reih", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die seuffzer sind verworffne zeugen,", "tokens": ["Die", "seuff\u00b7zer", "sind", "ver\u00b7worff\u00b7ne", "zeu\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und m\u00fcssen nur umsonst aus treuen hertzen steigen.", "tokens": ["Und", "m\u00fcs\u00b7sen", "nur", "um\u00b7sonst", "aus", "treu\u00b7en", "hert\u00b7zen", "stei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach, sch\u00f6ne Daphne! strenges Kind!", "tokens": ["Ach", ",", "sch\u00f6\u00b7ne", "Daph\u00b7ne", "!", "stren\u00b7ges", "Kind", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADJA", "NN", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wilst du den diamanten gleichen,", "tokens": ["Wilst", "du", "den", "di\u00b7a\u00b7man\u00b7ten", "glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die man mit blute mu\u00df erweichen,", "tokens": ["Die", "man", "mit", "blu\u00b7te", "mu\u00df", "er\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "VVFIN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So schaue, was dein knecht beginnt;", "tokens": ["So", "schau\u00b7e", ",", "was", "dein", "knecht", "be\u00b7ginnt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der um dein hertze zu gewinnen,", "tokens": ["Der", "um", "dein", "hert\u00b7ze", "zu", "ge\u00b7win\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier statt der dinte l\u00e4st der adern purpur rinnen.", "tokens": ["Hier", "statt", "der", "din\u00b7te", "l\u00e4st", "der", "a\u00b7dern", "pur\u00b7pur", "rin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVFIN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}