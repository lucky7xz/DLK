{"textgrid.poem.35463": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Gericht der Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es flog der Staub, und die Enge zwang \u2013", "tokens": ["Es", "flog", "der", "Staub", ",", "und", "die", "En\u00b7ge", "zwang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und alles so grau, verschwommen und kalt! \u2013", "tokens": ["Und", "al\u00b7les", "so", "grau", ",", "ver\u00b7schwom\u00b7men", "und", "kalt", "!", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "ADV", "ADJD", "$,", "VVPP", "KON", "ADJD", "$.", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du sprachest: Wo blieb dein stolzer Drang \u2013", "tokens": ["Du", "spra\u00b7chest", ":", "Wo", "blieb", "dein", "stol\u00b7zer", "Drang", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "VVFIN", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Deiner Seele gl\u00fchender Ueberschwang \u2013", "tokens": ["Dei\u00b7ner", "See\u00b7le", "gl\u00fc\u00b7hen\u00b7der", "Ue\u00b7ber\u00b7schwang", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$("], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Deiner Liebe todtrotzige Flammengewalt?", "tokens": ["Dei\u00b7ner", "Lie\u00b7be", "todt\u00b7rot\u00b7zi\u00b7ge", "Flam\u00b7men\u00b7ge\u00b7walt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+--+--+--+", "measure": "trochaic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Und ich hie\u00df dich gehen! \u2013 Die Enge zwang \u2013", "tokens": ["Und", "ich", "hie\u00df", "dich", "ge\u00b7hen", "!", "\u2013", "Die", "En\u00b7ge", "zwang", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "VVINF", "$.", "$(", "ART", "NN", "VVFIN", "$("], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und des Tages bleiches Larvenspiel ...", "tokens": ["Und", "des", "Ta\u00b7ges", "blei\u00b7ches", "Lar\u00b7ven\u00b7spiel", "..."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ich war so m\u00fcde ... der Vorhang fiel \u2013", "tokens": ["Ich", "war", "so", "m\u00fc\u00b7de", "...", "der", "Vor\u00b7hang", "fiel", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und mich umd\u00fcnstete F\u00e4ulnisgestank ...", "tokens": ["Und", "mich", "um\u00b7d\u00fcns\u00b7te\u00b7te", "F\u00e4ul\u00b7nis\u00b7ge\u00b7stank", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Und ich hie\u00df dich gehen! \u2013 Und nun kam die Nacht.", "tokens": ["Und", "ich", "hie\u00df", "dich", "ge\u00b7hen", "!", "\u2013", "Und", "nun", "kam", "die", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "VVINF", "$.", "$(", "KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Weit wichen die Schranken \u2013 die Einsamkeit", "tokens": ["Weit", "wi\u00b7chen", "die", "Schran\u00b7ken", "\u2013", "die", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$(", "ART", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Gebar mir ein gro\u00dfes, stolzstilles Leid,", "tokens": ["Ge\u00b7bar", "mir", "ein", "gro\u00b7\u00dfes", ",", "stolz\u00b7stil\u00b7les", "Leid", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Das trug triumphierender Freiheit Fracht! ...", "tokens": ["Das", "trug", "tri\u00b7um\u00b7phie\u00b7ren\u00b7der", "Frei\u00b7heit", "Fracht", "!", "..."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.5": {"line.1": {"text": "Du gingst! Was bist du nicht bei mir geblieben?", "tokens": ["Du", "gingst", "!", "Was", "bist", "du", "nicht", "bei", "mir", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kanntest du nicht mein grenzenlos Lieben?", "tokens": ["Kann\u00b7test", "du", "nicht", "mein", "gren\u00b7zen\u00b7los", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "ADJD", "ADJA", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Zu Tag fliegt der Staub \u2013 nur die Enge zwingt,", "tokens": ["Zu", "Tag", "fliegt", "der", "Staub", "\u2013", "nur", "die", "En\u00b7ge", "zwingt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$(", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Bis sie den Trotzigsten zerrieben \u2013", "tokens": ["Bis", "sie", "den", "Trot\u00b7zigs\u00b7ten", "zer\u00b7rie\u00b7ben", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$("], "meter": "-+-++--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Du gingst! Was bist du nicht bei mir geblieben?", "tokens": ["Du", "gingst", "!", "Was", "bist", "du", "nicht", "bei", "mir", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Kanntest du nicht mein grenzenlos Lieben?", "tokens": ["Kann\u00b7test", "du", "nicht", "mein", "gren\u00b7zen\u00b7los", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "ADJD", "ADJA", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Das wandelte sich in der Einsamkeit", "tokens": ["Das", "wan\u00b7del\u00b7te", "sich", "in", "der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Zu hei\u00dfem, br\u00fcnstig mahnendem Leid,", "tokens": ["Zu", "hei\u00b7\u00dfem", ",", "br\u00fcns\u00b7tig", "mah\u00b7nen\u00b7dem", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Das flehend vor dir niedersinkt!", "tokens": ["Das", "fle\u00b7hend", "vor", "dir", "nie\u00b7der\u00b7sinkt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVPP", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du gingst! Was bist du nicht bei mir geblieben?", "tokens": ["Du", "gingst", "!", "Was", "bist", "du", "nicht", "bei", "mir", "ge\u00b7blie\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kanntest du nicht mein grenzenlos Lieben?", "tokens": ["Kann\u00b7test", "du", "nicht", "mein", "gren\u00b7zen\u00b7los", "Lie\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKNEG", "PPOSAT", "ADJD", "ADJA", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Das brandet zur Nacht, da die Engnis zerbrach!", "tokens": ["Das", "bran\u00b7det", "zur", "Nacht", ",", "da", "die", "Eng\u00b7nis", "zer\u00b7brach", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Es hie\u00df dich gehen der staubgraue Tag", "tokens": ["Es", "hie\u00df", "dich", "ge\u00b7hen", "der", "staub\u00b7grau\u00b7e", "Tag"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.3": {"text": "Und die bl\u00f6de Sprache des Lichts!", "tokens": ["Und", "die", "bl\u00f6\u00b7de", "Spra\u00b7che", "des", "Lichts", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Ich liege schlaflos. Und es ergreift mich tief,", "tokens": ["Ich", "lie\u00b7ge", "schlaf\u00b7los", ".", "Und", "es", "er\u00b7greift", "mich", "tief", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "$.", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+----+-+", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Da\u00df meiner Liebe Gewalt dich nicht heimw\u00e4rts rief \u2013", "tokens": ["Da\u00df", "mei\u00b7ner", "Lie\u00b7be", "Ge\u00b7walt", "dich", "nicht", "heim\u00b7w\u00e4rts", "rief", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "PPER", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Meine Sehnsucht, die nimmer zu z\u00fcgeln!", "tokens": ["Mei\u00b7ne", "Sehn\u00b7sucht", ",", "die", "nim\u00b7mer", "zu", "z\u00fc\u00b7geln", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Da! Rauscht's nicht durch die Schatten der Nacht", "tokens": ["Da", "!", "Rauscht's", "nicht", "durch", "die", "Schat\u00b7ten", "der", "Nacht"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$.", "NE", "PTKNEG", "APPR", "ART", "NN", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Von leise schlagenden Fl\u00fcgeln?", "tokens": ["Von", "lei\u00b7se", "schla\u00b7gen\u00b7den", "Fl\u00fc\u00b7geln", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich w\u00e4hne: es ringen die Boten sich los", "tokens": ["Ich", "w\u00e4h\u00b7ne", ":", "es", "rin\u00b7gen", "die", "Bo\u00b7ten", "sich", "los"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "ART", "NN", "PRF", "PTKVZ"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Aus der Finsternis schl\u00fcndigem Kraterscho\u00df,", "tokens": ["Aus", "der", "Fins\u00b7ter\u00b7nis", "schl\u00fcn\u00b7di\u00b7gem", "Kra\u00b7ter\u00b7scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "--+--+--+-+", "measure": "anapaest.tri.plus"}, "line.5": {"text": "Mir tagestraurigem Helden", "tokens": ["Mir", "ta\u00b7ge\u00b7strau\u00b7ri\u00b7gem", "Hel\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.6": {"text": "Das Urteil zu vermelden \u2013", "tokens": ["Das", "Ur\u00b7teil", "zu", "ver\u00b7mel\u00b7den", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Die Boten des stummen Gerichts!", "tokens": ["Die", "Bo\u00b7ten", "des", "stum\u00b7men", "Ge\u00b7richts", "!"], "token_inf