{"textgrid.poem.44161": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die M\u00fcnzkunst seufzte nechst bey diesem Cabinette:", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die M\u00fcnzkunst seufzte nechst bey diesem Cabinette:", "tokens": ["Die", "M\u00fcnz\u00b7kunst", "seufz\u00b7te", "nechst", "bey", "die\u00b7sem", "Ca\u00b7bi\u00b7net\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ach, wenn es auch das Bild von dem Besizer h\u00e4tte!", "tokens": ["Ach", ",", "wenn", "es", "auch", "das", "Bild", "von", "dem", "Be\u00b7si\u00b7zer", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und w\u00fcntschte de\u00dfen Schlag in Gold und Erz zu sehn.", "tokens": ["Und", "w\u00fcnt\u00b7schte", "de\u00b7\u00dfen", "Schlag", "in", "Gold", "und", "Erz", "zu", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Schweig, sprach die Poesie, von Silber und Metalle!", "tokens": ["Schweig", ",", "sprach", "die", "Poe\u00b7sie", ",", "von", "Sil\u00b7ber", "und", "Me\u00b7tal\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "