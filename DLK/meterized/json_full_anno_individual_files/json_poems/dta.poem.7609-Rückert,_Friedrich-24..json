{"dta.poem.7609": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "24.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dem jungen Wolfe will der Hirt die Wolfsart nehmen,", "tokens": ["Dem", "jun\u00b7gen", "Wol\u00b7fe", "will", "der", "Hirt", "die", "Wolf\u00b7sart", "neh\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Mutterschaf mu\u00df ihn zu s\u00e4ugen sich bequemen.", "tokens": ["Das", "Mut\u00b7ter\u00b7schaf", "mu\u00df", "ihn", "zu", "s\u00e4u\u00b7gen", "sich", "be\u00b7que\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKZU", "VVFIN", "PRF", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die gute Mutter s\u00e4ugt an einer Brust ein Lamm,", "tokens": ["Die", "gu\u00b7te", "Mut\u00b7ter", "s\u00e4ugt", "an", "ei\u00b7ner", "Brust", "ein", "Lamm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zur andern einen Wolf, als seien beid' Ein Stamm.", "tokens": ["Zur", "an\u00b7dern", "ei\u00b7nen", "Wolf", ",", "als", "sei\u00b7en", "beid'", "Ein", "Stamm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ART", "NE", "$,", "KOKOM", "VAFIN", "PIS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als flockig ward das Lamm, ward sein Milchbruder zottig;", "tokens": ["Als", "flo\u00b7ckig", "ward", "das", "Lamm", ",", "ward", "sein", "Milch\u00b7bru\u00b7der", "zot\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der Pflegemutter Milch sie d\u00fcnkt' ihm d\u00fcnn und schottig.", "tokens": ["Der", "Pfle\u00b7ge\u00b7mut\u00b7ter", "Milch", "sie", "d\u00fcnkt'", "ihm", "d\u00fcnn", "und", "schot\u00b7tig", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch als er einst die Brust gesogen bis aufs Blut,", "tokens": ["Doch", "als", "er", "einst", "die", "Brust", "ge\u00b7so\u00b7gen", "bis", "aufs", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVPP", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da d\u00fcnkte so gew\u00fcrzt nun erst die Milch ihm gut.", "tokens": ["Da", "d\u00fcnk\u00b7te", "so", "ge\u00b7w\u00fcrzt", "nun", "erst", "die", "Milch", "ihm", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "ADV", "ADV", "ART", "NN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "\u201ewo s\u00fc\u00df ist Milch und Blut, ist feist das Fleisch und zart:\u201c", "tokens": ["\u201e", "wo", "s\u00fc\u00df", "ist", "Milch", "und", "Blut", ",", "ist", "feist", "das", "Fleisch", "und", "zart", ":", "\u201c"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PWAV", "ADJD", "VAFIN", "NN", "KON", "NN", "$,", "VAFIN", "ADJD", "ART", "NN", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So ward sich unbewu\u00dft bewu\u00dft die Wolfesart.", "tokens": ["So", "ward", "sich", "un\u00b7be\u00b7wu\u00dft", "be\u00b7wu\u00dft", "die", "Wol\u00b7fe\u00b7sart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PRF", "ADJD", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Die Mutter mit dem Bi\u00df, den Bruder mit der Tatze", "tokens": ["Die", "Mut\u00b7ter", "mit", "dem", "Bi\u00df", ",", "den", "Bru\u00b7der", "mit", "der", "Tat\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erw\u00fcrgt' er und entsprang dem Pferch mit einem Satze.", "tokens": ["Er\u00b7w\u00fcrgt'", "er", "und", "ent\u00b7sprang", "dem", "Pferch", "mit", "ei\u00b7nem", "Sat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KON", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Froh \u00fcberrascht der Sohn im Walde Vater Wolfen,", "tokens": ["Froh", "\u00fc\u00b7berr\u00b7ascht", "der", "Sohn", "im", "Wal\u00b7de", "Va\u00b7ter", "Wol\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "APPRART", "NN", "NN", "NE", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hat in dem Beruf bald treulich ihm geholfen.", "tokens": ["Und", "hat", "in", "dem", "Be\u00b7ruf", "bald", "treu\u00b7lich", "ihm", "ge\u00b7hol\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPR", "ART", "NN", "ADV", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Die Lust an Schaffleisch wollt' er lehren seinem Sohn;", "tokens": ["Die", "Lust", "an", "Schaf\u00b7fleisch", "wollt'", "er", "leh\u00b7ren", "sei\u00b7nem", "Sohn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VMFIN", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der aber sprach: Die lernt' ich an der Mutter schon.", "tokens": ["Der", "a\u00b7ber", "sprach", ":", "Die", "lernt'", "ich", "an", "der", "Mut\u00b7ter", "schon", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$.", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}