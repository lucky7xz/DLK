{"dta.poem.10313": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "Ii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ekein sch\u00f6n\u2019rer Tod ist in der Welt,", "tokens": ["\u201e", "kein", "sch\u00f6n'\u00b7rer", "Tod", "ist", "in", "der", "Welt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIAT", "ADJA", "NN", "VAFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als wer vor\u2019m Feind erschlagen,", "tokens": ["Als", "wer", "vor'm", "Feind", "er\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Auf gr\u00fcner Heid\u2019, im freien Feld,", "tokens": ["Auf", "gr\u00fc\u00b7ner", "Heid'", ",", "im", "frei\u00b7en", "Feld", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darf nicht h\u00f6r\u2019n gro\u00df Wehklagen;", "tokens": ["Darf", "nicht", "h\u00f6r'n", "gro\u00df", "Weh\u00b7kla\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "VVFIN", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Im engen Bett nur Ein\u2019r allein", "tokens": ["Im", "en\u00b7gen", "Bett", "nur", "Ein'r", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ADV", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mu\u00df an den Todesreihen:", "tokens": ["Mu\u00df", "an", "den", "To\u00b7des\u00b7rei\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "Hier findet er Gesellschaft fein,", "tokens": ["Hier", "fin\u00b7det", "er", "Ge\u00b7sell\u00b7schaft", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fall\u2019n mit wie Kr\u00e4uter im Maien.\u201c", "tokens": ["Fall'n", "mit", "wie", "Kr\u00e4u\u00b7ter", "im", "Mai\u00b7en", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "APPR", "KOKOM", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}}}}