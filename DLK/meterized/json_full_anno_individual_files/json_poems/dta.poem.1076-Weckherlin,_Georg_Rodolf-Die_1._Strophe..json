{"dta.poem.1076": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 1. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wach auf/ meine sehl/ es ist zeit/", "tokens": ["Wach", "auf", "/", "mei\u00b7ne", "sehl", "/", "es", "ist", "zeit", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$(", "PPOSAT", "NN", "$(", "PPER", "VAFIN", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Deine schuld der welt zu bezeugen;", "tokens": ["Dei\u00b7ne", "schuld", "der", "welt", "zu", "be\u00b7zeu\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Es ist eine schand still zuschweigen/", "tokens": ["Es", "ist", "ei\u00b7ne", "schand", "still", "zu\u00b7schwei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "VVFIN", "ADJD", "VVINF", "$("], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wan die nohtdurft die red gebeut.", "tokens": ["Wan", "die", "noht\u00b7durft", "die", "red", "ge\u00b7beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "VVFIN", "ART", "NE", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "O Buwinck hausen/ dessen gunst", "tokens": ["O", "Bu\u00b7win\u00b7ck", "hau\u00b7sen", "/", "des\u00b7sen", "gunst"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "VVFIN", "$(", "PRELAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Kan mich nach wunsch von sorgen freyhen/", "tokens": ["Kan", "mich", "nach", "wunsch", "von", "sor\u00b7gen", "frey\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "ADJD", "APPR", "NN", "VVINF", "$("], "meter": "---+-+-+-", "measure": "unknown.measure.tri"}, "line.7": {"text": "Verachte nicht die newe kunst/", "tokens": ["Ver\u00b7ach\u00b7te", "nicht", "die", "ne\u00b7we", "kunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "So mir dir neun schwestern verleihen;", "tokens": ["So", "mir", "dir", "neun", "schwes\u00b7tern", "ver\u00b7lei\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "PPER", "CARD", "VVINF", "VVINF", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.9": {"text": "Sondern h\u00f6r mit wie s\u00fcssem klang", "tokens": ["Son\u00b7dern", "h\u00f6r", "mit", "wie", "s\u00fcs\u00b7sem", "klang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PWAV", "PIS", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Meiner niemahls ber\u00fchrten saiten", "tokens": ["Mei\u00b7ner", "nie\u00b7mahls", "be\u00b7r\u00fchr\u00b7ten", "sai\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADV", "VVINF", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.11": {"text": "Ich dein lob in meinem gesang", "tokens": ["Ich", "dein", "lob", "in", "mei\u00b7nem", "ge\u00b7sang"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.12": {"text": "Will durch die weite welt au\u00dfspraiten.", "tokens": ["Will", "durch", "die", "wei\u00b7te", "welt", "au\u00df\u00b7sprai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}