{"textgrid.poem.56491": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "1L: Papierne Kronen zieren sie. Sie tragen", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Papierne Kronen zieren sie. Sie tragen", "tokens": ["Pa\u00b7pier\u00b7ne", "Kro\u00b7nen", "zie\u00b7ren", "sie", ".", "Sie", "tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "NN", "VVFIN", "PPER", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Holzst\u00f6cke aufrecht auf den spitzen Knien.", "tokens": ["Holz\u00b7st\u00f6\u00b7cke", "auf\u00b7recht", "auf", "den", "spit\u00b7zen", "Kni\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Und ihre langen, wei\u00dfen Hemden schlagen", "tokens": ["Und", "ih\u00b7re", "lan\u00b7gen", ",", "wei\u00b7\u00dfen", "Hem\u00b7den", "schla\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Um ihren Bauch wie K\u00f6nigshermelin.", "tokens": ["Um", "ih\u00b7ren", "Bauch", "wie", "K\u00f6\u00b7nigs\u00b7her\u00b7me\u00b7lin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "KOKOM", "NE", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ein Volk von Christussen, das leise schwebt", "tokens": ["Ein", "Volk", "von", "Chris\u00b7tus\u00b7sen", ",", "das", "lei\u00b7se", "schwebt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos"