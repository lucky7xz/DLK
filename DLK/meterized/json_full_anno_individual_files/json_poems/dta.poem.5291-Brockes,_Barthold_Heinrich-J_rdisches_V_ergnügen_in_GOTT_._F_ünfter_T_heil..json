{"dta.poem.5291": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "J rdisches  \n  V ergn\u00fcgen  \n in  \n  GOTT .  \n  F \u00fcnfter  T heil.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Und endlich, wie aus Millionen B\u00e4umen", "tokens": ["Und", "end\u00b7lich", ",", "wie", "aus", "Mil\u00b7lion\u00b7en", "B\u00e4u\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "PWAV", "APPR", "NN", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Viel Millionen Blumen keimen,", "tokens": ["Viel", "Mil\u00b7lion\u00b7en", "Blu\u00b7men", "kei\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die all\u2019 in Fr\u00fchling nicht allein die Erde schm\u00fccken,", "tokens": ["Die", "all'", "in", "Fr\u00fch\u00b7ling", "nicht", "al\u00b7lein", "die", "Er\u00b7de", "schm\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "NN", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wovon uns auch die Fr\u00fccht\u2019 erquicken;", "tokens": ["Wo\u00b7von", "uns", "auch", "die", "Fr\u00fccht'", "er\u00b7qui\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer, sag\u2019 ich, die\u00df erwegt, wird wol mit Recht ger\u00fchret,", "tokens": ["Wer", ",", "sag'", "ich", ",", "die\u00df", "er\u00b7wegt", ",", "wird", "wol", "mit", "Recht", "ge\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "PDS", "VVFIN", "$,", "VAFIN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit Recht zu aller Ding\u2019 allm\u00e4chtgem HErrn gef\u00fchret,", "tokens": ["Mit", "Recht", "zu", "al\u00b7ler", "Ding'", "all\u00b7m\u00e4cht\u00b7gem", "Herrn", "ge\u00b7f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PIAT", "NN", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und zu der wahren Qvell des Himmels und der Erden", "tokens": ["Und", "zu", "der", "wah\u00b7ren", "Qvell", "des", "Him\u00b7mels", "und", "der", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Bewunderung und Lob und Danck getrieben werden.", "tokens": ["Be\u00b7wun\u00b7de\u00b7rung", "und", "Lob", "und", "Danck", "ge\u00b7trie\u00b7ben", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}