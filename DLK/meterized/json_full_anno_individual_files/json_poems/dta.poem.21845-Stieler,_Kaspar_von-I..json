{"dta.poem.21845": {"metadata": {"author": {"name": "Stieler, Kaspar von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1660", "urn": "urn:nbn:de:kobv:b4-20586-2", "language": ["de:0.99"], "booktitle": "Filidor der Dorfferer [i. e. Stieler, Kaspar von]: Die Geharnschte Venus. Hamburg, 1660."}, "poem": {"stanza.1": {"line.1": {"text": "Der erste Griff der Liebe", "tokens": ["Der", "ers\u00b7te", "Griff", "der", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "geriethe mir durch Diebe/", "tokens": ["ge\u00b7rie\u00b7the", "mir", "durch", "Die\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "nun stehl ich immer zu.", "tokens": ["nun", "stehl", "ich", "im\u00b7mer", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Der schlauen Diebereyen", "tokens": ["Der", "schlau\u00b7en", "Die\u00b7be\u00b7re\u00b7yen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "kan sonst mich nichts befreyen/", "tokens": ["kan", "sonst", "mich", "nichts", "be\u00b7fre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "PIS", "VVINF", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "als Unruh und die Ruh.", "tokens": ["als", "Un\u00b7ruh", "und", "die", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}