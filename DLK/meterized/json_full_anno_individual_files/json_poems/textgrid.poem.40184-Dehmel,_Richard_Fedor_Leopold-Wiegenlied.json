{"textgrid.poem.40184": {"metadata": {"author": {"name": "Dehmel, Richard Fedor Leopold", "birth": "N.A.", "death": "N.A."}, "title": "Wiegenlied", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4ume, tr\u00e4ume, du mein s\u00fc\u00dfes Leben,", "tokens": ["Tr\u00e4u\u00b7me", ",", "tr\u00e4u\u00b7me", ",", "du", "mein", "s\u00fc\u00b7\u00dfes", "Le\u00b7ben", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PPER", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "von dem Garten, der im Himmel bl\u00fcht, \u2013", "tokens": ["von", "dem", "Gar\u00b7ten", ",", "der", "im", "Him\u00b7mel", "bl\u00fcht", ",", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "APPRART", "NN", "VVFIN", "$,", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Engel wohnen da und weben", "tokens": ["En\u00b7gel", "woh\u00b7nen", "da", "und", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "um dein Seelchen deiner Mutter Lied:", "tokens": ["um", "dein", "Seel\u00b7chen", "dei\u00b7ner", "Mut\u00b7ter", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Tr\u00e4ume, tr\u00e4ume, Bl\u00fcte meiner Liebe,", "tokens": ["Tr\u00e4u\u00b7me", ",", "tr\u00e4u\u00b7me", ",", "Bl\u00fc\u00b7te", "mei\u00b7ner", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "punct", "word",