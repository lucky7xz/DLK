{"dta.poem.21033": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XcVII.  \n Frauen  N.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Der Ottomannen Schild liegt hier in guter Ruh/", "tokens": ["Der", "Ot\u00b7to\u00b7man\u00b7nen", "Schild", "liegt", "hier", "in", "gu\u00b7ter", "Ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein ieder dr\u00fccke doch die d\u00fcnne Nase zu.", "tokens": ["Ein", "ie\u00b7der", "dr\u00fc\u00b7cke", "doch", "die", "d\u00fcn\u00b7ne", "Na\u00b7se", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der tr\u00e4gt kein Vortheil weg/ so lang alhier verhar-", "tokens": ["Der", "tr\u00e4gt", "kein", "Vor\u00b7theil", "weg", "/", "so", "lang", "al\u00b7hier", "ver\u00b7ha\u00b7r"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PIAT", "NN", "PTKVZ", "$(", "ADV", "ADJD", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "ret/", "tokens": ["ret", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Denn diese Leiche stanck zuvor sie war verscharret.", "tokens": ["Denn", "die\u00b7se", "Lei\u00b7che", "stanck", "zu\u00b7vor", "sie",