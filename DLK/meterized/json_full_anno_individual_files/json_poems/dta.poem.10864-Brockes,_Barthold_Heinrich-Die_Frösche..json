{"dta.poem.10864": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Fr\u00f6sche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngst h\u00f6rt ich von neuen dem lauten Gew\u00e4sche,", "tokens": ["J\u00fcngst", "h\u00f6rt", "ich", "von", "neu\u00b7en", "dem", "lau\u00b7ten", "Ge\u00b7w\u00e4\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dem quarrenden Knarren geschw\u00e4tziger Fr\u00f6sche,", "tokens": ["Dem", "quar\u00b7ren\u00b7den", "Knar\u00b7ren", "ge\u00b7schw\u00e4t\u00b7zi\u00b7ger", "Fr\u00f6\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "In ihrer ungest\u00f6hrten Ruh,", "tokens": ["In", "ih\u00b7rer", "un\u00b7ge\u00b7st\u00f6hr\u00b7ten", "Ruh", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit einiger Betrachtung zu.", "tokens": ["Mit", "ei\u00b7ni\u00b7ger", "Be\u00b7trach\u00b7tung", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mich deucht, da\u00df ihr verwirrt und mancherley Geschrey,", "tokens": ["Mich", "deucht", ",", "da\u00df", "ihr", "ver\u00b7wirrt", "und", "man\u00b7cher\u00b7ley", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "KON", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "In etwan f\u00fcnf bis sechserley,", "tokens": ["In", "et\u00b7wan", "f\u00fcnf", "bis", "sech\u00b7ser\u00b7ley", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "CARD", "APPR", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mehr Ver\u00e4ndrungen nicht, einzutheilen sey.", "tokens": ["Und", "mehr", "Ver\u00b7\u00e4n\u00b7drun\u00b7gen", "nicht", ",", "ein\u00b7zu\u00b7thei\u00b7len", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PTKNEG", "$,", "VVPP", "VAFIN", "$."], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.8": {"text": "Der gr\u00f6\u00dfte Haufe scheint, auf Menschen Art, zu murren,", "tokens": ["Der", "gr\u00f6\u00df\u00b7te", "Hau\u00b7fe", "scheint", ",", "auf", "Men\u00b7schen", "Art", ",", "zu", "mur\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "APPR", "NN", "NN", "$,", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Und, \u00fcm ein Nichts, zu l\u00e4rmen und zu knurren.", "tokens": ["Und", ",", "\u00fcm", "ein", "Nichts", ",", "zu", "l\u00e4r\u00b7men", "und", "zu", "knur\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "ART", "PIS", "$,", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Verschiedne sagen: ", "tokens": ["Ver\u00b7schied\u00b7ne", "sa\u00b7gen", ":"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Als Philosophen f\u00fcr.", "tokens": ["Als", "Phi\u00b7lo\u00b7so\u00b7phen", "f\u00fcr", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "Mit unbescheidnem Trotz, schien mancher, als ein Lehrer,", "tokens": ["Mit", "un\u00b7be\u00b7scheid\u00b7nem", "Trotz", ",", "schien", "man\u00b7cher", ",", "als", "ein", "Leh\u00b7rer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Mit einem sch\u00e4rfern Ton, sich \u00fcber seine H\u00f6rer,", "tokens": ["Mit", "ei\u00b7nem", "sch\u00e4r\u00b7fern", "Ton", ",", "sich", "\u00fc\u00b7ber", "sei\u00b7ne", "H\u00f6\u00b7rer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRF", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Durch nichts, als strengern Schall, sich eifrig zu bestreben,", "tokens": ["Durch", "nichts", ",", "als", "stren\u00b7gern", "Schall", ",", "sich", "eif\u00b7rig", "zu", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "ADJA", "NN", "$,", "PRF", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Hervor zu thun und zu erheben;", "tokens": ["Her\u00b7vor", "zu", "thun", "und", "zu", "er\u00b7he\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sie schienen mit Gewalt, die andern zu belehren,", "tokens": ["Sie", "schie\u00b7nen", "mit", "Ge\u00b7walt", ",", "die", "an\u00b7dern", "zu", "be\u00b7leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "PRELS", "PIS", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und mit der Lungen mehr, als des Verstandes Kraft,", "tokens": ["Und", "mit", "der", "Lun\u00b7gen", "mehr", ",", "als", "des", "Ver\u00b7stan\u00b7des", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.18": {"text": "Verschiedner Sachen Eigenschaft,", "tokens": ["Ver\u00b7schied\u00b7ner", "Sa\u00b7chen", "Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Mit gr\u00fcndlichem Bericht, den H\u00f6rern zu erkl\u00e4ren;", "tokens": ["Mit", "gr\u00fcnd\u00b7li\u00b7chem", "Be\u00b7richt", ",", "den", "H\u00f6\u00b7rern", "zu", "er\u00b7kl\u00e4\u00b7ren", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wovon doch, wie es lie\u00df, verschiedne l\u00e4ngst gefunden,", "tokens": ["Wo\u00b7von", "doch", ",", "wie", "es", "lie\u00df", ",", "ver\u00b7schied\u00b7ne", "l\u00e4ngst", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "$,", "PWAV", "PPER", "VVFIN", "$,", "ADJA", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df die so wohl, als sie, von allem nichts verstunden.", "tokens": ["Da\u00df", "die", "so", "wohl", ",", "als", "sie", ",", "von", "al\u00b7lem", "nichts", "ver\u00b7stun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADV", "$,", "KOUS", "PPER", "$,", "APPR", "PIS", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Ein kurz, doch hell Gequick, als ein Gepfeif, entfuhr", "tokens": ["Ein", "kurz", ",", "doch", "hell", "Ge\u00b7quick", ",", "als", "ein", "Ge\u00b7pfeif", ",", "ent\u00b7fuhr"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "$,", "ADV", "ADJD", "NN", "$,", "KOUS", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Verschiednen hier und dort. Die\u00df lie\u00df, als wenn sie nur", "tokens": ["Ver\u00b7schied\u00b7nen", "hier", "und", "dort", ".", "Die\u00df", "lie\u00df", ",", "als", "wenn", "sie", "nur"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "ADV", "$.", "PDS", "VVFIN", "$,", "KOKOM", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Mit jenen, da\u00df sie sich zu sehr erh\u00fcben,", "tokens": ["Mit", "je\u00b7nen", ",", "da\u00df", "sie", "sich", "zu", "sehr", "er\u00b7h\u00fc\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "$,", "KOUS", "PPER", "PRF", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Ein laut Gesp\u00f6tte trieben.", "tokens": ["Ein", "laut", "Ge\u00b7sp\u00f6t\u00b7te", "trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Noch andre schienen mir, mit unbesorgtem Lachen,", "tokens": ["Noch", "and\u00b7re", "schie\u00b7nen", "mir", ",", "mit", "un\u00b7be\u00b7sorg\u00b7tem", "La\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Jm warmen Sonnenstral, recht lustig sich zu machen.", "tokens": ["Jm", "war\u00b7men", "Son\u00b7nen\u00b7stral", ",", "recht", "lus\u00b7tig", "sich", "zu", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ADV", "ADJD", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und diese S\u00e4nger kamen mir,", "tokens": ["Und", "die\u00b7se", "S\u00e4n\u00b7ger", "ka\u00b7men", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Von allen, als die Kl\u00fcgsten, f\u00fcr.", "tokens": ["Von", "al\u00b7len", ",", "als", "die", "Kl\u00fcgs\u00b7ten", ",", "f\u00fcr", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "$,", "KOUS", "ART", "NN", "$,", "APPR", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}