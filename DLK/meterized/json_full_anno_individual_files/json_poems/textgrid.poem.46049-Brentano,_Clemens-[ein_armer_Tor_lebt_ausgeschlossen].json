{"textgrid.poem.46049": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[ein armer Tor lebt ausgeschlossen]", "genre": "verse", "period": "N.A.", "pub_year": 1830, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein armer Tor lebt ausgeschlossen", "tokens": ["Ein", "ar\u00b7mer", "Tor", "lebt", "aus\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Draus vor der Stadt bei einem Baum.", "tokens": ["Draus", "vor", "der", "Stadt", "bei", "ei\u00b7nem", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er dient den Reisenden zum Possen,", "tokens": ["Er", "dient", "den", "Rei\u00b7sen\u00b7den", "zum", "Pos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nickt f\u00fcr die trockne Rinde kaum.", "tokens": ["Nickt", "f\u00fcr", "die", "trock\u00b7ne", "Rin\u00b7de", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Doch von der Sonne Steigen, Neigen,", "tokens": ["Doch", "von", "der", "Son\u00b7ne", "Stei\u00b7gen", ",", "Nei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Bis zu der Sonne Niedergang", "tokens": ["Bis", "zu", "der", "Son\u00b7ne", "Nie\u00b7der\u00b7gang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schwingt er sich an den Palmenzweigen", "tokens": ["Schwingt", "er", "sich", "an", "den", "Pal\u00b7men\u00b7zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit ewig heiligem Gesang. \u2013", "tokens": ["Mit", "e\u00b7wig", "hei\u00b7li\u00b7gem", "Ge\u00b7sang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er singet nur die beiden Worte", "tokens": ["Er", "sin\u00b7get", "nur", "die", "bei\u00b7den", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ave Maria fort und fort,", "tokens": ["A\u00b7ve", "Ma\u00b7ria", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus seines Mundes frommer Pforte,", "tokens": ["Aus", "sei\u00b7nes", "Mun\u00b7des", "from\u00b7mer", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kam niemals noch ein andres Wort.", "tokens": ["Kam", "nie\u00b7mals", "noch", "ein", "and\u00b7res", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und als er endlich ausgeschwungen,", "tokens": ["Und", "als", "er", "end\u00b7lich", "aus\u00b7ge\u00b7schwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Am Abend bei dem Palmbaum lag,", "tokens": ["Am", "A\u00b7bend", "bei", "dem", "Palm\u00b7baum", "lag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hat er schon sterbend noch gesungen", "tokens": ["Hat", "er", "schon", "ster\u00b7bend", "noch", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ave Maria bis zum Tag.", "tokens": ["A\u00b7ve", "Ma\u00b7ria", "bis", "zum", "Tag", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Es nahten sich des Weges Boten,", "tokens": ["Es", "nah\u00b7ten", "sich", "des", "We\u00b7ges", "Bo\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Erstaunt, weil sich der Tor nicht schwang,", "tokens": ["Er\u00b7staunt", ",", "weil", "sich", "der", "Tor", "nicht", "schwang", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KOUS", "PRF", "ART", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und scharrten bald den armen Toten", "tokens": ["Und", "scharr\u00b7ten", "bald", "den", "ar\u00b7men", "To\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Am Baume ein ohn' Sang und Klang.", "tokens": ["Am", "Bau\u00b7me", "ein", "ohn'", "Sang", "und", "Klang", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ein Schw\u00e4tzer, der ihn oft verlachte", "tokens": ["Ein", "Schw\u00e4t\u00b7zer", ",", "der", "ihn", "oft", "ver\u00b7lach\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Reist eine Zeit nachher vorbei,", "tokens": ["Reist", "ei\u00b7ne", "Zeit", "nach\u00b7her", "vor\u00b7bei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und naht dem Baume stolz und dachte,", "tokens": ["Und", "naht", "dem", "Bau\u00b7me", "stolz", "und", "dach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was half sein Schwingen und Geschrei.", "tokens": ["Was", "half", "sein", "Schwin\u00b7gen", "und", "Ge\u00b7schrei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da spielt ein Wehen in den Zweigen", "tokens": ["Da", "spielt", "ein", "We\u00b7hen", "in", "den", "Zwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Auf jedem Blatt der Schw\u00e4tzer sieht", "tokens": ["Auf", "je\u00b7dem", "Blatt", "der", "Schw\u00e4t\u00b7zer", "sieht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ave Maria steigen, neigen", "tokens": ["A\u00b7ve", "Ma\u00b7ria", "stei\u00b7gen", ",", "nei\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["NE", "NE", "VVFIN", "$,", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit goldner Schrift des Toren Lied.", "tokens": ["Mit", "gold\u00b7ner", "Schrift", "des", "To\u00b7ren", "Lied", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Es fasset ihn das Liebeswunder,", "tokens": ["Es", "fas\u00b7set", "ihn", "das", "Lie\u00b7bes\u00b7wun\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Er k\u00fcndet es der ganzen Welt,", "tokens": ["Er", "k\u00fcn\u00b7det", "es", "der", "gan\u00b7zen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und macht zum Gru\u00df viel Herzen munter,", "tokens": ["Und", "macht", "zum", "Gru\u00df", "viel", "Her\u00b7zen", "mun\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und hat viel Schw\u00e4tzen eingestellt.", "tokens": ["Und", "hat", "viel", "Schw\u00e4t\u00b7zen", "ein\u00b7ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Nach unsers Heilands wahren Worten", "tokens": ["Nach", "un\u00b7sers", "Hei\u00b7lands", "wah\u00b7ren", "Wor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Selig die Armen in dem Geist", "tokens": ["Se\u00b7lig", "die", "Ar\u00b7men", "in", "dem", "Geist"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Der arme Tor, der selig worden", "tokens": ["Der", "ar\u00b7me", "Tor", ",", "der", "se\u00b7lig", "wor\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "ADJD", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der selige Solinus hei\u00dft. \u2013", "tokens": ["Der", "se\u00b7li\u00b7ge", "So\u00b7li\u00b7nus", "hei\u00dft", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nach der Erl\u00f6sung seufzt und ringet", "tokens": ["Nach", "der", "Er\u00b7l\u00f6\u00b7sung", "seufzt", "und", "rin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit uns sich alle Kreatur \u2013", "tokens": ["Mit", "uns", "sich", "al\u00b7le", "Kre\u00b7a\u00b7tur", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PRF", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur wer treu wie Solinus singet,", "tokens": ["Nur", "wer", "treu", "wie", "So\u00b7li\u00b7nus", "sin\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ADJD", "KOKOM", "NE", "VVFIN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der l\u00f6st die Fesseln der Natur.", "tokens": ["Der", "l\u00f6st", "die", "Fes\u00b7seln", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "O Seligkeit der beiden Worte", "tokens": ["O", "Se\u00b7lig\u00b7keit", "der", "bei\u00b7den", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "NN", "ART", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ave Maria fort und fort,", "tokens": ["A\u00b7ve", "Ma\u00b7ria", "fort", "und", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Erl\u00f6send t\u00f6nst du im Akkorde", "tokens": ["Er\u00b7l\u00f6\u00b7send", "t\u00f6nst", "du", "im", "Ak\u00b7kor\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gott, Mensch, im fleischgewordne