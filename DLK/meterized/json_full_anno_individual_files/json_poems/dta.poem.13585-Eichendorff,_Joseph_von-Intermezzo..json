{"dta.poem.13585": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Intermezzo.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Bildni\u00df wunderselig", "tokens": ["Dein", "Bild\u00b7ni\u00df", "wun\u00b7der\u00b7se\u00b7lig"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hab' ich in Herzensgrund,", "tokens": ["Hab'", "ich", "in", "Her\u00b7zens\u00b7grund", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das sieht so frisch und fr\u00f6hlich", "tokens": ["Das", "sieht", "so", "frisch", "und", "fr\u00f6h\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mich an zu jeder Stund'.", "tokens": ["Mich", "an", "zu", "je\u00b7der", "Stund'", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein Herz still in sich singet", "tokens": ["Mein", "Herz", "still", "in", "sich", "sin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein altes, sch\u00f6nes Lied,", "tokens": ["Ein", "al\u00b7tes", ",", "sch\u00f6\u00b7nes", "Lied", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Das in die Luft sich schwinget", "tokens": ["Das", "in", "die", "Luft", "sich", "schwin\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "NN", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und zu dir eilig zieht.", "tokens": ["Und", "zu", "dir", "ei\u00b7lig", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}