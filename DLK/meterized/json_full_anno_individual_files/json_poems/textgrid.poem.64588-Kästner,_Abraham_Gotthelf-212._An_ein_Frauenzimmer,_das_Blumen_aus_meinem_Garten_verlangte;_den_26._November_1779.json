{"textgrid.poem.64588": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "212. An ein Frauenzimmer, das Blumen aus meinem Garten verlangte; den 26. November 1779", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was, glaubst du, k\u00f6nnte jetzt in meinem Garten bl\u00fchn?", "tokens": ["Was", ",", "glaubst", "du", ",", "k\u00f6nn\u00b7te", "jetzt", "in", "mei\u00b7nem", "Gar\u00b7ten", "bl\u00fchn", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "$,", "VVFIN", "PPER", "$,", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erfrorne Myrten nur, und traurig Wintergr\u00fcn.", "tokens": ["Er\u00b7fror\u00b7ne", "Myr\u00b7ten", "nur", ",", "und", "trau\u00b7rig", "Win\u00b7ter\u00b7gr\u00fcn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADV", "$,", "KON", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein Bl\u00fcmchen lie\u00df sich nicht durch Sturm und Frost besiegen,", "tokens": ["Ein", "Bl\u00fcm\u00b7chen", "lie\u00df", "sich", "nicht", "durch", "Sturm", "und", "Frost", "be\u00b7sie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "PTKNEG", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und freut sich, da\u00df es lebt, um dich noch zu vergn\u00fcgen.", "tokens": ["Und", "freut", "sich", ",", "da\u00df", "es", "lebt", ",", "um", "dich", "noch", "zu", "ver\u00b7gn\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "$,", "KOUS", "PPER", "VVFIN", "$,", "KOUI", "PRF", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Auch mich dr\u00fcckt lange schon des Alters Winter schwer,", "tokens": ["Auch", "mich", "dr\u00fcckt", "lan\u00b7ge", "schon", "des", "Al\u00b7ters", "Win\u00b7ter", "schwer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und, leider! kommt f\u00fcr mich kein Fr\u00fchling wieder her.", "tokens": ["Und", ",", "lei\u00b7der", "!", "kommt", "f\u00fcr", "mich", "kein", "Fr\u00fch\u00b7ling", "wie\u00b7der", "her", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "$.", "VVFIN", "APPR", "PPER", "PIAT", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So denke, was du kannst von meinem Witz erwarten,", "tokens": ["So", "den\u00b7ke", ",", "was", "du", "kannst", "von", "mei\u00b7nem", "Witz", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "VMFIN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und fodre nicht von ihm was Bessers als vom Garten.", "tokens": ["Und", "fod\u00b7re", "nicht", "von", "ihm", "was", "Bes\u00b7sers", "als", "vom", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos":