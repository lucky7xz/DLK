{"dta.poem.19633": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Der sterbende Cromwell.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Vor der K\u00f6nigsburg in n\u00e4cht'ger Stunde", "tokens": ["Vor", "der", "K\u00f6\u00b7nigs\u00b7burg", "in", "n\u00e4cht'\u00b7ger", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Knickt der Tod die Eichen in die Runde,", "tokens": ["Knickt", "der", "Tod", "die", "Ei\u00b7chen", "in", "die", "Run\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Drinnen sucht er dann ein z\u00e4her Leben", "tokens": ["Drin\u00b7nen", "sucht", "er", "dann", "ein", "z\u00e4\u00b7her", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Aus den Wurzeln allgemach zu heben \u2014", "tokens": ["Aus", "den", "Wur\u00b7zeln", "all\u00b7ge\u00b7mach", "zu", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Whitehall ist Cromwell's Sterbest\u00e4tte,", "tokens": ["Whi\u00b7te\u00b7hall", "ist", "Crom\u00b7well's", "Ster\u00b7be\u00b7st\u00e4t\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NE", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Ein Waldenser kniet an seinem Bette!", "tokens": ["Ein", "Wal\u00b7den\u00b7ser", "kniet", "an", "sei\u00b7nem", "Bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "\u201eherr, ich komm', ein Kind des welschen Thales,", "tokens": ["\u201e", "herr", ",", "ich", "komm'", ",", "ein", "Kind", "des", "wel\u00b7schen", "Tha\u00b7les", ","], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "PPER", "VVFIN", "$,", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wo Du bist der Schutzgott jedes Mahles,", "tokens": ["Wo", "Du", "bist", "der", "Schutz\u00b7gott", "je\u00b7des", "Mah\u00b7les", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.9": {"text": "Unsern Dank auf Deine Knie zu legen,", "tokens": ["Un\u00b7sern", "Dank", "auf", "Dei\u00b7ne", "Knie", "zu", "le\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.10": {"text": "Leben, Cromwell, mu\u00dft Du unsertwegen!", "tokens": ["Le\u00b7ben", ",", "Crom\u00b7well", ",", "mu\u00dft", "Du", "un\u00b7sert\u00b7we\u00b7gen", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NE", "$,", "VMFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.11": {"text": "Rom befehdet uns mit seinen Pfaffen,", "tokens": ["Rom", "be\u00b7feh\u00b7det", "uns", "mit", "sei\u00b7nen", "Pfaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.12": {"text": "Unser Herzog r\u00fcstet frevle Waffen", "tokens": ["Un\u00b7ser", "Her\u00b7zog", "r\u00fcs\u00b7tet", "frev\u00b7le", "Waf\u00b7fen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.13": {"text": "Gegen unser Thal, den lautern Glauben", "tokens": ["Ge\u00b7gen", "un\u00b7ser", "Thal", ",", "den", "lau\u00b7tern", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.14": {"text": "Will er oder uns das Leben rauben!", "tokens": ["Will", "er", "o\u00b7der", "uns", "das", "Le\u00b7ben", "rau\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "KON", "PPER", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.15": {"text": "Doch Du sahst in Deinen Schmerzensn\u00e4chten", "tokens": ["Doch", "Du", "sahst", "in", "Dei\u00b7nen", "Schmer\u00b7zens\u00b7n\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.16": {"text": "Uns gefoltert schon von Henkersknechten", "tokens": ["Uns", "ge\u00b7fol\u00b7tert", "schon", "von", "Hen\u00b7kers\u00b7knech\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVPP", "ADV", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.17": {"text": "Und Du hobest Dich in Fieberschw\u00fcle", "tokens": ["Und", "Du", "ho\u00b7best", "Dich", "in", "Fie\u00b7ber\u00b7schw\u00fc\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PPER", "APPR", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.18": {"text": "Auf den Arm gest\u00fctzt empor vom Pf\u00fchle", "tokens": ["Auf", "den", "Arm", "ge\u00b7st\u00fctzt", "em\u00b7por", "vom", "Pf\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "PTKVZ", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.19": {"text": "Und Du drohtest \u00fcber Meer gewendet,", "tokens": ["Und", "Du", "droh\u00b7test", "\u00fc\u00b7ber", "Meer", "ge\u00b7wen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.20": {"text": "Pfaffen, Henker blieben ungesendet \u2014", "tokens": ["Pfaf\u00b7fen", ",", "Hen\u00b7ker", "blie\u00b7ben", "un\u00b7ge\u00b7sen\u00b7det"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Wenn wir, Cromwell, Deine S\u00f6hne w\u00e4ren,", "tokens": ["Wenn", "wir", ",", "Crom\u00b7well", ",", "Dei\u00b7ne", "S\u00f6h\u00b7ne", "w\u00e4\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "NE", "$,", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.22": {"text": "Herber k\u00f6nnten wir Dich nicht entbehren!", "tokens": ["Her\u00b7ber", "k\u00f6nn\u00b7ten", "wir", "Dich", "nicht", "ent\u00b7beh\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.23": {"text": "Dennoch \u2014 Wie Du leidest, Herr \u2014 uns\u00e4glich \u2014", "tokens": ["Den\u00b7noch", "Wie", "Du", "lei\u00b7dest", ",", "Herr", "un\u00b7s\u00e4g\u00b7lich"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "$(", "PWAV", "PPER", "VVFIN", "$,", "NN", "$(", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Deine Qualen werden unertr\u00e4glich? \u2014", "tokens": ["Dei\u00b7ne", "Qua\u00b7len", "wer\u00b7den", "un\u00b7er\u00b7tr\u00e4g\u00b7lich", "?"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.25": {"text": "Dennoch \u2014 ob uns Hartes sei beschieden \u2014", "tokens": ["Den\u00b7noch", "ob", "uns", "Har\u00b7tes", "sei", "be\u00b7schie\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "KOUS", "PPER", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Friedestifter, fahre hin in Frieden!\u201c", "tokens": ["Frie\u00b7des\u00b7tif\u00b7ter", ",", "fah\u00b7re", "hin", "in", "Frie\u00b7den", "!", "\u201c"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "$,", "VVFIN", "ADV", "APPR", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}}}}