{"dta.poem.23532": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Denkmalw\u00fcthigen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1840", "urn": "urn:nbn:de:kobv:b4-200905192626", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr denket jetzt: uns blieb nur das Ged\u00e4chtni\u00df", "tokens": ["Ihr", "den\u00b7ket", "jetzt", ":", "uns", "blieb", "nur", "das", "Ge\u00b7d\u00e4cht\u00b7ni\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der weiland gro\u00dfen Thaten zum Verm\u00e4chtni\u00df,", "tokens": ["Der", "wei\u00b7land", "gro\u00b7\u00dfen", "Tha\u00b7ten", "zum", "Ver\u00b7m\u00e4cht\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und haben wir ein Denkmal nur gesetzt,", "tokens": ["Und", "ha\u00b7ben", "wir", "ein", "Denk\u00b7mal", "nur", "ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ist Gro\u00dfes auch von uns geschehn zuletzt.", "tokens": ["Ist", "Gro\u00b7\u00dfes", "auch", "von", "uns", "ge\u00b7schehn", "zu\u00b7letzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "APPR", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Begn\u00fcgt euch nicht, da\u00df uns die gro\u00dfen Ahnen", "tokens": ["Be\u00b7gn\u00fcgt", "euch", "nicht", ",", "da\u00df", "uns", "die", "gro\u00b7\u00dfen", "Ah\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "An unsers Volkes Gr\u00f6\u00df' und Tugend mahnen!", "tokens": ["An", "un\u00b7sers", "Vol\u00b7kes", "Gr\u00f6\u00df'", "und", "Tu\u00b7gend", "mah\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das Gro\u00dfe lasst uns selber stets erneu'n,", "tokens": ["Das", "Gro\u00b7\u00dfe", "lasst", "uns", "sel\u00b7ber", "stets", "er\u00b7neu'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So kann uns nur das Gro\u00dfe wahrhaft freu'n.", "tokens": ["So", "kann", "uns", "nur", "das", "Gro\u00b7\u00dfe", "wahr\u00b7haft", "freu'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}