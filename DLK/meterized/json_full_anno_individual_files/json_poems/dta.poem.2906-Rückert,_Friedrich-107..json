{"dta.poem.2906": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "107.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Auf Reisen willst du gehn? was willst du sehn auf Reisen?", "tokens": ["Auf", "Rei\u00b7sen", "willst", "du", "gehn", "?", "was", "willst", "du", "sehn", "auf", "Rei\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "VVINF", "$.", "PWS", "VMFIN", "PPER", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df dir die Lust vergehn, die Lust zu gehn auf Reisen!", "tokens": ["La\u00df", "dir", "die", "Lust", "ver\u00b7gehn", ",", "die", "Lust", "zu", "gehn", "auf", "Rei\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$,", "ART", "NN", "PTKZU", "VVINF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Welt ist immer jung, du bist geworden alt,", "tokens": ["Die", "Welt", "ist", "im\u00b7mer", "jung", ",", "du", "bist", "ge\u00b7wor\u00b7den", "alt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,", "PPER", "VAFIN", "VAPP", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das merkst du weniger am alten Aufenthalt.", "tokens": ["Das", "merkst", "du", "we\u00b7ni\u00b7ger", "am", "al\u00b7ten", "Auf\u00b7ent\u00b7halt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das sagt im fremden Raum dir jeder frische Baum:", "tokens": ["Das", "sagt", "im", "frem\u00b7den", "Raum", "dir", "je\u00b7der", "fri\u00b7sche", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "ADJA", "NN", "PPER", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein Lenz ist abgebl\u00fcht, und ausgetr\u00e4umt dein Traum.", "tokens": ["Dein", "Lenz", "ist", "ab\u00b7ge\u00b7bl\u00fcht", ",", "und", "aus\u00b7ge\u00b7tr\u00e4umt", "dein", "Traum", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum rath' ich dir, wenn Rath du willst annehmen: Reise,", "tokens": ["Drum", "ra\u00b7th'", "ich", "dir", ",", "wenn", "Rath", "du", "willst", "an\u00b7neh\u00b7men", ":", "Rei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "$,", "KOUS", "NN", "PPER", "VMFIN", "VVINF", "$.", "NN", "$,"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.2": {"text": "Nicht gradaus wie der Wind, nur wie die Sonn im Kreise!", "tokens": ["Nicht", "gra\u00b7daus", "wie", "der", "Wind", ",", "nur", "wie", "die", "Sonn", "im", "Krei\u00b7se", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "KOKOM", "ART", "NN", "$,", "ADV", "KOKOM", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Heb in Gedanken dich zu ihr empor, und schau", "tokens": ["Heb", "in", "Ge\u00b7dan\u00b7ken", "dich", "zu", "ihr", "em\u00b7por", ",", "und", "schau"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "APPR", "NN", "PRF", "APPR", "PPER", "PTKVZ", "$,", "KON", "NN"], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.2": {"text": "Herab: die Erd' ist gr\u00fcn, soweit der Himmel blau.", "tokens": ["Her\u00b7ab", ":", "die", "Erd'", "ist", "gr\u00fcn", ",", "so\u00b7weit", "der", "Him\u00b7mel", "blau", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ART", "NN", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}