{"dta.poem.11298": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf einen gewissen   mathematicum.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du bist und heist ein mathematicus,", "tokens": ["Du", "bist", "und", "heist", "ein", "ma\u00b7the\u00b7ma\u00b7ti\u00b7cus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "KON", "VVFIN", "ART", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und dieses ist nicht ohne vorbedacht geschehn;", "tokens": ["Und", "die\u00b7ses", "ist", "nicht", "oh\u00b7ne", "vor\u00b7be\u00b7dacht", "ge\u00b7schehn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PTKNEG", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich mache diesen schlu\u00df:", "tokens": ["Ich", "ma\u00b7che", "die\u00b7sen", "schlu\u00df", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Du bist vorl\u00e4ngst dazu erkohren,", "tokens": ["Du", "bist", "vor\u00b7l\u00e4ngst", "da\u00b7zu", "er\u00b7koh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Drum hat die g\u00fctige natur", "tokens": ["Drum", "hat", "die", "g\u00fc\u00b7ti\u00b7ge", "na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich gleich, als du zur welt gebohren,", "tokens": ["Dich", "gleich", ",", "als", "du", "zur", "welt", "ge\u00b7boh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "An statt der waden nur mit linien versehn.", "tokens": ["An", "statt", "der", "wa\u00b7den", "nur", "mit", "li\u00b7ni\u00b7en", "ver\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "ADV", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}}}}