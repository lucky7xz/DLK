{"dta.poem.20700": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Die Nadelstichsheilung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ditmar spielte mit Minetten               ", "tokens": ["Dit\u00b7mar", "spiel\u00b7te", "mit", "Mi\u00b7net\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und sie war ihm zugewandt,", "tokens": ["Und", "sie", "war", "ihm", "zu\u00b7ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als ob Beyde sich gekannt", "tokens": ["Als", "ob", "Bey\u00b7de", "sich", "ge\u00b7kannt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "KOUS", "PIS", "PRF", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mondenlang schon h\u00e4tten.", "tokens": ["Mon\u00b7den\u00b7lang", "schon", "h\u00e4t\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VAFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Auf sein Knie hub er das Kind,", "tokens": ["Auf", "sein", "Knie", "hub", "er", "das", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und wie nun die Kinder sind,", "tokens": ["Und", "wie", "nun", "die", "Kin\u00b7der", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Rasch und leicht wie Mayenwind,", "tokens": ["Rasch", "und", "leicht", "wie", "Ma\u00b7yen\u00b7wind", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "KOKOM", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Fl\u00fcchtig wie des Rehes", "tokens": ["Fl\u00fcch\u00b7tig", "wie", "des", "Re\u00b7hes"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "KOKOM", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Jugendlicher Sprung ins Gras,", "tokens": ["Ju\u00b7gend\u00b7li\u00b7cher", "Sprung", "ins", "Gras", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "War Minettchen, und da sa\u00df", "tokens": ["War", "Mi\u00b7nett\u00b7chen", ",", "und", "da", "sa\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "NN", "$,", "KON", "ADV", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Eine Nadel bei dem Spas", "tokens": ["Ei\u00b7ne", "Na\u00b7del", "bei", "dem", "Spas"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.12": {"text": "In dem Sch\u00fcrzchen, und des Wehes", "tokens": ["In", "dem", "Sch\u00fcrz\u00b7chen", ",", "und", "des", "We\u00b7hes"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "KON", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Von dem kleinen Nadelstich", "tokens": ["Von", "dem", "klei\u00b7nen", "Na\u00b7del\u00b7stich"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.14": {"text": "Sch\u00e4mte Ditmars Finger sich,", "tokens": ["Sch\u00e4m\u00b7te", "Dit\u00b7mars", "Fin\u00b7ger", "sich", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "PRF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.15": {"text": "Hing herunter und verheilte", "tokens": ["Hing", "her\u00b7un\u00b7ter", "und", "ver\u00b7heil\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Sich mit seinem eignen Blut;", "tokens": ["Sich", "mit", "sei\u00b7nem", "eig\u00b7nen", "Blut", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Ein halb Viertelst\u00fcndchen weilte", "tokens": ["Ein", "halb", "Vier\u00b7tel\u00b7st\u00fcnd\u00b7chen", "weil\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Dieses Schmerzes Wuth \u2014", "tokens": ["Die\u00b7ses", "Schmer\u00b7zes", "Wuth"], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Aber wenn der ", "tokens": ["A\u00b7ber", "wenn", "der"], "token_info": ["word", "word", "word"], "pos": ["KON", "KOUS", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.20": {"text": "Mit erwachsnen Mienchens spielt,", "tokens": ["Mit", "er\u00b7wachs\u00b7nen", "Mien\u00b7chens", "spielt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Die schon gro\u00df sind, und vern\u00fcnftig,", "tokens": ["Die", "schon", "gro\u00df", "sind", ",", "und", "ver\u00b7n\u00fcnf\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VAFIN", "$,", "KON", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Wenn Er da Verwundung f\u00fchlt,", "tokens": ["Wenn", "Er", "da", "Ver\u00b7wun\u00b7dung", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.23": {"text": "Von des sch\u00f6nsten Auges Blicken,", "tokens": ["Von", "des", "sch\u00f6ns\u00b7ten", "Au\u00b7ges", "Bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Von der Lippen Grazie;", "tokens": ["Von", "der", "Lip\u00b7pen", "Gra\u00b7zie", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "--+-+-", "measure": "anapaest.init"}, "line.25": {"text": "O dann thuts im Herzen weh,", "tokens": ["O", "dann", "thuts", "im", "Her\u00b7zen", "weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "APPRART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Und man mu\u00df sich flehend b\u00fccken,", "tokens": ["Und", "man", "mu\u00df", "sich", "fle\u00b7hend", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "PRF", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Da\u00df Gott Amors Bruder eilt,", "tokens": ["Da\u00df", "Gott", "A\u00b7mors", "Bru\u00b7der", "eilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.28": {"text": "Der die Wunde heilt.", "tokens": ["Der", "die", "Wun\u00b7de", "heilt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}}}}