{"textgrid.poem.40572": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Bas' und Vetter tafeln im Frei'n", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bas' und Vetter tafeln im Frei'n", "tokens": ["Bas'", "und", "Vet\u00b7ter", "ta\u00b7feln", "im", "Frei'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Unterm Lindenbaum;", "tokens": ["Un\u00b7term", "Lin\u00b7den\u00b7baum", ";"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Sitzt auch ein singendes V\u00f6gelein", "tokens": ["Sitzt", "auch", "ein", "sin\u00b7gen\u00b7des", "V\u00f6\u00b7ge\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "In dem schattigen Raum.", "tokens": ["In", "dem", "schat\u00b7ti\u00b7gen", "Raum", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Und es meinen zu verstehn", "tokens": ["Und", "es", "mei\u00b7nen", "zu", "ver\u00b7stehn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "PTKZU", "VVINF"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Solches Wort die Zwei:", "tokens": ["Sol\u00b7ches", "Wort", "die", "Zwei", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "CARD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbwie ist Gottes Welt so sch\u00f6n,", "tokens": ["\u00bb", "wie", "ist", "Got\u00b7tes", "Welt", "so", "sch\u00f6n", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOKOM", "VAFIN", "NN", "NN", "ADV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Sch\u00f6n und gro\u00df und frei!\u00ab", "tokens": ["Sch\u00f6n", "und", "gro\u00df", "und", "frei", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "KON", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Vettern griff des Vogels Sang", "tokens": ["Vet\u00b7tern", "griff", "des", "Vo\u00b7gels", "Sang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief wohl in die Brust,", "tokens": ["Tief", "wohl", "in", "die", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df vom Rasensitz er sprang", "tokens": ["Da\u00df", "vom", "Ra\u00b7sen\u00b7sitz", "er", "sprang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Voll von Wanderlust!", "tokens": ["Voll", "von", "Wan\u00b7der\u00b7lust", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "\u00bbb\u00e4schen, meinen Stab hervor!", "tokens": ["\u00bb", "b\u00e4\u00b7schen", ",", "mei\u00b7nen", "Stab", "her\u00b7vor", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "VVINF", "$,", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnell mein B\u00fcndel geschnallt!", "tokens": ["Schnell", "mein", "B\u00fcn\u00b7del", "ge\u00b7schnallt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "H\u00e4ng' mir um mein Kugelrohr", "tokens": ["H\u00e4ng'", "mir", "um", "mein", "Ku\u00b7gel\u00b7rohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Gegen die B\u00e4ren im Wald!", "tokens": ["Ge\u00b7gen", "die", "B\u00e4\u00b7ren", "im", "Wald", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.5": {"line.1": {"text": "Meinen Sonntagsstaat umschling'", "tokens": ["Mei\u00b7nen", "Sonn\u00b7tags\u00b7staat", "um\u00b7schling'"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Einer Blouse Flor,", "tokens": ["Ei\u00b7ner", "Blou\u00b7se", "Flor", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Draus entpuppt der Schmetterling", "tokens": ["Draus", "ent\u00b7puppt", "der", "Schmet\u00b7ter\u00b7ling"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Fliegt verj\u00fcngt hervor!", "tokens": ["Fliegt", "ver\u00b7j\u00fcngt", "her\u00b7vor", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Tubus komm, mir doppelt n\u00fctz,", "tokens": ["Tu\u00b7bus", "komm", ",", "mir", "dop\u00b7pelt", "n\u00fctz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "PPER", "ADJD", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Fernen ziehst du heran;", "tokens": ["Fer\u00b7nen", "ziehst", "du", "he\u00b7ran", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "R\u00e4uber, dich haltend f\u00fcr Gesch\u00fctz,", "tokens": ["R\u00e4u\u00b7ber", ",", "dich", "hal\u00b7tend", "f\u00fcr", "Ge\u00b7sch\u00fctz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRF", "ADJD", "APPR", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "H\u00e4ltst du fern im Bann!", "tokens": ["H\u00e4ltst", "du", "fern", "im", "Bann", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "B\u00e4schen, Pfeif' und Knaster auch!", "tokens": ["B\u00e4\u00b7schen", ",", "Pfeif'", "und", "Knas\u00b7ter", "auch", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn zu klar die Luft,", "tokens": ["Wenn", "zu", "klar", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "H\u00fcll' ich die Landschaft leis in Rauch,", "tokens": ["H\u00fcll'", "ich", "die", "Land\u00b7schaft", "leis", "in", "Rauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "ADJD", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da ich sie lieb' im Duft.", "tokens": ["Da", "ich", "sie", "lieb'", "im", "Duft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Einen Blitzableiter mir pflanz'", "tokens": ["Ei\u00b7nen", "Blitz\u00b7ab\u00b7lei\u00b7ter", "mir", "pflanz'"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVFIN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Auf den Regenschirm,", "tokens": ["Auf", "den", "Re\u00b7gen\u00b7schirm", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df ich so gesichert ganz,", "tokens": ["Da\u00df", "ich", "so", "ge\u00b7si\u00b7chert", "ganz", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "ADV", "$,"], "meter": "+---+-+", "measure": "dactylic.init"}, "line.4": {"text": "Ob es regn' und st\u00fcrm'!", "tokens": ["Ob", "es", "regn'", "und", "st\u00fcrm'", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "Flaschenkeller, Triumph und Sieg", "tokens": ["Fla\u00b7schen\u00b7kel\u00b7ler", ",", "Tri\u00b7umph", "und", "Sieg"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Menschlichen Geistes du!", "tokens": ["Menschli\u00b7chen", "Geis\u00b7tes", "du", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da\u00df noch Haus und Hof ich tr\u00fcg',", "tokens": ["Da\u00df", "noch", "Haus", "und", "Hof", "ich", "tr\u00fcg'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NN", "KON", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Schnecken gleich, dazu!", "tokens": ["Schne\u00b7cken", "gleich", ",", "da\u00b7zu", "!"], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["NN", "ADV", "$,", "PAV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Lebe wohl, und das Weinen la\u00df!", "tokens": ["Le\u00b7be", "wohl", ",", "und", "das", "Wei\u00b7nen", "la\u00df", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Ziehn jetzt kann ich getrost!", "tokens": ["Ziehn", "jetzt", "kann", "ich", "ge\u00b7trost", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VMFIN", "PPER", "VVPP", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Wenn ich etwa vergessen was,", "tokens": ["Wenn", "ich", "et\u00b7wa", "ver\u00b7ges\u00b7sen", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVPP", "PWS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sende mir's nach per Post.\u00ab", "tokens": ["Sen\u00b7de", "mir's", "nach", "per", "Post", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NE", "APPR", "ADJA", "NN", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.11": {"line.1": {"text": "Als der Vetter so zum Gehn", "tokens": ["Als", "der", "Vet\u00b7ter", "so", "zum", "Gehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "APPRART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sich hat angeschickt,", "tokens": ["Sich", "hat", "an\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "VAFIN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Da begab sich's, da\u00df das Gehn", "tokens": ["Da", "be\u00b7gab", "sich's", ",", "da\u00df", "das", "Gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "$,", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ihm gar nicht mehr gl\u00fcckt.", "tokens": ["Ihm", "gar", "nicht", "mehr", "gl\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "ADV", "VVFIN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "V\u00f6glein von dem Baum entweicht,", "tokens": ["V\u00f6\u00b7glein", "von", "dem", "Baum", "ent\u00b7weicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Singt ins Blau hinein:", "tokens": ["Singt", "ins", "Blau", "hin\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "\u00bbfederleicht, ja federleicht", "tokens": ["\u00bb", "fe\u00b7der\u00b7leicht", ",", "ja", "fe\u00b7der\u00b7leicht"], "token_info": ["punct", "word", "punct", "word", "word"], "pos": ["$(", "ADJD", "$,", "ADV", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mu\u00df der Wandrer sein!\u00ab", "tokens": ["Mu\u00df", "der", "Wand\u00b7rer", "sein", "!", "\u00ab"], "toke