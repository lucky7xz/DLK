{"dta.poem.19467": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "I .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Ich lag an einem Raine", "tokens": ["Ich", "lag", "an", "ei\u00b7nem", "Rai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit meinem d\u00fcrren Stab.", "tokens": ["Mit", "mei\u00b7nem", "d\u00fcr\u00b7ren", "Stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was lauf' ich? Meine Beine", "tokens": ["Was", "lauf'", "ich", "?", "Mei\u00b7ne", "Bei\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "PPER", "$.", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Erlaufen nur das Grab ...", "tokens": ["Er\u00b7lau\u00b7fen", "nur", "das", "Grab", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ein Wandrer zog derenden,", "tokens": ["Ein", "Wand\u00b7rer", "zog", "de\u00b7ren\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "War noch ein Knabe fast,", "tokens": ["War", "noch", "ein", "Kna\u00b7be", "fast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der hielt als Stab in H\u00e4nden", "tokens": ["Der", "hielt", "als", "Stab", "in", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KOUS", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den bl\u00fcthenreichsten Ast.", "tokens": ["Den", "bl\u00fc\u00b7then\u00b7reichs\u00b7ten", "Ast", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "\u201egr\u00fc\u00df Gott dich, sch\u00f6ner Wandrer!", "tokens": ["\u201e", "gr\u00fc\u00df", "Gott", "dich", ",", "sch\u00f6\u00b7ner", "Wand\u00b7rer", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "VVFIN", "NN", "PPER", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bist du es, Knabe Lenz?\u201c", "tokens": ["Bist", "du", "es", ",", "Kna\u00b7be", "Lenz", "?", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "NN", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Er rief: \u201eIch bin kein Andrer", "tokens": ["Er", "rief", ":", "\u201e", "Ich", "bin", "kein", "A\u00b7ndrer"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "$(", "PPER", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und komme von Florenz!\u201c", "tokens": ["Und", "kom\u00b7me", "von", "Flo\u00b7renz", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "APPR", "NE", "$.", "$("], "meter": "-+-++-", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Das mu\u00dfte mich erwecken.", "tokens": ["Das", "mu\u00df\u00b7te", "mich", "er\u00b7we\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "\u201ekind Lenz, ich wandre mit!\u201c", "tokens": ["\u201e", "kind", "Lenz", ",", "ich", "wand\u00b7re", "mit", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "PPER", "VVFIN", "PTKVZ", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir hoben unsre Stecken", "tokens": ["Wir", "ho\u00b7ben", "uns\u00b7re", "Ste\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In ", "tokens": ["In"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.5": {"line.1": {"text": "Die beiden St\u00e4be hoben", "tokens": ["Die", "bei\u00b7den", "St\u00e4\u00b7be", "ho\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Kind Lenz und ich zugleich;", "tokens": ["Kind", "Lenz", "und", "ich", "zu\u00b7gleich", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Auch meiner ward von oben", "tokens": ["Auch", "mei\u00b7ner", "ward", "von", "o\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "VAFIN", "APPR", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis unten bl\u00fcthenreich.", "tokens": ["Bis", "un\u00b7ten", "bl\u00fc\u00b7then\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}