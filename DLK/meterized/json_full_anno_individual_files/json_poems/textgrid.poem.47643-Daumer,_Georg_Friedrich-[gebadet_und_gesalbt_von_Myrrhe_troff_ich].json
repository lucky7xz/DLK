{"textgrid.poem.47643": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[gebadet und gesalbt von Myrrhe troff ich]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gebadet und gesalbt von Myrrhe troff ich,", "tokens": ["Ge\u00b7ba\u00b7det", "und", "ge\u00b7salbt", "von", "Myr\u00b7rhe", "troff", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von k\u00f6stlicher, balsamischen Geruchs;", "tokens": ["Von", "k\u00f6st\u00b7li\u00b7cher", ",", "bal\u00b7sa\u00b7mi\u00b7schen", "Ge\u00b7ruchs", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich harrte des Geliebten in der Nacht,", "tokens": ["Ich", "harr\u00b7te", "des", "Ge\u00b7lieb\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich harrete vergeblich und entschlief.", "tokens": ["Ich", "har\u00b7re\u00b7te", "ver\u00b7geb\u00b7lich", "und", "ent\u00b7schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da pocht es, horch! \u2013 Des Freundes Stimme t\u00f6nt:", "tokens": ["Da", "pocht", "es", ",", "horch", "!", "\u2013", "Des", "Freun\u00b7des", "Stim\u00b7me", "t\u00f6nt", ":"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "VVIMP", "$.", "$(", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "\u00bbthu' auf, o meine traute Schwesterseele,", "tokens": ["\u00bb", "thu'", "auf", ",", "o", "mei\u00b7ne", "trau\u00b7te", "Schwes\u00b7ter\u00b7see\u00b7le", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PTKVZ", "$,", "FM", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Thu' auf, o meine zarte Taube mir!", "tokens": ["Thu'", "auf", ",", "o", "mei\u00b7ne", "zar\u00b7te", "Tau\u00b7be", "mir", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "FM", "PPOSAT", "ADJA", "NN", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Feucht ist mein Haupt, es tr\u00e4uft die Locke mir", "tokens": ["Feucht", "ist", "mein", "Haupt", ",", "es", "tr\u00e4uft", "die", "Lo\u00b7cke", "mir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "ART", "NN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Vom Thau der Nacht; o s\u00e4ume nicht und \u00f6ffne!\u00ab", "tokens": ["Vom", "Thau", "der", "Nacht", ";", "o", "s\u00e4u\u00b7me", "nicht", "und", "\u00f6ff\u00b7ne", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$.", "FM", "FM", "PTKNEG", "KON", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Beraubt der Sinne bin ich vor Entz\u00fccken", "tokens": ["Be\u00b7raubt", "der", "Sin\u00b7ne", "bin", "ich", "vor", "Ent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "VAFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Ob dieser Stimme Klang; es schl\u00e4gt mein Herz", "tokens": ["Ob", "die\u00b7ser", "Stim\u00b7me", "Klang", ";", "es", "schl\u00e4gt", "mein", "Herz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "NN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Mit lautem Schlage seinem Gl\u00fcck entgegen;", "tokens": ["Mit", "lau\u00b7tem", "Schla\u00b7ge", "sei\u00b7nem", "Gl\u00fcck", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Ich fliege, rei\u00dfe den Riegel rasch zur\u00fcck", "tokens": ["Ich", "flie\u00b7ge", ",", "rei\u00b7\u00dfe", "den", "Rie\u00b7gel", "rasch", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "ART", "NN", "ADJD", "PTKVZ"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und sp\u00e4h' erschrocken in die leere Nacht,", "tokens": ["Und", "sp\u00e4h'", "er\u00b7schro\u00b7cken", "in", "die", "lee\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Entwichen ist, verschwunden ohne Spur", "tokens": ["Ent\u00b7wi\u00b7chen", "ist", ",", "ver\u00b7schwun\u00b7den", "oh\u00b7ne", "Spur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "VVPP", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Mein s\u00fc\u00dfes Heil. Ich rufe \u2013 niemand h\u00f6rt!", "tokens": ["Mein", "s\u00fc\u00b7\u00dfes", "Heil", ".", "Ich", "ru\u00b7fe", "\u2013", "nie\u00b7mand", "h\u00f6rt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "PPER", "VVFIN", "$(", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.17": {"text": "Ich h\u00fclle mich in meinen Mantel ein,", "tokens": ["Ich", "h\u00fcl\u00b7le", "mich", "in", "mei\u00b7nen", "Man\u00b7tel", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.18": {"text": "Ich walle manche dunkle Stra\u00dfe hin,", "tokens": ["Ich", "wal\u00b7le", "man\u00b7che", "dunk\u00b7le", "Stra\u00b7\u00dfe", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.19": {"text": "Ich wage mich in's freie Gefild hinaus,", "tokens": ["Ich", "wa\u00b7ge", "mich", "in's", "frei\u00b7e", "Ge\u00b7fild", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "APZR", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.20": {"text": "Ich suche meinen Freund und such' umsonst.", "tokens": ["Ich", "su\u00b7che", "mei\u00b7nen", "Freund", "und", "such'", "um\u00b7sonst", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Da finden mich die W\u00e4chter, die die Mauern", "tokens": ["Da", "fin\u00b7den", "mich", "die", "W\u00e4ch\u00b7ter", ",", "die", "die", "Mau\u00b7ern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NE", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Der Stadt umwandeln; sie ergreifen mich,", "tokens": ["Der", "Stadt", "um\u00b7wan\u00b7deln", ";", "sie", "er\u00b7grei\u00b7fen", "mich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$.", "PPER", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "Sie rei\u00dfen mir den Mantel ab, sie schlagen", "tokens": ["Sie", "rei\u00b7\u00dfen", "mir", "den", "Man\u00b7tel", "ab", ",", "sie", "schla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Mich weh und wund mit ihrer rauhen Faust \u2013", "tokens": ["Mich", "weh", "und", "wund", "mit", "ih\u00b7rer", "rau\u00b7hen", "Faust", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "KON", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Da wach' ich auf und merk', es ist ein Traum,", "tokens": ["Da", "wach'", "ich", "auf", "und", "merk'", ",", "es", "ist", "ein", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Ein b\u00f6ser Traum. \u2013 O ich beschw\u00f6r' euch, T\u00f6chter", "tokens": ["Ein", "b\u00f6\u00b7ser", "Traum", ".", "\u2013", "O", "ich", "be\u00b7schw\u00f6r'", "euch", ",", "T\u00f6ch\u00b7ter"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NE", "PPER", "VVFIN", "PPER", "$,", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Jerusalems, begegnet euch mein Freund,", "tokens": ["Je\u00b7ru\u00b7sa\u00b7lems", ",", "be\u00b7geg\u00b7net", "euch", "mein", "Freund", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "Sagt ihm, da\u00df ich vor Liebe matt und krank,", "tokens": ["Sagt", "ihm", ",", "da\u00df", "ich", "vor", "Lie\u00b7be", "matt", "und", "krank", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "PPER", "APPR", "NN", "PTKVZ", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Thut ihm die Leiden meiner Seele kund,", "tokens": ["Thut", "ihm", "die", "Lei\u00b7den", "mei\u00b7ner", "See\u00b7le", "kund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Beschw\u00f6ret ihn, so wie ich euch beschw\u00f6re:", "tokens": ["Be\u00b7schw\u00f6\u00b7ret", "ihn", ",", "so", "wie", "ich", "euch", "be\u00b7schw\u00f6\u00b7re", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADV", "KOKOM", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Er eile, komme, k\u00fcsse mich gesund!", "tokens": ["Er", "ei\u00b7le", ",", "kom\u00b7me", ",", "k\u00fcs\u00b7se", "mich", "ge\u00b7sund", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Gebadet und gesalbt von Myrrhe troff ich,", "tokens": ["Ge\u00b7ba\u00b7det", "und", "ge\u00b7salbt", "von", "Myr\u00b7rhe", "troff", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "VVPP", "APPR", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Von k\u00f6stlicher, balsamischen Geruchs;", "tokens": ["Von", "k\u00f6st\u00b7li\u00b7cher", ",", "bal\u00b7sa\u00b7mi\u00b7schen", "Ge\u00b7ruchs", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich harrte des Geliebten in der Nacht,", "tokens": ["Ich", "harr\u00b7te", "des", "Ge\u00b7lieb\u00b7ten", "in", "der", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich harrete vergeblich und entschlief.", "tokens": ["Ich", "har\u00b7re\u00b7te", "ver\u00b7geb\u00b7lich", "und", "ent\u00b7schlief", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da pocht es, horch! \u2013 Des Freundes Stimme t\u00f6nt:", "tokens": ["Da", "pocht", "es", ",", "horch", "!", "\u2013", "Des", "Freun\u00b7des", "Stim\u00b7me", "t\u00f6nt", ":"], "token_info": ["word", "word", "word", "punct", "wor