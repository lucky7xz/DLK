{"dta.poem.20935": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Abelard an Heloissen.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Mein Schreiben ist verderbt/ die Feder ist verschnitte\u0303", "tokens": ["Mein", "Schrei\u00b7ben", "ist", "ver\u00b7derbt", "/", "die", "Fe\u00b7der", "ist", "ver\u00b7schnit\u00b7t\u1ebd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "ART", "NN", "VAFIN", "NE"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Tinte fleust nicht mehr/ wie sie zuvor gethan/", "tokens": ["Die", "Tin\u00b7te", "fleust", "nicht", "mehr", "/", "wie", "sie", "zu\u00b7vor", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$(", "PWAV", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird ein kleiner Brief dich umb Verzeihung", "tokens": ["Es", "wird", "ein", "klei\u00b7ner", "Brief", "dich", "umb", "Ver\u00b7zei\u00b7hung"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PRF", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "bitten", "tokens": ["bit\u00b7ten"], "token_info": ["word"], "pos": ["VVINF"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Da\u00df ich forthin als Mann/ dich nicht bedienen kan.", "tokens": ["Da\u00df", "ich", "for\u00b7thin", "als", "Mann", "/", "dich", "nicht", "be\u00b7die\u00b7nen", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "KOUS", "NN", "$(", "PPER", "PTKNEG", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dein Abelard ist nicht/ was er zuvor gewesen/", "tokens": ["Dein", "A\u00b7be\u00b7lard", "ist", "nicht", "/", "was", "er", "zu\u00b7vor", "ge\u00b7we\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PTKNEG", "$(", "PWS", "PPER", "ADV", "VAPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Er fl\u00f6st dir k\u00fcnftig nicht die Zucker-Tropfen ein", "tokens": ["Er", "fl\u00f6st", "dir", "k\u00fcnf\u00b7tig", "nicht", "die", "Zu\u00b7cke\u00b7rTrop\u00b7fen", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "PTKNEG", "ART", "NN", "ART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Du kanst bey mir nicht mehr die Liebes-Apfel lesen.", "tokens": ["Du", "kanst", "bey", "mir", "nicht", "mehr", "die", "Lie\u00b7bes\u00b7Ap\u00b7fel", "le\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dich heist man ohne Lust/ mich ohne Kr\u00e4ften seyn.", "tokens": ["Dich", "heist", "man", "oh\u00b7ne", "Lust", "/", "mich", "oh\u00b7ne", "Kr\u00e4f\u00b7ten", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "NN", "$(", "PPER", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Kein fleischlich Jubel-Jahr ist mehr von mir zu hoffe\u0303", "tokens": ["Kein", "fleischlich", "Ju\u00b7bel\u00b7Jahr", "ist", "mehr", "von", "mir", "zu", "hoff\u1ebd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "PPER", "APPR", "NE"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.11": {"text": "Nach dem ich lebenslang die Faste halten mu\u00df/", "tokens": ["Nach", "dem", "ich", "le\u00b7bens\u00b7lang", "die", "Fas\u00b7te", "hal\u00b7ten", "mu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Das Messer/ so mich schnied/ das hat dich auch ge-", "tokens": ["Das", "Mes\u00b7ser", "/", "so", "mich", "schnied", "/", "das", "hat", "dich", "auch", "ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$(", "PDS", "VAFIN", "PPER", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "troffen", "tokens": ["trof\u00b7fen"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.14": {"text": "Man g\u00f6nnt dir ferner nichts als einen schlaffen Ku\u00df", "tokens": ["Man", "g\u00f6nnt", "dir", "fer\u00b7ner", "nichts", "als", "ei\u00b7nen", "schlaf\u00b7fen", "Ku\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "PIS", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Helisse meynt vielleicht/ da\u00df ich ein Retzel schreibe", "tokens": ["He\u00b7lis\u00b7se", "meynt", "viel\u00b7leicht", "/", "da\u00df", "ich", "ein", "Ret\u00b7zel", "schrei\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "$(", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und ein verw\u00f6rrter Schertz den Brieff bekleiden soll/", "tokens": ["Und", "ein", "ver\u00b7w\u00f6rr\u00b7ter", "Schertz", "den", "Brieff", "be\u00b7klei\u00b7den", "soll", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Nein! was die Seele qv\u00e4lt/ das qvilt aus meinem", "tokens": ["Nein", "!", "was", "die", "See\u00b7le", "qv\u00e4lt", "/", "das", "qvilt", "aus", "mei\u00b7nem"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "PWS", "ART", "NN", "VVFIN", "$(", "PDS", "VVFIN", "APPR", "PPOSAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Leibe/", "tokens": ["Lei\u00b7be", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Sie ist der kalten Angst/ er heisser Schmertzen voll.", "tokens": ["Sie", "ist", "der", "kal\u00b7ten", "Angst", "/", "er", "heis\u00b7ser", "Schmert\u00b7zen", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "PPER", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wo ist der edle Lentz/ wo bleibt die s\u00fcsse Stunde/", "tokens": ["Wo", "ist", "der", "ed\u00b7le", "Lentz", "/", "wo", "bleibt", "die", "s\u00fcs\u00b7se", "Stun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "ADJA", "NN", "$(", "PWAV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Als mich der heisse Strahl der Liebes Sonne stach/", "tokens": ["Als", "mich", "der", "heis\u00b7se", "Strahl", "der", "Lie\u00b7bes", "Son\u00b7ne", "stach", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Als ich die Negeln dir auf dem Zinober Munde/", "tokens": ["Als", "ich", "die", "Ne\u00b7geln", "dir", "auf", "dem", "Zi\u00b7no\u00b7ber", "Mun\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Und in der engen Scho\u00df die Zucker-Rosen brach.", "tokens": ["Und", "in", "der", "en\u00b7gen", "Scho\u00df", "die", "Zu\u00b7cke\u00b7rRo\u00b7sen", "brach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ich kan im Geiste noch den s\u00fcssen Honig schmecken/", "tokens": ["Ich", "kan", "im", "Geis\u00b7te", "noch", "den", "s\u00fcs\u00b7sen", "Ho\u00b7nig", "schme\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der mir aus deinem Mund auf meinen Lippen flo\u00df.", "tokens": ["Der", "mir", "aus", "dei\u00b7nem", "Mund", "auf", "mei\u00b7nen", "Lip\u00b7pen", "flo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Was eingeschlafen lag/ das kontest du erwecken/", "tokens": ["Was", "ein\u00b7ge\u00b7schla\u00b7fen", "lag", "/", "das", "kon\u00b7test", "du", "er\u00b7we\u00b7cken", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVIZU", "VVFIN", "$(", "PDS", "VMFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Du warst mein Seelen Zug und ich dein Leibgeno\u00df.", "tokens": ["Du", "warst", "mein", "See\u00b7len", "Zug", "und", "ich", "dein", "Leib\u00b7ge\u00b7no\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "NN", "KON", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die s\u00fcsse K\u00fctzelung die spielt mir noch im Hertzen/", "tokens": ["Die", "s\u00fcs\u00b7se", "K\u00fct\u00b7ze\u00b7lung", "die", "spielt", "mir", "noch", "im", "Hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Als in dem warmen Schnee ich rothe Beeren la\u00df/", "tokens": ["Als", "in", "dem", "war\u00b7men", "Schnee", "ich", "ro\u00b7the", "Bee\u00b7ren", "la\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "PPER", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Recht satt von Buhlerey/ und voll von Liebes-", "tokens": ["Recht", "satt", "von", "Buh\u00b7le\u00b7rey", "/", "und", "voll", "von", "Lie\u00b7bes"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADJD", "APPR", "NN", "$(", "KON", "ADJD", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.31": {"text": "Schertzen/", "tokens": ["Schert\u00b7zen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.32": {"text": "Auf des Gel\u00fcckes Scho\u00df/ und auch auf deiner sa\u00df.", "tokens": ["Auf", "des", "Ge\u00b7l\u00fc\u00b7ckes", "Scho\u00df", "/", "und", "auch", "auf", "dei\u00b7ner", "sa\u00df", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$(", "KON", "ADV", "APPR", "PPOSAT", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Mein Fr\u00fchling ist verbl\u00fcth/ es ist mein Winter kom\u0303e\u0303", "tokens": ["Mein", "Fr\u00fch\u00b7ling", "ist", "ver\u00b7bl\u00fcth", "/", "es", "ist", "mein", "Win\u00b7ter", "kom\u0303\u1ebd"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "PPER", "VAFIN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die nackte Liebe scheut erkalten Reif und Schnee/", "tokens": ["Die", "nack\u00b7te", "Lie\u00b7be", "scheut", "er\u00b7kal\u00b7ten", "Reif", "und", "Schnee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Dein falscher Vetter hat mir meine\u0303 schatz genom\u0303en/", "tokens": ["Dein", "fal\u00b7scher", "Vet\u00b7ter", "hat", "mir", "mein\u1ebd", "schatz", "ge\u00b7nom\u0303en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "Er stielt mir meine Lust/ un\u0303 schenckt mir Ach un\u0303 Weh.", "tokens": ["Er", "stielt", "mir", "mei\u00b7ne", "Lust", "/", "u\u00f1", "schenckt", "mir", "Ach", "u\u00f1", "Weh", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Er kan mich f\u00fcglich nicht von deiner Seite treiben/", "tokens": ["Er", "kan", "mich", "f\u00fcg\u00b7lich", "nicht", "von", "dei\u00b7ner", "Sei\u00b7te", "trei\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "So raubt sein Henckers Sin\u0303/ mich endlich selber mir/", "tokens": ["So", "raubt", "sein", "Hen\u00b7ckers", "Si\u00f1", "/", "mich", "end\u00b7lich", "sel\u00b7ber", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "NE", "$(", "PPER", "ADV", "ADV", "PPER", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Was m\u00e4nlich in mir lag/ da\u00df hie\u00df er mir entleiben.", "tokens": ["Was", "m\u00e4n\u00b7lich", "in", "mir", "lag", "/", "da\u00df", "hie\u00df", "er", "mir", "ent\u00b7lei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "APPR", "PPER", "VVFIN", "$(", "KOUS", "VVFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Vor Perlen findest du die leere Muschel hier.", "tokens": ["Vor", "Per\u00b7len", "fin\u00b7dest", "du", "die", "lee\u00b7re", "Mu\u00b7schel", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Ach wie verfolget mich das fl\u00fcchtige Gel\u00fccke/", "tokens": ["Ach", "wie", "ver\u00b7fol\u00b7get", "mich", "das", "fl\u00fcch\u00b7ti\u00b7ge", "Ge\u00b7l\u00fc\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VVFIN", "PPER", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Ich meynt es richte mir ein Bett\u2019 aus Liljen zu/", "tokens": ["Ich", "meynt", "es", "rich\u00b7te", "mir", "ein", "Bett'", "aus", "Lil\u00b7jen", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "PTKZU", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Ich w\u00e4r\u2019 ein weisser Zweck von seinem Liebes Blicke.", "tokens": ["Ich", "w\u00e4r'", "ein", "weis\u00b7ser", "Zweck", "von", "sei\u00b7nem", "Lie\u00b7bes", "Bli\u00b7cke", "."]