{"dta.poem.13616": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Sommerschw\u00fcle .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "O weg mit Reim und Leierklang und Singen!", "tokens": ["O", "weg", "mit", "Reim", "und", "Lei\u00b7er\u00b7klang", "und", "Sin\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "KON", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fa\u00df', Leben, wieder mich lebendig an!", "tokens": ["Fa\u00df'", ",", "Le\u00b7ben", ",", "wie\u00b7der", "mich", "le\u00b7ben\u00b7dig", "an", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "NN", "$,", "ADV", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Deiner Woge will ich freudig ringen,", "tokens": ["Mit", "Dei\u00b7ner", "Wo\u00b7ge", "will", "ich", "freu\u00b7dig", "rin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die tief mich st\u00fcrzt, hebt mich auch himmelan.", "tokens": ["Die", "tief", "mich", "st\u00fcrzt", ",", "hebt", "mich", "auch", "him\u00b7me\u00b7lan", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Im Sturme spannt der Adler seine Schwingen \u2014", "tokens": ["Im", "Stur\u00b7me", "spannt", "der", "Ad\u00b7ler", "sei\u00b7ne", "Schwin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Blas' zu! da sp\u00fcr' ich wieder, da\u00df ich Mann!", "tokens": ["Blas'", "zu", "!", "da", "sp\u00fcr'", "ich", "wie\u00b7der", ",", "da\u00df", "ich", "Mann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "ADV", "VVFIN", "PPER", "ADV", "$,", "KOUS", "PPER", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Viel lieber will ich raschen Tod erwerben,", "tokens": ["Viel", "lie\u00b7ber", "will", "ich", "ra\u00b7schen", "Tod", "er\u00b7wer\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Als, so verschmachtend, lebenslang zu sterben.", "tokens": ["Als", ",", "so", "ver\u00b7schmach\u00b7tend", ",", "le\u00b7bens\u00b7lang", "zu", "ster\u00b7ben", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "ADV", "VVPP", "$,", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}