{"textgrid.poem.48765": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "52. Mons. Christian Herpichen, F\u00fcrstl. Holsteinischer Gesandten Violgambisten", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin zu fr\u00fch' erwacht von deines Tages Flammen,", "tokens": ["Ich", "bin", "zu", "fr\u00fch'", "er\u00b7wacht", "von", "dei\u00b7nes", "Ta\u00b7ges", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der mehr als andre gl\u00e4nzt, ich bin zu zeitlich hier.", "tokens": ["der", "mehr", "als", "and\u00b7re", "gl\u00e4nzt", ",", "ich", "bin", "zu", "zeit\u00b7lich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch gib das Bretspiel her und nim es an mit mir,", "tokens": ["Doch", "gib", "das", "Bret\u00b7spiel", "her", "und", "nim", "es", "an", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "KON", "VVIMP", "PPER", "APPR", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "es gilt mir beides gleich verkehren oder dammen.", "tokens": ["es", "gilt", "mir", "bei\u00b7des", "gleich", "ver\u00b7keh\u00b7ren", "o\u00b7der", "dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "ADV", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Indessen findet sich die ganze Zunft zusammen,", "tokens": ["In\u00b7des\u00b7sen", "fin\u00b7det", "sich", "die", "gan\u00b7ze", "Zunft", "zu\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "die auf dich fr\u00f6lich ist. Denn setz' uns feistes Bier", "tokens": ["die", "auf", "dich", "fr\u00f6\u00b7lich", "ist", ".", "Denn", "setz'", "uns", "feis\u00b7tes", "Bier"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PPER", "ADJD", "VAFIN", "$.", "KON", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und reinen Reinfall f\u00fcr, der reicher wird an Zier,", "tokens": ["und", "rei\u00b7nen", "Re\u00b7in\u00b7fall", "f\u00fcr", ",", "der", "rei\u00b7cher", "wird", "an", "Zier", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "$,", "PRELS", "ADJD", "VAFIN", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "wenn du eins streichest drein auf deiner Violgammen.", "tokens": ["wenn", "du", "eins", "strei\u00b7chest", "drein", "auf", "dei\u00b7ner", "Vi\u00b7ol\u00b7gam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So la\u00df uns heute tun, so la\u00df uns morgen machen,", "tokens": ["So", "la\u00df", "uns", "heu\u00b7te", "tun", ",", "so", "la\u00df", "uns", "mor\u00b7gen", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "VVINF", "$,", "ADV", "VVIMP", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "bis da\u00df der dritte Tag uns fr\u00f6lich an wird lachen,", "tokens": ["bis", "da\u00df", "der", "drit\u00b7te", "Tag", "uns", "fr\u00f6\u00b7lich", "an", "wird", "la\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "ART", "ADJA", "NN", "PPER", "ADJD", "PTKVZ", "VAFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "den wir auch geben drein. Ich lobe diesen Man,", "tokens": ["den", "wir", "auch", "ge\u00b7ben", "drein", ".", "Ich", "lo\u00b7be", "die\u00b7sen", "Man", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "PTKVZ", "$.", "PPER", "VVFIN", "PDAT", "PIS", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "der seiner Zeit gebraucht, weil er ihr kan genie\u00dfen,", "tokens": ["der", "sei\u00b7ner", "Zeit", "ge\u00b7braucht", ",", "weil", "er", "ihr", "kan", "ge\u00b7nie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "PPER", "PPER", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "und unverdrossen ist sein Leben auch zu schlie\u00dfen,", "tokens": ["und", "un\u00b7ver\u00b7dros\u00b7sen", "ist", "sein", "Le\u00b7ben", "auch", "zu", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn er noch lachen mag und l\u00e4nger leben kan.", "tokens": ["wenn", "er", "noch", "la\u00b7chen", "mag", "und", "l\u00e4n\u00b7ger", "le\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "KON", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich bin zu fr\u00fch' erwacht von deines Tages Flammen,", "tokens": ["Ich", "bin", "zu", "fr\u00fch'", "er\u00b7wacht", "von", "dei\u00b7nes", "Ta\u00b7ges", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "VVPP", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "der mehr als andre gl\u00e4nzt, ich bin zu zeitlich hier.", "tokens": ["der", "mehr", "als", "and\u00b7re", "gl\u00e4nzt", ",", "ich", "bin", "zu", "zeit\u00b7lich", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "KOKOM", "PIS", "VVFIN", "$,", "PPER", "VAFIN", "PTKA", "ADJD", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Doch gib das Bretspiel her und nim es an mit mir,", "tokens": ["Doch", "gib", "das", "Bret\u00b7spiel", "her", "und", "nim", "es", "an", "mit", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "ART", "NN", "PTKVZ", "KON", "VVIMP