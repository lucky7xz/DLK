{"dta.poem.10976": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Zuk\u00fcnftige.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie ein Vorwurf, welcher nah, unsern Augen gr\u00f6\u00dfer\nscheinet,", "tokens": ["Wie", "ein", "Vor\u00b7wurf", ",", "wel\u00b7cher", "nah", ",", "un\u00b7sern", "Au\u00b7gen", "gr\u00f6\u00b7\u00dfer", "schei\u00b7net", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "PRELS", "ADJD", "$,", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Als ein ferner, welcher gr\u00f6\u00dfer: Also glaub ich, da\u00df man", "tokens": ["Als", "ein", "fer\u00b7ner", ",", "wel\u00b7cher", "gr\u00f6\u00b7\u00dfer", ":", "Al\u00b7so", "glaub", "ich", ",", "da\u00df", "man"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "ART", "ADV", "$,", "PRELS", "ADJD", "$.", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PIS"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Auch von einer nahen Wollust, da\u00df dieselbe gr\u00f6\u00dfer schier,", "tokens": ["Auch", "von", "ei\u00b7ner", "na\u00b7hen", "Wol\u00b7lust", ",", "da\u00df", "die\u00b7sel\u00b7be", "gr\u00f6\u00b7\u00dfer", "schier", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "$,", "KOUS", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Als die noch weit gr\u00f6\u00dfern L\u00fcste des noch fernen Himmels,", "tokens": ["Als", "die", "noch", "weit", "gr\u00f6\u00b7\u00dfern", "L\u00fcs\u00b7te", "des", "noch", "fer\u00b7nen", "Him\u00b7mels", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADV", "ADJD", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Weil jedoch das, was zuk\u00fcnftig, doch auch gegenw\u00e4rtig", "tokens": ["Weil", "je\u00b7doch", "das", ",", "was", "zu\u00b7k\u00fcnf\u00b7tig", ",", "doch", "auch", "ge\u00b7gen\u00b7w\u00e4r\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ADV", "PDS", "$,", "PRELS", "ADJD", "$,", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.6": {"text": "Sieht man leicht, wie sehr ein jeder, in der Meynung, sich", "tokens": ["Sieht", "man", "leicht", ",", "wie", "sehr", "ein", "je\u00b7der", ",", "in", "der", "Mey\u00b7nung", ",", "sich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PIS", "ADJD", "$,", "PWAV", "ADV", "ART", "PIS", "$,", "APPR", "ART", "NN", "$,", "PRF"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}}}}