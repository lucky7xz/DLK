{"dta.poem.9721": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Sie foderte im spiel hertzen.  \n C. E.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du spielest hertzen aus/ und sprichst zu mir: bekennt!", "tokens": ["Du", "spie\u00b7lest", "hert\u00b7zen", "aus", "/", "und", "sprichst", "zu", "mir", ":", "be\u00b7kennt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PTKVZ", "$(", "KON", "VVFIN", "APPR", "PPER", "$.", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da doch mein hertze l\u00e4ngst vom leibe mir getrennt:", "tokens": ["Da", "doch", "mein", "hert\u00b7ze", "l\u00e4ngst", "vom", "lei\u00b7be", "mir", "ge\u00b7trennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "APPRART", "VVFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ich habe denn kein hertz/ als nur des hertzens rinden/", "tokens": ["Ich", "ha\u00b7be", "denn", "kein", "hertz", "/", "als", "nur", "des", "hert\u00b7zens", "rin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PIAT", "NN", "$(", "KOKOM", "ADV", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wiltu mein hertze selbst/ kanstu bey dir es finden.", "tokens": ["Wil\u00b7tu", "mein", "hert\u00b7ze", "selbst", "/", "kans\u00b7tu", "bey", "dir", "es", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "ADV", "$(", "VMFIN", "APPR", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}