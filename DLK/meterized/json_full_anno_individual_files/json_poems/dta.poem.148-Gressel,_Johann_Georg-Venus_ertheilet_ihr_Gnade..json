{"dta.poem.148": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Venus  ertheilet ihr Gnade.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Weil dir dein Fehler leid/ und weil du dich geb\u00fcckt", "tokens": ["Weil", "dir", "dein", "Feh\u00b7ler", "leid", "/", "und", "weil", "du", "dich", "ge\u00b7b\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "ADJD", "$(", "KON", "KOUS", "PPER", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In Unterth\u00e4nigkeit zu meinen weissen F\u00fcssen", "tokens": ["In", "Un\u00b7ter\u00b7th\u00e4\u00b7nig\u00b7keit", "zu", "mei\u00b7nen", "weis\u00b7sen", "F\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und weil den Frevelmuth der Buhler b\u00fcssen m\u00fcssen/", "tokens": ["Und", "weil", "den", "Fre\u00b7vel\u00b7muth", "der", "Buh\u00b7ler", "b\u00fcs\u00b7sen", "m\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So bist du nun begnadt/ mein Zorn ist fortgeschickt.", "tokens": ["So", "bist", "du", "nun", "be\u00b7gnadt", "/", "mein", "Zorn", "ist", "fort\u00b7ge\u00b7schickt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$(", "PPOSAT", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du zierst durch deine Bu\u00df mein lichtes Rosen-Haupt", "tokens": ["Du", "zierst", "durch", "dei\u00b7ne", "Bu\u00df", "mein", "lich\u00b7tes", "Ro\u00b7sen\u00b7Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Darum du einen Krantz von gr\u00fcnen Lorbern windest.", "tokens": ["Da\u00b7rum", "du", "ei\u00b7nen", "Krantz", "von", "gr\u00fc\u00b7nen", "Lor\u00b7bern", "win\u00b7dest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Indem du dich bey mir mit Reu und Leid einfindest/", "tokens": ["In\u00b7dem", "du", "dich", "bey", "mir", "mit", "Reu", "und", "Leid", "ein\u00b7fin\u00b7dest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "APPR", "NE", "KON", "NN", "VVFIN", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.8": {"text": "So hast du mich dadurch mit Myrthen-Laub belaubt.", "tokens": ["So", "hast", "du", "mich", "da\u00b7durch", "mit", "Myr\u00b7then\u00b7Laub", "be\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "PAV", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn bey der ", "tokens": ["Wenn", "bey", "der"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "APPR", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.10": {"text": "Wenn Weisheit und Verstand die ", "tokens": ["Wenn", "Weis\u00b7heit", "und", "Ver\u00b7stand", "die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So mu\u00df mir ", "tokens": ["So", "mu\u00df", "mir"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER"], "meter": "+--", "measure": "dactylic.init"}, "line.12": {"text": "Der meine Pracht allein vor allen liebgewinnt.", "tokens": ["Der", "mei\u00b7ne", "Pracht", "al\u00b7lein", "vor", "al\u00b7len", "lieb\u00b7ge\u00b7winnt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "APPR", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "So hat denn meine Zier den Sieg allein erlangt/", "tokens": ["So", "hat", "denn", "mei\u00b7ne", "Zier", "den", "Sieg", "al\u00b7lein", "er\u00b7langt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Der rauhe ", "tokens": ["Der", "rau\u00b7he"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "Er gibt den Zeugen ab von meinem sch\u00f6nen Siege/", "tokens": ["Er", "gibt", "den", "Zeu\u00b7gen", "ab", "von", "mei\u00b7nem", "sch\u00f6\u00b7nen", "Sie\u00b7ge", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und wie ich da so sch\u00f6n mit Pracht und Schmuck geprangt.", "tokens": ["Und", "wie", "ich", "da", "so", "sch\u00f6n", "mit", "Pracht", "und", "Schmuck", "ge\u00b7prangt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADV", "ADJD", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Jhr ", "tokens": ["Ihr"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.18": {"text": "Ich habe dieses mahl auch wieder \u00fcberwunden", "tokens": ["Ich", "ha\u00b7be", "die\u00b7ses", "mahl", "auch", "wie\u00b7der", "\u00fc\u00b7berw\u00b7un\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDAT", "ADV", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Lysett' hat sich bey mir in Demuth eingefunden/", "tokens": ["Ly\u00b7sett'", "hat", "sich", "bey", "mir", "in", "De\u00b7muth", "ein\u00b7ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PRF", "APPR", "PPER", "APPR", "NN", "VVPP", "$("], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.20": {"text": "Nun werd\u2019 ich K\u00f6nig in der Lieb\u2019 und Sch\u00f6nheit seyn.", "tokens": ["Nun", "werd'", "ich", "K\u00f6\u00b7nig", "in", "der", "Lieb'", "und", "Sch\u00f6n\u00b7heit", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "NN", "APPR", "ART", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}