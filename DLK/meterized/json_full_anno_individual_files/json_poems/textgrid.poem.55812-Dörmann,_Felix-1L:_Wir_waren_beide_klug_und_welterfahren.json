{"textgrid.poem.55812": {"metadata": {"author": {"name": "D\u00f6rmann, Felix", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wir waren beide klug und welterfahren", "genre": "verse", "period": "N.A.", "pub_year": 1857, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wir waren beide klug und welterfahren", "tokens": ["Wir", "wa\u00b7ren", "bei\u00b7de", "klug", "und", "wel\u00b7ter\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIS", "ADJD", "KON", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und hatten viel geliebt und viel vergessen", "tokens": ["Und", "hat\u00b7ten", "viel", "ge\u00b7liebt", "und", "viel", "ver\u00b7ges\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "ADV", "VVPP", "KON", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und alle beide scheuten wir die Liebe.", "tokens": ["Und", "al\u00b7le", "bei\u00b7de", "scheu\u00b7ten", "wir", "die", "Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PIS", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wir wussten ja genau, o so genau,", "tokens": ["Wir", "wuss\u00b7ten", "ja", "ge\u00b7nau", ",", "o", "so", "ge\u00b7nau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "FM", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Dass Liebe th\u00f6richt oder elend mache", "tokens": ["Dass", "Lie\u00b7be", "th\u00f6\u00b7richt", "o\u00b7der", "e\u00b7lend", "ma\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ADJD", "KON", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und dass der Augenblick uns sicher gr\u00fc\u00dfte,", "tokens": ["Und", "dass", "der", "Au\u00b7gen\u00b7blick", "uns", "si\u00b7cher", "gr\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Wo wir bereuten, dass wir menschlich schwach", "tokens": ["Wo", "wir", "be\u00b7reu\u00b7ten", ",", "dass", "wir", "menschlich", "schwach"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.8": {"text": "Den feingepflanzten Trieben der Natur", "tokens": ["Den", "fein\u00b7ge\u00b7pflanz\u00b7ten", "Trie\u00b7ben", "der", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Erlegen, dass wir uns der Leidenschaft", "tokens": ["Er\u00b7le\u00b7gen", ",", "dass", "wir", "uns", "der", "Lei\u00b7den\u00b7schaft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "KOUS", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Der unbequem-brutalen \u00fcberlassen.", "tokens": ["Der", "un\u00b7be\u00b7quem\u00b7bru\u00b7ta\u00b7len", "\u00fc\u00b7ber\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Und \u00fcber\u00e4ngstlich fast vermieden wir", "tokens": ["Und", "\u00fc\u00b7be\u00b7r\u00e4ngst\u00b7lich", "fast", "ver\u00b7mie\u00b7den", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Begegnung und Gespr\u00e4ch, ja das Geringste,", "tokens": ["Be\u00b7geg\u00b7nung", "und", "Ge\u00b7spr\u00e4ch", ",", "ja", "das", "Ge\u00b7rings\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "ADV", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das irgendwie im Stand gewesen w\u00e4re", "tokens": ["Das", "ir\u00b7gend\u00b7wie", "im", "Stand", "ge\u00b7we\u00b7sen", "w\u00e4\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "APPRART", "NN", "VAPP", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Uns nah' zu bringen, leicht nur zu verkn\u00fcpfen.", "tokens": ["Uns", "nah'", "zu", "brin\u00b7gen", ",", "leicht", "nur", "zu", "ver\u00b7kn\u00fcp\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKZU", "VVINF", "$,", "ADJD", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da kam ein Tag, ein unvergess'ner Tag,", "tokens": ["Da", "kam", "ein", "Tag", ",", "ein", "un\u00b7ver\u00b7ge\u00b7ss'\u00b7ner", "Tag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wo sie Dich fanden mit verzerrten Lippen,", "tokens": ["Wo", "sie", "Dich", "fan\u00b7den", "mit", "ver\u00b7zerr\u00b7ten", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit fahlem Antlitz, starrem Auge, nah'", "tokens": ["Mit", "fah\u00b7lem", "Ant\u00b7litz", ",", "star\u00b7rem", "Au\u00b7ge", ",", "nah'"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Dem Tode schon, den Du berufen und", "tokens": ["Dem", "To\u00b7de", "schon", ",", "den", "Du", "be\u00b7ru\u00b7fen", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "PRELS", "PPER", "VVPP", "KON"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ersehnt mit trotzig-ungest\u00fcmer Lust,", "tokens": ["Er\u00b7sehnt", "mit", "trot\u00b7zig\u00b7un\u00b7ge\u00b7st\u00fc\u00b7mer", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Weil Du des Lebens schlammig-tr\u00fcbe Qual", "tokens": ["Weil", "Du", "des", "Le\u00b7bens", "schlam\u00b7mig\u00b7tr\u00fc\u00b7be", "Qual"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nicht l\u00e4nger tragen wolltest, tragen konntest,", "tokens": ["Nicht", "l\u00e4n\u00b7ger", "tra\u00b7gen", "woll\u00b7test", ",", "tra\u00b7gen", "konn\u00b7test", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVINF", "VMFIN", "$,", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Weil Deiner Seele m\u00e4chtigstes Gef\u00fchl", "tokens": ["Weil", "Dei\u00b7ner", "See\u00b7le", "m\u00e4ch\u00b7tigs\u00b7tes", "Ge\u00b7f\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Nach Liebe schrie, nach jener alten Liebe", "tokens": ["Nach", "Lie\u00b7be", "schrie", ",", "nach", "je\u00b7ner", "al\u00b7ten", "Lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Mit s\u00fc\u00dfem Anfang und mit schaalem Ende,", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "An\u00b7fang", "und", "mit", "schaa\u00b7lem", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Nach jener Liebe, die Du fliehen wolltest.", "tokens": ["Nach", "je\u00b7ner", "Lie\u00b7be", ",", "die", "Du", "flie\u00b7hen", "woll\u00b7test", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,", "PRELS", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und als Du so vor mir lagst, reglos, k\u00fchl,", "tokens": ["Und", "als", "Du", "so", "vor", "mir", "lagst", ",", "reg\u00b7los", ",", "k\u00fchl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "APPR", "PPER", "VVFIN", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und ich des Leides langverh\u00fcllte Spur,", "tokens": ["Und", "ich", "des", "Lei\u00b7des", "lang\u00b7ver\u00b7h\u00fcll\u00b7te", "Spur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Den dumpfen Groll, dass Dich der Tod verschm\u00e4ht,", "tokens": ["Den", "dum\u00b7pfen", "Groll", ",", "dass", "Dich", "der", "Tod", "ver\u00b7schm\u00e4ht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.15": {"text": "Die Lebensangst und Lebensgier zugleich,", "tokens": ["Die", "Le\u00b7ben\u00b7sangst", "und", "Le\u00b7bens\u00b7gier", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Und Deine ganze, stolze, qualverkl\u00e4rte", "tokens": ["Und", "Dei\u00b7ne", "gan\u00b7ze", ",", "stol\u00b7ze", ",", "qual\u00b7ver\u00b7kl\u00e4r\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Medusensch\u00f6nheit sah \u2013 da kam es pl\u00f6tzlich", "tokens": ["Me\u00b7du\u00b7sen\u00b7sch\u00f6n\u00b7heit", "sah", "\u2013", "da", "kam", "es", "pl\u00f6tz\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "$(", "ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Wie hei\u00dfer Thauwind \u00fcber mich! \u2013 Begrab'ne,", "tokens": ["Wie", "hei\u00b7\u00dfer", "Thau\u00b7wind", "\u00fc\u00b7ber", "mich", "!", "\u2013", "Be\u00b7grab'\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "PPER", "$.", "$(", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wilddunkle Sehnsucht stieg aus ihrem Sarg,", "tokens": ["Wild\u00b7dunk\u00b7le", "Sehn\u00b7sucht", "stieg", "aus", "ih\u00b7rem", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Aufrauschte donnertosend der Gef\u00fchle", "tokens": ["Auf\u00b7rauschte", "don\u00b7ner\u00b7to\u00b7send", "der", "Ge\u00b7f\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.21": {"text": "Vereister Flammenstrom und rollte brausend", "tokens": ["Ver\u00b7eis\u00b7ter", "Flam\u00b7mens\u00b7trom", "und", "roll\u00b7te", "brau\u00b7send"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "In gro\u00dfen Wellen durch die Seele hin \u2013 \u2013", "tokens": ["In", "gro\u00b7\u00dfen", "Wel\u00b7len", "durch", "die", "See\u00b7le", "hin", "\u2013", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "PTKVZ", "$(", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.23": {"text": "\u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013 \u2013", "tokens": ["\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013", "\u2013"], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.24": {"text": "Und heut' umfang' ich Deine warmdurchwogten,", "tokens": ["Und", "heut'", "um\u00b7fang'", "ich", "Dei\u00b7ne", "warm\u00b7durch\u00b7wog\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PPOSAT", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.25": {"text": "Prunkvoll-gew\u00f6lbten, bernsteinblassen Glieder", "tokens": ["Prunk\u00b7voll\u00b7ge\u00b7w\u00f6lb\u00b7ten", ",", "bern\u00b7stein\u00b7blas\u00b7sen", "Glie\u00b7der"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+--+--++-+-", "measure": "dactylic.di.plus"}, "line.26": {"text": "In worteloser Andacht, wonneschaudernd,", "tokens": ["In", "wor\u00b7te\u00b7lo\u00b7ser", "An\u00b7dacht", ",", "won\u00b7ne\u00b7schau\u00b7dernd", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Und meine K\u00fcsse werden zum Gebet.", "tokens": ["Und", "mei\u00b7ne", "K\u00fcs\u00b7se", "wer\u00b7den", "zum", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPRART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.28": {"text": "Besinnungslos vor Liebe senken wir", "tokens": ["Be\u00b7sin\u00b7nungs\u00b7los", "vor", "Lie\u00b7be", "sen\u00b7ken", "wir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "PPER"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Die Augen ineinander, und ich trinke", "tokens": ["Die", "Au\u00b7gen", "in\u00b7ein\u00b7an\u00b7der", ",", "und", "ich", "trin\u00b7ke"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.30": {"text": "In durstigen Z\u00fcgen Deinen Athem \u2013 Du", "tokens": ["In", "durs\u00b7ti\u00b7gen", "Z\u00fc\u00b7gen", "Dei\u00b7nen", "A\u00b7them", "\u2013", "Du"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "PPOSAT", "NN", "$(", "PPER"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Den meinen! \u2013 Schmerzlich fast umschn\u00fcren sich", "tokens": ["Den", "mei\u00b7nen", "!", "\u2013", "Schmerz\u00b7lich", "fast", "um\u00b7schn\u00fc\u00b7ren", "sich"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "$(", "ADJD", "ADV", "VVFIN", "PRF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.32": {"text": "Die trocken-hei\u00dfen H\u00e4nde. \u2013 Stundenlang", "tokens": ["Die", "tro\u00b7cken\u00b7hei\u00b7\u00dfen", "H\u00e4n\u00b7de", ".", "\u2013", "Stun\u00b7den\u00b7lang"], "token_info": ["word", "word", "word", "punct", "punct", "word"], "pos": ["ART", "ADJA", "NN", "$.", "$(", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Kein lauter Ton, nur tiefgezog'ne Seufzer", "tokens": ["Kein", "lau\u00b7ter", "Ton", ",", "nur", "tief\u00b7ge\u00b7zo\u00b7g'\u00b7ne", "Seuf\u00b7zer"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIAT", "PIAT", "NN", "$,", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.34": {"text": "Und saugend-schwere Flammenk\u00fcsse oder", "tokens": ["Und", "sau\u00b7gen\u00b7dschwe\u00b7re", "Flam\u00b7men\u00b7k\u00fcs\u00b7se", "o\u00b7der"], "token_info": ["word", "word", "word", "word"],