ord", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Doch Angst und Sorge waren drin.", "tokens": ["Doch", "Angst", "und", "Sor\u00b7ge", "wa\u00b7ren", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Der Schlaf verlie\u00df sein Haus,", "tokens": ["Der", "Schlaf", "ver\u00b7lie\u00df", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Verdacht und Argwohn nisteten sich ein,", "tokens": ["Ver\u00b7dacht", "und", "Arg\u00b7wohn", "nis\u00b7te\u00b7ten", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Er mu\u00dfte immer auf der Lauer sein,", "tokens": ["Er", "mu\u00df\u00b7te", "im\u00b7mer", "auf", "der", "Lau\u00b7er", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Und r\u00fchrte sich nur eine Maus,", "tokens": ["Und", "r\u00fchr\u00b7te", "sich", "nur", "ei\u00b7ne", "Maus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "So schien's ein Dieb; und endlich lief der Arme hin", "tokens": ["So", "schien's", "ein", "Dieb", ";", "und", "end\u00b7lich", "lief", "der", "Ar\u0