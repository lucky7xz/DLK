{"textgrid.poem.60700": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Seifensieder sang vom Morgen bis zum Abend.", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Seifensieder sang vom Morgen bis zum Abend.", "tokens": ["Ein", "Sei\u00b7fen\u00b7sie\u00b7der", "sang", "vom", "Mor\u00b7gen", "bis", "zum", "A\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "APPR", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es war entz\u00fcckend, ihn zu sehn,", "tokens": ["Es", "war", "ent\u00b7z\u00fc\u00b7ckend", ",", "ihn", "zu", "sehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihn zu h\u00f6ren, war noch mehr erlabend.", "tokens": ["Und", "ihn", "zu", "h\u00f6\u00b7ren", ",", "war", "noch", "mehr", "er\u00b7la\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKZU", "VVINF", "$,", "VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Er sang so laut wie zehn", "tokens": ["Er", "sang", "so", "laut", "wie", "zehn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "KOKOM", "CARD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und war zufriedner als die sieben Weisen.", "tokens": ["Und", "war", "zu\u00b7fried\u00b7ner", "als", "die", "sie\u00b7ben", "Wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "KOKOM", "ART", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sein Nachbar doch war nicht so froh.", "tokens": ["Sein", "Nach\u00b7bar", "doch", "war", "nicht", "so", "froh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der sa\u00df im Golde zwar bis \u00fcber beide Ohren,", "tokens": ["Der", "sa\u00df", "im", "Gol\u00b7de", "zwar", "bis", "\u00fc\u00b7ber", "bei\u00b7de", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "ADV", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Doch gl\u00fccklich war er keineswegs zu preisen,", "tokens": ["Doch", "gl\u00fcck\u00b7lich", "war", "er", "kei\u00b7nes\u00b7wegs", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Weil ihn der Schlaf und auch der Frohsinn floh.", "tokens": ["Weil", "ihn", "der", "Schlaf", "und", "auch", "der", "Froh\u00b7sinn", "floh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "KON", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Kaum war bei Tagesgraun ein wenig Schlaf gekommen,", "tokens": ["Kaum", "war", "bei", "Ta\u00b7ges\u00b7graun", "ein", "we\u00b7nig", "Schlaf", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "NN", "ART", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Begann der Seifensieder zu rumoren", "tokens": ["Be\u00b7gann", "der", "Sei\u00b7fen\u00b7sie\u00b7der", "zu", "ru\u00b7mo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Und seine Lieder in die Welt zu schmettern.", "tokens": ["Und", "sei\u00b7ne", "Lie\u00b7der", "in", "die", "Welt", "zu", "schmet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Der Reiche fing nat\u00fcrlich an zu wettern.", "tokens": ["Der", "Rei\u00b7che", "fing", "na\u00b7t\u00fcr\u00b7lich", "an", "zu", "wet\u00b7tern", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "ADV", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Als er sich ausgetobt, denkt er beklommen:", "tokens": ["Als", "er", "sich", "aus\u00b7ge\u00b7tobt", ",", "denkt", "er", "be\u00b7klom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "$,", "VVFIN", "PPER", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Weshalb kann man den Schlaf nicht k\u00e4uflich haben,", "tokens": ["We\u00b7shalb", "kann", "man", "den", "Schlaf", "nicht", "k\u00e4uf\u00b7lich", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PIS", "ART", "NN", "PTKNEG", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wie man sich doch auch andre Gottesgaben,", "tokens": ["Wie", "man", "sich", "doch", "auch", "and\u00b7re", "Got\u00b7tes\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Zum Beispiel Speisen und Getr\u00e4nke, kaufen kann?", "tokens": ["Zum", "Bei\u00b7spiel", "Spei\u00b7sen", "und", "Ge\u00b7tr\u00e4n\u00b7ke", ",", "kau\u00b7fen", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dann l\u00e4\u00dft er sich den S\u00e4nger kommen:", "tokens": ["Dann", "l\u00e4\u00dft", "er", "sich", "den", "S\u00e4n\u00b7ger", "kom\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.19": {"text": "\u00bbgregor, was nehmt Ihr wohl im Jahre ein?\u00ab", "tokens": ["\u00bb", "gre\u00b7gor", ",", "was", "nehmt", "Ihr", "wohl", "im", "Jah\u00b7re", "ein", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NE", "$,", "PWS", "VVFIN", "PPER", "ADV", "APPRART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "\u00bbim Jahre?\u00ab fragt der Mann", "tokens": ["\u00bb", "im", "Jah\u00b7re", "?", "\u00ab", "fragt", "der", "Mann"], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word"], "pos": ["$(", "APPRART", "NN", "$.", "$(", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Und schaut so recht treuherzig drein;", "tokens": ["Und", "schaut", "so", "recht", "treu\u00b7her\u00b7zig", "drein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "\u00bbwie f\u00e4ngt man solche Rechnung an?", "tokens": ["\u00bb", "wie", "f\u00e4ngt", "man", "sol\u00b7che", "Rech\u00b7nung", "an", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VVFIN", "PIS", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Ich z\u00e4hle nicht, wieviel ein Jahr mir bringt \u2013", "tokens": ["Ich", "z\u00e4h\u00b7le", "nicht", ",", "wie\u00b7viel", "ein", "Jahr", "mir", "bringt", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "ART", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Ich bin zufrieden, wenn's gelingt,", "tokens": ["Ich", "bin", "zu\u00b7frie\u00b7den", ",", "wenn's", "ge\u00b7lingt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "KOUS", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Da\u00df man sich gl\u00fccklich durch des Jahres M\u00fchen singt.\u00ab", "tokens": ["Da\u00df", "man", "sich", "gl\u00fcck\u00b7lich", "durch", "des", "Jah\u00b7res", "M\u00fc\u00b7hen", "singt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIS", "PRF", "ADJD", "APPR", "ART", "NN", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "\u00bbnun gut, mein Freund, so sag,", "tokens": ["\u00bb", "nun", "gut", ",", "mein", "Freund", ",", "so", "sag", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["$(", "ADV", "ADJD", "$,", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Wieviel verdienst du denn am Tag?\u00ab", "tokens": ["Wie\u00b7viel", "ver\u00b7dienst", "du", "denn", "am", "Tag", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "\u00bbbald mehr, bald weniger; das Ungl\u00fcck ist allein:", "tokens": ["\u00bb", "bald", "mehr", ",", "bald", "we\u00b7ni\u00b7ger", ";", "das", "Un\u00b7gl\u00fcck", "ist", "al\u00b7lein", ":"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "ADV", "ADV", "$.", "ART", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Es geht so mancher Feiertag mit drein,", "tokens": ["Es", "geht", "so", "man\u00b7cher", "Fei\u00b7er\u00b7tag", "mit", "drein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PIAT", "NN", "APPR", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Sonst w\u00e4re mein Verdienst nicht klein.", "tokens": ["Sonst", "w\u00e4\u00b7re", "mein", "Ver\u00b7dienst", "nicht", "klein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Mit immer neuen w\u00fcrzt der Pfarrer seine Predigt.\u00ab", "tokens": ["Mit", "im\u00b7mer", "neu\u00b7en", "w\u00fcrzt", "der", "Pfar\u00b7rer", "sei\u00b7ne", "Pre\u00b7digt", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "ADV", "ADJA", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der Geldmann lacht der Offenherzigkeit", "tokens": ["Der", "Geld\u00b7mann", "lacht", "der", "Of\u00b7fen\u00b7her\u00b7zig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.33": {"text": "Und sagt: \u00bbVielleicht, da\u00df dies hier dich entsch\u00e4digt!", "tokens": ["Und", "sagt", ":", "\u00bb", "Viel\u00b7leicht", ",", "da\u00df", "dies", "hier", "dich", "ent\u00b7sch\u00e4\u00b7digt", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "ADV", "$,", "KOUS", "PDS", "ADV", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Nimm diese hundert Taler, halte sie in Hut,", "tokens": ["Nimm", "die\u00b7se", "hun\u00b7dert", "Ta\u00b7ler", ",", "hal\u00b7te", "sie", "in", "Hut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "CARD", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Sie helfen dir aus Geldverlegenheit.\u00ab", "tokens": ["Sie", "hel\u00b7fen", "dir", "aus", "Geld\u00b7ver\u00b7le\u00b7gen\u00b7heit", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.36": {"text": "Der Seifensieder nimmt das teure Gut \u2013", "tokens": ["Der", "Sei\u00b7fen\u00b7sie\u00b7der", "nimmt", "das", "teu\u00b7re", "Gut", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.37": {"text": "Ihm ist, als habe er das ganze Geld bekommen,", "tokens": ["Ihm", "ist", ",", "als", "ha\u00b7be", "er", "das", "gan\u00b7ze", "Geld", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "VAFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das in den letzten hundert Jahren", "tokens": ["Das", "in", "den", "letz\u00b7ten", "hun\u00b7dert", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "APPR", "ART", "ADJA", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.39": {"text": "Den Weg durch diese Welt genommen.", "tokens": ["Den", "Weg", "durch", "die\u00b7se", "Welt", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Froh kehrt er heim, verbirgt den Schatz.", "tokens": ["Froh", "kehrt", "er", "heim", ",", "ver\u00b7birgt", "den", "Schatz", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Doch ach, wo waren", "tokens": ["Doch", "ach", ",", "wo", "wa\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "XY", "$,", "PWAV", "VAFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.42": {"text": "Die Lieder und der Frohsinn hin?", "tokens": ["Die", "Lie\u00b7der", "und", "der", "Froh\u00b7sinn", "hin", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Jetzt war f\u00fcr sie in seinem Haus kein Platz,", "tokens": ["Jetzt", "war", "f\u00fcr", "sie", "in", "sei\u00b7nem", "Haus", "kein", "Platz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "APPR", "PPOSAT", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.44": {"text": "Doch Angst und Sorge waren drin.", "tokens": ["Doch", "Angst", "und", "Sor\u00b7ge", "wa\u00b7ren", "drin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.45": {"text": "Der Schlaf verlie\u00df sein Haus,", "tokens": ["Der", "Schlaf", "ver\u00b7lie\u00df", "sein", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.46": {"text": "Verdacht und Argwohn nisteten sich ein,", "tokens": ["Ver\u00b7dacht", "und", "Arg\u00b7wohn", "nis\u00b7te\u00b7ten", "sich", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "PRF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Er mu\u00dfte immer auf der Lauer sein,", "tokens": ["Er", "mu\u00df\u00b7te", "im\u00b7mer", "auf", "der", "Lau\u00b7er", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.48": {"text": "Und r\u00fchrte sich nur 