{"textgrid.poem.39852": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Das ist doch Hohn! das gleicht doch bittrem Spotte!", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Das ist doch Hohn! das gleicht doch bittrem Spotte!", "tokens": ["Das", "ist", "doch", "Hohn", "!", "das", "gleicht", "doch", "bitt\u00b7rem", "Spot\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "NN", "$.", "PDS", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ja, mein Versprechen hab' ich schlecht gehalten,", "tokens": ["Ja", ",", "mein", "Ver\u00b7spre\u00b7chen", "hab'", "ich", "schlecht", "ge\u00b7hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPOSAT", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du kennst der Laune t\u00fcckische Gewalten,", "tokens": ["Du", "kennst", "der", "Lau\u00b7ne", "t\u00fc\u00b7cki\u00b7sche", "Ge\u00b7wal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sie spornt das Fl\u00fcgelpferd oft aus dem Trotte.", "tokens": ["Sie", "spornt", "das", "Fl\u00fc\u00b7gel\u00b7pferd", "oft", "aus", "dem", "Trot\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Doch spotte du der Sp\u00f6ttereien Rotte!", "tokens": ["Doch", "spot\u00b7te", "du", "der", "Sp\u00f6t\u00b7te\u00b7rei\u00b7en", "Rot\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Denn, hast du rechte Sch\u00e4tze zu verwalten,", "tokens": ["Denn", ",", "hast", "du", "rech\u00b7te", "Sch\u00e4t\u00b7ze", "zu", "ver\u00b7wal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PPER", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die f\u00fcrchten keines Witzes Spukgestalten", "tokens": ["Die", "f\u00fcrch\u00b7ten", "kei\u00b7nes", "Wit\u00b7zes", "Spuk\u00b7ge\u00b7stal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und liegen, wo nicht Rost sie fri\u00dft, noch Motte:", "tokens": ["Und", "lie\u00b7gen", ",", "wo", "nicht", "Rost", "sie", "fri\u00dft", ",", "noch", "Mot\u00b7te", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PTKNEG", "NN", "PPER", "VVFIN", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Geh, nimm dir ein Sonett aus diesem Horte,", "tokens": ["Geh", ",", "nimm", "dir", "ein", "So\u00b7nett", "aus", "die\u00b7sem", "Hor\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "PPER", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fein, stolz, gedankenreich, mit gift'ger Spitze!", "tokens": ["Fein", ",", "stolz", ",", "ge\u00b7dan\u00b7ken\u00b7reich", ",", "mit", "gift'\u00b7ger", "Spit\u00b7ze", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Schnell' ab vom Bogen seine goldnen Worte!", "tokens": ["Schnell'", "ab", "vom", "Bo\u00b7gen", "sei\u00b7ne", "gold\u00b7nen", "Wor\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "APPRART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Hier steh' ich, Freund, und meine Brust ist offen.", "tokens": ["Hier", "steh'", "ich", ",", "Freund", ",", "und", "mei\u00b7ne", "Brust", "ist", "of\u00b7fen", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "$,", "KON", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So waffne dich doch nur mit einem Blitze!", "tokens": ["So", "waff\u00b7ne", "dich", "doch", "nur", "mit", "ei\u00b7nem", "Blit\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie will ich jubeln, wenn ich bin getroffen!", "tokens": ["Wie", "will", "ich", "ju\u00b7beln", ",", "wenn", "ich", "bin", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Das is