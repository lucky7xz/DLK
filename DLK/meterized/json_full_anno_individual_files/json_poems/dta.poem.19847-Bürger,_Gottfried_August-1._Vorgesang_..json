{"dta.poem.19847": {"metadata": {"author": {"name": "B\u00fcrger, Gottfried August", "birth": "N.A.", "death": "N.A."}, "title": "1.  \n  Vorgesang .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1778", "urn": "urn:nbn:de:kobv:b4-20090519672", "language": ["de:0.99"], "booktitle": "B\u00fcrger, Gottfried August: Gedichte. G\u00f6ttingen, 1778."}, "poem": {"stanza.1": {"line.1": {"text": "Morgen liebe, wer die Liebe               ", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Unter hellen Melodieen", "tokens": ["Un\u00b7ter", "hel\u00b7len", "Me\u00b7lo\u00b7di\u00b7e\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Ist der junge Mai erwacht.", "tokens": ["Ist", "der", "jun\u00b7ge", "Mai", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht, wie seine Schl\u00e4fe gl\u00fchen!", "tokens": ["Seht", ",", "wie", "sei\u00b7ne", "Schl\u00e4\u00b7fe", "gl\u00fc\u00b7hen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihm Wang\u2019 und Auge lacht!", "tokens": ["Wie", "ihm", "Wang'", "und", "Au\u00b7ge", "lacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ueber kr\u00e4utervollen Rasen,", "tokens": ["Ue\u00b7ber", "kr\u00e4u\u00b7ter\u00b7vol\u00b7len", "Ra\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ueber Hainen schwebet er.", "tokens": ["Ue\u00b7ber", "Hai\u00b7nen", "schwe\u00b7bet", "er", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Kleine laue Weste blasen", "tokens": ["Klei\u00b7ne", "lau\u00b7e", "Wes\u00b7te", "bla\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wolger\u00fcche vor ihm her.", "tokens": ["Wol\u00b7ge\u00b7r\u00fc\u00b7che", "vor", "ihm", "her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Segenvolle Wolken streuen", "tokens": ["Se\u00b7gen\u00b7vol\u00b7le", "Wol\u00b7ken", "streu\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Warme Tropfen auf die Flur,", "tokens": ["War\u00b7me", "Trop\u00b7fen", "auf", "die", "Flur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Geben Nahrung und Gedeihen", "tokens": ["Ge\u00b7ben", "Nah\u00b7rung", "und", "Ge\u00b7dei\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Jedem Kinde der Natur.", "tokens": ["Je\u00b7dem", "Kin\u00b7de", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.4": {"line.1": {"text": "Lieb\u2019 und Gegenliebe paaret", "tokens": ["Lieb'", "und", "Ge\u00b7gen\u00b7lie\u00b7be", "paa\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Gottes Freundlichkeit,", "tokens": ["Die\u00b7ses", "Got\u00b7tes", "Freund\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und sein s\u00fcssestes versparet", "tokens": ["Und", "sein", "s\u00fcs\u00b7ses\u00b7tes", "ver\u00b7spa\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jedes Thier auf diese Zeit.", "tokens": ["Je\u00b7des", "Thier", "auf", "die\u00b7se", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "APPR", "PDAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wann das Laub ihr Nest umschattet,", "tokens": ["Wann", "das", "Laub", "ihr", "Nest", "um\u00b7schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Paaren alle V\u00f6gel sich.", "tokens": ["Paa\u00b7ren", "al\u00b7le", "V\u00f6\u00b7gel", "sich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Was da lebet, das begattet", "tokens": ["Was", "da", "le\u00b7bet", ",", "das", "be\u00b7gat\u00b7tet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PWS", "ADV", "VVFIN", "$,", "PDS", "VVFIN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Um die Zeit der Bl\u00fcthe sich.", "tokens": ["Um", "die", "Zeit", "der", "Bl\u00fc\u00b7the", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "PRF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Schauet! Freudiger und r\u00f6ter", "tokens": ["Schau\u00b7et", "!", "Freu\u00b7di\u00b7ger", "und", "r\u00f6\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$.", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bricht des Tages Morgen an,", "tokens": ["Bricht", "des", "Ta\u00b7ges", "Mor\u00b7gen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als im Anbegin, da Aether", "tokens": ["Als", "im", "An\u00b7be\u00b7gin", ",", "da", "A\u00b7e\u00b7ther"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "APPRART", "NN", "$,", "KOUS", "NN"], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Mutter Tellus liebgewan;", "tokens": ["Mut\u00b7ter", "Tel\u00b7lus", "lieb\u00b7ge\u00b7wan", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und ihr Schoos von ihrem Gatten", "tokens": ["Und", "ihr", "Schoos", "von", "ih\u00b7rem", "Gat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Floren und den Lenz empfing;", "tokens": ["Flo\u00b7ren", "und", "den", "Lenz", "emp\u00b7fing", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und des ersten Haines Schatten", "tokens": ["Und", "des", "ers\u00b7ten", "Hai\u00b7nes", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um die Neugebornen hing.", "tokens": ["Um", "die", "Neu\u00b7ge\u00b7bor\u00b7nen", "hing", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.8": {"line.1": {"text": "Als der erste Fr\u00fchling bl\u00fchte,", "tokens": ["Als", "der", "ers\u00b7te", "Fr\u00fch\u00b7ling", "bl\u00fch\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wand, erzeugt aus Kronus Blut,", "tokens": ["Wand", ",", "er\u00b7zeugt", "aus", "Kro\u00b7nus", "Blut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "NE", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "G\u00f6ttin Venus Afrodite,", "tokens": ["G\u00f6t\u00b7tin", "Ve\u00b7nus", "Af\u00b7ro\u00b7di\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "NE", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei gelinder Wogenflut,", "tokens": ["Bei", "ge\u00b7lin\u00b7der", "Wo\u00b7gen\u00b7flut", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich alm\u00e4hlig aus des grauen", "tokens": ["Sich", "al\u00b7m\u00e4h\u00b7lig", "aus", "des", "grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "ADJD", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ozeans verborgnem Schoos,", "tokens": ["O\u00b7ze\u00b7ans", "ver\u00b7borg\u00b7nem", "Schoos", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Angestaunet von den blauen", "tokens": ["An\u00b7ge\u00b7stau\u00b7net", "von", "den", "blau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wasserungeheuern, los.", "tokens": ["Was\u00b7se\u00b7run\u00b7ge\u00b7heu\u00b7ern", ",", "los", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schon gekant!", "tokens": ["Schon", "ge\u00b7kant", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Morgen liebe, wer die Liebe", "tokens": ["Mor\u00b7gen", "lie\u00b7be", ",", "wer", "die", "Lie\u00b7be"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$,", "PWS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie empfand!", "tokens": ["Nie", "emp\u00b7fand", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}}}}}