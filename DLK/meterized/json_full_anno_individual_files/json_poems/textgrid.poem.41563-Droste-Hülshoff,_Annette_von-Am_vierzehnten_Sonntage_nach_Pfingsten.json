{"textgrid.poem.41563": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am vierzehnten Sonntage nach Pfingsten", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wer ist es der mir nahe steht?", "tokens": ["Wer", "ist", "es", "der", "mir", "na\u00b7he", "steht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wen mu\u00df ich meinen Bruder nennen?", "tokens": ["Wen", "mu\u00df", "ich", "mei\u00b7nen", "Bru\u00b7der", "nen\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wem meine liebste Gabe g\u00f6nnen?", "tokens": ["Wem", "mei\u00b7ne", "liebs\u00b7te", "Ga\u00b7be", "g\u00f6n\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wem reichen eh er noch gefleht?", "tokens": ["Wem", "rei\u00b7chen", "eh", "er", "noch", "ge\u00b7fleht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, la\u00df auf meine Stirne tr\u00e4ufen,", "tokens": ["O", ",", "la\u00df", "auf", "mei\u00b7ne", "Stir\u00b7ne", "tr\u00e4u\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVIMP", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Du Starker, deiner Weisheit Tau!", "tokens": ["Du", "Star\u00b7ker", ",", "dei\u00b7ner", "Weis\u00b7heit", "Tau", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df mich den rechten Stein ergreifen", "tokens": ["La\u00df", "mich", "den", "rech\u00b7ten", "Stein", "er\u00b7grei\u00b7fen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu deines Tempels ew'gem Bau!", "tokens": ["Zu", "dei\u00b7nes", "Tem\u00b7pels", "ew'\u00b7gem", "Bau", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Er den getragen gleicher Scho\u00df,", "tokens": ["Er", "den", "ge\u00b7tra\u00b7gen", "glei\u00b7cher", "Scho\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und der an gleicher Brust gesogen,", "tokens": ["Und", "der", "an", "glei\u00b7cher", "Brust", "ge\u00b7so\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm bin ich willenlos gewogen,", "tokens": ["Ihm", "bin", "ich", "wil\u00b7len\u00b7los", "ge\u00b7wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nichts rei\u00dft des Blutes Bande los.", "tokens": ["Nichts", "rei\u00dft", "des", "Blu\u00b7tes", "Ban\u00b7de", "los", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Auch wer die gleichen L\u00fcfte zieht,", "tokens": ["Auch", "wer", "die", "glei\u00b7chen", "L\u00fcf\u00b7te", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "An gleichen Bodens Quell getrunken,", "tokens": ["An", "glei\u00b7chen", "Bo\u00b7dens", "Quell", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "F\u00fcr ihn auch hat Natur den Funken", "tokens": ["F\u00fcr", "ihn", "auch", "hat", "Na\u00b7tur", "den", "Fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VAFIN", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In jedem Busen angegl\u00fcht.", "tokens": ["In", "je\u00b7dem", "Bu\u00b7sen", "an\u00b7ge\u00b7gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "So der in selben Glaubens Band", "tokens": ["So", "der", "in", "sel\u00b7ben", "Glau\u00b7bens", "Band"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "APPR", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Am selben Hochaltare knieet,", "tokens": ["Am", "sel\u00b7ben", "Hoc\u00b7hal\u00b7ta\u00b7re", "kni\u00b7e\u00b7et", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wo mich gleiche Richtung ziehet,", "tokens": ["Und", "wo", "mich", "glei\u00b7che", "Rich\u00b7tung", "zie\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei's an Gem\u00fct, sei's an Verstand,", "tokens": ["Sei's", "an", "Ge\u00b7m\u00fct", ",", "sei's", "an", "Ver\u00b7stand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "$,", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie alle sind mir wie gegeben", "tokens": ["Sie", "al\u00b7le", "sind", "mir", "wie", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "PIS", "VAFIN", "PPER", "KOKOM", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "In meines eignen Herdes Hut,", "tokens": ["In", "mei\u00b7nes", "eig\u00b7nen", "Her\u00b7des", "Hut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sind Fasern all von meinem Leben,", "tokens": ["Sind", "Fa\u00b7sern", "all", "von", "mei\u00b7nem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sind Tropfen all von meinem Blut.", "tokens": ["Sind", "Trop\u00b7fen", "all", "von", "mei\u00b7nem", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PIAT", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch wenn in heimatferner Luft", "tokens": ["Doch", "wenn", "in", "hei\u00b7mat\u00b7fer\u00b7ner", "Luft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sucht \u00e4ngstlich ein bek\u00fcmmert Wesen", "tokens": ["Sucht", "\u00e4ngst\u00b7lich", "ein", "be\u00b7k\u00fcm\u00b7mert", "We\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJD", "ART", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der fremden Z\u00fcge Schrift zu lesen,", "tokens": ["Der", "frem\u00b7den", "Z\u00fc\u00b7ge", "Schrift", "zu", "le\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo niemand seinen Namen ruft;", "tokens": ["Wo", "nie\u00b7mand", "sei\u00b7nen", "Na\u00b7men", "ruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann nahe dich, und woll' es nennen", "tokens": ["Dann", "na\u00b7he", "dich", ",", "und", "woll'", "es", "nen\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KON", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit jedem Liebesworte nur,", "tokens": ["Mit", "je\u00b7dem", "Lie\u00b7bes\u00b7wor\u00b7te", "nur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Dann magst die Fackel du entbrennen", "tokens": ["Dann", "magst", "die", "Fa\u00b7ckel", "du", "ent\u00b7bren\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die nicht entz\u00fcndete Natur!", "tokens": ["Die", "nicht", "ent\u00b7z\u00fcn\u00b7de\u00b7te", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn an deines Tempels Tor", "tokens": ["Und", "wenn", "an", "dei\u00b7nes", "Tem\u00b7pels", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht einer einsam, ausgeschlossen,", "tokens": ["Steht", "ei\u00b7ner", "ein\u00b7sam", ",", "aus\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Tr\u00e4nen doch vor Gott geflossen,", "tokens": ["Des", "Tr\u00e4\u00b7nen", "doch", "vor", "Gott", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Seufzer doch erreicht sein Ohr,", "tokens": ["Des", "Seuf\u00b7zer", "doch", "er\u00b7reicht", "sein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem magst du deine Rechte reichen", "tokens": ["Dem", "magst", "du", "dei\u00b7ne", "Rech\u00b7te", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und aufw\u00e4rts deuten nach dem Blau,", "tokens": ["Und", "auf\u00b7w\u00e4rts", "deu\u00b7ten", "nach", "dem", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.6": {"line.1": {"text": "Und gar wenn sich gen einen regt", "tokens": ["Und", "gar", "wenn", "sich", "gen", "ei\u00b7nen", "regt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PRF", "APPR", "ART", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dir ein heftig Widerstreben,", "tokens": ["In", "dir", "ein", "hef\u00b7tig", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil andre Weise ihm gegeben", "tokens": ["Weil", "and\u00b7re", "Wei\u00b7se", "ihm", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als dir der Himmel zugelegt,", "tokens": ["Als", "dir", "der", "Him\u00b7mel", "zu\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Fehl, mit Albernheit im Bunde,", "tokens": ["Wenn", "Fehl", ",", "mit", "Al\u00b7bern\u00b7heit", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ersticken will der Liebe Saat;", "tokens": ["Er\u00b7sti\u00b7cken", "will", "der", "Lie\u00b7be", "Saat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Reich ihm die Hand! dies ist die Stunde", "tokens": ["Reich", "ihm", "die", "Hand", "!", "dies", "ist", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "$.", "PDS", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wo das Gebot sich pr\u00fcfend naht.", "tokens": ["Wo", "das", "Ge\u00b7bot", "sich", "pr\u00fc\u00b7fend", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ja, selbst an des Verruchten Blick,", "tokens": ["Ja", ",", "selbst", "an", "des", "Ver\u00b7ruch\u00b7ten", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erd und Himmel m\u00f6chte h\u00f6hnen,", "tokens": ["Der", "Erd", "und", "Him\u00b7mel", "m\u00f6ch\u00b7te", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00dft du in Milde dich gew\u00f6hnen,", "tokens": ["Mu\u00dft", "du", "in", "Mil\u00b7de", "dich", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darfst schaudern, \u2013 aber nicht zur\u00fcck. \u2013", "tokens": ["Darfst", "schau\u00b7dern", ",", "\u2013", "a\u00b7ber", "nicht", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVINF", "$,", "$(", "ADV", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, kannst du ihn in Jesu Christ", "tokens": ["O", ",", "kannst", "du", "ihn", "in", "Je\u00b7su", "Christ"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umschleichen, sp\u00e4hend seine Wunden,", "tokens": ["Um\u00b7schlei\u00b7chen", ",", "sp\u00e4\u00b7hend", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann erst hast du den Stein gefunden,", "tokens": ["Dann", "erst", "hast", "du", "den", "Stein", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann wei\u00dft du wer dein N\u00e4chster ist!", "tokens": ["Dann", "wei\u00dft", "du", "wer", "dein", "N\u00e4chs\u00b7ter", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPOSAT", "NN", "VAFIN", "$."], "mete