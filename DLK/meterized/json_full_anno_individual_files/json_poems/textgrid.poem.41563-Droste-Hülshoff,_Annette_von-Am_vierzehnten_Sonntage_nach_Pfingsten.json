Die", "nicht", "ent\u00b7z\u00fcn\u00b7de\u00b7te", "Na\u00b7tur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Und wenn an deines Tempels Tor", "tokens": ["Und", "wenn", "an", "dei\u00b7nes", "Tem\u00b7pels", "Tor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "PPOSAT", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Steht einer einsam, ausgeschlossen,", "tokens": ["Steht", "ei\u00b7ner", "ein\u00b7sam", ",", "aus\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "ART", "ADJD", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Tr\u00e4nen doch vor Gott geflossen,", "tokens": ["Des", "Tr\u00e4\u00b7nen", "doch", "vor", "Gott", "ge\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des Seufzer doch erreicht sein Ohr,", "tokens": ["Des", "Seuf\u00b7zer", "doch", "er\u00b7reicht", "sein", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem magst du deine Rechte reichen", "tokens": ["Dem", "magst", "du", "dei\u00b7ne", "Rech\u00b7te", "rei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und aufw\u00e4rts deuten nach dem Blau,", "tokens": ["Und", "auf\u00b7w\u00e4rts", "deu\u00b7ten", "nach", "dem", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wo ", "tokens": ["Wo"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.8": {"text": "F\u00fcr ", "tokens": ["F\u00fcr"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}}, "stanza.13": {"line.1": {"text": "Und gar wenn sich gen einen regt", "tokens": ["Und", "gar", "wenn", "sich", "gen", "ei\u00b7nen", "regt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "KOUS", "PRF", "APPR", "ART", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In dir ein heftig Widerstreben,", "tokens": ["In", "dir", "ein", "hef\u00b7tig", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil andre Weise ihm gegeben", "tokens": ["Weil", "and\u00b7re", "Wei\u00b7se", "ihm", "ge\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als dir der Himmel zugelegt,", "tokens": ["Als", "dir", "der", "Him\u00b7mel", "zu\u00b7ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn Fehl, mit Albernheit im Bunde,", "tokens": ["Wenn", "Fehl", ",", "mit", "Al\u00b7bern\u00b7heit", "im", "Bun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "$,", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ersticken will der Liebe Saat;", "tokens": ["Er\u00b7sti\u00b7cken", "will", "der", "Lie\u00b7be", "Saat", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Reich ihm die Hand! dies ist die Stunde", "tokens": ["Reich", "ihm", "die", "Hand", "!", "dies", "ist", "die", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN", "$.", "PDS", "VAFIN", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Wo das Gebot sich pr\u00fcfend naht.", "tokens": ["Wo", "das", "Ge\u00b7bot", "sich", "pr\u00fc\u00b7fend", "naht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ja, selbst an des Verruchten Blick,", "tokens": ["Ja", ",", "selbst", "an", "des", "Ver\u00b7ruch\u00b7ten", "Blick", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Erd und Himmel m\u00f6chte h\u00f6hnen,", "tokens": ["Der", "Erd", "und", "Him\u00b7mel", "m\u00f6ch\u00b7te", "h\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00dft du in Milde dich gew\u00f6hnen,", "tokens": ["Mu\u00dft", "du", "in", "Mil\u00b7de", "dich", "ge\u00b7w\u00f6h\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Darfst schaudern, \u2013 aber nicht zur\u00fcck. \u2013", "tokens": ["Darfst", "schau\u00b7dern", ",", "\u2013", "a\u00b7ber", "nicht", "zu\u00b7r\u00fcck", ".", "\u2013"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PAV", "VVINF", "$,", "$(", "ADV", "PTKNEG", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O, kannst du ihn in Jesu Christ", "tokens": ["O", ",", "kannst", "du", "ihn", "in", "Je\u00b7su", "Christ"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "VMFIN", "PPER", "PPER", "APPR", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Umschleichen, sp\u00e4hend seine Wunden,", "tokens": ["Um\u00b7schlei\u00b7chen", ",", "sp\u00e4\u00b7hend", "sei\u00b7ne", "Wun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann erst hast du den Stein gefunden,", "tokens": ["Dann", "erst", "hast", "du", "den", "Stein", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Dann wei\u00dft du wer dein N\u00e4chster ist!", "tokens": ["Dann", "wei\u00dft", "du", "wer", "dein", "N\u00e4chs\u00b7ter", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PWS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}