{"textgrid.poem.39487": {"metadata": {"author": {"name": "Schlegel, August Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sind's Tr\u00e4ume, die dem Sinn vor\u00fcber walten,", "genre": "verse", "period": "N.A.", "pub_year": 1798, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sind's Tr\u00e4ume, die dem Sinn vor\u00fcber walten,", "tokens": ["Sin\u00b7d's", "Tr\u00e4u\u00b7me", ",", "die", "dem", "Sinn", "vor\u00b7\u00fc\u00b7ber", "wal\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PRELS", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Und die ein Morgenl\u00fcftchen mit sich rafft?", "tokens": ["Und", "die", "ein", "Mor\u00b7gen\u00b7l\u00fcft\u00b7chen", "mit", "sich", "rafft", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und seh' ich wirklich: welch ein Zauber schafft,", "tokens": ["Und", "seh'", "ich", "wirk\u00b7lich", ":", "welch", "ein", "Zau\u00b7ber", "schafft", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$.", "PWAT", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df Hellas Wunder neu sich mir entfalten!", "tokens": ["Da\u00df", "Hel\u00b7las", "Wun\u00b7der", "neu", "sich", "mir", "ent\u00b7fal\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NN", "ADJD", "PRF", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Er ist's, der Bildner redender Gestalten:", "tokens": ["Er", "ist's", ",", "der", "Bild\u00b7ner", "re\u00b7den\u00b7der", "Ge\u00b7stal\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sein Feuerblick, sein Gang, der Arme Kraft,", "tokens": ["Sein", "Feu\u00b7er\u00b7blick", ",", "sein", "Gang", ",", "der", "Ar\u00b7me", "Kraft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die Denkerstirn, die tiefe Leidenschaft,", "tokens": ["Die", "Den\u00b7kers\u00b7tirn", ",", "die", "tie\u00b7fe", "Lei\u00b7den\u00b7schaft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die m\u00e4chtig ringt, das H\u00f6chste festzuhalten.", "tokens": ["Die", "m\u00e4ch\u00b7tig", "ringt", ",", "das", "H\u00f6chs\u00b7te", "fest\u00b7zu\u00b7hal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Was z\u00fcrnst du noch dem Werke deiner Hand,", "tokens": ["Was", "z\u00fcrnst", "du", "noch", "dem", "Wer\u00b7ke", "dei\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dem Spiegel deiner sch\u00f6pferischen Seele,", "tokens": ["Dem", "Spie\u00b7gel", "dei\u00b7ner", "sch\u00f6p\u00b7fe\u00b7ri\u00b7schen", "See\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als ob ihm Leben zur Vollendung fehle?", "tokens": ["Als", "ob", "ihm", "Le\u00b7ben", "zur", "Vol\u00b7len\u00b7dung", "feh\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die hohe Kunst, der sich dein Geist verband,", "tokens": ["Die", "ho\u00b7he", "Kunst", ",", "der", "sich", "dein", "Geist", "ver\u00b7band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Schon f\u00fchlst du sie von deiner Glut erwarmen;", "tokens": ["Schon", "f\u00fchlst", "du", "sie", "von", "dei\u00b7ner", "Glut", "er\u00b7war\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie steigt herab und ruht in deinen Armen.", "tokens": ["Sie", "steigt", "her\u00b7ab", "und", "ruht", "in", "dei\u00b7nen", "Ar\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sind's Tr\u00e4ume, die dem Sinn vor\u00fcber walten,", "tokens": ["Sin\u00b7d's", "Tr\u00e4u\u00b7me", ",", "die", "dem", "Sinn", "vor\u00b7\u00fc\u00b7ber", "wal\u00b7ten", ","], "token_info