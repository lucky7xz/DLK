{"textgrid.poem.50987": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Augen, meine lieben Fensterlein,", "genre": "verse", "period": "N.A.", "pub_year": 1879, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Augen, meine lieben Fensterlein,", "tokens": ["Au\u00b7gen", ",", "mei\u00b7ne", "lie\u00b7ben", "Fens\u00b7ter\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Gebt mir schon so lange holden Schein,", "tokens": ["Gebt", "mir", "schon", "so", "lan\u00b7ge", "hol\u00b7den", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Lasset freundlich Bild um Bild herein:", "tokens": ["Las\u00b7set", "freund\u00b7lich", "Bild", "um", "Bild", "her\u00b7ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "NN", "APPR", "NN", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Einmal werdet ihr verdunkelt sein!", "tokens": ["Ein\u00b7mal", "wer\u00b7det", "ihr", "ver\u00b7dun\u00b7kelt", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "VAINF", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Fallen einst die m\u00fcden Lider zu,", "tokens": ["Fal\u00b7len", "einst", "die", "m\u00fc\u00b7den", "Li\u00b7der", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "L\u00f6scht ihr aus, dann hat die Seele Ruh;", "tokens": ["L\u00f6scht", "ihr", "aus", ",", "dann", "hat", "die", "See\u00b7le", "Ruh", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "$,", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Tastend streift sie ab die Wanderschuh',", "tokens": ["Tas\u00b7tend", "streift", "sie", "ab", "die", "Wan\u00b7der\u00b7schuh'", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Legt sich auch in ihre finstre Truh.", "tokens": ["Legt", "sich", "auch", "in", "ih\u00b7re", "finst\u00b7re", "Truh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Noch zwei F\u00fcnklein sieht sie glimmend stehn,", "tokens": ["Noch", "zwei", "F\u00fcn\u00b7klein", "sieht", "sie", "glim\u00b7mend", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "NN", "VVFIN", "PPER", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Wie zwei Sternlein innerlich zu sehn,", "tokens": ["Wie", "zwei", "Stern\u00b7lein", "in\u00b7ner\u00b7lich", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "CARD", "NN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Bis sie schwanken und dann auch vergehn,", "tokens": ["Bis", "sie", "schwan\u00b7ken", "und", "dann", "auch", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "wo