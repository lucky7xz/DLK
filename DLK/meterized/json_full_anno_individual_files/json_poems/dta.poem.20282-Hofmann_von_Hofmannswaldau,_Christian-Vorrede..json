{"dta.poem.20282": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr feinde wi\u00dft ihr nicht/ mit wem ihr habt gekriegt?", "tokens": ["Ihr", "fein\u00b7de", "wi\u00dft", "ihr", "nicht", "/", "mit", "wem", "ihr", "habt", "ge\u00b7kriegt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "PTKNEG", "$(", "APPR", "PWS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es ist der erden Mars/ der R\u00f6mer Alexander/", "tokens": ["Es", "ist", "der", "er\u00b7den", "Mars", "/", "der", "R\u00f6\u00b7mer", "A\u00b7lex\u00b7an\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$(", "ART", "NN", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des grossen C\u00e4sars sohn. So einer als der ander", "tokens": ["Des", "gros\u00b7sen", "C\u00e4\u00b7sars", "sohn", ".", "So", "ei\u00b7ner", "als", "der", "an\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$.", "ADV", "PIS", "KOKOM", "ART", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat den gebrauch an sich: Er kommt/ er sieht und siegt.", "tokens": ["Hat", "den", "ge\u00b7brauch", "an", "sich", ":", "Er", "kommt", "/", "er", "sieht", "und", "siegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "PRF", "$.", "PPER", "VVFIN", "$(", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}