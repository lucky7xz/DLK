{"textgrid.poem.67894": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "20. Gl\u00fcckseligkeit der Ehe", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf, Liebe! La\u00df kein Mi\u00dfbehagen", "tokens": ["Auf", ",", "Lie\u00b7be", "!", "La\u00df", "kein", "Mi\u00df\u00b7be\u00b7ha\u00b7gen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$,", "NN", "$.", "VVIMP", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Uns nehmen unsre Himmelsruh;", "tokens": ["Uns", "neh\u00b7men", "uns\u00b7re", "Him\u00b7mels\u00b7ruh", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was soll uns Thorensorge plagen", "tokens": ["Was", "soll", "uns", "Tho\u00b7ren\u00b7sor\u00b7ge", "pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Gottes Eden schliessen zu?", "tokens": ["Und", "Got\u00b7tes", "E\u00b7den", "schlies\u00b7sen", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Da\u00df etwa F\u00fcrsten nicht verkl\u00e4ren", "tokens": ["Da\u00df", "et\u00b7wa", "F\u00fcrs\u00b7ten", "nicht", "ver\u00b7kl\u00e4\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Adeltiteln unser Blut?", "tokens": ["Mit", "A\u00b7del\u00b7ti\u00b7teln", "un\u00b7ser", "Blut", "