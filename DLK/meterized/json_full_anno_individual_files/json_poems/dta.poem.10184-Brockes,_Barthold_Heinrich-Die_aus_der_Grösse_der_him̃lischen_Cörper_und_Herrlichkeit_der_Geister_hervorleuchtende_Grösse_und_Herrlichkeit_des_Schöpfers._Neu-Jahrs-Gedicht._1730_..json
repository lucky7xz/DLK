{"dta.poem.10184": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die aus der Gr\u00f6sse der him\u0303lischen C\u00f6rper  \n und Herrlichkeit der Geister hervorleuchtende  \n Gr\u00f6sse und Herrlichkeit des Sch\u00f6pfers.  \n Neu-Jahrs-Gedicht.  \n  1730 .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "GoTTHEJT, Die Du, blo\u00df aus Liebe,", "tokens": ["GoTTHEJT", ",", "Die", "Du", ",", "blo\u00df", "aus", "Lie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "PPER", "$,", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Aller Himmel Himmel Heer,", "tokens": ["Al\u00b7ler", "Him\u00b7mel", "Him\u00b7mel", "Heer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonnen, Welte, Lufft und Meer,", "tokens": ["Son\u00b7nen", ",", "Wel\u00b7te", ",", "Lufft", "und", "Meer", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller Creaturen Pracht,", "tokens": ["Al\u00b7ler", "Crea\u00b7tu\u00b7ren", "Pracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Und, zusammt dem Geist des Lichts,", "tokens": ["Und", ",", "zu\u00b7sammt", "dem", "Geist", "des", "Lichts", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VVFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Aus der tieffen Nacht des Nichts,", "tokens": ["Aus", "der", "tief\u00b7fen", "Nacht", "des", "Nichts", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.7": {"text": "Auch den strengen Flu\u00df der Zeit,", "tokens": ["Auch", "den", "stren\u00b7gen", "Flu\u00df", "der", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die in stiller Schnelligkeit,", "tokens": ["Die", "in", "stil\u00b7ler", "Schnel\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Wie es scheinet, flieht und fliesset,", "tokens": ["Wie", "es", "schei\u00b7net", ",", "flieht", "und", "flies\u00b7set", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und ins Meer der Ewigkeit", "tokens": ["Und", "ins", "Meer", "der", "E\u00b7wig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.11": {"text": "Ungehemmet sich ergiesset,", "tokens": ["Un\u00b7ge\u00b7hem\u00b7met", "sich", "er\u00b7gies\u00b7set", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Durch ein Wort hervor gebracht!", "tokens": ["Durch", "ein", "Wort", "her\u00b7vor", "ge\u00b7bracht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "La\u00df! ach la\u00df uns uns bestreben,", "tokens": ["La\u00df", "!", "ach", "la\u00df", "uns", "uns", "be\u00b7stre\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "XY", "VVIMP", "PPER", "PRF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Da\u00df wir Dir, von unserm Leben,", "tokens": ["Da\u00df", "wir", "Dir", ",", "von", "un\u00b7serm", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wenigstens zur Wechsel-Zeit,", "tokens": ["We\u00b7nigs\u00b7tens", "zur", "Wech\u00b7sel\u00b7Zeit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.16": {"text": "Einen Theil zum Opfer geben,", "tokens": ["Ei\u00b7nen", "Theil", "zum", "Op\u00b7fer", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Und, in der Beschaffenheit", "tokens": ["Und", ",", "in", "der", "Be\u00b7schaf\u00b7fen\u00b7heit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Der Gesch\u00f6pf, und Herrlichkeit,", "tokens": ["Der", "Ge\u00b7sch\u00f6pf", ",", "und", "Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.19": {"text": "Deine Herrlichkeit erheben!", "tokens": ["Dei\u00b7ne", "Herr\u00b7lich\u00b7keit", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Wo eine Zeit von uns des Sch\u00f6pfers Ruhm erfodert,", "tokens": ["Wo", "ei\u00b7ne", "Zeit", "von", "uns", "des", "Sch\u00f6p\u00b7fers", "Ruhm", "er\u00b7fo\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "PPER", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ie dem menschlichen Geschlecht,", "tokens": ["Wo", "ie", "dem", "menschli\u00b7chen", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nach seinen Pflichten, und mit Recht", "tokens": ["Nach", "sei\u00b7nen", "Pflich\u00b7ten", ",", "und", "mit", "Recht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein frohes Andacht-Feur in Brust und Seele lodert;", "tokens": ["Ein", "fro\u00b7hes", "An\u00b7dacht\u00b7Feur", "in", "Brust", "und", "See\u00b7le", "lo\u00b7dert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So ist es diese Zeit, da wir zur Sonnen-Glut,", "tokens": ["So", "ist", "es", "die\u00b7se", "Zeit", ",", "da", "wir", "zur", "Son\u00b7nen\u00b7Glut", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "$,", "KOUS", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem Spiegel G\u00f6ttlicher Vollkommenheiten,", "tokens": ["Dem", "Spie\u00b7gel", "G\u00f6tt\u00b7li\u00b7cher", "Voll\u00b7kom\u00b7men\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Creaturen Seel\u2019, und aller Fruchtbarkeiten,", "tokens": ["Der", "Crea\u00b7tu\u00b7ren", "Seel'", ",", "und", "al\u00b7ler", "Frucht\u00b7bar\u00b7kei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "KON", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.8": {"text": "Und alles Segens Quell, der Licht- und Lebens-Fluth,", "tokens": ["Und", "al\u00b7les", "Se\u00b7gens", "Quell", ",", "der", "Licht", "und", "Le\u00b7bens\u00b7Fluth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "NN", "$,", "ART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die, voller W\u00e4rm\u2019 und Krafft, sich \u00fcberall ergiesset,", "tokens": ["Die", ",", "vol\u00b7ler", "W\u00e4rm'", "und", "Krafft", ",", "sich", "\u00fc\u00b7be\u00b7rall", "er\u00b7gies\u00b7set", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADJA", "NN", "KON", "NN", "$,", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und, als ein Seegens-Flu\u00df, Lufft, Erd\u2019 und Fluth durch-", "tokens": ["Und", ",", "als", "ein", "See\u00b7gens\u00b7Flu\u00df", ",", "Lufft", ",", "Erd'", "und", "Fluth", "durch"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "KOUS", "ART", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Aufs neu uns wieder drehn.", "tokens": ["Aufs", "neu", "uns", "wie\u00b7der", "drehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJD", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}