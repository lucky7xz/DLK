{"textgrid.poem.28025": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "40.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Ehstand ist zur Zeit dem Uhrwerck zu vergleichen,", "tokens": ["Der", "Eh\u00b7stand", "ist", "zur", "Zeit", "dem", "Uhr\u00b7werck", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nach dem Wetter offt von rechter Spur wil weichen.", "tokens": ["Das", "nach", "dem", "Wet\u00b7ter", "offt", "von", "rech\u00b7ter", "Spur", "wil", "wei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Unruh, die keinmal sol stehen, ist das lieben,", "tokens": ["Die", "Un\u00b7ruh", ",", "die", "kein\u00b7mal", "sol", "ste\u00b7hen", ",", "ist", "das", "lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "VMFIN", "VVINF", "$,", "VAFIN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die vom Gewichte doch de\u00df Gl\u00fcckes wird getrieben;", "tokens": ["Die", "vom", "Ge\u00b7wich\u00b7te", "doch", "de\u00df", "Gl\u00fc\u00b7ckes", "wird", "ge\u00b7trie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "ADV", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Hammer ist der Mann; die Glock ist seine Frau,", "tokens": ["Der", "Ham\u00b7mer", "ist", "der", "Mann", ";", "die", "Glock", "ist", "sei\u00b7ne", "Frau", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$.", "ART", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die schlagen sonsten nicht, als wann das Wetter rau;", "tokens": ["Die", "schla\u00b7gen", "sons\u00b7ten", "nicht", ",", "als", "wann", "das", "Wet\u00b7ter", "rau", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "$,", "KOUS", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie schlagen gleich nun zwey, drey, minder oder mehr,", "tokens": ["Sie", "schla\u00b7gen", "gleich", "nun", "zwey", ",", "drey", ",", "min\u00b7der", "o\u00b7der", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "CARD", "$,", "CARD", "$,", "ADV", "KON", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So ist doch dieser Klang gantz sch\u00e4dlich dem Geh\u00f6r.", "tokens": ["So", "ist", "doch", "die\u00b7ser", "Klang", "gantz", "sch\u00e4d\u00b7lich", "dem", "Ge\u00b7h\u00f6r", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDAT", "NN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Ehstand ist zur Zeit dem Uh