{"dta.poem.7555": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "54.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Geist des Menschen denkt nur durch den Gegensatz;", "tokens": ["Der", "Geist", "des", "Men\u00b7schen", "denkt", "nur", "durch", "den", "Ge\u00b7gen\u00b7satz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drum ist der Gegensatz im Denkgedicht am Platz.", "tokens": ["Drum", "ist", "der", "Ge\u00b7gen\u00b7satz", "im", "Denk\u00b7ge\u00b7dicht", "am", "Platz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VAFIN", "ART", "NN", "APPRART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Auch im gef\u00fchlten Lied sind tiefe Gegens\u00e4tze,", "tokens": ["Auch", "im", "ge\u00b7f\u00fchl\u00b7ten", "Lied", "sind", "tie\u00b7fe", "Ge\u00b7gen\u00b7s\u00e4t\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Anmuthig \u00fcberh\u00fcllt vom flie\u00dfenden Geschw\u00e4tze.", "tokens": ["An\u00b7mut\u00b7hig", "\u00fc\u00b7berh\u00b7\u00fcllt", "vom", "flie\u00b7\u00dfen\u00b7den", "Ge\u00b7schw\u00e4t\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.3": {"line.1": {"text": "Wo des Gef\u00fchles Flut an Felsen scharfgezackt", "tokens": ["Wo", "des", "Ge\u00b7f\u00fch\u00b7les", "Flut", "an", "Fel\u00b7sen", "scharf\u00b7ge\u00b7zackt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Leidenschaft sich bricht, sinds Gegens\u00e4tze nackt.", "tokens": ["Der", "Lei\u00b7den\u00b7schaft", "sich", "bricht", ",", "sinds", "Ge\u00b7gen\u00b7s\u00e4t\u00b7ze", "nackt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVFIN", "$,", "VAFIN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}