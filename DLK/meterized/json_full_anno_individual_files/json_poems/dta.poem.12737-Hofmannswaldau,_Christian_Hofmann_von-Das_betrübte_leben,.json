{"dta.poem.12737": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das betr\u00fcbte leben,", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mjt schmertzen pflegen uns die m\u00fctter zu geb\u00e4hren;", "tokens": ["Mjt", "schmert\u00b7zen", "pfle\u00b7gen", "uns", "die", "m\u00fct\u00b7ter", "zu", "ge\u00b7b\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "VVFIN", "PPER", "ART", "ADJA", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit schmertzen ziehen uns die m\u00fctter gleichfalls auf;", "tokens": ["Mit", "schmert\u00b7zen", "zie\u00b7hen", "uns", "die", "m\u00fct\u00b7ter", "gleich\u00b7falls", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VVFIN", "PPER", "ART", "ADJA", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit schmertzen m\u00fcssen sie dem tod uns auch gew\u00e4hren;", "tokens": ["Mit", "schmert\u00b7zen", "m\u00fcs\u00b7sen", "sie", "dem", "tod", "uns", "auch", "ge\u00b7w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVINF", "VMFIN", "PPER", "ART", "NN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wie? ist das leben nicht ein langer schmertzens-lauff!", "tokens": ["Wie", "?", "ist", "das", "le\u00b7ben", "nicht", "ein", "lan\u00b7ger", "schmert\u00b7zens\u00b7lauff", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VAFIN", "PDS", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}