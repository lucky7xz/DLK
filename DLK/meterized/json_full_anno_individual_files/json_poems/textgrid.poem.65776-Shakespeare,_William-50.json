r", ",", "das", "mich", "tr\u00e4gt", ",", "der", "Mit\u00b7ge\u00b7plag\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Von meinem Gram, trabt schwer und tr\u00e4gt die Last in mir,", "tokens": ["Von", "mei\u00b7nem", "Gram", ",", "trabt", "schwer", "und", "tr\u00e4gt", "die", "Last", "in", "mir", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVFIN", "ADJD", "KON", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Als wenn ein dunkler Trieb dem Armen sagte:", "tokens": ["Als", "wenn", "ein", "dunk\u00b7ler", "Trieb", "dem", "Ar\u00b7men", "sag\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sein Reiter liebt nicht Eil, die ihn entf\u00fchrt von dir.", "tokens": ["Sein", "Rei\u00b7ter", "liebt", "nicht", "Eil", ",", "die", "ihn", "ent\u00b7f\u00fchrt", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "NN", "$,", "PRELS", "PPER", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ihn k\u00f6nnen blut'ge Sporen nicht beschw\u00f6ren,", "tokens": ["Ihn", "k\u00f6n\u00b7nen", "blut'\u00b7ge", "Spo\u00b7ren", "nicht", "be\u00b7schw\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADJA", "NN", "PTKNEG", "VVINF", "$,"], "me