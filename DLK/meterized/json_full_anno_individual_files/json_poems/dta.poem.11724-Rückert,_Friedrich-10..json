{"dta.poem.11724": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie du verschieden hast den Gott in dir empfunden,", "tokens": ["Wie", "du", "ver\u00b7schie\u00b7den", "hast", "den", "Gott", "in", "dir", "emp\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVPP", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verschieden findest du ihn auch in Schrifturkunden.", "tokens": ["Ver\u00b7schie\u00b7den", "fin\u00b7dest", "du", "ihn", "auch", "in", "Schrif\u00b7tur\u00b7kun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ist er in dir darum dir wen'ger offenbar,", "tokens": ["Ist", "er", "in", "dir", "da\u00b7rum", "dir", "wen'\u00b7ger", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "PAV", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Offenbarung dort deswegen minder wahr?", "tokens": ["Die", "Of\u00b7fen\u00b7ba\u00b7rung", "dort", "des\u00b7we\u00b7gen", "min\u00b7der", "wahr", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PAV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er zeigt dir dieses bald, bald jenes Angesicht,", "tokens": ["Er", "zeigt", "dir", "die\u00b7ses", "bald", ",", "bald", "je\u00b7nes", "An\u00b7ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "ADV", "$,", "ADV", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch immer ist es klar und sch\u00f6n und hold und licht.", "tokens": ["Doch", "im\u00b7mer", "ist", "es", "klar", "und", "sch\u00f6n", "und", "hold", "und", "licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ADJD", "KON", "ADJD", "KON", "ADJD", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Die Urkund' ist von ihm in Herz und Buch gesenkt,", "tokens": ["Die", "Ur\u00b7kund'", "ist", "von", "ihm", "in", "Herz", "und", "Buch", "ge\u00b7senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPR", "PPER", "APPR", "NN", "KON", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie goldner Lebenswein in buntes Glas geschenkt.", "tokens": ["Wie", "gold\u00b7ner", "Le\u00b7bens\u00b7wein", "in", "bun\u00b7tes", "Glas", "ge\u00b7schenkt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Als fl\u00fcssigen Smaragd, als thauenden Rubin,", "tokens": ["Als", "fl\u00fcs\u00b7si\u00b7gen", "Sma\u00b7ragd", ",", "als", "thau\u00b7en\u00b7den", "Ru\u00b7bin", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "KOUS", "ADJA", "NE", "$,"], "meter": "-+--++-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Als schmelzenden Sapphir, doch immer trinkst du ihn.", "tokens": ["Als", "schmel\u00b7zen\u00b7den", "Sap\u00b7phir", ",", "doch", "im\u00b7mer", "trinkst", "du", "ihn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "$,", "ADV", "ADV", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--++-+-+-+", "measure": "iambic.hexa.relaxed"}}}}}