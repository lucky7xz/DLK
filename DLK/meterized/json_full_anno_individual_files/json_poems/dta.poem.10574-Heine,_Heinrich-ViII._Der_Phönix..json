{"dta.poem.10574": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "ViII.  \n  Der Ph\u00f6nix.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Es kommt ein Vogel geflogen aus Westen,", "tokens": ["Es", "kommt", "ein", "Vo\u00b7gel", "ge\u00b7flo\u00b7gen", "aus", "Wes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Er fliegt gen Osten,", "tokens": ["Er", "fliegt", "gen", "Os\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Nach der \u00f6stlichen Gartenheimath,", "tokens": ["Nach", "der", "\u00f6st\u00b7li\u00b7chen", "Gar\u00b7ten\u00b7hei\u00b7math", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Wo Spezereien duften und wachsen,", "tokens": ["Wo", "Spe\u00b7ze\u00b7rei\u00b7en", "duf\u00b7ten", "und", "wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "VVFIN", "KON", "VVINF", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Palmen rauschen und Brunnen k\u00fchlen \u2014", "tokens": ["Und", "Pal\u00b7men", "rau\u00b7schen", "und", "Brun\u00b7nen", "k\u00fch\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVINF", "KON", "NN", "VVINF", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Und fliegend singt der Wundervogel:", "tokens": ["Und", "flie\u00b7gend", "singt", "der", "Wun\u00b7der\u00b7vo\u00b7gel", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "\u201esie liebt ihn! sie liebt ihn!", "tokens": ["\u201e", "sie", "liebt", "ihn", "!", "sie", "liebt", "ihn", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Sie tr\u00e4gt sein Bildni\u00df im kleinen Herzen,", "tokens": ["Sie", "tr\u00e4gt", "sein", "Bild\u00b7ni\u00df", "im", "klei\u00b7nen", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und tr\u00e4gt es s\u00fc\u00df und heimlich verborgen,", "tokens": ["Und", "tr\u00e4gt", "es", "s\u00fc\u00df", "und", "heim\u00b7lich", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Und wei\u00df es selbst nicht!", "tokens": ["Und", "wei\u00df", "es", "selbst", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Aber im Traume steht er vor ihr,", "tokens": ["A\u00b7ber", "im", "Trau\u00b7me", "steht", "er", "vor", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Sie bittet und weint und k\u00fc\u00dft seine H\u00e4nde,", "tokens": ["Sie", "bit\u00b7tet", "und", "weint", "und", "k\u00fc\u00dft", "sei\u00b7ne", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Und ruft seinen Namen", "tokens": ["Und", "ruft", "sei\u00b7nen", "Na\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Und rufend erwacht sie und liegt erschrocken,", "tokens": ["Und", "ru\u00b7fend", "er\u00b7wacht", "sie", "und", "liegt", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.9": {"text": "Und reibt sich verwundert die sch\u00f6nen Augen \u2014", "tokens": ["Und", "reibt", "sich", "ver\u00b7wun\u00b7dert", "die", "sch\u00f6\u00b7nen", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Sie liebt ihn! Sie liebt ihn!\u201c", "tokens": ["Sie", "liebt", "ihn", "!", "Sie", "liebt", "ihn", "!", "\u201c"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}}}}