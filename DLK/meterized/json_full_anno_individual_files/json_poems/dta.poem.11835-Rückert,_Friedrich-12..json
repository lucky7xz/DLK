{"dta.poem.11835": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "12.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Kie\u00df der Reue wird ein Edelstein genannt,", "tokens": ["Der", "Kie\u00df", "der", "Reu\u00b7e", "wird", "ein", "E\u00b7del\u00b7stein", "ge\u00b7nannt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der sch\u00f6nres Namens dir ist als Smaragd bekannt.", "tokens": ["Der", "sch\u00f6n\u00b7res", "Na\u00b7mens", "dir", "ist", "als", "Sma\u00b7ragd", "be\u00b7kannt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPER", "VAFIN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich sage dir, warum er hei\u00dfe Kie\u00df der Reue;", "tokens": ["Ich", "sa\u00b7ge", "dir", ",", "wa\u00b7rum", "er", "hei\u00b7\u00dfe", "Kie\u00df", "der", "Reu\u00b7e", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PWAV", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sich an ird'schem Glanz kein Herz vollkommen freue!", "tokens": ["Da\u00df", "sich", "an", "ird'\u00b7schem", "Glanz", "kein", "Herz", "voll\u00b7kom\u00b7men", "freu\u00b7e", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "PIAT", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Als Alexander zog der Held ohn' Hindernis", "tokens": ["Als", "A\u00b7lex\u00b7an\u00b7der", "zog", "der", "Held", "ohn'", "Hin\u00b7der\u00b7nis"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "VVFIN", "ART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von West zu Ost, und kam ins Land der Finsternis,", "tokens": ["Von", "West", "zu", "Ost", ",", "und", "kam", "ins", "Land", "der", "Fins\u00b7ter\u00b7nis", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NE", "$,", "KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Wo er des Lebens Brunn gesucht, den er nicht fand,", "tokens": ["Wo", "er", "des", "Le\u00b7bens", "Brunn", "ge\u00b7sucht", ",", "den", "er", "nicht", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "NN", "VVPP", "$,", "PRELS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Drang er mit seinem Heer tief in die Felsenwand.", "tokens": ["Drang", "er", "mit", "sei\u00b7nem", "Heer", "tief", "in", "die", "Fel\u00b7sen\u00b7wand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}}, "stanza.5": {"line.1": {"text": "Da h\u00f6rten sie von fern den Brunn des Lebens rauschen;", "tokens": ["Da", "h\u00f6r\u00b7ten", "sie", "von", "fern", "den", "Brunn", "des", "Le\u00b7bens", "rau\u00b7schen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ADJD", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch wo er flie\u00df' und wie, das war nicht zu erlauschen.", "tokens": ["Doch", "wo", "er", "flie\u00df'", "und", "wie", ",", "das", "war", "nicht", "zu", "er\u00b7lau\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PTKVZ", "KON", "PWAV", "$,", "PDS", "VAFIN", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Vor ihren Augen schwebt' ein falscher Wasserschein,", "tokens": ["Vor", "ih\u00b7ren", "Au\u00b7gen", "schwebt'", "ein", "fal\u00b7scher", "Was\u00b7ser\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der f\u00fchrt' itzt durstig sie heraus, wie erst hinein.", "tokens": ["Der", "f\u00fchrt'", "itzt", "durs\u00b7tig", "sie", "he\u00b7raus", ",", "wie", "erst", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "ADJD", "PPER", "PTKVZ", "$,", "PWAV", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Da sahen sie den Grund mit gr\u00fcnem Kie\u00df bestreut;", "tokens": ["Da", "sa\u00b7hen", "sie", "den", "Grund", "mit", "gr\u00fc\u00b7nem", "Kie\u00df", "be\u00b7streut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die davon nahmen, und die nicht, hats gleich gereut.", "tokens": ["Die", "da\u00b7von", "nah\u00b7men", ",", "und", "die", "nicht", ",", "hats", "gleich", "ge\u00b7reut", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PAV", "VVFIN", "$,", "KON", "ART", "PTKNEG", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+--+----+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Smaragde waren es, da sie ans Licht gekommen,", "tokens": ["Sma\u00b7rag\u00b7de", "wa\u00b7ren", "es", ",", "da", "sie", "ans", "Licht", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$,", "KOUS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und alle reut' es, die davon nichts mitgenommen.", "tokens": ["Und", "al\u00b7le", "reut'", "es", ",", "die", "da\u00b7von", "nichts", "mit\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "$,", "PRELS", "PAV", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Mehr aber reut' es die was mitgenommen hatten,", "tokens": ["Mehr", "a\u00b7ber", "reut'", "es", "die", "was", "mit\u00b7ge\u00b7nom\u00b7men", "hat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "ART", "PIS", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Weil sie das beste doch gelassen in den Schatten.", "tokens": ["Weil", "sie", "das", "bes\u00b7te", "doch", "ge\u00b7las\u00b7sen", "in", "den", "Schat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "ADV", "VVPP", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Denn wer die Gn\u00fcge nicht gesch\u00f6pft im Lebensbronnen,", "tokens": ["Denn", "wer", "die", "Gn\u00fc\u00b7ge", "nicht", "ge\u00b7sch\u00f6pft", "im", "Le\u00b7bens\u00b7bron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PTKNEG", "VVPP", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der findet, o mein Sohn, nur Reue selbst in Wonnen.", "tokens": ["Der", "fin\u00b7det", ",", "o", "mein", "Sohn", ",", "nur", "Reu\u00b7e", "selbst", "in", "Won\u00b7nen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "FM", "PPOSAT", "NN", "$,", "ADV", "NN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}