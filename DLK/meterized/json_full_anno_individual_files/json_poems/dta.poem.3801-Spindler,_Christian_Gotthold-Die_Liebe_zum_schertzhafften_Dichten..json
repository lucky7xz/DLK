{"dta.poem.3801": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe zum schertzhafften Dichten.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Dich, Liebe, hat mein Kiel besungen,", "tokens": ["Dich", ",", "Lie\u00b7be", ",", "hat", "mein", "Kiel", "be\u00b7sun\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und dieses that ich sehr gezwungen,", "tokens": ["Und", "die\u00b7ses", "that", "ich", "sehr", "ge\u00b7zwun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Weil offt ein Feind, weil offt der Neid", "tokens": ["Weil", "offt", "ein", "Feind", ",", "weil", "offt", "der", "Neid"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "$,", "KOUS", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auch selbst die Unschulds-vollen Sachen", "tokens": ["Auch", "selbst", "die", "Un\u00b7schulds\u00b7vol\u00b7len", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Kan Lasterhafft und sch\u00e4ndlich machen,", "tokens": ["Kan", "Las\u00b7ter\u00b7hafft", "und", "sch\u00e4nd\u00b7lich", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er ein freches C-der schreyt.", "tokens": ["Wenn", "er", "ein", "fre\u00b7ches", "C\u00b7\u00b7der", "schreyt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da heists: Ein allzufreyes Schertzen", "tokens": ["Da", "heists", ":", "Ein", "all\u00b7zu\u00b7fre\u00b7yes", "Schert\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zeugt auch von einem freyen Hertzen,", "tokens": ["Zeugt", "auch", "von", "ei\u00b7nem", "frey\u00b7en", "Hert\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und wer dergleichen Grillen heckt,", "tokens": ["Und", "wer", "derg\u00b7lei\u00b7chen", "Gril\u00b7len", "heckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PIS", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ist selbsten hintern Strauch gesteckt.", "tokens": ["Ist", "selbs\u00b7ten", "hin\u00b7tern", "Strauch", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du kennest meine Sayten-Th\u00f6ne,", "tokens": ["Du", "ken\u00b7nest", "mei\u00b7ne", "Say\u00b7ten\u00b7\u00b7T\u00b7h\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Niemahls sang ich vor eine Sch\u00f6ne,", "tokens": ["Nie\u00b7mahls", "sang", "ich", "vor", "ei\u00b7ne", "Sch\u00f6\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Nie lidt ich deiner Herrschafft Joch;", "tokens": ["Nie", "lidt", "ich", "dei\u00b7ner", "Herr\u00b7schafft", "Joch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nie f\u00fchlt ich deiner Regung Triebe,", "tokens": ["Nie", "f\u00fchlt", "ich", "dei\u00b7ner", "Re\u00b7gung", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nie dient ich dir, verha\u00dfte Liebe,", "tokens": ["Nie", "dient", "ich", "dir", ",", "ver\u00b7ha\u00df\u00b7te", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und dir zu Ehren sing ich doch.", "tokens": ["Und", "dir", "zu", "Eh\u00b7ren", "sing", "ich", "doch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Di\u00df scheint sich sehr zu widersprechen,", "tokens": ["Di\u00df", "scheint", "sich", "sehr", "zu", "wi\u00b7der\u00b7spre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und wird die freche Mi\u00dfgunst stechen.", "tokens": ["Und", "wird", "die", "fre\u00b7che", "Mi\u00df\u00b7gunst", "ste\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Sie wetzt bereits den Geiffer-Zahn;", "tokens": ["Sie", "wetzt", "be\u00b7reits", "den", "Geif\u00b7fer\u00b7Zahn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "O! Liebe, nimm dich meiner an.", "tokens": ["O", "!", "Lie\u00b7be", ",", "nimm", "dich", "mei\u00b7ner", "an", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "$,", "VVIMP", "PPER", "PPOSAT", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bezeuge meiner Unschuld Proben,", "tokens": ["Be\u00b7zeu\u00b7ge", "mei\u00b7ner", "Un\u00b7schuld", "Pro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Denn will ich dich gedoppelt loben,", "tokens": ["Denn", "will", "ich", "dich", "ge\u00b7dop\u00b7pelt", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Hab ich nur erst Gelegenheit.", "tokens": ["Hab", "ich", "nur", "erst", "Ge\u00b7le\u00b7gen\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bezeuge nur, da\u00df ich dich hasse,", "tokens": ["Be\u00b7zeu\u00b7ge", "nur", ",", "da\u00df", "ich", "dich", "has\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und niemahls bey mir wurtzeln lasse,", "tokens": ["Und", "nie\u00b7mahls", "bey", "mir", "wurt\u00b7zeln", "las\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bezeuge, wie ich h\u00f6chst erfreut", "tokens": ["Be\u00b7zeu\u00b7ge", ",", "wie", "ich", "h\u00f6chst", "er\u00b7freut"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sehr offte deine Macht verlachet,", "tokens": ["Sehr", "off\u00b7te", "dei\u00b7ne", "Macht", "ver\u00b7la\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und deinen Trieb zu nicht gemachet.", "tokens": ["Und", "dei\u00b7nen", "Trieb", "zu", "nicht", "ge\u00b7ma\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Bezeug\u2019, ich singe nicht vor dich,", "tokens": ["Be\u00b7zeug'", ",", "ich", "sin\u00b7ge", "nicht", "vor", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PTKNEG", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nein, etwas anders zwinget mich.", "tokens": ["Nein", ",", "et\u00b7was", "an\u00b7ders", "zwin\u00b7get", "mich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Niemahls sang meine schlechte Leyer", "tokens": ["Nie\u00b7mahls", "sang", "mei\u00b7ne", "schlech\u00b7te", "Le\u00b7yer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Von Deinem ungez\u00e4mten Feuer,", "tokens": ["Von", "Dei\u00b7nem", "un\u00b7ge\u00b7z\u00e4m\u00b7ten", "Feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wodurch der Menschen Hertze gl\u00fcht.", "tokens": ["Wo\u00b7durch", "der", "Men\u00b7schen", "Hert\u00b7ze", "gl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du hast mich nie ins Netz gekrieget,", "tokens": ["Du", "hast", "mich", "nie", "ins", "Netz", "ge\u00b7krie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Du hast noch nie mein Hertz besieget,", "tokens": ["Du", "hast", "noch", "nie", "mein", "Hertz", "be\u00b7sie\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da doch dein Zwang sehr viele zieht.", "tokens": ["Da", "doch", "dein", "Zwang", "sehr", "vie\u00b7le", "zieht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, wird man von mir anders sprechen,", "tokens": ["Und", ",", "wird", "man", "von", "mir", "an\u00b7ders", "spre\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "VAFIN", "PIS", "APPR", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So must du mich, o Liebe! r\u00e4chen,", "tokens": ["So", "must", "du", "mich", ",", "o", "Lie\u00b7be", "!", "r\u00e4\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "$,", "FM", "NN", "$.", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Auf! fange ihr argw\u00f6hnisch Hertz,", "tokens": ["Auf", "!", "fan\u00b7ge", "ihr", "arg\u00b7w\u00f6h\u00b7nisch", "Hertz", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Und r\u00e4che mich durch diesen Schertz.", "tokens": ["Und", "r\u00e4\u00b7che", "mich", "durch", "die\u00b7sen", "Schertz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}