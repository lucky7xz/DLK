ure": "trochaic.tetra"}, "line.4": {"text": "Und vor grosser Wollust lachen.", "tokens": ["Und", "vor", "gros\u00b7ser", "Wol\u00b7lust", "la\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Schau des Taubers artigs stellen/", "tokens": ["Schau", "des", "Tau\u00b7bers", "ar\u00b7tigs", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der um seinen Eh-Gesellen", "tokens": ["Der", "um", "sei\u00b7nen", "Eh\u00b7Ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Mit verliebten Murmeln fliegt/", "tokens": ["Mit", "ver\u00b7lieb\u00b7ten", "Mur\u00b7meln", "fliegt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Und sich k\u00fcssend zu ihr f\u00fcgt.", "tokens": ["Und", "sich", "k\u00fcs\u00b7send", "zu", "ihr", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADJD", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "H\u00f6r die Nachtigalle an/", "tokens": ["H\u00f6r", "die", "Nach\u00b7ti\u00b7gal\u00b7le", "an", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Die von Zweig zu Zweigen springet/", "tokens": ["Die", "von", "Zweig", "zu", "Zwei\u00b7gen", "sprin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "APPR", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und so helle wie sie kan", "tokens": ["Und", "so", "hel\u00b7le", "wie", "sie", "kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJA", "KOKOM", "PPER", "VMFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit erhobner Stimme singet/", "tokens": ["Mit", "er\u00b7hob\u00b7ner", "Stim\u00b7me", "sin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht die hellen Liebes-Flammen", "tokens": ["Seht", "die", "hel\u00b7len", "Lie\u00b7bes\u00b7Flam\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Schlagen \u00fcber mich zusammen/", "tokens": ["Schla\u00b7gen", "\u00fc\u00b7ber", "mich", "zu\u00b7sam\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "PTKVZ", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sehet! wie mein Hertze brennt/", "tokens": ["Se\u00b7het", "!", "wie", "mein", "Hert\u00b7ze", "brennt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "KOKOM", "PPOSAT", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ihr ", "tokens": ["Die", "ihr"], "token_info": ["word", "word"], "pos": ["ART", "PPOSAT"], "meter": "-+", "measure": "iambic.single"}}, "stanza.5": {"line.1": {"text": "Und als wenn dir unbewust?", "tokens": ["Und", "als", "wenn", "dir", "un\u00b7be\u00b7wust", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOKOM", "KOUS", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Was die kluge Schlange treibet/", "tokens": ["Was", "die", "klu\u00b7ge", "Schlan\u00b7ge", "trei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Die das Gifft vor ihrer Lust", "tokens": ["Die", "das", "Gifft", "vor", "ih\u00b7rer", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Erst der Erden einverleibet/", "tokens": ["Erst", "der", "Er\u00b7den", "ein\u00b7ver\u00b7lei\u00b7bet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Drauf zu ihrem Buhlen eilet/", "tokens": ["Drauf", "zu", "ih\u00b7rem", "Buh\u00b7len", "ei\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Der die Liebe mit ihr theilet;", "tokens": ["Der", "die", "Lie\u00b7be", "mit", "ihr", "thei\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Denn die Liebe will allein", "tokens": ["Denn", "die", "Lie\u00b7be", "will", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VMFIN", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Nur lust-und nicht gifftig seyn.", "tokens": ["Nur", "lust\u00b7\u00b7und", "nicht", "giff\u00b7tig", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD"