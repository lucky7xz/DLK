{"dta.poem.229": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Schreibst du es der Feindschafft zu?", "tokens": ["Schreibst", "du", "es", "der", "Feind\u00b7schafft", "zu", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df der Bock gern bey der Ziegen/", "tokens": ["Da\u00df", "der", "Bock", "gern", "bey", "der", "Zie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NE", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und der Ochse bey der Kuh", "tokens": ["Und", "der", "O\u00b7chse", "bey", "der", "Kuh"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Mag im gr\u00fcnen Grase liegen.", "tokens": ["Mag", "im", "gr\u00fc\u00b7nen", "Gra\u00b7se", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Ist es Hassen oder Lieben", "tokens": ["Ist", "es", "Has\u00b7sen", "o\u00b7der", "Lie\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "NN", "KON", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Was die Turtel-Tauben \u00fcben?", "tokens": ["Was", "die", "Tur\u00b7tel\u00b7Tau\u00b7ben", "\u00fc\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Wenn der Tauber gantz vergn\u00fcgt", "tokens": ["Wenn", "der", "Tau\u00b7ber", "gantz", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Sich zur treuen Tauben f\u00fcgt.", "tokens": ["Sich", "zur", "treu\u00b7en", "Tau\u00b7ben", "f\u00fcgt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Meynst du/ da\u00df die Grausahmkeit", "tokens": ["Meynst", "du", "/", "da\u00df", "die", "Grau\u00b7sahm\u00b7keit"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "PPER", "$(", "KOUS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Jhren Ursprung hergenommen", "tokens": ["Ih\u00b7ren", "Ur\u00b7sprung", "her\u00b7ge\u00b7nom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Von der s\u00fcssen Fr\u00fchlings-Zeit?", "tokens": ["Von", "der", "s\u00fcs\u00b7sen", "Fr\u00fch\u00b7lings\u00b7Zeit", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die jetzt lieblich angekommen/", "tokens": ["Die", "jetzt", "lieb\u00b7lich", "an\u00b7ge\u00b7kom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Und zur Liebes-Lust