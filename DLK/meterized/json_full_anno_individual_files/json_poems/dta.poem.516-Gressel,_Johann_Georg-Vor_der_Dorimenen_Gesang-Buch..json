{"dta.poem.516": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Vor der  Dorimenen  Gesang-Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du wilst/ ich soll etwas in dein Gesang-Buch schreiben/", "tokens": ["Du", "wilst", "/", "ich", "soll", "et\u00b7was", "in", "dein", "Ge\u00b7sang\u00b7Buch", "schrei\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$(", "PPER", "VMFIN", "PIS", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich setz die Feder an/ ich wei\u00df nicht was ich schreib/", "tokens": ["Ich", "setz", "die", "Fe\u00b7der", "an", "/", "ich", "wei\u00df", "nicht", "was", "ich", "schreib", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$(", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mir selbst ist unbekandt/ was ich da einverleib;", "tokens": ["Mir", "selbst", "ist", "un\u00b7be\u00b7kandt", "/", "was", "ich", "da", "ein\u00b7ver\u00b7leib", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "ADJD", "$(", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ich la\u00df den schwachen Kiel nach seinen Willen treiben.", "tokens": ["Ich", "la\u00df", "den", "schwa\u00b7chen", "Kiel", "nach", "sei\u00b7nen", "Wil\u00b7len", "trei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er schreibt: Ach lie\u00df es doch/ nimm ", "tokens": ["Er", "schreibt", ":", "Ach", "lie\u00df", "es", "doch", "/", "nimm"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VVFIN", "$.", "ITJ", "VVFIN", "PPER", "ADV", "$(", "VVIMP"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Er schreibt/ da\u00df ich dein Knecht/ ja gar dein eigen bin.", "tokens": ["Er", "schreibt", "/", "da\u00df", "ich", "dein", "Knecht", "/", "ja", "gar", "dein", "ei\u00b7gen", "bin", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "PPOSAT", "NN", "$(", "ADV", "ADV", "PPOSAT", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}