{"textgrid.poem.23922": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Letzte Bitte", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df mich noch einmal dir ins schwarze Auge sehn,", "tokens": ["La\u00df", "mich", "noch", "ein\u00b7mal", "dir", "ins", "schwar\u00b7ze", "Au\u00b7ge", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "PPER", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "La\u00df mich noch einmal tief ins hei\u00dfe Dunkel senken", "tokens": ["La\u00df", "mich", "noch", "ein\u00b7mal", "tief", "ins", "hei\u00b7\u00dfe", "Dun\u00b7kel", "sen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADV", "ADJD", "APPRART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den trunkenen Blick, dann will ich weitergehn", "tokens": ["Den", "trun\u00b7ke\u00b7nen", "Blick", ",", "dann", "will", "ich", "wei\u00b7ter\u00b7gehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "VMFIN", "PPER", "VVINF"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und dich vergessen ... Nur in harter Zeit,", "tokens": ["Und", "dich", "ver\u00b7ges\u00b7sen", "...", "Nur", "in", "har\u00b7ter", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVPP", "$(", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wenn sich der Sehnsucht Augen r\u00fcckw\