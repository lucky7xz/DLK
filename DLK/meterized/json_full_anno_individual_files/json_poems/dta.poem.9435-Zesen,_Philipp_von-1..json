{"dta.poem.9435": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach Liebste/ wie soll ich dein Angesicht preisen!", "tokens": ["Ach", "Liebs\u00b7te", "/", "wie", "soll", "ich", "dein", "An\u00b7ge\u00b7sicht", "prei\u00b7sen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "PWAV", "VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-++--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ach Freundin/ wie sch\u00f6ne wie sch\u00f6ne bisin/", "tokens": ["Ach", "Freun\u00b7din", "/", "wie", "sch\u00f6\u00b7ne", "wie", "sch\u00f6\u00b7ne", "bi\u00b7sin", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$(", "KOKOM", "ADJA", "KOKOM", "ADJA", "NN", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Die schw\u00e4rtzlichen Augen auch zierlich sich weisen", "tokens": ["Die", "schw\u00e4rtz\u00b7li\u00b7chen", "Au\u00b7gen", "auch", "zier\u00b7lich", "sich", "wei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ADJD", "PRF", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Dirrch deine geflochtene Z\u00f6pfe dazu/", "tokens": ["Dirrch", "dei\u00b7ne", "ge\u00b7floch\u00b7te\u00b7ne", "Z\u00f6p\u00b7fe", "da\u00b7zu", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PAV", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Sie schimmern im dunckeln", "tokens": ["Sie", "schim\u00b7mern", "im", "dun\u00b7ckeln"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Wie lichte Karsunckeln/", "tokens": ["Wie", "lich\u00b7te", "Kar\u00b7sun\u00b7ckeln", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "und leuchten herf\u00fcr;", "tokens": ["und", "leuch\u00b7ten", "her\u00b7f\u00fcr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die Taube mu\u00df weichen", "tokens": ["Die", "Tau\u00b7be", "mu\u00df", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "und kann nicht erreichen", "tokens": ["und", "kann", "nicht", "er\u00b7rei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PTKNEG", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Das blitzen der augen/ die liebliche Zier.", "tokens": ["Das", "blit\u00b7zen", "der", "au\u00b7gen", "/", "die", "lieb\u00b7li\u00b7che", "Zier", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.2": {"line.1": {"text": "Wie jenseit dem Eufrat die lustigen Ziegen", "tokens": ["Wie", "jen\u00b7seit", "dem", "Euf\u00b7rat", "die", "lus\u00b7ti\u00b7gen", "Zie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Auf Galaad h\u00fcpfen und gleichen dem klee/", "tokens": ["Auf", "Ga\u00b7laad", "h\u00fcp\u00b7fen", "und", "glei\u00b7chen", "dem", "klee", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "KON", "ADJA", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "So m\u00fcssen die Haare sich schwingen und fl\u00fcgen", "tokens": ["So", "m\u00fcs\u00b7sen", "die", "Haa\u00b7re", "sich", "schwin\u00b7gen", "und", "fl\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "VVINF", "KON", "VVINF"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "umb deine Verliebete stirne wie schnee.", "tokens": ["umb", "dei\u00b7ne", "Ver\u00b7lie\u00b7be\u00b7te", "stir\u00b7ne", "wie", "schnee", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Den Z\u00e4hnen ingleichen", "tokens": ["Den", "Z\u00e4h\u00b7nen", "in\u00b7glei\u00b7chen"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Die Herde mu\u00df weichen", "tokens": ["Die", "Her\u00b7de", "mu\u00df", "wei\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "im wasser geschwem\u0303t/", "tokens": ["im", "was\u00b7ser", "ge\u00b7schwem\u0303t", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "$("], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Die allzumahl tr\u00e4get/", "tokens": ["Die", "all\u00b7zu\u00b7mahl", "tr\u00e4\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Viel wollust erreget/", "tokens": ["Viel", "wol\u00b7lust", "er\u00b7re\u00b7get", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Die jmmer mit doppelten fr\u00fcchten ank\u00f6m\u0303t.", "tokens": ["Die", "jm\u00b7mer", "mit", "dop\u00b7pel\u00b7ten", "fr\u00fcch\u00b7ten", "an\u00b7k\u00f6m\u0303t", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Die Lippen den r\u00f6thlichen Rosen sich gleichen/", "tokens": ["Die", "Lip\u00b7pen", "den", "r\u00f6th\u00b7li\u00b7chen", "Ro\u00b7sen", "sich", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "PRF", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.2": {"text": "Dein sprechen ist lieblich und s\u00fc\u00dfe wie Wein.", "tokens": ["Dein", "spre\u00b7chen", "ist", "lieb\u00b7lich", "und", "s\u00fc\u00b7\u00dfe", "wie", "Wein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VAFIN", "ADJD", "KON", "VVFIN", "KOKOM", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Der Granat an farbe den Wangen mu\u00df weichen/", "tokens": ["Der", "Gra\u00b7nat", "an", "far\u00b7be", "den", "Wan\u00b7gen", "mu\u00df", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die zwischen den Haaren vollf\u00fchren den schein.", "to