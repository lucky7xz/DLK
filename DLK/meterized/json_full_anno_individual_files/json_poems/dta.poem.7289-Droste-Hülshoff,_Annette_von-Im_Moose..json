{"dta.poem.7289": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Im Moose.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst die Nacht dem sonnenm\u00fcden Land", "tokens": ["Als", "j\u00fcngst", "die", "Nacht", "dem", "son\u00b7nen\u00b7m\u00fc\u00b7den", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der D\u00e4mmrung leise Boten hat gesandt,", "tokens": ["Der", "D\u00e4mm\u00b7rung", "lei\u00b7se", "Bo\u00b7ten", "hat", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da lag ich einsam noch in Waldes Moose.", "tokens": ["Da", "lag", "ich", "ein\u00b7sam", "noch", "in", "Wal\u00b7des", "Moo\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ADV", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die dunklen Zweige nickten so vertraut,", "tokens": ["Die", "dunk\u00b7len", "Zwei\u00b7ge", "nick\u00b7ten", "so", "ver\u00b7traut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "An meiner Wange fl\u00fcsterte das Kraut,", "tokens": ["An", "mei\u00b7ner", "Wan\u00b7ge", "fl\u00fcs\u00b7ter\u00b7te", "das", "Kraut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Unsichtbar duftete die Haiderose.", "tokens": ["Un\u00b7sicht\u00b7bar", "duf\u00b7te\u00b7te", "die", "Hai\u00b7de\u00b7ro\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}}}}}