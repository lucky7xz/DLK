{"dta.poem.7736": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "16.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Erst baut Natur den Leib, ein Haus mit Sinnenthoren,", "tokens": ["Erst", "baut", "Na\u00b7tur", "den", "Leib", ",", "ein", "Haus", "mit", "Sin\u00b7nen\u00b7tho\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NN", "ART", "NN", "$,", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Worin ein fremdes Kind, der Geist, dann wird geboren.", "tokens": ["Wo\u00b7rin", "ein", "frem\u00b7des", "Kind", ",", "der", "Geist", ",", "dann", "wird", "ge\u00b7bo\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er findet Hausger\u00e4th und braucht es nach Gefallen,", "tokens": ["Er", "fin\u00b7det", "Haus\u00b7ge\u00b7r\u00e4\u00b7th", "und", "braucht", "es", "nach", "Ge\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und wenn er dann das Haus verl\u00e4\u00dft, wird es zerfallen.", "tokens": ["Und", "wenn", "er", "dann", "das", "Haus", "ver\u00b7l\u00e4\u00dft", ",", "wird", "es", "zer\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch die Baumeisterin baut immer Neues wieder,", "tokens": ["Doch", "die", "Bau\u00b7meis\u00b7te\u00b7rin", "baut", "im\u00b7mer", "Neu\u00b7es", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "NN", "ADV", "$,"], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Und lockt den Himmelsgast zur ird'schen Einkehr nieder.", "tokens": ["Und", "lockt", "den", "Him\u00b7mels\u00b7gast", "zur", "ird'\u00b7schen", "Ein\u00b7kehr", "nie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}