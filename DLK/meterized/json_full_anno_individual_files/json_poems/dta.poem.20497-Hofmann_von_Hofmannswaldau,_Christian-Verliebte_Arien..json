{"dta.poem.20497": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.71", "et:0.28"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Clorinde/ kanst du lustig stehen/", "tokens": ["Clo\u00b7rin\u00b7de", "/", "kanst", "du", "lus\u00b7tig", "ste\u00b7hen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$(", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn einer rose sch\u00f6nes haupt", "tokens": ["Wenn", "ei\u00b7ner", "ro\u00b7se", "sch\u00f6\u00b7nes", "haupt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auff ihrem stocke mu\u00df vergehen/", "tokens": ["Auff", "ih\u00b7rem", "sto\u00b7cke", "mu\u00df", "ver\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht etwan von der faust geraubt/", "tokens": ["Nicht", "et\u00b7wan", "von", "der", "faust", "ge\u00b7raubt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.5": {"text": "So sie mit w\u00fcrden k\u00f6nte tragen/", "tokens": ["So", "sie", "mit", "w\u00fcr\u00b7den", "k\u00f6n\u00b7te", "tra\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "APPR", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sie legen auff die sch\u00f6ne brust/", "tokens": ["Sie", "le\u00b7gen", "auff", "die", "sch\u00f6\u00b7ne", "brust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und mit erfrischtem hertzen sagen:", "tokens": ["Und", "mit", "er\u00b7frischtem", "hert\u00b7zen", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "----+-+-", "measure": "unknown.measure.di"}, "line.8": {"text": "Hier ist ein k\u00f6nigreich voll lust.", "tokens": ["Hier", "ist", "ein", "k\u00f6\u00b7nig\u00b7reich", "voll", "lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nein/ nein/ du schaust best\u00fcrtzt darnieder/", "tokens": ["Nein", "/", "nein", "/", "du", "schaust", "be\u00b7st\u00fcrtzt", "dar\u00b7nie\u00b7der", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$(", "PTKANT", "$(", "PPER", "VVFIN", "ADJD", "PAV", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "L\u00e4st tieffe seuffzer von dir gehn/", "tokens": ["L\u00e4st", "tief\u00b7fe", "seuff\u00b7zer", "von", "dir", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das kleinste deiner zarten glieder", "tokens": ["Das", "kleins\u00b7te", "dei\u00b7ner", "zar\u00b7ten", "glie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mu\u00df in dem trauer-stande stehn;", "tokens": ["Mu\u00df", "in", "dem", "trau\u00b7e\u00b7rstan\u00b7de", "stehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie klagstu \u00fcber solche sachen/", "tokens": ["Wie", "klags\u00b7tu", "\u00fc\u00b7ber", "sol\u00b7che", "sa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "APPR", "PIS", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die man in allen g\u00e4rten bricht?", "tokens": ["Die", "man", "in", "al\u00b7len", "g\u00e4r\u00b7ten", "bricht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "La\u00df dich dasselbe traurig machen/", "tokens": ["La\u00df", "dich", "das\u00b7sel\u00b7be", "trau\u00b7rig", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PDAT", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "So dir ein \u00fcbel urtheil spricht.", "tokens": ["So", "dir", "ein", "\u00fc\u00b7bel", "ur\u00b7theil", "spricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Erkenne dich/ und lerne kennen/", "tokens": ["Er\u00b7ken\u00b7ne", "dich", "/", "und", "ler\u00b7ne", "ken\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$(", "KON", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df/ wo itzt blut und bl\u00fcte siegt/", "tokens": ["Da\u00df", "/", "wo", "itzt", "blut", "und", "bl\u00fc\u00b7te", "siegt", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "PWAV", "ADV", "NN", "KON", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo allerhand begierden brennen/", "tokens": ["Wo", "al\u00b7ler\u00b7hand", "be\u00b7gier\u00b7den", "bren\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und eine faule wurtzel liegt/", "tokens": ["Und", "ei\u00b7ne", "fau\u00b7le", "wurt\u00b7zel", "liegt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es werden deine zarte wangen", "tokens": ["Es", "wer\u00b7den", "dei\u00b7ne", "zar\u00b7te", "wan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht stetig rosen-st\u00f6cke seyn/", "tokens": ["Nicht", "ste\u00b7tig", "ro\u00b7sen\u00b7st\u00f6\u00b7cke", "seyn", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJA", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bey ihnen f\u00e4llt so wohl das prangen/", "tokens": ["Bey", "ih\u00b7nen", "f\u00e4llt", "so", "wohl", "das", "pran\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ADV", "ADV", "PDS", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Als bey der rose zier und schein.", "tokens": ["Als", "bey", "der", "ro\u00b7se", "zier", "und", "schein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "ADJA", "NN", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Drum la\u00df mich deine rosen brechen/", "tokens": ["Drum", "la\u00df", "mich", "dei\u00b7ne", "ro\u00b7sen", "bre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Weil hier noch stock und bl\u00fcte neu/", "tokens": ["Weil", "hier", "noch", "stock", "und", "bl\u00fc\u00b7te", "neu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "ADJD", "KON", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach weine! werd ich selten sprechen/", "tokens": ["Ach", "wei\u00b7ne", "!", "werd", "ich", "sel\u00b7ten", "spre\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "$.", "VAFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Das frische gras giebt welckes heu.", "tokens": ["Das", "fri\u00b7sche", "gras", "giebt", "wel\u00b7ckes", "heu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PWAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Komm/ komm/ und folge meiner lehre/", "tokens": ["Komm", "/", "komm", "/", "und", "fol\u00b7ge", "mei\u00b7ner", "leh\u00b7re", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Venus hat es auch gethan/", "tokens": ["Die", "Ve\u00b7nus", "hat", "es", "auch", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und tausend mehr/ was ist die ehre?", "tokens": ["Und", "tau\u00b7send", "mehr", "/", "was", "ist", "die", "eh\u00b7re", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "ADV", "$(", "PWS", "VAFIN", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ein kluges nichts/ ein blosser wahn.", "tokens": ["Ein", "klu\u00b7ges", "nichts", "/", "ein", "blos\u00b7ser", "wahn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "PIS", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}