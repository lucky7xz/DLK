{"textgrid.poem.59396": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Winter", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie von Nacht verhangen,", "tokens": ["Wie", "von", "Nacht", "ver\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wu\u00dft nicht, was ich will,", "tokens": ["Wu\u00dft", "nicht", ",", "was", "ich", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schon so lange, lange", "tokens": ["Schon", "so", "lan\u00b7ge", ",", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "War ich totenstill.", "tokens": ["War", "ich", "to\u00b7tens\u00b7till", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.2": {"line.1": {"text": "Liegt die Welt voll Schmerzen,", "tokens": ["Liegt", "die", "Welt", "voll", "Schmer\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Will's auch drau\u00dfen schnein:", "tokens": ["Will's", "auch", "drau\u00b7\u00dfen", "schnein", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wache auf, mein Herze,", "tokens": ["Wa\u00b7che", "auf", ",", "mein", "Her\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "PPOSAT", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Fr\u00fchling mu\u00df es sein!", "tokens": ["Fr\u00fch\u00b7ling", "mu\u00df", "es", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VAINF", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Was mich frech wollt fassen,", "tokens": ["Was", "mich", "frech", "wollt", "fas\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADJD", "VMFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "'s ist nur Wogenschaum,", "tokens": ["'s", "ist", "nur", "Wo\u00b7gen\u00b7schaum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Falsche Ehr, Not, Hassen,", "tokens": ["Fal\u00b7sche", "Ehr", ",", "Not", ",", "Has\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Welt, ich sp\u00fcr dich kaum.", "tokens": ["Welt", ",", "ich", "sp\u00fcr", "dich", "kaum", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Breite nur die Fl\u00fcgel", "tokens": ["Brei\u00b7te", "nur", "die", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wieder, sch\u00f6nes Ro\u00df,", "tokens": ["Wie\u00b7der", ",", "sch\u00f6\u00b7nes", "Ro\u00df", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Frei la\u00df ich die Z\u00fcgel,", "tokens": ["Frei", "la\u00df", "ich", "die", "Z\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVIMP", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "So brich durch, Geno\u00df!", "tokens": ["So", "brich", "durch", ",", "Ge\u00b7no\u00df", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$,", "VVFIN", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.5": {"line.1": {"text": "Und hat ausgeklungen", "tokens": ["Und", "hat", "aus\u00b7ge\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "VAFIN", "VVPP"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Liebeslust und Leid,", "tokens": ["Lie\u00b7bes\u00b7lust", "und", "Leid", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Um die wir gerungen", "tokens": ["Um", "die", "wir", "ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "PPER", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "In der sch\u00f6nsten Zeit;", "tokens": ["In", "der", "sch\u00f6ns\u00b7ten", "Zeit", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "Nun so trag mich weiter,", "tokens": ["Nun", "so", "trag", "mich", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wo das W\u00fcnschen aus \u2013", "tokens": ["Wo", "das", "W\u00fcn\u00b7schen", "aus", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PTKVZ", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wie wird mir so heiter,", "tokens": ["Wie", "wird", "mir", "so", "hei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Ro\u00df, bring mich nach Haus!", "tokens": ["Ro\u00df", ",", "bring", "mich", "nach", "Haus", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Wie von Nacht verhangen,", "tokens": ["Wie", "von", "Nacht", "ver\u00b7han\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wu\u00dft nicht, was ich will,", "tokens": ["Wu\u00dft", "nicht", ",", "was", "ich", "will", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VMFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Schon so lange, lange", "tokens": ["Schon", "so", "lan\u00b7ge", ",", "lan\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "ADV", "ADV", "$,", "ADV"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "War ich totenstill.", "tokens": ["War", "ich", "to\u00b7tens\u00b7till", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Liegt die Welt voll Schmerzen,", "tokens": ["Liegt", "die", "Welt", "voll", "Schmer