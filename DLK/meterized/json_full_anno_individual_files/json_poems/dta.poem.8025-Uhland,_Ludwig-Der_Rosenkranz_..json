{"dta.poem.8025": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Der Rosenkranz .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "In des Maies holden Tagen,", "tokens": ["In", "des", "Mai\u00b7es", "hol\u00b7den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "In der Aue Blumenglanz,", "tokens": ["In", "der", "Au\u00b7e", "Blu\u00b7men\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Edle Knappen fechten, jagen", "tokens": ["Ed\u00b7le", "Knap\u00b7pen", "fech\u00b7ten", ",", "ja\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um den werthen Rosenkranz.", "tokens": ["Um", "den", "wert\u00b7hen", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wollen nicht mit leichtem Finger", "tokens": ["Wol\u00b7len", "nicht", "mit", "leich\u00b7tem", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Blumen pfl\u00fccken auf dem Plan,", "tokens": ["Blu\u00b7men", "pfl\u00fc\u00b7cken", "auf", "dem", "Plan", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollen sie, als wackre Ringer,", "tokens": ["Wol\u00b7len", "sie", ",", "als", "wack\u00b7re", "Rin\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aus der Jungfrau Hand empfahn.", "tokens": ["Aus", "der", "Jung\u00b7frau", "Hand", "em\u00b7pfahn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVPP", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "In der Laube sitzt die Stille,", "tokens": ["In", "der", "Lau\u00b7be", "sitzt", "die", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die mit Staunen Jeder sieht,", "tokens": ["Die", "mit", "Stau\u00b7nen", "Je\u00b7der", "sieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "PIS", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Die in solcher Jugendf\u00fclle", "tokens": ["Die", "in", "sol\u00b7cher", "Ju\u00b7gend\u00b7f\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heut zum ersten Male bl\u00fcht.", "tokens": ["Heut", "zum", "ers\u00b7ten", "Ma\u00b7le", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Volle Rosenzweig\u2019 umwanken,", "tokens": ["Vol\u00b7le", "Ro\u00b7sen\u00b7zweig'", "um\u00b7wan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als ein Schattenhut, ihr Haupt;", "tokens": ["Als", "ein", "Schat\u00b7ten\u00b7hut", ",", "ihr", "Haupt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Reben mit den Bl\u00fcthenranken", "tokens": ["Re\u00b7ben", "mit", "den", "Bl\u00fc\u00b7then\u00b7ran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Halten ihren Leib umlaubt.", "tokens": ["Hal\u00b7ten", "ih\u00b7ren", "Leib", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Sieh! im Eisenkleid ein Reiter", "tokens": ["Sieh", "!", "im", "Ei\u00b7sen\u00b7kleid", "ein", "Rei\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$.", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht auf krankem Ro\u00df daher,", "tokens": ["Zieht", "auf", "kran\u00b7kem", "Ro\u00df", "da\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "PAV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Senkt die Lanz\u2019, als m\u00fcder Streiter,", "tokens": ["Senkt", "die", "Lanz'", ",", "als", "m\u00fc\u00b7der", "Strei\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "KOUS", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Neigt das Haupt, wie schlummerschwer.", "tokens": ["Neigt", "das", "Haupt", ",", "wie", "schlum\u00b7mer\u00b7schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PWAV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "D\u00fcrre Wangen, graue Locken;", "tokens": ["D\u00fcr\u00b7re", "Wan\u00b7gen", ",", "grau\u00b7e", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Seiner Hand entfiel der Zaum.", "tokens": ["Sei\u00b7ner", "Hand", "ent\u00b7fiel", "der", "Zaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Pl\u00f6tzlich f\u00e4hrt er auf, erschrocken,", "tokens": ["Pl\u00f6tz\u00b7lich", "f\u00e4hrt", "er", "auf", ",", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "$,", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Wie erwacht aus bangem Traum.", "tokens": ["Wie", "er\u00b7wacht", "aus", "ban\u00b7gem", "Traum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "\u201eseyd gegr\u00fc\u00dft auf diesen Auen,", "tokens": ["\u201e", "seyd", "ge\u00b7gr\u00fc\u00dft", "auf", "die\u00b7sen", "Au\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VAFIN", "VVPP", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sch\u00f6nste Jungfrau, edle Herrn!", "tokens": ["Sch\u00f6ns\u00b7te", "Jung\u00b7frau", ",", "ed\u00b7le", "Herrn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "D\u00fcrfet nicht ob mir ergrauen,", "tokens": ["D\u00fcr\u00b7fet", "nicht", "ob", "mir", "er\u00b7grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "KOUS", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eure Spiele schau\u2019 ich gern.", "tokens": ["Eu\u00b7re", "Spie\u00b7le", "schau'", "ich", "gern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gerne m\u00f6cht\u2019 ich f\u00fcr mein Leben", "tokens": ["Ger\u00b7ne", "m\u00f6cht'", "ich", "f\u00fcr", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit euch brechen einen Speer,", "tokens": ["Mit", "euch", "bre\u00b7chen", "ei\u00b7nen", "Speer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Aber meine Arme beben,", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Ar\u00b7me", "be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Meine Kniee wanken sehr.", "tokens": ["Mei\u00b7ne", "Kni\u00b7ee", "wan\u00b7ken", "sehr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Kenne solche Zeitvertreibe,", "tokens": ["Ken\u00b7ne", "sol\u00b7che", "Zeit\u00b7ver\u00b7trei\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bin bei Lanz\u2019 und Schwerdt ergraut,", "tokens": ["Bin", "bei", "Lanz'", "und", "Schwerdt", "er\u00b7graut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "KON", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Panzer liegt mir noch am Leibe,", "tokens": ["Pan\u00b7zer", "liegt", "mir", "noch", "am", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dem Drachen seine Haut.", "tokens": ["Wie", "dem", "Dra\u00b7chen", "sei\u00b7ne", "Haut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Auf dem Lande Kampf und Wunden,", "tokens": ["Auf", "dem", "Lan\u00b7de", "Kampf", "und", "Wun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Meere Wog\u2019 und Sturm;", "tokens": ["Auf", "dem", "Mee\u00b7re", "Wo\u00b7g'", "und", "Sturm", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "KON", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Ruhe hab\u2019 ich nie gefunden,", "tokens": ["Ru\u00b7he", "hab'", "ich", "nie", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Als ein Jahr im finstern Thurm.", "tokens": ["Als", "ein", "Jahr", "im", "fins\u00b7tern", "Thurm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Weh! verlorne Tag\u2019 und N\u00e4chte!", "tokens": ["Weh", "!", "ver\u00b7lor\u00b7ne", "Tag'", "und", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Minne hat mich nie begl\u00fcckt;", "tokens": ["Min\u00b7ne", "hat", "mich", "nie", "be\u00b7gl\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nie hat dich, du rauhe Rechte!", "tokens": ["Nie", "hat", "dich", ",", "du", "rau\u00b7he", "Rech\u00b7te", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weiche Frauenhand gedr\u00fcckt.", "tokens": ["Wei\u00b7che", "Frau\u00b7en\u00b7hand", "ge\u00b7dr\u00fcckt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Denn noch war dem Erdenthale", "tokens": ["Denn", "noch", "war", "dem", "Er\u00b7dent\u00b7ha\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jene Blumenjungfrau fern,", "tokens": ["Je\u00b7ne", "Blu\u00b7men\u00b7jung\u00b7frau", "fern", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+++", "measure": "unknown.measure.penta"}, "line.7": {"text": "Die mir heut zum ersten Male", "tokens": ["Die", "mir", "heut", "zum", "ers\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Aufgeht, als ein neuer Stern.", "tokens": ["Auf\u00b7geht", ",", "als", "ein", "neu\u00b7er", "Stern", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Wehe! k\u00f6nnt\u2019 ich mich verj\u00fcngen!", "tokens": ["We\u00b7he", "!", "k\u00f6nnt'", "ich", "mich", "ver\u00b7j\u00fcn\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lernen wollt\u2019 ich Saitenkunst,", "tokens": ["Ler\u00b7nen", "wollt'", "ich", "Sai\u00b7ten\u00b7kunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Minnelieder wollt\u2019 ich singen,", "tokens": ["Min\u00b7ne\u00b7lie\u00b7der", "wollt'", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werbend um der S\u00fc\u00dfen Gunst.", "tokens": ["Wer\u00b7bend", "um", "der", "S\u00fc\u00b7\u00dfen", "Gunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "In des Maies holden Tagen,", "tokens": ["In", "des", "Mai\u00b7es", "hol\u00b7den", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "In der Aue Blumenglanz,", "tokens": ["In", "der", "Au\u00b7e", "Blu\u00b7men\u00b7glanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Wollt\u2019 ich freudig fechten, jagen,", "tokens": ["Wollt'", "ich", "freu\u00b7dig", "fech\u00b7ten", ",", "ja\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Um den werthen Rosenkranz.", "tokens": ["Um", "den", "wert\u00b7hen", "Ro\u00b7sen\u00b7kranz", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Weh! zu fr\u00fch bin ich geboren!", "tokens": ["Weh", "!", "zu", "fr\u00fch", "bin", "ich", "ge\u00b7bo\u00b7ren", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PTKA", "ADJD", "VAFIN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Erst beginnt die goldne Zeit.", "tokens": ["Erst", "be\u00b7ginnt", "die", "gold\u00b7ne", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zorn und Neid hat sich verloren,", "tokens": ["Zorn", "und", "Neid", "hat", "sich", "ver\u00b7lo\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Fr\u00fchling ewig sich erneut.", "tokens": ["Fr\u00fch\u00b7ling", "e\u00b7wig", "sich", "er\u00b7neut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PRF", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Sie, in ihrer Rosenlaube,", "tokens": ["Sie", ",", "in", "ih\u00b7rer", "Ro\u00b7sen\u00b7lau\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird des Reiches Herrin seyn.", "tokens": ["Wird", "des", "Rei\u00b7ches", "Her\u00b7rin", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ich mu\u00df hin zu Nacht und Staube,", "tokens": ["Ich", "mu\u00df", "hin", "zu", "Nacht", "und", "Stau\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Auf mich f\u00e4llt der Leichenstein!\u201c", "tokens": ["Auf", "mich", "f\u00e4llt", "der", "Lei\u00b7chen\u00b7stein", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Als der Alte dies gesprochen,", "tokens": ["Als", "der", "Al\u00b7te", "dies", "ge\u00b7spro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PDS", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Er die bleichen Lippen schlo\u00df.", "tokens": ["Er", "die", "blei\u00b7chen", "Lip\u00b7pen", "schlo\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seine Augen sind gebrochen,", "tokens": ["Sei\u00b7ne", "Au\u00b7gen", "sind", "ge\u00b7bro\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinken will er von dem Ro\u00df.", "tokens": ["Sin\u00b7ken", "will", "er", "von", "dem", "Ro\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch die edeln Knappen eilen,", "tokens": ["Doch", "die", "e\u00b7deln", "Knap\u00b7pen", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Legen ihn in\u2019s Gr\u00fcne hin;", "tokens": ["Le\u00b7gen", "ihn", "in's", "Gr\u00fc\u00b7ne", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Ach! kein Balsam kann ihn heilen,", "tokens": ["Ach", "!", "kein", "Bal\u00b7sam", "kann", "ihn", "hei\u00b7len", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PIAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Keine Stimme wecket ihn.", "