asure": "trochaic.tetra"}, "line.7": {"text": "Ob es gleich dir frommet wenig,", "tokens": ["Ob", "es", "gleich", "dir", "from\u00b7met", "we\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PPER", "VVFIN", "PIS", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Blumenkranz dem todten Mann.\u201c", "tokens": ["Blu\u00b7men\u00b7kranz", "dem", "tod\u00b7ten", "Mann", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}