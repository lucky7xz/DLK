{"dta.poem.20990": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "LiV .  \n Eines Pauren.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.85", "cy:0.14"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Das Erdreich gab mir Brod/ das Brod erhielt mein", "tokens": ["Das", "Er\u00b7dreich", "gab", "mir", "Brod", "/", "das", "Brod", "er\u00b7hielt", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$(", "ART", "NN", "VVFIN", "PPOSAT"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Leben/", "tokens": ["Le\u00b7ben", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Vor Brod hab ich das Fleisch der Erden hingegebe\u0303.", "tokens": ["Vor", "Brod", "hab", "ich", "das", "Fleisch", "der", "Er\u00b7den", "hin\u00b7ge\u00b7ge\u00b7b\u1ebd", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.4": {"text": "Ich gebe wol zu viel. Das Fleisch kam aus der Erden/", "tokens": ["Ich", "ge\u00b7be", "wol", "zu", "viel", ".", "Das", "Fleisch", "kam", "aus", "der", "Er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKA", "PIS", "$.", "ART", "NN", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und mu\u00df auch/ was es war/ in kurtzem wieder werde\u0303.", "tokens": ["Und", "mu\u00df", "auch", "/", "was", "es", "war", "/", "in", "kurt\u00b7zem", "wie\u00b7der", "werd\u1ebd", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "$(", "PWS", "PPER", "VAFIN", "$(", "APPR", "ADJA", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}