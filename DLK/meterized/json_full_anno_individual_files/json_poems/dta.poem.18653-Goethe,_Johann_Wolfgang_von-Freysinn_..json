{"dta.poem.18653": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang von", "birth": "N.A.", "death": "N.A."}, "title": "Freysinn .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1819", "urn": "urn:nbn:de:kobv:b4-200905191692", "language": ["de:0.99"], "booktitle": "Goethe, Johann Wolfgang von: West-\u00f6stlicher Divan. Stuttgart, 1819."}, "poem": {"stanza.1": {"line.1": {"text": "Lasst mich nur auf meinem Sattel gelten!", "tokens": ["Lasst", "mich", "nur", "auf", "mei\u00b7nem", "Sat\u00b7tel", "gel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Bleibt in euren H\u00fctten, euren Zelten!", "tokens": ["Bleibt", "in", "eu\u00b7ren", "H\u00fct\u00b7ten", ",", "eu\u00b7ren", "Zel\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und ich reite froh in alle Ferne,", "tokens": ["Und", "ich", "rei\u00b7te", "froh", "in", "al\u00b7le", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Ueber meiner M\u00fctze nur die Sterne.", "tokens": ["Ue\u00b7ber", "mei\u00b7ner", "M\u00fct\u00b7ze", "nur", "die", "Ster\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Er hat euch die Gestirne gesetzt", "tokens": ["Er", "hat", "euch", "die", "Ge\u00b7stir\u00b7ne", "ge\u00b7setzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ART", "NN", "VVPP"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Als Leiter zu Land und See;", "tokens": ["Als", "Lei\u00b7ter", "zu", "Land", "und", "See", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Damit ihr euch daran erg\u00f6tzt,", "tokens": ["Da\u00b7mit", "ihr", "euch", "da\u00b7ran", "er\u00b7g\u00f6tzt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Stets blickend in die H\u00f6h.", "tokens": ["Stets", "bli\u00b7ckend", "in", "die", "H\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}