{"dta.poem.5561": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von den Weltweisen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.85", "af:0.14"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie, oder denen, die vergn\u00fcgt", "tokens": ["Wie", ",", "o\u00b7der", "de\u00b7nen", ",", "die", "ver\u00b7gn\u00fcgt"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "$,", "KON", "PDS", "$,", "PRELS", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit der ", "tokens": ["Mit", "der"], "token_info": ["word", "word"], "pos": ["APPR", "ART"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ohn\u2019 da\u00df sich zu dem Geist ein fester Vorwurf f\u00fcg\u2019t;", "tokens": ["Ohn'", "da\u00df", "sich", "zu", "dem", "Geist", "ein", "fes\u00b7ter", "Vor\u00b7wurf", "f\u00fcg't", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PRF", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die gantz besch\u00e4fftiget mit einem ", "tokens": ["Die", "gantz", "be\u00b7sch\u00e4ff\u00b7ti\u00b7get", "mit", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "APPR", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und, mit ", "tokens": ["Und", ",", "mit"], "token_info": ["word", "punct", "word"], "pos": ["KON", "$,", "APPR"], "meter": "+-", "measure": "trochaic.single"}, "line.6": {"text": "Nur glauben: Wahrheits-Liebe sey", "tokens": ["Nur", "glau\u00b7ben", ":", "Wahr\u00b7heits\u00b7Lie\u00b7be", "sey"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "VVINF", "$.", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nichts als Betr\u00fcgerey?", "tokens": ["Nichts", "als", "Be\u00b7tr\u00fc\u00b7ge\u00b7rey", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}