{"dta.poem.5673": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mir ist nicht unbekannt,", "tokens": ["Mir", "ist", "nicht", "un\u00b7be\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df man gar offt verwegne Blinde fand,", "tokens": ["Da\u00df", "man", "gar", "offt", "ver\u00b7weg\u00b7ne", "Blin\u00b7de", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die frech gelehrt: als wenn die Heil\u2019gen Eigenschaften", "tokens": ["Die", "frech", "ge\u00b7lehrt", ":", "als", "wenn", "die", "Heil'\u00b7gen", "Ei\u00b7gen\u00b7schaf\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "VVPP", "$.", "KOKOM", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der ", "tokens": ["Der"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und eitle Phantasien w\u00e4ren.", "tokens": ["Und", "eit\u00b7le", "Phan\u00b7ta\u00b7si\u00b7en", "w\u00e4\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VAFIN", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.6": {"text": "\u201edie Menschen, sprachen sie, so voller Furcht und Schrecken,", "tokens": ["\u201e", "die", "Men\u00b7schen", ",", "spra\u00b7chen", "sie", ",", "so", "vol\u00b7ler", "Furcht", "und", "Schre\u00b7cken", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "\u201evoll Schwachheit, Pein und Elend stecken,", "tokens": ["\u201e", "voll", "Schwach\u00b7heit", ",", "Pein", "und", "E\u00b7lend", "ste\u00b7cken", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "NN", "$,", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "\u201edie haben sich, bald f\u00fcrchterliche G\u00f6tter,", "tokens": ["\u201e", "die", "ha\u00b7ben", "sich", ",", "bald", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "G\u00f6t\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "PRF", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "\u201ebald andre gegentheils als Helffer und Erretter,", "tokens": ["\u201e", "bald", "and\u00b7re", "ge\u00b7gen\u00b7theils", "als", "Helf\u00b7fer", "und", "Er\u00b7ret\u00b7ter", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "PIS", "ADV", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eselbst ausgedacht,", "tokens": ["\u201e", "selbst", "aus\u00b7ge\u00b7dacht", ","], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "ADV", "VVPP", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "\u201eund aufs Altar gebracht:", "tokens": ["\u201e", "und", "aufs", "Al\u00b7tar", "ge\u00b7bracht", ":"], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.12": {"text": "\u201egespenster, die sie haben wollten,", "tokens": ["\u201e", "ge\u00b7spens\u00b7ter", ",", "die", "sie", "ha\u00b7ben", "woll\u00b7ten", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "$,", "PRELS", "PPER", "VAINF", "VMFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "\u201eda\u00df sie sie r\u00e4chen bald-bald ihnen helffen sollten.", "tokens": ["\u201e", "da\u00df", "sie", "sie", "r\u00e4\u00b7chen", "bald\u00b7bald", "ih\u00b7nen", "helf\u00b7fen", "soll\u00b7ten", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "PPER", "PPER", "VVFIN", "ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}