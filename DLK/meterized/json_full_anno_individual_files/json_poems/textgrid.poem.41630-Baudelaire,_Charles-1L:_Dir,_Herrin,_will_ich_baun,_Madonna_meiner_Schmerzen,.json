{"textgrid.poem.41630": {"metadata": {"author": {"name": "Baudelaire, Charles", "birth": "N.A.", "death": "N.A."}, "title": "1L: Dir, Herrin, will ich baun, Madonna meiner Schmerzen,", "genre": "verse", "period": "N.A.", "pub_year": 1844, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dir, Herrin, will ich baun, Madonna meiner Schmerzen,", "tokens": ["Dir", ",", "Her\u00b7rin", ",", "will", "ich", "baun", ",", "Ma\u00b7don\u00b7na", "mei\u00b7ner", "Schmer\u00b7zen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "$,", "VMFIN", "PPER", "VVINF", "$,", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verborgenen Altar in meinem tiefsten Herzen,", "tokens": ["Ver\u00b7bor\u00b7ge\u00b7nen", "Al\u00b7tar", "in", "mei\u00b7nem", "tiefs\u00b7ten", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Dir in des Busens Nacht errichten einen Thron,", "tokens": ["Dir", "in", "des", "Bu\u00b7sens", "Nacht", "er\u00b7rich\u00b7ten", "ei\u00b7nen", "Thron", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fern weltlicher Begier und kalter Blicke Hohn,", "tokens": ["Fern", "welt\u00b7li\u00b7cher", "Be\u00b7gier", "und", "kal\u00b7ter", "Bli\u00b7cke", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PWAT", "NN", "KON", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "In einer Nische von Azur und goldnem Flitter,", "tokens": ["In", "ei\u00b7ner", "Ni\u00b7sche", "von", "A\u00b7zur", "und", "gold\u00b7nem", "Flit\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NE", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Wo einem Standbild gleich du l\u00e4chelst durch das Gitter,", "tokens": ["Wo", "ei\u00b7nem", "Stand\u00b7bild", "gleich", "du", "l\u00e4\u00b7chelst", "durch", "das", "Git\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das meine Verse dir geschmiedet aus Metall,", "tokens": ["Das", "mei\u00b7ne", "Ver\u00b7se", "dir", "ge\u00b7schmie\u00b7det", "aus", "Me\u00b7tall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Das wunderbar geschm\u00fcckt mit Reimen von Kristall.", "tokens": ["Das", "wun\u00b7der\u00b7bar", "ge\u00b7schm\u00fcckt", "mit", "Rei\u00b7men", "von", "Kris\u00b7tall", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVPP", "APPR", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.9": {"text": "Dein sei ein Diadem, das leuchtet wie die Sonne.", "tokens": ["Dein", "sei", "ein", "Di\u00b7a\u00b7dem", ",", "das", "leuch\u00b7tet", "wie", "die", "Son\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VAFIN", "ART", "NN", "$,", "PDS", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+--+--+-+-+-", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "In meiner Eifersucht, o sterbliche Madonne,", "tokens": ["In", "mei\u00b7ner", "Ei\u00b7fer\u00b7sucht", ",", "o", "sterb\u00b7li\u00b7che", "Ma\u00b7don\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "FM", "FM", "FM", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Will ich umkleiden dich mit starren Mantels Pracht,", "tokens": ["Will", "ich", "um\u00b7klei\u00b7den", "dich", "mit", "star\u00b7ren", "Man\u00b7tels", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "PRF", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Barbarisch, steif und schwer, gepanzert mit Verdacht,", "tokens": ["Bar\u00b7ba\u00b7risch", ",", "steif", "und", "schwer", ",", "ge\u00b7pan\u00b7zert", "mit", "Ver\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "KON", "ADJD", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.13": {"text": "Der einer R\u00fcstung gleich den sch\u00f6nen Leib umschimmert", "tokens": ["Der", "ei\u00b7ner", "R\u00fcs\u00b7tung", "gleich", "den", "sch\u00f6\u00b7nen", "Leib", "um\u00b7schim\u00b7mert"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ADV", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und nicht von Perlen, nein, von meinen Tr\u00e4nen flimmert,", "tokens": ["Und", "nicht", "von", "Per\u00b7len", ",", "nein", ",", "von", "mei\u00b7nen", "Tr\u00e4\u00b7nen", "flim\u00b7mert", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "$,", "PTKANT", "$,", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Mein Sehnen sei dein Kleid, das bebend sich dir neigt,", "tokens": ["Mein", "Seh\u00b7nen", "sei", "dein", "Kleid", ",", "das", "be\u00b7bend", "sich", "dir", "neigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "ADJD", "PRF", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Mein Sehnen, wellengleich, das niedersinkt und steigt.", "tokens": ["Mein", "Seh\u00b7nen", ",", "wel\u00b7len\u00b7gleich", ",", "das", "nie\u00b7der\u00b7sinkt", "und", "steigt", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADJD", "$,", "PDS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Sich wiegend auf den H\u00f6hn, im Tal nach Rast verlangend,", "tokens": ["Sich", "wie\u00b7gend", "auf", "den", "H\u00f6hn", ",", "im", "Tal", "nach", "Rast", "ver\u00b7lan\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVPP", "APPR", "ART", "NN", "$,", "APPRART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Den wei\u00df und rosgen Leib mit einem Ku\u00df umfangend.", "tokens": ["Den", "wei\u00df", "und", "ros\u00b7gen", "Leib", "mit", "ei\u00b7nem", "Ku\u00df", "um\u00b7fan\u00b7gend", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Aus Ehrfurcht wirk ich dir der Seidenschuhe Paar", "tokens": ["Aus", "Ehr\u00b7furcht", "wirk", "ich", "dir", "der", "Sei\u00b7den\u00b7schu\u00b7he", "Paar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPER", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und bring den F\u00fc\u00dfen sie, den g\u00f6ttergleichen, dar,", "tokens": ["Und", "bring", "den", "F\u00fc\u00b7\u00dfen", "sie", ",", "den", "g\u00f6t\u00b7ter\u00b7glei\u00b7chen", ",", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPER", "$,", "ART", "ADJA", "$,", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Da\u00df dich umschlie\u00dfend sie in einer z\u00e4rtlich-leisen", "tokens": ["Da\u00df", "dich", "um\u00b7schlie\u00b7\u00dfend", "sie", "in", "ei\u00b7ner", "z\u00e4rt\u00b7lich\u00b7lei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Umarmung mir getreu der F\u00fc\u00dfe Abbild weisen.", "tokens": ["Um\u00b7ar\u00b7mung", "mir", "ge\u00b7treu", "der", "F\u00fc\u00b7\u00dfe", "Ab\u00b7bild", "wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und kann trotz aller Kunst, der ich von je gewohnt,", "tokens": ["Und", "kann", "trotz", "al\u00b7ler", "Kunst", ",", "der", "ich", "von", "je", "ge\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PIAT", "NN", "$,", "PRELS", "PPER", "APPR", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich nicht als Schemel dir verleihn den Silbermond,", "tokens": ["Ich", "nicht", "als", "Sche\u00b7mel", "dir", "ver\u00b7leihn", "den", "Sil\u00b7ber\u00b7mond", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "KOUS", "NN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Leg ich die Schlange, die mir grimm das Herz zerbissen,", "tokens": ["Leg", "ich", "die", "Schlan\u00b7ge", ",", "die", "mir", "grimm", "das", "Herz", "zer\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu F\u00fc\u00dfen dir, da\u00df du gleich einer sieggewissen,", "tokens": ["Zu", "F\u00fc\u00b7\u00dfen", "dir", ",", "da\u00df", "du", "gleich", "ei\u00b7ner", "sieg\u00b7ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hilfreichen K\u00f6nigin, stolz l\u00e4chelnd niedertrittst", "tokens": ["Hilf\u00b7rei\u00b7chen", "K\u00f6\u00b7ni\u00b7gin", ",", "stolz", "l\u00e4\u00b7chelnd", "nie\u00b7der\u00b7trittst"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "ADJD", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Das Unget\u00fcm, das Ha\u00df und giftgen Geifer spritzt.", "tokens": ["Das", "Un\u00b7ge\u00b7t\u00fcm", ",", "das", "Ha\u00df", "und", "gift\u00b7gen", "Gei\u00b7fer", "spritzt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dir will ich, K\u00f6nigin der Jungfraun, all mein Denken", "tokens": ["Dir", "will", "ich", ",", "K\u00f6\u00b7ni\u00b7gin", "der", "Jung\u00b7fraun", ",", "all", "mein", "Den\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "$,", "NN", "ART", "NN", "$,", "PIAT", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vor blumigem Altar gleich Weihekerzen schenken,", "tokens": ["Vor", "blu\u00b7mi\u00b7gem", "Al\u00b7tar", "gleich", "Wei\u00b7he\u00b7ker\u00b7zen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf da\u00df besternend sie erhellter W\u00f6lbung Blaun", "tokens": ["Auf", "da\u00df", "bes\u00b7ter\u00b7nend", "sie", "er\u00b7hell\u00b7ter", "W\u00f6l\u00b7bung", "Blaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ADJD", "PPER", "ADJA", "NN", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Nach dir nur allezeit mit Flammenaugen schaun.", "tokens": ["Nach", "dir", "nur", "al\u00b7le\u00b7zeit", "mit", "Flam\u00b7men\u00b7au\u00b7gen", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie meine W\u00fcnsche all um dich bezaubert irren,", "tokens": ["Wie", "mei\u00b7ne", "W\u00fcn\u00b7sche", "all", "um", "dich", "be\u00b7zau\u00b7bert", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PIAT", "APPR", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird alles Oliban und Benzoe und Myrrhen;", "tokens": ["Wird", "al\u00b7les", "O\u00b7li\u00b7ban", "und", "Ben\u00b7zoe", "und", "Myr\u00b7rhen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NE", "KON", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Zu dir, verschneit Gebirg, hebt still und feierlich", "tokens": ["Zu", "dir", ",", "ver\u00b7schneit", "Ge\u00b7birg", ",", "hebt", "still", "und", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADJD", "NN", "$,", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein sturmesdunkler Geist in Weihrauchwolken sich.", "tokens": ["Mein", "stur\u00b7mes\u00b7dunk\u00b7ler", "Geist", "in", "Weih\u00b7rauch\u00b7wol\u00b7ken", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Da\u00df du das Ebenbild der Jungfrau m\u00f6gest scheinen,", "tokens": ["Da\u00df", "du", "das", "E\u00b7ben\u00b7bild", "der", "Jung\u00b7frau", "m\u00f6\u00b7gest", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will gl\u00fchnde Liebe ich mit Grausamkeit vereinen,", "tokens": ["Will", "gl\u00fchn\u00b7de", "Lie\u00b7be", "ich", "mit", "Grau\u00b7sam\u00b7keit", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tods\u00fcnden w\u00e4hl ich mir in heilger Siebenzahl,", "tokens": ["To\u00b7ds\u00fcn\u00b7den", "w\u00e4hl", "ich", "mir", "in", "heil\u00b7ger", "Sie\u00b7ben\u00b7zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PRF", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein reuger Henker sch\u00e4rf ich Dolche draus von Stahl,", "tokens": ["Ein", "reu\u00b7ger", "Hen\u00b7ker", "sch\u00e4rf", "ich", "Dol\u00b7che", "draus", "von", "Stahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "PAV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und einem Gaukler gleich in seelenlosem Spiele", "tokens": ["Und", "ei\u00b7nem", "Gauk\u00b7ler", "gleich", "in", "see\u00b7len\u00b7lo\u00b7sem", "Spie\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Nehm deiner Liebe