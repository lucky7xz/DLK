measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vor blumigem Altar gleich Weihekerzen schenken,", "tokens": ["Vor", "blu\u00b7mi\u00b7gem", "Al\u00b7tar", "gleich", "Wei\u00b7he\u00b7ker\u00b7zen", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Auf da\u00df besternend sie erhellter W\u00f6lbung Blaun", "tokens": ["Auf", "da\u00df", "bes\u00b7ter\u00b7nend", "sie", "er\u00b7hell\u00b7ter", "W\u00f6l\u00b7bung", "Blaun"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "KOUS", "ADJD", "PPER", "ADJA", "NN", "NN"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.10": {"text": "Nach dir nur allezeit mit Flammenaugen schaun.", "tokens": ["Nach", "dir", "nur", "al\u00b7le\u00b7zeit", "mit", "Flam\u00b7men\u00b7au\u00b7gen", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ADV", "ADV", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Wie meine W\u00fcnsche all um dich bezaubert irren,", "tokens": ["Wie", "mei\u00b7ne", "W\u00fcn\u00b7sche", "all", "um", "dich", "be\u00b7zau\u00b7bert", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "PIAT", "APPR", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wird alles Oliban und Benzoe und Myrrhen;", "tokens": ["Wird", "al\u00b7les", "O\u00b7li\u00b7ban", "und", "Ben\u00b7zoe", "und", "Myr\u00b7rhen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "NE", "KON", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Zu dir, verschneit Gebirg, hebt still und feierlich", "tokens": ["Zu", "dir", ",", "ver\u00b7schneit", "Ge\u00b7birg", ",", "hebt", "still", "und", "fei\u00b7er\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "$,", "ADJD", "NN", "$,", "VVFIN", "PTKVZ", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Mein sturmesdunkler Geist in Weihrauchwolken sich.", "tokens": ["Mein", "stur\u00b7mes\u00b7dunk\u00b7ler", "Geist", "in", "Weih\u00b7rauch\u00b7wol\u00b7ken", "sich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "NN", "PRF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Da\u00df du das Ebenbild der Jungfrau m\u00f6gest scheinen,", "tokens": ["Da\u00df", "du", "das", "E\u00b7ben\u00b7bild", "der", "Jung\u00b7frau", "m\u00f6\u00b7gest", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will gl\u00fchnde Liebe ich mit Grausamkeit vereinen,", "tokens": ["Will", "gl\u00fchn\u00b7de", "Lie\u00b7be", "ich", "mit", "Grau\u00b7sam\u00b7keit", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADJA", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Tods\u00fcnden w\u00e4hl ich mir in heilger Siebenzahl,", "tokens": ["To\u00b7ds\u00fcn\u00b7den", "w\u00e4hl", "ich", "mir", "in", "heil\u00b7ger", "Sie\u00b7ben\u00b7zahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "wor