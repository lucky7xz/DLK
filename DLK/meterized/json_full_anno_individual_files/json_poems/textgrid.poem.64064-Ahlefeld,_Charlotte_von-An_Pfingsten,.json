{"textgrid.poem.64064": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "An Pfingsten,", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sey mir gesegnet, Du, den Menschenliebe", "tokens": ["Sey", "mir", "ge\u00b7seg\u00b7net", ",", "Du", ",", "den", "Men\u00b7schen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "$,", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Beim schweren Amte unterst\u00fctzt mit Kraft,", "tokens": ["Beim", "schwe\u00b7ren", "Am\u00b7te", "un\u00b7ter\u00b7st\u00fctzt", "mit", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du, der im Chaos dumpf verworrner Triebe,", "tokens": ["Du", ",", "der", "im", "Chaos", "dumpf", "ver\u00b7worr\u00b7ner", "Trie\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "APPRART", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Geistes Leben v\u00e4terlich erschafft.", "tokens": ["Des", "Geis\u00b7tes", "Le\u00b7ben", "v\u00e4\u00b7ter\u00b7lich", "er\u00b7schafft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "In nimmer aufgehellten Finsternissen", "tokens": ["In", "nim\u00b7mer", "auf\u00b7ge\u00b7hell\u00b7ten", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Erweckst Du milde der Erkenntniss Strahl,", "tokens": ["Er\u00b7weckst", "Du", "mil\u00b7de", "der", "Er\u00b7kennt\u00b7niss", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und linderst sanft durch Unterricht und Wissen", "tokens": ["Und", "lin\u00b7derst", "sanft", "durch", "Un\u00b7ter\u00b7richt", "und", "Wis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Sprache Mangel und der Taubheit Qual.", "tokens": ["Der", "Spra\u00b7che", "Man\u00b7gel", "und", "der", "Taub\u00b7heit", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O fahre fort! \u2013 Es ziemte eine Krone", "tokens": ["O", "fah\u00b7re", "fort", "!", "\u2013", "Es", "ziem\u00b7te", "ei\u00b7ne", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKVZ", "$.", "$(", "PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zum wohlverdienten Schmucke Deinem Haupt,", "tokens": ["Zum", "wohl\u00b7ver\u00b7dien\u00b7ten", "Schmu\u00b7cke", "Dei\u00b7nem", "Haupt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch heil'ger ist der Kranz, der ihn umlaubt.", "tokens": ["Doch", "heil'\u00b7ger", "ist", "der", "Kranz", ",", "der", "ihn", "um\u00b7laubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er welket nie, denn Deine Thaten leben,", "tokens": ["Er", "wel\u00b7ket", "nie", ",", "denn", "Dei\u00b7ne", "Tha\u00b7ten", "le\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KON", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und Dein Bewusstseyn und Dein reines Streben", "tokens": ["Und", "Dein", "Be\u00b7wuss\u00b7tseyn", "und", "Dein", "rei\u00b7nes", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "KON", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--++-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Vereinigen sich schon zum h\u00f6chsten Lohne.", "tokens": ["Ver\u00b7ei\u00b7ni\u00b7gen", "sich", "schon", "zum", "h\u00f6chs\u00b7ten", "Loh\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Sey mir gesegnet, Du, den Menschenliebe", "tokens": ["Sey", "mir", "ge\u00b7seg\u00b7net", ",", "Du", ",", "den", "Men\u00b7schen\u00b7lie\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "PPER", "$,", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Beim schweren Amte unterst\u00fctzt mit Kraft,", "tokens": ["Beim", "schwe\u00b7ren", "Am\u00b7te", "un\u00b7ter\u00b7st\u00fctzt", "mit", "Kraft", ","], "token_info": ["word", "word", "word", "wor