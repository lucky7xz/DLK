{"textgrid.poem.31175": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "An mehrere Kritiker", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ja, diese Welt starrt voller Klippen,", "tokens": ["Ja", ",", "die\u00b7se", "Welt", "starrt", "vol\u00b7ler", "Klip\u00b7pen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PDAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Jeder sehe, wie er's treibt;", "tokens": ["Ein", "Je\u00b7der", "se\u00b7he", ",", "wie", "er's", "treibt", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Denn glattrasirt wie eure Lippen,", "tokens": ["Denn", "glatt\u00b7ra\u00b7sirt", "wie", "eu\u00b7re", "Lip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sind auch die Worte, die ihr schreibt!", "tokens": ["Sind", "auch", "die", "Wor\u00b7te", ",", "die", "ihr", "schreibt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Auch seid ihr durch und durch \u00bbaesthetisch\u00ab", "tokens": ["Auch", "seid", "ihr", "durch", "und", "durch", "\u00bb", "a\u00b7e\u00b7sthe\u00b7tisch", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPR", "KON", "