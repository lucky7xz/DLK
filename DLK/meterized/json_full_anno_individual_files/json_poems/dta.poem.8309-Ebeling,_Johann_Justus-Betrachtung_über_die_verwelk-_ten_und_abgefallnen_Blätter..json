{"dta.poem.8309": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtung \u00fcber die verwelk-  \n ten und abgefallnen Bl\u00e4tter.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198782", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Da liegen die durch Sturm und Wetter,", "tokens": ["Da", "lie\u00b7gen", "die", "durch", "Sturm", "und", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von B\u00e4umen abgestreiften Bl\u00e4tter", "tokens": ["Von", "B\u00e4u\u00b7men", "ab\u00b7ge\u00b7streif\u00b7ten", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die ihrer Gipfel runden Kranz", "tokens": ["Die", "ih\u00b7rer", "Gip\u00b7fel", "run\u00b7den", "Kranz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Geschm\u00fckt mit einem g\u00fcldnen Glanz;", "tokens": ["Ge\u00b7schm\u00fckt", "mit", "ei\u00b7nem", "g\u00fcld\u00b7nen", "Glanz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie liegen nun zu deren F\u00fcssen,", "tokens": ["Sie", "lie\u00b7gen", "nun", "zu", "de\u00b7ren", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PRELAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die sie vorhero zieren m\u00fcssen.", "tokens": ["Die", "sie", "vor\u00b7he\u00b7ro", "zie\u00b7ren", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "VVINF", "VMINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Vorgestern sahe man mit Prangen,", "tokens": ["Vor\u00b7ges\u00b7tern", "sa\u00b7he", "man", "mit", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie noch auf ihrem Gipfel hangen", "tokens": ["Sie", "noch", "auf", "ih\u00b7rem", "Gip\u00b7fel", "han\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da ihr in gelb verwandelt Gr\u00fcn,", "tokens": ["Da", "ihr", "in", "gelb", "ver\u00b7wan\u00b7delt", "Gr\u00fcn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJD", "VVPP", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie ein bemahltes Goldblech schien;", "tokens": ["Wie", "ein", "be\u00b7mahl\u00b7tes", "Gold\u00b7blech", "schien", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sind zerstreuet von den Winden,", "tokens": ["Sie", "sind", "zer\u00b7streu\u00b7et", "von", "den", "Win\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und auf bemoosten Grund zu finden.", "tokens": ["Und", "auf", "be\u00b7moos\u00b7ten", "Grund", "zu", "fin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie liegen in verworfner Menge,", "tokens": ["Sie", "lie\u00b7gen", "in", "ver\u00b7worf\u00b7ner", "Men\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch als ein g\u00fcldenes Gepr\u00e4nge,", "tokens": ["Noch", "als", "ein", "g\u00fcl\u00b7de\u00b7nes", "Ge\u00b7pr\u00e4n\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und scheinen bei dem Sonnenstrahl,", "tokens": ["Und", "schei\u00b7nen", "bei", "dem", "Son\u00b7nen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als eine grosse M\u00fcnzen Zahl:", "tokens": ["Als", "ei\u00b7ne", "gros\u00b7se", "M\u00fcn\u00b7zen", "Zahl", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jedoch in wenig Tages-Stunden,", "tokens": ["Je\u00b7doch", "in", "we\u00b7nig", "Ta\u00b7ges\u00b7Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind sie aus dem Gesicht verschwunden.", "tokens": ["Sind", "sie", "aus", "dem", "Ge\u00b7sicht", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die N\u00e4sse schmelzt sie in der Eile,", "tokens": ["Die", "N\u00e4s\u00b7se", "schmelzt", "sie", "in", "der", "Ei\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und bringt sie zur geschwinden F\u00e4ule,", "tokens": ["Und", "bringt", "sie", "zur", "ge\u00b7schwin\u00b7den", "F\u00e4u\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Daraus wenn sie in Mist verkehrt,", "tokens": ["Da\u00b7raus", "wenn", "sie", "in", "Mist", "ver\u00b7kehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich k\u00fcnftig das Gestr\u00e4uche n\u00e4hrt,", "tokens": ["Sich", "k\u00fcnf\u00b7tig", "das", "Ge\u00b7str\u00e4u\u00b7che", "n\u00e4hrt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da sie in Lenz aus andern B\u00e4umen,", "tokens": ["Da", "sie", "in", "Lenz", "aus", "an\u00b7dern", "B\u00e4u\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit sch\u00f6nen Flor von neuen keimen.", "tokens": ["Mit", "sch\u00f6\u00b7nen", "Flor", "von", "neu\u00b7en", "kei\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da sehet ihr! ihr stolzen Reichen,", "tokens": ["Da", "se\u00b7het", "ihr", "!", "ihr", "stol\u00b7zen", "Rei\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ihr den B\u00e4umen zu vergleichen,", "tokens": ["Die", "ihr", "den", "B\u00e4u\u00b7men", "zu", "ver\u00b7glei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das Schiksal eurer G\u00fcter an,", "tokens": ["Das", "Schik\u00b7sal", "eu\u00b7rer", "G\u00fc\u00b7ter", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und wie es euch ergehen kan,", "tokens": ["Und", "wie", "es", "euch", "er\u00b7ge\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn euch der Winter sp\u00e4ter Jahre,", "tokens": ["Wenn", "euch", "der", "Win\u00b7ter", "sp\u00e4\u00b7ter", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zulezt bringt zu der Todten-Baare.", "tokens": ["Zu\u00b7lezt", "bringt", "zu", "der", "Tod\u00b7ten\u00b7Baa\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Jhr prangt jezo mit euren Sch\u00e4zen,", "tokens": ["Ihr", "prangt", "je\u00b7zo", "mit", "eu\u00b7ren", "Sch\u00e4\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr k\u00f6nnt am Mammon euch erg\u00f6zen,", "tokens": ["Ihr", "k\u00f6nnt", "am", "Mam\u00b7mon", "euch", "er\u00b7g\u00f6\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Allein was ist das eitle Geld,", "tokens": ["Al\u00b7lein", "was", "ist", "das", "eit\u00b7le", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das euer Herze an sich h\u00e4lt,", "tokens": ["Das", "eu\u00b7er", "Her\u00b7ze", "an", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ist das Gold, das euch so zieret?", "tokens": ["Was", "ist", "das", "Gold", ",", "das", "euch", "so", "zie\u00b7ret", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schmuk der sich gar bald verlieret.", "tokens": ["Ein", "Schmuk", "der", "sich", "gar", "bald", "ver\u00b7lie\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PRF", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Des Sommers warme Sonnenblikke,", "tokens": ["Des", "Som\u00b7mers", "war\u00b7me", "Son\u00b7nen\u00b7blik\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und euer scheinendes Gel\u00fckke:", "tokens": ["Und", "eu\u00b7er", "schei\u00b7nen\u00b7des", "Ge\u00b7l\u00fck\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die dauren eine kurze Zeit,", "tokens": ["Die", "dau\u00b7ren", "ei\u00b7ne", "kur\u00b7ze", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es folgen auf die Heiterkeit", "tokens": ["Es", "fol\u00b7gen", "auf", "die", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des Sommers, kalte Witterungen,", "tokens": ["Des", "Som\u00b7mers", ",", "kal\u00b7te", "Wit\u00b7te\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und auf das Gl\u00fck stets Aenderungen.", "tokens": ["Und", "auf", "das", "Gl\u00fck", "stets", "A\u00b7en\u00b7de\u00b7run\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "NE", "$."], "meter": "-+-+----+-", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Der Zeiten Wechsel sind geschwinde,", "tokens": ["Der", "Zei\u00b7ten", "Wech\u00b7sel", "sind", "ge\u00b7schwin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Schiksahls st\u00fcrmerische Winde,", "tokens": ["Des", "Schik\u00b7sahls", "st\u00fcr\u00b7me\u00b7ri\u00b7sche", "Win\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erheben sich als wie ein Nord,", "tokens": ["Er\u00b7he\u00b7ben", "sich", "als", "wie", "ein", "Nord", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und treiben von uns wieder fort,", "tokens": ["Und", "trei\u00b7ben", "von", "uns", "wie\u00b7der", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was wir vorher mit Lust besessen,", "tokens": ["Was", "wir", "vor\u00b7her", "mit", "Lust", "be\u00b7ses\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird schmerzhaft wiederum vergessen.", "tokens": ["Wird", "schmerz\u00b7haft", "wie\u00b7de\u00b7rum", "ver\u00b7ges\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Die Kr\u00e4fte der Natur vergehen,", "tokens": ["Die", "Kr\u00e4f\u00b7te", "der", "Na\u00b7tur", "ver\u00b7ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr plaget euch mit kranken Wehen,", "tokens": ["Ihr", "pla\u00b7get", "euch", "mit", "kran\u00b7ken", "We\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die eur erstarrter K\u00f6rper f\u00fchlt,", "tokens": ["Die", "eur", "er\u00b7starr\u00b7ter", "K\u00f6r\u00b7per", "f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Krankheit, wie ein Pfeil durchw\u00fchlt,", "tokens": ["Den", "Krank\u00b7heit", ",", "wie", "ein", "Pfeil", "durch\u00b7w\u00fchlt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Jhr seht, wenn ihr so \u00e4ngstlich keichet,", "tokens": ["Ihr", "seht", ",", "wenn", "ihr", "so", "\u00e4ngst\u00b7lich", "kei\u00b7chet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Gold und Reichthum von euch weichet.", "tokens": ["Wie", "Gold", "und", "Reicht\u00b7hum", "von", "euch", "wei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "NN", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Da liegt es noch zu euren F\u00fcssen,", "tokens": ["Da", "liegt", "es", "noch", "zu", "eu\u00b7ren", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhr m\u00fc\u00dft die starren Augen schliessen,", "tokens": ["Ihr", "m\u00fc\u00dft", "die", "star\u00b7ren", "Au\u00b7gen", "schlies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr werft noch einen matten Blik,", "tokens": ["Ihr", "werft", "noch", "ei\u00b7nen", "mat\u00b7ten", "Blik", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Auf euren gelben Schaz zur\u00fck:", "tokens": ["Auf", "eu\u00b7ren", "gel\u00b7ben", "Schaz", "zu\u00b7r\u00fck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und seht bei eurem kranken Sterben,", "tokens": ["Und", "seht", "bei", "eu\u00b7rem", "kran\u00b7ken", "Ster\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ihn zertheilen frohe Erben.", "tokens": ["Wie", "ihn", "zer\u00b7thei\u00b7len", "fro\u00b7he", "Er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Jhr f\u00fchlt, da\u00df euch der Geiz gereuet,", "tokens": ["Ihr", "f\u00fchlt", ",", "da\u00df", "euch", "der", "Geiz", "ge\u00b7reu\u00b7et", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Da ihr den Reichthum schon zerstreuet", "tokens": ["Da", "ihr", "den", "Reicht\u00b7hum", "schon", "zer\u00b7streu\u00b7et"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als wie von Wind, zertheilt, verweht,", "tokens": ["Als", "wie", "von", "Wind", ",", "zer\u00b7theilt", ",", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "KOKOM", "APPR", "NN", "$,", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in der Welt verfliegen seht.", "tokens": ["Und", "in", "der", "Welt", "ver\u00b7flie\u00b7gen", "seht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Was ihr so \u00e4ngstlich aufgehoben,", "tokens": ["Was", "ihr", "so", "\u00e4ngst\u00b7lich", "auf\u00b7ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist lustig wiederum zerstoben.", "tokens": ["Ist", "lus\u00b7tig", "wie\u00b7de\u00b7rum", "zer\u00b7sto\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Der eine i\u00e4gt es durch die Kehlen,", "tokens": ["Der", "ei\u00b7ne", "i\u00b7\u00e4gt", "es", "durch", "die", "Keh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und macht sich von den sauren Qu\u00e4len,", "tokens": ["Und", "macht", "sich", "von", "den", "sau\u00b7ren", "Qu\u00e4\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Von eurer Kummervollen Plag,", "tokens": ["Von", "eu\u00b7rer", "Kum\u00b7mer\u00b7vol\u00b7len", "Plag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun einen lustig guten Tag:", "tokens": ["Nun", "ei\u00b7nen", "lus\u00b7tig", "gu\u00b7ten", "Tag", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er l\u00e4st eur Gold in Wein zerfliessen,", "tokens": ["Er", "l\u00e4st", "eur", "Gold", "in", "Wein", "zer\u00b7flies\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Was ihr erspart, wil er geniessen.", "tokens": ["Was", "ihr", "er\u00b7spart", ",", "wil", "er", "ge\u00b7nies\u00b7sen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VMFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Der andre tr\u00e4gt auf andre Weise,", "tokens": ["Der", "and\u00b7re", "tr\u00e4gt", "auf", "and\u00b7re", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch eine weit entfernte Reise", "tokens": ["Durch", "ei\u00b7ne", "weit", "ent\u00b7fern\u00b7te", "Rei\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den Mammon, euer Gut und Geld,", "tokens": ["Den", "Mam\u00b7mon", ",", "eu\u00b7er", "Gut", "und", "Geld", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In alle L\u00e4nder dieser Welt:", "tokens": ["In", "al\u00b7le", "L\u00e4n\u00b7der", "die\u00b7ser", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es schmelzt der Abgott den ihr ehret,", "tokens": ["Es", "schmelzt", "der", "Ab\u00b7gott", "den", "ihr", "eh\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der euch durch seinen Schein beth\u00f6ret.", "tokens": ["Der", "euch", "durch", "sei\u00b7nen", "Schein", "be\u00b7th\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Und der den ihr gar oft nicht kennet,", "tokens": ["Und", "der", "den", "ihr", "gar", "oft", "nicht", "ken\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "PPER", "ADV", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Den ihr auch euer Gut misg\u00f6nnet,", "tokens": ["Den", "ihr", "auch", "eu\u00b7er", "Gut", "mis\u00b7g\u00f6n\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erlangt es, und er steigt empor,", "tokens": ["Er\u00b7langt", "es", ",", "und", "er", "steigt", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KON", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und k\u00f6mmt dadurch in sch\u00f6nen Flor:", "tokens": ["Und", "k\u00f6mmt", "da\u00b7durch", "in", "sch\u00f6\u00b7nen", "Flor", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wunderbahr gehts mit den Dingen,", "tokens": ["So", "wun\u00b7der\u00b7bahr", "gehts", "mit", "den", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die wir mit M\u00fch zusammen bringen.", "tokens": ["Die", "wir", "mit", "M\u00fch", "zu\u00b7sam\u00b7men", "brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "Und wolt ihr das im Beispiel sehen,", "tokens": ["Und", "wolt", "ihr", "das", "im", "Bei\u00b7spiel", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PDS", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So merket nur der Bl\u00e4tter Wehen:", "tokens": ["So", "mer\u00b7ket", "nur", "der", "Bl\u00e4t\u00b7ter", "We\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und schaut im Herbst es an, und glaubt,", "tokens": ["Und", "schaut", "im", "Herbst", "es", "an", ",", "und", "glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "PPER", "PTKVZ", "$,", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So wie der Baum da wird entlaubt:", "tokens": ["So", "wie", "der", "Baum", "da", "wird", "ent\u00b7laubt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird dereinsten eur Vergn\u00fcgen,", "tokens": ["So", "wird", "de\u00b7reins\u00b7ten", "eur", "Ver\u00b7gn\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Von euch fort, auseinander fliegen.", "tokens": ["Von", "euch", "fort", ",", "aus\u00b7ein\u00b7an\u00b7der", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}