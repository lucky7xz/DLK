{"dta.poem.10429": {"metadata": {"author": {"name": "Heine, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "LiI.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1827", "urn": "urn:nbn:de:kobv:b4-200905192211", "language": ["de:0.99"], "booktitle": "Heine, Heinrich: Buch der Lieder. Hamburg, 1827."}, "poem": {"stanza.1": {"line.1": {"text": "Vergiftet sind meine Lieder;", "tokens": ["Ver\u00b7gif\u00b7tet", "sind", "mei\u00b7ne", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie k\u00f6nnt' es anders seyn?", "tokens": ["Wie", "k\u00f6nnt'", "es", "an\u00b7ders", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Du hast mir ja Gift gegossen", "tokens": ["Du", "hast", "mir", "ja", "Gift", "ge\u00b7gos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "NN", "VVPP"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "In's bl\u00fchende Leben hinein.", "tokens": ["In's", "bl\u00fc\u00b7hen\u00b7de", "Le\u00b7ben", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Vergiftet sind meine Lieder;", "tokens": ["Ver\u00b7gif\u00b7tet", "sind", "mei\u00b7ne", "Lie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Wie k\u00f6nnt' es anders seyn?", "tokens": ["Wie", "k\u00f6nnt'", "es", "an\u00b7ders", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich trage im Herzen viel Schlangen,", "tokens": ["Ich", "tra\u00b7ge", "im", "Her\u00b7zen", "viel", "Schlan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "PIAT", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und dich, Geliebte mein.", "tokens": ["Und", "dich", ",", "Ge\u00b7lieb\u00b7te", "mein", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "$,", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}