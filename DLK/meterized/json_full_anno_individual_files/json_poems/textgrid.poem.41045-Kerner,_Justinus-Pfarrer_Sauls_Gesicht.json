{"textgrid.poem.41045": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Pfarrer Sauls Gesicht", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Saul schuf sich Himmel, schuf sich Gott", "tokens": ["Saul", "schuf", "sich", "Him\u00b7mel", ",", "schuf", "sich", "Gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "NN", "$,", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nach eignem bunten Dichten,", "tokens": ["Nach", "eig\u00b7nem", "bun\u00b7ten", "Dich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Wunden Jesu sind ihm Spott,", "tokens": ["Die", "Wun\u00b7den", "Je\u00b7su", "sind", "ihm", "Spott", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PPER", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm kindische Geschichten;", "tokens": ["Ihm", "kin\u00b7di\u00b7sche", "Ge\u00b7schich\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "\u00bbdas H\u00f6chste\u00ab, spricht er, \u00bbist Verstand,", "tokens": ["\u00bb", "das", "H\u00f6chs\u00b7te", "\u00ab", ",", "spricht", "er", ",", "\u00bb", "ist", "Ver\u00b7stand", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "$(", "$,", "VVFIN", "PPER", "$,", "$(", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der schlichte Glaube Kindertand.\u00ab", "tokens": ["Der", "schlich\u00b7te", "Glau\u00b7be", "Kin\u00b7der\u00b7tand", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Umsonst der Gattin Rede strebt,", "tokens": ["Um\u00b7sonst", "der", "Gat\u00b7tin", "Re\u00b7de", "strebt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Harten zu bekehren,", "tokens": ["Den", "Har\u00b7ten", "zu", "be\u00b7keh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie spricht: \u00bbBald hab' ich ausgelebt,", "tokens": ["Sie", "spricht", ":", "\u00bb", "Bald", "hab'", "ich", "aus\u00b7ge\u00b7lebt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ADV", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kurz wird der Traum noch w\u00e4hren;", "tokens": ["Kurz", "wird", "der", "Traum", "noch", "w\u00e4h\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dann gebe Gott, da\u00df meine Leich'", "tokens": ["Dann", "ge\u00b7be", "Gott", ",", "da\u00df", "mei\u00b7ne", "Leich'"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich mache durch ein Zeichen weich.\u00ab", "tokens": ["Dich", "ma\u00b7che", "durch", "ein", "Zei\u00b7chen", "weich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bald ging sie ein in Gottes Ruh'", "tokens": ["Bald", "ging", "sie", "ein", "in", "Got\u00b7tes", "Ruh'"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "APPR", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus hartem Streit hienieden,", "tokens": ["Aus", "har\u00b7tem", "Streit", "hien\u00b7ie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Er dr\u00fcckt ihr sanft die Augen zu", "tokens": ["Er", "dr\u00fcckt", "ihr", "sanft", "die", "Au\u00b7gen", "zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "ART", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und spricht: \u00bbWir sind geschieden!", "tokens": ["Und", "spricht", ":", "\u00bb", "Wir", "sind", "ge\u00b7schie\u00b7den", "!"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "$(", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Denn hin ist hin und tot ist tot,", "tokens": ["Denn", "hin", "ist", "hin", "und", "tot", "ist", "tot", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "ADV", "KON", "ADJD", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So hei\u00dft das eiserne Gebot!\u00ab", "tokens": ["So", "hei\u00dft", "das", "ei\u00b7ser\u00b7ne", "Ge\u00b7bot", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+---+", "measure": "unknown.measure.tri"}}, "stanza.4": {"line.1": {"text": "Da\u00df es so worden, ist ihm arg,", "tokens": ["Da\u00df", "es", "so", "wor\u00b7den", ",", "ist", "ihm", "arg", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAPP", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er geht in seinem Jammer,", "tokens": ["Er", "geht", "in", "sei\u00b7nem", "Jam\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Bevor man sie gelegt in Sarg,", "tokens": ["Be\u00b7vor", "man", "sie", "ge\u00b7legt", "in", "Sarg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In ihre Totenkammer,", "tokens": ["In", "ih\u00b7re", "To\u00b7ten\u00b7kam\u00b7mer", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Er schaut sie an mit tr\u00fcbem Blick", "tokens": ["Er", "schaut", "sie", "an", "mit", "tr\u00fc\u00b7bem", "Blick"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und f\u00fchlt in sich verlornes Gl\u00fcck.", "tokens": ["Und", "f\u00fchlt", "in", "sich", "ver\u00b7lor\u00b7nes", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PRF", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Da richtet sich die Leich' empor,", "tokens": ["Da", "rich\u00b7tet", "sich", "die", "Leich'", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Kreuzt auf der Brust die Arme,", "tokens": ["Kreuzt", "auf", "der", "Brust", "die", "Ar\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und aus dem kalten Mund hervor", "tokens": ["Und", "aus", "dem", "kal\u00b7ten", "Mund", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt's: \u00bbGott sich dein erbarme!", "tokens": ["T\u00f6nt's", ":", "\u00bb", "Gott", "sich", "dein", "er\u00b7bar\u00b7me", "!"], "token_info": ["word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "$(", "NN", "PRF", "PPOSAT", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was du nicht glaubtest, wahrlich ist: \u2013", "tokens": ["Was", "du", "nicht", "glaub\u00b7test", ",", "wahr\u00b7lich", "ist", ":", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "VVFIN", "$,", "ADV", "VAFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Er h\u00f6rt's, ein Schauer packt ihn leis,", "tokens": ["Er", "h\u00f6rt's", ",", "ein", "Schau\u00b7er", "packt", "ihn", "leis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VVFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Er gehet bleich von hinnen,", "tokens": ["Er", "ge\u00b7het", "bleich", "von", "hin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "In seiner Freunde bunten Kreis,", "tokens": ["In", "sei\u00b7ner", "Freun\u00b7de", "bun\u00b7ten", "Kreis", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Doch spricht er da: \u00bbDen Sinnen", "tokens": ["Doch", "spricht", "er", "da", ":", "\u00bb", "Den", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$.", "$(", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Traut nicht; was ich erfahren, ist", "tokens": ["Traut", "nicht", ";", "was", "ich", "er\u00b7fah\u00b7ren", ",", "ist"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKNEG", "$.", "PWS", "PPER", "VVINF", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Blendwerk oder Weiberlist.\u00ab", "tokens": ["Ein", "Blend\u00b7werk", "o\u00b7der", "Wei\u00b7ber\u00b7list", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er hat es nicht bekannt der Welt,", "tokens": ["Er", "hat", "es", "nicht", "be\u00b7kannt", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Doch wird fortan er stille,", "tokens": ["Doch", "wird", "for\u00b7tan", "er", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die \u00e4u\u00dfere Gestalt zerf\u00e4llt;", "tokens": ["Die", "\u00e4u\u00b7\u00dfe\u00b7re", "Ge\u00b7stalt", "zer\u00b7f\u00e4llt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Als tot liegt seine H\u00fclle,", "tokens": ["Als", "tot", "liegt", "sei\u00b7ne", "H\u00fcl\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da kreuzen seine Arme sich", "tokens": ["Da", "kreu\u00b7zen", "sei\u00b7ne", "Ar\u00b7me", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und st\u00f6hnt sein Mund: \u00bbEin Tor war ich!\u00ab", "tokens": ["Und", "st\u00f6hnt", "sein", "Mund", ":", "\u00bb", "Ein", "Tor", "war", "ich", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$.", "$(", "ART", "NN", "VAFIN", "PPER", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Saul schuf sich Himmel, schuf sich Gott", "tokens": ["Saul", "schuf", "sich", "Him\u00b7mel", ",", "schuf", "sich", "Gott"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PRF", "NN", "$,", "VVFIN", "PRF", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}