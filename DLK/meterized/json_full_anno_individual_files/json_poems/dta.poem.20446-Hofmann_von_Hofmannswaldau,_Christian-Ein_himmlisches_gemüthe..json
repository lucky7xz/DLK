{"dta.poem.20446": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Ein himmlisches gem\u00fcthe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein himmlisches gem\u00fcthe/ so geist und feuer h\u00e4lt/", "tokens": ["Ein", "himm\u00b7li\u00b7sches", "ge\u00b7m\u00fc\u00b7the", "/", "so", "geist", "und", "feu\u00b7er", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "ADJD", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Und nicht nur dem gebl\u00fcte zum diener ist bestellt/", "tokens": ["Und", "nicht", "nur", "dem", "ge\u00b7bl\u00fc\u00b7te", "zum", "die\u00b7ner", "ist", "be\u00b7stellt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ADV", "ART", "NN", "APPRART", "PDS", "VAFIN", "VVPP", "$("], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Schaut des gel\u00fcckes gaben/", "tokens": ["Schaut", "des", "ge\u00b7l\u00fc\u00b7ckes", "ga\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles/ was wir haben/", "tokens": ["Und", "al\u00b7les", "/", "was", "wir", "ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "$(", "PWS", "PPER", "VAFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit halben augen an.", "tokens": ["Mit", "hal\u00b7ben", "au\u00b7gen", "an", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Es rei\u00dft der sch\u00f6nheit gl\u00e4ntzen nicht seinen vorsatz ein/", "tokens": ["Es", "rei\u00dft", "der", "sch\u00f6n\u00b7heit", "gl\u00e4nt\u00b7zen", "nicht", "sei\u00b7nen", "vor\u00b7satz", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "PTKNEG", "PPOSAT", "NN", "ART", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es kennt der liebe grentzen/ und wei\u00df den falschen schein", "tokens": ["Es", "kennt", "der", "lie\u00b7be", "grent\u00b7zen", "/", "und", "wei\u00df", "den", "fal\u00b7schen", "schein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "$(", "KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Dem wesen abzuziehen/", "tokens": ["Dem", "we\u00b7sen", "ab\u00b7zu\u00b7zie\u00b7hen", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVIZU", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und alles di\u00df zu fliehen/", "tokens": ["Und", "al\u00b7les", "di\u00df", "zu", "flie\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "PDS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So uns verleiten kan.", "tokens": ["So", "uns", "ver\u00b7lei\u00b7ten", "kan", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es lacht mit leeren h\u00e4nden/ die unschuld ist sein gold/", "tokens": ["Es", "lacht", "mit", "lee\u00b7ren", "h\u00e4n\u00b7den", "/", "die", "un\u00b7schuld", "ist", "sein", "gold", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "$(", "ART", "ADJD", "VAFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Gebrauch kan ihn nicht blenden/ die tugend ist sein sold.", "tokens": ["Ge\u00b7brauch", "kan", "ihn", "nicht", "blen\u00b7den", "/", "die", "tu\u00b7gend", "ist", "sein", "sold", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$(", "ART", "NN", "VAFIN", "VAINF", "VMFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Immittelst noth und schmertzen", "tokens": ["Im\u00b7mit\u00b7telst", "noth", "und", "schmert\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "NN", "KON", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt in den reinen hertzen", "tokens": ["Bleibt", "in", "den", "rei\u00b7nen", "hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die hoffnung unverwand.", "tokens": ["Die", "hoff\u00b7nung", "un\u00b7ver\u00b7wand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Es liebt auff dieser erden nichts mehr als geist und gott.", "tokens": ["Es", "liebt", "auff", "die\u00b7ser", "er\u00b7den", "nichts", "mehr", "als", "geist", "und", "gott", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "NN", "PIS", "PIS", "KOKOM", "VVPP", "KON", "NN", "$."], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Es mag kein ketzer werden/ verehren staub und koth/", "tokens": ["Es", "mag", "kein", "ket\u00b7zer", "wer\u00b7den", "/", "ver\u00b7eh\u00b7ren", "staub", "und", "koth", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "VAINF", "$(", "VVFIN", "PTKVZ", "KON", "NN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es trennen seine sinnen/", "tokens": ["Es", "tren\u00b7nen", "sei\u00b7ne", "sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit eisernem beginnen", "tokens": ["Mit", "ei\u00b7ser\u00b7nem", "be\u00b7gin\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "VVINF"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Der erden faules land.", "tokens": ["Der", "er\u00b7den", "fau\u00b7les", "land", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Wenn alle felsen wancken/ so bleidt es unber\u00fchrt/", "tokens": ["Wenn", "al\u00b7le", "fel\u00b7sen", "wan\u00b7cken", "/", "so", "bleidt", "es", "un\u00b7be\u00b7r\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVINF", "$(", "ADV", "VVFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Beschlossen in den schrancken/ der uns zum himmel f\u00fchrt/", "tokens": ["Be\u00b7schlos\u00b7sen", "in", "den", "schran\u00b7cken", "/", "der", "uns", "zum", "him\u00b7mel", "f\u00fchrt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "VVINF", "$(", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Und denckt/ wie diese sachen/", "tokens": ["Und", "denckt", "/", "wie", "die\u00b7se", "sa\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$(", "KOKOM", "PDS", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So uns offt knechtisch machen/", "tokens": ["So", "uns", "offt", "knech\u00b7tisch", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Verrauschen mit der zeit.", "tokens": ["Ver\u00b7rau\u00b7schen", "mit", "der", "zeit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Es traut ihm nicht zu bauen auff wasser und auff sand/", "tokens": ["Es", "traut", "ihm", "nicht", "zu", "bau\u00b7en", "auff", "was\u00b7ser", "und", "auff", "sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "APPR", "ADJA", "KON", "APPR", "VVFIN", "$("], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Will nicht der erden trauen/ und sucht ein fester land/", "tokens": ["Will", "nicht", "der", "er\u00b7den", "trau\u00b7en", "/", "und", "sucht", "ein", "fes\u00b7ter", "land", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$(", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Worauff es hoffnung setzet/", "tokens": ["Wo\u00b7rauff", "es", "hoff\u00b7nung", "set\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "NN", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und was es recht ergetzet/", "tokens": ["Und", "was", "es", "recht", "er\u00b7get\u00b7zet", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ist GOtt und ewigkeit.", "tokens": ["Ist", "Gott", "und", "e\u00b7wig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}