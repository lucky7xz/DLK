{"dta.poem.7910": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "6.  Winterreise .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Bei diesem kalten Wehen", "tokens": ["Bei", "die\u00b7sem", "kal\u00b7ten", "We\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sind alle Stra\u00dfen leer,", "tokens": ["Sind", "al\u00b7le", "Stra\u00b7\u00dfen", "leer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Wasser stille stehen,", "tokens": ["Die", "Was\u00b7ser", "stil\u00b7le", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich aber schweif\u2019 umher.", "tokens": ["Ich", "a\u00b7ber", "schweif'", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Sonne scheint so tr\u00fcbe,", "tokens": ["Die", "Son\u00b7ne", "scheint", "so", "tr\u00fc\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mu\u00df fr\u00fch hinuntergehn,", "tokens": ["Mu\u00df", "fr\u00fch", "hin\u00b7un\u00b7ter\u00b7gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VMFIN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erloschen ist die Liebe,", "tokens": ["Er\u00b7lo\u00b7schen", "ist", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die Lust kann nicht bestehn.", "tokens": ["Die", "Lust", "kann", "nicht", "be\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Nun geht der Wald zu Ende,", "tokens": ["Nun", "geht", "der", "Wald", "zu", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Im Dorfe mach\u2019 ich Halt,", "tokens": ["Im", "Dor\u00b7fe", "mach'", "ich", "Halt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da w\u00e4rm\u2019 ich mir die H\u00e4nde,", "tokens": ["Da", "w\u00e4rm'", "ich", "mir", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bleibt auch das Herze kalt.", "tokens": ["Bleibt", "auch", "das", "Her\u00b7ze", "kalt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PDS", "VVFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}