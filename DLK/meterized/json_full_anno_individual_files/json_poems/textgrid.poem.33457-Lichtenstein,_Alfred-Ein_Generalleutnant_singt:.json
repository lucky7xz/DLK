{"textgrid.poem.33457": {"metadata": {"author": {"name": "Lichtenstein, Alfred", "birth": "N.A.", "death": "N.A."}, "title": "Ein Generalleutnant singt:", "genre": "verse", "period": "N.A.", "pub_year": 1914, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin der Herr Divisionskommandeur,", "tokens": ["Ich", "bin", "der", "Herr", "Di\u00b7vi\u00b7si\u00b7ons\u00b7kom\u00b7man\u00b7deur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Seine Exzellenz.", "tokens": ["Sei\u00b7ne", "Ex\u00b7zel\u00b7lenz", "."], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ich habe erreicht, was menschenm\u00f6glich ist.", "tokens": ["Ich", "ha\u00b7be", "er\u00b7reicht", ",", "was", "men\u00b7schen\u00b7m\u00f6g\u00b7lich", "ist", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$,", "PRELS", "ADJD", "VAFIN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Ein sch\u00f6nes Bewu\u00dftsein.", "tokens": ["Ein", "sch\u00f6\u00b7nes", "Be\u00b7wu\u00df\u00b7tsein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Vor mir beugen das Knie", "tokens": ["Vor", "mir", "beu\u00b7gen", "das", "Knie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVINF", "ART", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Hauptleute und Regimentschefs,", "tokens": ["Haupt\u00b7leu\u00b7te", "und", "Re\u00b7gi\u00b7ment\u00b7schefs", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$