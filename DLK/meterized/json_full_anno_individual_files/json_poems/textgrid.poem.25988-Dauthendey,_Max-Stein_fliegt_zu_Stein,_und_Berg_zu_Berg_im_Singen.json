{"textgrid.poem.25988": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Stein fliegt zu Stein, und Berg zu Berg im Singen", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Springbrunn spielte in dem Garten, der verschwiegen,", "tokens": ["Ein", "Spring\u00b7brunn", "spiel\u00b7te", "in", "dem", "Gar\u00b7ten", ",", "der", "ver\u00b7schwie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und lernte sich im hei\u00dfen Mittag fliegen.", "tokens": ["Und", "lern\u00b7te", "sich", "im", "hei\u00b7\u00dfen", "Mit\u00b7tag", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sein Pl\u00e4tschern und sein Tun, in das die Rosen starrten,", "tokens": ["Sein", "Pl\u00e4t\u00b7schern", "und", "sein", "Tun", ",", "in", "das", "die", "Ro\u00b7sen", "starr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN", "$,", "APPR", "PRELS", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Benarrten meine Schritte, und ich ging", "tokens": ["Be\u00b7narr\u00b7ten", "mei\u00b7ne", "Schrit\u00b7te", ",", "und", "ich", "ging"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "lin