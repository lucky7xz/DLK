{"textgrid.poem.59915": {"metadata": {"author": {"name": "Bodenstedt, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "7.", "genre": "verse", "period": "N.A.", "pub_year": 1855, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachts kam im Traum zu mir ein Engel,", "tokens": ["Nachts", "kam", "im", "Traum", "zu", "mir", "ein", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPRART", "NN", "APPR", "PPER", "ART", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der hatte vom Himmel den Abschied bekommen,", "tokens": ["Der", "hat\u00b7te", "vom", "Him\u00b7mel", "den", "Ab\u00b7schied", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.3": {"text": "Weil er, voll lauter irdischer M\u00e4ngel,", "tokens": ["Weil", "er", ",", "voll", "lau\u00b7ter", "ir\u00b7di\u00b7scher", "M\u00e4n\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADJD", "PIAT", "ADJA", "NN", "$,"], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Das Himmelreich f\u00fcr die Erde genommen.", "tokens": ["Das", "Him\u00b7mel\u00b7reich", "f\u00fcr", "die", "Er\u00b7de", "ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Gott sprach zu ihm am Tag des Gerichtes:", "tokens": ["Gott", "sprach", "zu", "ihm", "am", "Tag", "des", "Ge\u00b7rich\u00b7tes", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was man einmal ist, das mu\u00df man ganz sein;", "tokens": ["Was", "man", "ein\u00b7mal", "ist", ",", "das", "mu\u00df", "man", "ganz", "sein", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADV", "VAFIN", "$,", "PDS", "VMFIN", "PIS", "ADV", "VAINF", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Im Himmel himmlischen Angesichtes", "tokens": ["Im", "Him\u00b7mel", "himm\u00b7li\u00b7schen", "An\u00b7ge\u00b7sich\u00b7tes"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mu\u00df man voll lauter himmlischem Glanz sein.", "tokens": ["Mu\u00df", "man", "voll", "lau\u00b7ter", "himm\u00b7li\u00b7schem", "Glanz", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "ADJD", "PIAT", "ADJA", "NN", "VAINF", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Die Erde hat Wein, Gesang und Liebe,", "tokens": ["Die", "Er\u00b7de", "hat", "Wein", ",", "Ge\u00b7sang", "und", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Der Himmel hat seinen himmlischen Segen.", "tokens": ["Der", "Him\u00b7mel", "hat", "sei\u00b7nen", "himm\u00b7li\u00b7schen", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "So lange dein Herz voll irdischer Triebe,", "tokens": ["So", "lan\u00b7ge", "dein", "Herz", "voll", "ir\u00b7di\u00b7scher", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPOSAT", "NN", "ADJD", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Sollst du der irdischen Freuden pflegen!", "tokens": ["Sollst", "du", "der", "ir\u00b7di\u00b7schen", "Freu\u00b7den", "pfle\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Wer nicht im Leben erstrebt das Beste,", "tokens": ["Wer", "nicht", "im", "Le\u00b7ben", "er\u00b7strebt", "das", "Bes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "APPRART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Was meine Gnade bereitet auf Erden,", "tokens": ["Was", "mei\u00b7ne", "Gna\u00b7de", "be\u00b7rei\u00b7tet", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Dem bleiben zu viele irdische Reste,", "tokens": ["Dem", "blei\u00b7ben", "zu", "vie\u00b7le", "ir\u00b7di\u00b7sche", "Res\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVINF", "PTKZU", "PIAT", "ADJA", "NN", "$,"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der kann auch im Himmel nicht gl\u00fccklich werden.", "tokens": ["Der", "kann", "auch", "im", "Him\u00b7mel", "nicht", "gl\u00fcck\u00b7lich", "wer