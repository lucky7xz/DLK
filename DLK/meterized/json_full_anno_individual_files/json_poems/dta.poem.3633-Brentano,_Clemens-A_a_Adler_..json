{"dta.poem.3633": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "A a   Adler .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wohl auf ihr klein Waldv\u00f6gelein, die ihr in L\u00fcf-               ", "tokens": ["Wohl", "auf", "ihr", "klein", "Wald\u00b7v\u00f6\u00b7ge\u00b7lein", ",", "die", "ihr", "in", "L\u00fcf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "ADJD", "NN", "$,", "PRELS", "PPER", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "ten schwebt,", "tokens": ["ten", "schwebt", ","], "token_info": ["word", "word", "punct"], "pos": ["FM", "VVFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Stimmt an, lobt Gott den Herren mein, singt all, die", "tokens": ["Stimmt", "an", ",", "lobt", "Gott", "den", "Her\u00b7ren", "mein", ",", "singt", "all", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "NN", "ART", "NN", "PPOSAT", "$,", "VVFIN", "PIAT", "$,", "PRELS"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Stimm erhebt;", "tokens": ["Stimm", "er\u00b7hebt", ";"], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVFIN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Denn Gott hat euch erschaffen, sich selbst zu Lob und Ehr,", "tokens": ["Denn", "Gott", "hat", "euch", "er\u00b7schaf\u00b7fen", ",", "sich", "selbst", "zu", "Lob", "und", "Ehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "PPER", "VVPP", "$,", "PRF", "ADV", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Sang, Feder, Schnabel, Waffen, kommt alles von ihm her.", "tokens": ["Sang", ",", "Fe\u00b7der", ",", "Schna\u00b7bel", ",", "Waf\u00b7fen", ",", "kommt", "al\u00b7les", "von", "ihm", "her", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "VVFIN", "PIS", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Der aller V\u00f6gel K\u00f6nig ist, macht billig den Anfang,", "tokens": ["Der", "al\u00b7ler", "V\u00f6\u00b7gel", "K\u00f6\u00b7nig", "ist", ",", "macht", "bil\u00b7lig", "den", "An\u00b7fang", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "VAFIN", "$,", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Komm Adler! komm hervor, wo bist? stimm an den Vo-", "tokens": ["Komm", "Ad\u00b7ler", "!", "komm", "her\u00b7vor", ",", "wo", "bist", "?", "stimm", "an", "den", "Vo"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "$.", "VVFIN", "PTKVZ", "$,", "PWAV", "VAFIN", "$.", "ADJD", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.3": {"text": "gelsang,", "tokens": ["gel\u00b7sang", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Der Vorzug dir geb\u00fchret, kein Vogel ist dir gleich,", "tokens": ["Der", "Vor\u00b7zug", "dir", "ge\u00b7b\u00fch\u00b7ret", ",", "kein", "Vo\u00b7gel", "ist", "dir", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "VVFIN", "$,", "PIAT", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Drum dich im Wappen f\u00fchret, der Kaiser und das Reich.", "tokens": ["Drum", "dich", "im", "Wap\u00b7pen", "f\u00fch\u00b7ret", ",", "der", "Kai\u00b7ser", "und", "das", "Reich", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "APPRART", "NN", "VVFIN", "$,", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.3": {"line.1": {"text": "Die Bachstelz thut oft schnappen, und f\u00e4ngt der", "tokens": ["Die", "Bach\u00b7stelz", "thut", "oft", "schnap\u00b7pen", ",", "und", "f\u00e4ngt", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "VVINF", "$,", "KON", "VVFIN", "ART"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "M\u00fccken viel,", "tokens": ["M\u00fc\u00b7cken", "viel", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "ADV", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Es h\u00f6rt nicht auf zu knappen ihr langer Pfannenstiel,", "tokens": ["Es", "h\u00f6rt", "nicht", "auf", "zu", "knap\u00b7pen", "ihr", "lan\u00b7ger", "Pfan\u00b7nen\u00b7stiel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "APPR", "PTKZU", "VVINF", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Den Schweif thut sie stets zwingen, sie l\u00e4\u00dft ihm niemals", "tokens": ["Den", "Schweif", "thut", "sie", "stets", "zwin\u00b7gen", ",", "sie", "l\u00e4\u00dft", "ihm", "nie\u00b7mals"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ADV"], "meter": "-+---+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ruh,", "tokens": ["Ruh", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Wenn andre V\u00f6gel singen, schl\u00e4gt sie den Tackt dazu.", "tokens": ["Wenn", "and\u00b7re", "V\u00f6\u00b7gel", "sin\u00b7gen", ",", "schl\u00e4gt", "sie", "den", "Tackt", "da\u00b7zu", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "VVINF", "$,", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.4": {"line.1": {"text": "Das lieb Canariv\u00f6gelein k\u00f6mmt her aus fremdem Land,", "tokens": ["Das", "lieb", "Ca\u00b7na\u00b7ri\u00b7v\u00f6\u00b7ge\u00b7lein", "k\u00f6mmt", "her", "aus", "frem\u00b7dem", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Es singt gar sch\u00f6n, zart, hell und rein, wie allen ist", "tokens": ["Es", "singt", "gar", "sch\u00f6n", ",", "zart", ",", "hell", "und", "rein", ",", "wie", "al\u00b7len", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$,", "PWAV", "PIS", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "bekannt,", "tokens": ["be\u00b7kannt", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Den Zucker fri\u00dft es gerne, doch nimmt es auch vorlieb,", "tokens": ["Den", "Zu\u00b7cker", "fri\u00dft", "es", "ger\u00b7ne", ",", "doch", "nimmt", "es", "auch", "vor\u00b7lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Wenn man ihm Hanfsaamkerne, und R\u00fcbesaamen giebt.", "tokens": ["Wenn", "man", "ihm", "Hanf\u00b7saam\u00b7ker\u00b7ne", ",", "und", "R\u00fc\u00b7be\u00b7saa\u00b7men", "giebt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "NN", "$,", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.5": {"line.1": {"text": "Merk auf wie lockt so lieblich mir, der sch\u00f6ne Distelfink,", "tokens": ["Merk", "auf", "wie", "lockt", "so", "lieb\u00b7lich", "mir", ",", "der", "sch\u00f6\u00b7ne", "Dis\u00b7tel\u00b7fink", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "KOKOM", "VVFIN", "ADV", "ADJD", "PPER", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.2": {"text": "Bei\u00dft Distel auf und sticht sich nit, sein Witz ist nit", "tokens": ["Bei\u00dft", "Dis\u00b7tel", "auf", "und", "sticht", "sich", "nit", ",", "sein", "Witz", "ist", "nit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "PTKVZ", "KON", "VVFIN", "PRF", "PTKNEG", "$,", "PPOSAT", "NN", "VAFIN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "gering,", "tokens": ["ge\u00b7ring", ","], "token_info": ["word", "punct"], "pos": ["ADJD", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Gar wohl ist er gezieret, sch\u00f6n gelb und roth bekleidt,", "tokens": ["Gar", "wohl", "ist", "er", "ge\u00b7zie\u00b7ret", ",", "sch\u00f6n", "gelb", "und", "roth", "be\u00b7kleidt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "VVPP", "$,", "ADJD", "ADJD", "KON", "ADJD", "ADJD", "$,"], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Sein Stimm er nie verlieret, singt fr\u00f6hlich alle Zeit.", "tokens": ["Sein", "Stimm", "er", "nie", "ver\u00b7lie\u00b7ret", ",", "singt", "fr\u00f6h\u00b7lich", "al\u00b7le", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADV", "VVFIN", "$,", "VVFIN", "ADJD", "PIAT", "NN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Emmeriz bis zum Abend spat, singt \u00fcbel, \u00fcbel hin,", "tokens": ["Der", "Em\u00b7me\u00b7riz", "bis", "zum", "A\u00b7bend", "spat", ",", "singt", "\u00fc\u00b7bel", ",", "\u00fc\u00b7bel", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "APPRART", "NN", "VVFIN", "$,", "VVFIN", "ADJD", "$,", "ADJD", "PTKVZ", "$,"], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Er sagt, wenns Feld nur Aehren hat, ich auch ein Schnit-", "tokens": ["Er", "sagt", ",", "wenns", "Feld", "nur", "A\u00b7eh\u00b7ren", "hat", ",", "ich", "auch", "ein", "Schnit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "NN", "ADV", "NN", "VAFIN", "$,", "PPER", "ADV", "ART", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "ter bin,", "tokens": ["ter", "bin", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAFIN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Im Feld thut er sich n\u00e4hren, bleibt Tag und Nacht", "tokens": ["Im", "Feld", "thut", "er", "sich", "n\u00e4h\u00b7ren", ",", "bleibt", "Tag", "und", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "PRF", "VVINF", "$,", "VVFIN", "NN", "KON", "NN"], "meter": "-+---+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "darauf,", "tokens": ["da\u00b7rauf", ","], "token_info": ["word", "punct"], "pos": ["PAV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "Was Gott ihm thut beschehren, das klaubt er flei\u00dfig auf.", "tokens": ["Was", "Gott", "ihm", "thut", "be\u00b7scheh\u00b7ren", ",", "das", "klaubt", "er", "flei\u00b7\u00dfig", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "PPER", "VVFIN", "VVINF", "$,", "PDS", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.7": {"line.1": {"text": "Des Morgens fr\u00fch, des Abends spat, der Fink hat", "tokens": ["Des", "Mor\u00b7gens", "fr\u00fch", ",", "des", "A\u00b7bends", "spat", ",", "der", "Fink", "hat"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "ADJD", "$,", "ART", "NN", "VVFIN", "$,", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "keine Ruh,", "tokens": ["kei\u00b7ne", "Ruh", ","], "token_info": ["word", "word", "punct"], "pos": ["PIAT", "NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Die Musen er ins Gr\u00fcne lad't mit seinem Reit her zu,", "tokens": ["Die", "Mu\u00b7sen", "er", "ins", "Gr\u00fc\u00b7ne", "lad't", "mit", "sei\u00b7nem", "Reit", "her", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "APZR", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Fr\u00fch ist gar gut studieren, wenns k\u00fchl, still, ruhig ist,", "tokens": ["Fr\u00fch", "ist", "gar", "gut", "stu\u00b7die\u00b7ren", ",", "wenns", "k\u00fchl", ",", "still", ",", "ru\u00b7hig", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ADV", "ADJD", "VVINF", "$,", "KOUS", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAFIN", "$,"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.5": {"text": "Steh auf und thu's probieren, du fauler ", "tokens": ["Steh", "auf", "und", "thu's", "pro\u00b7bie\u00b7ren", ",", "du", "fau\u00b7ler"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PTKVZ", "KON", "NE", "VVINF", "$,", "PPER", "ADJA"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Fr\u00f6hlich der Fink im Fr\u00fchling singt, sa sa, sa sa", "tokens": ["Fr\u00f6h\u00b7lich", "der", "Fink", "im", "Fr\u00fch\u00b7ling", "singt", ",", "sa", "sa", ",", "sa", "sa"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "ART", "NN", "APPRART", "NN", "VVFIN", "$,", "FM", "FM", "$,", "FM.it", "FM.it"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "hui Dieb,", "tokens": ["hui", "Dieb", ","], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Im ganzen Wald sein Stimm erklingt, wenns Wetter", "tokens": ["Im", "gan\u00b7zen", "Wald", "sein", "Stimm", "er\u00b7klingt", ",", "wenns", "Wet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "nicht zu tr\u00fcb,", "tokens": ["nicht", "zu", "tr\u00fcb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Die Dieb will er verjagen, die rund heraus er schilt,", "tokens": ["Die", "Dieb", "will", "er", "ver\u00b7ja\u00b7gen", ",", "die", "rund", "he\u00b7raus", "er", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,", "PRELS", "ADJD", "PTKVZ", "PPER", "VVFIN", "$,"], "meter": "-+---+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Dem Sperling thut er sagen, da\u00df er viel Waizen stiehlt.", "tokens": ["Dem", "Sper\u00b7ling", "thut", "er", "sa\u00b7gen", ",", "da\u00df", "er", "viel", "Wai\u00b7zen", "stiehlt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "VVINF", "$,", "KOUS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Ein rother, dir gar wohl bekannt, ist sch\u00f6n, doch", "tokens": ["Ein", "ro\u00b7ther", ",", "dir", "gar", "wohl", "be\u00b7kannt", ",", "ist", "sch\u00f6n", ",", "doch"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ART", "ADJA", "$,", "PPER", "ADV", "ADV", "ADJD", "$,", "VAFIN", "AD