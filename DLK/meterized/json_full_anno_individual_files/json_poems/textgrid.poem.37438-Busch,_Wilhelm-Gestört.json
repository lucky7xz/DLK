{"textgrid.poem.37438": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Gest\u00f6rt", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich gedachte still zu sitzen,", "tokens": ["Ich", "ge\u00b7dach\u00b7te", "still", "zu", "sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Doch sogleich begann das Treiben:", "tokens": ["Doch", "sog\u00b7leich", "be\u00b7gann", "das", "Trei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du mu\u00dft gehen, laufen, schwitzen,", "tokens": ["Du", "mu\u00dft", "ge\u00b7hen", ",", "lau\u00b7fen", ",", "schwit\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$,", "VVFIN", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um so forsch, wie wir, zu bleiben.", "tokens": ["Um", "so", "forsch", ",", "wie", "wir", ",", "zu", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUI", "ADV", "ADJD", "$,", "PWAV", "PPER", "$,", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und sie wollten mir nach ihrer", "tokens": ["Und", "sie", "woll\u00b7ten", "mir", "nach", "ih\u00b7rer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "PPER", "APPR", "PPOSAT"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mode keine Ruhe g\u00f6nnen,", "tokens": ["Mo\u00b7de", "kei\u00b7ne", "Ru\u00b7he", "g\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Gleich wie Boten und Hausierer", "tokens": ["Gleich", "wie", "Bo\u00b7ten", "und", "Hau\u00b7sie\u00b7rer"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "NN", "KON", "NN"], "meter": "+-+--+--", "measure": "trochaic.tri.relaxed"}, "line.4": {"text": "Sollt ich hin und wieder rennen.", "tokens": ["Sollt", "ich", "hin", "und", "wie\u00b7der", "ren\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PTKVZ", "KON", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich besah mir diese Geister,", "tokens": ["Ich", "be\u00b7sah", "mir", "die\u00b7se", "Geis\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Diese ungest\u00fcmen Treiber.", "tokens": ["Die\u00b7se", "un\u00b7ge\u00b7st\u00fc\u00b7men", "Trei\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Oft sind solche weisen Meister", "tokens": ["Oft", "sind", "sol\u00b7che", "wei\u00b7sen", "Meis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Grad die \u00e4rgsten Klageweiber.", "tokens": ["Grad", "die", "\u00e4rgs\u00b7ten", "Kla\u00b7ge\u00b7wei\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "troch