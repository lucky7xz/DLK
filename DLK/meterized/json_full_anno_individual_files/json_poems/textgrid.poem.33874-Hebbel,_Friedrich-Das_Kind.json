{"textgrid.poem.33874": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Kind", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Mutter lag im Todtenschrein,", "tokens": ["Die", "Mut\u00b7ter", "lag", "im", "Tod\u00b7ten\u00b7schrein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zum letzten Mal geschm\u00fcckt;", "tokens": ["Zum", "letz\u00b7ten", "Mal", "ge\u00b7schm\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da spielt das kleine Kind herein,", "tokens": ["Da", "spielt", "das", "klei\u00b7ne", "Kind", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das staunend sie erblickt.", "tokens": ["Das", "stau\u00b7nend", "sie", "er\u00b7blickt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Blumenkron' im blonden Haar", "tokens": ["Die", "Blu\u00b7men\u00b7kron'", "im", "blon\u00b7den", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gef\u00e4llt ihm gar zu sehr,", "tokens": ["Ge\u00b7f\u00e4llt", "ihm", "gar", "zu", "sehr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKA", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Busenblumen, bunt und klar,", "tokens": ["Die", "Bu\u00b7sen\u00b7blu\u00b7men", ",", "bunt", "und", "klar", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum Strau\u00df gereiht, noch mehr.", "tokens": ["Zum", "Strau\u00df", "ge\u00b7reiht", ",", "noch", "mehr", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NE", "VVPP", "$,", "ADV", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und sanft und schmeichelnd ruft es aus:", "tokens": ["Und", "sanft", "und", "schmei\u00b7chelnd", "ruft", "es", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "KON", "ADJD", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du liebe Mutter, gieb", "tokens": ["Du", "lie\u00b7be", "Mut\u00b7ter", ",", "gieb"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PPER", "ADJA", "NN", "$,", "VVIMP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mir eine Blum' aus deinem Strau\u00df,", "tokens": ["Mir", "ei\u00b7ne", "Blum'", "aus", "dei\u00b7nem", "Strau\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "APPR", "PPOSAT", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab' dich auch so lieb!", "tokens": ["Ich", "hab'", "dich", "auch", "so", "lieb", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und als die Mutter es nicht thut,", "tokens": ["Und", "als", "die", "Mut\u00b7ter", "es", "nicht", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da denkt das Kind f\u00fcr sich:", "tokens": ["Da", "denkt", "das", "Kind", "f\u00fcr", "sich", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie schl\u00e4ft, doch wenn sie ausgeruht,", "tokens": ["Sie", "schl\u00e4ft", ",", "doch", "wenn", "sie", "aus\u00b7ge\u00b7ruht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADV", "KOUS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So thut sie's sicherlich.", "tokens": ["So", "thut", "sie's", "si\u00b7cher\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Schleicht fort, so leis' es immer kann,", "tokens": ["Schleicht", "fort", ",", "so", "leis'", "es", "im\u00b7mer", "kann", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADV", "VVFIN", "PPER", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und schlie\u00dft die Th\u00fcre sacht", "tokens": ["Und", "schlie\u00dft", "die", "Th\u00fc\u00b7re", "sacht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und lauscht von Zeit zu Zeit daran,", "tokens": ["Und", "lauscht", "von", "Zeit", "zu", "Zeit", "da\u00b7ran", ","], "token_info": ["word", "word", "word", 