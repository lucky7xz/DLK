{"dta.poem.11955": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du ruhst, mit deiner Lust am Stande der Natur,", "tokens": ["Du", "ruhst", ",", "mit", "dei\u00b7ner", "Lust", "am", "Stan\u00b7de", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch nicht auf diesem Stand, doch auf dem Staate nur.", "tokens": ["Doch", "nicht", "auf", "die\u00b7sem", "Stand", ",", "doch", "auf", "dem", "Staa\u00b7te", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "PDAT", "NN", "$,", "ADV", "APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du w\u00fcrdest, einsam wie du bist, mit allen Listen,", "tokens": ["Du", "w\u00fcr\u00b7dest", ",", "ein\u00b7sam", "wie", "du", "bist", ",", "mit", "al\u00b7len", "Lis\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADJD", "KOKOM", "PPER", "VAFIN", "$,", "APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit allen Kr\u00e4ften, nicht dein nacktes Daseyn fristen.", "tokens": ["Mit", "al\u00b7len", "Kr\u00e4f\u00b7ten", ",", "nicht", "dein", "nack\u00b7tes", "Da\u00b7seyn", "fris\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Dich in Gedanken gar des Himmels zu ergehn,", "tokens": ["Dich", "in", "Ge\u00b7dan\u00b7ken", "gar", "des", "Him\u00b7mels", "zu", "er\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "ADV", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Das w\u00fcrd' im ew'gen Furcht- und Nothstand dir vergehn.", "tokens": ["Das", "w\u00fcrd'", "im", "ew'\u00b7gen", "Furcht", "und", "Noth\u00b7stand", "dir", "ver\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPRART", "ADJA", "TRUNC", "KON", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Drum danke Gott, da\u00df so die Welt ist eingerichtet,", "tokens": ["Drum", "dan\u00b7ke", "Gott", ",", "da\u00df", "so", "die", "Welt", "ist", "ein\u00b7ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "NN", "$,", "KOUS", "ADV", "ART", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df sie zu Gute kommt auch dem der drauf verzichtet;", "tokens": ["Da\u00df", "sie", "zu", "Gu\u00b7te", "kommt", "auch", "dem", "der", "drauf", "ver\u00b7zich\u00b7tet", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "VVFIN", "ADV", "ART", "ART", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Da\u00df der Bed\u00fcrfnisse Verband nur l\u00e4\u00dft entsprie\u00dfen", "tokens": ["Da\u00df", "der", "Be\u00b7d\u00fcrf\u00b7nis\u00b7se", "Ver\u00b7band", "nur", "l\u00e4\u00dft", "ent\u00b7sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "ADV", "VVFIN", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Bed\u00fcrfnislosigkeit und g\u00f6ttliches Genie\u00dfen.", "tokens": ["Be\u00b7d\u00fcrf\u00b7nis\u00b7lo\u00b7sig\u00b7keit", "und", "g\u00f6tt\u00b7li\u00b7ches", "Ge\u00b7nie\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}