{"textgrid.poem.52890": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: In diesen Zellen schlafen sie,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In diesen Zellen schlafen sie,", "tokens": ["In", "die\u00b7sen", "Zel\u00b7len", "schla\u00b7fen", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Mittelding' von Mensch und Tiere,", "tokens": ["Die", "Mit\u00b7tel\u00b7ding'", "von", "Mensch", "und", "Tie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Behandelt wie das liebe Vieh,", "tokens": ["Be\u00b7han\u00b7delt", "wie", "das", "lie\u00b7be", "Vieh", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie dies gestreckt auf alle Viere.", "tokens": ["Wie", "dies", "ge\u00b7streckt", "auf", "al\u00b7le", "Vie\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PDS", "VVPP", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wie dumpf, wie dunstig rings ums Haus", "tokens": ["Wie", "dumpf", ",", "wie", "duns\u00b7tig", "rings", "ums", "Haus"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und drin welch Toben, Stampfen, Schreien!", "tokens": ["Und", "drin", "welch", "To\u00b7ben", ",", "Stamp\u00b7fen", ",", "Schrei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Hier Lieder voller frohem Graus,", "tokens": ["Hier", "Lie\u00b7der", "vol\u00b7ler", "fro\u00b7hem", "Graus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dort irrer Glieder Selbstkasteien!", "tokens": ["Dort", "ir\u00b7rer", "Glie\u00b7der", "Selbst\u00b7kas\u00b7tei\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "O Wahnsinn! Schreckliches Gespenst,", "tokens": ["O", "Wahn\u00b7sinn", "!", "Schreck\u00b7li\u00b7ches", "Ge\u00b7spenst", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Gei\u00dfel in entfleischten H\u00e4nden,", "tokens": ["Die", "Gei\u00b7\u00dfel", "in", "ent\u00b7fleischten", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Wenn du bald frech vor\u00fcberrennst,", "tokens": ["Wenn", "du", "bald", "frech", "vor\u00b7\u00fc\u00b7ber\u00b7rennst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Bald lauernd schleichst an unsern W\u00e4nden,", "tokens": ["Bald", "lau\u00b7ernd", "schleichst", "an", "un\u00b7sern", "W\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wer b\u00fcrgt daf\u00fcr, da\u00df deine Faust", "tokens": ["Wer", "b\u00fcrgt", "da\u00b7f\u00fcr", ",", "da\u00df", "dei\u00b7ne", "Faust"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PAV", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Nicht pl\u00f6tzlich unsren Scheitel treffe,", "tokens": ["Nicht", "pl\u00f6tz\u00b7lich", "un\u00b7sren", "Schei\u00b7tel", "tref\u00b7fe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df, der bei den Tollen haust,", "tokens": ["Und", "da\u00df", ",", "der", "bei", "den", "Tol\u00b7len", "haust", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der Geist nicht l\u00e4ngst uns selber \u00e4ffe?", "tokens": ["Der", "Geist", "nicht", "l\u00e4ngst", "uns", "sel\u00b7ber", "\u00e4f\u00b7fe", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "ADV", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Die kranke Lieb', den kranken Stolz,", "tokens": ["Die", "kran\u00b7ke", "Lieb'", ",", "den", "kran\u00b7ken", "Stolz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wir sperren sie in ehrne St\u00e4be,", "tokens": ["Wir", "sper\u00b7ren", "sie", "in", "ehr\u00b7ne", "St\u00e4\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Um unser Ma\u00df aus d\u00fcrrem Holz", "tokens": ["Um", "un\u00b7ser", "Ma\u00df", "aus", "d\u00fcr\u00b7rem", "Holz"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ziehn wir jedwede Wucherrebe,", "tokens": ["Ziehn", "wir", "jed\u00b7we\u00b7de", "Wu\u00b7cher\u00b7re\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Was nicht so denkt, wie wir, und nicht", "tokens": ["Was", "nicht", "so", "denkt", ",", "wie", "wir", ",", "und", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PTKNEG", "ADV", "VVFIN", "$,", "PWAV", "PPER", "$,", "KON", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So f\u00fchlt, das z\u00e4hlen wir zu Kranken,", "tokens": ["So", "f\u00fchlt", ",", "das", "z\u00e4h\u00b7len", "wir", "zu", "Kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ob nicht just Gesundheit spricht", "tokens": ["Und", "ob", "nicht", "just", "Ge\u00b7sund\u00b7heit", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihren taumelnden Gedanken?", "tokens": ["Aus", "ih\u00b7ren", "tau\u00b7meln\u00b7den", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So sperrst du auch den L\u00f6wen ein,", "tokens": ["So", "sperrst", "du", "auch", "den", "L\u00f6\u00b7wen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du zeigst ihn keck in deinen Gittern,", "tokens": ["Du", "zeigst", "ihn", "keck", "in", "dei\u00b7nen", "Git\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und f\u00fchlest doch bei seinem Schrei'n", "tokens": ["Und", "f\u00fch\u00b7lest", "doch", "bei", "sei\u00b7nem", "Schrei'n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Herz im Leibe dir erzittern;", "tokens": ["Das", "Herz", "im", "Lei\u00b7be", "dir", "er\u00b7zit\u00b7tern", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nennst du ihn toll, nennst du ihn frei,", "tokens": ["Nennst", "du", "ihn", "toll", ",", "nennst", "du", "ihn", "frei", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "ADJD", "$,", "VVFIN", "PPER", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wenn er zerrei\u00dft, der ihn geh\u00fctet,", "tokens": ["Wenn", "er", "zer\u00b7rei\u00dft", ",", "der", "ihn", "ge\u00b7h\u00fc\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und seinem Zwingherrn stolz vorbei", "tokens": ["Und", "sei\u00b7nem", "Zwing\u00b7herrn", "stolz", "vor\u00b7bei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Blutlechzend durch die Gassen w\u00fctet?", "tokens": ["Blut\u00b7lech\u00b7zend", "durch", "die", "Gas\u00b7sen", "w\u00fc\u00b7tet", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Pocht auf das Monopol \u00bbVernunft\u00ab", "tokens": ["Pocht", "auf", "das", "Mo\u00b7no\u00b7pol", "\u00bb", "Ver\u00b7nunft", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$(", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht allzufest in Euren Sitzen,", "tokens": ["Nicht", "all\u00b7zu\u00b7fest", "in", "Eu\u00b7ren", "Sit\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Gro\u00df ist der Narren heil'ge Zunft,", "tokens": ["Gro\u00df", "ist", "der", "Nar\u00b7ren", "heil'\u00b7ge", "Zunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Dies Haus stets offen f\u00fcr Novizen.", "tokens": ["Dies", "Haus", "stets", "of\u00b7fen", "f\u00fcr", "No\u00b7vi\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "NN", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die dort am letzten Fenster, war", "tokens": ["Die", "dort", "am", "letz\u00b7ten", "Fens\u00b7ter", ",", "war"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADV", "APPRART", "ADJA", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vor Jahren eine schmucke Dirne,", "tokens": ["Vor", "Jah\u00b7ren", "ei\u00b7ne", "schmu\u00b7cke", "Dir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Demanten blitzten ihr im Haar", "tokens": ["De\u00b7man\u00b7ten", "blitz\u00b7ten", "ihr", "im", "Haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und Anmut von der sch\u00f6nen Stirne.", "tokens": ["Und", "An\u00b7mut", "von", "der", "sch\u00f6\u00b7nen", "Stir\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Um ihres Mundes L\u00e4cheln rang", "tokens": ["Um", "ih\u00b7res", "Mun\u00b7des", "L\u00e4\u00b7cheln", "rang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUI", "PPOSAT", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Heer von albernen Gesellen,", "tokens": ["Ein", "Heer", "von", "al\u00b7ber\u00b7nen", "Ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Jetzt lacht sie, da\u00df den Gang entlang", "tokens": ["Jetzt", "lacht", "sie", ",", "da\u00df", "den", "Gang", "ent\u00b7lang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "APPO"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die T\u00f6ne schrecklich widergellen;", "tokens": ["Die", "T\u00f6\u00b7ne", "schreck\u00b7lich", "wi\u00b7der\u00b7gel\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Einst kniete man vor diesem Weib,", "tokens": ["Einst", "knie\u00b7te", "man", "vor", "die\u00b7sem", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Jetzt sieh', wie sie sich schamlos windet", "tokens": ["Jetzt", "sieh'", ",", "wie", "sie", "sich", "scham\u00b7los", "win\u00b7det"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PTKVZ", "$,", "PWAV", "PPER", "PRF", "ADJD", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und gierig den entweihten Leib", "tokens": ["Und", "gie\u00b7rig", "den", "ent\u00b7weih\u00b7ten", "Leib"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dem Knechte beut, des Hand sie bindet.", "tokens": ["Dem", "Knech\u00b7te", "beut", ",", "des", "Hand", "sie", "bin\u00b7det", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich f\u00fchlte, wenn ich n\u00e4chtig schritt", "tokens": ["Ich", "f\u00fchl\u00b7te", ",", "wenn", "ich", "n\u00e4ch\u00b7tig", "schritt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl oft so was von Wahnsinns N\u00e4he,", "tokens": ["Wohl", "oft", "so", "was", "von", "Wahn\u00b7sinns", "N\u00e4\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "PWS", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dicht hinter mir ein plumper Tritt,", "tokens": ["Dicht", "hin\u00b7ter", "mir", "ein", "plum\u00b7per", "Tritt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im Ohr Gel\u00e4chter und Gekr\u00e4he;", "tokens": ["Im", "Ohr", "Ge\u00b7l\u00e4ch\u00b7ter", "und", "Ge\u00b7kr\u00e4\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Es packte mich im Nackenhaar", "tokens": ["Es", "pack\u00b7te", "mich", "im", "Na\u00b7cken\u00b7haar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und raunte schauerliche Weisen,", "tokens": ["Und", "raun\u00b7te", "schau\u00b7er\u00b7li\u00b7che", "Wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und aus dem Dunkel starrte klar", "tokens": ["Und", "aus", "dem", "Dun\u00b7kel", "starr\u00b7te", "klar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Aug' mich an mit Flammenkreisen.", "tokens": ["Ein", "Aug'", "mich", "an", "mit", "Flam\u00b7men\u00b7krei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "APPR", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Das ist, wovor mir bangt und graust:", "tokens": ["Das", "ist", ",", "wo\u00b7vor", "mir", "bangt", "und", "graust", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "PWAV", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur nicht in dieses Hauses Schrecken,", "tokens": ["Nur", "nicht", "in", "die\u00b7ses", "Hau\u00b7ses", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nicht unter jener Henker Faust,", "tokens": ["Nicht", "un\u00b7ter", "je\u00b7ner", "Hen\u00b7ker", "Faust", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "PDAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht in das Schrei'n und Z\u00e4hneblecken!", "tokens": ["Nicht", "in", "das", "Schrei'n", "und", "Z\u00e4h\u00b7ne\u00b7ble\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART",