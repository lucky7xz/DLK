 "iambic.tetra"}, "line.5": {"text": "Was nicht so denkt, wie wir, und nicht", "tokens": ["Was", "nicht", "so", "denkt", ",", "wie", "wir", ",", "und", "nicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PWS", "PTKNEG", "ADV", "VVFIN", "$,", "PWAV", "PPER", "$,", "KON", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "So f\u00fchlt, das z\u00e4hlen wir zu Kranken,", "tokens": ["So", "f\u00fchlt", ",", "das", "z\u00e4h\u00b7len", "wir", "zu", "Kran\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und ob nicht just Gesundheit spricht", "tokens": ["Und", "ob", "nicht", "just", "Ge\u00b7sund\u00b7heit", "spricht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PTKNEG", "ADV", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Aus ihren taumelnden Gedanken?", "tokens": ["Aus", "ih\u00b7ren", "tau\u00b7meln\u00b7den", "Ge\u00b7dan\u00b7ken", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "So sperrst du auch den L\u00f6wen ein,", "tokens": ["So", "sperrst", "du", "auch", "den", "L\u00f6\u00b7wen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du zeigst ihn keck in deinen Gittern,", "tokens": ["Du", "zeigst", "ihn", "keck", "in", "dei\u00b7nen", "Git\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", 