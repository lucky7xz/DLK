{"dta.poem.13328": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Von der verbl\u00fcmten Schreibart. Ist die Fertsetzung der Abhandl. von dem Sinnr.  \n und Scharssinn, aus der Ankl. des verd. Geschmacks.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1742", "urn": "urn:nbn:de:kobv:b4-200905198519", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als j\u00fcngst mein Auge sich in die Saphirne Tiefe", "tokens": ["Als", "j\u00fcngst", "mein", "Au\u00b7ge", "sich", "in", "die", "Sa\u00b7phir\u00b7ne", "Tie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "PRF", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die weder Grund noch Strand, noch Ziel, noch End", "tokens": ["Die", "we\u00b7der", "Grund", "noch", "Strand", ",", "noch", "Ziel", ",", "noch", "End"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "KON", "NN", "ADV", "NN", "$,", "ADV", "NN", "$,", "ADV", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "(umschr\u00e4nckt,", "tokens": ["(", "um\u00b7schr\u00e4nckt", ","], "token_info": ["punct", "word", "punct"], "pos": ["$(", "VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Ins unerforschte Meer des holen Luft-Raums ", "tokens": ["Ins", "un\u00b7er\u00b7forschte", "Meer", "des", "ho\u00b7len", "Luft\u00b7Raums"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und mein verschlungner Blick bald hie bald dahin liefe", "tokens": ["Und", "mein", "ver\u00b7schlung\u00b7ner", "Blick", "bald", "hie", "bald", "da\u00b7hin", "lie\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "ADV", "ADV", "ADV", "PAV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch immer tiefer ", "tokens": ["Doch", "im\u00b7mer", "tie\u00b7fer"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADV", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}}}}}