{"textgrid.poem.65946": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Der Rittmeister", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine Schwadron am Waldessaum,", "tokens": ["Ei\u00b7ne", "Schwad\u00b7ron", "am", "Wal\u00b7des\u00b7saum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Schwarze Husaren. Stehn wie der Baum,", "tokens": ["Schwar\u00b7ze", "Hu\u00b7sa\u00b7ren", ".", "Stehn", "wie", "der", "Baum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Die Linke am Sattelknopf.", "tokens": ["Die", "Lin\u00b7ke", "am", "Sat\u00b7tel\u00b7knopf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Vergoldet vom letzten Tagesstrahl", "tokens": ["Ver\u00b7gol\u00b7det", "vom", "letz\u00b7ten", "Ta\u00b7ges\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Pferdehals, blitzender Reiterstahl,", "tokens": ["Pfer\u00b7de\u00b7hals", ",", "blit\u00b7zen\u00b7der", "Rei\u00b7ter\u00b7stahl", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.6": {"text": "Kolpak und Totenkopf.", "tokens": ["Kol\u00b7pak", "und", "To\u00b7ten\u00b7kopf", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drei\u00dfig Schritte vor der Front", "tokens": ["Drei\u00b7\u00dfig", "Schrit\u00b7te", "vor", "der", "Front"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["CARD", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Der Rittmeister grell \u00fcbersonnt,", "tokens": ["Der", "Ritt\u00b7meis\u00b7ter", "grell", "\u00fc\u00b7ber\u00b7sonnt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-++--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Den S\u00e4bel m\u00e4hnenquer.", "tokens": ["Den", "S\u00e4\u00b7bel", "m\u00e4h\u00b7nen\u00b7quer", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Tief in die bleiche Stirn ger\u00fcckt", "tokens": ["Tief", "in", "die", "blei\u00b7che", "Stirn", "ge\u00b7r\u00fcckt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Pelzm\u00fctze, sp\u00e4ht er vorgeb\u00fcckt,", "tokens": ["Die", "Pelz\u00b7m\u00fct\u00b7ze", ",", "sp\u00e4ht", "er", "vor\u00b7ge\u00b7b\u00fcckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "VVFIN", "PPER", "VVPP", "$,"], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}, "line.6": {"text": "Mit Geierblick umher.", "tokens": ["Mit", "Gei\u00b7er\u00b7blick", "um\u00b7her", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Links auf leichtem Schimmel dicht", "tokens": ["Links", "auf", "leich\u00b7tem", "Schim\u00b7mel", "dicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ADJA", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Sein Trompeter, ein flachsblond Milchgesicht.", "tokens": ["Sein", "Trom\u00b7pe\u00b7ter", ",", "ein", "flachs\u00b7blond", "Milch\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+---+-+-+", "measure": "dactylic.init"}, "line.3": {"text": "Der sieht mit leisem Graun", "tokens": ["Der", "sieht", "mit", "lei\u00b7sem", "Graun"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ihn reglos halten auf dem Fleck,", "tokens": ["Ihn", "reg\u00b7los", "hal\u00b7ten", "auf", "dem", "Fleck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie festgewurzelt Mann und Scheck,", "tokens": ["Wie", "fest\u00b7ge\u00b7wur\u00b7zelt", "Mann", "und", "Scheck", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Bild aus Stein gehaun.", "tokens": ["Ein", "Bild", "aus", "Stein", "ge\u00b7haun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "S\u00e4belwink! Signal! Tra\u2013a\u2013ab! Trab!", "tokens": ["S\u00e4\u00b7bel\u00b7wink", "!", "Sig\u00b7nal", "!", "Tra", "\u2013", "a", "\u2013", "ab", "!", "Trab", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NE", "$.", "NE", "$(", "NE", "$(", "PTKVZ", "$.", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Nun jagt der Victoria die Kr\u00e4nze ab,", "tokens": ["Nun", "jagt", "der", "Vic\u00b7to\u00b7ria", "die", "Kr\u00e4n\u00b7ze", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und wenn sie die H\u00f6lle verschanzt.", "tokens": ["Und", "wenn", "sie", "die", "H\u00f6l\u00b7le", "ver\u00b7schanzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Mit hartem Huf stampft Feld und Frucht", "tokens": ["Mit", "har\u00b7tem", "Huf", "stampft", "Feld", "und", "Frucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Schnellf\u00fc\u00dfige Siegeseifersucht,", "tokens": ["Schnell\u00b7f\u00fc\u00b7\u00dfi\u00b7ge", "Sie\u00b7ge\u00b7sei\u00b7fer\u00b7sucht", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Dass Kraut und Scholle tanzt.", "tokens": ["Dass", "Kraut", "und", "Schol\u00b7le", "tanzt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Hurra! in den Feind! Dragoner sind's. Drauf!", "tokens": ["Hur\u00b7ra", "!", "in", "den", "Feind", "!", "Dra\u00b7go\u00b7ner", "sind'", "s.", "Drauf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "abbreviation", "word", "punct"], "pos": ["ITJ", "$.", "APPR", "ART", "NN", "$.", "FM.it", "FM.it", "FM.it", "FM.it", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Walkt sie, Jungen! Haut sie zu Hauf!", "tokens": ["Walkt", "sie", ",", "Jun\u00b7gen", "!", "Haut", "sie", "zu", "Hauf", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$.", "NN", "PPER", "APPR", "NN", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Klinge an Klinge blitzt.", "tokens": ["Klin\u00b7ge", "an", "Klin\u00b7ge", "blitzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "VVFIN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Der Rittmeister mitten im dichtesten Knaul.", "tokens": ["Der", "Ritt\u00b7meis\u00b7ter", "mit\u00b7ten", "im", "dich\u00b7tes\u00b7ten", "Knaul", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.5": {"text": "Rechts herab, links herab, hoch vom Gaul.", "tokens": ["Rechts", "her\u00b7ab", ",", "links", "her\u00b7ab", ",", "hoch", "vom", "Gaul", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADV", "$,", "ADJD", "APPRART", "NN", "$."], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.6": {"text": "Und jeder Hieb, der sitzt.", "tokens": ["Und", "je\u00b7der", "Hieb", ",", "der", "sitzt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Das ist ein Zerren, Stich und Sto\u00df,", "tokens": ["Das", "ist", "ein", "Zer\u00b7ren", ",", "Stich", "und", "Sto\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sinken, St\u00fcrzen sattellos.", "tokens": ["Ein", "Sin\u00b7ken", ",", "St\u00fcr\u00b7zen", "sat\u00b7tel\u00b7los", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Br\u00fcllend prallt Wut in Wut.", "tokens": ["Br\u00fcl\u00b7lend", "prallt", "Wut", "in", "Wut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "NN", "APPR", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Und wie verzogen sind Staub und Schwall,", "tokens": ["Und", "wie", "ver\u00b7zo\u00b7gen", "sind", "Staub", "und", "Schwall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVPP", "VAFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Gegl\u00e4ttet ruhn die Wogen all,", "tokens": ["Ge\u00b7gl\u00e4t\u00b7tet", "ruhn", "die", "Wo\u00b7gen", "all", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VVFIN", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Sand verrinnt die Flut.", "tokens": ["Im", "Sand", "ver\u00b7rinnt", "die", "Flut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Zerrissen Ross und Reiter, weh!", "tokens": ["Zer\u00b7ris\u00b7sen", "Ross", "und", "Rei\u00b7ter", ",", "weh", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "NE", "KON", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Gefallen wie Halme im Sommer j\u00e4h,", "tokens": ["Ge\u00b7fal\u00b7len", "wie", "Hal\u00b7me", "im", "Som\u00b7mer", "j\u00e4h", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Vorm Siegessichelschlag.", "tokens": ["Vorm", "Sie\u00b7ges\u00b7si\u00b7chel\u00b7schlag", "."], "token_info": ["word", "word", "punct"], "pos": ["APPRART", "NN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Am Boden b\u00fcgellos Held an Held,", "tokens": ["Am", "Bo\u00b7den", "b\u00fc\u00b7gel\u00b7los", "Held", "an", "Held", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "NN", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Reiterlos rasen die Pferde durchs Feld,", "tokens": ["Rei\u00b7ter\u00b7los", "ra\u00b7sen", "die", "Pfer\u00b7de", "durchs", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+-----+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Blutrot stirbt der Tag.", "tokens": ["Blut\u00b7rot", "stirbt", "der", "Tag", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Nur einer entkam. Ihn trug sein Scheck", "tokens": ["Nur", "ei\u00b7ner", "ent\u00b7kam", ".", "Ihn", "trug", "sein", "Scheck"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIS", "VVFIN", "$.", "PPER", "VVFIN", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.re