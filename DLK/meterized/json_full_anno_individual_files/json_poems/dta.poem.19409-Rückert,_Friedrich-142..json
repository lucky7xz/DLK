{"dta.poem.19409": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "142.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Bilderb\u00fcchelchen hat heut mich unterhalten", "tokens": ["Ein", "Bil\u00b7der\u00b7b\u00fc\u00b7chel\u00b7chen", "hat", "heut", "mich", "un\u00b7ter\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Voll doppelg\u00fcltiger zweideutiger Gestalten.", "tokens": ["Voll", "dop\u00b7pel\u00b7g\u00fcl\u00b7ti\u00b7ger", "zwei\u00b7deu\u00b7ti\u00b7ger", "Ge\u00b7stal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ein Bild, grad' angesehn, glich einem sch\u00f6nen Schatze,", "tokens": ["Ein", "Bild", ",", "grad'", "an\u00b7ge\u00b7sehn", ",", "glich", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Schat\u00b7ze", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VVPP", "$,", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann auf den Kopf gestellt, ward es zu einer Fratze.", "tokens": ["Dann", "auf", "den", "Kopf", "ge\u00b7stellt", ",", "ward", "es", "zu", "ei\u00b7ner", "Frat\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP", "$,", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Hier war ein Jud' im Bart, was dort ein Eber ward,", "tokens": ["Hier", "war", "ein", "Jud'", "im", "Bart", ",", "was", "dort", "ein", "E\u00b7ber", "ward", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein alter Kahlkopf hier, dort eine Jungfrau zart.", "tokens": ["Ein", "al\u00b7ter", "Kahl\u00b7kopf", "hier", ",", "dort", "ei\u00b7ne", "Jung\u00b7frau", "zart", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$,", "ADV", "ART", "NN", "ADJD", "$."], "meter": "-+-+--+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Hier schien ein Eselskopf, was dort ein Weiser schien;", "tokens": ["Hier", "schien", "ein", "E\u00b7sels\u00b7kopf", ",", "was", "dort", "ein", "Wei\u00b7ser", "schien", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und so war jedem Schein sein Gegenschein verliehn.", "tokens": ["Und", "so", "war", "je\u00b7dem", "Schein", "sein", "Ge\u00b7gen\u00b7schein", "ver\u00b7liehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ich dachte: Wem's gef\u00e4llt, der kann die ganze Welt", "tokens": ["Ich", "dach\u00b7te", ":", "Wem's", "ge\u00b7f\u00e4llt", ",", "der", "kann", "die", "gan\u00b7ze", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "NE", "VVPP", "$,", "PRELS", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Betrachten wie dis Buch, auf Fu\u00df und Kopf gestellt.", "tokens": ["Be\u00b7trach\u00b7ten", "wie", "dis", "Buch", ",", "auf", "Fu\u00df", "und", "Kopf", "ge\u00b7stellt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "PDS", "NN", "$,", "APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Wie manches ist darin zu schelten und zu loben,", "tokens": ["Wie", "man\u00b7ches", "ist", "da\u00b7rin", "zu", "schel\u00b7ten", "und", "zu", "lo\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PAV", "PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Jenach man es beschaut von unten oder oben.", "tokens": ["Je\u00b7nach", "man", "es", "be\u00b7schaut", "von", "un\u00b7ten", "o\u00b7der", "o\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "PPER", "ADJD", "APPR", "ADV", "KON", "ADV", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}