{"dta.poem.6493": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Ton.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hiedurch belebet sich die Fabel auf das neu,", "tokens": ["Hie\u00b7durch", "be\u00b7le\u00b7bet", "sich", "die", "Fa\u00b7bel", "auf", "das", "neu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wir wissen nun, welch\u2019 eine Stimm\u2019 es sey,", "tokens": ["Wir", "wis\u00b7sen", "nun", ",", "welch'", "ei\u00b7ne", "Stimm'", "es", "sey", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWAT", "ART", "NN", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die, wie man spricht, so \u00e4ngstlich sich beklaget.", "tokens": ["Die", ",", "wie", "man", "spricht", ",", "so", "\u00e4ngst\u00b7lich", "sich", "be\u00b7kla\u00b7get", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "PWAV", "PIS", "VVFIN", "$,", "ADV", "ADJD", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}