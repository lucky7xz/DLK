{"dta.poem.7441": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Gegen Rom.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Noch einen Fluch schlepp' ich herbei:", "tokens": ["Noch", "ei\u00b7nen", "Fluch", "schlepp'", "ich", "her\u00b7bei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Fluch \u00fcber Dich, o Petri Sohn!", "tokens": ["Fluch", "\u00fc\u00b7ber", "Dich", ",", "o", "Pe\u00b7tri", "Sohn", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "$,", "FM", "FM", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Fluch \u00fcber Deine Clerisei!", "tokens": ["Fluch", "\u00fc\u00b7ber", "Dei\u00b7ne", "Cle\u00b7ri\u00b7sei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fluch \u00fcber Deinen S\u00fcndenthron!", "tokens": ["Fluch", "\u00fc\u00b7ber", "Dei\u00b7nen", "S\u00fcn\u00b7den\u00b7thron", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Nur Gift und Galle war, o Pabst,", "tokens": ["Nur", "Gift", "und", "Gal\u00b7le", "war", ",", "o", "Pabst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Was Du vom Pol bis zu den Tropen", "tokens": ["Was", "Du", "vom", "Pol", "bis", "zu", "den", "Tro\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "NN", "APPR", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Welt mit Deinem Scepter gabst,", "tokens": ["Der", "Welt", "mit", "Dei\u00b7nem", "Scep\u00b7ter", "gabst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit Deinem Scepter von Ysopen.", "tokens": ["Mit", "Dei\u00b7nem", "Scep\u00b7ter", "von", "Y\u00b7so\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Weh Dir, Europa's Canaan,", "tokens": ["Weh", "Dir", ",", "Eu\u00b7ro\u00b7pa's", "Ca\u00b7naan", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "NE", "NE", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Das einen Brutus einst gezeugt,", "tokens": ["Das", "ei\u00b7nen", "Bru\u00b7tus", "einst", "ge\u00b7zeugt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NE", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und jetzt sich vor dem Vatikan", "tokens": ["Und", "jetzt", "sich", "vor", "dem", "Va\u00b7ti\u00b7kan"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit feigem Sklavengru\u00dfe beugt;", "tokens": ["Mit", "fei\u00b7gem", "Skla\u00b7ven\u00b7gru\u00b7\u00dfe", "beugt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Im Fleisch der Menschheit ward zum Pfahl", "tokens": ["Im", "Fleisch", "der", "Menschheit", "ward", "zum", "Pfahl"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "VAFIN", "APPRART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Die Wiege des Rienzi Cola,", "tokens": ["Die", "Wie\u00b7ge", "des", "Rien\u00b7zi", "Co\u00b7la", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NE", "NE", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Seit Luthern traf des Bannes Strahl", "tokens": ["Seit", "Lu\u00b7thern", "traf", "des", "Ban\u00b7nes", "Strahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und seit loyal dort nur Loyola.", "tokens": ["Und", "seit", "lo\u00b7yal", "dort", "nur", "Lo\u00b7yo\u00b7la", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NE", "ADV", "ADV", "NE", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Der Boden, der von Honig trof,", "tokens": ["Der", "Bo\u00b7den", ",", "der", "von", "Ho\u00b7nig", "trof", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur Thr\u00e4nen bringt er noch hervor,", "tokens": ["Nur", "Thr\u00e4\u00b7nen", "bringt", "er", "noch", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seit Heinrich in des Pfaffen Hof,", "tokens": ["Seit", "Hein\u00b7rich", "in", "des", "Pfaf\u00b7fen", "Hof", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Knecht im B\u00fc\u00dferhemde, fror;", "tokens": ["Ein", "Knecht", "im", "B\u00fc\u00b7\u00dfer\u00b7hem\u00b7de", ",", "fror", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sein Weihrauch ist ein Grabgeruch,", "tokens": ["Sein", "Weih\u00b7rauch", "ist", "ein", "Grab\u00b7ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Eden wurde zur Sahara,", "tokens": ["Das", "E\u00b7den", "wur\u00b7de", "zur", "Sa\u00b7ha\u00b7ra", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "APPRART", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und zu Italiens Leichentuch", "tokens": ["Und", "zu", "I\u00b7ta\u00b7li\u00b7ens", "Lei\u00b7chen\u00b7tuch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "NE", "NN"], "meter": "+-++--+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Die farbengl\u00fchende Tiara.", "tokens": ["Die", "far\u00b7ben\u00b7gl\u00fc\u00b7hen\u00b7de", "Ti\u00b7a\u00b7ra", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Doch spreiz' Dich nicht, Du stolzes Rom,", "tokens": ["Doch", "sprei\u00b7z'", "Dich", "nicht", ",", "Du", "stol\u00b7zes", "Rom", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "$,", "PPER", "ADJA", "NE", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Dir ist ein baldig Ziel gesetzt;", "tokens": ["Dir", "ist", "ein", "bal\u00b7dig", "Ziel", "ge\u00b7setzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Du bist ein l\u00e4ngst versiegter Strom,", "tokens": ["Du", "bist", "ein", "l\u00e4ngst", "ver\u00b7sieg\u00b7ter", "Strom", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der keines Kindes Mund mehr letzt;", "tokens": ["Der", "kei\u00b7nes", "Kin\u00b7des", "Mund", "mehr", "letzt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist ein tiefgefallen Land,", "tokens": ["Du", "bist", "ein", "tief\u00b7ge\u00b7fal\u00b7len", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Du bist das auferstandne Babel,", "tokens": ["Du", "bist", "das", "auf\u00b7er\u00b7stand\u00b7ne", "Ba\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Trug ist Deine rechte Hand,", "tokens": ["Der", "Trug", "ist", "Dei\u00b7ne", "rech\u00b7te", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dein Schwert das M\u00e4hrchen und die Fabel.", "tokens": ["Dein", "Schwert", "das", "M\u00e4hr\u00b7chen", "und", "die", "Fa\u00b7bel", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und ob Du Diener Dir erk\u00fcrst", "tokens": ["Und", "ob", "Du", "Die\u00b7ner", "Dir", "er\u00b7k\u00fcrst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In aller Welt, Du mu\u00dft vergehn;", "tokens": ["In", "al\u00b7ler", "Welt", ",", "Du", "mu\u00dft", "ver\u00b7gehn", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es kann wohl ohne Kirchenf\u00fcrst", "tokens": ["Es", "kann", "wohl", "oh\u00b7ne", "Kir\u00b7chen\u00b7f\u00fcrst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Geist, der heilige, bestehn.", "tokens": ["Der", "Geist", ",", "der", "hei\u00b7li\u00b7ge", ",", "be\u00b7stehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du Autokrat im H\u00f6llenpfuhl,", "tokens": ["Du", "Au\u00b7to\u00b7krat", "im", "H\u00f6l\u00b7len\u00b7pfuhl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Empfange noch mein letztes Zeter:", "tokens": ["Emp\u00b7fan\u00b7ge", "noch", "mein", "letz\u00b7tes", "Ze\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Du Herrscher auf St. Petri Stuhl,", "tokens": ["Du", "Herr\u00b7scher", "auf", "St.", "Pe\u00b7tri", "Stuhl", ","], "token_info": ["word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NE", "NE", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "F\u00fcrwahr! Du gleichest jenem Peter \u2014", "tokens": ["F\u00fcr\u00b7wahr", "!", "Du", "glei\u00b7chest", "je\u00b7nem", "Pe\u00b7ter"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "PPER", "VVFIN", "PDAT", "NE", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dem keine Glut ins Antlitz flammt,", "tokens": ["Dem", "kei\u00b7ne", "Glut", "ins", "Ant\u00b7litz", "flammt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man ob G\u00f6ttern h\u00e4lt Gericht,", "tokens": ["Wenn", "man", "ob", "G\u00f6t\u00b7tern", "h\u00e4lt", "Ge\u00b7richt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "KOUS", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der, wenn man sie zum Kreuz verdammt,", "tokens": ["Der", ",", "wenn", "man", "sie", "zum", "Kreuz", "ver\u00b7dammt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PIS", "PPER", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Noch ruft: \u201eIch kenn' die Menschen nicht!\u201c", "tokens": ["Noch", "ruft", ":", "\u201e", "Ich", "kenn'", "die", "Men\u00b7schen", "nicht", "!", "\u201c"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der, wenn die Erde selbst sich h\u00e4rmt", "tokens": ["Der", ",", "wenn", "die", "Er\u00b7de", "selbst", "sich", "h\u00e4rmt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "$,", "KOUS", "ART", "NN", "ADV", "PRF", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und tief in sich zusammenschaudert,", "tokens": ["Und", "tief", "in", "sich", "zu\u00b7sam\u00b7men\u00b7schau\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Am Feuer seine H\u00e4nde w\u00e4rmt", "tokens": ["Am", "Feu\u00b7er", "sei\u00b7ne", "H\u00e4n\u00b7de", "w\u00e4rmt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und mit des Richters M\u00e4gden plaudert.", "tokens": ["Und", "mit", "des", "Rich\u00b7ters", "M\u00e4g\u00b7den", "plau\u00b7dert", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du bist kein Fels, wie Petrus war,", "tokens": ["Du", "bist", "kein", "Fels", ",", "wie", "Pet\u00b7rus", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PWAV", "FM", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du bist nur feig und schwach, wie er;", "tokens": ["Du", "bist", "nur", "feig", "und", "schwach", ",", "wie", "er", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "$,", "PWAV", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Morgenhauch bringt Dir Gefahr", "tokens": ["Ein", "Mor\u00b7gen\u00b7hauch", "bringt", "Dir", "Ge\u00b7fahr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und streut Dein Reich wie Sand umher!", "tokens": ["Und", "streut", "Dein", "Reich", "wie", "Sand", "um\u00b7her", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "KOKOM", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du wirst erliegen, L\u00fcgenhirt,", "tokens": ["Du", "wirst", "er\u00b7lie\u00b7gen", ",", "L\u00fc\u00b7gen\u00b7hirt", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "VVINF", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Emp\u00f6ren werden sich die Denker,", "tokens": ["Em\u00b7p\u00f6\u00b7ren", "wer\u00b7den", "sich", "die", "Den\u00b7ker", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Das Brausen des Jahrhunderts wird", "tokens": ["Das", "Brau\u00b7sen", "des", "Jahr\u00b7hun\u00b7derts", "wird"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Zertr\u00fcmmern seine letzten Henker!", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mern", "sei\u00b7ne", "letz\u00b7ten", "Hen\u00b7ker", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}