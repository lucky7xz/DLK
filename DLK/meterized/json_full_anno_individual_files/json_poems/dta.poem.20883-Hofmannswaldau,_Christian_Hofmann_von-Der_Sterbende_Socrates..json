{"dta.poem.20883": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Der  \n Sterbende  \n  Socrates.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.99"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Sein falscher Geist wil immer wancken/", "tokens": ["Sein", "fal\u00b7scher", "Geist", "wil", "im\u00b7mer", "wan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und fust auf nichts was er erkiest/", "tokens": ["Und", "fust", "auf", "nichts", "was", "er", "er\u00b7kiest", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PIS", "PWS", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Er folgt dem Gaumen bey dem Krancken/", "tokens": ["Er", "folgt", "dem", "Gau\u00b7men", "bey", "dem", "Kran\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem alles herb und bitter ist.", "tokens": ["Dem", "al\u00b7les", "herb", "und", "bit\u00b7ter", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "KON", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Mein Ph\u00e4don schaue wie die Sinnen/", "tokens": ["Mein", "Ph\u00e4\u00b7don", "schau\u00b7e", "wie", "die", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KOKOM", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich red in diesem nicht zu scharff/", "tokens": ["Ich", "red", "in", "die\u00b7sem", "nicht", "zu", "scharff", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PDAT", "PTKNEG", "PTKA", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gar bald verf\u00fchret werden k\u00f6nnen/", "tokens": ["Gar", "bald", "ver\u00b7f\u00fch\u00b7ret", "wer\u00b7den", "k\u00f6n\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAINF", "VMFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und man den Artzt auch hier bedarff.", "tokens": ["Und", "man", "den", "Artzt", "auch", "hier", "be\u00b7darff", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ART", "NN", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Man findet tausend tausend Sachen", "tokens": ["Man", "fin\u00b7det", "tau\u00b7send", "tau\u00b7send", "Sa\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "CARD", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die in der Warheits Wage gehn/", "tokens": ["Die", "in", "der", "War\u00b7heits", "Wa\u00b7ge", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da kein Mensch darff Zweiffel machen/", "tokens": ["Und", "da", "kein", "Mensch", "darff", "Zweif\u00b7fel", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "VMFIN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df sie nicht im Gewichte stehn.", "tokens": ["Da\u00df", "sie", "nicht", "im", "Ge\u00b7wich\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Der Mangel aber der Gedancken/", "tokens": ["Der", "Man\u00b7gel", "a\u00b7ber", "der", "Ge\u00b7dan\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Reist manche gute Sache ein;", "tokens": ["Reist", "man\u00b7che", "gu\u00b7te", "Sa\u00b7che", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann wer begint nicht offt zu wancken?", "tokens": ["Dann", "wer", "be\u00b7gint", "nicht", "offt", "zu", "wan\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VVFIN", "PTKNEG", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Drum trachte man recht klug zu seyn.", "tokens": ["Drum", "trach\u00b7te", "man", "recht", "klug", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PIS", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich zwar den Vortheil zu geniessen/", "tokens": ["Ich", "zwar", "den", "Vor\u00b7theil", "zu", "ge\u00b7nies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und hinzugehn Vergn\u00fcgungs voll/", "tokens": ["Und", "hin\u00b7zu\u00b7gehn", "Ver\u00b7gn\u00fc\u00b7gungs", "voll", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und ihr/ damit ihr stets k\u00f6nt wissen/", "tokens": ["Und", "ihr", "/", "da\u00b7mit", "ihr", "stets", "k\u00f6nt", "wis\u00b7sen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "$(", "KOUS", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie man der Wei\u00dfheit brauchen soll.", "tokens": ["Wie", "man", "der", "Wei\u00df\u00b7heit", "brau\u00b7chen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Dann weil ich heute noch soll scheiden/", "tokens": ["Dann", "weil", "ich", "heu\u00b7te", "noch", "soll", "schei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "ADV", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und mich der bleiche Todt begr\u00fcst/", "tokens": ["Und", "mich", "der", "blei\u00b7che", "Todt", "be\u00b7gr\u00fcst", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So wil ich diesen F\u00fcrwurff meiden/", "tokens": ["So", "wil", "ich", "die\u00b7sen", "F\u00fcr\u00b7wurff", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als liebt ich ungereimten Zwist.", "tokens": ["Als", "liebt", "ich", "un\u00b7ge\u00b7reim\u00b7ten", "Zwist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Wer liebt doch ein vergifftes Hertze/", "tokens": ["Wer", "liebt", "doch", "ein", "ver\u00b7giff\u00b7tes", "Hert\u00b7ze", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "dem nichts als Z\u00e4nckerey behagt/", "tokens": ["dem", "nichts", "als", "Z\u00e4n\u00b7cke\u00b7rey", "be\u00b7hagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "KOKOM", "NN", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df mit Hoffart-reichen Schertze", "tokens": ["Und", "da\u00df", "mit", "Hoff\u00b7ar\u00b7trei\u00b7chen", "Schert\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Offt ein best\u00fcrtzt Gem\u00fchte plagt.", "tokens": ["Offt", "ein", "be\u00b7st\u00fcrtzt", "Ge\u00b7m\u00fch\u00b7te", "plagt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Mu\u00df man zu ihrer Schaar gerahten/", "tokens": ["Mu\u00df", "man", "zu", "ih\u00b7rer", "Schaar", "ge\u00b7rah\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PIS", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So find man eine tr\u00fcbe Grufft/", "tokens": ["So", "find", "man", "ei\u00b7ne", "tr\u00fc\u00b7be", "Grufft", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Jhr Kunst-st\u00fcck ist ein schwartzer Schatten/", "tokens": ["Ihr", "Kunst\u00b7st\u00fcck", "ist", "ein", "schwart\u00b7zer", "Schat\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Meinung Dunst und Lufft:", "tokens": ["Und", "ih\u00b7re", "Mei\u00b7nung", "Dunst", "und", "Lufft", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der gantze Zweck von ihrem Wissen/", "tokens": ["Der", "gant\u00b7ze", "Zweck", "von", "ih\u00b7rem", "Wis\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist nichts als Nacht und Tunckelheit/", "tokens": ["Ist", "nichts", "als", "Nacht", "und", "Tun\u00b7ckel\u00b7heit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "KOKOM", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie haben sich allzeit beflissen", "tokens": ["Sie", "ha\u00b7ben", "sich", "all\u00b7zeit", "be\u00b7flis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PRF", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der Farben der Betriegligkeit.", "tokens": ["Der", "Far\u00b7ben", "der", "Be\u00b7trieg\u00b7lig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Man find sie nicht auf festen Gr\u00fcnden/", "tokens": ["Man", "find", "sie", "nicht", "auf", "fes\u00b7ten", "Gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PTKNEG", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jhn klebt Betrug und Falschheit an/", "tokens": ["Jhn", "klebt", "Be\u00b7trug", "und", "Falschheit", "an", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "KON", "NN", "PTKVZ", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Damit man m\u00f6chte scheinbar finden/", "tokens": ["Da\u00b7mit", "man", "m\u00f6ch\u00b7te", "schein\u00b7bar", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VMFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was keiner leichtlich glauben kan.", "tokens": ["Was", "kei\u00b7ner", "leicht\u00b7lich", "glau\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich nun geh\u00e4ssig diesen Dingen/", "tokens": ["Ich", "nun", "ge\u00b7h\u00e4s\u00b7sig", "die\u00b7sen", "Din\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df euer Sinn und Urtheil frey/", "tokens": ["La\u00df", "eu\u00b7er", "Sinn", "und", "Ur\u00b7theil", "frey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "KON", "NN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und trachte nur dahin zu dringen/", "tokens": ["Und", "trach\u00b7te", "nur", "da\u00b7hin", "zu", "drin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PAV", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo meine Meinung sicher sey.", "tokens": ["Wo", "mei\u00b7ne", "Mei\u00b7nung", "si\u00b7cher", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Was soll ich euren Geist verletzen", "tokens": ["Was", "soll", "ich", "eu\u00b7ren", "Geist", "ver\u00b7let\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Durch ungegr\u00fcnter Worte Schein?", "tokens": ["Durch", "un\u00b7ge\u00b7gr\u00fcn\u00b7ter", "Wor\u00b7te", "Schein", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein F\u00fcrsatz ist mich zu ergetzen/", "tokens": ["Mein", "F\u00fcr\u00b7satz", "ist", "mich", "zu", "er\u00b7get\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Di\u00df soll mein Zweck und Ende seyn.", "tokens": ["Di\u00df", "soll", "mein", "Zweck", "und", "En\u00b7de", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PPOSAT", "NN", "KON", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Ist diese Sache recht ergr\u00fcndet/", "tokens": ["Ist", "die\u00b7se", "Sa\u00b7che", "recht", "er\u00b7gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "NN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So hab ich gar genug gethan/", "tokens": ["So", "hab", "ich", "gar", "ge\u00b7nug", "ge\u00b7than", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer nun allhier Vergn\u00fcgung findet/", "tokens": ["Wer", "nun", "all\u00b7hier", "Ver\u00b7gn\u00fc\u00b7gung", "fin\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der nehme sie mit Willen an.", "tokens": ["Der", "neh\u00b7me", "sie", "mit", "Wil\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}