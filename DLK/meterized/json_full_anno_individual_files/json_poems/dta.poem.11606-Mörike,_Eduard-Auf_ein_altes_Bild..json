{"dta.poem.11606": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Auf ein altes Bild.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "In gr\u00fcner Landschaft Sommerflor,", "tokens": ["In", "gr\u00fc\u00b7ner", "Land\u00b7schaft", "Som\u00b7mer\u00b7flor", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei k\u00fchlem Wasser, Schilf und Rohr,", "tokens": ["Bei", "k\u00fch\u00b7lem", "Was\u00b7ser", ",", "Schilf", "und", "Rohr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Schau, wie das Kn\u00e4blein S\u00fcndelos", "tokens": ["Schau", ",", "wie", "das", "Kn\u00e4\u00b7blein", "S\u00fcn\u00b7de\u00b7los"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ART", "NN", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Frei spielet auf der Jungfrau Schos!", "tokens": ["Frei", "spie\u00b7let", "auf", "der", "Jung\u00b7frau", "Schos", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und dort im Walde wonnesam,", "tokens": ["Und", "dort", "im", "Wal\u00b7de", "won\u00b7ne\u00b7sam", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach, gr\u00fcnet schon des Kreuzes Stamm!", "tokens": ["Ach", ",", "gr\u00fc\u00b7net", "schon", "des", "Kreu\u00b7zes", "Stamm", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}