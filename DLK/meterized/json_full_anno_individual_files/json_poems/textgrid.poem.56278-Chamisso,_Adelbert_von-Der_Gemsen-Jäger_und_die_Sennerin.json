rd", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleibe, du Gute, mir hold;", "tokens": ["So", "blei\u00b7be", ",", "du", "Gu\u00b7te", ",", "mir", "hold", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "NN", "$,", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Hast keinen Grund zu weinen,", "tokens": ["Hast", "kei\u00b7nen", "Grund", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimm diesen Ring von Gold.", "tokens": ["Nimm", "die\u00b7sen", "Ring", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Ein Haus, das mir geh\u00f6ret,", "tokens": ["Ein", "Haus", ",", "das", "mir", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort dr\u00fcben im anderen Tal,", "tokens": ["Dort", "dr\u00fc\u00b7ben", "im", "an\u00b7de\u00b7ren", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Stutzer, auf Gletscher und Felsen", "tokens": ["Mein", "Stut\u00b7zer", ",", "auf", "Glet\u00b7scher", "und", "Fel\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die fl\u00fcchtigen Gemsen zumal:", "tokens": ["Die", "fl\u00fcch\u00b7ti\u00b7gen", "Gem\u00b7sen", "zu\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.28": {"line.1": {"text": "Ich kann dich ehrlich ern\u00e4hren,", "tokens": ["Ich", "kann", "dich", "ehr\u00b7lich", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du liebe Sennerin mein;", "tokens": ["Du", "lie\u00b7be", "Sen\u00b7ne\u00b7rin", "mein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und steiget zu Tal der Winter,", "tokens": ["Und", "stei\u00b7get", "zu", "Tal", "der", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Soll unsere Hochzeit sein.", "tokens": ["Soll", "un\u00b7se\u00b7re", "Hoch\u00b7zeit", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}}}}