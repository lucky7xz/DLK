{"textgrid.poem.56278": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "Der Gemsen-J\u00e4ger und die Sennerin", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nimm mich verirrten J\u00e4ger,", "tokens": ["Nimm", "mich", "ver\u00b7irr\u00b7ten", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Du gute Sennerin, auf;", "tokens": ["Du", "gu\u00b7te", "Sen\u00b7ne\u00b7rin", ",", "auf", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es lockte mich \u00fcber die Gletscher", "tokens": ["Es", "lock\u00b7te", "mich", "\u00fc\u00b7ber", "die", "Glet\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Gemse mit fl\u00fcchtigem Lauf.", "tokens": ["Die", "Gem\u00b7se", "mit", "fl\u00fcch\u00b7ti\u00b7gem", "Lauf", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Bin fremd auf dieser Alpe,", "tokens": ["Bin", "fremd", "auf", "die\u00b7ser", "Al\u00b7pe", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Verlassen f\u00fcr und f\u00fcr;", "tokens": ["Ver\u00b7las\u00b7sen", "f\u00fcr", "und", "f\u00fcr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "KON", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "In rauher Nacht verschlie\u00dfe", "tokens": ["In", "rau\u00b7her", "Nacht", "ver\u00b7schlie\u00b7\u00dfe"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht hart mir deine T\u00fcr. \u2013", "tokens": ["Nicht", "hart", "mir", "dei\u00b7ne", "T\u00fcr", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PTKNEG", "ADJD", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Mu\u00df, J\u00e4ger, wohl sie verschlie\u00dfen,", "tokens": ["Mu\u00df", ",", "J\u00e4\u00b7ger", ",", "wohl", "sie", "ver\u00b7schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "$,", "NN", "$,", "ADV", "PPER", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich bin ja ganz allein,", "tokens": ["Ich", "bin", "ja", "ganz", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Gar eng ist meine H\u00fctte,", "tokens": ["Gar", "eng", "ist", "mei\u00b7ne", "H\u00fct\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "F\u00fcr dich kein Lager darein. \u2013", "tokens": ["F\u00fcr", "dich", "kein", "La\u00b7ger", "da\u00b7rein", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPER", "PIAT", "NN", "PTKVZ", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Nur Schutz an deinem Herde,", "tokens": ["Nur", "Schutz", "an", "dei\u00b7nem", "Her\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein Lager begehr ich nicht;", "tokens": ["Ein", "La\u00b7ger", "be\u00b7gehr", "ich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich scheide, sobald die Gletscher", "tokens": ["Ich", "schei\u00b7de", ",", "so\u00b7bald", "die", "Glet\u00b7scher"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Sich f\u00e4rben mit r\u00f6tlichem Licht. \u2013", "tokens": ["Sich", "f\u00e4r\u00b7ben", "mit", "r\u00f6t\u00b7li\u00b7chem", "Licht", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.5": {"line.1": {"text": "Und wenn ich ein dich lie\u00dfe...,", "tokens": ["Und", "wenn", "ich", "ein", "dich", "lie\u00b7\u00dfe", "...", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "PPER", "VVFIN", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "O J\u00e4ger, la\u00df mich in Ruh,", "tokens": ["O", "J\u00e4\u00b7ger", ",", "la\u00df", "mich", "in", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVIMP", "PPER", "APPR", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nachrede g\u00e4b's und Geschichten;", "tokens": ["Nach\u00b7re\u00b7de", "g\u00e4b's", "und", "Ge\u00b7schich\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "KON", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "Was sagte der Hirt dazu? \u2013", "tokens": ["Was", "sag\u00b7te", "der", "Hirt", "da\u00b7zu", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Hirt soll mich nicht h\u00f6ren,", "tokens": ["Der", "Hirt", "soll", "mich", "nicht", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das, Gute, versprech ich dir:", "tokens": ["Das", ",", "Gu\u00b7te", ",", "ver\u00b7sprech", "ich", "dir", ":"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "NN", "$,", "VVFIN", "PPER", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich halte mich friedlich und stille,", "tokens": ["Ich", "hal\u00b7te", "mich", "fried\u00b7lich", "und", "stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Bef\u00fcrchte doch nichts von mir. \u2013", "tokens": ["Be\u00b7f\u00fcrch\u00b7te", "doch", "nichts", "von", "mir", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ADV", "PIS", "APPR", "PPER", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Und willst du dich halten, o J\u00e4ger,", "tokens": ["Und", "willst", "du", "dich", "hal\u00b7ten", ",", "o", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "VVINF", "$,", "FM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ein stiller und friedlicher Gast,", "tokens": ["Ein", "stil\u00b7ler", "und", "fried\u00b7li\u00b7cher", "Gast", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KON", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "So werd ich herein dich lassen;", "tokens": ["So", "werd", "ich", "her\u00b7ein", "dich", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PTKVZ", "PPER", "VVINF", "$."], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Die Nacht ist zu grausig doch fast.", "tokens": ["Die", "Nacht", "ist", "zu", "grau\u00b7sig", "doch", "fast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "ADV", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.8": {"line.1": {"text": "Sie \u00f6ffnete leise die T\u00fcre", "tokens": ["Sie", "\u00f6ff\u00b7ne\u00b7te", "lei\u00b7se", "die", "T\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und lie\u00df den J\u00e4ger herein;", "tokens": ["Und", "lie\u00df", "den", "J\u00e4\u00b7ger", "her\u00b7ein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Es loderte gastlich vom Herde", "tokens": ["Es", "lo\u00b7der\u00b7te", "gast\u00b7lich", "vom", "Her\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "APPRART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die Flamme mit freundlichem Schein.", "tokens": ["Die", "Flam\u00b7me", "mit", "freund\u00b7li\u00b7chem", "Schein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.9": {"line.1": {"text": "Und bei dem Scheine sahen", "tokens": ["Und", "bei", "dem", "Schei\u00b7ne", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die beiden sich staunend an \u2013", "tokens": ["Die", "bei\u00b7den", "sich", "stau\u00b7nend", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PRF", "ADJD", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Nacht ist ihnen vergangen,", "tokens": ["Die", "Nacht", "ist", "ih\u00b7nen", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Morgen zu d\u00e4mmern begann.", "tokens": ["Der", "Mor\u00b7gen", "zu", "d\u00e4m\u00b7mern", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.10": {"line.1": {"text": "Wie lie\u00df ich dich ein, o J\u00e4ger,", "tokens": ["Wie", "lie\u00df", "ich", "dich", "ein", ",", "o", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich wei\u00df nicht, wie es kam;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "es", "kam", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun r\u00f6tet der Morgen die Gletscher", "tokens": ["Nun", "r\u00f6\u00b7tet", "der", "Mor\u00b7gen", "die", "Glet\u00b7scher"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und meine Wangen die Scham.", "tokens": ["Und", "mei\u00b7ne", "Wan\u00b7gen", "die", "Scham", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "O lieber, lieber J\u00e4ger,", "tokens": ["O", "lie\u00b7ber", ",", "lie\u00b7ber", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So schnell vergangen die Nacht!", "tokens": ["So", "schnell", "ver\u00b7gan\u00b7gen", "die", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVPP", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Auf, auf! du mu\u00dft nun scheiden,", "tokens": ["Auf", ",", "auf", "!", "du", "mu\u00dft", "nun", "schei\u00b7den", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$,", "PTKVZ", "$.", "PPER", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bevor der Hirt noch erwacht.", "tokens": ["Be\u00b7vor", "der", "Hirt", "noch", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Und mu\u00df f\u00fcr heut ich scheiden,", "tokens": ["Und", "mu\u00df", "f\u00fcr", "heut", "ich", "schei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "So bleibe, du Gute, mir hold;", "tokens": ["So", "blei\u00b7be", ",", "du", "Gu\u00b7te", ",", "mir", "hold", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PPER", "NN", "$,", "PPER", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Hast keinen Grund zu weinen,", "tokens": ["Hast", "kei\u00b7nen", "Grund", "zu", "wei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nimm diesen Ring von Gold.", "tokens": ["Nimm", "die\u00b7sen", "Ring", "von", "Gold", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Ein Haus, das mir geh\u00f6ret,", "tokens": ["Ein", "Haus", ",", "das", "mir", "ge\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Dort dr\u00fcben im anderen Tal,", "tokens": ["Dort", "dr\u00fc\u00b7ben", "im", "an\u00b7de\u00b7ren", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Stutzer, auf Gletscher und Felsen", "tokens": ["Mein", "Stut\u00b7zer", ",", "auf", "Glet\u00b7scher", "und", "Fel\u00b7sen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Die fl\u00fcchtigen Gemsen zumal:", "tokens": ["Die", "fl\u00fcch\u00b7ti\u00b7gen", "Gem\u00b7sen", "zu\u00b7mal", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.14": {"line.1": {"text": "Ich kann dich ehrlich ern\u00e4hren,", "tokens": ["Ich", "kann", "dich", "ehr\u00b7lich", "er\u00b7n\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Du liebe Sennerin mein;", "tokens": ["Du", "lie\u00b7be", "Sen\u00b7ne\u00b7rin", "mein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPOSAT", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und steiget zu Tal der Winter,", "tokens": ["Und", "stei\u00b7get", "zu", "Tal", "der", "Win\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Soll unsere Hochzeit sein.", "tokens": ["Soll", "un\u00b7se\u00b7re", "Hoch\u00b7zeit", "sein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "Nimm mich verirrten J\u00e4ger,", "tokens": ["Nimm",