, "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.23": {"line.1": {"text": "Und bei dem Scheine sahen", "tokens": ["Und", "bei", "dem", "Schei\u00b7ne", "sa\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die beiden sich staunend an \u2013", "tokens": ["Die", "bei\u00b7den", "sich", "stau\u00b7nend", "an", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "PRF", "ADJD", "PTKVZ", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Nacht ist ihnen vergangen,", "tokens": ["Die", "Nacht", "ist", "ih\u00b7nen", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Der Morgen zu d\u00e4mmern begann.", "tokens": ["Der", "Mor\u00b7gen", "zu", "d\u00e4m\u00b7mern", "be\u00b7gann", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.24": {"line.1": {"text": "Wie lie\u00df ich dich ein, o J\u00e4ger,", "tokens": ["Wie", "lie\u00df", "ich", "dich", "ein", ",", "o", "J\u00e4\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "PTKVZ", "$,", "FM", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Ich wei\u00df nicht, wie es kam;", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wie", "es", "kam", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nun r\u00f6tet der 