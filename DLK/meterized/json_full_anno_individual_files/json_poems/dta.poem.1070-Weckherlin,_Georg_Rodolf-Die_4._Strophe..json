{"dta.poem.1070": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 4. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Andere k\u00fcnsten gleicherwei\u00df/", "tokens": ["An\u00b7de\u00b7re", "k\u00fcns\u00b7ten", "glei\u00b7cher\u00b7wei\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Welche den adel edel machen/", "tokens": ["Wel\u00b7che", "den", "a\u00b7del", "e\u00b7del", "ma\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Seind so einig mit deinem flie\u00df/", "tokens": ["Seind", "so", "ei\u00b7nig", "mit", "dei\u00b7nem", "flie\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Das du r\u00fchmlich in allen sachen:", "tokens": ["Das", "du", "r\u00fchm\u00b7lich", "in", "al\u00b7len", "sa\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "APPR", "PIS", "VVINF", "$."], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ph", "tokens": ["Ph"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Gleich sihest/ hat dir \u00fcber geben", "tokens": ["Gleich", "si\u00b7hest", "/", "hat", "dir", "\u00fc\u00b7ber", "ge\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$(", "VAFIN", "PPER", "APPR", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Die Castalisch gene\u00fcnte schar/", "tokens": ["Die", "Cas\u00b7ta\u00b7lisch", "ge\u00b7ne\u00fcn\u00b7te", "schar", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Jhr gesang wider zu beleben;", "tokens": ["Ihr", "ge\u00b7sang", "wi\u00b7der", "zu", "be\u00b7le\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.9": {"text": "Vnd du machst das ich vnderfang/", "tokens": ["Vnd", "du", "machst", "das", "ich", "vn\u00b7der\u00b7fang", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "PPER", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Erst mit vngezwungnem klang", "tokens": ["Der", "Erst", "mit", "vn\u00b7ge\u00b7zwung\u00b7nem", "klang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Die G\u00f6tter auf Dorischen saiten", "tokens": ["Die", "G\u00f6t\u00b7ter", "auf", "Do\u00b7ri\u00b7schen", "sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Teutsch-lieblich spilend au\u00df zubraiten.", "tokens": ["Teut\u00b7schlieb\u00b7lich", "spi\u00b7lend", "au\u00df", "zu\u00b7brai\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "PTKVZ", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}