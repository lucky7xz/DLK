{"textgrid.poem.52931": {"metadata": {"author": {"name": "Dingelstedt, Franz von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du Stadt der Bildung und des Tees, der K\u00fcnste und der N\u00fccken,", "genre": "verse", "period": "N.A.", "pub_year": 1847, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du Stadt der Bildung und des Tees, der K\u00fcnste und der N\u00fccken,", "tokens": ["Du", "Stadt", "der", "Bil\u00b7dung", "und", "des", "Tees", ",", "der", "K\u00fcns\u00b7te", "und", "der", "N\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "NN", "KON", "ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Leb wohl, der Dichter weist entt\u00e4uscht auf ewig dir den R\u00fccken!", "tokens": ["Leb", "wohl", ",", "der", "Dich\u00b7ter", "weist", "ent\u00b7t\u00e4uscht", "auf", "e\u00b7wig", "dir", "den", "R\u00fc\u00b7cken", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "ART", "NN", "VVFIN", "ADJD", "APPR", "ADJD", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Kalt d\u00fcnkt' es ihm, so lang er sa\u00df in deinen stolzen Mauern,", "tokens": ["Kalt", "d\u00fcnkt'", "es", "ihm", ",", "so", "lang", "er", "sa\u00df", "in", "dei\u00b7nen", "stol\u00b7zen", "Mau\u00b7ern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "$,", "ADV", "ADJD", "PPER", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und niemals wollt ihm drin ein Lied, ein heimatliches, gl\u00fccken.", "tokens": ["Und", "nie\u00b7mals", "wollt", "ihm", "drin", "ein", "Lied", ",", "ein", "hei\u00b7mat\u00b7li\u00b7ches", ",", "gl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VMFIN", "PPER", "ADV", "ART", "NN", "$,", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.5": {"text": "F\u00fcr schweres Gold erkauft' er sich nur federleichte Liebe", "tokens": ["F\u00fcr", "schwe\u00b7res", "Gold", "er\u00b7kauft'", "er", "sich", "nur", "fe\u00b7der\u00b7leich\u00b7te", "Lie\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "PRF", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Und konnte keine M\u00e4nnerhand warm und vertrauend dr\u00fccken.", "tokens": ["Und", "konn\u00b7te", "kei\u00b7ne", "M\u00e4n\u00b7ner\u00b7hand", "warm", "und", "ver\u00b7trau\u00b7end", "dr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIAT", "NN", "ADJD", "KON", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "In deinen Linden wohnt kein Lenz, kein Herz in den Pal\u00e4sten,", "tokens": ["In", "dei\u00b7nen", "Lin\u00b7den", "wohnt", "kein", "Lenz", ",", "kein", "Herz", "in", "den", "Pa\u00b7l\u00e4s\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NE", "VVFIN", "PIAT", "NN", "$,", "PIAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Und sollten sie durch Pracht und Glanz den Blinden selbst entz\u00fccken;", "tokens": ["Und", "soll\u00b7ten", "sie", "durch", "Pracht", "und", "Glanz", "den", "Blin\u00b7den", "selbst", "ent\u00b7z\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "APPR", "NN", "KON", "NN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.9": {"text": "Auf deinen Stra\u00dfen h\u00fcpft geschminkt die Armut und die L\u00fcge,", "tokens": ["Auf", "dei\u00b7nen", "Stra\u00b7\u00dfen", "h\u00fcpft", "ge\u00b7schminkt", "die", "Ar\u00b7mut", "und", "die", "L\u00fc\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "VVPP", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Verleumdung schl\u00e4gt und Heuchelei dem Laster goldne Br\u00fccken,", "tokens": ["Ver\u00b7leum\u00b7dung", "schl\u00e4gt", "und", "Heu\u00b7che\u00b7lei", "dem", "Las\u00b7ter", "gold\u00b7ne", "Br\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.11": {"text": "Und wenn die Fr\u00f6mmler vor dem Kreuz sich tief und s\u00fc\u00dflich b\u00fccken,", "tokens": ["Und", "wenn", "die", "Fr\u00f6mm\u00b7ler", "vor", "dem", "Kreuz", "sich", "tief", "und", "s\u00fc\u00df\u00b7lich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "APPR", "ART", "NN", "PRF", "ADJD", "KON", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.12": {"text": "So wissen sie doch tiefer noch vor Kreuzen sich zu b\u00fccken,", "tokens": ["So", "wis\u00b7sen", "sie", "doch", "tie\u00b7fer", "noch", "vor", "Kreu\u00b7zen", "sich", "zu", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "ADV", "APPR", "NN", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.13": {"text": "Dein K\u00f6nig r\u00e4umt und baut in dir, er schafft mit Allmachts-H\u00e4nden", "tokens": ["Dein", "K\u00f6\u00b7nig", "r\u00e4umt", "und", "baut", "in", "dir", ",", "er", "schafft", "mit", "All\u00b7machts\u00b7H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.14": {"text": "Nichts mehr als ein musivisch Werk aus hundert-tausend St\u00fccken,", "tokens": ["Nichts", "mehr", "als", "ein", "mu\u00b7si\u00b7visch", "Werk", "aus", "hun\u00b7der\u00b7ttau\u00b7send", "St\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "PIS", "KOKOM", "ART", "ADJD", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+--+--+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.15": {"text": "Die Dichter ruft er fern und nah, die Maler und die S\u00e4nger", "tokens": ["Die", "Dich\u00b7ter", "ruft", "er", "fern", "und", "nah", ",", "die", "Ma\u00b7ler", "und", "die", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.16": {"text": "Und stopft mit gro\u00dfen Namen aus der gro\u00dfen M\u00e4nner L\u00fccken.", "tokens": ["Und", "stopft", "mit", "gro\u00b7\u00dfen", "Na\u00b7men", "aus", "der", "gro\u00b7\u00dfen", "M\u00e4n\u00b7ner", "L\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.17": {"text": "Die Namen tun es freilich nicht, und sein sie europ\u00e4isch,", "tokens": ["Die", "Na\u00b7men", "tun", "es", "frei\u00b7lich", "nicht", ",", "und", "sein", "sie", "eu\u00b7ro\u00b7p\u00e4\u00b7isch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "KON", "PPOSAT", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+--+", "measure": "iambic.septa.chol"}, "line.18": {"text": "Sie k\u00f6nnen nur als S\u00e4ulenzier des Tempels Neubau schm\u00fccken;", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nur", "als", "S\u00e4u\u00b7len\u00b7zier", "des", "Tem\u00b7pels", "Neu\u00b7bau", "schm\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "KOUS", "NN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.19": {"text": "Doch nur der Jugend tapfre Hand, nur frischer Geister Streben", "tokens": ["Doch", "nur", "der", "Ju\u00b7gend", "tapf\u00b7re", "Hand", ",", "nur", "fri\u00b7scher", "Geis\u00b7ter", "Stre\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "NN", "ADJA", "NN", "$,", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.20": {"text": "Kann von dem Baum der Gegenwart lebend'ge Fr\u00fcchte pfl\u00fccken.", "tokens": ["Kann", "von", "dem", "Baum", "der", "Ge\u00b7gen\u00b7wart", "le\u00b7ben\u00b7d'\u00b7ge", "Fr\u00fcch\u00b7te", "pfl\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "ART", "NN", "ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.21": {"text": "Leicht welkt der beste Lorbeerkranz auf alters-kahler Scheitel,", "tokens": ["Leicht", "welkt", "der", "bes\u00b7te", "Lor\u00b7beer\u00b7kranz", "auf", "al\u00b7ter\u00b7skah\u00b7ler", "Schei\u00b7tel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.22": {"text": "Und ein Genie geht auch nicht weit auf Stelzen oder Kr\u00fccken.", "tokens": ["Und", "ein", "Ge\u00b7nie", "geht", "auch", "nicht", "weit", "auf", "Stel\u00b7zen", "o\u00b7der", "Kr\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PTKNEG", "ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.23": {"text": "Ihr schreit genug, Ihr schreibt genug, Ihr seht durch Eure Brillen", "tokens": ["Ihr", "schreit", "ge\u00b7nug", ",", "Ihr", "schreibt", "ge\u00b7nug", ",", "Ihr", "seht", "durch", "Eu\u00b7re", "Bril\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADV", "$,", "PPER", "VVFIN", "ADV", "$,", "PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.24": {"text": "Im Kater einen L\u00f6wen gleich und Adler in den M\u00fccken;", "tokens": ["Im", "Ka\u00b7ter", "ei\u00b7nen", "L\u00f6\u00b7wen", "gleich", "und", "Ad\u00b7ler", "in", "den", "M\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "KON", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.25": {"text": "Wir aber, hinterm Berge hier, wir lassen uns nicht blenden,", "tokens": ["Wir", "a\u00b7ber", ",", "hin\u00b7term", "Ber\u00b7ge", "hier", ",", "wir", "las\u00b7sen", "uns", "nicht", "blen\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "APPRART", "NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.26": {"text": "Wir wissen auch, was rechtes Haar, was Z\u00f6pfe und Per\u00fccken.", "tokens": ["Wir", "wis\u00b7sen", "auch", ",", "was", "rech\u00b7tes", "Haar", ",", "was", "Z\u00f6p\u00b7fe", "und", "Pe\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "ADJA", "NN", "$,", "PWS", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.27": {"text": "Das sag' ich Euch in vieler Sinn, und sollt' es Euch verletzen,", "tokens": ["Das", "sag'", "ich", "Euch", "in", "vie\u00b7ler", "Sinn", ",", "und", "sollt'", "es", "Euch", "ver\u00b7let\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "APPR", "PIAT", "NN", "$,", "KON", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.28": {"text": "So m\u00f6gt Ihr Euch am rechten Fleck ganz ungehindert j\u00fccken.", "tokens": ["So", "m\u00f6gt", "Ihr", "Euch", "am", "rech\u00b7ten", "Fleck", "ganz", "un\u00b7ge\u00b7hin\u00b7dert", "j\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "APPRART", "ADJA", "NN", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.29": {"text": "Und w\u00e4r' ich schlechten Reimen hold, ich w\u00fc\u00dfte wohl noch manches,", "tokens": ["Und", "w\u00e4r'", "ich", "schlech\u00b7ten", "Rei\u00b7men", "hold", ",", "ich", "w\u00fc\u00df\u00b7te", "wohl", "noch", "man\u00b7ches", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "ADJD", "$,", "PPER", "VVFIN", "ADV", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.30": {"text": "Das trefflich pa\u00dft auf Euren Stolz, auf Eure alten T\u00fccken:", "tokens": ["Das", "treff\u00b7lich", "pa\u00dft", "auf", "Eu\u00b7ren", "Stolz", ",", "auf", "Eu\u00b7re", "al\u00b7ten", "T\u00fc\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.31": {"text": "Ihr wi\u00dft doch, was \u00bbersticken\u00ab hei\u00dft, was \u00bbzwicken\u00ab, \u00bbflicken\u00ab, \u00bbknicken\u00ab,", "tokens": ["Ihr", "wi\u00dft", "doch", ",", "was", "\u00bb", "er\u00b7sti\u00b7cken", "\u00ab", "hei\u00dft", ",", "was", "\u00bb", "zwi\u00b7cken", "\u00ab", ",", "\u00bb", "fli\u00b7cken", "\u00ab", ",", "\u00bb", "kni\u00b7cken", "\u00ab", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PWS", "$(", "VVINF", "$(", "VVFIN", "$,", "PWS", "$(", "VVINF", "$(", "$,", "$(", "VVINF", "$(", "$,", "$(", "VVFIN", "$(", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.32": {"text": "Was geistige \u00bbFabriken\u00ab sind und stille \u00bbKatholiken\u00ab, \u2013?", "tokens": ["Was", "geis\u00b7ti\u00b7ge", "\u00bb", "Fab\u00b7ri\u00b7ken", "\u00ab", "sind", "und", "stil\u00b7le", "\u00bb", "Ka\u00b7tho\u00b7li\u00b7ken", "\u00ab", ",", "\u2013", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct", "punct", "punct", "punct"], "pos": ["PWS", "ADJA", "$(", "NN", "$(", "VAFIN", "KON", "VVFIN", "$(", "NN", "$(", "$,", "$(", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.33": {"text": "Allein ich hab' es selber satt und weise, mit Behagen,", "tokens": ["Al\u00b7lein", "ich", "hab'", "es", "sel\u00b7ber", "satt", "und", "wei\u00b7se", ",", "mit", "Be\u00b7ha\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "PPER", "ADV", "ADJD", "KON", "VVFIN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.34": {"text": "Du eitle, kalte, falsche Stadt auf ewig dir den R\u00fccken! \u2013", "tokens": ["Du", "eit\u00b7le", ",", "kal\u00b7te", ",", "fal\u00b7sche", "Stadt", "auf", "e\u00b7wig", "dir", "den", "R\u00fc\u00b7cken", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "ADJA", "NN", "APPR", "ADJD", "PPER", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Du Stadt der Bildung und des Tees, der K\u00fcnste und der N\u00fccken,", "tokens": ["Du", "Stadt", "der