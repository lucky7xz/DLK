 "line.4": {"text": "T\u00e4glich S\u00fcnd' auf S\u00fcnde h\u00e4uft.", "tokens": ["T\u00e4g\u00b7lich", "S\u00fcnd'", "auf", "S\u00fcn\u00b7de", "h\u00e4uft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach! du willst so gern ihn halten,", "tokens": ["Ach", "!", "du", "willst", "so", "gern", "ihn", "hal\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "ADV", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Willst in Leid und Gram vergehn,", "tokens": ["Willst", "in", "Leid", "und", "Gram", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sollst den h\u00f6llischen Gewalten", "tokens": ["Sollst", "den", "h\u00f6l\u00b7li\u00b7schen", "Ge\u00b7wal\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "So dein Selbst gegeben sehn. \u2013", "tokens": ["So", "dein", "Selbst", "ge\u00b7ge\u00b7ben", "sehn", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "VVINF", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Herr! wie kann auf dieser Erden", "tokens": ["Herr", "!", "wie", "kann", "auf", "die\u00b7ser", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PWAV", "VMFIN", "APPR", "PDAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Friede nahen meinem Sinn?", "