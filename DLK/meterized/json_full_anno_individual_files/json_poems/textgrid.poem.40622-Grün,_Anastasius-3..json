{"textgrid.poem.40622": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zur M\u00f6ve ward mein Lied und kommt mit schrillem Ruf geflogen,", "tokens": ["Zur", "M\u00f6\u00b7ve", "ward", "mein", "Lied", "und", "kommt", "mit", "schril\u00b7lem", "Ruf", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ihr Fittig streift unst\u00e4ten Flugs die noch emp\u00f6rten Wogen,", "tokens": ["Ihr", "Fit\u00b7tig", "streift", "un\u00b7st\u00e4\u00b7ten", "Flugs", "die", "noch", "em\u00b7p\u00f6r\u00b7ten", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Durchs Zucken ihres Flatterns geht ein tiefer Zug von Treue,", "tokens": ["Durchs", "Zu\u00b7cken", "ih\u00b7res", "Flat\u00b7terns", "geht", "ein", "tie\u00b7fer", "Zug", "von", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Dem sturmbedrohten Schiffe folgt sie nimmerm\u00fcd' aufs Neue.", "tokens": ["Dem", "sturm\u00b7be\u00b7droh\u00b7ten", "Schif\u00b7fe", "folgt", "sie", "nim\u00b7mer\u00b7m\u00fcd'", "aufs", "Neu\u00b7e", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.2": {"line.1": {"text": "Es war ein sch\u00f6nes starkes Schiff; jetzt wankt es durch die Klippen,", "tokens": ["Es", "war", "ein", "sch\u00f6\u00b7nes", "star\u00b7kes", "Schiff", ";", "jetzt", "wankt", "es", "durch", "die", "Klip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$.", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Unheimlich \u00e4chzt und b\u00e4nglich st\u00f6hnt's durch Takelwerk und Rippen,", "tokens": ["Un\u00b7heim\u00b7lich", "\u00e4chzt", "und", "b\u00e4ng\u00b7lich", "st\u00f6hnt's", "durch", "Ta\u00b7kel\u00b7werk", "und", "Rip\u00b7pen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "ADJD", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Der stolze Namen \u00bbAustria\u00ab steht golden am Altane,", "tokens": ["Der", "stol\u00b7ze", "Na\u00b7men", "\u00bb", "Aust\u00b7ria", "\u00ab", "steht", "gol\u00b7den", "am", "Al\u00b7ta\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "$(", "VVFIN", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Die Wimpel prasseln windgepeitscht, wirr flaggt die Kaiserfahne.", "tokens": ["Die", "Wim\u00b7pel", "pras\u00b7seln", "wind\u00b7ge\u00b7peitscht", ",", "wirr", "flaggt", "die", "Kai\u00b7ser\u00b7fah\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VVFIN", "$,", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.3": {"line.1": {"text": "Doch prunkt's mit welken Kr\u00e4nzen noch, die Bord und Maste kr\u00f6nen;", "tokens": ["Doch", "prunkt's", "mit", "wel\u00b7ken", "Kr\u00e4n\u00b7zen", "noch", ",", "die", "Bord", "und", "Mas\u00b7te", "kr\u00f6\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "APPR", "PWAT", "NN", "ADV", "$,", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "O werft den Flitter in das Meer, Sturmg\u00f6tter zu vers\u00f6hnen!", "tokens": ["O", "werft", "den", "Flit\u00b7ter", "in", "das", "Meer", ",", "Sturm\u00b7g\u00f6t\u00b7ter", "zu", "ver\u00b7s\u00f6h\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Am Schnabel gl\u00e4nzt des Kaisers Bild, des todten, firni\u00dfhelle,", "tokens": ["Am", "Schna\u00b7bel", "gl\u00e4nzt", "des", "Kai\u00b7sers", "Bild", ",", "des", "tod\u00b7ten", ",", "fir\u00b7ni\u00df\u00b7hel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "NN", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Mich d\u00fcnkt, das alte starre Bild emp\u00f6rt noch mehr die Welle.", "tokens": ["Mich", "d\u00fcnkt", ",", "das", "al\u00b7te", "star\u00b7re", "Bild", "em\u00b7p\u00f6rt", "noch", "mehr", "die", "Wel\u00b7le", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "ADJA", "ADJA", "NN", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.4": {"line.1": {"text": "Im Raum der Sklavenballast sehnt, in Ketten, sich nach Landung;", "tokens": ["Im", "Raum", "der", "Skla\u00b7ven\u00b7bal\u00b7last", "sehnt", ",", "in", "Ket\u00b7ten", ",", "sich", "nach", "Lan\u00b7dung", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,", "APPR", "NN", "$,", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Mich d\u00fcnkt, die Arme w\u00e4ren gut, zu rudern aus der Brandung!", "tokens": ["Mich", "d\u00fcnkt", ",", "die", "Ar\u00b7me", "w\u00e4\u00b7ren", "gut", ",", "zu", "ru\u00b7dern", "aus", "der", "Bran\u00b7dung", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ADJD", "$,", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Das Steuer h\u00e4lt ein greiser Mann, fast mumienhaft verwittert;", "tokens": ["Das", "Steu\u00b7er", "h\u00e4lt", "ein", "grei\u00b7ser", "Mann", ",", "fast", "mu\u00b7mi\u00b7en\u00b7haft", "ver\u00b7wit\u00b7tert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "Ihr meint: er steure, doch ist's nur ein Ruck der Hand, die zittert.", "tokens": ["Ihr", "meint", ":", "er", "steu\u00b7re", ",", "doch", "ist's", "nur", "ein", "Ruck", "der", "Hand", ",", "die", "zit\u00b7tert", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VVFIN", "$,", "ADV", "VAFIN", "ADV", "ART", "NN", "ART", "NN", "$,", "PRELS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.5": {"line.1": {"text": "Zum Kompa\u00df ist sein Haupt gebeugt, als pr\u00fcf' er Wind' und Richtung,", "tokens": ["Zum", "Kom\u00b7pa\u00df", "ist", "sein", "Haupt", "ge\u00b7beugt", ",", "als", "pr\u00fcf'", "er", "Wind'", "und", "Rich\u00b7tung", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KOUS", "NN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Doch schlief er ein, ihn selbst besiegt Ersch\u00f6pfung und Vernichtung.", "tokens": ["Doch", "schlief", "er", "ein", ",", "ihn", "selbst", "be\u00b7siegt", "Er\u00b7sch\u00f6p\u00b7fung", "und", "Ver\u00b7nich\u00b7tung", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "ADV", "VVPP", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "O armes Schiff, wer f\u00fchre dich im Sturm, dem ungeheuern?", "tokens": ["O", "ar\u00b7mes", "Schiff", ",", "wer", "f\u00fch\u00b7re", "dich", "im", "Sturm", ",", "dem", "un\u00b7ge\u00b7heu\u00b7ern", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "PWS", "VVFIN", "PPER", "APPRART", "NN", "$,", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Weh, soll nur jener Schmachpilot \u00bbdas bl\u00f6de Gl\u00fcck\u00ab dich steuern!", "tokens": ["Weh", ",", "soll", "nur", "je\u00b7ner", "Schmach\u00b7pi\u00b7lot", "\u00bb", "das", "bl\u00f6\u00b7de", "Gl\u00fcck", "\u00ab", "dich", "steu\u00b7ern", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "ADV", "PDAT", "NN", "$(", "ART", "ADJA", "NN", "$(", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.6": {"line.1": {"text": "Vor Schmerz aufschrillt der M\u00f6ve Schrei; die Seheraugen schauen", "tokens": ["Vor", "Schmerz", "auf\u00b7schrillt", "der", "M\u00f6\u00b7ve", "Schrei", ";", "die", "Se\u00b7her\u00b7au\u00b7gen", "schau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$.", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Mit Tr\u00fcmmern schon bes\u00e4t die Fluth, o Bild voll Schmerz und Grauen!", "tokens": ["Mit", "Tr\u00fcm\u00b7mern", "schon", "be\u00b7s\u00e4t", "die", "Fluth", ",", "o", "Bild", "voll", "Schmerz", "und", "Grau\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ADJD", "ART", "NN", "$,", "FM", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die Brandung donnert; taumelnd st\u00f6\u00dft der Kiel auf Felsenrippen,", "tokens": ["Die", "Bran\u00b7dung", "don\u00b7nert", ";", "tau\u00b7melnd", "st\u00f6\u00dft", "der", "Kiel", "auf", "Fel\u00b7sen\u00b7rip\u00b7pen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "ADJD", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Das Krachen des Zerfallens dr\u00f6hnt weithin durch \u00f6de Klippen.", "tokens": ["Das", "Kra\u00b7chen", "des", "Zer\u00b7fal\u00b7lens", "dr\u00f6hnt", "weit\u00b7hin", "durch", "\u00f6\u00b7de", "Klip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.7": {"line.1": {"text": "Das Schiffsvolk bricht mit wilder Kraft der Todesangst die Ketten,", "tokens": ["Das", "Schiffs\u00b7volk", "bricht", "mit", "wil\u00b7der", "Kraft", "der", "To\u00b7de\u00b7sangst", "die", "Ket\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Der springt ins Boot, dem helf' ein Brett das Jammerleben retten;", "tokens": ["Der", "springt", "ins", "Boot", ",", "dem", "hel\u00b7f'", "ein", "Brett", "das", "Jam\u00b7mer\u00b7le\u00b7ben", "ret\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "$,", "PRELS", "VVFIN", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "So treiben sie dahin, doch nicht wohin die Herzen zielen,", "tokens": ["So", "trei\u00b7ben", "sie", "da\u00b7hin", ",", "doch", "nicht", "wo\u00b7hin", "die", "Her\u00b7zen", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PAV", "$,", "ADV", "PTKNEG", "PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Nur wie des Windes Laune will und wie die Wellen spielen.", "tokens": ["Nur", "wie", "des", "Win\u00b7des", "Lau\u00b7ne", "will", "und", "wie", "die", "Wel\u00b7len", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "ART", "NN", "NN", "VMFIN", "KON", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.8": {"line.1": {"text": "Seefahrern gibt ein Ruderst\u00fcck vom Wrack noch sp\u00e4te Kunde,", "tokens": ["See\u00b7fah\u00b7rern", "gibt", "ein", "Ru\u00b7der\u00b7st\u00fcck", "vom", "Wrack", "noch", "sp\u00e4\u00b7te", "Kun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "APPRART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.2": {"text": "Der stolze Namen \u00bbAustria\u00ab ist eingebrannt dem Funde.", "tokens": ["Der", "stol\u00b7ze", "Na\u00b7men", "\u00bb", "Aust\u00b7ria", "\u00ab", "ist", "ein\u00b7ge\u00b7brannt", "dem", "Fun\u00b7de", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "NN", "$(", "VAFIN", "ADJD", "ART", "NN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Es war ein sch\u00f6nes m\u00e4cht'ges Schiff aus kerngesunden Eichen", "tokens": ["Es", "war", "ein", "sch\u00f6\u00b7nes", "m\u00e4cht'\u00b7ges", "Schiff", "aus", "kern\u00b7ge\u00b7sun\u00b7den", "Ei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "ADJA", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und k\u00f6nnte noch auf freiem Meer mit vollen Segeln streichen!", "tokens": ["Und", "k\u00f6nn\u00b7te", "noch", "auf", "frei\u00b7em", "Meer", "mit", "vol\u00b7len", "Se\u00b7geln", "strei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "ADV", "APPR", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}}, "stanza.9": {"line.1": {"text": "Zur M\u00f6ve ward mein Lied und kommt mit schrillem Ruf geflogen,", "tokens": ["Zur", "M\u00f6\u00b7ve", "ward", "mein", "Lied", "und", "kommt", "mit", "schril\u00b7lem", "Ruf", "ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VAFIN", "PPOSAT", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Ihr Fittig streift unst\u00e4ten Flugs die noch emp\u00f6rten Wogen,", "tokens": ["Ihr", "Fit\u00b7tig", "streift", "un\u00b7st\u00e4\u00b7ten", "Flugs", "die", "noch", "em\u00b7p\u00f6r\u00b7ten", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Durchs Zucken ihres Flatterns geht ein tiefer Zug von Treue,", "tokens": ["Durchs", "Zu\u00b7cken", "ih\u00b7res", "Flat\u00b7terns", "geht", "ein", "tie\u00b7fer", "Zug", "von", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": 