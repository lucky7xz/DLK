{"textgrid.poem.62931": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Satan sprach die alte Schlange:", "genre": "verse", "period": "N.A.", "pub_year": 1783, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Satan sprach die alte Schlange:", "tokens": ["Zu", "Sa\u00b7tan", "sprach", "die", "al\u00b7te", "Schlan\u00b7ge", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich borgte dir zum Untergange", "tokens": ["Ich", "borg\u00b7te", "dir", "zum", "Un\u00b7ter\u00b7gan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Des Menschen meinen Balg; allein was war mein Lohn?", "tokens": ["Des", "Men\u00b7schen", "mei\u00b7nen", "Balg", ";", "al\u00b7lein", "was", "war", "mein", "Lohn", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$.", "ADV", "PWS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Des R\u00e4chers Fluch und der noch \u00e4rgre Hohn,", "tokens": ["Des", "R\u00e4\u00b7chers", "Fluch", "und", "der", "noch", "\u00e4r\u00b7gre", "Hohn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als Wurm auf meinem Bauch zu gehen.", "tokens": ["Als", "Wurm", "auf", "mei\u00b7nem", "Bauch", "zu", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "PPOSAT", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Kann deine Kunst mein Ungemach", "tokens": ["Kann", "dei\u00b7ne", "Kunst", "mein", "Un\u00b7ge\u00b7mach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPOSAT", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nicht lindern, ha! so mu\u00dft du mir gestehen,", "tokens": ["Nicht", "lin\u00b7dern", ",", "ha", "!", "so", "mu\u00dft", "du", "mir", "ge\u00b7ste\u00b7hen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVINF", "$,", "ITJ", "$.", "ADV", "VMFIN", "PPER", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Mein Freund, du bist auch gar zu schwach.", "tokens": ["Mein", "Freund", ",", "du", "bist", "auch", "gar", "zu", "schwach", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Ich kann es und du sollst es sehen,", "tokens": ["Ich", "kann", "es", "und", "du", "sollst", "es", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "KON", "PPER", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Rief der Verf\u00fchrer br\u00fcllend aus.", "tokens": ["Rief", "der", "Ver\u00b7f\u00fch\u00b7rer", "br\u00fcl\u00b7lend", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.11": {"text": "Er speyt die Natter an. Aus ihrem R\u00fccken sprie\u00dfen", "tokens": ["Er", "speyt", "die", "Nat\u00b7ter", "an", ".", "Aus", "ih\u00b7rem", "R\u00fc\u00b7cken", "sprie\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Zween Fl\u00fcgel, gleich der Fledermaus;", "tokens": ["Zween", "Fl\u00fc\u00b7gel", ",", "gleich", "der", "Fle\u00b7der\u00b7maus", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Ihr Bauch erhebet sich auf gelben Hahnenf\u00fc\u00dfen", "tokens": ["Ihr", "Bauch", "er\u00b7he\u00b7bet", "sich", "auf", "gel\u00b7ben", "Hah\u00b7nen\u00b7f\u00fc\u00b7\u00dfen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Und zeigt der schauernden Natur", "tokens": ["Und", "zeigt", "der", "schau\u00b7ern\u00b7den", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Den grassen Basilisk. Mit h\u00f6llischem Vergn\u00fcgen", "tokens": ["Den", "gras\u00b7sen", "Ba\u00b7si\u00b7lisk", ".", "Mit", "h\u00f6l\u00b7li\u00b7schem", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Schaut Satan auf sein Werk. Die neue Kreatur", "tokens": ["Schaut", "Sa\u00b7tan", "auf", "sein", "Werk", ".", "Die", "neu\u00b7e", "Kre\u00b7a\u00b7tur"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Versucht es bald zu gehen, bald zu fliegen,", "tokens": ["Ver\u00b7sucht", "es", "bald", "zu", "ge\u00b7hen", ",", "bald", "zu", "flie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKZU", "VVINF", "$,", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.18": {"text": "Und zischt den R\u00e4cher aus. Itzt bleibt ihr trunkner Blick", "tokens": ["Und", "zischt", "den", "R\u00e4\u00b7cher", "aus", ".", "Itzt", "bleibt", "ihr", "trunk\u00b7ner", "Blick"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "PTKVZ", "$.", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Auf einem klaren Bache kleben:", "tokens": ["Auf", "ei\u00b7nem", "kla\u00b7ren", "Ba\u00b7che", "kle\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Sie sieht ihr Bild und f\u00e4hrt zur\u00fcck", "tokens": ["Sie", "sieht", "ihr", "Bild", "und", "f\u00e4hrt", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "Und haucht bereits ihr junges Leben", "tokens": ["Und", "haucht", "be\u00b7reits", "ihr", "jun\u00b7ges", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "In ihres Sch\u00f6pfers Hand. Allein der alte Wicht", "tokens": ["In", "ih\u00b7res", "Sch\u00f6p\u00b7fers", "Hand", ".", "Al\u00b7lein", "der", "al\u00b7te", "Wicht"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$.", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Fa\u00dft lachend sie beym Kamm: \u00bbwas soll das dumme Beben,", "tokens": ["Fa\u00dft", "la\u00b7chend", "sie", "beym", "Kamm", ":", "\u00bb", "was", "soll", "das", "dum\u00b7me", "Be\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "APPRART", "NN", "$.", "$(", "PWS", "VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Gef\u00e4llst du dir im neuen Schmucke nicht?\u00ab", "tokens": ["Ge\u00b7f\u00e4llst", "du", "dir", "im", "neu\u00b7en", "Schmu\u00b7cke", "nicht", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "PPER", "APPRART", "ADJA", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Der Basilisk erwacht: \u00bbvergieb mir meinen Schrecken,", "tokens": ["Der", "Ba\u00b7si\u00b7lisk", "er\u00b7wacht", ":", "\u00bb", "ver\u00b7gieb", "mir", "mei\u00b7nen", "Schre\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$.", "$(", "VVIMP", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Mein bl\u00f6der Geist war nicht darauf gefa\u00dft,", "tokens": ["Mein", "bl\u00f6\u00b7der", "Geist", "war", "nicht", "da\u00b7rauf", "ge\u00b7fa\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PTKNEG", "PAV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.27": {"text": "Im K\u00f6rper, den du mir gegeben hast,", "tokens": ["Im", "K\u00f6r\u00b7per", ",", "den", "du", "mir", "ge\u00b7ge\u00b7ben", "hast", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "PPER", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.28": {"text": "So manchen Zug des Deinen zu entdecken.\u00ab", "tokens": ["So", "man\u00b7chen", "Zug", "des", "Dei\u00b7nen", "zu", "ent\u00b7de\u00b7cken", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PIAT", "NN", "ART", "NN", "PTKZU", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Ey nun, ich mach es wie mein Feind", "tokens": ["Ey", "nun", ",", "ich", "mach", "es", "wie", "mein", "Feind"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "$,", "PPER", "VVFIN", "PPER", "KOKOM", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Dort oben in dem Sterngefilde,", "tokens": ["Dort", "o\u00b7ben", "in", "dem", "Stern\u00b7ge\u00b7fil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Versetzt der Schalk: ich schaffe meinen Freund \u2013", "tokens": ["Ver\u00b7setzt", "der", "Schalk", ":", "ich", "schaf\u00b7fe", "mei\u00b7nen", "Freund", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."