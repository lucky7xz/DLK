{"dta.poem.12714": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eines Ober-amtmanns.  \n  D.  V. A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der l\u00e4nder eigenschafft und wechsel mit der see,", "tokens": ["Der", "l\u00e4n\u00b7der", "ei\u00b7gen\u00b7schafft", "und", "wech\u00b7sel", "mit", "der", "see", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die kugel-runde welt und ihre vier quartire", "tokens": ["Die", "ku\u00b7gel\u00b7run\u00b7de", "welt", "und", "ih\u00b7re", "vier", "quar\u00b7ti\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "PPOSAT", "CARD", "NN"], "meter": "-+-+-+-+--+--", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Begriff mein enger sinn: itzt da ich nicht studire,", "tokens": ["Be\u00b7griff", "mein", "en\u00b7ger", "sinn", ":", "itzt", "da", "ich", "nicht", "stu\u00b7di\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$.", "ADV", "KOUS", "PPER", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erblick ich unter mir di\u00df p\u00fcnctgen aus der h\u00f6h.", "tokens": ["Er\u00b7blick", "ich", "un\u00b7ter", "mir", "di\u00df", "p\u00fcnct\u00b7gen", "aus", "der", "h\u00f6h", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "PPER", "PDS", "VVFIN", "APPR", "ART", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Was wunder, da\u00df ich kont die welt so bald hinlegen?", "tokens": ["Was", "wun\u00b7der", ",", "da\u00df", "ich", "kont", "die", "welt", "so", "bald", "hin\u00b7le\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PPER", "VMFIN", "ART", "NN", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die kugel und ein punct ist leichtlich zu bewegen.", "tokens": ["Die", "ku\u00b7gel", "und", "ein", "punct", "ist", "leicht\u00b7lich", "zu", "be\u00b7we\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJD", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}