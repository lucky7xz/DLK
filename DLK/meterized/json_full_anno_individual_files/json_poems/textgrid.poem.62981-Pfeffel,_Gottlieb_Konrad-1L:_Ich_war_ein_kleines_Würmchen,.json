{"textgrid.poem.62981": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich war ein kleines W\u00fcrmchen,", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich war ein kleines W\u00fcrmchen,", "tokens": ["Ich", "war", "ein", "klei\u00b7nes", "W\u00fcrm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch kaum vier Spannen gro\u00df,", "tokens": ["Noch", "kaum", "vier", "Span\u00b7nen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und pikt in einer Laube", "tokens": ["Und", "pikt", "in", "ei\u00b7ner", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An einer goldnen Traube", "tokens": ["An", "ei\u00b7ner", "gold\u00b7nen", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf meiner Mutter Schoo\u00df.", "tokens": ["Auf", "mei\u00b7ner", "Mut\u00b7ter", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da stieg ein schwarzer Drache,", "tokens": ["Da", "stieg", "ein", "schwar\u00b7zer", "Dra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Mutter sah ihn nicht,", "tokens": ["Die", "Mut\u00b7ter", "sah", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus einer faulen Pf\u00fctze,", "tokens": ["Aus", "ei\u00b7ner", "fau\u00b7len", "Pf\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und blies, wie fahle Blitze", "tokens": ["Und", "blies", ",", "wie", "fah\u00b7le", "Blit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Gift mir ins Gesicht.", "tokens": ["Sein", "Gift", "mir", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Da ward es pl\u00f6tzlich dunkel", "tokens": ["Da", "ward", "es", "pl\u00f6tz\u00b7lich", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und einsam um mich her,", "tokens": ["Und", "ein\u00b7sam", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es konnten meine Augen", "tokens": ["Es", "konn\u00b7ten", "mei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Licht mehr in sich saugen,", "tokens": ["Kein", "Licht", "mehr", "in", "sich", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sonne schien nicht mehr.", "tokens": ["Die", "Son\u00b7ne", "schien", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "O Mutter, liebe Mutter!", "tokens": ["O", "Mut\u00b7ter", ",", "lie\u00b7be", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rief ich der Guten zu,", "tokens": ["Rief", "ich", "der", "Gu\u00b7ten", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hieng an ihrer Wange,", "tokens": ["Und", "hieng", "an", "ih\u00b7rer", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie bang ist mir, wie bange,", "tokens": ["Wie", "bang", "ist", "mir", ",", "wie", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo bin ich, wo bist du?", "tokens": ["Wo", "bin", "ich", ",", "wo", "bist", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Sie netzte mich mit Thr\u00e4nen,", "tokens": ["Sie", "netz\u00b7te", "mich", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rief den im Himmel an,", "tokens": ["Rief", "den", "im", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bat Menschen mir zu helfen,", "tokens": ["Bat", "Men\u00b7schen", "mir", "zu", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keiner konnte helfen", "tokens": ["Und", "kei\u00b7ner", "konn\u00b7te", "hel\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von allen, die mich sahn.", "tokens": ["Von", "al\u00b7len", ",", "die", "mich", "sahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "So schlich ich lang im Finstern", "tokens": ["So", "schlich", "ich", "lang", "im", "Fins\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An ihrer Hand umher.", "tokens": ["An", "ih\u00b7rer", "Hand", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entw\u00f6hnt vom bunten Tande,", "tokens": ["Ent\u00b7w\u00f6hnt", "vom", "bun\u00b7ten", "Tan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fand nie mein Geist die Bande,", "tokens": ["Fand", "nie", "mein", "Geist", "die", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Worin er lag, zu schwer.", "tokens": ["Wo\u00b7rin", "er", "lag", ",", "zu", "schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "An einem Feste Gottes,", "tokens": ["An", "ei\u00b7nem", "Fes\u00b7te", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ich ein Lied ihm sang,", "tokens": ["Als", "ich", "ein", "Lied", "ihm", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6rt ich Fl\u00fcgel schwirren", "tokens": ["Da", "h\u00f6rt", "ich", "Fl\u00fc\u00b7gel", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eine Stimme girren,", "tokens": ["Und", "ei\u00b7ne", "Stim\u00b7me", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So sanft wie Fl\u00f6tenklang;", "tokens": ["So", "sanft", "wie", "Fl\u00f6\u00b7ten\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Sie sprach: ich bin der Engel", "tokens": ["Sie", "sprach", ":", "ich", "bin", "der", "En\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der s\u00fc\u00dfen Harmonie,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der oft den Menschenkindern,", "tokens": ["Der", "oft", "den", "Men\u00b7schen\u00b7kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens Gram zu lindern,", "tokens": ["Des", "Le\u00b7bens", "Gram", "zu", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon seine Harfe lieh.", "tokens": ["Schon", "sei\u00b7ne", "Har\u00b7fe", "lieh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du kennest mich: auf Erden", "tokens": ["Du", "ken\u00b7nest", "mich", ":", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hie\u00df ich C\u00e4cilia;", "tokens": ["Hie\u00df", "ich", "C\u00e4\u00b7ci\u00b7lia", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mein Lob sang Popens Laute,", "tokens": ["Mein", "Lob", "sang", "Po\u00b7pens", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Solon Fr\u00e4nklin baute", "tokens": ["Und", "So\u00b7lon", "Fr\u00e4nk\u00b7lin", "bau\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir die Harmonika:", "tokens": ["Mir", "die", "Har\u00b7mo\u00b7ni\u00b7ka", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.10": {"line.1": {"text": "Heil dir! zu deinem Troste", "tokens": ["Heil", "dir", "!", "zu", "dei\u00b7nem", "Tros\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich herabgesandt.", "tokens": ["Bin", "ich", "her\u00b7ab\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie fa\u00dft mir Hand und Kehle,", "tokens": ["Sie", "fa\u00dft", "mir", "Hand", "und", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eine neue Seele", "tokens": ["Und", "ei\u00b7ne", "neu\u00b7e", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durchstr\u00f6mte Kehl und Hand.", "tokens": ["Durch\u00b7str\u00f6m\u00b7te", "Kehl", "und", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Sie schied. Auf meinem Schoo\u00dfe", "tokens": ["Sie", "schied", ".", "Auf", "mei\u00b7nem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fand ich ein Saitenspiel.", "tokens": ["Fand", "ich", "ein", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Laut verdrang mein Leiden;", "tokens": ["Sein", "Laut", "ver\u00b7drang", "mein", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Busen schmolz in Freuden", "tokens": ["Mein", "Bu\u00b7sen", "schmolz", "in", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Harmoniegef\u00fchl.", "tokens": ["Und", "Har\u00b7mo\u00b7nie\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Einst spielt ich in dem Tempel", "tokens": ["Einst", "spielt", "ich", "in", "dem", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das heilge Meisterst\u00fcck", "tokens": ["Das", "heil\u00b7ge", "Meis\u00b7ter\u00b7st\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des gro\u00dfen Pergolese;", "tokens": ["Des", "gro\u00b7\u00dfen", "Per\u00b7go\u00b7le\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da h\u00f6rte mich Therese,", "tokens": ["Da", "h\u00f6r\u00b7te", "mich", "The\u00b7re\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sorgte f\u00fcr mein Gl\u00fcck.", "tokens": ["Und", "sorg\u00b7te", "f\u00fcr", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "O lebte sie!... doch schweige", "tokens": ["O", "leb\u00b7te", "sie", "!", "...", "doch", "schwei\u00b7ge"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "$(", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein allzuwacher Schmerz!", "tokens": ["Mein", "all\u00b7zu\u00b7wa\u00b7cher", "Schmerz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fand ich in S\u00fcd und Westen", "tokens": ["Fand", "ich", "in", "S\u00fcd", "und", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Menschen, die mich tr\u00f6sten,", "tokens": ["Nicht", "Men\u00b7schen", ",", "die", "mich", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht Balsam f\u00fcr mein Herz?", "tokens": ["Nicht", "Bal\u00b7sam", "f\u00fcr", "mein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "S\u00fc\u00df ists, wenn meine Cymbel", "tokens": ["S\u00fc\u00df", "ists", ",", "wenn", "mei\u00b7ne", "Cym\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins Mark der Seele dringt,", "tokens": ["Ins", "Mark", "der", "See\u00b7le", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann ein edler Hirte", "tokens": ["Und", "dann", "ein", "ed\u00b7ler", "Hir\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00f6lker eine Myrte", "tokens": ["Der", "V\u00f6l\u00b7ker", "ei\u00b7ne", "Myr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir um den Scheitel schlingt.", "tokens": ["Mir", "um", "den", "Schei\u00b7tel", "schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Doch s\u00fc\u00dfer, traute Freunde!", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", ",", "trau\u00b7te", "Freun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Euer H\u00e4ndedruck,", "tokens": ["Ist", "Eu\u00b7er", "H\u00e4n\u00b7de\u00b7druck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind Eure sanften Thr\u00e4nen;", "tokens": ["Sind", "Eu\u00b7re", "sanf\u00b7ten", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja diese, diese kr\u00f6nen", "tokens": ["Ja", "die\u00b7se", ",", "die\u00b7se", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKANT", "PDS", "$,", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich mehr als Perlenschmuck.", "tokens": ["Mich", "mehr", "als", "Per\u00b7len\u00b7schmuck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Ich war ein kleines W\u00fcrmchen,", "tokens": ["Ich", "war", "ein", "klei\u00b7nes", "W\u00fcrm\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Noch kaum vier Spannen gro\u00df,", "tokens": ["Noch", "kaum", "vier", "Span\u00b7nen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "CARD", "NN", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und pikt in einer Laube", "tokens": ["Und", "pikt", "in", "ei\u00b7ner", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "An einer goldnen Traube", "tokens": ["An", "ei\u00b7ner", "gold\u00b7nen", "Trau\u00b7be"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Auf meiner Mutter Schoo\u00df.", "tokens": ["Auf", "mei\u00b7ner", "Mut\u00b7ter", "Schoo\u00df", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Da stieg ein schwarzer Drache,", "tokens": ["Da", "stieg", "ein", "schwar\u00b7zer", "Dra\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Mutter sah ihn nicht,", "tokens": ["Die", "Mut\u00b7ter", "sah", "ihn", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus einer faulen Pf\u00fctze,", "tokens": ["Aus", "ei\u00b7ner", "fau\u00b7len", "Pf\u00fct\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und blies, wie fahle Blitze", "tokens": ["Und", "blies", ",", "wie", "fah\u00b7le", "Blit\u00b7ze"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "PWAV", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Sein Gift mir ins Gesicht.", "tokens": ["Sein", "Gift", "mir", "ins", "Ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Da ward es pl\u00f6tzlich dunkel", "tokens": ["Da", "ward", "es", "pl\u00f6tz\u00b7lich", "dun\u00b7kel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und einsam um mich her,", "tokens": ["Und", "ein\u00b7sam", "um", "mich", "her", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Es konnten meine Augen", "tokens": ["Es", "konn\u00b7ten", "mei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Kein Licht mehr in sich saugen,", "tokens": ["Kein", "Licht", "mehr", "in", "sich", "sau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Die Sonne schien nicht mehr.", "tokens": ["Die", "Son\u00b7ne", "schien", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "O Mutter, liebe Mutter!", "tokens": ["O", "Mut\u00b7ter", ",", "lie\u00b7be", "Mut\u00b7ter", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rief ich der Guten zu,", "tokens": ["Rief", "ich", "der", "Gu\u00b7ten", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und hieng an ihrer Wange,", "tokens": ["Und", "hieng", "an", "ih\u00b7rer", "Wan\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wie bang ist mir, wie bange,", "tokens": ["Wie", "bang", "ist", "mir", ",", "wie", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo bin ich, wo bist du?", "tokens": ["Wo", "bin", "ich", ",", "wo", "bist", "du", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Sie netzte mich mit Thr\u00e4nen,", "tokens": ["Sie", "netz\u00b7te", "mich", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Rief den im Himmel an,", "tokens": ["Rief", "den", "im", "Him\u00b7mel", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bat Menschen mir zu helfen,", "tokens": ["Bat", "Men\u00b7schen", "mir", "zu", "hel\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und keiner konnte helfen", "tokens": ["Und", "kei\u00b7ner", "konn\u00b7te", "hel\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Von allen, die mich sahn.", "tokens": ["Von", "al\u00b7len", ",", "die", "mich", "sahn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIS", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.21": {"line.1": {"text": "So schlich ich lang im Finstern", "tokens": ["So", "schlich", "ich", "lang", "im", "Fins\u00b7tern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPER", "ADJD", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "An ihrer Hand umher.", "tokens": ["An", "ih\u00b7rer", "Hand", "um\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Entw\u00f6hnt vom bunten Tande,", "tokens": ["Ent\u00b7w\u00f6hnt", "vom", "bun\u00b7ten", "Tan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Fand nie mein Geist die Bande,", "tokens": ["Fand", "nie", "mein", "Geist", "die", "Ban\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Worin er lag, zu schwer.", "tokens": ["Wo\u00b7rin", "er", "lag", ",", "zu", "schwer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PTKA", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.22": {"line.1": {"text": "An einem Feste Gottes,", "tokens": ["An", "ei\u00b7nem", "Fes\u00b7te", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als ich ein Lied ihm sang,", "tokens": ["Als", "ich", "ein", "Lied", "ihm", "sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da h\u00f6rt ich Fl\u00fcgel schwirren", "tokens": ["Da", "h\u00f6rt", "ich", "Fl\u00fc\u00b7gel", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eine Stimme girren,", "tokens": ["Und", "ei\u00b7ne", "Stim\u00b7me", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "So sanft wie Fl\u00f6tenklang;", "tokens": ["So", "sanft", "wie", "Fl\u00f6\u00b7ten\u00b7klang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.23": {"line.1": {"text": "Sie sprach: ich bin der Engel", "tokens": ["Sie", "sprach", ":", "ich", "bin", "der", "En\u00b7gel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der s\u00fc\u00dfen Harmonie,", "tokens": ["Der", "s\u00fc\u00b7\u00dfen", "Har\u00b7mo\u00b7nie", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der oft den Menschenkindern,", "tokens": ["Der", "oft", "den", "Men\u00b7schen\u00b7kin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Lebens Gram zu lindern,", "tokens": ["Des", "Le\u00b7bens", "Gram", "zu", "lin\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Schon seine Harfe lieh.", "tokens": ["Schon", "sei\u00b7ne", "Har\u00b7fe", "lieh", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.24": {"line.1": {"text": "Du kennest mich: auf Erden", "tokens": ["Du", "ken\u00b7nest", "mich", ":", "auf", "Er\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$.", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hie\u00df ich C\u00e4cilia;", "tokens": ["Hie\u00df", "ich", "C\u00e4\u00b7ci\u00b7lia", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NE", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Mein Lob sang Popens Laute,", "tokens": ["Mein", "Lob", "sang", "Po\u00b7pens", "Lau\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "NN", "$,"], "meter": "-++--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und Solon Fr\u00e4nklin baute", "tokens": ["Und", "So\u00b7lon", "Fr\u00e4nk\u00b7lin", "bau\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "NE", "NE", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir die Harmonika:", "tokens": ["Mir", "die", "Har\u00b7mo\u00b7ni\u00b7ka", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ART", "NE", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.25": {"line.1": {"text": "Heil dir! zu deinem Troste", "tokens": ["Heil", "dir", "!", "zu", "dei\u00b7nem", "Tros\u00b7te"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Bin ich herabgesandt.", "tokens": ["Bin", "ich", "her\u00b7ab\u00b7ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie fa\u00dft mir Hand und Kehle,", "tokens": ["Sie", "fa\u00dft", "mir", "Hand", "und", "Keh\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und eine neue Seele", "tokens": ["Und", "ei\u00b7ne", "neu\u00b7e", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Durchstr\u00f6mte Kehl und Hand.", "tokens": ["Durch\u00b7str\u00f6m\u00b7te", "Kehl", "und", "Hand", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.26": {"line.1": {"text": "Sie schied. Auf meinem Schoo\u00dfe", "tokens": ["Sie", "schied", ".", "Auf", "mei\u00b7nem", "Schoo\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Fand ich ein Saitenspiel.", "tokens": ["Fand", "ich", "ein", "Sai\u00b7ten\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sein Laut verdrang mein Leiden;", "tokens": ["Sein", "Laut", "ver\u00b7drang", "mein", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "APPR", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mein Busen schmolz in Freuden", "tokens": ["Mein", "Bu\u00b7sen", "schmolz", "in", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "ADJD", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und Harmoniegef\u00fchl.", "tokens": ["Und", "Har\u00b7mo\u00b7nie\u00b7ge\u00b7f\u00fchl", "."], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.27": {"line.1": {"text": "Einst spielt ich in dem Tempel", "tokens": ["Einst", "spielt", "ich", "in", "dem", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Das heilge Meisterst\u00fcck", "tokens": ["Das", "heil\u00b7ge", "Meis\u00b7ter\u00b7st\u00fcck"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Des gro\u00dfen Pergolese;", "tokens": ["Des", "gro\u00b7\u00dfen", "Per\u00b7go\u00b7le\u00b7se", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da h\u00f6rte mich Therese,", "tokens": ["Da", "h\u00f6r\u00b7te", "mich", "The\u00b7re\u00b7se", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Und sorgte f\u00fcr mein Gl\u00fcck.", "tokens": ["Und", "sorg\u00b7te", "f\u00fcr", "mein", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.28": {"line.1": {"text": "O lebte sie!... doch schweige", "tokens": ["O", "leb\u00b7te", "sie", "!", "...", "doch", "schwei\u00b7ge"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "$.", "$(", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mein allzuwacher Schmerz!", "tokens": ["Mein", "all\u00b7zu\u00b7wa\u00b7cher", "Schmerz", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Fand ich in S\u00fcd und Westen", "tokens": ["Fand", "ich", "in", "S\u00fcd", "und", "Wes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nicht Menschen, die mich tr\u00f6sten,", "tokens": ["Nicht", "Men\u00b7schen", ",", "die", "mich", "tr\u00f6s\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Nicht Balsam f\u00fcr mein Herz?", "tokens": ["Nicht", "Bal\u00b7sam", "f\u00fcr", "mein", "Herz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "NE", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.29": {"line.1": {"text": "S\u00fc\u00df ists, wenn meine Cymbel", "tokens": ["S\u00fc\u00df", "ists", ",", "wenn", "mei\u00b7ne", "Cym\u00b7bel"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ins Mark der Seele dringt,", "tokens": ["Ins", "Mark", "der", "See\u00b7le", "dringt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann ein edler Hirte", "tokens": ["Und", "dann", "ein", "ed\u00b7ler", "Hir\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der V\u00f6lker eine Myrte", "tokens": ["Der", "V\u00f6l\u00b7ker", "ei\u00b7ne", "Myr\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mir um den Scheitel schlingt.", "tokens": ["Mir", "um", "den", "Schei\u00b7tel", "schlingt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.30": {"line.1": {"text": "Doch s\u00fc\u00dfer, traute Freunde!", "tokens": ["Doch", "s\u00fc\u00b7\u00dfer", ",", "trau\u00b7te", "Freun\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADJD", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist Euer H\u00e4ndedruck,", "tokens": ["Ist", "Eu\u00b7er", "H\u00e4n\u00b7de\u00b7druck", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sind Eure sanften Thr\u00e4nen;", "tokens": ["Sind", "Eu\u00b7re", "sanf\u00b7ten", "Thr\u00e4\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ja diese, diese kr\u00f6nen", "tokens": ["Ja", "die\u00b7se", ",", "die\u00b7se", "kr\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKANT", "PDS", "$,", "PDAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mich mehr als Perlenschmuck.", "tokens": ["Mich", "mehr", "als", "Per\u00b7len\u00b7schmuck", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "KOUS", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}