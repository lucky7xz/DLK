{"dta.poem.19828": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Trinklied .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die liebste Buhle, die ich han,               ", "tokens": ["Die", "liebs\u00b7te", "Buh\u00b7le", ",", "die", "ich", "han", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die liegt beim Wirth im Keller,", "tokens": ["Die", "liegt", "beim", "Wirth", "im", "Kel\u00b7ler", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPRART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Sie hat ein h\u00f6lzern R\u00f6cklein an,", "tokens": ["Sie", "hat", "ein", "h\u00f6l\u00b7zern", "R\u00f6ck\u00b7lein", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hei\u00dft der Muskateller.", "tokens": ["Und", "hei\u00dft", "der", "Mus\u00b7ka\u00b7tel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+--+--", "measure": "iambic.di.relaxed"}, "line.5": {"text": "Sie hat mich n\u00e4chten trunken gemacht,", "tokens": ["Sie", "hat", "mich", "n\u00e4ch\u00b7ten", "trun\u00b7ken", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Und fr\u00f6hlich mir den Tag vollbracht,", "tokens": ["Und", "fr\u00f6h\u00b7lich", "mir", "den", "Tag", "voll\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Drum w\u00fcnsch ich ihr ein gute Nacht.", "tokens": ["Drum", "w\u00fcnsch", "ich", "ihr", "ein", "gu\u00b7te", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie hat mich auch so angelacht,", "tokens": ["Sie", "hat", "mich", "auch", "so", "an\u00b7ge\u00b7lacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df ich die Sprach verlohren,", "tokens": ["Da\u00df", "ich", "die", "Sprach", "ver\u00b7loh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und hat mir gestern Bauchweh gemacht", "tokens": ["Und", "hat", "mir", "ge\u00b7stern", "Bauch\u00b7weh", "ge\u00b7macht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "ADJA", "NN", "VVPP"], "meter": "-+--+---+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Wohl zwischen meinen Ohren,", "tokens": ["Wohl", "zwi\u00b7schen", "mei\u00b7nen", "Oh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum thu ich ihr ein Possen heut,", "tokens": ["Drum", "thu", "ich", "ihr", "ein", "Pos\u00b7sen", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "PPER", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und bring zu ihr ein andre Maid,", "tokens": ["Und", "bring", "zu", "ihr", "ein", "and\u00b7re", "Maid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die mag mit ihr bestehn den Streit.", "tokens": ["Die", "mag", "mit", "ihr", "be\u00b7stehn", "den", "Streit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun M\u00e4gdlein halt dein Kr\u00e4nzlein fest,", "tokens": ["Nun", "M\u00e4gd\u00b7lein", "halt", "dein", "Kr\u00e4nz\u00b7lein", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df du nicht k\u00f6mmst zum weichen,", "tokens": ["Da\u00df", "du", "nicht", "k\u00f6mmst", "zum", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "APPRART", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mein Wein thut heut gewi\u00df sein Best,", "tokens": ["Mein", "Wein", "thut", "heut", "ge\u00b7wi\u00df", "sein", "Best", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar sanft wird er einschleichen.", "tokens": ["Gar", "sanft", "wird", "er", "ein\u00b7schlei\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mein Herz h\u00e4lt Wasser als ein Sieb,", "tokens": ["Mein", "Herz", "h\u00e4lt", "Was\u00b7ser", "als", "ein", "Sieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "NN", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Buhl, er ist mir gar zu lieb.", "tokens": ["Mein", "Buhl", ",", "er", "ist", "mir", "gar", "zu", "lieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PPER", "VAFIN", "PPER", "ADV", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Steig ein, schleich ein du lieber Dieb.", "tokens": ["Steig", "ein", ",", "schleich", "ein", "du", "lie\u00b7ber", "Dieb", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "ADJD", "ART", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Soll ich mein Kr\u00e4nzlein halten fest,", "tokens": ["Soll", "ich", "mein", "Kr\u00e4nz\u00b7lein", "hal\u00b7ten", "fest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Das sein h\u00e4ngt an der Pforten,", "tokens": ["Das", "sein", "h\u00e4ngt", "an", "der", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAINF", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Viel lieber w\u00e4r ich nie gewest", "tokens": ["Viel", "lie\u00b7ber", "w\u00e4r", "ich", "nie", "ge\u00b7west"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In diesem schweren Orden.", "tokens": ["In", "die\u00b7sem", "schwe\u00b7ren", "Or\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dein Buhl dreht mir die Sinnen all,", "tokens": ["Dein", "Buhl", "dreht", "mir", "die", "Sin\u00b7nen", "all", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "PIAT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Gl\u00e4slein hat ein glatten Schall,", "tokens": ["Das", "Gl\u00e4s\u00b7lein", "hat", "ein", "glat\u00b7ten", "Schall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Gieb acht mein Knab, da\u00df ich nit fall.", "tokens": ["Gieb", "acht", "mein", "Knab", ",", "da\u00df", "ich", "nit", "fall", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "CARD", "PPOSAT", "NN", "$,", "KOUS", "PPER", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und wenn er in ein faul Fa\u00df k\u00e4m,", "tokens": ["Und", "wenn", "er", "in", "ein", "faul", "Fa\u00df", "k\u00e4m", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-++-++", "measure": "unknown.measure.penta"}, "line.2": {"text": "So m\u00fc\u00dft mein Wein versauren,", "tokens": ["So", "m\u00fc\u00dft", "mein", "Wein", "ver\u00b7sau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und wenn ich eine andre n\u00e4hm,", "tokens": ["Und", "wenn", "ich", "ei\u00b7ne", "and\u00b7re", "n\u00e4hm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "So m\u00fc\u00dft mein Herz vertrauren;", "tokens": ["So", "m\u00fc\u00dft", "mein", "Herz", "ver\u00b7trau\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Drum will mein Buhl mir stehen bei,", "tokens": ["Drum", "will", "mein", "Buhl", "mir", "ste\u00b7hen", "bei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VMFIN", "PPOSAT", "NN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er lehrt mich sagen also frei,", "tokens": ["Er", "lehrt", "mich", "sa\u00b7gen", "al\u00b7so", "frei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df ich dich mein mit steter Treu.", "tokens": ["Da\u00df", "ich", "dich", "mein", "mit", "ste\u00b7ter", "Treu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "PPOSAT", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Und w\u00e4r ein F\u00e4\u00dflein noch so rein,", "tokens": ["Und", "w\u00e4r", "ein", "F\u00e4\u00df\u00b7lein", "noch", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So findt man Drusen drinnen,", "tokens": ["So", "findt", "man", "Dru\u00b7sen", "drin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und w\u00e4r ein Knabe noch so fein,", "tokens": ["Und", "w\u00e4r", "ein", "Kna\u00b7be", "noch", "so", "fein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ART", "NN", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist er doch falsch von Sinnen.", "tokens": ["Ist", "er", "doch", "falsch", "von", "Sin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Mit Spinnen voll ein Zuckerlad,", "tokens": ["Mit", "Spin\u00b7nen", "voll", "ein", "Zu\u00b7cker\u00b7lad", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "O Weh, der mich verf\u00fchret hat", "tokens": ["O", "Weh", ",", "der", "mich", "ver\u00b7f\u00fch\u00b7ret", "hat"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "PRELS", "PPER", "VVPP", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf diesen steilen Rebenpfad.", "tokens": ["Auf", "die\u00b7sen", "stei\u00b7len", "Re\u00b7ben\u00b7pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ach M\u00e4gdlein la\u00df dein Weinen seyn,", "tokens": ["Ach", "M\u00e4gd\u00b7lein", "la\u00df", "dein", "Wei\u00b7nen", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVIMP", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bis da\u00df geweint die Reben,", "tokens": ["Bis", "da\u00df", "ge\u00b7weint", "die", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "VVPP", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bringst du mir ein Kn\u00e4belein,", "tokens": ["Und", "bringst", "du", "mir", "ein", "Kn\u00e4\u00b7be\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Winzer soll es geben,", "tokens": ["Ein", "Win\u00b7zer", "soll", "es", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Und bringst du ein klein M\u00e4gdelein,", "tokens": ["Und", "bringst", "du", "ein", "klein", "M\u00e4g\u00b7del\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Solls n\u00e4hen mit der Nadel fein", "tokens": ["Solls", "n\u00e4\u00b7hen", "mit", "der", "Na\u00b7del", "fein"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Den Schlemmern ihre Hemdelein.", "tokens": ["Den", "Schlem\u00b7mern", "ih\u00b7re", "Hem\u00b7del\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}