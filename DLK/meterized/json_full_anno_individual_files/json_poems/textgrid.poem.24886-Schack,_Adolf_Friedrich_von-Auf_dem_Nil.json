{"textgrid.poem.24886": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Auf dem Nil", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Welch ein Geheimnis bergen deine Wellen,", "tokens": ["Welch", "ein", "Ge\u00b7heim\u00b7nis", "ber\u00b7gen", "dei\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O alter Nil, der ferneher,", "tokens": ["O", "al\u00b7ter", "Nil", ",", "der", "fer\u00b7ne\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Tropensonnenstrahlen deine Quellen", "tokens": ["Wo", "Tro\u00b7pen\u00b7son\u00b7nen\u00b7strah\u00b7len", "dei\u00b7ne", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Gletscherhaupt des Mondgebirgs erhellen,", "tokens": ["Am", "Glet\u00b7scher\u00b7haupt", "des", "Mond\u00b7ge\u00b7birgs", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du sinnend gleitest in das Meer?", "tokens": ["Du", "sin\u00b7nend", "glei\u00b7test", "in", "das", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Von deinem Wogenspiele sanft gehoben,", "tokens": ["Von", "dei\u00b7nem", "Wo\u00b7gen\u00b7spie\u00b7le", "sanft", "ge\u00b7ho\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Blick' ich, ans Steuer hingeschmiegt,", "tokens": ["Blick'", "ich", ",", "ans", "Steu\u00b7er", "hin\u00b7ge\u00b7schmiegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bald auf zur blauen Himmelsw\u00f6lbung droben,", "tokens": ["Bald", "auf", "zur", "blau\u00b7en", "Him\u00b7mels\u00b7w\u00f6l\u00b7bung", "dro\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "APPRART", "ADJA", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bald abw\u00e4rts, wo, aus Silberglanz gewoben,", "tokens": ["Bald", "ab\u00b7w\u00e4rts", ",", "wo", ",", "aus", "Sil\u00b7ber\u00b7glanz", "ge\u00b7wo\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein zweiter Sternenhimmel liegt.", "tokens": ["Ein", "zwei\u00b7ter", "Ster\u00b7nen\u00b7him\u00b7mel", "liegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bleikuppeln ragen, wei\u00dfgezinnte St\u00e4dte", "tokens": ["Blei\u00b7kup\u00b7peln", "ra\u00b7gen", ",", "wei\u00df\u00b7ge\u00b7zinn\u00b7te", "St\u00e4d\u00b7te"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.2": {"text": "Hervor aus dunklem Palmenwald,", "tokens": ["Her\u00b7vor", "aus", "dunk\u00b7lem", "Pal\u00b7men\u00b7wald", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Moscheen und goldne Halbmondminarete,", "tokens": ["Mo\u00b7scheen", "und", "gold\u00b7ne", "Halb\u00b7mond\u00b7mi\u00b7na\u00b7re\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von denen oft ein Rufen zum Gebete,", "tokens": ["Von", "de\u00b7nen", "oft", "ein", "Ru\u00b7fen", "zum", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ADV", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die Flut im Nachtwind kr\u00e4uselnd, schallt.", "tokens": ["Die", "Flut", "im", "Nacht\u00b7wind", "kr\u00e4u\u00b7selnd", ",", "schallt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Grabhallen, draus den Staub der Pharaonen", "tokens": ["Grab\u00b7hal\u00b7len", ",", "draus", "den", "Staub", "der", "Pha\u00b7ra\u00b7o\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "PAV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Wind der W\u00fcste lang verstreut,", "tokens": ["Der", "Wind", "der", "W\u00fcs\u00b7te", "lang", "ver\u00b7streut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zertr\u00fcmmerte Pal\u00e4ste und Pylonen", "tokens": ["Zer\u00b7tr\u00fcm\u00b7mer\u00b7te", "Pa\u00b7l\u00e4s\u00b7te", "und", "Py\u00b7lo\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bei H\u00fctten Lehms, drin braune Fellahs wohnen,", "tokens": ["Bei", "H\u00fct\u00b7ten", "Lehms", ",", "drin", "brau\u00b7ne", "Fel\u00b7lahs", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "NE", "$,", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Das \u00e4rmliche Geschlecht des Heut!", "tokens": ["Das", "\u00e4rm\u00b7li\u00b7che", "Ge\u00b7schlecht", "des", "Heut", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann Obelisken, noch zur Sonne steigend,", "tokens": ["Dann", "O\u00b7be\u00b7lis\u00b7ken", ",", "noch", "zur", "Son\u00b7ne", "stei\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "$,", "ADV", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Pyramiden von Granit,", "tokens": ["Und", "Py\u00b7ra\u00b7mi\u00b7den", "von", "Gra\u00b7nit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NE", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Gesunkne Riesentempel, ewig schweigend,", "tokens": ["Ge\u00b7sunk\u00b7ne", "Rie\u00b7sen\u00b7tem\u00b7pel", ",", "e\u00b7wig", "schwei\u00b7gend", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "ADJD", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Bildern noch des Rhamses K\u00e4mpfe zeigend,", "tokens": ["In", "Bil\u00b7dern", "noch", "des", "Rham\u00b7ses", "K\u00e4mp\u00b7fe", "zei\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie er das Weltreich sich erstritt!", "tokens": ["Wie", "er", "das", "Welt\u00b7reich", "sich", "er\u00b7stritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "An S\u00e4ulenst\u00fcrzen, die schon Tr\u00fcmmer waren,", "tokens": ["An", "S\u00e4u\u00b7len\u00b7st\u00fcr\u00b7zen", ",", "die", "schon", "Tr\u00fcm\u00b7mer", "wa\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da Nacht Europa noch umschlang,", "tokens": ["Da", "Nacht", "Eu\u00b7ro\u00b7pa", "noch", "um\u00b7schlang", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zieht mit den hochgehalsten Dromedaren,", "tokens": ["Zieht", "mit", "den", "hoch\u00b7ge\u00b7hals\u00b7ten", "Dro\u00b7me\u00b7da\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Umweht vom Staube von f\u00fcnftausend Jahren,", "tokens": ["Um\u00b7weht", "vom", "Stau\u00b7be", "von", "f\u00fcnf\u00b7tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der Karawanenzug entlang.", "tokens": ["Der", "Ka\u00b7ra\u00b7wa\u00b7nen\u00b7zug", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Vorbei! Stets weiter werd' ich fortgezogen,", "tokens": ["Vor\u00b7bei", "!", "Stets", "wei\u00b7ter", "werd'", "ich", "fort\u00b7ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "ADV", "ADV", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als ende nimmerdar die Fahrt;", "tokens": ["Als", "en\u00b7de", "nim\u00b7mer\u00b7dar", "die", "Fahrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie traumhaft murmeln um mein Haupt die Wogen,", "tokens": ["Wie", "traum\u00b7haft", "mur\u00b7meln", "um", "mein", "Haupt", "die", "Wo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Sterne tauchen auf am Himmelsbogen,", "tokens": ["Und", "Ster\u00b7ne", "tau\u00b7chen", "auf", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Die nie des Nordens Blick gewahrt.", "tokens": ["Die", "nie", "des", "Nor\u00b7dens", "Blick", "ge\u00b7wahrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Welch ein Geheimnis bergen deine Wellen,", "tokens": ["Welch", "ein", "Ge\u00b7heim\u00b7nis", "ber\u00b7gen", "dei\u00b7ne", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "O alter Nil, der ferneher,", "tokens": ["O", "al\u00b7ter", "Nil", ",", "der", "fer\u00b7ne\u00b7her", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wo Tropensonnenstrahlen deine Quellen", "tokens": ["Wo", "Tro\u00b7pen\u00b7son\u00b7nen\u00b7strah\u00b7len", "dei\u00b7ne", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Am Gletscherhaupt des Mondgebirgs erhellen,", "tokens": ["Am", "Glet\u00b7scher\u00b7haupt", "des", "Mond\u00b7ge\u00b7birgs", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Du sinnend gleitest in das Meer?", "tokens": ["Du", "sin\u00b7nend", "glei\u00b7test", "in", "das", "Meer", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Welch ein Geheimnis bergen deine Wellen,", "tokens": ["Welch", "ein", "Ge\u