{"dta.poem.23838": {"metadata": {"author": {"name": "Canitz, Friedrich Rudolph Ludwig von", "birth": "N.A.", "death": "N.A."}, "title": "Uber das Bildni\u00df des Printzen von  \n Wallis.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1700", "urn": "urn:nbn:de:kobv:b4-200905197532", "language": ["de:0.99"], "booktitle": "[Canitz, Friedrich Rudolph Ludwig von]: Neben-Stunden Unterschiedener Gedichte. [Hrsg. v. Joachim Lange]. Berlin, 1700."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Vater hei\u00dft mich Sohn/ die Schwestern sagen nein/", "tokens": ["Ein", "Va\u00b7ter", "hei\u00dft", "mich", "Sohn", "/", "die", "Schwes\u00b7tern", "sa\u00b7gen", "nein", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "NN", "$(", "ART", "NN", "VVFIN", "PTKANT", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wollen nicht einmahl der Mutter Zeugni\u00df glauben/", "tokens": ["Und", "wol\u00b7len", "nicht", "ein\u00b7mahl", "der", "Mut\u00b7ter", "Zeug\u00b7ni\u00df", "glau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PTKNEG", "ADV", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So kan dann zwischen mir/ und die mir Tronen rauben/", "tokens": ["So", "kan", "dann", "zwi\u00b7schen", "mir", "/", "und", "die", "mir", "Tro\u00b7nen", "rau\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ADV", "APPR", "PPER", "$(", "KON", "PRELS", "PPER", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Allein GOtt und die Zeit/ sonst niemand Richter seyn.", "tokens": ["Al\u00b7lein", "Gott", "und", "die", "Zeit", "/", "sonst", "nie\u00b7mand", "Rich\u00b7ter", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "ART", "NN", "$(", "ADV", "PIS", "NN", "VAINF", "$."], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}}}}}