{"dta.poem.952": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "S IEG   D ES   S OMMERS", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Du willst mit mir ein reich der sonne stiften               ", "tokens": ["Du", "willst", "mit", "mir", "ein", "reich", "der", "son\u00b7ne", "stif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "PPER", "ART", "ADJD", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Darinnen uns allein die freude ziere               ", "tokens": ["Da\u00b7rin\u00b7nen", "uns", "al\u00b7lein", "die", "freu\u00b7de", "zie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "ADV", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie heilige die haine und die triften               ", "tokens": ["Sie", "hei\u00b7li\u00b7ge", "die", "hai\u00b7ne", "und", "die", "trif\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "KON", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Eh unsre pracht und ihre sich verliere             ", "tokens": ["Eh", "uns\u00b7re", "pracht", "und", "ih\u00b7re", "sich", "ver\u00b7lie\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Dass dieses s&#252;sse leben uns gen&#252;ge               ", "tokens": ["Dass", "die\u00b7ses", "s", "&#252;", "sse", "le\u00b7ben", "uns", "gen", "&#252;", "ge"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PDAT", "FM", "FM", "FM", "VVFIN", "PPER", "APPR", "NE", "NE"], "meter": "-+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Dass wir hier wohnen dankbereite g&#228;ste               ", "tokens": ["Dass", "wir", "hier", "woh\u00b7nen", "dank\u00b7be\u00b7rei\u00b7te", "g", "&#228;", "ste"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VVFIN", "NE", "$(", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und wort und lied ersinnst du dass gef&#252;ge               ", "tokens": ["Und", "wort", "und", "lied", "er\u00b7sinnst", "du", "dass", "gef", "&#252;", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "PPER", "PDS", "VVFIN", "$(", "NE"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Die klagen flattern in die h\u00f6chsten &#228;ste             ", "tokens": ["Die", "kla\u00b7gen", "flat\u00b7tern", "in", "die", "h\u00f6chs\u00b7ten", "&#228;", "ste"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["ART", "ADJA", "VVINF", "APPR", "ART", "ADJA", "$(", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Du singst das lied der summenden gemarken               ", "tokens": ["Du", "singst", "das", "lied", "der", "sum\u00b7men\u00b7den", "ge\u00b7mar\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Das sanfte lied vor einer th&#252;r am abend               ", "tokens": ["Das", "sanf\u00b7te", "lied", "vor", "ei\u00b7ner", "th", "&#252;", "r", "am", "a\u00b7bend"], "token_info": ["word", "word", "word", "word", "word", "word", "XML_entity", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "$(", "FM.la", "FM.la", "FM.la"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und lehrest dulden wie die einfach starken               ", "tokens": ["Und", "leh\u00b7rest", "dul\u00b7den", "wie", "die", "ein\u00b7fach", "star\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "VVINF", "KOKOM", "ART", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In l&#228;cheln jede thr&#228;ne scheu begrabend:             ", "tokens": ["In", "l", "&#228;", "cheln", "je\u00b7de", "thr", "&#228;", "ne", "scheu", "be\u00b7gra\u00b7bend", ":"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word", "word", "word", "punct"], "pos": ["APPR", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Die v\u00f6gel fliehen vor den herben schlehen               ", "tokens": ["Die", "v\u00f6\u00b7gel", "flie\u00b7hen", "vor", "den", "her\u00b7ben", "schle\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "APPR", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die falter bergen sich in sturmestoben               ", "tokens": ["Die", "fal\u00b7ter", "ber\u00b7gen", "sich", "in", "stur\u00b7me\u00b7sto\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PRF", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie funkeln wieder auf so er verstoben &#8212;               ", "tokens": ["Sie", "fun\u00b7keln", "wie\u00b7der", "auf", "so", "er", "ver\u00b7sto\u00b7ben", "&#8212;"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und wer hat jemals blumen weinen sehen?             ", "tokens": ["Und", "wer", "hat", "je\u00b7mals", "blu\u00b7men", "wei\u00b7nen", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VAFIN", "ADV", "VVINF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}