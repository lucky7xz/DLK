{"textgrid.poem.25269": {"metadata": {"author": {"name": "Wille, Bruno", "birth": "N.A.", "death": "N.A."}, "title": "1L: Hoch stand ich auf dem Dach/ und sah", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hoch stand ich auf dem Dach/ und sah", "tokens": ["Hoch", "stand", "ich", "auf", "dem", "Dach", "/", "und", "sah"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "$(", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seltsamste Morgenglut:", "tokens": ["Selt\u00b7sams\u00b7te", "Mor\u00b7gen\u00b7glut", ":"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Rings wogte \u00fcber die H\u00e4user hin", "tokens": ["Rings", "wog\u00b7te", "\u00fc\u00b7ber", "die", "H\u00e4u\u00b7ser", "hin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein Meer von Brand und Blut.", "tokens": ["Ein", "Meer", "von", "Brand", "und", "Blut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wild br\u00fcllte die schwarzrot qualmende Schlacht;", "tokens": ["Wild", "br\u00fcll\u00b7te", "die", "schwarz\u00b7rot", "qual\u00b7men\u00b7de", "Schlacht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "++--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Mit zornigem Knattern schossen", "tokens": ["Mit", "zor\u00b7ni\u00b7gem", "Knat\u00b7tern", "schos\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Behelmte Feinde zu uns empor.", "tokens": ["Be\u00b7helm\u00b7te", "Fein\u00b7de", "zu", "uns", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Doch es trotzten fest die Genossen,", "tokens": ["Doch", "es", "trotz\u00b7ten", "fest", "die", "Ge\u00b7nos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Wie Felsen im schlagenden Hagelsturm.", "tokens": ["Wie", "Fel\u00b7sen", "im", "schla\u00b7gen\u00b7den", "Ha\u00b7gel\u00b7sturm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.10": {"text": "Verheerende Bomben schwangen sie", "tokens": ["Ver\u00b7hee\u00b7ren\u00b7de", "Bom\u00b7ben", "schwan\u00b7gen", "sie"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVFIN", "PPER"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.11": {"text": "Und manchmal durch das Schlachtgetos", "tokens": ["Und", "manch\u00b7mal", "durch", "das", "Schlacht\u00b7ge\u00b7tos"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Die Marseillaise fangen sie.", "tokens": ["Die", "Mar\u00b7seil\u00b7laise", "fan\u00b7gen", "sie", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.13": {"text": "Ihr wollustgirrendes Mordlied pfiff", "tokens": ["Ihr", "wol\u00b7lust\u00b7gir\u00b7ren\u00b7des", "Mord\u00b7lied", "pfiff"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.14": {"text": "Eine Kugel an meinem Ohr;", "tokens": ["Ei\u00b7ne", "Ku\u00b7gel", "an", "mei\u00b7nem", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.15": {"text": "Da b\u00e4umte sich meine Seele j\u00e4h", "tokens": ["Da", "b\u00e4um\u00b7te", "sich", "mei\u00b7ne", "See\u00b7le", "j\u00e4h"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PRF", "PPOSAT", "NN", "ADJD"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Gleich w\u00fctiger Schlange empor.", "tokens": ["Gleich", "w\u00fc\u00b7ti\u00b7ger", "Schlan\u00b7ge", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.17": {"text": "Den Sprengball z\u00fcckte die krallende Faust", "tokens": ["Den", "Spreng\u00b7ball", "z\u00fcck\u00b7te", "die", "kral\u00b7len\u00b7de", "Faust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.18": {"text": "Nach den feindlich st\u00fcrmenden Massen", "tokens": ["Nach", "den", "feind\u00b7lich", "st\u00fcr\u00b7men\u00b7den", "Mas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.19": {"text": "Und schmi\u00df des Todes rei\u00dfende Saat", "tokens": ["Und", "schmi\u00df", "des", "To\u00b7des", "rei\u00b7\u00dfen\u00b7de", "Saat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.20": {"text": "Hinunter mit jauchzendem Hassen.", "tokens": ["Hin\u00b7un\u00b7ter", "mit", "jauch\u00b7zen\u00b7dem", "Has\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.21": {"text": "Und dumpf ...", "tokens": ["Und", "dumpf", "..."], "token_info": ["word", "word", "punct"], "pos": ["KON", "ADJD", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "...Ein Rollen, ein Peitschengeklatsch", "tokens": ["...", "Ein", "Rol\u00b7len", ",", "ein", "Peit\u00b7schen\u00b7ge\u00b7klatsch"], "token_info": ["punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "NN", "$,", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Und Getrappel/ goldflirrender Schein;", "tokens": ["Und", "Ge\u00b7trap\u00b7pel", "/", "gold\u00b7flir\u00b7ren\u00b7der", "Schein", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "NN", "$(", "ADJA", "NN", "$."], "meter": "--+-+---+", "measure": "anapaest.init"}, "line.3": {"text": "Und sieh, die Morgensonne strahlt", "tokens": ["Und", "sieh", ",", "die", "Mor\u00b7gen\u00b7son\u00b7ne", "strahlt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zum offenen Fenster herein.", "tokens": ["Zum", "of\u00b7fe\u00b7nen", "Fens\u00b7ter", "her\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Im Bette lieg ich/ es war ein Traum!", "tokens": ["Im", "Bet\u00b7te", "lieg", "ich", "/", "es", "war", "ein", "Traum", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$(", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nicht Kugeln, die Schwalben girren", "tokens": ["Nicht", "Ku\u00b7geln", ",", "die", "Schwal\u00b7ben", "gir\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und schie\u00dfen um mein l\u00e4ndliches Dach.", "tokens": ["Und", "schie\u00b7\u00dfen", "um", "mein", "l\u00e4nd\u00b7li\u00b7ches", "Dach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+---+--+", "measure": "iambic.tri.chol"}, "line.8": {"text": "Und droben im Mattblau schwirren", "tokens": ["Und", "dro\u00b7ben", "im", "Matt\u00b7blau", "schwir\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Lichtfrohe Lerchen. Durch tauige Flur", "tokens": ["Licht\u00b7fro\u00b7he", "Ler\u00b7chen", ".", "Durch", "tau\u00b7i\u00b7ge", "Flur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+--+-+---+", "measure": "iambic.tetra.invert"}, "line.10": {"text": "Trabt munter das Pferd mit dem Wagen;", "tokens": ["Trabt", "mun\u00b7ter", "das", "Pferd", "mit", "dem", "Wa\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.11": {"text": "Drauf sitzt der junge Bauer und schmaucht", "tokens": ["Drauf", "sitzt", "der", "jun\u00b7ge", "Bau\u00b7er", "und", "schmaucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Sein Pfeifchen mit Behagen", "tokens": ["Sein", "Pfei\u00b7fchen", "mit", "Be\u00b7ha\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Und f\u00e4hrt so sicher hinein in die Welt ...", "tokens": ["Und", "f\u00e4hrt", "so", "si\u00b7cher", "hin\u00b7ein", "in", "die", "Welt", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "ADV", "APPR", "ART", "NN", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich aber, ich seufze und schwanke", "tokens": ["Ich", "a\u00b7ber", ",", "ich", "seuf\u00b7ze", "und", "schwan\u00b7ke"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "$,", "PPER", "VVFIN", "KON", "VVFIN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Und bin auf bangem Lager hier", "tokens": ["Und", "bin", "auf", "ban\u00b7gem", "La\u00b7ger", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein zweifelnder Gedanke.", "tokens": ["Ein", "zwei\u00b7feln\u00b7der", "Ge\u00b7dan\u00b7ke", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Noch h\u00e4lt der Zorn, der gl\u00fchende Traum", "tokens": ["Noch", "h\u00e4lt", "der", "Zorn", ",", "der", "gl\u00fc\u00b7hen\u00b7de", "Traum"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Mein Herz in banger Stockung,", "tokens": ["Mein", "Herz", "in", "ban\u00b7ger", "Sto\u00b7ckung", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Und schon umschmeichelt mich so s\u00fc\u00df", "tokens": ["Und", "schon", "um\u00b7schmei\u00b7chelt", "mich", "so", "s\u00fc\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Lebens liebliche Lockung.", "tokens": ["Des", "Le\u00b7bens", "lieb\u00b7li\u00b7che", "Lo\u00b7ckung", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.8": {"text": "Da schwindelt mir; Verwirrung, Scham,", "tokens": ["Da", "schwin\u00b7delt", "mir", ";", "Ver\u00b7wir\u00b7rung", ",", "Scham", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Sie \u00fcberfluten hei\u00df mich;", "tokens": ["Sie", "\u00fc\u00b7berf\u00b7lu\u00b7ten", "hei\u00df", "mich", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PPER", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.10": {"text": "O ich vermessner, armer Tor!", "tokens": ["O", "ich", "ver\u00b7mess\u00b7ner", ",", "ar\u00b7mer", "Tor", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Was bin ich? Und was wei\u00df ich?", "tokens": ["Was", "bin", "ich", "?", "Und", "was", "wei\u00df", "ich", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$.", "KON", "PWS", "VVFIN", "PPER", "$."], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Ich bin nur ein Halm im wogenden Feld", "tokens": ["Ich", "bin", "nur", "ein", "Halm", "im", "wo\u00b7gen\u00b7den", "Feld"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "APPRART", "ADJA", "NN"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Und w\u00e4hnte, ich sei das Feld;", "tokens": ["Und", "w\u00e4hn\u00b7te", ",", "ich", "sei", "das", "Feld", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PPER", "VAFIN", "ART", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.14": {"text": "Und ich wanke, schwanke in Lieb und Ha\u00df,", "tokens": ["Und", "ich", "wan\u00b7ke", ",", "schwan\u00b7ke", "in", "Lieb", "und", "Ha\u00df", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "--+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "Und mir d\u00e4ucht, ich bewege die Welt.", "tokens": ["Und", "mir", "d\u00e4ucht", ",", "ich", "be\u00b7we\u00b7ge", "die", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "$,", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.16": {"text": "O ich Irrtum und schw\u00e4chlicher Widerspruch!", "tokens": ["O", "ich", "Irr\u00b7tum", "und", "schw\u00e4ch\u00b7li\u00b7cher", "Wi\u00b7der\u00b7spruch", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Und doch! Was hier erwacht", "tokens": ["Und", "doch", "!", "Was", "hier", "er\u00b7wacht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "$.", "PWS", "ADV", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "So grimm und k\u00fchn, ist Irrtum nicht,", "tokens": ["So", "grimm", "und", "k\u00fchn", ",", "ist", "Irr\u00b7tum", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "$,", "VAFIN", "NE", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist Zwietracht nicht, ist Macht.", "tokens": ["Ist", "Zwiet\u00b7racht", "nicht", ",", "ist", "Macht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "NN", "PTKNEG", "$,", "VAFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ich bin die einige Macht, bin Lieb", "tokens": ["Ich", "bin", "die", "ei\u00b7ni\u00b7ge", "Macht", ",", "bin", "Lieb"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "PIAT", "NN", "$,", "VAFIN", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Und Ha\u00df mit einem Male,", "tokens": ["Und", "Ha\u00df", "mit", "ei\u00b7nem", "Ma\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "So einig wie Kastanienfrucht und ihre Stachelschale.", "tokens": ["So", "ei\u00b7nig", "wi