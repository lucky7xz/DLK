{"textgrid.poem.54736": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "50. Uber seine Beschreibung des Freyst\u00e4dtischen Brandes", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Umb da ich deine Glutt und letzte Noht beschrieben/", "tokens": ["Umb", "da", "ich", "dei\u00b7ne", "Glutt", "und", "letz\u00b7te", "Noht", "be\u00b7schrie\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "KOUS", "PPER", "PPOSAT", "NN", "KON", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Freystadt/ und wie du seyst in dem Feur gebliben/", "tokens": ["O", "Freys\u00b7tadt", "/", "und", "wie", "du", "seyst", "in", "dem", "Feur", "ge\u00b7bli\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$(", "KON", "PWAV", "PPER", "VAFIN", "APPR", "ART", "NN", "VVPP", "$("], "meter": "--+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Drewt man mit Ha\u00df und Hohn ich bildet' es mir ein", "tokens": ["Drewt", "man", "mit", "Ha\u00df", "und", "Hohn", "ich", "bil\u00b7det'", "es", "mir", "ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "APPR", "NN", "KON", "NN", "PPER", "VVFIN", "PPER", "PPER", "ART"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "Bald Anfangs/ da\u00df es mir nicht w\u00fcrde freye seyn/", "tokens": ["Bald", "An\u00b7fangs", "/", "da\u00df", "es", "mir", "nicht", "w\u00fcr\u00b7de", "frey\u00b7e", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["