{"textgrid.poem.42844": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Nach kurzer Fahrt getrennt", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es reimt sich was,", "tokens": ["Es", "reimt", "sich", "was", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "PIS", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und es schleimt sich was,", "tokens": ["Und", "es", "schleimt", "sich", "was", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "PIS", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "In den Austern im K\u00f6lner September.", "tokens": ["In", "den", "Aus\u00b7tern", "im", "K\u00f6l\u00b7ner", "Sep\u00b7tem\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "NN", "NN", "$."], "meter": "--+--+-+--", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ich sitze \u2013 und niemand sonst ist dabei \u2013", "tokens": ["Ich", "sit\u00b7ze", "\u2013", "und", "nie\u00b7mand", "sonst", "ist", "da\u00b7bei", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KON", "PIS", "ADV", "VAFIN", "PAV", "$("], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Vor blinkenden Lichtern in der Bastei,", "tokens": ["Vor", "blin\u00b7ken\u00b7den", "Lich\u00b7tern", "in", "der", "Bas\u00b7tei", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "And I remember.", "tokens": ["And", "I", "re\u00b7mem\u00b7ber", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Heute wird nicht gegeizt,", "tokens": ["Heu\u00b7te", "wird", "nicht", "ge\u00b7geizt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PTKNEG", "VVPP", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Wird mit Champagner geheizt,", "tokens": ["Wird", "mit", "Cham\u00b7pag\u00b7ner", "ge\u00b7heizt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "F\u00fcr dich s\u00f6ffe ich Tinte.", "tokens": ["F\u00fcr", "dich", "s\u00f6f\u00b7fe", "ich", "Tin\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Paris ist nicht weit von hier.", "tokens": ["Pa\u00b7ris", "ist", "nicht", "weit", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADJD", "APPR", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "K\u00f6nnten wir! \u2013 Wollen wir", "tokens": ["K\u00f6nn\u00b7ten", "wir", "!", "\u2013", "Wol\u00b7len", "wir"], "token_info": ["word", "word", "punct", "punct", "word", "word"], "pos": ["VMFIN", "PPER", "$.", "$(", "VVFIN", "PPER"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.6": {"text": "Uns dort treffen, Lobintte??", "tokens": ["Uns", "dort", "tref\u00b7fen", ",", "Lo\u00b7bint\u00b7te", "??"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Es reimt sich was,", "tokens": ["Es", "re