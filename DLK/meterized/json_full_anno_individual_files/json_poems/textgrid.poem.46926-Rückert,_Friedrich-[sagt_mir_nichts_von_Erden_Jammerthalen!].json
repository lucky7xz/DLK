{"textgrid.poem.46926": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[sagt mir nichts von Erden Jammerthalen!]", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagt mir nichts von Erden Jammerthalen!", "tokens": ["Sagt", "mir", "nichts", "von", "Er\u00b7den", "Jam\u00b7mer\u00b7tha\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "APPR", "NN", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Qu\u00e4lt euch nicht, so habt ihr keine Qualen.", "tokens": ["Qu\u00e4lt", "euch", "nicht", ",", "so", "habt", "ihr", "kei\u00b7ne", "Qua\u00b7len", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "$,", "ADV", "VAFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "La\u00dft mich freu'n der Erd', auf die der Himmel", "tokens": ["La\u00dft", "mich", "freu'n", "der", "Erd'", ",", "auf", "die", "der", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "VVFIN", "ART", "NN", "$,", "APPR", "PRELS", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "T\u00e4glich ausgie\u00dft volle Freudenschalen!", "tokens": ["T\u00e4g\u00b7lich", "aus\u00b7gie\u00dft", "vol\u00b7le", "Freu\u00b7den\u00b7scha\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Soll ich klagen, da\u00df zwei Himmelsbilder", "tokens": ["Soll", "ich", "kla\u00b7gen", ",", "da\u00df", "zwei", "Him\u00b7mels\u00b7bil\u00b7der"], 