rd", "word"], "pos": ["APPRART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Noch blitzend sich der Sonnenstrahl;", "tokens": ["Noch", "blit\u00b7zend", "sich", "der", "Son\u00b7nen\u00b7strahl", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PRF", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und aus der Schlucht herab, wo Indiens Feige", "tokens": ["Und", "aus", "der", "Schlucht", "her\u00b7ab", ",", "wo", "In\u00b7di\u00b7ens", "Fei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "$,", "PWAV", "NE", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf sonnverbrannten Zackenfelsen gl\u00fcht,", "tokens": ["Auf", "sonn\u00b7ver\u00b7brann\u00b7ten", "Za\u00b7cken\u00b7fel\u00b7sen", "gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schwebt m\u00fcden Fittichs durch die Mandelzweige", "tokens": ["Schwebt", "m\u00fc\u00b7den", "Fit\u00b7tichs", "durch", "die", "Man\u00b7del\u00b7zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das W\u00fcstenkind, der hei\u00dfe S\u00fcd.", "tokens": ["Das", "W\u00fcs\u00b7ten\u00b7kind", ",", "der", "hei\u00b7\u00dfe", "S\u00fcd", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Gleich einer Sultanin, die nach dem Bade", "tokens": ["Gleich", "ei\u00b7ner", "Sul\u00b7ta\u00b7nin", ",", "die", "nach", "dem", "Ba\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Im Palmenhaine, m\u00e4rchenlauschend, liegt,", "tokens": ["Im", "Pal\u00b7men\u00b7hai\u00b7ne", ",", "m\u00e4r\u00b7chen\u00b7lau\u00b7schend", ",", "liegt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPRART", "NN", "$,", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ruht wollustvoll Palermo am Gestade,", "tokens": ["Ruht", "wol\u00b7lust\u00b7voll", "Pa\u00b7ler\u00b7mo", "am", "Ge\u00b7sta\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VMFIN", "NE", "APPRART", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Vom Wellenschlag in Traum gewiegt.", "tokens": ["Vom", "Wel\u00b7len\u00b7schlag", "in", "Traum", "ge\u00b7wiegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch nachts, so sagt man, oft geht durch die Wogen", "tokens": ["Doch", "nachts", ",", "so", "sagt", "man", ",", "oft", "geht", "durch", "die", "Wo\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "$,", "ADV", "VVFIN", "PIS", "$,", "ADV", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein dumpfes Murmeln; sch\u00e4umend wallt die Flut;", "tokens": ["Ein", "dum\u00b7pfes", "Mur\u00b7meln", ";", "sch\u00e4u\u00b7mend", "wallt", "die", "Flut", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schwarz t\u00fcrmen Wolken sich am Himmelsbogen,", "tokens": ["Schwarz", "t\u00fcr\u00b7men", "Wol\u00b7ken", "sich", "am", "Him\u00b7mels\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "PRF", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Durchflammt von roter Nordscheinglut.", "tokens": ["Durch\u00b7flammt", "von", "ro\u00b7ter", "Nord\u00b7schein\u00b7glut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und Blitze zucken; Donner rollt; Walk\u00fcren", "tokens": ["Und", "Blit\u00b7ze", "zu\u00b7cken", ";", "Don\u00b7ner", "rollt", ";", "Wal\u00b7k\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["KON", "NN", "VVINF", "$.", "NN", "VVFIN", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit goldnem Helm ziehn durch die Nacht hindurch;", "tokens": ["Mit", "gold\u00b7nem", "Helm", "ziehn", "durch", "die", "Nacht", "hin\u00b7durch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "ART", "NN", "PAV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mit Krachen \u00f6ffnen sich die eh'rnen Th\u00fcren", "tokens": ["Mit", "Kra\u00b7chen", "\u00f6ff\u00b7nen", "sich", "die", "eh'r\u00b7nen", "Th\u00fc\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "PRF", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu Odins hoher G\u00f6tterburg;", "tokens": ["Zu", "O\u00b7dins", "ho\u00b7her", "G\u00f6t\u00b7ter\u00b7burg", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und Schiffe sieht man schwanken; Waffendr\u00f6hnen", "tokens": ["Und", "Schif\u00b7fe", "sieht", "man", "schwan\u00b7ken", ";", "Waf\u00b7fen\u00b7dr\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "NN", "VVFIN", "PIS", "VVINF", "$.", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Kriegerruf, vom Sturme halb ged\u00e4mpft,", "tokens": ["Und", "Krie\u00b7ger\u00b7ruf", ",", "vom", "Stur\u00b7me", "halb", "ge\u00b7d\u00e4mpft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "APPRART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hallt auf dem Meer, wo mit den W\u00fcstens\u00f6hnen", "tokens": ["Hallt", "auf", "dem", "Meer", ",", "wo", "mit", "den", "W\u00fcs\u00b7ten\u00b7s\u00f6h\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des Nordmanns Heere lang gek\u00e4mpft.", "tokens": ["Des", "Nord\u00b7manns", "Hee\u00b7re", "lang", "ge\u00b7k\u00e4mpft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Hinab vom Schlo\u00df arabischer Emire,", "tokens": ["Hin\u00b7ab", "vom", "Schlo\u00df", "a\u00b7ra\u00b7bi\u00b7scher", "E\u00b7mi\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "ADJA", "