 "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich w\u00fc\u00dfte ein f\u00fcr allemal,", "tokens": ["Ich", "w\u00fc\u00df\u00b7te", "ein", "f\u00fcr", "al\u00b7le\u00b7mal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was an der Sache ist.", "tokens": ["Was", "an", "der", "Sa\u00b7che", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Und weil mich denn kein Teufel st\u00f6rt,", "tokens": ["Und", "weil", "mich", "denn", "kein", "Teu\u00b7fel", "st\u00f6rt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So schlief ich recht gesund,", "tokens": ["So", "schlief", "ich", "recht", "ge\u00b7sund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "W\u00e4r wohlgen\u00e4hrt und hochverehrt", "tokens": ["W\u00e4r", "wohl\u00b7ge\u00b7n\u00e4hrt", "und", "hoch\u00b7ver\u00b7ehrt"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und w\u00fcrde kugelrund.", "tokens": ["Und", "w\u00fcr\u00b7de", "ku\u00b7gel\u00b7rund", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "K\u00e4m dann die b\u00f6se Fastenzeit,", "tokens": ["K\u00e