{"textgrid.poem.67180": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn aus der Ferne, da wir geschieden sind,", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn aus der Ferne, da wir geschieden sind,", "tokens": ["Wenn", "aus", "der", "Fer\u00b7ne", ",", "da", "wir", "ge\u00b7schie\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich dir noch kennbar bin, die Vergangenheit,", "tokens": ["Ich", "dir", "noch", "kenn\u00b7bar", "bin", ",", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O du Teilhaber meiner Leiden!", "tokens": ["O", "du", "Teil\u00b7ha\u00b7ber", "mei\u00b7ner", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einiges Gute bezeichnen dir kann,", "tokens": ["Ei\u00b7ni\u00b7ges", "Gu\u00b7te", "be\u00b7zeich\u00b7nen", "dir", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VMFIN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "So sage, wie erwartet die Freundin dich?", "tokens": ["So", "sa\u00b7ge", ",", "wie", "er\u00b7war\u00b7tet", "die", "Freun\u00b7din", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In jenen G\u00e4rten, da nach entsetzlicher", "tokens": ["In", "je\u00b7nen", "G\u00e4r\u00b7ten", ",", "da", "nach", "ent\u00b7setz\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "APPR", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und dunkler Zeit wir uns gefunden?", "tokens": ["Und", "dunk\u00b7ler", "Zeit", "wir", "uns", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier an den Str\u00f6men der heilgen Urwelt.", "tokens": ["Hier", "an", "den", "Str\u00f6\u00b7men", "der", "heil\u00b7gen", "Ur\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}}, "stanza.3": {"line.1": {"text": "Das mu\u00df ich sagen, einiges Gutes war", "tokens": ["Das", "mu\u00df", "ich", "sa\u00b7gen", ",", "ei\u00b7ni\u00b7ges", "Gu\u00b7tes", "war"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VMFIN", "PPER", "VVINF", "$,", "PIAT", "NN", "VAFIN"], "meter": "+--+--+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "In deinen Blicken, als in den Fernen du", "tokens": ["In", "dei\u00b7nen", "Bli\u00b7cken", ",", "als", "in", "den", "Fer\u00b7nen", "du"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KOUS", "APPR", "ART", "ADJA", "PPER"], "meter": "-+-+-++-+-+", "measure": "unknown.measure.hexa"}, "line.3": {"text": "Dich einmal fr\u00f6hlich umgesehen,", "tokens": ["Dich", "ein\u00b7mal", "fr\u00f6h\u00b7lich", "um\u00b7ge\u00b7se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Immer verschlossener Mensch, mit finstrem", "tokens": ["Im\u00b7mer", "ver\u00b7schlos\u00b7se\u00b7ner", "Mensch", ",", "mit", "fins\u00b7trem"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJA", "NN", "$,", "APPR", "ADJA"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Aussehn. Wie flossen Stunden dahin, wie still", "tokens": ["Aus\u00b7sehn", ".", "Wie", "flos\u00b7sen", "Stun\u00b7den", "da\u00b7hin", ",", "wie", "still"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVPP", "$.", "PWAV", "VVFIN", "NN", "PAV", "$,", "PWAV", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "War meine Seele \u00fcber der Wahrheit, da\u00df", "tokens": ["War", "mei\u00b7ne", "See\u00b7le", "\u00fc\u00b7ber", "der", "Wahr\u00b7heit", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,", "KOUS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Ich so getrennt gewesen w\u00e4re?", "tokens": ["Ich", "so", "ge\u00b7trennt", "ge\u00b7we\u00b7sen", "w\u00e4\u00b7re", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "VAPP", "VAFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ja! ich gestand es, ich war die deine.", "tokens": ["Ja", "!", "ich", "ge\u00b7stand", "es", ",", "ich", "war", "die", "dei\u00b7ne", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PPER", "VVFIN", "PPER", "$,", "PPER", "VAFIN", "ART", "PPOSAT", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wahrhaftig! wie du alles Bekannte mir", "tokens": ["Wahr\u00b7haf\u00b7tig", "!", "wie", "du", "al\u00b7les", "Be\u00b7kann\u00b7te", "mir"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$.", "PWAV", "PPER", "PIAT", "NN", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In mein Ged\u00e4chtnis bringen und schreiben willst,", "tokens": ["In", "mein", "Ge\u00b7d\u00e4cht\u00b7nis", "brin\u00b7gen", "und", "schrei\u00b7ben", "willst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVINF", "KON", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Mit Briefen, so ergeht es mir auch,", "tokens": ["Mit", "Brie\u00b7fen", ",", "so", "er\u00b7geht", "es", "mir", "auch", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ADV", "VVFIN", "PPER", "PPER", "ADV", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Da\u00df ich Vergangenes alles sage.", "tokens": ["Da\u00df", "ich", "Ver\u00b7gan\u00b7ge\u00b7nes", "al\u00b7les", "sa\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "PIS", "VVFIN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Wars Fr\u00fchling? war es Sommer? die Nachtigall", "tokens": ["Wars", "Fr\u00fch\u00b7ling", "?", "war", "es", "Som\u00b7mer", "?", "die", "Nach\u00b7ti\u00b7gall"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "NN", "$.", "VAFIN", "PPER", "NN", "$.", "ART", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Mit s\u00fc\u00dfem Liede lebte mit V\u00f6geln, die", "tokens": ["Mit", "s\u00fc\u00b7\u00dfem", "Lie\u00b7de", "leb\u00b7te", "mit", "V\u00f6\u00b7geln", ",", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "APPR", "NN", "$,", "PRELS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nicht ferne waren im Geb\u00fcsche", "tokens": ["Nicht", "fer\u00b7ne", "wa\u00b7ren", "im", "Ge\u00b7b\u00fc\u00b7sche"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "VAFIN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und mit Ger\u00fcchen umgaben B\u00e4um uns.", "tokens": ["Und", "mit", "Ge\u00b7r\u00fc\u00b7chen", "um\u00b7ga\u00b7ben", "B\u00e4um", "uns", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVFIN", "NN", "PPER", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Die klaren G\u00e4nge, niedres Gestr\u00e4uch und Sand,", "tokens": ["Die", "kla\u00b7ren", "G\u00e4n\u00b7ge", ",", "nie\u00b7dres", "Ge\u00b7str\u00e4uch", "und", "Sand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Auf dem wir traten, machten erfreulicher", "tokens": ["Auf", "dem", "wir", "tra\u00b7ten", ",", "mach\u00b7ten", "er\u00b7freu\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "PRELS", "PPER", "VVFIN", "$,", "VVFIN", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und lieblicher die Hyazinthe", "tokens": ["Und", "lieb\u00b7li\u00b7cher", "die", "Hy\u00b7a\u00b7zin\u00b7the"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder die Tulpe, Viole, Nelke.", "tokens": ["O\u00b7der", "die", "Tul\u00b7pe", ",", "Vi\u00b7o\u00b7le", ",", "Nel\u00b7ke", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "NN", "$,", "NE", "$."], "meter": "---+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Um W\u00e4nd und Mauern gr\u00fcnte der Efeu, gr\u00fcnt'", "tokens": ["Um", "W\u00e4nd", "und", "Mau\u00b7ern", "gr\u00fcn\u00b7te", "der", "E\u00b7feu", ",", "gr\u00fcnt'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUI", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$,", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein selig Dunkel hoher Alleen. Oft", "tokens": ["Ein", "se\u00b7lig", "Dun\u00b7kel", "ho\u00b7her", "Al\u00b7leen", ".", "Oft"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "ADJD", "ADJD", "ADJA", "NN", "$.", "NN"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Des Abends, Morgens waren dort wir,", "tokens": ["Des", "A\u00b7bends", ",", "Mor\u00b7gens", "wa\u00b7ren", "dort", "wir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "VAFIN", "ADV", "PPER", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Redeten manches und sahn uns froh an.", "tokens": ["Re\u00b7de\u00b7ten", "man\u00b7ches", "und", "sahn", "uns", "froh", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KON", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.9": {"line.1": {"text": "In meinen Armen lebte der J\u00fcngling auf,", "tokens": ["In", "mei\u00b7nen", "Ar\u00b7men", "leb\u00b7te", "der", "J\u00fcng\u00b7ling", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der, noch verlassen, aus den Gefilden kam,", "tokens": ["Der", ",", "noch", "ver\u00b7las\u00b7sen", ",", "aus", "den", "Ge\u00b7fil\u00b7den", "kam", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "ADV", "VVINF", "$,", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Die er mir wies, mit einer Schwermut,", "tokens": ["Die", "er", "mir", "wies", ",", "mit", "ei\u00b7ner", "Schwer\u00b7mut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PPER", "VVFIN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber die Namen der seltnen Orte", "tokens": ["A\u00b7ber", "die", "Na\u00b7men", "der", "selt\u00b7nen", "Or\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "ART", "ADJA", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Und alles Sch\u00f6ne hatt er behalten, das", "tokens": ["Und", "al\u00b7les", "Sch\u00f6\u00b7ne", "hatt", "er", "be\u00b7hal\u00b7ten", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "PIAT", "NN", "VAFIN", "PPER", "VVINF", "$,", "PRELS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "An seligen Gestaden, auch mir sehr wert,", "tokens": ["An", "se\u00b7li\u00b7gen", "Ge\u00b7sta\u00b7den", ",", "auch", "mir", "sehr", "wert", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADV", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Im heimatlichen Lande bl\u00fchet", "tokens": ["Im", "hei\u00b7mat\u00b7li\u00b7chen", "Lan\u00b7de", "bl\u00fc\u00b7het"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oder verborgen, aus hoher Aussicht,", "tokens": ["O\u00b7der", "ver\u00b7bor\u00b7gen", ",", "aus", "ho\u00b7her", "Aus\u00b7sicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Allwo das Meer auch einer beschauen kann,", "tokens": ["All\u00b7wo", "das", "Meer", "auch", "ei\u00b7ner", "be\u00b7schau\u00b7en", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "PIS", "VVINF", "VMFIN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Doch keiner sein will. Nehme vorlieb, und denk", "tokens": ["Doch", "kei\u00b7ner", "sein", "will", ".", "Neh\u00b7me", "vor\u00b7lieb", ",", "und", "denk"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "VAINF", "VMFIN", "$.", "NN", "VVFIN", "$,", "KON", "VVFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "An die, die noch vergn\u00fcgt ist, darum,", "tokens": ["An", "die", ",", "die", "noch", "ver\u00b7gn\u00fcgt", "ist", ",", "da\u00b7rum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "ADV", "VVPP", "VAFIN", "$,", "PAV", "$,"], "meter": "---+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Weil der entz\u00fcckende Tag uns anschien,", "tokens": ["Weil", "der", "ent\u00b7z\u00fc\u00b7cken\u00b7de", "Tag", "uns", "an\u00b7schien", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.12": {"line.1": {"text": "Der mit Gest\u00e4ndnis oder der H\u00e4nde Druck", "tokens": ["Der", "mit", "Ge\u00b7st\u00e4nd\u00b7nis", "o\u00b7der", "der", "H\u00e4n\u00b7de", "Druck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "NN", "KON", "ART", "NN", "NN"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Anhub, der uns vereinet. Ach! wehe mir!", "tokens": ["An\u00b7hub", ",", "der", "uns", "ver\u00b7ei\u00b7net", ".", "Ach", "!", "we\u00b7he", "mir", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$.", "ITJ", "$.", "VVFIN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Es waren sch\u00f6ne Tage. Aber", "tokens": ["Es", "wa\u00b7ren", "sch\u00f6\u00b7ne", "Ta\u00b7ge", ".", "A\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ADJA", "NN", "$.", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Traurige D\u00e4mmerung folgte nachher.", "tokens": ["Trau\u00b7ri\u00b7ge", "D\u00e4m\u00b7me\u00b7rung", "folg\u00b7te", "nach\u00b7her", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "ADV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.13": {"line.1": {"text": "Du seiest so allein in der sch\u00f6nen Welt,", "tokens": ["Du", "sei\u00b7est", "so", "al\u00b7lein", "in", "der", "sch\u00f6\u00b7nen", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Behauptest du mir immer, Geliebter! das", "tokens": ["Be\u00b7haup\u00b7test", "du", "mir", "im\u00b7mer", ",", "Ge\u00b7lieb\u00b7ter", "!", "das"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["VVFIN", "PPER", "PPER", "ADV", "$,", "NN", "$.", "ART"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wei\u00dft aber du nicht,", "tokens": ["Wei\u00dft", "a\u00b7ber", "du", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "PTKNEG", "$,"], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.14": {"line.1": {"text": "Wenn aus der Ferne, da wir geschieden sind,", "tokens": ["Wenn", "aus", "der", "Fer\u00b7ne", ",", "da", "wir", "ge\u00b7schie\u00b7den", "sind", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "$,", "KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ich dir noch kennbar bin, die Vergangenheit,", "tokens": ["Ich", "dir", "noch", "kenn\u00b7bar", "bin", ",", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "ADJD", "VAFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "O du Teilhaber meiner Leiden!", "tokens": ["O", "du", "Teil\u00b7ha\u00b7ber", "mei\u00b7ner", "Lei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Einiges Gute bezeichnen dir kann,", "tokens": ["Ei\u00b7ni\u00b7ges", "Gu\u00b7te", "be\u00b7zeich\u00b7nen", "dir", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "VMFIN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}}, "stanza.15": {"line.1": {"text": "So sage, wie erwartet die Freundin dich?", "tokens": ["So", "sa\u00b7ge", ",", "wie", "er\u00b7war\u00b7tet", "die", "Freun\u00b7din", "dich", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PWAV", "VVFIN", "ART", "NN", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In jenen G\u00e4rten, da nach entsetzlicher", "tokens": ["In", "je\u00b7nen", "G\u00e4r\u00b7ten", ",", "da", "nach", "ent\u00b7setz\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "KOUS", "APPR", "ADJA"], "meter": "-+-+--+-+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und dunkler Zeit wir uns gefunden?", "tokens": ["Und", "dunk\u00b7ler", "Zeit", "wir", "uns", "ge\u00b7fun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hier an den Str\u00f6men der heilgen Urwelt.", "tokens": ["Hier", "an", "den", "Str\u00f6\u00b7men", "der", "heil\u00b7gen", "Ur\u00b7welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "+--+--+---", "measure": "dactylic.tri.plus"}}, "stanza.16": {"line.1": {"text": "Das mu\u