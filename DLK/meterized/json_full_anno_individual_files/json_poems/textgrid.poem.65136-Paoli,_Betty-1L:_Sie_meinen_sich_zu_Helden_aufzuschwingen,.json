{"textgrid.poem.65136": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sie meinen sich zu Helden aufzuschwingen,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sie meinen sich zu Helden aufzuschwingen,", "tokens": ["Sie", "mei\u00b7nen", "sich", "zu", "Hel\u00b7den", "auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn sie, begl\u00fcckt von heil'ger Liebe Segen,", "tokens": ["Wenn", "sie", ",", "be\u00b7gl\u00fcckt", "von", "heil'\u00b7ger", "Lie\u00b7be", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "VVPP", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Trotz bieten des Geschickes rauhen Schl\u00e4gen,", "tokens": ["Trotz", "bie\u00b7ten", "des", "Ge\u00b7schi\u00b7ckes", "rau\u00b7hen", "Schl\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Schmerz der Erde l\u00e4chelnd niederringen.", "tokens": ["Den", "Schmerz", "der", "Er\u00b7de", "l\u00e4\u00b7chelnd", "nie\u00b7der\u00b7rin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wohl dem, der nur die Opfer hat zu bringen,", "tokens": ["Wohl", "dem", ",", "der", "nur", "die", "Op\u00b7fer", "hat", "zu", "brin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "$,", "PRELS", "ADV", "ART", "NN", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die \u00e4u\u00dfere M\u00e4chte streng ihm auferlegen!", "tokens": ["Die", "\u00e4u\u00b7\u00dfe\u00b7re", "M\u00e4ch\u00b7te", "streng", "ihm", "auf\u00b7er\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Kein Zwiespalt wird in seiner Brust sich regen", "tokens": ["Kein", "Zwies\u00b7palt", "wird", "in", "sei\u00b7ner", "Brust", "sich", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VAFIN", "APPR", "PPOSAT", "NN", "PRF", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und, Ein's mit sich, wird er die Welt bezwingen!", "tokens": ["Und", ",", "Ein's", "mit", "sich", ",", "wird", "er", "die", "Welt", "be\u00b7zwin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PIS", "APPR", "PRF", "$,", "VAFIN", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.3": {"line.1": {"text": "Der Kranz, der hell sich schlingt durch seine Haare", "tokens": ["Der", "Kranz", ",", "der", "hell", "sich", "schlingt", "durch", "sei\u00b7ne", "Haa\u00b7re"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "PRF", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zu leichten Kaufes ward er ihm beschieden.", "tokens": ["Zu", "leich\u00b7ten", "Kau\u00b7fes", "ward", "er", "ihm", "be\u00b7schie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VAFIN", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich opf're Bess'res auf dem Brandaltare:", "tokens": ["Ich", "opf'\u00b7re", "Bess'\u00b7res", "auf", "dem", "Bran\u00b7dal\u00b7ta\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Der Seele Reinheit und des Herzens Frieden,", "tokens": ["Der", "See\u00b7le", "Rein\u00b7heit", "und", "des", "Her\u00b7zens", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Den stolzen Sinn f\u00fcr's Rechte und f\u00fcr's Wahre,", "tokens": ["Den", "stol\u00b7zen", "Sinn", "f\u00fcr's", "Rech\u00b7te", "und", "f\u00fcr's", "Wah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "KON", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein Heil im Jenseits, meine Ruh' hienieden!", "tokens": ["Mein", "Heil", "im", "Jen\u00b7seits", ",", "mei\u00b7ne", "Ruh'", "hien\u00b7ie\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$,", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie meinen sich zu Helden aufzuschwingen,", "tokens": ["Sie", "mei\u00b7nen", "sich", "zu", "Hel\u00b7den", "auf\u00b7zu\u00b7schwin\u00b7gen", ","], "token_i