{"dta.poem.9482": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Vi .  \n ODE/  \n  Darinnen die ersten beyden Verse Tro-  \n ch\u00e4isch/ der 3. und 4. Jambisch/ der 5. wider Tro-  \n ch\u00e4isch/ der 6. und 7. aber Dacty-  \n lisch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nein ich mu\u00df", "tokens": ["Nein", "ich", "mu\u00df"], "token_info": ["word", "word", "word"], "pos": ["PTKANT", "PPER", "VMFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Ku\u00df \u00fcmb ku\u00df", "tokens": ["Ku\u00df", "\u00fcmb", "ku\u00df"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPR", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Vergelten nicht/", "tokens": ["Ver\u00b7gel\u00b7ten", "nicht", "/"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mein Sch\u00f6nes Licht.", "tokens": ["Mein", "Sch\u00f6\u00b7nes", "Licht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}