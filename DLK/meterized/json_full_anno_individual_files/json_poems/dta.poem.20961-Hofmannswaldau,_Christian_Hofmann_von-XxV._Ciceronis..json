{"dta.poem.20961": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "XxV.  \n Ciceronis.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.85", "af:0.14"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "In diesen kleinen Raum ward ", "tokens": ["In", "die\u00b7sen", "klei\u00b7nen", "Raum", "ward"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der das ber\u00fchmte Rom nach Willen hat bewegt.", "tokens": ["Der", "das", "be\u00b7r\u00fchm\u00b7te", "Rom", "nach", "Wil\u00b7len", "hat", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NE", "APPR", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mein Leser scheue nicht das schlechte Grab zu k\u00fcssen.", "tokens": ["Mein", "Le\u00b7ser", "scheu\u00b7e", "nicht", "das", "schlech\u00b7te", "Grab", "zu", "k\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es liegt der R\u00f6mer Mund zu deinen schlechten F\u00fcs-", "tokens": ["Es", "liegt", "der", "R\u00f6\u00b7mer", "Mund", "zu", "dei\u00b7nen", "schlech\u00b7ten", "F\u00fcs"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "APPR", "PPOSAT", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}