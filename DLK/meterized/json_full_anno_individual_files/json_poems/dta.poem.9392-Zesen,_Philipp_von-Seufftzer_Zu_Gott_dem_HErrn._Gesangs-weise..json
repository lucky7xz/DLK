{"dta.poem.9392": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "Seufftzer  \n  Zu Gott dem HErrn.  \n Gesangs-weise.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wann der Tag/ das Kind der Sonnen", "tokens": ["Wann", "der", "Tag", "/", "das", "Kind", "der", "Son\u00b7nen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$(", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "aus dem g\u00fcldnen Zimmer geht/", "tokens": ["aus", "dem", "g\u00fcld\u00b7nen", "Zim\u00b7mer", "geht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "wann die Fackel ist entbronnen", "tokens": ["wann", "die", "Fa\u00b7ckel", "ist", "ent\u00b7bron\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VAFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und das Feld entdecket steht/", "tokens": ["und", "das", "Feld", "ent\u00b7de\u00b7cket", "steht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "wann die Vogel singen", "tokens": ["wann", "die", "Vo\u00b7gel", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "und die st\u00fcgel schwingen/", "tokens": ["und", "die", "st\u00fc\u00b7gel", "schwin\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "ist mein Hertze schon bereit", "tokens": ["ist", "mein", "Hert\u00b7ze", "schon", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "VVFIN", "ADV", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich ", "tokens": ["Dich"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.9": {"text": "Meine seustzen wollen brechen", "tokens": ["Mei\u00b7ne", "seust\u00b7zen", "wol\u00b7len", "bre\u00b7chen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVINF", "VMFIN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "aus dem munde vor der zeit.", "tokens": ["aus", "dem", "mun\u00b7de", "vor", "der", "zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nach demselben mier verlanget/", "tokens": ["Nach", "dem\u00b7sel\u00b7ben", "mier", "ver\u00b7lan\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "das dem Himmel ist bewust/", "tokens": ["das", "dem", "Him\u00b7mel", "ist", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "der mit solchen sachen pranget/", "tokens": ["der", "mit", "sol\u00b7chen", "sa\u00b7chen", "pran\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "VVFIN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da nur lauter Lieb und Lust/", "tokens": ["da", "nur", 