et", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "jhre lust verbleichet", "tokens": ["jhre", "lust", "ver\u00b7blei\u00b7chet"], "token_info": ["word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Alles ist nur eitel hier;", "tokens": ["Al\u00b7les", "ist", "nur", "ei\u00b7tel", "hier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.8": {"text": "Dr\u00fcmb hab\u2019 ich mich Gott ergeben", "tokens": ["Dr\u00fcmb", "hab'", "ich", "mich", "Gott", "er\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PRF", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "und verhoffe bald zu leben", "tokens": ["und", "ver\u00b7hof\u00b7fe", "bald", "zu", "le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "in verkl\u00e4hrter Leibes-zier.", "tokens": ["in", "ver\u00b7kl\u00e4hr\u00b7ter", "Lei\u00b7bes\u00b7zier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}