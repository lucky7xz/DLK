{"dta.poem.3992": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Griechenland .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' ich dich im Schatten der Platanen,", "tokens": ["H\u00e4tt'", "ich", "dich", "im", "Schat\u00b7ten", "der", "Pla\u00b7ta\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo durch Blumen der Ilissus rann,", "tokens": ["Wo", "durch", "Blu\u00b7men", "der", "I\u00b7lis\u00b7sus", "rann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Wo die J\u00fcnglinge sich Ruhm ersannen,", "tokens": ["Wo", "die", "J\u00fcng\u00b7lin\u00b7ge", "sich", "Ruhm", "er\u00b7san\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "PRF", "NN", "VVINF", "$,"], "meter": "+-++--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Wo die Herzen Sokrates gewann,", "tokens": ["Wo", "die", "Her\u00b7zen", "Sok\u00b7ra\u00b7tes", "ge\u00b7wann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NE", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Wo Aspasia durch Myrten wallte,", "tokens": ["Wo", "As\u00b7pa\u00b7sia", "durch", "Myr\u00b7ten", "wall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wo der br\u00fcderlichen Freude Ruf", "tokens": ["Wo", "der", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Freu\u00b7de", "Ruf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "ADJA", "NN", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Aus der l\u00e4rmenden Agora schallte,", "tokens": ["Aus", "der", "l\u00e4r\u00b7men\u00b7den", "A\u00b7go\u00b7ra", "schall\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.8": {"text": "Wo mein Plato Paradiese schuf;", "tokens": ["Wo", "mein", "Pla\u00b7to", "Pa\u00b7ra\u00b7die\u00b7se", "schuf", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NE", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Wo den Fr\u00fchling Festges\u00e4nge w\u00fcrzten,", "tokens": ["Wo", "den", "Fr\u00fch\u00b7ling", "Fest\u00b7ge\u00b7s\u00e4n\u00b7ge", "w\u00fcrz\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die Fluten der Begeisterung", "tokens": ["Wo", "die", "Flu\u00b7ten", "der", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Von Minervens heil'gem Berge st\u00fcrzten \u2014", "tokens": ["Von", "Mi\u00b7ner\u00b7vens", "heil'\u00b7gem", "Ber\u00b7ge", "st\u00fcrz\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "VVFIN", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Besch\u00fctzerin zur Huldigung \u2014", "tokens": ["Der", "Be\u00b7sch\u00fct\u00b7ze\u00b7rin", "zur", "Hul\u00b7di\u00b7gung"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$("], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.5": {"text": "Wo in tausend s\u00fc\u00dfen Dichterstunden,", "tokens": ["Wo", "in", "tau\u00b7send", "s\u00fc\u00b7\u00dfen", "Dich\u00b7ter\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "CARD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Wie ein G\u00f6ttertraum, das Alter schwand.", "tokens": ["Wie", "ein", "G\u00f6t\u00b7ter\u00b7traum", ",", "das", "Al\u00b7ter", "schwand", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "H\u00e4tt' ich da, Geliebter! dich gefunden,", "tokens": ["H\u00e4tt'", "ich", "da", ",", "Ge\u00b7lieb\u00b7ter", "!", "dich", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "NN", "$.", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Wie vor Jahren dieses Herz dich fand!", "tokens": ["Wie", "vor", "Jah\u00b7ren", "die\u00b7ses", "Herz", "dich", "fand", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "PDAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Ach! wie anders h\u00e4tt' ich dich umschlungen! \u2014", "tokens": ["Ach", "!", "wie", "an\u00b7ders", "h\u00e4tt'", "ich", "dich", "um\u00b7schlun\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ITJ", "$.", "PWAV", "ADV", "VAFIN", "PPER", "PRF", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Marathons Heroen s\u00e4ngst du mir,", "tokens": ["Ma\u00b7ra\u00b7thons", "He\u00b7roen", "s\u00e4ngst", "du", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "VVFIN", "PPER", "PPER", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und die sch\u00f6nste der Begeisterungen", "tokens": ["Und", "die", "sch\u00f6ns\u00b7te", "der", "Be\u00b7geis\u00b7te\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "L\u00e4chelte vom trunknen Auge dir,", "tokens": ["L\u00e4\u00b7chel\u00b7te", "vom", "trunk\u00b7nen", "Au\u00b7ge", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "PPER", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Deine Brust verj\u00fcngten Siegsgef\u00fchle,", "tokens": ["Dei\u00b7ne", "Brust", "ver\u00b7j\u00fcng\u00b7ten", "Siegs\u00b7ge\u00b7f\u00fch\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und dein Haupt vom Lorberzweig umspielt,", "tokens": ["Und", "dein", "Haupt", "vom", "Lor\u00b7ber\u00b7zweig", "um\u00b7spielt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "F\u00fchlte nicht des Lebens dumpfe Schw\u00fcle,", "tokens": ["F\u00fchl\u00b7te", "nicht", "des", "Le\u00b7bens", "dump\u00b7fe", "Schw\u00fc\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Die so karg der Hauch der Freude k\u00fchlt.", "tokens": ["Die", "so", "karg", "der", "Hauch", "der", "Freu\u00b7de", "k\u00fchlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.4": {"line.1": {"text": "Ist der Stern der Liebe dir verschwunden?", "tokens": ["Ist", "der", "Stern", "der", "Lie\u00b7be", "dir", "ver\u00b7schwun\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der Jugend holdes Rosenlicht?", "tokens": ["Und", "der", "Ju\u00b7gend", "hol\u00b7des", "Ro\u00b7sen\u00b7licht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! umtanzt von Hellas goldnen Stunden,", "tokens": ["Ach", "!", "um\u00b7tanzt", "von", "Hel\u00b7las", "gold\u00b7nen", "Stun\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVPP", "APPR", "NE", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "F\u00fchltest du die Flucht der Jahre nicht!", "tokens": ["F\u00fchl\u00b7test", "du", "die", "Flucht", "der", "Jah\u00b7re", "nicht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Ewig, wie der Vesta Flamme, gl\u00fchte", "tokens": ["E\u00b7wig", ",", "wie", "der", "Ves\u00b7ta", "Flam\u00b7me", ",", "gl\u00fch\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJD", "$,", "PWAV", "ART", "NE", "NN", "$,", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Muth und Liebe dort in jeder Brust,", "tokens": ["Muth", "und", "Lie\u00b7be", "dort", "in", "je\u00b7der", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Wie die Frucht der Hesperiden, bl\u00fchte", "tokens": ["Wie", "die", "Frucht", "der", "Hes\u00b7pe\u00b7ri\u00b7den", ",", "bl\u00fch\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ART", "NN", "ART", "NN", "$,", "VVFIN"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Ewig dort der Jugend s\u00fc\u00dfe Lust.", "tokens": ["E\u00b7wig", "dort", "der", "Ju\u00b7gend", "s\u00fc\u00b7\u00dfe", "Lust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "H\u00e4tte doch von diesen goldnen Jahren", "tokens": ["H\u00e4t\u00b7te", "doch", "von", "die\u00b7sen", "gold\u00b7nen", "Jah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einen Theil das Schicksal dir bescheert;", "tokens": ["Ei\u00b7nen", "Theil", "das", "Schick\u00b7sal", "dir", "be\u00b7scheert", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Diese reitzenden Athener waren", "tokens": ["Die\u00b7se", "reit\u00b7zen\u00b7den", "A\u00b7the\u00b7ner", "wa\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "VAFIN"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Deines gl\u00fchenden Gesangs so werth;", "tokens": ["Dei\u00b7nes", "gl\u00fc\u00b7hen\u00b7den", "Ge\u00b7sangs", "so", "werth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Hingelehnt am frohen Saitenspiele", "tokens": ["Hin\u00b7ge\u00b7lehnt", "am", "fro\u00b7hen", "Sai\u00b7ten\u00b7spie\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Bei der s\u00fc\u00dfen Chiertraube Blut,", "tokens": ["Bei", "der", "s\u00fc\u00b7\u00dfen", "Chier\u00b7trau\u00b7be", "Blut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "H\u00e4ttest du vom st\u00fcrmischen Gew\u00fchle", "tokens": ["H\u00e4t\u00b7test", "du", "vom", "st\u00fcr\u00b7mi\u00b7schen", "Ge\u00b7w\u00fch\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Der Agora gl\u00fchend ausgeruht.", "tokens": ["Der", "A\u00b7go\u00b7ra", "gl\u00fc\u00b7hend", "aus\u00b7ge\u00b7ruht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "$."], "meter": "-++-+-+-+", "measure": "unknown.measure.penta"}}, "stanza.6": {"line.1": {"text": "Ach! es h\u00e4tt' in jenen bessern Tagen", "tokens": ["Ach", "!", "es", "h\u00e4tt'", "in", "je\u00b7nen", "bes\u00b7sern", "Ta\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "PPER", "VAFIN", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Nicht umsonst so br\u00fcderlich und gro\u00df", "tokens": ["Nicht", "um\u00b7sonst", "so", "br\u00fc\u00b7der\u00b7lich", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "ADV", "ADJD", "KON", "ADJD"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "F\u00fcr ein Volk dein liebend Herz geschlagen,", "tokens": ["F\u00fcr", "ein", "Volk", "dein", "lie\u00b7bend", "Herz", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPOSAT", "ADJD", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Dem so gern des Dankes Z\u00e4hre flo\u00df! \u2014", "tokens": ["Dem", "so", "gern", "des", "Dan\u00b7kes", "Z\u00e4h\u00b7re", "flo\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "ADV", "ADV", "ART", "ADJA", "NN", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Harre nur! sie k\u00f6mmt gewi\u00df die Stunde,", "tokens": ["Har\u00b7re", "nur", "!", "sie", "k\u00f6mmt", "ge\u00b7wi\u00df", "die", "Stun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$.", "PPER", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Die das G\u00f6ttliche vom Staube trennt!", "tokens": ["Die", "das", "G\u00f6tt\u00b7li\u00b7che", "vom", "Stau\u00b7be", "trennt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.7": {"text": "Stirb! du suchst auf diesem Erdenrunde,", "tokens": ["Stirb", "!", "du", "suchst", "auf", "die\u00b7sem", "Er\u00b7den\u00b7run\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VVFIN", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.8": {"text": "Edler Geist! umsonst dein Element.", "tokens": ["Ed\u00b7ler", "Geist", "!", "um\u00b7sonst", "dein", "E\u00b7le\u00b7ment", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Attika, die Riesin ist gefallen;", "tokens": ["At\u00b7ti\u00b7ka", ",", "die", "Rie\u00b7sin", "ist", "ge\u00b7fal\u00b7len", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wo die alten G\u00f6tters\u00f6hne ruh'n,", "tokens": ["Wo", "die", "al\u00b7ten", "G\u00f6t\u00b7ter\u00b7s\u00f6h\u00b7ne", "ruh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Im Ruin gest\u00fcrzter Marmorhallen", "tokens": ["Im", "Ru\u00b7in", "ge\u00b7st\u00fcrz\u00b7ter", "Mar\u00b7mo\u00b7rhal\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADJA", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Br\u00fctet ew'ge Todesstille nun,", "tokens": ["Br\u00fc\u00b7tet", "ew'\u00b7ge", "To\u00b7des\u00b7stil\u00b7le", "nun", ","], "token_info": ["word"