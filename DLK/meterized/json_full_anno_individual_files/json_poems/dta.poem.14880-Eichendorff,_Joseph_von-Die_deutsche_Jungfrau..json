{"dta.poem.14880": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die deutsche Jungfrau.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "\u201ejungfrau komm in die Arme mein!", "tokens": ["\u201e", "jung\u00b7frau", "komm", "in", "die", "Ar\u00b7me", "mein", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "VVFIN", "APPR", "ART", "NN", "PPOSAT", "$."], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.2": {"text": "Sollst Deines Siegers Herrin sein.", "tokens": ["Sollst", "Dei\u00b7nes", "Sie\u00b7gers", "Her\u00b7rin", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Will bau'n Dir einen Pallast sch\u00f6n,", "tokens": ["Will", "bau'n", "Dir", "ei\u00b7nen", "Pal\u00b7last", "sch\u00f6n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In pr\u00e4cht'gen Kleidern sollst Du geh'n.", "tokens": ["In", "pr\u00e4cht'\u00b7gen", "Klei\u00b7dern", "sollst", "Du", "geh'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["APPR", "ADJA", "NN", "VMFIN", "PPER", "VVFIN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}