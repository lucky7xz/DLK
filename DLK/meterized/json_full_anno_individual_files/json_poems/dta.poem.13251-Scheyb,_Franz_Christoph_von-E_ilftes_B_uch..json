{"dta.poem.13251": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, sahen wir Best\u00fcrzung, Wiederwillen,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "sa\u00b7hen", "wir", "Be\u00b7st\u00fcr\u00b7zung", ",", "Wie\u00b7der\u00b7wil\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "VVFIN", "PPER", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwundrung oder Trost den ganzen Saal erf\u00fcllen.", "tokens": ["Ver\u00b7wund\u00b7rung", "o\u00b7der", "Trost", "den", "gan\u00b7zen", "Saal", "er\u00b7f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "265Auch in der K\u00f6niginn wird sonderbar gemerckt,", "tokens": ["in", "der", "K\u00f6\u00b7ni\u00b7ginn", "wird", "son\u00b7der\u00b7bar", "ge\u00b7merckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Da\u00df ihr was heimliches die Seele r\u00fchrt und st\u00e4rckt.", "tokens": ["Da\u00df", "ihr", "was", "heim\u00b7li\u00b7ches", "die", "See\u00b7le", "r\u00fchrt", "und", "st\u00e4rckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "ADJA", "ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der Ehgemahl erweist zwar anfangs gleiche Sinnen,", "tokens": ["Der", "Eh\u00b7ge\u00b7mahl", "er\u00b7weist", "zwar", "an\u00b7fangs", "glei\u00b7che", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Doch l\u00e4\u00dft er endlich auch sein Herz dadurch gewinnen.", "tokens": ["Doch", "l\u00e4\u00dft", "er", "end\u00b7lich", "auch", "sein", "Herz", "da\u00b7durch", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "ADV", "PPOSAT", "NN", "PAV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie redt ihm in das Ohr, und deutet auf das Herz;", "tokens": ["Sie", "redt", "ihm", "in", "das", "Ohr", ",", "und", "deu\u00b7tet", "auf", "das", "Herz", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "270Hierauf bezeigt er sich theils ernsthaft, theils in Scherz.", "tokens": ["be\u00b7zeigt", "er", "sich", "theils", "ernst\u00b7haft", ",", "theils", "in", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "ADJD", "$,", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}