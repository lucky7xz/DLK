{"dta.poem.21572": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Gedankenfl\u00fcge .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Nichts kann in dieser Welt in Nichts verschwinden,", "tokens": ["Nichts", "kann", "in", "die\u00b7ser", "Welt", "in", "Nichts", "ver\u00b7schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "PDAT", "NN", "APPR", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Etwas bleibt stets was ein Etwas war,", "tokens": ["Ein", "Et\u00b7was", "bleibt", "stets", "was", "ein", "Et\u00b7was", "war", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "ADV", "PWS", "ART", "ADV", "VAFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In andrer Form nur mu\u00df sich\u2019s wiederfinden,", "tokens": ["In", "an\u00b7drer", "Form", "nur", "mu\u00df", "sich's", "wie\u00b7der\u00b7fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Aus Raum und Zeit stellt sich der Wechsel dar:", "tokens": ["Aus", "Raum", "und", "Zeit", "stellt", "sich", "der", "Wech\u00b7sel", "dar", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Die Bl\u00e4tter keimen, gr\u00fcnen und verwehen,", "tokens": ["Die", "Bl\u00e4t\u00b7ter", "kei\u00b7men", ",", "gr\u00fc\u00b7nen", "und", "ver\u00b7we\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$,", "ADJA", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Geschlechter kommen und Geschlechter gehen.", "tokens": ["Ge\u00b7schlech\u00b7ter", "kom\u00b7men", "und", "Ge\u00b7schlech\u00b7ter", "ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Eins nur beharrt in der Ver\u00e4ndrung Wogen", "tokens": ["Eins", "nur", "be\u00b7harrt", "in", "der", "Ver\u00b7\u00e4n\u00b7drung", "Wo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und baut sich fort, wenn alles steigt und f\u00e4llt.", "tokens": ["Und", "baut", "sich", "fort", ",", "wenn", "al\u00b7les", "steigt", "und", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ", "$,", "KOUS", "PIS", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es \u00fcberw\u00f6lbt mit hoch erhab\u2019nem Bogen", "tokens": ["Es", "\u00fc\u00b7berw\u00b7\u00f6lbt", "mit", "hoch", "er\u00b7hab'\u00b7nem", "Bo\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Zeitenstrom der k\u00f6rperlichen Welt:", "tokens": ["Den", "Zei\u00b7ten\u00b7strom", "der", "k\u00f6r\u00b7per\u00b7li\u00b7chen", "Welt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Das ist die Br\u00fccke, die der Geist geschlagen,", "tokens": ["Das", "ist", "die", "Br\u00fc\u00b7cke", ",", "die", "der", "Geist", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "$,", "PRELS", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Um uns vom Irdischen zu Gott zu tragen.", "tokens": ["Um", "uns", "vom", "Ir\u00b7di\u00b7schen", "zu", "Gott", "zu", "tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPRART", "NN", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das Volk des Perikles hat ausgerungen", "tokens": ["Das", "Volk", "des", "Pe\u00b7rik\u00b7les", "hat", "aus\u00b7ge\u00b7run\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Und l\u00e4ngst erblindete sein Ehrenschild,", "tokens": ["Und", "l\u00e4ngst", "er\u00b7blin\u00b7de\u00b7te", "sein", "Eh\u00b7ren\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Homers und Pindars Leier ist verklungen", "tokens": ["Ho\u00b7mers", "und", "Pin\u00b7dars", "Lei\u00b7er", "ist", "ver\u00b7klun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NN", "NN", "VAFIN", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Und ach, zermalmt des Phidias G\u00f6tterbild:", "tokens": ["Und", "ach", ",", "zer\u00b7malmt", "des", "Phi\u00b7di\u00b7as", "G\u00f6t\u00b7ter\u00b7bild", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Doch ob auch die Akropolis zerfallen,", "tokens": ["Doch", "ob", "auch", "die", "Ak\u00b7ro\u00b7po\u00b7lis", "zer\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der Geist von Hellas lebt noch in uns Allen.", "tokens": ["Der", "Geist", "von", "Hel\u00b7las", "lebt", "noch", "in", "uns", "Al\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVFIN", "ADV", "APPR", "PPER", "NE", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Denn er allein hat mit Titanenkr\u00e4ften", "tokens": ["Denn", "er", "al\u00b7lein", "hat", "mit", "Ti\u00b7ta\u00b7nen\u00b7kr\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "ADV", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In alter Zeit den festen Grund gelegt,", "tokens": ["In", "al\u00b7ter", "Zeit", "den", "fes\u00b7ten", "Grund", "ge\u00b7legt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der \u00fcber sich auf pr\u00e4cht\u2019gen S\u00e4ulensch\u00e4ften", "tokens": ["Der", "\u00fc\u00b7ber", "sich", "auf", "pr\u00e4cht'\u00b7gen", "S\u00e4u\u00b7len\u00b7sch\u00e4f\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "PRF", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der Ewigkeit gewalt\u2019gen Tempel tr\u00e4gt.", "tokens": ["Der", "E\u00b7wig\u00b7keit", "ge\u00b7walt'\u00b7gen", "Tem\u00b7pel", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Wohl sieht man Volk auf Volk dran weiter bauen,", "tokens": ["Wohl", "sieht", "man", "Volk", "auf", "Volk", "dran", "wei\u00b7ter", "bau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "NN", "APPR", "NN", "PAV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch wird ihn je die Welt vollendet schauen?", "tokens": ["Doch", "wird", "ihn", "je", "die", "Welt", "voll\u00b7en\u00b7det", "schau\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}