{"dta.poem.12066": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "86.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Krieg Aller gegen All' ist Sinn der Wissenschaft.", "tokens": ["Krieg", "Al\u00b7ler", "ge\u00b7gen", "All'", "ist", "Sinn", "der", "Wis\u00b7sen\u00b7schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PIS", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was Alles seyn will, bleibt nothwendig mangelhaft.", "tokens": ["Was", "Al\u00b7les", "seyn", "will", ",", "bleibt", "noth\u00b7wen\u00b7dig", "man\u00b7gel\u00b7haft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAINF", "VMFIN", "$,", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo jeder will die Welt mit seiner Spann' ausspannen,", "tokens": ["Wo", "je\u00b7der", "will", "die", "Welt", "mit", "sei\u00b7ner", "Spann'", "aus\u00b7span\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VMFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seiner Formeln Zwang die Kr\u00e4ft' und Geister bannen.", "tokens": ["In", "sei\u00b7ner", "For\u00b7meln", "Zwang", "die", "Kr\u00e4ft'", "und", "Geis\u00b7ter", "ban\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Wo jeder Denkherr flugs den andern st\u00f6\u00dft vom Thron;", "tokens": ["Wo", "je\u00b7der", "Denk\u00b7herr", "flugs", "den", "an\u00b7dern", "st\u00f6\u00dft", "vom", "Thron", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "NN", "ADV", "ART", "ADJA", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was er dem Vater that, erwartet er vom Sohn.", "tokens": ["Was", "er", "dem", "Va\u00b7ter", "that", ",", "er\u00b7war\u00b7tet", "er", "vom", "Sohn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "VVFIN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sie glauben alle, da\u00df sie bis zum Ende drangen,", "tokens": ["Sie", "glau\u00b7ben", "al\u00b7le", ",", "da\u00df", "sie", "bis", "zum", "En\u00b7de", "dran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und jeder folgende mu\u00df an von vorne fangen.", "tokens": ["Und", "je\u00b7der", "fol\u00b7gen\u00b7de", "mu\u00df", "an", "von", "vor\u00b7ne", "fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "ADJA", "VMFIN", "APPR", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Der alte Brei wird umger\u00fchrt im neuen Topf;", "tokens": ["Der", "al\u00b7te", "Brei", "wird", "um\u00b7ge\u00b7r\u00fchrt", "im", "neu\u00b7en", "Topf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "VVPP", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was auf den F\u00fc\u00dfen stand, das steht nun auf dem Kopf.", "tokens": ["Was", "auf", "den", "F\u00fc\u00b7\u00dfen", "stand", ",", "das", "steht", "nun", "auf", "dem", "Kopf", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "La\u00df diesem Chaos uns der Meinungen entfliehn,", "tokens": ["La\u00df", "die\u00b7sem", "Chaos", "uns", "der", "Mei\u00b7nun\u00b7gen", "ent\u00b7fliehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PDAT", "NN", "PPER", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Zur\u00fcck ins heitere Gebiet der Kunst uns ziehn.", "tokens": ["Zu\u00b7r\u00fcck", "ins", "hei\u00b7te\u00b7re", "Ge\u00b7biet", "der", "Kunst", "uns", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "ADJA", "NN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ihr Fr\u00fchlingsch\u00f6pferhauch entfaltet bunte Welten,", "tokens": ["Ihr", "Fr\u00fch\u00b7ling\u00b7sch\u00f6p\u00b7fer\u00b7hauch", "ent\u00b7fal\u00b7tet", "bun\u00b7te", "Wel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die rund und ruh'nd in sich, einander lassen gelten.", "tokens": ["Die", "rund", "und", "ruh'nd", "in", "sich", ",", "ein\u00b7an\u00b7der", "las\u00b7sen", "gel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "APPR", "PRF", "$,", "PRF", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Gleich Blumen bl\u00fchen sie, und welken Blumen-gleich,", "tokens": ["Gleich", "Blu\u00b7men", "bl\u00fc\u00b7hen", "sie", ",", "und", "wel\u00b7ken", "Blu\u00b7men\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "PPER", "$,", "KON", "PWAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Auslebend Glanz und Duft, und sterbend samenreich.", "tokens": ["Aus\u00b7le\u00b7bend", "Glanz", "und", "Duft", ",", "und", "ster\u00b7bend", "sa\u00b7men\u00b7reich", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "NN", "KON", "NN", "$,", "KON", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Was hat ein Denker denn ergr\u00fcndet und begr\u00fcndet,", "tokens": ["Was", "hat", "ein", "Den\u00b7ker", "denn", "er\u00b7gr\u00fcn\u00b7det", "und", "be\u00b7gr\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "ADV", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nicht ein Sehermund in Ahnung vorverk\u00fcndet?", "tokens": ["Das", "nicht", "ein", "Se\u00b7her\u00b7mund", "in", "Ah\u00b7nung", "vor\u00b7ver\u00b7k\u00fcn\u00b7det", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Und welches Wissen ist nicht blasengleich zerronnen,", "tokens": ["Und", "wel\u00b7ches", "Wis\u00b7sen", "ist", "nicht", "bla\u00b7sen\u00b7gleich", "zer\u00b7ron\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAT", "NN", "VAFIN", "PTKNEG", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nicht in Kunstkristall Gediegenheit gewonnen?", "tokens": ["Das", "nicht", "in", "Kunst\u00b7kris\u00b7tall", "Ge\u00b7die\u00b7gen\u00b7heit", "ge\u00b7won\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "APPR", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "O Sch\u00f6nheit, bring es doch der Schwester Weisheit bei,", "tokens": ["O", "Sch\u00f6n\u00b7heit", ",", "bring", "es", "doch", "der", "Schwes\u00b7ter", "Weis\u00b7heit", "bei", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ohne dich ein Bild sie ohn' Erscheinung sei.", "tokens": ["Da\u00df", "oh\u00b7ne", "dich", "ein", "Bild", "sie", "ohn'", "Er\u00b7schei\u00b7nung", "sei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "PRF", "ART", "NN", "PPER", "APPR", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}