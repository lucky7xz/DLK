{"textgrid.poem.48157": {"metadata": {"author": {"name": "M\u00fcller-Jahnke, Clara", "birth": "N.A.", "death": "N.A."}, "title": "1L: Liebling, la\u00df mich schlafen gehen,", "genre": "verse", "period": "N.A.", "pub_year": 1882, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Liebling, la\u00df mich schlafen gehen,", "tokens": ["Lieb\u00b7ling", ",", "la\u00df", "mich", "schla\u00b7fen", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVIMP", "PPER", "VVINF", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "la\u00df mich ruhn an deiner Seite!", "tokens": ["la\u00df", "mich", "ruhn", "an", "dei\u00b7ner", "Sei\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Du mein seliges Geschick!", "tokens": ["Du", "mein", "se\u00b7li\u00b7ges", "Ge\u00b7schick", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Hohe blasse Tr\u00e4ume stehen", "tokens": ["Ho\u00b7he", "blas\u00b7se", "Tr\u00e4u\u00b7me", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "uns zu H\u00e4upten. In die Weite,", "tokens": ["uns", "zu", "H\u00e4up\u00b7ten", ".", "In", "die", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "$.", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "in die sternenstille Weite", "tokens": ["in", "die", "ster\u00b7nen\u00b7stil\u00b7le", "Wei\u00