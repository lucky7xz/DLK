{"textgrid.poem.67520": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die Verh\u00e4ngnisse", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Verh\u00e4ngnisse weben und weben", "tokens": ["Die", "Ver\u00b7h\u00e4ng\u00b7nis\u00b7se", "we\u00b7ben", "und", "we\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "KON", "VVINF"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.2": {"text": "Unerm\u00fcdet der Sterblichen Schicksal.", "tokens": ["Un\u00b7er\u00b7m\u00fc\u00b7det", "der", "Sterb\u00b7li\u00b7chen", "Schick\u00b7sal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "NN", "NN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Aus reichem Rocken zieht", "tokens": ["Aus", "rei\u00b7chem", "Ro\u00b7cken", "zieht"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "W\u00e4hlend Klotho den vielgef\u00e4rbeten Faden,", "tokens": ["W\u00e4h\u00b7lend", "Klo\u00b7tho", "den", "viel\u00b7ge\u00b7f\u00e4r\u00b7be\u00b7ten", "Fa\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Dem Einen dunkel, dem Andern hell,", "tokens": ["Dem", "Ei\u00b7nen", "dun\u00b7kel", ",", "dem", "An\u00b7dern", "hell", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJD", "$,", "ART", "ADJA", "ADJD", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Rastlos immer. Lachesis weitet und hebt", "tokens": ["Rast\u