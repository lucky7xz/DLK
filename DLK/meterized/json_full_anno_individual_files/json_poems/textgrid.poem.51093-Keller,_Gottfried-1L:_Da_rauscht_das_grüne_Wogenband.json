": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.15": {"line.1": {"text": "O steigt zum Himmel, Lied und Wort!", "tokens": ["O", "steigt", "zum", "Him\u00b7mel", ",", "Lied", "und", "Wort", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPRART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Schwebt jubelnd ob dem tiefen Rhein!", "tokens": ["Schwebt", "ju\u00b7belnd", "ob", "dem", "tie\u00b7fen", "Rhein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "KOUS", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Hier ist ein stiller Freiheitsport,", "tokens": ["Hier", "ist", "ein", "stil\u00b7ler", "Frei\u00b7heits\u00b7port", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und hier wie dorten schweigt der Hain!\u00ab", "tokens": ["Und", "hier", "wie", "dor\u00b7ten", "schweigt", "der", "Hain", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "KOKOM", "ADV", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Da raschelt's dr\u00fcben, und der Scherg,", "tokens": ["Da", "ra\u00b7schelt's", "dr\u00fc\u00b7ben", ",", "und", "der", "Scherg", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zweif\u00e4rbig, reckt das Ohr herein \u2013", "tokens": ["Zwei\u00b7f\u00e4r\u00b7big", ",", "reckt", "das", "Ohr", "her\u00b7ein", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fliehe rasch hinan den Berg.", "tokens": ["Ich", "flie\u00b7he", "rasch", "hi\u00b7nan", "den", "Berg", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ade, du stiller Ort am Rhein!", "tokens": ["A\u00b7de", ",", "du", "stil\u00b7ler", "Ort", "am", "Rhein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "APPRART", "NE", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}}}}