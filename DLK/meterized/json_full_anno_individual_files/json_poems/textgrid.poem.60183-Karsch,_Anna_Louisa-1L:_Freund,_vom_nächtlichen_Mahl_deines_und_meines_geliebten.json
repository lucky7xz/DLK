{"textgrid.poem.60183": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Freund, vom n\u00e4chtlichen Mahl deines und meines geliebten", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freund, vom n\u00e4chtlichen Mahl deines und meines geliebten", "tokens": ["Freund", ",", "vom", "n\u00e4cht\u00b7li\u00b7chen", "Mahl", "dei\u00b7nes", "und", "mei\u00b7nes", "ge\u00b7lieb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "PPOSAT", "KON", "PPOSAT", "ADJA"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sulzers gekommen, verbiet ich der Ruh,", "tokens": ["Sul\u00b7zers", "ge\u00b7kom\u00b7men", ",", "ver\u00b7biet", "ich", "der", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df sie mich eher nicht reizt, bis ich Gedanken geschrieben;", "tokens": ["Da\u00df", "sie", "mich", "e\u00b7her", "nicht", "reizt", ",", "bis", "ich", "Ge\u00b7dan\u00b7ken", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Tausende flattern dir zu!", "tokens": ["Tau\u00b7sen\u00b7de", "flat\u00b7tern", "dir", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.2": {"line.1": {"text": "Wie von Herzen der Braut einzelne Sorgen entfliehen", "tokens": ["Wie", "von", "Her\u00b7zen", "der", "Braut", "ein\u00b7zel\u00b7ne", "Sor\u00b7gen", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu dem Geliebten ins ferne Gezelt,", "tokens": ["Zu", "dem", "Ge\u00b7lieb\u00b7ten", "ins", "fer\u00b7ne", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob sie der Krieger noch denkt? also sorg ich, ob immer", "tokens": ["Ob", "sie", "der", "Krie\u00b7ger", "noch", "denkt", "?", "al\u00b7so", "sorg", "ich", ",", "ob", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV"], "meter": "-+-+-+++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Mein Gesang dir gef\u00e4llt.", "tokens": ["Mein", "Ge\u00b7sang", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Achtzehnmahl flohe die Nacht vor dem kommenden Tage;", "tokens": ["Acht\u00b7zehn\u00b7mahl", "flo\u00b7he", "die", "Nacht", "vor", "dem", "kom\u00b7men\u00b7den", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aber noch schattigt, mit Dunkel noch voll;", "tokens": ["A\u00b7ber", "noch", "schat\u00b7tigt", ",", "mit", "Dun\u00b7kel", "noch", "voll", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie die Wolke, so schwer ist die Seele der Sapho", "tokens": ["Wie", "die", "Wol\u00b7ke", ",", "so", "schwer", "ist", "die", "See\u00b7le", "der", "Sa\u00b7pho"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "$,", "ADV", "ADJD", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "--+--+--+-+-+", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Wenn sie schwingen sich soll.", "tokens": ["Wenn", "sie", "schwin\u00b7gen", "sich", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VMFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "Du von Ph\u00f6bus Altar nahmst mit H\u00e4nden der Freundschaft", "tokens": ["Du", "von", "Ph\u00f6\u00b7bus", "Al\u00b7tar", "nahmst", "mit", "H\u00e4n\u00b7den", "der", "Freund\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "APPR", "NE", "NN", "VVFIN", "APPR", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Flammende Br\u00e4nde. Den heiligen Raub", "tokens": ["Flam\u00b7men\u00b7de", "Br\u00e4n\u00b7de", ".", "Den", "hei\u00b7li\u00b7gen", "Raub"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "$.", "ART", "ADJA", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Billigt selber der Gott; doch, dem Feuer gefolget,", "tokens": ["Bil\u00b7ligt", "sel\u00b7ber", "der", "Gott", ";", "doch", ",", "dem", "Feu\u00b7er", "ge\u00b7fol\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$.", "ADV", "$,", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Ist er ferner mir taub.", "tokens": ["Ist", "er", "fer\u00b7ner", "mir", "taub", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PPER", "ADJD", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Ist im Tempel bey dir, der, am rieselnden Bache,", "tokens": ["Ist", "im", "Tem\u00b7pel", "bey", "dir", ",", "der", ",", "am", "rie\u00b7seln\u00b7den", "Ba\u00b7che", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "APPR", "PPER", "$,", "PRELS", "$,", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Unter den B\u00e4umen von Musen bewohnt,", "tokens": ["Un\u00b7ter", "den", "B\u00e4u\u00b7men", "von", "Mu\u00b7sen", "be\u00b7wohnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Die Orcane nicht f\u00fchlt; selbst die Kinder der Franzen", "tokens": ["Die", "Or\u00b7ca\u00b7ne", "nicht", "f\u00fchlt", ";", "selbst", "die", "Kin\u00b7der", "der", "Fran\u00b7zen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVFIN", "$.", "ADV", "ART", "NN", "ART", "NN"], "meter": "-++--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Haben alles geschont;", "tokens": ["Ha\u00b7ben", "al\u00b7les", "ge\u00b7schont", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Sonst ein gieriges Volk, das in Gottes Geb\u00e4uden", "tokens": ["Sonst", "ein", "gie\u00b7ri\u00b7ges", "Volk", ",", "das", "in", "Got\u00b7tes", "Ge\u00b7b\u00e4u\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "NN", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Priester geschimpfet, Alt\u00e4re best\u00fcrmt", "tokens": ["Pries\u00b7ter", "ge\u00b7schimp\u00b7fet", ",", "Al\u00b7t\u00e4\u00b7re", "be\u00b7st\u00fcrmt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NN", "VVPP", "$,", "NN", "VVPP"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Aber heilige Furcht grif die Herzen der Krieger;", "tokens": ["A\u00b7ber", "hei\u00b7li\u00b7ge", "Furcht", "grif", "die", "Her\u00b7zen", "der", "Krie\u00b7ger", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Dichter werden beschirmt.", "tokens": ["Dich\u00b7ter", "wer\u00b7den", "be\u00b7schirmt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.7": {"line.1": {"text": "Freund, sie st\u00f6hren dich nicht, bleib und spiele die Laute!", "tokens": ["Freund", ",", "sie", "st\u00f6h\u00b7ren", "dich", "nicht", ",", "bleib", "und", "spie\u00b7le", "die", "Lau\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,", "VVFIN", "KON", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Scipio machte mit siegender Hand", "tokens": ["Sci\u00b7pio", "mach\u00b7te", "mit", "sie\u00b7gen\u00b7der", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+--+", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihren Hannibal angst, der die Wunde des linken", "tokens": ["Ih\u00b7ren", "Han\u00b7ni\u00b7bal", "angst", ",", "der", "die", "Wun\u00b7de", "des", "lin\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVPP", "$,", "PRELS", "ART", "NN", "ART", "ADJA"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Armes t\u00f6dlich empfand.", "tokens": ["Ar\u00b7mes", "t\u00f6d\u00b7lich", "emp\u00b7fand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.8": {"line.1": {"text": "Jetzt verbindet er sie, horcht nur bangem Erwarten", "tokens": ["Jetzt", "ver\u00b7bin\u00b7det", "er", "sie", ",", "horcht", "nur", "ban\u00b7gem", "Er\u00b7war\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "$,", "VVFIN", "ADV", "ADJA", "NN"], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Donnernde Worte die Ludewig schilt,", "tokens": ["Don\u00b7nern\u00b7de", "Wor\u00b7te", "die", "Lu\u00b7de\u00b7wig", "schilt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NE", "VVFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Doch nun kennet der Hof endlich, da\u00df er mit Grimme", "tokens": ["Doch", "nun", "ken\u00b7net", "der", "Hof", "end\u00b7lich", ",", "da\u00df", "er", "mit", "Grim\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN", "ADV", "$,", "KOUS", "PPER", "APPR", "NN"], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Alle G\u00f6tter erf\u00fcllt,", "tokens": ["Al\u00b7le", "G\u00f6t\u00b7ter", "er\u00b7f\u00fcllt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Flucht dem blutigen Zank, giebt den murrenden V\u00f6lkern", "tokens": ["Flucht", "dem", "blu\u00b7ti\u00b7gen", "Zank", ",", "giebt", "den", "mur\u00b7ren\u00b7den", "V\u00f6l\u00b7kern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "VVFIN", "ART", "ADJA", "NN"], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Ihres Verlangens entfernetes Ziel!", "tokens": ["Ih\u00b7res", "Ver\u00b7lan\u00b7gens", "ent\u00b7fer\u00b7ne\u00b7tes", "Ziel", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Dennoch stehen um Friedrich, wie geb\u00fcrgige Wetter,", "tokens": ["Den\u00b7noch", "ste\u00b7hen", "um", "Fried\u00b7rich", ",", "wie", "ge\u00b7b\u00fcr\u00b7gi\u00b7ge", "Wet\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NE", "$,", "PWAV", "ADJA", "NN", "$,"], "meter": "+-+--+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Seiner Feinde noch viel.", "tokens": ["Sei\u00b7ner", "Fein\u00b7de", "noch", "viel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "ADV", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.10": {"line.1": {"text": "Ihn erblickt' ich im Traum, wie die Stirne des Ph\u00f6bus", "tokens": ["Ihn", "er\u00b7blickt'", "ich", "im", "Traum", ",", "wie", "die", "Stir\u00b7ne", "des", "Ph\u00f6\u00b7bus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "ART", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Wenn er vom Wagen des Tages herab", "tokens": ["Wenn", "er", "vom", "Wa\u00b7gen", "des", "Ta\u00b7ges", "her\u00b7ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "ART", "NN", "ADV"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sieht in blumichtes Thal, also sah' ich die Stirne,", "tokens": ["Sieht", "in", "blu\u00b7mich\u00b7tes", "Thal", ",", "al\u00b7so", "sah'", "ich", "die", "Stir\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "ADV", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Welche Strahlen sie gab!", "tokens": ["Wel\u00b7che", "Strah\u00b7len", "sie", "gab", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VVFIN", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "Vor ihr flohen dahin die Phantomen der Zagheit", "tokens": ["Vor", "ihr", "flo\u00b7hen", "da\u00b7hin", "die", "Phan\u00b7to\u00b7men", "der", "Zag\u00b7heit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVFIN", "PAV", "ART", "NN", "ART", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Aus den Kriegern die unter ihm stehn!", "tokens": ["Aus", "den", "Krie\u00b7gern", "die", "un\u00b7ter", "ihm", "stehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "APPR", "PPER", "VVINF", "$."], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "Das unb\u00e4rtige Volk, wie die j\u00fcngsten Spartaner,", "tokens": ["Das", "un\u00b7b\u00e4r\u00b7ti\u00b7ge", "Volk", ",", "wie", "die", "j\u00fcng\u00b7sten", "Spar\u00b7ta\u00b7ner", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+---+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Tapferm\u00fcthig und sch\u00f6n", "tokens": ["Tap\u00b7fer\u00b7m\u00fct\u00b7hig", "und", "sch\u00f6n"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "KON", "ADJD"], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.12": {"line.1": {"text": "Ists, und f\u00fcrchtet den Tod nicht in hohlem Get\u00f6se", "tokens": ["Ists", ",", "und", "f\u00fcrch\u00b7tet", "den", "Tod", "nicht", "in", "hoh\u00b7lem", "Ge\u00b7t\u00f6\u00b7se"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "+-+--+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Feindlicher Schilde, sie rufen die Schlacht", "tokens": ["Feind\u00b7li\u00b7cher", "Schil\u00b7de", ",", "sie", "ru\u00b7fen", "die", "Schlacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Und ein festlicher Eyd f\u00fcr den gr\u00f6ssesten K\u00f6nig", "tokens": ["Und", "ein", "fest\u00b7li\u00b7cher", "Eyd", "f\u00fcr", "den", "gr\u00f6s\u00b7ses\u00b7ten", "K\u00f6\u00b7nig"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.4": {"text": "Hat sie eisern gemacht!", "tokens": ["Hat", "sie", "ei\u00b7sern", "ge\u00b7macht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.13": {"line.1": {"text": "Stimm die Saiten o Freund! hohe Sieges-Ges\u00e4nge", "tokens": ["Stimm", "die", "Sai\u00b7ten", "o", "Freund", "!", "ho\u00b7he", "Sie\u00b7ges\u00b7Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "FM", "NN", "$.", "ADJA", "NN"], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Heischet das Jauchzen; mir th\u00f6net ins Ohr", "tokens": ["Hei\u00b7schet", "das", "Jauch\u00b7zen", ";", "mir", "th\u00f6\u00b7net", "ins", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Freude niemals geh\u00f6rt, muthig will ich sie singen", "tokens": ["Freu\u00b7de", "nie\u00b7mals", "ge\u00b7h\u00f6rt", ",", "mut\u00b7hig", "will", "ich", "sie", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "$,", "ADJD", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Doch, du! singe mir vor!", "tokens": ["Doch", ",", "du", "!", "sin\u00b7ge", "mir", "vor", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.14": {"line.1": {"text": "Freund, vom n\u00e4chtlichen Mahl deines und meines geliebten", "tokens": ["Freund", ",", "vom", "n\u00e4cht\u00b7li\u00b7chen", "Mahl", "dei\u00b7nes", "und", "mei\u00b7nes", "ge\u00b7lieb\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "APPRART", "ADJA", "NN", "PPOSAT", "KON", "PPOSAT", "ADJA"], "meter": "+-+---+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Sulzers gekommen, verbiet ich der Ruh,", "tokens": ["Sul\u00b7zers", "ge\u00b7kom\u00b7men", ",", "ver\u00b7biet", "ich", "der", "Ruh", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVPP", "$,", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df sie mich eher nicht reizt, bis ich Gedanken geschrieben;", "tokens": ["Da\u00df", "sie", "mich", "e\u00b7her", "nicht", "reizt", ",", "bis", "ich", "Ge\u00b7dan\u00b7ken", "ge\u00b7schrie\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$,", "KOUS", "PPER", "NN", "VVPP", "$."], "meter": "-+-+--+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Tausende flattern dir zu!", "tokens": ["Tau\u00b7sen\u00b7de", "flat\u00b7tern", "dir", "zu", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+--+--+", "measure": "dactylic.tri"}}, "stanza.15": {"line.1": {"text": "Wie von Herzen der Braut einzelne Sorgen entfliehen", "tokens": ["Wie", "von", "Her\u00b7zen", "der", "Braut", "ein\u00b7zel\u00b7ne", "Sor\u00b7gen", "ent\u00b7flie\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "ART", "NN", "ADJA", "NN", "VVINF"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Zu dem Geliebten ins ferne Gezelt,", "tokens": ["Zu", "dem", "Ge\u00b7lieb\u00b7ten", "ins", "fer\u00b7ne", "Ge\u00b7zelt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ob sie der Krieger noch denkt? also sorg ich, ob immer", "tokens": ["Ob", "sie", "der", "Krie\u00b7ger", "noch", "denkt", "?", "al\u00b7so", "sorg", "ich", ",", "ob", "im\u00b7mer"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV"], "meter": "-+-+-+++-+-+-+", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Mein Gesang dir gef\u00e4llt.", "tokens": ["Mein", "Ge\u00b7sang", "dir", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.16": {"line.1": {"text": "Achtzehnmahl flohe die Nacht vor dem kommenden Tage;", "tokens": ["Acht\u00b7zehn\u00b7mahl", "flo\u00b7he", "die", "Nacht", "vor", "dem", "kom\u00b7men\u00b7den", "Ta\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Aber noch schattigt, mit Dunkel noch voll;", "tokens": ["A\u00b7ber", "noch", "schat\u00b7tigt", ",", "mit", "Dun\u00b7kel", "noch", "voll", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "$,", "APPR", "NN", "ADV", "ADJD", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Wie die Wolke, so schwer ist die Seele der