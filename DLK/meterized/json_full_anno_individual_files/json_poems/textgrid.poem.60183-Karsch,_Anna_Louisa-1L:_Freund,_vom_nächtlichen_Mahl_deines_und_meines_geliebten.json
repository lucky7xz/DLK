er": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.26": {"line.1": {"text": "Stimm die Saiten o Freund! hohe Sieges-Ges\u00e4nge", "tokens": ["Stimm", "die", "Sai\u00b7ten", "o", "Freund", "!", "ho\u00b7he", "Sie\u00b7ges\u00b7Ge\u00b7s\u00e4n\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "FM", "NN", "$.", "ADJA", "NN"], "meter": "+-+--++-+--+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Heischet das Jauchzen; mir th\u00f6net ins Ohr", "tokens": ["Hei\u00b7schet", "das", "Jauch\u00b7zen", ";", "mir", "th\u00f6\u00b7net", "ins", "Ohr"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$.", "PPER", "VVFIN", "APPRART", "NN"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Freude niemals geh\u00f6rt, muthig will ich sie singen", "tokens": ["Freu\u00b7de", "nie\u00b7mals", "ge\u00b7h\u00f6rt", ",", "mut\u00b7hig", "will", "ich", "sie", "sin\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "VVFIN", "$,", "ADJD", "VMFIN", "PPER", "PPER", "VVINF"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Doch, du! singe mir vor!", "tokens": ["Doch", ",", "du", "!", "sin\u00b7ge", "mir", "vor", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "$.", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}}}}