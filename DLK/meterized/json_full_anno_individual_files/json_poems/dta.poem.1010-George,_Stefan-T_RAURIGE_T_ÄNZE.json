{"dta.poem.1010": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "T RAURIGE   T \u00c4NZE", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Langsame stunden &#252;berm fluss               ", "tokens": ["Lang\u00b7sa\u00b7me", "stun\u00b7den", "&#252;", "berm", "fluss"], "token_info": ["word", "word", "XML_entity", "word", "word"], "pos": ["FM.la", "FM.la", "FM.la", "FM.la", "FM.la"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Die welle zischt wie im verdruss               ", "tokens": ["Die", "wel\u00b7le", "zischt", "wie", "im", "ver\u00b7druss"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da von dem feuchten wind gefrischt               ", "tokens": ["Da", "von", "dem", "feuch\u00b7ten", "wind", "ge\u00b7frischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein schein bald blendet bald verwischt             ", "tokens": ["Ein", "schein", "bald", "blen\u00b7det", "bald", "ver\u00b7wischt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "ADV", "VVFIN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wir standen hand in hand am strand               ", "tokens": ["Wir", "stan\u00b7den", "hand", "in", "hand", "am", "strand"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da sah sie &#228;hren in dem sand               ", "tokens": ["Da", "sah", "sie", "&#228;", "hren", "in", "dem", "sand"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$(", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sie trat hinzu und brach davon               ", "tokens": ["Sie", "trat", "hin\u00b7zu", "und", "brach", "da\u00b7von"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "KON", "VVFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und fand auf diesen tag den ton             ", "tokens": ["Und", "fand", "auf", "die\u00b7sen", "tag", "den", "ton"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "PDAT", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Beginnend klang er hell und leicht               ", "tokens": ["Be\u00b7gin\u00b7nend", "klang", "er", "hell", "und", "leicht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie von dem ziel das wir erreicht               ", "tokens": ["Wie", "von", "dem", "ziel", "das", "wir", "er\u00b7reicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "PPER", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann ward er dumpfer als sie sang               ", "tokens": ["Dann", "ward", "er", "dum\u00b7pfer", "als", "sie", "sang"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "KOKOM", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom fernen gl&#252;ck wie bang! wie lang!             ", "tokens": ["Vom", "fer\u00b7nen", "gl", "&#252;", "ck", "wie", "bang", "!", "wie", "lang", "!"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NE", "$(", "NE", "KOKOM", "ADJD", "$.", "PWAV", "ADJD", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}}}}