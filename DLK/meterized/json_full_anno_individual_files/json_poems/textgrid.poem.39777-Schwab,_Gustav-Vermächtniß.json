{"textgrid.poem.39777": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "Verm\u00e4chtni\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es kommt die Zeit, da ich nicht mehr zu sagen,", "tokens": ["Es", "kommt", "die", "Zeit", ",", "da", "ich", "nicht", "mehr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was dieses Lied euch deuten soll, vermag;", "tokens": ["Was", "die\u00b7ses", "Lied", "euch", "deu\u00b7ten", "soll", ",", "ver\u00b7mag", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PDAT", "NN", "PPER", "VVINF", "VMFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da dieser Mund auf eure Gr\u00fc\u00df' und Fragen", "tokens": ["Da", "die\u00b7ser", "Mund", "auf", "eu\u00b7re", "Gr\u00fc\u00df'", "und", "Fra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "NN", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Tief schweigen wird, und nun mein letzter Tag", "tokens": ["Tief", "schwei\u00b7gen", "wird", ",", "und", "nun", "mein", "letz\u00b7ter", "Tag"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVINF", "VAFIN", "$,", "KON", "ADV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mir ohne Sang und Lust wird n\u00e4chtlich tagen:", "tokens": ["Mir", "oh\u00b7ne", "Sang", "und", "Lust", "wird", "n\u00e4cht\u00b7lich", "ta\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "KON", "NN", "VAFIN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Drum eh' dies Leben hemmt der j\u00e4he Schlag,", "tokens": ["Drum", "eh'", "dies", "Le\u00b7ben", "hemmt", "der", "j\u00e4\u00b7he", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "KOUS", "PDS", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So lang' es noch beim Frohen bleibt und Alten,", "tokens": ["So", "lang'", "es", "noch", "beim", "Fro\u00b7hen", "bleibt", "und", "Al\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "APPRART", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "H\u00f6rt, wie ich's ewig wissen will gehalten.", "tokens": ["H\u00f6rt", ",", "wie", "ich's", "e\u00b7wig", "wis\u00b7sen", "will", "ge\u00b7hal\u00b7ten", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll ich der Erste sein, der von euch scheidet,", "tokens": ["Soll", "ich", "der", "Ers\u00b7te", "sein", ",", "der", "von", "euch", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "VAINF", "$,", "PRELS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sollt ihr mich starr und stille liegen sehn,", "tokens": ["Sollt", "ihr", "mich", "starr", "und", "stil\u00b7le", "lie\u00b7gen", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJD", "KON", "ADJA", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So soll der Anblick, dran der Schmerz sich weidet,", "tokens": ["So", "soll", "der", "An\u00b7blick", ",", "dran", "der", "Schmerz", "sich", "wei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "$,", "PAV", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vor eurer Seele schnell vor\u00fcbergehn;", "tokens": ["Vor", "eu\u00b7rer", "See\u00b7le", "schnell", "vor\u00b7\u00fc\u00b7ber\u00b7gehn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Nie soll das Bild des Freundes, wie er leidet,", "tokens": ["Nie", "soll", "das", "Bild", "des", "Freun\u00b7des", ",", "wie", "er", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und wie er stumm im Tode mu\u00df vergehn,", "tokens": ["Und", "wie", "er", "stumm", "im", "To\u00b7de", "mu\u00df", "ver\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "ADJD", "APPRART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Sein bleiches Antlitz nie, wann ihr in Freuden", "tokens": ["Sein", "blei\u00b7ches", "Ant\u00b7litz", "nie", ",", "wann", "ihr", "in", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ADV", "$,", "PWAV", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Den Bund erneut, euch Wein und Lied verleiden.", "tokens": ["Den", "Bund", "er\u00b7neut", ",", "euch", "Wein", "und", "Lied", "ver\u00b7lei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nein! wie ihm Lust und Liebe stets gelungen,", "tokens": ["Nein", "!", "wie", "ihm", "Lust", "und", "Lie\u00b7be", "stets", "ge\u00b7lun\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PWAV", "PPER", "NN", "KON", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie er, lebendig steh'nd im Br\u00fcderkreis,", "tokens": ["Wie", "er", ",", "le\u00b7ben\u00b7dig", "steh'nd", "im", "Br\u00fc\u00b7der\u00b7kreis", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "ADJD", "ADJD", "APPRART", "NN", "$,"], "meter": "-----+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Hoch den Pokal in fester Hand geschwungen", "tokens": ["Hoch", "den", "Po\u00b7kal", "in", "fes\u00b7ter", "Hand", "ge\u00b7schwun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "NN", "APPR", "ADJA", "NN", "VVPP"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.4": {"text": "Zu der versammelten Gemeine Preis;", "tokens": ["Zu", "der", "ver\u00b7sam\u00b7mel\u00b7ten", "Ge\u00b7mei\u00b7ne", "Preis", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "-+-+---+-+", "measure": "zehnsilber"}, "line.5": {"text": "Bei sch\u00f6nen Namen festlich angeklungen,", "tokens": ["Bei", "sch\u00f6\u00b7nen", "Na\u00b7men", "fest\u00b7lich", "an\u00b7ge\u00b7klun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die Wangen gl\u00fchend und die Blicke hei\u00df;", "tokens": ["Die", "Wan\u00b7gen", "gl\u00fc\u00b7hend", "und", "die", "Bli\u00b7cke", "hei\u00df", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KON", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und mit Gesang zur br\u00fcderlichen Flechte", "tokens": ["Und", "mit", "Ge\u00b7sang", "zur", "br\u00fc\u00b7der\u00b7li\u00b7chen", "Flech\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "NN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Euch rings geboten seine deutsche Rechte:", "tokens": ["Euch", "rings", "ge\u00b7bo\u00b7ten", "sei\u00b7ne", "deut\u00b7sche", "Rech\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "So soll er Allen vor der Seele stehen,", "tokens": ["So", "soll", "er", "Al\u00b7len", "vor", "der", "See\u00b7le", "ste\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "NE", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als f\u00fchrt' er noch ein Leben unter euch,", "tokens": ["Als", "f\u00fchrt'", "er", "noch", "ein", "Le\u00b7ben", "un\u00b7ter", "euch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Als k\u00f6nntet ihr ihn h\u00f6ren noch und sehen,", "tokens": ["Als", "k\u00f6nn\u00b7tet", "ihr", "ihn", "h\u00f6\u00b7ren", "noch", "und", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "PPER", "VVFIN", "ADV", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als w\u00e4r' er froh und allen Andern gleich.", "tokens": ["Als", "w\u00e4r'", "er", "froh", "und", "al\u00b7len", "An\u00b7dern", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ADJD", "KON", "PIAT", "ADJA", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ihr m\u00fc\u00dft nicht glauben, da\u00df aus euren N\u00e4hen", "tokens": ["Ihr", "m\u00fc\u00dft", "nicht", "glau\u00b7ben", ",", "da\u00df", "aus", "eu\u00b7ren", "N\u00e4\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "KOUS", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Er lang entschwunden, fern vom Freudenreich,", "tokens": ["Er", "lang", "ent\u00b7schwun\u00b7den", ",", "fern", "vom", "Freu\u00b7den\u00b7reich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVPP", "$,", "ADJD", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nur unterm Boden, den ihr fr\u00f6hlich tretet,", "tokens": ["Nur", "un\u00b7term", "Bo\u00b7den", ",", "den", "ihr", "fr\u00f6h\u00b7lich", "tre\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Sein Lager tief und stille sich gebettet.", "tokens": ["Sein", "La\u00b7ger", "tief", "und", "stil\u00b7le", "sich", "ge\u00b7bet\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJD", "KON", "VVFIN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "So bleibe denn bei euren Bundesfesten", "tokens": ["So", "blei\u00b7be", "denn", "bei", "eu\u00b7ren", "Bun\u00b7des\u00b7fes\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Kein Sitz noch Glas zu seiner Ehre leer;", "tokens": ["Kein", "Sitz", "noch", "Glas", "zu", "sei\u00b7ner", "Eh\u00b7re", "leer", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "NN", "APPR", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Noch eine L\u00fcck' auch in dem treuen, festen,", "tokens": ["Noch", "ei\u00b7ne", "L\u00fcck", "auch", "in", "dem", "treu\u00b7en", ",", "fes\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "NN", "ADV", "APPR", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Verschlungnen Kranz der Br\u00fcderh\u00e4nde mehr.", "tokens": ["Ver\u00b7schlung\u00b7nen", "Kranz", "der", "Br\u00fc\u00b7der\u00b7h\u00e4n\u00b7de", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Denkt nur, wie er den theuren Kreis am besten", "tokens": ["Denkt", "nur", ",", "wie", "er", "den", "theu\u00b7ren", "Kreis", "am", "bes\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$,", "PWAV", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Beherrschen kann vom blauen Himmel her,", "tokens": ["Be\u00b7herr\u00b7schen", "kann", "vom", "blau\u00b7en", "Him\u00b7mel", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "APPRART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und wie er blickt auf die verbundnen Rechten,", "tokens": ["Und", "wie", "er", "blickt", "auf", "die", "ver\u00b7bund\u00b7nen", "Rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "PPER", "VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Bundesglied, aus sternenhellen N\u00e4chten.", "tokens": ["Ein", "Bun\u00b7des\u00b7glied", ",", "aus", "ster\u00b7nen\u00b7hel\u00b7len", "N\u00e4ch\u00b7ten", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Es kommt die Zeit, da ich nicht mehr zu sagen,", "tokens": ["Es", "kommt", "die", "Zeit", ",", "da", "ich", "nicht", "mehr", "zu", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "po