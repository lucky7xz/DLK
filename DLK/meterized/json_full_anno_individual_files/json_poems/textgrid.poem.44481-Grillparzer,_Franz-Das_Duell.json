{"textgrid.poem.44481": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Das Duell", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Hase und das Lamm im Streite,", "tokens": ["Der", "Ha\u00b7se", "und", "das", "Lamm", "im", "Strei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie fordern sich zum Zweikampf aus.", "tokens": ["Sie", "for\u00b7dern", "sich", "zum", "Zwei\u00b7kampf", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das Windspiel, ob geneigt gleich einer Seite,", "tokens": ["Das", "Wind\u00b7spiel", ",", "ob", "ge\u00b7neigt", "gleich", "ei\u00b7ner", "Sei\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "ADJD", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Soll Richter sein dem blutgen Strau\u00df.", "tokens": ["Soll", "Rich\u00b7ter", "sein", "dem", "blut\u00b7gen", "Strau\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "PPOSAT", "ART", "ADJA", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der Tag erscheint, der Hase sucht das Weite,", "tokens": ["Der", "Tag", "er\u00b7scheint", ",", "der", "Ha\u00b7se", "sucht", "das", "Wei\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "NN", "