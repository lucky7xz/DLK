{"dta.poem.10848": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Gott ist die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ich den grossen GOtt, mir in Ge-\ndanken bilde,", "tokens": ["Wenn", "ich", "den", "gros\u00b7sen", "Gott", ",", "mir", "in", "Ge", "dan\u00b7ken", "bil\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "PPER", "APPR", "TRUNC", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Eigenschaften seh, die unbegreif-", "tokens": ["Die", "Ei\u00b7gen\u00b7schaf\u00b7ten", "seh", ",", "die", "un\u00b7be\u00b7greif"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So wird mein Geist vertieft, und", "tokens": ["So", "wird", "mein", "Geist", "ver\u00b7tieft", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "VVPP", "$,", "KON"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich finde nichts an ihm als eine lautre Milde;", "tokens": ["Ich", "fin\u00b7de", "nichts", "an", "ihm", "als", "ei\u00b7ne", "laut\u00b7re", "Mil\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "KOUS", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Kaum wird die Denkungs-Kraft der Seele wieder", "tokens": ["Kaum", "wird", "die", "Den\u00b7kungs\u00b7Kraft", "der", "See\u00b7le", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "ART", "NN", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und hebt sich zu dem Licht der Gottheit allgemach.", "tokens": ["Und", "hebt", "sich", "zu", "dem", "Licht", "der", "Got\u00b7theit", "all\u00b7ge\u00b7mach", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So \u00f6fnet sich der Mund durch die erregten Triebe,", "tokens": ["So", "\u00f6f\u00b7net", "sich", "der", "Mund", "durch", "die", "er\u00b7reg\u00b7ten", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und spricht: Ich sp\u00fcre klar, da\u00df ", "tokens": ["Und", "spricht", ":", "Ich", "sp\u00fc\u00b7re", "klar", ",", "da\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "$.", "PPER", "VVFIN", "ADJD", "$,", "KOUS"], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}