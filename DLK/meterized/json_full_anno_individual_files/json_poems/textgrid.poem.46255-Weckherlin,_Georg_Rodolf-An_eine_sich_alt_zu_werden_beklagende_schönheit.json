{"textgrid.poem.46255": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "An eine sich alt zu werden beklagende sch\u00f6nheit", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nein! ihr seid noch nicht alt; so zart, so sch\u00f6n, so klar", "tokens": ["Nein", "!", "ihr", "seid", "noch", "nicht", "alt", ";", "so", "zart", ",", "so", "sch\u00f6n", ",", "so", "klar"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "$.", "PPER", "VAFIN", "ADV", "PTKNEG", "ADJD", "$.", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "pfleg ist stets euer fleisch, farb und auch zu erfahren,", "tokens": ["pfleg", "ist", "stets", "eu\u00b7er", "fleisch", ",", "farb", "und", "auch", "zu", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ADV", "PPOSAT", "NN", "$,", "VVFIN", "KON", "ADV", "PTKZU", "VVINF", "$,"], "meter": "+--+-+--+--+-", "measure": "iambic.penta.invert"}, "line.3": {"text": "da\u00df ihr mir billig jung; frisch, hurtig, s\u00fc\u00df ist zwar", "tokens": ["da\u00df", "ihr", "mir", "bil\u00b7lig", "jung", ";", "frisch", ",", "hur\u00b7tig", ",", "s\u00fc\u00df", "ist", "zwar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADJD", "$.", "ADJD", "$,", "ADJD", "$,", "ADJD", "VAFIN", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der glatten jugend lieb und fr\u00fcling unsrer jahren.", "tokens": ["der", "glat\u00b7ten", "ju\u00b7gend", "lieb", "und", "fr\u00fc\u00b7ling", "uns\u00b7rer", "jah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df unser sommer auch ganz liebreich sei, ist wahr;", "tokens": ["Da\u00df", "un\u00b7ser", "som\u00b7mer", "auch", "ganz", "lieb\u00b7reich", "sei", ",", "ist", "wahr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADV", "ADV", "ADV", "ADJD", "VAFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "doch ist die hitz so gro\u00df, da\u00df, sein gedrank zu sparen,", "tokens": ["doch", "ist", "die", "hitz", "so", "gro\u00df", ",", "da\u00df", ",", "sein", "ge\u00b7drank", "zu", "spa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "ADJD", "$,", "KOUS", "$,", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "ihm kaum kan m\u00f6glich sein, daher er mat und bar", "tokens": ["ihm", "kaum", "kan", "m\u00f6g\u00b7lich", "sein", ",", "da\u00b7her", "er", "mat", "und", "bar"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADJD", "VAINF", "$,", "PAV", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "durch der lieb starken brunst in tausenden gefahren.", "tokens": ["durch", "der", "lieb", "star\u00b7ken", "brunst", "in", "tau\u00b7sen\u00b7den", "ge\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "APPR", "ADJA", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wa nu, weil noch zu jung, fruchtlos des fr\u00fclings zeit,", "tokens": ["Wa", "nu", ",", "weil", "noch", "zu", "jung", ",", "frucht\u00b7los", "des", "fr\u00fc\u00b7lings", "zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "KOUS", "ADV", "PTKA", "ADJD", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "der sommer vil zu hei\u00df: ist weder zu verschweigen,", "tokens": ["der", "som\u00b7mer", "vil", "zu", "hei\u00df", ":", "ist", "we\u00b7der", "zu", "ver\u00b7schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PTKA", "ADJD", "$.", "VAFIN", "KON", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "noch zu erheben gnug des hebrsts lustreiche beut;", "tokens": ["noch", "zu", "er\u00b7he\u00b7ben", "gnug", "des", "hebrsts", "lust\u00b7rei\u00b7che", "beut", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKZU", "VVINF", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Dan er ergetzet uns mit so liebreichen feigen,", "tokens": ["Dan", "er", "er\u00b7get\u00b7zet", "uns", "mit", "so", "lieb\u00b7rei\u00b7chen", "fei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+-", "measure": "dactylic.di.plus"}, "line.13": 