{"dta.poem.19265": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "112.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Such immer was du machst, zu machen besser immer,", "tokens": ["Such", "im\u00b7mer", "was", "du", "machst", ",", "zu", "ma\u00b7chen", "bes\u00b7ser", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "PWS", "PPER", "VVFIN", "$,", "PTKZU", "VVINF", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch halte drum, was du gemacht hast, nicht f\u00fcr schlimmer.", "tokens": ["Doch", "hal\u00b7te", "drum", ",", "was", "du", "ge\u00b7macht", "hast", ",", "nicht", "f\u00fcr", "schlim\u00b7mer", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PAV", "$,", "PWS", "PPER", "VVPP", "VAFIN", "$,", "PTKNEG", "APPR", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der dunkeln Wurzel mag die lichte Bl\u00fct' entstammen,", "tokens": ["Der", "dun\u00b7keln", "Wur\u00b7zel", "mag", "die", "lich\u00b7te", "Bl\u00fct'", "ent\u00b7stam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie hat darum kein Recht die Wurzel zu verdammen.", "tokens": ["Sie", "hat", "da\u00b7rum", "kein", "Recht", "die", "Wur\u00b7zel", "zu", "ver\u00b7dam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punc