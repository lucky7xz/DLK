{"dta.poem.5320": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Gesang der V\u00f6gel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1736", "urn": "urn:nbn:de:kobv:b4-200905198582", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir die V\u00f6gel singen h\u00f6ren,", "tokens": ["Wenn", "wir", "die", "V\u00f6\u00b7gel", "sin\u00b7gen", "h\u00f6\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So la\u00dft ihr Zwitschern uns doch lehren,", "tokens": ["So", "la\u00dft", "ihr", "Zwit\u00b7schern", "uns", "doch", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPOSAT", "NN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie gro\u00df, wie w\u00fcrdig zu verehren", "tokens": ["Wie", "gro\u00df", ",", "wie", "w\u00fcr\u00b7dig", "zu", "ver\u00b7eh\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "$,", "PWAV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So ihr als unser Sch\u00f6pfer sey!", "tokens": ["So", "ihr", "als", "un\u00b7ser", "Sch\u00f6p\u00b7fer", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "KOUS", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Sie werden\u2019s uns mit Lust erkl\u00e4hren.", "tokens": ["Sie", "wer\u00b7den's", "uns", "mit", "Lust", "er\u00b7kl\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Man achtet\u2019 eh, auf ihr Geschrey", "tokens": ["Man", "ach\u00b7tet'", "eh", ",", "auf", "ihr", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KOUS", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Auf eine wunderliche Weise:", "tokens": ["Auf", "ei\u00b7ne", "wun\u00b7der\u00b7li\u00b7che", "Wei\u00b7se", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Wir aber finden, GOtt zum Preise,", "tokens": ["Wir", "a\u00b7ber", "fin\u00b7den", ",", "Gott", "zum", "Prei\u00b7se", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVINF", "$,", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "In ihrem Singen mancherley.", "tokens": ["In", "ih\u00b7rem", "Sin\u00b7gen", "man\u00b7cher\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PIAT", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jhr s\u00fc\u00df und lieblich Lust-Geth\u00f6n", "tokens": ["Ihr", "s\u00fc\u00df", "und", "lieb\u00b7lich", "Lust\u00b7Get\u00b7h\u00f6n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "KON", "ADJD", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Giebt uns gar deutlich zu verstehn,", "tokens": ["Giebt", "uns", "gar", "deut\u00b7lich", "zu", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Da\u00df sie, den Sch\u00f6pfer zu erh\u00f6hn,", "tokens": ["Da\u00df", "sie", ",", "den", "Sch\u00f6p\u00b7fer", "zu", "er\u00b7h\u00f6hn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Und uns zu gleicher Zeit zu laben,", "tokens": ["Und", "uns", "zu", "glei\u00b7cher", "Zeit", "zu", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Der hellen Stimmen Wunder-Gaben,", "tokens": ["Der", "hel\u00b7len", "Stim\u00b7men", "Wun\u00b7der\u00b7Ga\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.15": {"text": "Wir das Geh\u00f6r, empfangen haben.", "tokens": ["Wir", "das", "Ge\u00b7h\u00f6r", ",", "emp\u00b7fan\u00b7gen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Wer beide Werckzeug\u2019 recht erweget,", "tokens": ["Wer", "bei\u00b7de", "Wer\u00b7ck\u00b7zeug'", "recht", "er\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "VAFIN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Der findet eine helle Spur", "tokens": ["Der", "fin\u00b7det", "ei\u00b7ne", "hel\u00b7le", "Spur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Von Wundern, welche der Natur", "tokens": ["Von", "Wun\u00b7dern", ",", "wel\u00b7che", "der", "Na\u00b7tur"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Von einem Wesen eingepr\u00e4get,", "tokens": ["Von", "ei\u00b7nem", "We\u00b7sen", "ein\u00b7ge\u00b7pr\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Das, wie an Lieb und Macht, zugleich", "tokens": ["Das", ",", "wie", "an", "Lieb", "und", "Macht", ",", "zu\u00b7gleich"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "$,", "PWAV", "APPR", "NN", "KON", "NN", "$,", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.21": {"text": "An Weisheit \u00fcberschwenglich reich.", "tokens": ["An", "Weis\u00b7heit", "\u00fc\u00b7bersc\u00b7hweng\u00b7lich", "reich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Das allen Dingen Seyn und Leben,", "tokens": ["Das", "al\u00b7len", "Din\u00b7gen", "Seyn", "und", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "PPOSAT", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.23": {"text": "Wie uns das unsrige, gegeben,", "tokens": ["Wie", "uns", "das", "uns\u00b7ri\u00b7ge", ",", "ge\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.24": {"text": "Das, ob es alles gleich erf\u00fcllet,", "tokens": ["Das", ",", "ob", "es", "al\u00b7les", "gleich", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.25": {"text": "Sich in die Creatur verh\u00fcllet;", "tokens": ["Sich", "in", "die", "Crea\u00b7tur", "ver\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Ein herrlich Wesen, welches man,", "tokens": ["Ein", "herr\u00b7lich", "We\u00b7sen", ",", "wel\u00b7ches", "man", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,", "PRELS", "PIS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Wenn wir sie recht mit Lust beschauen,", "tokens": ["Wenn", "wir", "sie", "recht", "mit", "Lust", "be\u00b7schau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.28": {"text": "Wie einen L\u00f6wen aus den Klauen,", "tokens": ["Wie", "ei\u00b7nen", "L\u00f6\u00b7wen", "aus", "den", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.29": {"text": "Aus ihrer Sch\u00f6nheit, kennen kann!", "tokens": ["Aus", "ih\u00b7rer", "Sch\u00f6n\u00b7heit", ",", "ken\u00b7nen", "kann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}