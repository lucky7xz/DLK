{"textgrid.poem.42775": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Seemannstreue", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nafikare necesse est.", "tokens": ["Na\u00b7fi\u00b7ka\u00b7re", "ne\u00b7ces\u00b7se", "est", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Meine l\u00e4ngste Braut war Alwine.", "tokens": ["Mei\u00b7ne", "l\u00e4ngs\u00b7te", "Braut", "war", "Al\u00b7wi\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihrer blauen Augen Gelatine", "tokens": ["Ih\u00b7rer", "blau\u00b7en", "Au\u00b7gen", "Ge\u00b7la\u00b7ti\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Ist schon l\u00e4ngst zerlaufen und verwest. \u2013", "tokens": ["Ist", "schon", "l\u00e4ngst", "zer\u00b7lau\u00b7fen", "und", "ver\u00b7west", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "KON", "VVFIN", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Alwine sang so sch\u00f6n das Lied:", "tokens": ["Al\u00b7wi\u00b7ne", "sang", "so", "sch\u00f6n", "das", "Lied", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "\u00bbein J\u00e4ger aus Kurpfalz\u00ab.", "tokens": ["\u00bb", "ein", "J\u00e4\u00b7ger", "aus", "Kur\u00b7pfalz", "\u00ab", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "APPR", "NN", "$(", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Wie Passatwind stand ihr der Humor.", "tokens": ["Wie", "Pas\u00b7sat\u00b7wind", "stand", "ihr", "der", "Hu\u00b7mor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u2013 Sonntags morgens wurde sie bestattet", "tokens": ["\u2013", "Sonn\u00b7tags", "mor\u00b7gens", "wur\u00b7de", "sie", "be\u00b7stat\u00b7tet"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "ADV", "ADV", "VAFIN", "PPER", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "In der Heide, wo kein B\u00e4umchen schattet,", "tokens": ["In", "der", "Hei\u00b7de", ",", "wo", "kein", "B\u00e4um\u00b7chen", "schat\u00b7tet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PWAV", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und auch ihre Unschuld einst verlor.", "tokens": ["Und", "auch", "ih\u00b7re", "Un\u00b7schuld", "einst", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.3": {"line.1": {"text": "Donnerstags grub ich sie wieder aus.", "tokens": ["Don\u00b7ners\u00b7tags", "grub", "ich", "sie", "wie\u00b7der", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Da kamen mir schon ihre Ohrlappen", "tokens": ["Da", "ka\u00b7men", "mir", "schon", "ih\u00b7re", "Ohr\u00b7lap\u00b7pen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sonderbar vor.", "tokens": ["So", "son\u00b7der\u00b7bar", "vor", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "Freitags grub ich sie dann wieder ein.", "tokens": ["Frei\u00b7tags", "grub", "ich", "sie", "dann", "wie\u00b7der", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Niemand sah das in der stillen Heide. \u2013", "tokens": ["Nie\u00b7mand", "sah", "das", "in", "der", "stil\u00b7len", "Hei\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "VVFIN", "ART", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Montags wieder aus. Von ihrem Kleide,", "tokens": ["Mon\u00b7tags", "wie\u00b7der", "aus", ".", "Von", "ih\u00b7rem", "Klei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKVZ", "$.", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Das man ihr ins Grab gegeben hatte,", "tokens": ["Das", "man", "ihr", "ins", "Grab", "ge\u00b7ge\u00b7ben", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PIS", "PPER", "APPRART", "NN", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Schnitt ich einer Handbreit gelber Seide,", "tokens": ["Schnitt", "ich", "ei\u00b7ner", "Hand\u00b7breit", "gel\u00b7ber", "Sei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Und die tr\u00e4gt mein Bruder als Krawatte. \u2013", "tokens": ["Und", "die", "tr\u00e4gt", "mein", "Bru\u00b7der", "als", "Kra\u00b7wat\u00b7te", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ART", "VVFIN", "PPOSAT", "NN", "KOUS", "NN", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.5": {"line.1": {"text": "Gruslig wars: Bei dunklem oder feuchten", "tokens": ["Grus\u00b7lig", "wars", ":", "Bei", "dunk\u00b7lem", "o\u00b7der", "feuch\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "$.", "APPR", "ADJA", "KON", "ADJA"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Wetter fing Alwine an zu leuchten.", "tokens": ["Wet\u00b7ter", "fing", "Al\u00b7wi\u00b7ne", "an", "zu", "leuch\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NE", "APZR", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Trotzdem parallel zu ihr verweilen", "tokens": ["Trotz\u00b7dem", "par\u00b7al\u00b7lel", "zu", "ihr", "ver\u00b7wei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "APPR", "PPER", "VVINF"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Wollt ich ewiglich und immerdar.", "tokens": ["Wollt", "ich", "e\u00b7wig\u00b7lich", "und", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "KON", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Bis sie schlie\u00dflich an den weichen Teilen", "tokens": ["Bis", "sie", "schlie\u00df\u00b7lich", "an", "den", "wei\u00b7chen", "Tei\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJD", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Schon ganz anders und ganz fl\u00fcssig war.", "tokens": ["Schon", "ganz", "an\u00b7ders", "und", "ganz", "fl\u00fcs\u00b7sig", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "KON", "ADV", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.6": {"line.1": {"text": "Aus. Ein. Aus; so grub ich viele Wochen.", "tokens": ["Aus", ".", "Ein", ".", "Aus", ";", "so", "grub", "ich", "vie\u00b7le", "Wo\u00b7chen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "$.", "ART", "$.", "NN", "$.", "ADV", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Doch es hat zuletzt zu schlecht gerochen.", "tokens": ["Doch", "es", "hat", "zu\u00b7letzt", "zu", "schlecht", "ge\u00b7ro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "ADV", "PTKA", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Und die Nase wurde blauer Saft,", "tokens": ["Und", "die", "Na\u00b7se", "wur\u00b7de", "blau\u00b7er", "Saft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wodrin lange Fadenw\u00fcrmer krochen. \u2013", "tokens": ["Wod\u00b7rin", "lan\u00b7ge", "Fa\u00b7den\u00b7w\u00fcr\u00b7mer", "kro\u00b7chen", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ADV", "NN", "VVINF", "$.", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.5": {"text": "Nichts f\u00fcr ungut: Das war ekelhaft. \u2013", "tokens": ["Nichts", "f\u00fcr", "un\u00b7gut", ":", "Das", "war", "e\u00b7kel\u00b7haft", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PIS", "APPR", "ADJD", "$.", "PDS", "VAFIN", "ADJD", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.6": {"text": "Und zuletzt sind mir die schl\u00fcpfrigen Knochen", "tokens": ["Und", "zu\u00b7letzt", "sind", "mir", "die", "schl\u00fcpf\u00b7ri\u00b7gen", "Kno\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.7": {"text": "Ausgeglitten und in lauter St\u00fccke zerbrochen.", "tokens": ["Aus\u00b7ge\u00b7glit\u00b7ten", "und", "in", "lau\u00b7ter", "St\u00fc\u00b7cke", "zer\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}}, "stanza.7": {"line.1": {"text": "Und so nahm ich Abschied von die St\u00fccke.", "tokens": ["Und", "so", "nahm", "ich", "Ab\u00b7schied", "von", "die", "St\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Ging mit einem Schoner nach Iquique,", "tokens": ["Ging", "mit", "ei\u00b7nem", "Scho\u00b7ner", "nach", "I\u00b7qui\u00b7que", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "APPR", "NE", "$,"], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ohne jemals wieder ihr Gebein", "tokens": ["Oh\u00b7ne", "je\u00b7mals", "wie\u00b7der", "ihr", "Ge\u00b7bein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Auszugraben. Oder anzufassen.", "tokens": ["Aus\u00b7zu\u00b7gra\u00b7ben", ".", "O\u00b7der", "an\u00b7zu\u00b7fas\u00b7sen", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$.", "KON", "VVIZU", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.8": {"line.1": {"text": "Denn man soll die Toten schlafen lassen.", "tokens": ["Denn", "man", "soll", "die", "To\u00b7ten", "schla\u00b7fen", "las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VMFIN", "ART", "NN", "VVINF", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.9": {"line.1": {"text": "Nafikare necesse est.", "tokens": ["Na\u00b7fi\u00b7ka\u00b7re", "ne\u00b7ces\u00b7se", "est", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.la", "FM.la", "FM.la", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Meine l\u00e4ngste Braut war Alwine.", "tokens": ["Mei\u00b7ne", "l\u00e4ngs\u00b7te", "Braut", "war", "Al\u00b7wi\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "NE", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Ihrer blauen Augen Gelatine", "tokens": ["Ih\u00b7rer", "blau\u00b7en", "Au\u00b7gen", "Ge\u00b7la\u00b7ti\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "NN"], "meter": "+-+-+--+--", "measure": "trochaic.tetra.