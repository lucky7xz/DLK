{"dta.poem.1646": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Vermischte  \n Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Wahr ists/ da\u00df Sterne reitzen k\u00fcnnen/ ", "tokens": ["Wahr", "ists", "/", "da\u00df", "Ster\u00b7ne", "reit\u00b7zen", "k\u00fcn\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "$(", "KOUS", "NN", "VVINF", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Doch kan man ihnen widerstehn/", "tokens": ["Doch", "kan", "man", "ih\u00b7nen", "wi\u00b7der\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PIS", "PPER", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur da\u00df der Menschen bl\u00f6de Sinnen", "tokens": ["Nur", "da\u00df", "der", "Men\u00b7schen", "bl\u00f6\u00b7de", "Sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Gar offt auff falschem Wege gehn:", "tokens": ["Gar", "offt", "auff", "fal\u00b7schem", "We\u00b7ge", "gehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein tapffrer Mutt und kluge Geister", "tokens": ["Ein", "tapf\u00b7frer", "Mutt", "und", "klu\u00b7ge", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind ihres Gl\u00fccks und aller Sternen Meister.", "tokens": ["Sind", "ih\u00b7res", "Gl\u00fccks", "und", "al\u00b7ler", "Ster\u00b7nen", "Meis\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "KON", "PIAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Der Himmel \u00fcbet uns zum Schaden", "tokens": ["Der", "Him\u00b7mel", "\u00fc\u00b7bet", "uns", "zum", "Scha\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl keine Sinnen-Tyranney/", "tokens": ["Wohl", "kei\u00b7ne", "Sin\u00b7nen\u00b7Ty\u00b7ran\u00b7ney", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nur da\u00df der Mensch sich zu entladen", "tokens": ["Nur", "da\u00df", "der", "Mensch", "sich", "zu", "ent\u00b7la\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "ART", "NN", "PRF", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Schuld dem Gl\u00fccke misset bey.", "tokens": ["Die", "Schuld", "dem", "Gl\u00fc\u00b7cke", "mis\u00b7set", "bey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ein tapffrer Mutt und kluge Geister", "tokens": ["Ein", "tapf\u00b7frer", "Mutt", "und", "klu\u00b7ge", "Geis\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sind ihres Gl\u