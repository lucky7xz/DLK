{"textgrid.poem.41046": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Sterbeszene", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es liegt ein Kind im Todeskampf,", "tokens": ["Es", "liegt", "ein", "Kind", "im", "To\u00b7des\u00b7kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stirn bedeckt vom Schwei\u00df, dem kalten,", "tokens": ["Die", "Stirn", "be\u00b7deckt", "vom", "Schwei\u00df", ",", "dem", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es richtet auf sich wie im Krampf,", "tokens": ["Es", "rich\u00b7tet", "auf", "sich", "wie", "im", "Krampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "KOKOM", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Will noch die H\u00e4ndlein betend falten.", "tokens": ["Will", "noch", "die", "H\u00e4nd\u00b7lein", "be\u00b7tend", "fal\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Das kann's nicht mehr, die Fingerlein", "tokens": ["Das", "kann's", "nicht", "mehr", ",", "die", "Fin\u00b7ger\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VMFIN", "PTKNEG", "ADV", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sich beugen nicht, hinsinkt die H\u00fclle.", "tokens": ["Sich", "beu\u00b7gen", "nicht", ",", "hin\u00b7sinkt", "die", "H\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PRF", "VVINF", "PTKNEG", "$,", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Laut, \u2013 es streckt sich sein Gebein,", "tokens": ["Ein", "Laut", ",", "\u2013", "es", "streckt", "sich", "sein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "$(", "PPER", "VVFIN", "PRF", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Kein Atem mehr, \u2013 rings Totenstille.", "tokens": ["Kein", "A\u00b7tem", "mehr", ",", "\u2013", "rings", "To\u00b7tens\u00b7til\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "ADV", "$,", "$(", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Es liegt ein Kind im Todeskampf,", "tokens": ["Es", "liegt", "ein", "Kind", "im", "To\u00b7des\u00b7kampf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Stirn bedeckt vom Schwei\u00df, dem kalten,", "tokens": ["Die", "Stirn", "be\u00b7deckt", "vom", "Schwei\u00df", ",", "dem", "kal\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "APPRART", "NN", "$,", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es richtet auf sich wie im Krampf,", "tokens": ["Es", "rich\u00b7tet", "auf", "sich", "wie", "im", "Krampf", ","], "token_info":