{"textgrid.poem.32423": {"metadata": {"author": {"name": "Lessing, Gotthold Ephraim", "birth": "N.A.", "death": "N.A."}, "title": "102. Auf Dorinden", "genre": "verse", "period": "N.A.", "pub_year": 1755, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sagt nicht, die ihr Dorinden kennt,", "tokens": ["Sagt", "nicht", ",", "die", "ihr", "Do\u00b7rin\u00b7den", "kennt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "PRELS", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da\u00df sie aus Eitelkeit nur in die Kirchen rennt;", "tokens": ["Da\u00df", "sie", "aus", "Ei\u00b7tel\u00b7keit", "nur", "in", "die", "Kir\u00b7chen", "rennt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da\u00df sie nicht betet, und nicht h\u00f6ret,", "tokens": ["Da\u00df", "sie", "nicht", "be\u00b7tet", ",", "und", "nicht", "h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVFIN", "$,", "KON", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und andre nur im Beten st\u00f6ret.", "tokens": ["Und", "and\u00b7re", "nur", "im", "Be\u00b7ten", "st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie bat, (mein eignes Ohr ist Zeuge;", "tokens": ["Sie", "bat", ",", "(", "mein", "eig\u00b7nes", "Ohr", "ist", "Zeu\u00b7ge", ";"], "token_info": ["word", "word", "punct",