{"textgrid.poem.60209": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du S\u00e4ngerin geheimer Klagen,", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du S\u00e4ngerin geheimer Klagen,", "tokens": ["Du", "S\u00e4n\u00b7ge\u00b7rin", "ge\u00b7hei\u00b7mer", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Geliebte Nachtigall! du singst;", "tokens": ["Ge\u00b7lieb\u00b7te", "Nach\u00b7ti\u00b7gall", "!", "du", "singst", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$.", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, la\u00df dir meinen Kummer sagen,", "tokens": ["Ach", ",", "la\u00df", "dir", "mei\u00b7nen", "Kum\u00b7mer", "sa\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df du ihn in Ges\u00e4nge bringst!", "tokens": ["Da\u00df", "du", "ihn", "in", "Ge\u00b7s\u00e4n\u00b7ge", "bringst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, klage den, der mir entzogen", "tokens": ["Ach", ",", "kla\u00b7ge", "den", ",", "der", "mir", "ent\u00b7zo\u00b7gen"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "VVFIN", "ART", "$,", "PRELS", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit allen meinen Freuden ist!", "tokens": ["Mit", "al\u00b7len", "mei\u00b7nen", "Freu\u00b7den", "ist", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Liebling ist dir auch entflogen,", "tokens": ["Dein", "Lieb\u00b7ling", "ist", "dir", "auch", "ent\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Um welchen du so traurig bist!", "tokens": ["Um", "wel\u00b7chen", "du", "so", "trau\u00b7rig", "bist", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PWAT", "PPER", "ADV", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Mein Liebling, den ich siebzehn Erndten", "tokens": ["Mein", "Lieb\u00b7ling", ",", "den", "ich", "sieb\u00b7zehn", "Ernd\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "CARD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Gekannt, gew\u00fcnscht, gehofft, geliebt,", "tokens": ["Ge\u00b7kannt", ",", "ge\u00b7w\u00fcnscht", ",", "ge\u00b7hofft", ",", "ge\u00b7liebt", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ach, der ist unter den Entfernten", "tokens": ["Ach", ",", "der", "ist", "un\u00b7ter", "den", "Ent\u00b7fern\u00b7ten"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PRELS", "VAFIN", "APPR", "ART", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Da, wo Gefahr das Zelt umgiebt!", "tokens": ["Da", ",", "wo", "Ge\u00b7fahr", "das", "Zelt", "um\u00b7giebt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wo gegen\u00fcber Feinde wohnen,", "tokens": ["Wo", "ge\u00b7gen\u00b7\u00fc\u00b7ber", "Fein\u00b7de", "woh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wo der f\u00fcrchterliche Tod,", "tokens": ["Und", "wo", "der", "f\u00fcrch\u00b7ter\u00b7li\u00b7che", "Tod", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit starkem Donner der Canonen,", "tokens": ["Mit", "star\u00b7kem", "Don\u00b7ner", "der", "Ca\u00b7no\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dem Gl\u00fccke meines Lebens droht!", "tokens": ["Dem", "Gl\u00fc\u00b7cke", "mei\u00b7nes", "Le\u00b7bens", "droht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Du kluger Vogel! siehst zu weilen", "tokens": ["Du", "klu\u00b7ger", "Vo\u00b7gel", "!", "siehst", "zu", "wei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADJA", "NN", "$.", "ADV", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich traurig an, als wollt in dir", "tokens": ["Mich", "trau\u00b7rig", "an", ",", "als", "wollt", "in", "dir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "PTKVZ", "$,", "KOUS", "VMFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dein Herz den Kummer mit mir theilen;", "tokens": ["Dein", "Herz", "den", "Kum\u00b7mer", "mit", "mir", "thei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "O f\u00fchl ihn doch, und singe mir!", "tokens": ["O", "f\u00fchl", "ihn", "doch", ",", "und", "sin\u00b7ge", "mir", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "$,", "KON", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sieh, was auf meiner blassen Wange", "tokens": ["Sieh", ",", "was", "auf", "mei\u00b7ner", "blas\u00b7sen", "Wan\u00b7ge"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die Thr\u00e4ne der Empfindung spricht:", "tokens": ["Die", "Thr\u00e4\u00b7ne", "der", "Emp\u00b7fin\u00b7dung", "spricht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbso klagt im traurigsten Gesange,", "tokens": ["\u00bb", "so", "klagt", "im", "trau\u00b7rigs\u00b7ten", "Ge\u00b7san\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ein Dichter bey den Gr\u00e4bern nicht!\u00ab", "tokens": ["Ein", "Dich\u00b7ter", "bey", "den", "Gr\u00e4\u00b7bern", "nicht", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich weine nicht des Freundes Z\u00e4hre,", "tokens": ["Ich", "wei\u00b7ne", "nicht", "des", "Freun\u00b7des", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich \u00e4chze Klagen einer Braut,", "tokens": ["Ich", "\u00e4ch\u00b7ze", "Kla\u00b7gen", "ei\u00b7ner", "Braut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die, wenn ihr Freund gefallen w\u00e4re,", "tokens": ["Die", ",", "wenn", "ihr", "Freund", "ge\u00b7fal\u00b7len", "w\u00e4\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "PPOSAT", "NN", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Gr\u00e4bern ihren Schmerz vertraut.", "tokens": ["Den", "Gr\u00e4\u00b7bern", "ih\u00b7ren", "Schmerz", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Den ganzen Tag h\u00f6r' ich das Knallen", "tokens": ["Den", "gan\u00b7zen", "Tag", "h\u00f6r'", "ich", "das", "Knal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des Treffens, und mein Traum bey Nacht", "tokens": ["Des", "Tref\u00b7fens", ",", "und", "mein", "Traum", "bey", "Nacht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "PPOSAT", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zeigt mir die Menschen, wie sie fallen,", "tokens": ["Zeigt", "mir", "die", "Men\u00b7schen", ",", "wie", "sie", "fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,", "PWAV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So f\u00e4llt mein Treuster in der Schlacht!", "tokens": ["So", "f\u00e4llt", "mein", "Treus\u00b7ter", "in", "der", "Schlacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "O! da sinkt neben seiner Leiche", "tokens": ["O", "!", "da", "sinkt", "ne\u00b7ben", "sei\u00b7ner", "Lei\u00b7che"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "$.", "ADV", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Die z\u00e4rtliche verla\u00dfne Braut!", "tokens": ["Die", "z\u00e4rt\u00b7li\u00b7che", "ver\u00b7la\u00df\u00b7ne", "Braut", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbkrieg, t\u00f6dte mich mit einem Streiche!\u00ab", "tokens": ["\u00bb", "krieg", ",", "t\u00f6d\u00b7te", "mich", "mit", "ei\u00b7nem", "Strei\u00b7che", "!", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "NN", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So st\u00f6hnt ihr letzter Seufzer laut.", "tokens": ["So", "st\u00f6hnt", "ihr", "letz\u00b7ter", "Seuf\u00b7zer", "laut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Sie stirbt, doch nein, sie wacht mit Schrecken", "tokens": ["Sie", "stirbt", ",", "doch", "nein", ",", "sie", "wacht", "mit", "Schre\u00b7cken"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "ADV", "PTKANT", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom schweren Traum zu klagen auf;", "tokens": ["Vom", "schwe\u00b7ren", "Traum", "zu", "kla\u00b7gen", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gram schl\u00e4ft in ihr, Gram kommt sie wecken;", "tokens": ["Gram", "schl\u00e4ft", "in", "ihr", ",", "Gram", "kommt", "sie", "we\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "APPR", "PPER", "$,", "NN", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+++-+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "So \u00e4ngstlich ist ihr Lebenslauf!", "tokens": ["So", "\u00e4ngst\u00b7lich", "ist", "ihr", "Le\u00b7bens\u00b7lauf", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Du Vogel hilf ihr klagend singen!", "tokens": 