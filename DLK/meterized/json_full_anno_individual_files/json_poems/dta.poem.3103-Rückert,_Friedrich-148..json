{"dta.poem.3103": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "148.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu eurer Finstrelei bekehret ihr mich nicht;", "tokens": ["Zu", "eu\u00b7rer", "Fins\u00b7tre\u00b7lei", "be\u00b7keh\u00b7ret", "ihr", "mich", "nicht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "PRF", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wei\u00df, die Sch\u00f6pfung sei ein heitres Gotteslicht.", "tokens": ["Ich", "wei\u00df", ",", "die", "Sch\u00f6p\u00b7fung", "sei", "ein", "heit\u00b7res", "Got\u00b7tes\u00b7licht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dem Lichte ward gesellt ein Schatten zum Geleite,", "tokens": ["Dem", "Lich\u00b7te", "ward", "ge\u00b7sellt", "ein", "Schat\u00b7ten", "zum", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "VVPP", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ihr habt euch gestellt auf diese Schattenseite.", "tokens": ["Und", "ihr", "habt", "euch", "ge\u00b7stellt", "auf", "die\u00b7se", "Schat\u00b7ten\u00b7sei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "PPER", "VVPP", "APPR", "PDAT", "NN", "$."], "meter": "--+--+-+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Nein, selbst von der Natur seid ihr die Schattenstelle;", "tokens": ["Nein", ",", "selbst", "von", "der", "Na\u00b7tur", "seid", "ihr", "die", "Schat\u00b7ten\u00b7stel\u00b7le", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "APPR", "ART", "NN", "VAFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verzehrt euch selber nur, so geht sie auf in Helle.", "tokens": ["Ver\u00b7zehrt", "euch", "sel\u00b7ber", "nur", ",", "so", "geht", "sie", "auf", "in", "Hel\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "$,", "ADV", "VVFIN", "PPER", "APPR", "APPR", "NE", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}