{"textgrid.poem.59966": {"metadata": {"author": {"name": "St\u00e4udlin, Gotthold Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3. An Schiller", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "J\u00fcngsthin log das Ger\u00fccht! dich habe die Rechte des Todes", "tokens": ["J\u00fcng\u00b7sthin", "log", "das", "Ge\u00b7r\u00fccht", "!", "dich", "ha\u00b7be", "die", "Rech\u00b7te", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Mitten auf herrlicher Bahn niedergeworfen ins Grab \u2013", "tokens": ["Mit\u00b7ten", "auf", "herr\u00b7li\u00b7cher", "Bahn", "nie\u00b7der\u00b7ge\u00b7wor\u00b7fen", "ins", "Grab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "APPRART", "NN", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Fr\u00fche habe des Genius Flamme das schw\u00e4chere Leben", "tokens": ["Fr\u00fc\u00b7he", "ha\u00b7be", "des", "Ge\u00b7nius", "Flam\u00b7me", "das", "schw\u00e4\u00b7che\u00b7re", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Deiner H\u00fclle verzehrt und sie gewandelt in Staub!", "tokens": ["Dei\u00b7ner", "H\u00fcl\u00b7le", "ver\u00b7zehrt", "und", "sie", "ge\u00b7wan\u00b7delt", "in", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Ach da rang um den Sohn Germania weinend die H\u00e4nde,", "tokens": ["Ach", "da", "rang", "um", "den", "Sohn", "Ger\u00b7ma\u00b7nia", "wei\u00b7nend", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "ART", "NN", "NE", "VVPP", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und wehklagte: So fr\u00fch gehst du zu Lessingen schon \u2013", "tokens": ["Und", "weh\u00b7klag\u00b7te", ":", "So", "fr\u00fch", "gehst", "du", "zu", "Les\u00b7sin\u00b7gen", "schon", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "ADJD", "VVFIN", "PPER", "APPR", "NN", "ADV", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Du mein Liebling wie er, in dem ich mit Mutterentz\u00fccken", "tokens": ["Du", "mein", "Lieb\u00b7ling", "wie", "er", ",", "in", "dem", "ich", "mit", "Mut\u00b7te\u00b7rent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KOKOM", "PPER", "$,", "APPR", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Shakespeare und Hume zugleich keimen und reifen mir sah;", "tokens": ["Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "und", "Hume", "zu\u00b7gleich", "kei\u00b7men", "und", "rei\u00b7fen", "mir", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "VVINF", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+--+--+", "measure": "dactylic.tetra.plus"}, "line.9": {"text": "Des gepries'nes Verdienst als einen r\u00e4chenden Stachel", "tokens": ["Des", "ge\u00b7pries'\u00b7nes", "Ver\u00b7dienst", "als", "ei\u00b7nen", "r\u00e4\u00b7chen\u00b7den", "Sta\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Schon dem britischen Stolz stolzer entgegen ich hielt!", "tokens": ["Schon", "dem", "bri\u00b7ti\u00b7schen", "Stolz", "stol\u00b7zer", "ent\u00b7ge\u00b7gen", "ich", "hielt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Sprach's und blickte voll Schmerz auf die unvollendeten Male", "tokens": ["Sprach's", "und", "blick\u00b7te", "voll", "Schmerz", "auf", "die", "un\u00b7voll\u00b7en\u00b7de\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ADJD", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Deines Geistes \u2013 so sch\u00f6n in der Entstehung \u2013 herab!", "tokens": ["Dei\u00b7nes", "Geis\u00b7tes", "\u2013", "so", "sch\u00f6n", "in", "der", "Ent\u00b7ste\u00b7hung", "\u2013", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ADJD", "APPR", "ART", "NN", "$(", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Da ert\u00f6nte mit einmal die Kund': Es habe Genesung", "tokens": ["Da", "er\u00b7t\u00f6n\u00b7te", "mit", "ein\u00b7mal", "die", "Kund'", ":", "Es", "ha\u00b7be", "Ge\u00b7ne\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "$.", "PPER", "VAFIN", "NN"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.14": {"text": "Ihren Balsam dir sanft \u00fcber die Schl\u00e4fe getr\u00e4uft!", "tokens": ["Ih\u00b7ren", "Bal\u00b7sam", "dir", "sanft", "\u00fc\u00b7ber", "die", "Schl\u00e4\u00b7fe", "ge\u00b7tr\u00e4uft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Siehe! da jubelten wieder die Tausende, welche dich ehren,", "tokens": ["Sie\u00b7he", "!", "da", "ju\u00b7bel\u00b7ten", "wie\u00b7der", "die", "Tau\u00b7sen\u00b7de", ",", "wel\u00b7che", "dich", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Thr\u00e4nte Freude, das Weib, Schillern zu lieben so wert \u2013", "tokens": ["Thr\u00e4n\u00b7te", "Freu\u00b7de", ",", "das", "Weib", ",", "Schil\u00b7lern", "zu", "lie\u00b7ben", "so", "wert", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "ADV", "ADJD", "$("], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Und dein z\u00e4rtlicher Vater! \u2013 Mir sagte die fr\u00f6hliche Kunde", "tokens": ["Und", "dein", "z\u00e4rt\u00b7li\u00b7cher", "Va\u00b7ter", "!", "\u2013", "Mir", "sag\u00b7te", "die", "fr\u00f6h\u00b7li\u00b7che", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Sein helleuchtender, sein himmelaufstrebender Blick!", "tokens": ["Sein", "hel\u00b7leuch\u00b7ten\u00b7der", ",", "sein", "him\u00b7me\u00b7lauf\u00b7stre\u00b7ben\u00b7der", "Blick", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-++-+-+-++--+", "measure": "iambic.septa.chol"}, "line.19": {"text": "H\u00e4ttest du ihn gesehen den Blick! Er h\u00e4tte zu einem", "tokens": ["H\u00e4t\u00b7test", "du", "ihn", "ge\u00b7se\u00b7hen", "den", "Blick", "!", "Er", "h\u00e4t\u00b7te", "zu", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "ART"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Meisterwerke, wie du keines noch schufst, dich entflammt!", "tokens": ["Meis\u00b7ter\u00b7wer\u00b7ke", ",", "wie", "du", "kei\u00b7nes", "noch", "schufst", ",", "dich", "ent\u00b7flammt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PIS", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "\u00bbmeines Daseins Wonne ist mein Einziger! L\u00e4chelnd entschlaf' ich", "tokens": ["\u00bb", "mei\u00b7nes", "Da\u00b7seins", "Won\u00b7ne", "ist", "mein", "Ein\u00b7zi\u00b7ger", "!", "L\u00e4\u00b7chelnd", "ent\u00b7schlaf'", "ich"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Mit dem Gedanken an ihn,\u00ab sagt der leuchtende Blick ....", "tokens": ["Mit", "dem", "Ge\u00b7dan\u00b7ken", "an", "ihn", ",", "\u00ab", "sagt", "der", "leuch\u00b7ten\u00b7de", "Blick", "...."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "$,", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.23": {"text": "O noch lange \u2013 so rufet dein Freund vom Neckargestade \u2013", "tokens": ["O", "noch", "lan\u00b7ge", "\u2013", "so", "ru\u00b7fet", "dein", "Freund", "vom", "Ne\u00b7ckar\u00b7ge\u00b7sta\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Bleibe des Redlichen Lust! Bleibe du Sueviens Stolz,", "tokens": ["Blei\u00b7be", "des", "Red\u00b7li\u00b7chen", "Lust", "!", "Blei\u00b7be", "du", "Sue\u00b7vi\u00b7ens", "Stolz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$.", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "+--+---+-+-+-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Die den h\u00f6hnenden Schwestern entgegen die ewigen Namen:", "tokens": ["Die", "den", "h\u00f6h\u00b7nen\u00b7den", "Schwes\u00b7tern", "ent\u00b7ge\u00b7gen", "die", "e\u00b7wi\u00b7gen", "Na\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Wieland und Schiller! ruft und zum Verstummen sie zwingt!", "tokens": ["Wie\u00b7land", "und", "Schil\u00b7ler", "!", "ruft", "und", "zum", "Ver\u00b7stum\u00b7men", "sie", "zwingt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$.", "VVFIN", "KON", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.27": {"text": "Mit dem Himmelgef\u00fchl der Gesundheit in Ader und Nerve", "tokens": ["Mit", "dem", "Him\u00b7mel\u00b7ge\u00b7f\u00fchl", "der", "Ge\u00b7sund\u00b7heit", "in", "A\u00b7der", "und", "Ner\u00b7ve"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Wandle mutig du fort auf der Unsterblichkeit Bahn,", "tokens": ["Wand\u00b7le", "mu\u00b7tig", "du", "fort", "auf", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "Wunderbarer Proteus! und werd' in vollendeter Gr\u00f6\u00dfe", "tokens": ["Wun\u00b7der\u00b7ba\u00b7rer", "Pro\u00b7teus", "!", "und", "werd'", "in", "voll\u00b7en\u00b7de\u00b7ter", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Deinem gl\u00fccklichen Volk Shakespeare und Hume zumal.", "tokens": ["Dei\u00b7nem", "gl\u00fcck\u00b7li\u00b7chen", "Volk", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "und", "Hume", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "J\u00fcngsthin log das Ger\u00fccht! dich habe die Rechte des Todes", "tokens": ["J\u00fcng\u00b7sthin", "log", "das", "Ge\u00b7r\u00fccht", "!", "dich", "ha\u00b7be", "die", "Rech\u00b7te", "des", "To\u00b7des"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "PPER", "VAFIN", "ART", "NN", "ART", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Mitten auf herrlicher Bahn niedergeworfen ins Grab \u2013", "tokens": ["Mit\u00b7ten", "auf", "herr\u00b7li\u00b7cher", "Bahn", "nie\u00b7der\u00b7ge\u00b7wor\u00b7fen", "ins", "Grab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "VVPP", "APPRART", "NN", "$("], "meter": "+--+--+---+--+", "measure": "dactylic.tri.plus"}, "line.3": {"text": "Fr\u00fche habe des Genius Flamme das schw\u00e4chere Leben", "tokens": ["Fr\u00fc\u00b7he", "ha\u00b7be", "des", "Ge\u00b7nius", "Flam\u00b7me", "das", "schw\u00e4\u00b7che\u00b7re", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN", "NN", "ART", "ADJA", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.4": {"text": "Deiner H\u00fclle verzehrt und sie gewandelt in Staub!", "tokens": ["Dei\u00b7ner", "H\u00fcl\u00b7le", "ver\u00b7zehrt", "und", "sie", "ge\u00b7wan\u00b7delt", "in", "Staub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVPP", "KON", "PPER", "VVPP", "APPR", "NN", "$."], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Ach da rang um den Sohn Germania weinend die H\u00e4nde,", "tokens": ["Ach", "da", "rang", "um", "den", "Sohn", "Ger\u00b7ma\u00b7nia", "wei\u00b7nend", "die", "H\u00e4n\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "VVFIN", "APPR", "ART", "NN", "NE", "VVPP", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und wehklagte: So fr\u00fch gehst du zu Lessingen schon \u2013", "tokens": ["Und", "weh\u00b7klag\u00b7te", ":", "So", "fr\u00fch", "gehst", "du", "zu", "Les\u00b7sin\u00b7gen", "schon", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "ADJD", "VVFIN", "PPER", "APPR", "NN", "ADV", "$("], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.7": {"text": "Du mein Liebling wie er, in dem ich mit Mutterentz\u00fccken", "tokens": ["Du", "mein", "Lieb\u00b7ling", "wie", "er", ",", "in", "dem", "ich", "mit", "Mut\u00b7te\u00b7rent\u00b7z\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "PPOSAT", "NN", "KOKOM", "PPER", "$,", "APPR", "PRELS", "PPER", "APPR", "NN"], "meter": "+-+-+--+--+--+-", "measure": "hexameter"}, "line.8": {"text": "Shakespeare und Hume zugleich keimen und reifen mir sah;", "tokens": ["Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "und", "Hume", "zu\u00b7gleich", "kei\u00b7men", "und", "rei\u00b7fen", "mir", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "ADV", "VVINF", "KON", "VVFIN", "PPER", "VVFIN", "$."], "meter": "+--+--+--+--+--+", "measure": "dactylic.tetra.plus"}, "line.9": {"text": "Des gepries'nes Verdienst als einen r\u00e4chenden Stachel", "tokens": ["Des", "ge\u00b7pries'\u00b7nes", "Ver\u00b7dienst", "als", "ei\u00b7nen", "r\u00e4\u00b7chen\u00b7den", "Sta\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KOKOM", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.10": {"text": "Schon dem britischen Stolz stolzer entgegen ich hielt!", "tokens": ["Schon", "dem", "bri\u00b7ti\u00b7schen", "Stolz", "stol\u00b7zer", "ent\u00b7ge\u00b7gen", "ich", "hielt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "ADJD", "PTKVZ", "PPER", "VVFIN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Sprach's und blickte voll Schmerz auf die unvollendeten Male", "tokens": ["Sprach's", "und", "blick\u00b7te", "voll", "Schmerz", "auf", "die", "un\u00b7voll\u00b7en\u00b7de\u00b7ten", "Ma\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "VVFIN", "ADJD", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.12": {"text": "Deines Geistes \u2013 so sch\u00f6n in der Entstehung \u2013 herab!", "tokens": ["Dei\u00b7nes", "Geis\u00b7tes", "\u2013", "so", "sch\u00f6n", "in", "der", "Ent\u00b7ste\u00b7hung", "\u2013", "her\u00b7ab", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ADV", "ADJD", "APPR", "ART", "NN", "$(", "ADV", "$."], "meter": "+-+--+---+--+", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Da ert\u00f6nte mit einmal die Kund': Es habe Genesung", "tokens": ["Da", "er\u00b7t\u00f6n\u00b7te", "mit", "ein\u00b7mal", "die", "Kund'", ":", "Es", "ha\u00b7be", "Ge\u00b7ne\u00b7sung"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADV", "ART", "NN", "$.", "PPER", "VAFIN", "NN"], "meter": "+-+-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.14": {"text": "Ihren Balsam dir sanft \u00fcber die Schl\u00e4fe getr\u00e4uft!", "tokens": ["Ih\u00b7ren", "Bal\u00b7sam", "dir", "sanft", "\u00fc\u00b7ber", "die", "Schl\u00e4\u00b7fe", "ge\u00b7tr\u00e4uft", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPER", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Siehe! da jubelten wieder die Tausende, welche dich ehren,", "tokens": ["Sie\u00b7he", "!", "da", "ju\u00b7bel\u00b7ten", "wie\u00b7der", "die", "Tau\u00b7sen\u00b7de", ",", "wel\u00b7che", "dich", "eh\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "ADV", "VVFIN", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+--+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Thr\u00e4nte Freude, das Weib, Schillern zu lieben so wert \u2013", "tokens": ["Thr\u00e4n\u00b7te", "Freu\u00b7de", ",", "das", "Weib", ",", "Schil\u00b7lern", "zu", "lie\u00b7ben", "so", "wert", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ART", "NN", "$,", "NN", "PTKZU", "VVINF", "ADV", "ADJD", "$("], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.17": {"text": "Und dein z\u00e4rtlicher Vater! \u2013 Mir sagte die fr\u00f6hliche Kunde", "tokens": ["Und", "dein", "z\u00e4rt\u00b7li\u00b7cher", "Va\u00b7ter", "!", "\u2013", "Mir", "sag\u00b7te", "die", "fr\u00f6h\u00b7li\u00b7che", "Kun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.18": {"text": "Sein helleuchtender, sein himmelaufstrebender Blick!", "tokens": ["Sein", "hel\u00b7leuch\u00b7ten\u00b7der", ",", "sein", "him\u00b7me\u00b7lauf\u00b7stre\u00b7ben\u00b7der", "Blick", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PPOSAT", "ADJA", "NN", "$."], "meter": "-++-+-+-++--+", "measure": "iambic.septa.chol"}, "line.19": {"text": "H\u00e4ttest du ihn gesehen den Blick! Er h\u00e4tte zu einem", "tokens": ["H\u00e4t\u00b7test", "du", "ihn", "ge\u00b7se\u00b7hen", "den", "Blick", "!", "Er", "h\u00e4t\u00b7te", "zu", "ei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "VVPP", "ART", "NN", "$.", "PPER", "VAFIN", "APPR", "ART"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.20": {"text": "Meisterwerke, wie du keines noch schufst, dich entflammt!", "tokens": ["Meis\u00b7ter\u00b7wer\u00b7ke", ",", "wie", "du", "kei\u00b7nes", "noch", "schufst", ",", "dich", "ent\u00b7flammt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PPER", "PIS", "ADV", "VVFIN", "$,", "PPER", "VVFIN", "$."], "meter": "+-+-+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.21": {"text": "\u00bbmeines Daseins Wonne ist mein Einziger! L\u00e4chelnd entschlaf' ich", "tokens": ["\u00bb", "mei\u00b7nes", "Da\u00b7seins", "Won\u00b7ne", "ist", "mein", "Ein\u00b7zi\u00b7ger", "!", "L\u00e4\u00b7chelnd", "ent\u00b7schlaf'", "ich"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["$(", "PPOSAT", "NN", "NN", "VAFIN", "PPOSAT", "NN", "$.", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+---+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.22": {"text": "Mit dem Gedanken an ihn,\u00ab sagt der leuchtende Blick ....", "tokens": ["Mit", "dem", "Ge\u00b7dan\u00b7ken", "an", "ihn", ",", "\u00ab", "sagt", "der", "leuch\u00b7ten\u00b7de", "Blick", "...."], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PPER", "$,", "$(", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.23": {"text": "O noch lange \u2013 so rufet dein Freund vom Neckargestade \u2013", "tokens": ["O", "noch", "lan\u00b7ge", "\u2013", "so", "ru\u00b7fet", "dein", "Freund", "vom", "Ne\u00b7ckar\u00b7ge\u00b7sta\u00b7de", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "ADV", "$(", "ADV", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "$("], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.24": {"text": "Bleibe des Redlichen Lust! Bleibe du Sueviens Stolz,", "tokens": ["Blei\u00b7be", "des", "Red\u00b7li\u00b7chen", "Lust", "!", "Blei\u00b7be", "du", "Sue\u00b7vi\u00b7ens", "Stolz", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "NN", "$.", "VVFIN", "PPER", "NE", "NN", "$,"], "meter": "+--+---+-+-+-+", "measure": "dactylic.di.plus"}, "line.25": {"text": "Die den h\u00f6hnenden Schwestern entgegen die ewigen Namen:", "tokens": ["Die", "den", "h\u00f6h\u00b7nen\u00b7den", "Schwes\u00b7tern", "ent\u00b7ge\u00b7gen", "die", "e\u00b7wi\u00b7gen", "Na\u00b7men", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.26": {"text": "Wieland und Schiller! ruft und zum Verstummen sie zwingt!", "tokens": ["Wie\u00b7land", "und", "Schil\u00b7ler", "!", "ruft", "und", "zum", "Ver\u00b7stum\u00b7men", "sie", "zwingt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NE", "$.", "VVFIN", "KON", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.27": {"text": "Mit dem Himmelgef\u00fchl der Gesundheit in Ader und Nerve", "tokens": ["Mit", "dem", "Him\u00b7mel\u00b7ge\u00b7f\u00fchl", "der", "Ge\u00b7sund\u00b7heit", "in", "A\u00b7der", "und", "Ner\u00b7ve"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "APPR", "NN", "KON", "NN"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.28": {"text": "Wandle mutig du fort auf der Unsterblichkeit Bahn,", "tokens": ["Wand\u00b7le", "mu\u00b7tig", "du", "fort", "auf", "der", "U\u00b7nsterb\u00b7lich\u00b7keit", "Bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "PPER", "PTKVZ", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.29": {"text": "Wunderbarer Proteus! und werd' in vollendeter Gr\u00f6\u00dfe", "tokens": ["Wun\u00b7der\u00b7ba\u00b7rer", "Pro\u00b7teus", "!", "und", "werd'", "in", "voll\u00b7en\u00b7de\u00b7ter", "Gr\u00f6\u00b7\u00dfe"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NE", "$.", "KON", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.30": {"text": "Deinem gl\u00fccklichen Volk Shakespeare und Hume zumal.", "tokens": ["Dei\u00b7nem", "gl\u00fcck\u00b7li\u00b7chen", "Volk", "Sha\u00b7ke\u00b7spe\u00b7a\u00b7re", "und", "Hume", "zu\u00b7mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NE", "KON", "NN", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}