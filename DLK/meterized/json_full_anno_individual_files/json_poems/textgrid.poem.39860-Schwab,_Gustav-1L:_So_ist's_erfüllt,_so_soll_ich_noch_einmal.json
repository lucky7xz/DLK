{"textgrid.poem.39860": {"metadata": {"author": {"name": "Schwab, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: So ist's erf\u00fcllt, so soll ich noch einmal", "genre": "verse", "period": "N.A.", "pub_year": 1821, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "So ist's erf\u00fcllt, so soll ich noch einmal", "tokens": ["So", "ist's", "er\u00b7f\u00fcllt", ",", "so", "soll", "ich", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sehen, wie der Dichtung Morgentraum,", "tokens": ["Sie", "se\u00b7hen", ",", "wie", "der", "Dich\u00b7tung", "Mor\u00b7gen\u00b7traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Wie seltnen Abends Zauberf\u00e4rbung kaum", "tokens": ["Wie", "selt\u00b7nen", "A\u00b7bends", "Zau\u00b7berf\u00b7\u00e4r\u00b7bung", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sie mir gezeigt in mondverkl\u00e4rtem Saal.", "tokens": ["Sie", "mir", "ge\u00b7zeigt", "in", "mond\u00b7ver\u00b7kl\u00e4r\u00b7tem", "Saal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ihr goldnes Haar webt sich aus Mondenstral,", "tokens": ["Ihr", "gold\u00b7nes", "Haar", "webt", "sich", "aus", "Mon\u00b7dens\u00b7tral", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Aus Mondenschatten ihrer Wimper Saum,", "tokens": ["Aus", "Mon\u00b7den\u00b7schat\u00b7ten", "ih\u00b7rer", "Wim\u00b7per", "Saum", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ihr schimmernd Kleid flie\u00dft wie der Wellenschaum", "tokens": ["Ihr", "schim\u00b7mernd", "Kleid", "flie\u00dft", "wie", "der", "Wel\u00b7len\u00b7schaum"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "NN", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des Felsenbachs, der niederwallt in's Thal.", "tokens": ["Des", "Fel\u00b7sen\u00b7bachs", ",", "der", "nie\u00b7der\u00b7wallt", "in's", "Thal", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Aus ihrem Aetherauge quillt ein Licht,", "tokens": ["Aus", "ih\u00b7rem", "A\u00b7e\u00b7ther\u00b7au\u00b7ge", "quillt", "ein", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Das Feuer taucht noch aus dem tiefen Blau,", "tokens": ["Das", "Feu\u00b7er", "taucht", "noch", "aus", "dem", "tie\u00b7fen", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "So sah mein Blick, so sang sie mein Gedicht.", "tokens": ["So", "sah", "mein", "Blick", ",", "so", "sang", "sie", "mein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$,", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Doch schmerzvoll ist mein Geist ihr zugekehrt;", "tokens": ["Doch", "schmerz\u00b7voll", "ist", "mein", "Geist", "ihr", "zu\u00b7ge\u00b7kehrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPOSAT", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Verg\u00e4nglich ist die Sch\u00f6nheit, wie der Thau \u2013", "tokens": ["Ver\u00b7g\u00e4ng\u00b7lich", "ist", "die", "Sch\u00f6n\u00b7heit", ",", "wie", "der", "Thau", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PWAV", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "War ihr Unsterbliches der Liebe wert?", "tokens": ["War", "ihr", "U\u00b7nsterb\u00b7li\u00b7ches", "der", "Lie\u00b7be", "wert", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.5": {"line.1": {"text": "So ist's erf\u00fcllt, so soll ich noch einmal", "tokens": ["So", "ist's", "er\u00b7f\u00fcllt", ",", "so", "soll", "ich", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "VVPP", "$,", "ADV", "VMFIN", "PPER", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Sie sehen, wie der Dichtung Morgentraum,", "tokens": ["Sie", "se\u00b7hen",