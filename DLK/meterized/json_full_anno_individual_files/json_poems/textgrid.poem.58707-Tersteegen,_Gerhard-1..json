{"textgrid.poem.58707": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Siehe, ich will viel Fischer aussenden, spricht der Herr, die sollen sie fischen. Jer. 16, 16", "tokens": ["Sie\u00b7he", ",", "ich", "will", "viel", "Fi\u00b7scher", "aus\u00b7sen\u00b7den", ",", "spricht", "der", "Herr", ",", "die", "sol\u00b7len", "sie", "fi\u00b7schen", ".", "Jer", ".", "16", ",", "16"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct", "number", "punct", "number"], "pos": ["VVIMP", "$,", "PPER", "VMFIN", "PIAT", "NN", "VVPP", "$,", "VVFIN", "ART", "NN", "$,", "PRELS", "VMFIN", "PPER", "VVINF", "$.", "NE", "$.", "CARD", "$,", "CARD"], "meter": "-+-+-+-+--+-+-+--+-+", "measure": "iambic.octa.plus.relaxed"}}, "stanza.2": {"line.1": {"text": "So fische, Gotteslieb', ach, fische so viel Herzen,", "tokens": ["So", "fi\u00b7sche", ",", "Got\u00b7tes\u00b7lieb'", ",", "ach", ",", "fi\u00b7sche", "so", "viel", "Her\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "NN", "$,", "ITJ", "$,", "VVFIN", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df Wollust keine fangen kann,", "tokens": ["Da\u00df", "Wol\u00b7lust", "kei\u00b7ne", "fan\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PIAT", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr Lockaas bringt nur Schmerzen;", "tokens": ["Ihr", "Loc\u00b7kaas", "bringt", "nur", "Schmer\u00b7zen", ";"], "