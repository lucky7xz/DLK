{"dta.poem.18080": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Ueber den fr\u00fchen Tod einer Fr\u00e4ulein.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198432", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Dein Leben, dessen End uns plaget,", "tokens": ["Dein", "Le\u00b7ben", ",", "des\u00b7sen", "End", "uns", "pla\u00b7get", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELAT", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "War wie ein Tag sch\u00f6n und nicht lang,", "tokens": ["War", "wie", "ein", "Tag", "sch\u00f6n", "und", "nicht", "lang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "ADJD", "KON", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Stern vor des Morgens Aufgang,", "tokens": ["Ein", "Stern", "vor", "des", "Mor\u00b7gens", "Auf\u00b7gang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADV", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Die R\u00f6thin w\u00e4hrend weil es taget,", "tokens": ["Die", "R\u00f6\u00b7thin", "w\u00e4h\u00b7rend", "weil", "es", "ta\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Seufftz aus einer edlen Brust,", "tokens": ["Ein", "Seufftz", "aus", "ei\u00b7ner", "ed\u00b7len", "Brust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Klag aus Lieb, nicht aus Unlust,", "tokens": ["Ein", "Klag", "aus", "Lieb", ",", "nicht", "aus", "Un\u00b7lust", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,", "PTKNEG", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Ein Nebel, den die Sonn verjaget.", "tokens": ["Ein", "Ne\u00b7bel", ",", "den", "die", "Sonn", "ver\u00b7ja\u00b7get", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Staub der mit dem Wind entstehet,", "tokens": ["Ein", "Staub", "der", "mit", "dem", "Wind", "ent\u00b7ste\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Thau in des Sommers Anbruch,", "tokens": ["Ein", "Thau", "in", "des", "Som\u00b7mers", "An\u00b7bruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ein Lufft mit lieblichem Geruch,", "tokens": ["Ein", "Lufft", "mit", "lieb\u00b7li\u00b7chem", "Ge\u00b7ruch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Schnee der Fr\u00fchlings-Zeit abgehet,", "tokens": ["Ein", "Schnee", "der", "Fr\u00fch\u00b7lings\u00b7Zeit", "ab\u00b7ge\u00b7het", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Blum die frisch und welck zugleich,", "tokens": ["Ein", "Blum", "die", "frisch", "und", "welck", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ART", "ADJD", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Regenbog von Farben reich,", "tokens": ["Ein", "Re\u00b7gen\u00b7bog", "von", "Far\u00b7ben", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Zweig welchen der Wind umwehet.", "tokens": ["Ein", "Zweig", "wel\u00b7chen", "der", "Wind", "um\u00b7we\u00b7het", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.3": {"line.1": {"text": "Ein Schaur in Sommer-Zeit vergossen,", "tokens": ["Ein", "Schaur", "in", "Som\u00b7mer\u00b7Zeit", "ver\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Ei\u00df an heissem Sonnenschein,", "tokens": ["Ein", "Ei\u00df", "an", "heis\u00b7sem", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Gla\u00df also br\u00fcchig als rein,", "tokens": ["Ein", "Gla\u00df", "al\u00b7so", "br\u00fc\u00b7chig", "als", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KOKOM", "ADJD", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Ein Wasser \u00fcber Nacht verflossen,", "tokens": ["Ein", "Was\u00b7ser", "\u00fc\u00b7ber", "Nacht", "ver\u00b7flos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Blitz zumahl geschwind und hell,", "tokens": ["Ein", "Blitz", "zu\u00b7mahl", "ge\u00b7schwind", "und", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Strahl schiessend herab gar schnell,", "tokens": ["Ein", "Strahl", "schies\u00b7send", "her\u00b7ab", "gar", "schnell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADV", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein Gel\u00e4chter mit Leid beschlossen.", "tokens": ["Ein", "Ge\u00b7l\u00e4ch\u00b7ter", "mit", "Leid", "be\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Ein Stimm die lieblich dahin f\u00e4hret,", "tokens": ["Ein", "Stimm", "die", "lieb\u00b7lich", "da\u00b7hin", "f\u00e4h\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJD", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Widerhall der Stimm in Eil,", "tokens": ["Ein", "Wi\u00b7der\u00b7hall", "der", "Stimm", "in", "Eil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Zeitvertreiben mit Kurtzweil,", "tokens": ["Ein", "Zeit\u00b7ver\u00b7trei\u00b7ben", "mit", "Kurt\u00b7zweil", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Traum der mit dem Schlaff aufh\u00f6ret,", "tokens": ["Ein", "Traum", "der", "mit", "dem", "Schlaff", "auf\u00b7h\u00f6\u00b7ret", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ein Flug des Vogels mit Begier,", "tokens": ["Ein", "Flug", "des", "Vo\u00b7gels", "mit", "Be\u00b7gier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schatt, wann die Sonn sticht herf\u00fcr,", "tokens": ["Ein", "Schatt", ",", "wann", "die", "Sonn", "sticht", "her\u00b7f\u00fcr", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ART", "NN", "VVFIN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.7": {"text": "Ein Rauch welchen der Wind zerst\u00f6ret.", "tokens": ["Ein", "Rauch", "wel\u00b7chen", "der", "Wind", "zer\u00b7st\u00f6\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PWAT", "ART", "NN", "VVFIN", "$."], "meter": "-+---+-+-", "measure": "dactylic.init"}}, "stanza.5": {"line.1": {"text": "Also dein Leben (schnell verflogen)", "tokens": ["Al\u00b7so", "dein", "Le\u00b7ben", "(", "schnell", "ver\u00b7flo\u00b7gen", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "$(", "ADJD", "VVPP", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Hat sich nicht anderst dann ein Tag,", "tokens": ["Hat", "sich", "nicht", "an\u00b7derst", "dann", "ein", "Tag", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "PTKNEG", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Stern, Morgenr\u00f6th, Seufftz, Nebel, Klag,", "tokens": ["Stern", ",", "Mor\u00b7gen\u00b7r\u00f6th", ",", "Seufftz", ",", "Ne\u00b7bel", ",", "Klag", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Staub, Thau, Lufft, Schnee, Blum, Regenbogen,", "tokens": ["Staub", ",", "Thau", ",", "Lufft", ",", "Schnee", ",", "Blum", ",", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NE", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Zweig, Schaur, Ei\u00df, Gla\u00df, Blitz, Wasserfall,", "tokens": ["Zweig", ",", "Schaur", ",", "Ei\u00df", ",", "Gla\u00df", ",", "Blitz", ",", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Strahl, Gel\u00e4chter, Stimm, Widerhall,", "tokens": ["Strahl", ",", "Ge\u00b7l\u00e4ch\u00b7ter", ",", "Stimm", ",", "Wi\u00b7der\u00b7hall", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-++-+", "measure": "unknown.measure.penta"}, "line.7": {"text": "Zeit, Traum, Flug, Schatt und Rauch verzogen.", "tokens": ["Zeit", ",", "Traum", ",", "Flug", ",", "Schatt", "und", "Rauch", "ver\u00b7zo\u00b7gen", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}