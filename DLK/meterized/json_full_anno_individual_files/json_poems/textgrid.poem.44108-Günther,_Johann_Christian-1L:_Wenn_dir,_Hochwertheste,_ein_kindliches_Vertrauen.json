{"textgrid.poem.44108": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn dir, Hochwertheste, ein kindliches Vertrauen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir, Hochwertheste, ein kindliches Vertrauen", "tokens": ["Wenn", "dir", ",", "Hoch\u00b7wert\u00b7hes\u00b7te", ",", "ein", "kind\u00b7li\u00b7ches", "Ver\u00b7trau\u00b7en"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ADJA", "$,", "ART", "ADJA", "NN"], "meter": "+-+-+--+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Zu deiner Muttertreu lieb und erfreulich ist,", "tokens": ["Zu", "dei\u00b7ner", "Mut\u00b7ter\u00b7treu", "lieb", "und", "er\u00b7freu\u00b7lich", "ist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJD", "KON", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So las den kleinen Sohn, der dir die H\u00e4nde k\u00fcst,", "tokens": ["So", "las", "den", "klei\u00b7nen", "Sohn", ",", "der", "dir", "die", "H\u00e4n\u00b7de", "k\u00fcst", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Gr\u00f6\u00dfe reicher Huld an diesem Tage schauen.", "tokens": ["Die", "Gr\u00f6\u00b7\u00dfe", "rei\u00b7cher", "Huld", "an", "die\u00b7sem", "Ta\u00b7ge", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Nimm mit geneigter Hand den schlecht- und armen Zoll", "tokens": ["Nimm", "mit", "ge\u00b7neig\u00b7ter", "Hand", "den", "schlecht", "und", "ar\u00b7men", "Zoll"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ADJA", "NN", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und las dies d\u00fcrre Blat aus deinen holden Augen", "tokens": ["Und", "las", "dies", "d\u00fcr\u00b7re", "Blat", "aus", "dei\u00b7nen", "hol\u00b7den", "Au\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PDS", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Anmuth, so ihm fehlt, anstatt der Dinte saugen!", "tokens": ["Die", "An\u00b7muth", ",", "so", "ihm", "fehlt", ",", "an\u00b7statt", "der", "Din\u00b7te", "sau\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "VVFIN", "$,", "KOUI", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Vielleicht macht dieser Thau die hohlen Nelcken voll.", "tokens": ["Viel\u00b7leicht", "macht", "die\u00b7ser", "Thau", "die", "hoh\u00b7len", "Nel\u00b7cken", "voll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Abend d\u00e4mpft bereits des Mittags Last und Hize.", "tokens": ["Der", "A\u00b7bend", "d\u00e4mpft", "be\u00b7reits", "des", "Mit\u00b7tags", "Last", "und", "Hi\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich komme ziemlich sp\u00e4t, doch langsam n\u00e4hrt sich auch;", "tokens": ["Ich", "kom\u00b7me", "ziem\u00b7lich", "sp\u00e4t", ",", "doch", "lang\u00b7sam", "n\u00e4hrt", "sich", "auch", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,", "ADV", "ADJD", "VVFIN", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du weist es schon vorhin, ich habe den Gebrauch,", "tokens": ["Du", "weist", "es", "schon", "vor\u00b7hin", ",", "ich", "ha\u00b7be", "den", "Ge\u00b7brauch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df ich gar selten viel aus \u00dcbereilung schwize.", "tokens": ["Da\u00df", "ich", "gar", "sel\u00b7ten", "viel", "aus", "\u00dc\u00b7be\u00b7rei\u00b7lung", "schwi\u00b7ze", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "ADV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Kindheit \u00fcberdies entschuldigt meine Pflicht,", "tokens": ["Die", "Kind\u00b7heit", "\u00fc\u00b7ber\u00b7dies", "ent\u00b7schul\u00b7digt", "mei\u00b7ne", "Pflicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die das erfreute Fest, so deinen Nahmen borget,", "tokens": ["Die", "das", "er\u00b7freu\u00b7te", "Fest", ",", "so", "dei\u00b7nen", "Nah\u00b7men", "bor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "$,", "ADV", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nicht eher durch den Reim, der dennoch schnarrt, versorget,", "tokens": ["Nicht", "e\u00b7her", "durch", "den", "Reim", ",", "der", "den\u00b7noch", "schnarrt", ",", "ver\u00b7sor\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PTKNEG", "ADV", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Weil mir die rechte Kraft zur Poesie gebricht.", "tokens": ["Weil", "mir", "die", "rech\u00b7te", "Kraft", "zur", "Poe\u00b7sie", "ge\u00b7bricht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}}, "stanza.3": {"line.1": {"text": "Was aber sollt ich dir zum Angebinde kaufen,", "tokens": ["Was", "a\u00b7ber", "sollt", "ich", "dir", "zum", "An\u00b7ge\u00b7bin\u00b7de", "kau\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "PPER", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ich ein Petrus bin, dem Gold und Silber fehlt?", "tokens": ["Da", "ich", "ein", "Pet\u00b7rus", "bin", ",", "dem", "Gold", "und", "Sil\u00b7ber", "fehlt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "FM", "VAFIN", "$,", "ART", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wer von Salat und Obst die Finger m\u00fcde zehlt,", "tokens": ["Wer", "von", "Sa\u00b7lat", "und", "Obst", "die", "Fin\u00b7ger", "m\u00fc\u00b7de", "zehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NE", "KON", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der kan vor den Gewinn gar leicht zum Koche laufen.", "tokens": ["Der", "kan", "vor", "den", "Ge\u00b7winn", "gar", "leicht", "zum", "Ko\u00b7che", "lau\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "APPR", "ART", "NN", "ADV", "ADJD", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die Kr\u00e4uter bringen mir nicht einen Heller ein,", "tokens": ["Die", "Kr\u00e4u\u00b7ter", "brin\u00b7gen", "mir", "nicht", "ei\u00b7nen", "Hel\u00b7ler", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKNEG", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und k\u00f6nt ich einen Marckt von Kirschengut bestellen,", "tokens": ["Und", "k\u00f6nt", "ich", "ei\u00b7nen", "Marckt", "von", "Kir\u00b7schen\u00b7gut", "be\u00b7stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So w\u00fcrden sicherlich mir anderthalb Florellen", "tokens": ["So", "w\u00fcr\u00b7den", "si\u00b7cher\u00b7lich", "mir", "an\u00b7der\u00b7thalb", "Flo\u00b7rel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PRF", "PPER", "CARD", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So wenig als ein Quart ans Herz gebunden seyn.", "tokens": ["So", "we\u00b7nig", "als", "ein", "Quart", "ans", "Herz", "ge\u00b7bun\u00b7den", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "KOKOM", "ART", "NN", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jedoch ein Sto\u00dfgebeth, ein Seufzer, eine Thr\u00e4ne,", "tokens": ["Je\u00b7doch", "ein", "Sto\u00df\u00b7ge\u00b7beth", ",", "ein", "Seuf\u00b7zer", ",", "ei\u00b7ne", "Thr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die vor dein Wohlergehn von mir gen Himmel steigt,", "tokens": ["Die", "vor", "dein", "Woh\u00b7ler\u00b7gehn", "von", "mir", "gen", "Him\u00b7mel", "steigt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPOSAT", "NN", "APPR", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gilt mehr als rotes Erz, weil es den Seegen zeigt,", "tokens": ["Gilt", "mehr", "als", "ro\u00b7tes", "Erz", ",", "weil", "es", "den", "See\u00b7gen", "zeigt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dem ich durch meinen Wuntsch den Weg zur Ankunft b\u00e4hne.", "tokens": ["Dem", "ich", "durch", "mei\u00b7nen", "Wunt\u00b7sch", "den", "Weg", "zur", "An\u00b7kunft", "b\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "PPOSAT", "NN", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Gott, deiner V\u00e4ter Gott, steh deinem Creuze bey,", "tokens": ["Gott", ",", "dei\u00b7ner", "V\u00e4\u00b7ter", "Gott", ",", "steh", "dei\u00b7nem", "Creu\u00b7ze", "bey", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er mindre seine Last und mehre dir die Jahre,", "tokens": ["Er", "mind\u00b7re", "sei\u00b7ne", "Last", "und", "meh\u00b7re", "dir", "die", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Damit des Alters Schmuck, das Silber grauer Haare,", "tokens": ["Da\u00b7mit", "des", "Al\u00b7ters", "Schmuck", ",", "das", "Sil\u00b7ber", "grau\u00b7er", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dereinst um deinen Schlaf der Ehrfurcht heilig sey!", "tokens": ["De\u00b7reinst", "um", "dei\u00b7nen", "Schlaf", "der", "Ehr\u00b7furcht", "hei\u00b7lig", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Dein Garthen baue dir ein Paradies auf Erden,", "tokens": ["Dein", "Gar\u00b7then", "bau\u00b7e", "dir", "ein", "Pa\u00b7ra\u00b7dies", "auf", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In welchem jeder Baum Vergn\u00fcgungsfr\u00fcchte tr\u00e4gt,", "tokens": ["In", "wel\u00b7chem", "je\u00b7der", "Baum", "Ver\u00b7gn\u00fc\u00b7gungs\u00b7fr\u00fcch\u00b7te", "tr\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die keines Unfalls Wind von ihren Zweigen schl\u00e4gt,", "tokens": ["Die", "kei\u00b7nes", "Un\u00b7falls", "Wind", "von", "ih\u00b7ren", "Zwei\u00b7gen", "schl\u00e4gt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bis sie von Gl\u00fccke reich, von Wollust m\u00fcrbe werden.", "tokens": ["Bis", "sie", "von", "Gl\u00fc\u00b7cke", "reich", ",", "von", "Wol\u00b7lust", "m\u00fcr\u00b7be", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "NN", "ADJD", "$,", "APPR", "NN", "VMFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Trinckt manchmahl Gottes Hand dir einen Creuzkelch zu,", "tokens": ["Trinckt", "manch\u00b7mahl", "Got\u00b7tes", "Hand", "dir", "ei\u00b7nen", "Creuz\u00b7kelch", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "NN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So misch ein Engel dir stets Zucker in die Myrrhen,", "tokens": ["So", "misch", "ein", "En\u00b7gel", "dir", "stets", "Zu\u00b7cker", "in", "die", "Myr\u00b7rhen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "PPER", "ADV", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Und mustu deinen Fu\u00df zur lezten Reise schirren,", "tokens": ["Und", "mus\u00b7tu", "dei\u00b7nen", "Fu\u00df", "zur", "lez\u00b7ten", "Rei\u00b7se", "schir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPOSAT", "NN", "APPRART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So bringe Canaan den m\u00fcden Leib zur Ruh!", "tokens": ["So", "brin\u00b7ge", "Ca\u00b7naan", "den", "m\u00fc\u00b7den", "Leib", "zur", "Ruh", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "NE", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Wenn dir, Hochwertheste, ein kindliches Vertrauen", "tokens": ["Wenn", "dir", ",", "Hoch\u00b7wert\u00b7hes\u00b7te", ",", "ein", "kind\u00b7li\u00b7ches", "Ver\u00b7trau\u00b7e