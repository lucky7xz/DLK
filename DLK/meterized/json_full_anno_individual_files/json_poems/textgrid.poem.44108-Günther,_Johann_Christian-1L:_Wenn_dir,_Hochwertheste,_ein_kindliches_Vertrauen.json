eter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Gott, deiner V\u00e4ter Gott, steh deinem Creuze bey,", "tokens": ["Gott", ",", "dei\u00b7ner", "V\u00e4\u00b7ter", "Gott", ",", "steh", "dei\u00b7nem", "Creu\u00b7ze", "bey", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPOSAT", "NN", "NN", "$,", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Er mindre seine Last und mehre dir die Jahre,", "tokens": ["Er", "mind\u00b7re", "sei\u00b7ne", "Last", "und", "meh\u00b7re", "dir", "die", "Jah\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "KON", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Damit des Alters Schmuck, das Silber grauer Haare,", "tokens": ["Da\u00b7mit", "des", "Al\u00b7ters", "Schmuck", ",", "das", "Sil\u00b7ber", "grau\u00b7er", "Haa\u00b7re", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Dereinst um deinen Schlaf der Ehrfurcht heilig sey!", "tokens": ["De\u00b7reinst", "um", "dei\u00b7nen", "Schlaf", "der", "Ehr\u00b7furcht", "hei\u00b7lig", "sey", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Dein Garthen baue dir ein Paradies auf Erden,", "tokens": ["Dein", "Gar\u00b7then", "bau\u00b7e", "dir", "ein", "Pa\u00b7ra\u00b7dies", "auf", "Er\u00b7den", ","