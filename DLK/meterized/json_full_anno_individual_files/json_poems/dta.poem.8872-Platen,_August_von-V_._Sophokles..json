{"dta.poem.8872": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "V .  \n  Sophokles.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Den Punkt zu schau'n, wo Mensch und Gott sich scheidet,", "tokens": ["Den", "Punkt", "zu", "schau'n", ",", "wo", "Mensch", "und", "Gott", "sich", "schei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKZU", "VVINF", "$,", "PWAV", "NN", "KON", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und was in ird'sche Worte du gekleidet,", "tokens": ["Und", "was", "in", "ird'\u00b7sche", "Wor\u00b7te", "du", "ge\u00b7klei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Das ward, vom Himmel aus, dir vorgesungen!", "tokens": ["Das", "ward", ",", "vom", "Him\u00b7mel", "aus", ",", "dir", "vor\u00b7ge\u00b7sun\u00b7gen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$,", "APPRART", "NN", "PTKVZ", "$,", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du bist in's Innre dieser Welt gedrungen", "tokens": ["Du", "bist", "in's", "Inn\u00b7re", "die\u00b7ser", "Welt", "ge\u00b7drun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPRART", "NN", "PDAT", "NN", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und kennst zugleich, was auf der Fl\u00e4che weidet:", "tokens": ["Und", "kennst", "zu\u00b7gleich", ",", "was", "auf", "der", "Fl\u00e4\u00b7che", "wei\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was nur ein Menschenbusen hofft und leidet,", "tokens": ["Was", "nur", "ein", "Men\u00b7schen\u00b7bu\u00b7sen", "hofft", "und", "lei\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Du sprachst es aus mit deinen tausend Zungen!", "tokens": ["Du", "sprachst", "es", "aus", "mit", "dei\u00b7nen", "tau\u00b7send", "Zun\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "APPR", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Nie bist du k\u00fchl zur N\u00fcchternheit versunken,", "tokens": ["Nie", "bist", "du", "k\u00fchl", "zur", "N\u00fcch\u00b7tern\u00b7heit", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du spr\u00fchtest in erhabener Verschwendung", "tokens": ["Du", "spr\u00fch\u00b7test", "in", "er\u00b7ha\u00b7be\u00b7ner", "Ver\u00b7schwen\u00b7dung"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.3": {"text": "Der goldnen Flamme lichte, dichte Funken!", "tokens": ["Der", "gold\u00b7nen", "Flam\u00b7me", "lich\u00b7te", ",", "dich\u00b7te", "Fun\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "An dich erging die heil'ge, gro\u00dfe Sendung,", "tokens": ["An", "dich", "er\u00b7ging", "die", "heil'\u00b7ge", ",", "gro\u00b7\u00dfe", "Sen\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du hast den Rausch der Poesie getrunken,", "tokens": ["Du", "hast", "den", "Rausch", "der", "Poe\u00b7sie", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Und schimmerst nun in strahlender Vollendung!", "tokens": ["Und", "schim\u00b7merst", "nun", "in", "strah\u00b7len\u00b7der", "Vol\u00b7len\u00b7dung", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}}}}}