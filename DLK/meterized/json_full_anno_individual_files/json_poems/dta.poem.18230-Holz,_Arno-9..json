{"dta.poem.18230": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "9.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Mir Blatt auf Blatt von dannen stiebt", "tokens": ["Mir", "Blatt", "auf", "Blatt", "von", "dan\u00b7nen", "stiebt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "APPR", "NN", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und ich nicht mehr die Worte finde,", "tokens": ["Und", "ich", "nicht", "mehr", "die", "Wor\u00b7te", "fin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PTKNEG", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie das Herz dem Herzen giebt!", "tokens": ["Wie", "sie", "das", "Herz", "dem", "Her\u00b7zen", "giebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Denn ach, die Lust singt immer leiser", "tokens": ["Denn", "ach", ",", "die", "Lust", "singt", "im\u00b7mer", "lei\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "NN", "VVFIN", "ADV", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und immer lauter schreit das Weh,", "tokens": ["Und", "im\u00b7mer", "lau\u00b7ter", "schreit", "das", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und l\u00e4ngst sind alle Hoffnungsreiser", "tokens": ["Und", "l\u00e4ngst", "sind", "al\u00b7le", "Hoff\u00b7nungs\u00b7rei\u00b7ser"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Begraben unterm Winterschnee.", "tokens": ["Be\u00b7gra\u00b7ben", "un\u00b7term", "Win\u00b7ter\u00b7schnee", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin so stumm und still geworden", "tokens": ["Ich", "bin", "so", "stumm", "und", "still", "ge\u00b7wor\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADJD", "KON", "ADJD", "VAPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und sing nur manchmal noch im Traum,", "tokens": ["Und", "sing", "nur", "manch\u00b7mal", "noch", "im", "Traum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch in den klagenden Akkorden", "tokens": ["Doch", "in", "den", "kla\u00b7gen\u00b7den", "Ak\u00b7kor\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "T\u00f6nt meiner Schmerzen Echo kaum.", "tokens": ["T\u00f6nt", "mei\u00b7ner", "Schmer\u00b7zen", "E\u00b7cho", "kaum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und will mir auch die Brust zerspringen,", "tokens": ["Und", "will", "mir", "auch", "die", "Brust", "zer\u00b7sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Es tr\u00e4gt kein Lied ihr Weh hinaus:", "tokens": ["Es", "tr\u00e4gt", "kein", "Lied", "ihr", "Weh", "hin\u00b7aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und so mu\u00df denn auch dies verklingen", "tokens": ["Und", "so", "mu\u00df", "denn", "auch", "dies", "ver\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VMFIN", "ADV", "ADV", "PDS", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Und ist doch lange noch nicht aus!", "tokens": ["Und", "ist", "doch", "lan\u00b7ge", "noch", "nicht", "aus", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "ADV", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}