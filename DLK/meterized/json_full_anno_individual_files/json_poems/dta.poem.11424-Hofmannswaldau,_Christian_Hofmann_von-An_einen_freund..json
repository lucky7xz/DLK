{"dta.poem.11424": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "An einen freund.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund! folge meinem rath, und meide burg und stadt,", "tokens": ["Freund", "!", "fol\u00b7ge", "mei\u00b7nem", "rath", ",", "und", "mei\u00b7de", "burg", "und", "stadt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "PPOSAT", "NN", "$,", "KON", "PIAT", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo mancher seinen GOtt und sich verlohren hat!", "tokens": ["Wo", "man\u00b7cher", "sei\u00b7nen", "Gott", "und", "sich", "ver\u00b7loh\u00b7ren", "hat", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "KON", "PRF", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Fleuch auf das stille land! Da sind nicht so viel s\u00fcnden:", "tokens": ["Fleuch", "auf", "das", "stil\u00b7le", "land", "!", "Da", "sind", "nicht", "so", "viel", "s\u00fcn\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$.", "ADV", "VAFIN", "PTKNEG", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und lerne durch die furcht, GOtt und dich selber finden!", "tokens": ["Und", "ler\u00b7ne", "durch", "die", "furcht", ",", "Gott", "und", "dich", "sel\u00b7ber", "fin\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$,", "NN", "KON", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}}}}}