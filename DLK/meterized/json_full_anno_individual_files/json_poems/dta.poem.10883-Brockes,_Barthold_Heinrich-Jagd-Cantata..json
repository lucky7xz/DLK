{"dta.poem.10883": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jagd-Cantata.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wie sch\u00f6n ist die bethaute Welt,", "tokens": ["Wie", "sch\u00f6n", "ist", "die", "be\u00b7thau\u00b7te", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Durch den entw\u00f6lkten Morgenstral!", "tokens": ["Durch", "den", "ent\u00b7w\u00f6lk\u00b7ten", "Mor\u00b7gen\u00b7stral", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie funkelt das getr\u00e4nkte Feld!", "tokens": ["Wie", "fun\u00b7kelt", "das", "ge\u00b7tr\u00e4nk\u00b7te", "Feld", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wie gl\u00e4nzt und schimmert Berg und Thal!", "tokens": ["Wie", "gl\u00e4nzt", "und", "schim\u00b7mert", "Berg", "und", "Thal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "KON", "VVFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}