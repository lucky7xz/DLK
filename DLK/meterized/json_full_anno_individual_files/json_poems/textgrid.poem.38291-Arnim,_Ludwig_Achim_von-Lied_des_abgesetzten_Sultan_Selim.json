{"textgrid.poem.38291": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Lied des abgesetzten Sultan Selim", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Guguck ist ein braver Mann,", "tokens": ["Der", "Gu\u00b7guck", "ist", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sieben Weiber brauchen kann;", "tokens": ["Der", "sie\u00b7ben", "Wei\u00b7ber", "brau\u00b7chen", "kann", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "CARD", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die erste kehrt die Stube aus,", "tokens": ["Die", "ers\u00b7te", "kehrt", "die", "Stu\u00b7be", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die zweite wirft den Unflath 'naus;", "tokens": ["Die", "zwei\u00b7te", "wirft", "den", "Un\u00b7flath", "'naus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die dritte nimmt den Flederwisch,", "tokens": ["Die", "drit\u00b7te", "nimmt", "den", "Fle\u00b7der\u00b7wisch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und kehrt des Guckuck seinen Tisch;", "tokens": ["Und", "kehrt", "des", "Gu\u00b7ckuck", "sei\u00b7nen", "Tisch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Die vierte bringt ihm Brod und Wein,", "tokens": ["Die", "vier\u00b7te", "bringt", "ihm", "Brod", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Die f\u00fcnfte schenkt ihm flei\u00dfig ein;", "tokens": ["Die", "f\u00fcnf\u00b7te", "schenkt", "ihm", "flei\u00b7\u00dfig", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die sechste macht sein Bettlein warm,", "tokens": ["Die", "sechs\u00b7te", "macht", "sein", "Bet\u00b7tlein", "warm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Die siebente schl\u00e4ft in seinem Arm.", "tokens": ["Die", "sie\u00b7ben\u00b7te", "schl\u00e4ft", "in", "sei\u00b7nem", "Arm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Der Guguck ist ein braver Mann,", "tokens": ["Der", "Gu\u00b7guck", "ist", "ein", "bra\u00b7ver", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der sieben Weiber brauchen kann;", "tokens": ["Der", "sie\u00b7ben", "Wei\u00b7ber", "brau\u00b7chen", "k