{"textgrid.poem.60631": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: Eine H\u00fcndin, deren Zeit gekommen,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine H\u00fcndin, deren Zeit gekommen,", "tokens": ["Ei\u00b7ne", "H\u00fcn\u00b7din", ",", "de\u00b7ren", "Zeit", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und die nicht wu\u00dfte, wo die dr\u00e4ngende B\u00fcrde sie", "tokens": ["Und", "die", "nicht", "wu\u00df\u00b7te", ",", "wo", "die", "dr\u00e4n\u00b7gen\u00b7de", "B\u00fcr\u00b7de", "sie"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "PTKNEG", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "PPER"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ablegen sollte, jammerte so herzbeklommen,", "tokens": ["Ab\u00b7le\u00b7gen", "soll\u00b7te", ",", "jam\u00b7mer\u00b7te", "so", "herz\u00b7be\u00b7klom\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "$,", "VVFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df schlie\u00dflich eine andre ihr die H\u00fctte lieh.", "tokens": ["Da\u00df", "schlie\u00df\u00b7lich", "ei\u00b7ne", "and\u00b7re", "ihr", "die", "H\u00fct\u00b7te", "lieh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "ADJA", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die H\u00fcndin schlo