{"textgrid.poem.39995": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Indem ich nun an diesem sch\u00f6nen Ort", "genre": "verse", "period": "N.A.", "pub_year": 1713, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Indem ich nun an diesem sch\u00f6nen Ort", "tokens": ["In\u00b7dem", "ich", "nun", "an", "die\u00b7sem", "sch\u00f6\u00b7nen", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Best\u00e4ndig neue Wunder sehe;", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "neu\u00b7e", "Wun\u00b7der", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erheb' ich mich von meinem Sitz', und gehe", "tokens": ["Er\u00b7heb'", "ich", "mich", "von", "mei\u00b7nem", "Sitz'", ",", "und", "ge\u00b7he"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit sanften Schritten weiter fort,", "tokens": ["Mit", "sanf\u00b7ten", "Schrit\u00b7ten", "wei\u00b7ter", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Worauf ich bald hernach,", "tokens": ["Wo\u00b7rauf", "ich", "bald", "her\u00b7nach", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "ADV", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "In einem nah' gelegnen Bach,", "tokens": ["In", "ei\u00b7nem", "nah'", "ge\u00b7leg\u00b7nen", "Bach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ein nicht unangenehm Gew\u00e4chse", "tokens": ["Ein", "nicht", "un\u00b7an\u00b7ge\u00b7nehm", "Ge\u00b7w\u00e4ch\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PTKNEG", "ADJD", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Geschw\u00e4tziger und froher Fr\u00f6sche,", "tokens": ["Ge\u00b7schw\u00e4t\u00b7zi\u00b7ger", "und", "fro\u00b7her", "Fr\u00f6\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Mit ungemeinen Freuden, h\u00f6rte,", "tokens": ["Mit", "un\u00b7ge\u00b7mei\u00b7nen", "Freu\u00b7den", ",", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Das, ob es gleich die Stille unterbrach,", "tokens": ["Das", ",", "ob", "es", "gleich", "die", "Stil\u00b7le", "un\u00b7ter\u00b7brach", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Mich dennoch nicht in meinem Dencken st\u00f6hrte.", "tokens": ["Mich", "den\u00b7noch", "nicht", "in", "mei\u00b7nem", "Den\u00b7cken", "st\u00f6hr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKNEG", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich dachte dem verwirreten Geschrey", "tokens": ["Ich", "dach\u00b7te", "dem", "ver\u00b7wir\u00b7re\u00b7ten", "Ge\u00b7schrey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-++--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Ein wenig nach,", "tokens": ["Ein", "we\u00b7nig", "nach", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und fand, da\u00df es nicht einerley,", "tokens": ["Und", "fand", ",", "da\u00df", "es", "nicht", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wohl aber sehr verschiedlich, sey.", "tokens": ["Wohl", "a\u00b7ber", "sehr", "ver\u00b7schied\u00b7lich", ",", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "ADV", "ADJD", "$,", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der eine quackt, viel hundert quarren.", "tokens": ["Der", "ei\u00b7ne", "quackt", ",", "viel", "hun\u00b7dert", "quar\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,", "ADV", "CARD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hier murret einer sanft, wenn dorten tausend knarren.", "tokens": ["Hier", "mur\u00b7ret", "ei\u00b7ner", "sanft", ",", "wenn", "dor\u00b7ten", "tau\u00b7send", "knar\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "$,", "KOUS", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wreckeckeckecks schreyt der, dort einer: merck' es, merck's.", "tokens": ["Wre\u00b7cke\u00b7cke\u00b7ckecks", "schreyt", "der", ",", "dort", "ei\u00b7ner", ":", "merck", "es", ",", "merck", "s."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "abbreviation"], "pos": ["NE", "VVFIN", "ART", "$,", "ADV", "ART", "$.", "VVFIN", "PPER", "$,", "VVFIN", "NE"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.8": {"text": "Merk's, schrieen ihrer viel'. Ich stutzte. Ruffest du,", "tokens": ["Merk's", ",", "schri\u00b7een", "ih\u00b7rer", "viel'", ".", "Ich", "stutz\u00b7te", ".", "Ruf\u00b7fest", "du", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "NN", "$.", "PPER", "VVFIN", "$.", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Sprach ich, o kleiner Frosch, dem Menschen: merck' es, zu?", "tokens": ["Sprach", "ich", ",", "o", "klei\u00b7ner", "Frosch", ",", "dem", "Men\u00b7schen", ":", "merck", "es", ",", "zu", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPER", "$,", "FM", "ADJA", "NN", "$,", "ART", "NN", "$.", "VVFIN", "PPER", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gewi\u00dflich, du hast recht: man macht so wenig Wercks", "tokens": ["Ge\u00b7wi\u00df\u00b7lich", ",", "du", "hast", "recht", ":", "man", "macht", "so", "we\u00b7nig", "Wercks"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "$,", "PPER", "VAFIN", "ADJD", "$.", "PIS", "VVFIN", "ADV", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Von aller Pracht und Sch\u00f6nheit, so die Welt,", "tokens": ["Von", "al\u00b7ler", "Pracht", "und", "Sch\u00f6n\u00b7heit", ",", "so", "die", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "KON", "NN", "$,", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Zumahl im Fr\u00fchling', in sich h\u00e4lt,", "tokens": ["Zu\u00b7mahl", "im", "Fr\u00fch\u00b7ling'", ",", "in", "sich", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Von allen g\u00f6ttlichen Gesch\u00f6pf- und Wunder-Wercken;", "tokens": ["Von", "al\u00b7len", "g\u00f6tt\u00b7li\u00b7chen", "Ge\u00b7sch\u00f6pf", "und", "Wun\u00b7der\u00b7\u00b7Wer\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "TRUNC", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Da\u00df wir nicht aufs Gesch\u00f6pf, nicht auf den Sch\u00f6pfer, mercken;", "tokens": ["Da\u00df", "wir", "nicht", "aufs", "Ge\u00b7sch\u00f6pf", ",", "nicht", "auf", "den", "Sch\u00f6p\u00b7fer", ",", "mer\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "APPRART", "NN", "$,", "PTKNEG", "APPR", "ART", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Da\u00df man kaum einst daran gedencket,", "tokens": ["Da\u00df", "man", "kaum", "einst", "da\u00b7ran", "ge\u00b7den\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "ADV", "PAV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.16": {"text": "Sich ihrer nicht erfreut, noch weniger auf Den,", "tokens": ["Sich", "ih\u00b7rer", "nicht", "er\u00b7freut", ",", "noch", "we\u00b7ni\u00b7ger", "auf", "Den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPOSAT", "PTKNEG", "ADJD", "$,", "ADV", "ADV", "APPR", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Der aller Dinge ", "tokens": ["Der", "al\u00b7ler", "Din\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["ART", "PIAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.18": {"text": "Der alles Herrliche geschaffen und uns schencket,", "tokens": ["Der", "al\u00b7les", "Herr\u00b7li\u00b7che", "ge\u00b7schaf\u00b7fen", "und", "uns", "schen\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "KON", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nebst einem frohen Danck, die frohe Seele lencket.", "tokens": ["Nebst", "ei\u00b7nem", "fro\u00b7hen", "Danck", ",", "die", "fro\u00b7he", "See\u00b7le", "len\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Ach! m\u00f6chte man doch einst, da\u00df dieses S\u00fcnde, fassen,", "tokens": ["Ach", "!", "m\u00f6ch\u00b7te", "man", "doch", "einst", ",", "da\u00df", "die\u00b7ses", "S\u00fcn\u00b7de", ",", "fas\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PIS", "ADV", "ADV", "$,", "KOUS", "PDAT", "NN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sich so gar vom Frosch daran erinnern lassen!", "tokens": ["Und", "sich", "so", "gar", "vom", "Frosch", "da\u00b7ran", "e\u00b7rin\u00b7nern", "las\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ADV", "ADV", "APPRART", "NN", "PAV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Aufs wenigste will ich, bist du gleich noch so klein,", "tokens": ["Aufs", "we\u00b7nigs\u00b7te", "will", "ich", ",", "bist", "du", "gleich", "noch", "so", "klein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "PIS", "VMFIN", "PPER", "$,", "VAFIN", "PPER", "ADV", "ADV", "ADV", "ADJD", "$,"], "meter": "-+--+-+----+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Beredter Frosch, dein aufmercksamer H\u00f6rer,", "tokens": ["Be\u00b7red\u00b7ter", "Frosch", ",", "dein", "auf\u00b7merck\u00b7sa\u00b7mer", "H\u00f6\u00b7rer", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Indem ich nun an diesem sch\u00f6nen Ort", "tokens": ["In\u00b7dem", "ich", "nun", "an", "die\u00b7sem", "sch\u00f6\u00b7nen", "Ort"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Best\u00e4ndig neue Wunder sehe;", "tokens": ["Be\u00b7st\u00e4n\u00b7dig", "neu\u00b7e", "Wun\u00b7der", "se\u00b7he", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Erheb' ich m