{"dta.poem.12081": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sang ein Wandersmann, als er die Welt durchlief:", "tokens": ["So", "sang", "ein", "Wan\u00b7ders\u00b7mann", ",", "als", "er", "die", "Welt", "durch\u00b7lief", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Berge sind zu hoch, die Th\u00e4ler sind zu tief.", "tokens": ["Die", "Ber\u00b7ge", "sind", "zu", "hoch", ",", "die", "Th\u00e4\u00b7ler", "sind", "zu", "tief", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Se'en sind zu todt, die Fl\u00fcsse zu lebendig,", "tokens": ["Die", "Se'\u00b7en", "sind", "zu", "todt", ",", "die", "Fl\u00fcs\u00b7se", "zu", "le\u00b7ben\u00b7dig", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Thiere sind zu dumm, die Menschen zu verst\u00e4ndig.", "tokens": ["Die", "Thie\u00b7re", "sind", "zu", "dumm", ",", "die", "Men\u00b7schen", "zu", "ver\u00b7st\u00e4n\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu dunkel ist die Nacht, der Tag ist alzu hell,", "tokens": ["Zu", "dun\u00b7kel", "ist", "die", "Nacht", ",", "der", "Tag", "ist", "al\u00b7zu", "hell", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "VAFIN", "ART", "NN", "$,", "ART", "NN", "VAFIN", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Mondschein ist zu bla\u00df, der Sonnenschein zu grell.", "tokens": ["Der", "Mond\u00b7schein", "ist", "zu", "bla\u00df", ",", "der", "Son\u00b7nen\u00b7schein", "zu", "grell", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "PTKA", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Der Himmel ist zu weit, die Erde mir zu enge;", "tokens": ["Der", "Him\u00b7mel", "ist", "zu", "weit", ",", "die", "Er\u00b7de", "mir", "zu", "en\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKA", "ADJD", "$,", "ART", "NN", "PPER", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich wollte, da\u00df ich w\u00e4r' am letzten meiner G\u00e4nge.", "tokens": ["Ich", "woll\u00b7te", ",", "da\u00df", "ich", "w\u00e4r'", "am", "letz\u00b7ten", "mei\u00b7ner", "G\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "KOUS", "PPER", "VAFIN", "APPRART", "ADJA", "PPOSAT", "NN", "$."], "meter": "-+-+-+