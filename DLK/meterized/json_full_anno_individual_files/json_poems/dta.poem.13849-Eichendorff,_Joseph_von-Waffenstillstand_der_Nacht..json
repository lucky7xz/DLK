{"dta.poem.13849": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Waffenstillstand der Nacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Pr\u00e4chtig war die Nacht nun aufgegangen,", "tokens": ["Pr\u00e4ch\u00b7tig", "war", "die", "Nacht", "nun", "auf\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Hatte alle m\u00fctterlich umfangen,", "tokens": ["Hat\u00b7te", "al\u00b7le", "m\u00fct\u00b7ter\u00b7lich", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Freund und Feind mit leisem Friedensku\u00df,", "tokens": ["Freund", "und", "Feind", "mit", "lei\u00b7sem", "Frie\u00b7dens\u00b7ku\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und, als wollt' der Herr vom Himmel steigen,", "tokens": ["Und", ",", "als", "wollt'", "der", "Herr", "vom", "Him\u00b7mel", "stei\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "VMFIN", "ART", "NN", "APPRART", "NN", "VVINF", "$,"], "meter": "--+-+-+-+-", "measure": "anapaest.init"}, "line.5": {"text": "H\u00f6rt' ich wieder durch das tiefe Schweigen", "tokens": ["H\u00f6rt'", "ich", "wie\u00b7der", "durch", "das", "tie\u00b7fe", "Schwei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.6": {"text": "Rings der W\u00e4lder feierlichen Gru\u00df.", "tokens": ["Rings", "der", "W\u00e4l\u00b7der", "fei\u00b7er\u00b7li\u00b7chen", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}}}}