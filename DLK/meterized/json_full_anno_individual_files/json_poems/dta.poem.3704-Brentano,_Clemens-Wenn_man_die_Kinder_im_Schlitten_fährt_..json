{"dta.poem.3704": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "Wenn man die Kinder im Schlitten f\u00e4hrt .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sch\u00e4fele hat ein K\u00fcttele an,               ", "tokens": ["Sch\u00e4\u00b7fe\u00b7le", "hat", "ein", "K\u00fct\u00b7te\u00b7le", "an", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "H\u00e4nget voller R\u00f6llen,", "tokens": ["H\u00e4n\u00b7get", "vol\u00b7ler", "R\u00f6l\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.3": {"text": "Wann es \u00fcber d' Gassen geht,", "tokens": ["Wann", "es", "\u00fc\u00b7ber", "d'", "Gas\u00b7sen", "geht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPR", "NE", "NN", "VVFIN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.4": {"text": "Fangen die ", "tokens": ["Fan\u00b7gen", "die"], "token_info": ["word", "word"], "pos": ["NN", "ART"], "meter": "+--", "measure": "dactylic.init"}}, "stanza.2": {"line.1": {"text": "Sch\u00e4fele komm,", "tokens": ["Sch\u00e4\u00b7fe\u00b7le", "komm", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Schlag mir die Tromm,", "tokens": ["Schlag", "mir", "die", "Tromm", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "F\u00fchr mir mein Fritzle", "tokens": ["F\u00fchr", "mir", "mein", "Fritz\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Im Schlittle herum.", "tokens": ["Im", "Schlitt\u00b7le", "he\u00b7rum", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}}}}