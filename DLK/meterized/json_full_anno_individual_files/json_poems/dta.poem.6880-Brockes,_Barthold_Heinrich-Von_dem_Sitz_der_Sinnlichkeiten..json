{"dta.poem.6880": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Sitz der Sinnlichkeiten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach! lasst uns denn, best\u00fcrtzt durch so viel Proben, schliessen,", "tokens": ["Ach", "!", "lasst", "uns", "denn", ",", "be\u00b7st\u00fcrtzt", "durch", "so", "viel", "Pro\u00b7ben", ",", "schlies\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "PPER", "ADV", "$,", "VVFIN", "APPR", "ADV", "PIAT", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df unsre Seelen so gedencken m\u00fcssen,", "tokens": ["Da\u00df", "uns\u00b7re", "See\u00b7len", "so", "ge\u00b7den\u00b7cken", "m\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wann Geister, durch ihr hefftiges Bewegen,", "tokens": ["Wann", "Geis\u00b7ter", ",", "durch", "ihr", "heff\u00b7ti\u00b7ges", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der innern Sinnen Kr\u00e4fft\u2019 erregen.", "tokens": ["Der", "in\u00b7nern", "Sin\u00b7nen", "Kr\u00e4fft'", "er\u00b7re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Doch k\u00f6nnen wir uns nicht beschweren,", "tokens": ["Doch", "k\u00f6n\u00b7nen", "wir", "uns", "nicht", "be\u00b7schwe\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Als ob die Sinnen uns betr\u00fcgen,", "tokens": ["Als", "ob", "die", "Sin\u00b7nen", "uns", "be\u00b7tr\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Sie k\u00f6nnen nicht was wahr, auch nicht was falsch ist, lehren,", "tokens": ["Sie", "k\u00f6n\u00b7nen", "nicht", "was", "wahr", ",", "auch", "nicht", "was", "falsch", "ist", ",", "leh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PWS", "ADJD", "$,", "ADV", "PTKNEG", "PWS", "ADJD", "VAFIN", "$,", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie sind nur blos bewegt. Was sie vom Vorwurff kriegen;", "tokens": ["Sie", "sind", "nur", "blos", "be\u00b7wegt", ".", "Was", "sie", "vom", "Vor\u00b7wurff", "krie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVFIN", "$.", "PWS", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Das geben sie zur\u00fcck. Ob sie, voll Lust und Schrecken", "tokens": ["Das", "ge\u00b7ben", "sie", "zu\u00b7r\u00fcck", ".", "Ob", "sie", ",", "voll", "Lust", "und", "Schre\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "PTKVZ", "$.", "KOUS", "PPER", "$,", "ADJD", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Gem\u00e4hld\u2019 in uns erwecken;", "tokens": ["Ge\u00b7m\u00e4hld'", "in", "uns", "er\u00b7we\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "So mu\u00df der Mensch bem\u00fcht seyn zu ergr\u00fcnden,", "tokens": ["So", "mu\u00df", "der", "Mensch", "be\u00b7m\u00fcht", "seyn", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "VVPP", "VAINF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Das, welches wir durch sie empfinden.", "tokens": ["Das", ",", "wel\u00b7ches", "wir", "durch", "sie", "emp\u00b7fin\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "$,", "PRELS", "PPER", "APPR", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Und unser Urtheil blos allein,", "tokens": ["Und", "un\u00b7ser", "Ur\u00b7theil", "blos", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Ist am Betruge Schuld, wenn wir von Dingen,", "tokens": ["Ist", "am", "Be\u00b7tru\u00b7ge", "Schuld", ",", "wenn", "wir", "von", "Din\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "NN", "$,", "KOUS", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Die unsre Sinnen uns vor Augen stellen,", "tokens": ["Die", "uns\u00b7re", "Sin\u00b7nen", "uns", "vor", "Au\u00b7gen", "stel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Da wir voll Vorurtheil und eitlem Grunde seyn,", "tokens": ["Da", "wir", "voll", "Vor\u00b7urt\u00b7heil", "und", "eit\u00b7lem", "Grun\u00b7de", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "NN", "KON", "ADJA", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "So offte falsche Schl\u00fcsse f\u00e4llen.", "tokens": ["So", "off\u00b7te", "fal\u00b7sche", "Schl\u00fcs\u00b7se", "f\u00e4l\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}