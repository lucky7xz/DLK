{"dta.poem.7775": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Die Geschichte des Davids,  \n K\u00f6nigs in Juda.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-200905198429", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem di\u00df David nun so alles \u00fcberleget,", "tokens": ["In\u00b7dem", "di\u00df", "Da\u00b7vid", "nun", "so", "al\u00b7les", "\u00fc\u00b7ber\u00b7le\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "NE", "ADV", "ADV", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ersieht er wie die Heerd sich \u00fcberall beweget,", "tokens": ["Er\u00b7sieht", "er", "wie", "die", "Heerd", "sich", "\u00fc\u00b7be\u00b7rall", "be\u00b7we\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und da\u00df ein grauser B\u00e4r zu seinem Hertzeleyd", "tokens": ["Und", "da\u00df", "ein", "grau\u00b7ser", "B\u00e4r", "zu", "sei\u00b7nem", "Hert\u00b7ze\u00b7leyd"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein Lamm daraus erwischt mit grosser Hefftigkeit.", "tokens": ["Ein", "Lamm", "da\u00b7raus", "er\u00b7wischt", "mit", "gros\u00b7ser", "Heff\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PAV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der David s\u00e4umet nicht dem Unthier nachzueilen,", "tokens": ["Der", "Da\u00b7vid", "s\u00e4u\u00b7met", "nicht", "dem", "Un\u00b7thier", "nach\u00b7zu\u00b7ei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PTKNEG", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Verfolgt ihn auf der Spur ohn ferneres Verweilen,", "tokens": ["Ver\u00b7folgt", "ihn", "auf", "der", "Spur", "ohn", "fer\u00b7ne\u00b7res", "Ver\u00b7wei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der starcke Gottes-Geist regieret seinen Muth,", "tokens": ["Der", "star\u00b7cke", "Got\u00b7tes\u00b7Geist", "re\u00b7gie\u00b7ret", "sei\u00b7nen", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Da\u00df er in heilgem Grimm was ungemeines thut.", "tokens": ["Da\u00df", "er", "in", "heil\u00b7gem", "Grimm", "was", "un\u00b7ge\u00b7mei\u00b7nes", "thut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NE", "PWS", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er kommt dem B\u00e4ren nah und dieser sieht den Knaben,", "tokens": ["Er", "kommt", "dem", "B\u00e4\u00b7ren", "nah", "und", "die\u00b7ser", "sieht", "den", "Kna\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "KON", "PDS", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Denckt, da\u00df er diesen Raub zu jenem auch will haben,", "tokens": ["Denckt", ",", "da\u00df", "er", "die\u00b7sen", "Raub", "zu", "je\u00b7nem", "auch", "will", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KOUS", "PPER", "PDAT", "NN", "APPR", "PDAT", "ADV", "VMFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Verl\u00e4\u00dft daher das Lamm und geht auf David lo\u00df,", "tokens": ["Ver\u00b7l\u00e4\u00dft", "da\u00b7her", "das", "Lamm", "und", "geht", "auf", "Da\u00b7vid", "lo\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "ART", "NN", "KON", "VVFIN", "APPR", "NE", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Er b\u00e4umt sich vor ihm auf, macht Klau und Rachen gro\u00df,", "tokens": ["Er", "b\u00e4umt", "sich", "vor", "ihm", "auf", ",", "macht", "Klau", "und", "Ra\u00b7chen", "gro\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER", "PTKVZ", "$,", "VVFIN", "NN", "KON", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er fasset David schon; der greifft ihm in den Rachen,", "tokens": ["Er", "fas\u00b7set", "Da\u00b7vid", "schon", ";", "der", "greifft", "ihm", "in", "den", "Ra\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "$.", "ART", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und h\u00e4lt ihn wie ein Lamm: der Starcke kan nichts machen,", "tokens": ["Und", "h\u00e4lt", "ihn", "wie", "ein", "Lamm", ":", "der", "Star\u00b7cke", "kan", "nichts", "ma\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "KOKOM", "ART", "NN", "$.", "ART", "NN", "VMFIN", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Den W\u00fcrger w\u00fcrget er, mit grosser St\u00e4rck und Macht,", "tokens": ["Den", "W\u00fcr\u00b7ger", "w\u00fcr\u00b7get", "er", ",", "mit", "gros\u00b7ser", "St\u00e4rck", "und", "Macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der jenen f\u00e4llen will, wird selbsten umgebracht.", "tokens": ["Der", "je\u00b7nen", "f\u00e4l\u00b7len", "will", ",", "wird", "selbs\u00b7ten", "um\u00b7ge\u00b7bracht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PDS", "VVINF", "VMFIN", "$,", "VAFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Hier sah man Gottes Krafft, die Davids schwachen H\u00e4nden", "tokens": ["Hier", "sah", "man", "Got\u00b7tes", "Krafft", ",", "die", "Da\u00b7vids", "schwa\u00b7chen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "NN", "NN", "$,", "PRELS", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Das ihm entf\u00fchrte Lamm wollt wiederum zuwenden;", "tokens": ["Das", "ihm", "ent\u00b7f\u00fchr\u00b7te", "Lamm", "wollt", "wie\u00b7de\u00b7rum", "zu\u00b7wen\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJA", "NN", "VMFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Und da dasselb aus Furcht f\u00fcr David selbsten l\u00e4uft,", "tokens": ["Und", "da", "das\u00b7selb", "aus", "Furcht", "f\u00fcr", "Da\u00b7vid", "selbs\u00b7ten", "l\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDAT", "APPR", "NN", "APPR", "NE", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Wodurch es die Gefahr unwissend doppelt h\u00e4uft;", "tokens": ["Wo\u00b7durch", "es", "die", "Ge\u00b7fahr", "un\u00b7wis\u00b7send", "dop\u00b7pelt", "h\u00e4uft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Indem es in dem Wald hofft seinen Schutz zu haben,", "tokens": ["In\u00b7dem", "es", "in", "dem", "Wald", "hofft", "sei\u00b7nen", "Schutz", "zu", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "VVFIN", "PPOSAT", "NN", "PTKZU", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Begegnet ihm ein L\u00f6w allda in vollem Traben,", "tokens": ["Be\u00b7geg\u00b7net", "ihm", "ein", "L\u00f6w", "all\u00b7da", "in", "vol\u00b7lem", "Tra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der es auch schier erwischt, wenn Davids sein Get\u00f6n", "tokens": ["Der", "es", "auch", "schier", "er\u00b7wischt", ",", "wenn", "Da\u00b7vids", "sein", "Ge\u00b7t\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADJD", "VVPP", "$,", "KOUS", "NE", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Der auf den L\u00f6wen drang, ihn nicht gemachet stehn.", "tokens": ["Der", "auf", "den", "L\u00f6\u00b7wen", "drang", ",", "ihn", "nicht", "ge\u00b7ma\u00b7chet", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$,", "PPER", "PTKNEG", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der L\u00f6w in Zorn erhitzt, spitzt seine scharffe Klauen,", "tokens": ["Der", "L\u00f6w", "in", "Zorn", "er\u00b7hitzt", ",", "spitzt", "sei\u00b7ne", "scharf\u00b7fe", "Klau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "VVFIN", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und springt auf David zu, der sonder einzigs Grauen", "tokens": ["Und", "springt", "auf", "Da\u00b7vid", "zu", ",", "der", "son\u00b7der", "ein\u00b7zigs", "Grau\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NE", "PTKVZ", "$,", "PRELS", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Den stoltzen Feind erwart, und als ein tapfrer Held", "tokens": ["Den", "stolt\u00b7zen", "Feind", "er\u00b7wart", ",", "und", "als", "ein", "tapf\u00b7rer", "Held"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "$,", "KON", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Jhn bey dem Bart erwischt, und drauf zu Boden f\u00e4llt.", "tokens": ["Jhn", "bey", "dem", "Bart", "er\u00b7wischt", ",", "und", "drauf", "zu", "Bo\u00b7den", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVPP", "$,", "KON", "PAV", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Hie setzt er sich auf ihn, h\u00e4lt ihn mit solcher St\u00e4rcke,", "tokens": ["Hie", "setzt", "er", "sich", "auf", "ihn", ",", "h\u00e4lt", "ihn", "mit", "sol\u00b7cher", "St\u00e4r\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPER", "$,", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$,"], "meter": "-+--+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Da\u00df man gestehen mu\u00df, di\u00df w\u00e4ren L\u00f6wenwercke,", "tokens": ["Da\u00df", "man", "ge\u00b7ste\u00b7hen", "mu\u00df", ",", "di\u00df", "w\u00e4\u00b7ren", "L\u00f6\u00b7wen\u00b7wer\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVINF", "VMFIN", "$,", "PDS", "VAFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Und ehe sich der L\u00f6w kan wieder heben auf,", "tokens": ["Und", "e\u00b7he", "sich", "der", "L\u00f6w", "kan", "wie\u00b7der", "he\u00b7ben", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VMFIN", "ADV", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Hat ihm ein Messer schon gek\u00fcrtzt den Lebenslauf.", "tokens": ["Hat", "ihm", "ein", "Mes\u00b7ser", "schon", "ge\u00b7k\u00fcrtzt", "den", "Le\u00b7bens\u00b7lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Der Held der Thiere liegt dem Helde hin zu F\u00fcssen,", "tokens": ["Der", "Held", "der", "Thie\u00b7re", "liegt", "dem", "Hel\u00b7de", "hin", "zu", "F\u00fcs\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ART", "NN", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Besinget Davids Sieg, mit seinem Blutvergiessen", "tokens": ["Be\u00b7sin\u00b7get", "Da\u00b7vids", "Sieg", ",", "mit", "sei\u00b7nem", "Blut\u00b7ver\u00b7gies\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "NN", "$,", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So ward das Lamm gerett, das der getreue Hirt", "tokens": ["So", "ward", "das", "Lamm", "ge\u00b7rett", ",", "das", "der", "ge\u00b7treu\u00b7e", "Hirt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "VVPP", "$,", "PRELS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Zu der erschrocknen Heerd mit Freuden wieder f\u00fchrt.", "tokens": ["Zu", "der", "er\u00b7schrock\u00b7nen", "Heerd", "mit", "Freu\u00b7den", "wie\u00b7der", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Es ward sein stilles Hertz durch dieses Wundersiegen", "tokens": ["Es", "ward", "sein", "stil\u00b7les", "Hertz", "durch", "die\u00b7ses", "Wun\u00b7der\u00b7sie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "ADJA", "NN", "APPR", "PDAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Nicht frech, noch voller Stoltz, nein, er lie\u00df ihm gen\u00fcgen,", "tokens": ["Nicht", "frech", ",", "noch", "vol\u00b7ler", "Stoltz", ",", "nein", ",", "er", "lie\u00df", "ihm", "ge\u00b7n\u00fc\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "ADV", "ADJA", "NN", "$,", "PTKANT", "$,", "PPER", "VVFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.39": {"text": "Da\u00df er was er bewacht, vom Tode nun befreyt,", "tokens": ["Da\u00df", "er", "was", "er", "be\u00b7wacht", ",", "vom", "To\u00b7de", "nun", "be\u00b7freyt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PWS", "PPER", "VVPP", "$,", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Und sagte niemand nichts, bis zu bequemer Zeit.", "tokens": ["Und", "sag\u00b7te", "nie\u00b7mand", "nichts", ",", "bis", "zu", "be\u00b7que\u00b7mer", "Zeit", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "PIS", "$,", "KOUS", "PTKA", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Er prie\u00df indessen Gott, den Herrscher aller Herren,", "tokens": ["Er", "prie\u00df", "in\u00b7des\u00b7sen", "Gott", ",", "den", "Herr\u00b7scher", "al\u00b7ler", "Her\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "NN", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Der seinen Arm gest\u00e4rckt, zu seines Namens Ehren,", "tokens": ["Der", "sei\u00b7nen", "Arm", "ge\u00b7st\u00e4rckt", ",", "zu", "sei\u00b7nes", "Na\u00b7mens", "Eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$,", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und bat, da\u00df dessen Gnad ihm g\u00e4be Kraft und St\u00e4rck,", "tokens": ["Und", "bat", ",", "da\u00df", "des\u00b7sen", "Gnad", "ihm", "g\u00e4\u00b7be", "Kraft", "und", "St\u00e4rck", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PRELAT", "NN", "PPER", "VVFIN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Nach dieser Zeit zu thun des Allerh\u00f6chsten Werck.", "tokens": ["Nach", "die\u00b7ser", "Zeit", "zu", "thun", "des", "Al\u00b7ler\u00b7h\u00f6chs\u00b7ten", "Werck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PTKZU", "VVINF", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}