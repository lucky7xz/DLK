 "line.15": {"text": "War in dem Saal.", "tokens": ["War", "in", "dem", "Saal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Zu Tisch", "tokens": ["Zu", "Tisch"], "token_info": ["word", "word"], "pos": ["APPR", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.17": {"text": "Manichem edlen Gast", "tokens": ["Ma\u00b7ni\u00b7chem", "ed\u00b7len", "Gast"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Zu groser Reverenze", "tokens": ["Zu", "gro\u00b7ser", "Re\u00b7ve\u00b7ren\u00b7ze"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Ein grose Summ", "tokens": ["Ein", "gro\u00b7se", "Summ"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Der Diensteleut", "tokens": ["Der", "Dien\u00b7ste\u00b7leut"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "Dienten der Schaar,", "tokens": ["Dien\u00b7ten", "der", "Schaar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.22": {"text": "Nach Art h\u00f6felicher Weise.", "tokens": ["Nach", "Art", "h\u00f6\u00b7fe\u00b7li\u00b7cher", "Wei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJA", "NN", "$."], "meter": "++-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.23": {"text": "Als ein End h\u00e4tt' das Mahle,", "tokens": ["Als", "ein", "End", "h\u00e4tt'", "das", "Mah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VAFIN", "ART", "NN", "$,"], "meter"