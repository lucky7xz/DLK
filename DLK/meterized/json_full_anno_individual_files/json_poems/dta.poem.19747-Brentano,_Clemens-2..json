 "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.21": {"text": "In Graben hoch,", "tokens": ["In", "Gra\u00b7ben", "hoch", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Indem ich aufgewachet,", "tokens": ["In\u00b7dem", "ich", "auf\u00b7ge\u00b7wa\u00b7chet", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Und da\u00fccht mir heimlich eben;", "tokens": ["Und", "da\u00fccht", "mir", "heim\u00b7lich", "e\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.24": {"text": "Der Traum bedeut", "tokens": ["Der", "Traum", "be\u00b7deut"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Die Wollust dieser Welte.", "tokens": ["Die", "Wol\u00b7lust", "die\u00b7ser", "Wel\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "Der Pracht, Gewalt und Ruhm", "tokens": ["Der", "Pracht", ",", "Ge\u00b7walt", "und", "Ruhm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Ist als ein Blum", "tokens": ["Ist", "als", "ein", "Blum"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "KOKOM", "ART", "NE"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.28": {"text": "In ihrer Zierde", "tokens": ["In", "ih\u00b7rer", "Zier\u00b7de"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Durch Regen sanft und k\u00fchlen Thau,", "tokens": ["Durch", "Re\u00b7gen", "sanft", "und", "k\u00fch\u00b7len", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Aufw\u00e4chset in dem Felde,", "tokens": ["Auf\u00b7w\u00e4ch\u00b7set", "in", "dem", "Fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "So Reifes Duft", "tokens": ["So", "Rei\u00b7fes", "Duft"], "token_info": ["word", "word", "word"], "pos": ["ADV", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.32": {"text": "Und kalte Luft", "tokens": ["Und", "kal\u00b7te", "Luft"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Geschwind \u00fcber sie thut blasen,", "tokens": ["Ge\u00b7schwind", "\u00fc\u00b7ber", "sie", "thut", "bla\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.34": {"text": "Bald sie verschmoret, dorret", "tokens": ["Bald", "sie", "ver\u00b7schmo\u00b7ret", ",", "dor\u00b7ret"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADV", "PPER", "VVFIN", "$,", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "In der Masen,", "tokens": ["In", "der", "Ma\u00b7sen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "--+-", "measure": "anapaest.init"}, "line.36": {"text": "Reichthum und Kunst,", "tokens": ["Reicht\u00b7hum", "und", "Kunst", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.37": {"text": "Freud, Lieb und Gunst,", "tokens": ["Freud", ",", "Lieb", "und", "Gunst", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Ehr und Gewalte,", "tokens": ["Ehr", "und", "Ge\u00b7wal\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.39": {"text": "Gepr\u00e4ng, Geschmuck und W\u00fcrde,", "tokens": ["Ge\u00b7pr\u00e4ng", ",", "Ge\u00b7schmuck", "und", "W\u00fcr\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Auf dieser Erde aller Stand", "tokens": ["Auf", "die\u00b7ser", "Er\u00b7de", "al\u00b7ler", "Stand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.41": {"text": "Steht es in Gl\u00fcck und bl\u00fchet heut,", "tokens": ["Steht", "es", "in", "Gl\u00fcck", "und", "bl\u00fc\u00b7het", "heut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "So schwindet es doch Morgen ab,", "tokens": ["So", "schwin\u00b7det", "es", "doch", "Mor\u00b7gen", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Und sinket endlich in das Grab,", "tokens": ["Und", "sin\u00b7ket", "end\u00b7lich", "in", "das", "Grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.44": {"text": "Was Fleisch und Blut konnt geben,", "tokens": ["Was", "Fleisch", "und", "Blut", "konnt", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "KON", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Das mu\u00df verderben, sterben", "tokens": ["Das", "mu\u00df", "ver\u00b7der\u00b7ben", ",", "ster\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["PDS", "VMFIN", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Jung und alte", "tokens": ["Jung", "und", "al\u00b7te"], "token_info": ["word", "word", "word"], "pos": ["NE", "KON", "ADJA"], "meter": "+-+-", "measure": "trochaic.di"}, "line.47": {"text": "Mann unde Frau,", "tokens": ["Mann", "un\u00b7de", "Frau", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.48": {"text": "Auf das Verg\u00e4nglich hier nit bau,", "tokens": ["Auf", "das", "Ver\u00b7g\u00e4ng\u00b7lich", "hier", "nit", "bau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PTKNEG", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Das als ein Traume, Schaume", "tokens": ["Das", "als", "ein", "Trau\u00b7me", ",", "Schau\u00b7me"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "KOUS", "ART", "NN", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.50": {"text": "Kommet um;", "tokens": ["Kom\u00b7met", "um", ";"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.51": {"text": "Fleuch, zeuch zum ewgen Leben.", "tokens": ["Fleuch", 