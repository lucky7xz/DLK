{"dta.poem.19747": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519168", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein mal lag ich               ", "tokens": ["Ein", "mal", "lag", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADV", "VVFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "In Schlafes Qual,", "tokens": ["In", "Schla\u00b7fes", "Qual", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mich d\u00e4ucht ich war", "tokens": ["Mich", "d\u00e4ucht", "ich", "war"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Auf einem Berg", "tokens": ["Auf", "ei\u00b7nem", "Berg"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Vor eime k\u00f6niglichen Pallast,", "tokens": ["Vor", "ei\u00b7me", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der war durchhauen pur", "tokens": ["Der", "war", "durch\u00b7hau\u00b7en", "pur"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Nach meisterlichen Sinnen,", "tokens": ["Nach", "meis\u00b7ter\u00b7li\u00b7chen", "Sin\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Bildwerk zierlich", "tokens": ["Bild\u00b7werk", "zier\u00b7lich"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Stand \u00fcberall", "tokens": ["Stand", "\u00fc\u00b7be\u00b7rall"], "token_info": ["word", "word"], "pos": ["NN", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.10": {"text": "Am Pallast stolz,", "tokens": ["Am", "Pal\u00b7last", "stolz", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.11": {"text": "Der war von Marmorquader;", "tokens": ["Der", "war", "von", "Mar\u00b7mor\u00b7qua\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "NE", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Fein war das Dach", "tokens": ["Fein", "war", "das", "Dach"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.13": {"text": "Von Kupfer braun,", "tokens": ["Von", "Kup\u00b7fer", "braun", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Berillen klar", "tokens": ["Be\u00b7ril\u00b7len", "klar"], "token_info": ["word", "word"], "pos": ["NN", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Das Fensterwerk.", "tokens": ["Das", "Fens\u00b7ter\u00b7werk", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.16": {"text": "Zu oberst von der Burg her glast", "tokens": ["Zu", "o\u00b7berst", "von", "der", "Burg", "her", "glast"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "APZR", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.17": {"text": "Von Gold ein Sonnenuhr,", "tokens": ["Von", "Gold", "ein", "Son\u00b7ne\u00b7nuhr", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "G\u00fclden waren die Zinnen.", "tokens": ["G\u00fcl\u00b7den", "wa\u00b7ren", "die", "Zin\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.19": {"text": "Ringweis ich sah", "tokens": ["Ring\u00b7weis", "ich", "sah"], "token_info": ["word", "word", "word"], "pos": ["NN", "PPER", "VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.20": {"text": "Darum einen Zaun", "tokens": ["Da\u00b7rum", "ei\u00b7nen", "Zaun"], "token_info": ["word", "word", "word"], "pos": ["PAV", "ART", "NN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.21": {"text": "Von Zederholz,", "tokens": ["Von", "Ze\u00b7der\u00b7holz", ","], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NE", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.22": {"text": "Die Pforte war Albater.", "tokens": ["Die", "Pfor\u00b7te", "war", "Al\u00b7ba\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Ich trat auf die Schlagbr\u00fccke,", "tokens": ["Ich", "trat", "auf", "die", "Schlag\u00b7br\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--++-", "measure": "iambic.tri.relaxed"}, "line.24": {"text": "Und sah ein Tanz", "tokens": ["Und", "sah", "ein", "Tanz"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.25": {"text": "Von minniglichen Bilden", "tokens": ["Von", "min\u00b7nig\u00b7li\u00b7chen", "Bil\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.26": {"text": "In diesem Pallast sch\u00f6n;", "tokens": ["In", "die\u00b7sem", "Pal\u00b7last", "sch\u00f6n", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Da gieng ich stehn", "tokens": ["Da", "gieng", "ich", "stehn"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "VVINF"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Zu dieser Pforten,", "tokens": ["Zu", "die\u00b7ser", "Pfor\u00b7ten", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.29": {"text": "Und blickte heimlich hinein,", "tokens": ["Und", "blick\u00b7te", "heim\u00b7lich", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "PTKVZ", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.30": {"text": "Die klaren Aeuglein spielten,", "tokens": ["Die", "kla\u00b7ren", "A\u00b7e\u00b7u\u00b7glein", "spiel\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.31": {"text": "Freundliche Wort", "tokens": ["Freund\u00b7li\u00b7che", "Wort"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.32": {"text": "Wurden gehort.", "tokens": ["Wur\u00b7den", "ge\u00b7hort", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "VVPP", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.33": {"text": "Die adelichen Jungen", "tokens": ["Die", "a\u00b7de\u00b7li\u00b7chen", "Jun\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Nach den Trometen (Fl\u00f6ten)", "tokens": ["Nach", "den", "Tro\u00b7me\u00b7ten", "(", "Fl\u00f6\u00b7ten", ")"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "$(", "NN", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.35": {"text": "H\u00f6felich sprungen,", "tokens": ["H\u00f6\u00b7fe\u00b7lich", "sprun\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.36": {"text": "Ihr jedes hat", "tokens": ["Ihr", "je\u00b7des", "hat"], "token_info": ["word", "word", "word"], "pos": ["PPER", "PIAT", "VAFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.37": {"text": "Von Sammt ein Wad,", "tokens": ["Von", "Sammt", "ein", "Wad", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.38": {"text": "Ein k\u00f6stlich Schauben,", "tokens": ["Ein", "k\u00f6st\u00b7lich", "Schau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Ring, Ketten, goldne Borten.", "tokens": ["Ring", ",", "Ket\u00b7ten", ",", "gold\u00b7ne", "Bor\u00b7ten", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.40": {"text": "Heidnisch war der Frauen Geberd,", "tokens": ["Heid\u00b7nisch", "war", "der", "Frau\u00b7en", "Ge\u00b7berd", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "ART", "NN", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.41": {"text": "Darauf jede mit Rosenkr\u00e4nz;", "tokens": ["Da\u00b7rauf", "je\u00b7de", "mit", "Ro\u00b7sen\u00b7kr\u00e4nz", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "APPR", "NE", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.42": {"text": "Der M\u00e4nner f\u00fcrstliches Gewand,", "tokens": ["Der", "M\u00e4n\u00b7ner", "f\u00fcrst\u00b7li\u00b7ches", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.43": {"text": "Von Sammet, Seiden und Taffant,", "tokens": ["Von", "Sam\u00b7met", ",", "Sei\u00b7den", "und", "Taf\u00b7fant", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "Damast und gulden St\u00fccken", "tokens": ["Da\u00b7mast", "und", "gul\u00b7den", "St\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "KON", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.45": {"text": "Von Perlen gl\u00e4nzen, Kr\u00e4nzen", "tokens": ["Von", "Per\u00b7len", "gl\u00e4n\u00b7zen", ",", "Kr\u00e4n\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["APPR", "NN", "VVINF", "$,", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.46": {"text": "Auf den Hauben.", "tokens": ["Auf", "den", "Hau\u00b7ben", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.47": {"text": "Im Herzen mein", "tokens": ["Im", "Her\u00b7zen", "mein"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT"], "meter": "-+-+", "measure": "iambic.di"}, "line.48": {"text": "Dacht, m\u00f6gt ich bei der Schaare sein!", "tokens": ["Dacht", ",", "m\u00f6gt", "ich", "bei", "der", "Schaa\u00b7re", "sein", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.49": {"text": "Ich wolt mich mischen unter sunder", "tokens": ["Ich", "wolt", "mich", "mi\u00b7schen", "un\u00b7ter", "sun\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "APPR", "NE"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.50": {"text": "und that gehn,", "tokens": ["und", "that", "gehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.51": {"text": "Das war mir frei gel\u00fccken.", "tokens": ["Das", "war", "mir", "frei", "ge\u00b7l\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Ich kam hinein,", "tokens": ["Ich", "kam", "hin\u00b7ein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Und sah die Tisch", "tokens": ["Und", "sah", "die", "Tisch"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Mit Pfeler Tuch", "tokens": ["Mit", "Pfe\u00b7ler", "Tuch"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Bedecket all,", "tokens": ["Be\u00b7de\u00b7cket", "all", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PIAT", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Mit Teppich war der Saal geziert,", "tokens": ["Mit", "Tep\u00b7pich", "war", "der", "Saal", "ge\u00b7ziert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-