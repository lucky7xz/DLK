{"dta.poem.8736": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "Xv.  \n  Das Grab im Busento.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Aus den Wassern schallt es Antwort, und in Wirbeln", "tokens": ["Aus", "den", "Was\u00b7sern", "schallt", "es", "Ant\u00b7wort", ",", "und", "in", "Wir\u00b7beln"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "NN", "$,", "KON", "APPR", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Und den Flu\u00df hinauf, hinunter, zieh'n die Schatten", "tokens": ["Und", "den", "Flu\u00df", "hin\u00b7auf", ",", "hin\u00b7un\u00b7ter", ",", "zieh'n", "die", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PTKVZ", "$,", "PTKVZ", "$,", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Die den Alarich beweinen, ihres Volkes besten Todten.", "tokens": ["Die", "den", "A\u00b7la\u00b7rich", "be\u00b7wei\u00b7nen", ",", "ih\u00b7res", "Vol\u00b7kes", "bes\u00b7ten", "Tod\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVINF", "$,", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.3": {"line.1": {"text": "Allzufr\u00fch und fern der Heimath mu\u00dften hier sie ihn", "tokens": ["All\u00b7zu\u00b7fr\u00fch", "und", "fern", "der", "Hei\u00b7math", "mu\u00df\u00b7ten", "hier", "sie", "ihn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "KON", "ADJD", "ART", "NN", "VMFIN", "ADV", "PPER", "PPER"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "W\u00e4hrend noch die Jugendlocken seine Schulter blond", "tokens": ["W\u00e4h\u00b7rend", "noch", "die", "Ju\u00b7gen\u00b7dlo\u00b7cken", "sei\u00b7ne", "Schul\u00b7ter", "blond"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ART", "NN", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Und am Ufer des Busento reihten sie sich um die Wette,", "tokens": ["Und", "am", "U\u00b7fer", "des", "Bu\u00b7sen\u00b7to", "reih\u00b7ten", "sie", "sich", "um", "die", "Wet\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "ART", "NE", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "--+--+--+--+--+-", "measure": "anapaest.tetra.plus"}, "line.2": {"text": "Um die Str\u00f6mung abzuleiten, gruben sie ein frisches Bette.", "tokens": ["Um", "die", "Str\u00f6\u00b7mung", "ab\u00b7zu\u00b7lei\u00b7ten", ",", "gru\u00b7ben", "sie", "ein", "fri\u00b7sches", "Bet\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "VVIZU", "$,", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}, "stanza.5": {"line.1": {"text": "In der wogenleeren H\u00f6hlung w\u00fchlten sie empor die Erde,", "tokens": ["In", "der", "wo\u00b7gen\u00b7lee\u00b7ren", "H\u00f6h\u00b7lung", "w\u00fchl\u00b7ten", "sie", "em\u00b7por", "die", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "ART", "NN", "$,"], "meter": "--+-+-+-+-+-+-+-", "measure": "anapaest.init"}, "line.2": {"text": "Senkten tief hinein den Leichnam, mit der R\u00fcstung, auf", "tokens": ["Senk\u00b7ten", "tief", "hin\u00b7ein", "den", "Leich\u00b7nam", ",", "mit", "der", "R\u00fcs\u00b7tung", ",", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "ADJD", "ADV", "ART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "Deckten dann mit Erde wieder ihn und seine stolze Habe,", "tokens": ["Deck\u00b7ten", "dann", "mit", "Er\u00b7de", "wie\u00b7der", "ihn", "und", "sei\u00b7ne", "stol\u00b7ze", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "ADV", "PPER", "KON", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da\u00df die hohen Stromgew\u00e4chse w\u00fcchsen aus dem Helden-", "tokens": ["Da\u00df", "die", "ho\u00b7hen", "Strom\u00b7ge\u00b7w\u00e4ch\u00b7se", "w\u00fcch\u00b7sen", "aus", "dem", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "APPR", "ART", "TRUNC"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}}, "stanza.7": {"line.1": {"text": "Abgelenkt zum zweyten Male, ward der Flu\u00df herbey-", "tokens": ["Ab\u00b7ge\u00b7lenkt", "zum", "zwey\u00b7ten", "Ma\u00b7le", ",", "ward", "der", "Flu\u00df", "her\u00b7bey"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "APPRART", "ADJA", "NN", "$,", "VAFIN", "ART", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "M\u00e4chtig in ihr altes Bette sch\u00e4umten die Busentowogen.", "tokens": ["M\u00e4ch\u00b7tig", "in", "ihr", "al\u00b7tes", "Bet\u00b7te", "sch\u00e4um\u00b7ten", "die", "Bu\u00b7sen\u00b7to\u00b7wo\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-+--+--+-", "measure": "trochaic.septa.relaxed"}}, "stanza.8": {"line.1": {"text": "Und es sang ein Chor von M\u00e4nnern: Schlaf' in deinen", "tokens": ["Und", "es", "sang", "ein", "Chor", "von", "M\u00e4n\u00b7nern", ":", "Schlaf'", "in", "dei\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$.", "NN", "APPR", "PPOSAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Keines R\u00f6mers schn\u00f6de Habsucht soll dir je dein Grab", "tokens": ["Kei\u00b7nes", "R\u00f6\u00b7mers", "schn\u00f6\u00b7de", "Hab\u00b7sucht", "soll", "dir", "je", "dein", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "ADJA", "NN", "VMFIN", "PPER", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.9": {"line.1": {"text": "Sangen's, und die Lobges\u00e4nge t\u00f6nten fort im Gothen-", "tokens": ["San\u00b7gen's", ",", "und", "die", "Lob\u00b7ge\u00b7s\u00e4n\u00b7ge", "t\u00f6n\u00b7ten", "fort", "im", "Go\u00b7then"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "KON", "ART", "NN", "VVFIN", "PTKVZ", "APPRART", "TRUNC"], "meter": "+---+-+-+-+-+-", "measure": "dactylic.init"}, "line.2": {"text": "W\u00e4lze sie, Busentowelle, w\u00e4lze sie von Meer zu Meere!", "tokens": ["W\u00e4l\u00b7ze", "sie", ",", "Bu\u00b7sen\u00b7to\u00b7wel\u00b7le", ",", "w\u00e4l\u00b7ze", "sie", "von", "Meer", "zu", "Mee\u00b7re", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "VVFIN", "PPER", "APPR", "NN", "APPR", "NN", "$."], "meter": "+--+----+-+-+-+-", "measure": "dactylic.di.plus"}}}}}