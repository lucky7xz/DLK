{"textgrid.poem.63507": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "Vorklang", "genre": "verse", "period": "N.A.", "pub_year": 1872, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "R\u00fchrt mich heut so seltsam wieder", "tokens": ["R\u00fchrt", "mich", "heut", "so", "selt\u00b7sam", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit langverschollnem Klang", "tokens": ["Wie", "mit", "lang\u00b7ver\u00b7scholl\u00b7nem", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser leichten Jugendlieder", "tokens": ["Die\u00b7ser", "leich\u00b7ten", "Ju\u00b7gend\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfverworrner Herzensdrang?", "tokens": ["S\u00fc\u00df\u00b7ver\u00b7worr\u00b7ner", "Her\u00b7zens\u00b7drang", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rauscht noch einmal auf die Welle", "tokens": ["Rauscht", "noch", "ein\u00b7mal", "auf", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des versunknen Jugendborns", "tokens": ["Des", "ver\u00b7sun\u00b7knen", "Ju\u00b7gend\u00b7borns"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dazwischen silberhelle", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "sil\u00b7ber\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "VVFIN"], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Widerhall des Wunderhorns?", "tokens": ["Wi\u00b7der\u00b7hall", "des", "Wun\u00b7der\u00b7horns", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und ich lausche halb verwundert,", "tokens": ["Und", "ich", "lau\u00b7sche", "halb", "ver\u00b7wun\u00b7dert", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Halb belustigt und erfreut.", "tokens": ["Halb", "be\u00b7lus\u00b7tigt", "und", "er\u00b7freut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "KON", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "War ich doch ein halb Jahrhundert", "tokens": ["War", "ich", "doch", "ein", "halb", "Jahr\u00b7hun\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "ADJD", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "J\u00fcnger dazumal als heut.", "tokens": ["J\u00fcn\u00b7ger", "da\u00b7zu\u00b7mal", "als", "heut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "KOKOM", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Manches h\u00f6r' ich euch erz\u00e4hlen,", "tokens": ["Man\u00b7ches", "h\u00f6r'", "ich", "euch", "er\u00b7z\u00e4h\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was ich damals lieblich fand,", "tokens": ["Was", "ich", "da\u00b7mals", "lieb\u00b7lich", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was mit euch, ihr Liederseelen,", "tokens": ["Was", "mit", "euch", ",", "ihr", "Lie\u00b7der\u00b7see\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Traumhaft wieder auferstand.", "tokens": ["Traum\u00b7haft", "wie\u00b7der", "auf\u00b7er\u00b7stand", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Spukt ihr nun am hellen Tage", "tokens": ["Spukt", "ihr", "nun", "am", "hel\u00b7len", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mit verwundertem Gesicht,", "tokens": ["Mit", "ver\u00b7wun\u00b7der\u00b7tem", "Ge\u00b7sicht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Merkt zu sp\u00e4t: die Mitwelt frage", "tokens": ["Merkt", "zu", "sp\u00e4t", ":", "die", "Mit\u00b7welt", "fra\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVIMP", "PTKA", "ADJD", "$.", "ART", "NN", "VVFIN"], "meter": "+-+-++--", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Nach Gespensterlyrik nicht?", "tokens": ["Nach", "Ge\u00b7spens\u00b7ter\u00b7ly\u00b7rik", "nicht", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Andre Zeiten, andre Lieder;", "tokens": ["And\u00b7re", "Zei\u00b7ten", ",", "and\u00b7re", "Lie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unausf\u00fcllbar ist die Kluft.", "tokens": ["Un\u00b7aus\u00b7f\u00fcll\u00b7bar", "ist", "die", "Kluft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kehrt getrost zum Schlafen wieder", "tokens": ["Kehrt", "ge\u00b7trost", "zum", "Schla\u00b7fen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "APPRART", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heim in die Familiengruft.", "tokens": ["Heim", "in", "die", "Fa\u00b7mi\u00b7li\u00b7en\u00b7gruft", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.7": {"line.1": {"text": "Aber wie? Ich h\u00f6r' euch bitten:", "tokens": ["A\u00b7ber", "wie", "?", "Ich", "h\u00f6r'", "euch", "bit\u00b7ten", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "$.", "PPER", "VVFIN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df uns, Alter, hier im Licht!", "tokens": ["La\u00df", "uns", ",", "Al\u00b7ter", ",", "hier", "im", "Licht", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "NN", "$,", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zwar nicht allzu wohlgelitten,", "tokens": ["Zwar", "nicht", "all\u00b7zu", "wohl\u00b7ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "PTKA", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aber tot sind wir noch nicht.", "tokens": ["A\u00b7ber", "tot", "sind", "wir", "noch", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VAFIN", "PPER", "ADV", "PTKNEG", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wirst uns nicht zur Ruhe bringen,", "tokens": ["Wirst", "uns", "nicht", "zur", "Ru\u00b7he", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denn nicht alle sind wir stumm.", "tokens": ["Denn", "nicht", "al\u00b7le", "sind", "wir", "stumm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "PIS", "VAFIN", "PPER", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Gehn doch auf Gesangesschwingen", "tokens": ["Gehn", "doch", "auf", "Ge\u00b7san\u00b7ges\u00b7schwin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser viele noch herum. \u2013", "tokens": ["Un\u00b7ser", "vie\u00b7le", "noch", "he\u00b7rum", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PIS", "ADV", "PTKVZ", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Nicht verhallen soll vergebens", "tokens": ["Nicht", "ver\u00b7hal\u00b7len", "soll", "ver\u00b7ge\u00b7bens"], "token_info": ["word", "word", "word", "word"], "pos": ["PTKNEG", "VVINF", "VMFIN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kindesflehn am Vaterohr;", "tokens": ["Kin\u00b7des\u00b7flehn", "am", "Va\u00b7te\u00b7rohr", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "So erfreut euch denn des Lebens,", "tokens": ["So", "er\u00b7freut", "euch", "denn", "des", "Le\u00b7bens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Das in euch noch quillt empor.", "tokens": ["Das", "in", "euch", "noch", "quillt", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "PPER", "ADV", "VVFIN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "Ach, ich bin aus guten Gr\u00fcnden", "tokens": ["Ach", ",", "ich", "bin", "aus", "gu\u00b7ten", "Gr\u00fcn\u00b7den"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gegen euch, ihr Kleinen, schwach!", "tokens": ["Ge\u00b7gen", "euch", ",", "ihr", "Klei\u00b7nen", ",", "schwach", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rufen uns nicht Jugends\u00fcnden", "tokens": ["Ru\u00b7fen", "uns", "nicht", "Ju\u00b7gend\u00b7s\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PTKNEG", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unsre Jugend wieder wach?", "tokens": ["Uns\u00b7re", "Ju\u00b7gend", "wie\u00b7der", "wach", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "R\u00fchrt mich heut so seltsam wieder", "tokens": ["R\u00fchrt", "mich", "heut", "so", "selt\u00b7sam", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "ADV", "ADJD", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie mit langverschollnem Klang", "tokens": ["Wie", "mit", "lang\u00b7ver\u00b7scholl\u00b7nem", "Klang"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieser leichten Jugendlieder", "tokens": ["Die\u00b7ser", "leich\u00b7ten", "Ju\u00b7gend\u00b7lie\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["PDAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "S\u00fc\u00dfverworrner Herzensdrang?", "tokens": ["S\u00fc\u00df\u00b7ver\u00b7worr\u00b7ner", "Her\u00b7zens\u00b7drang", "?"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Rauscht noch einmal auf die Welle", "tokens": ["Rauscht", "noch", "ein\u00b7mal", "auf", "die", "Wel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Des versunknen Jugendborns", "tokens": ["Des", "ver\u00b7sun\u00b7knen", "Ju\u00b7gend\u00b7borns"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und dazwischen silberhelle", "tokens": ["Und", "da\u00b7zwi\u00b7schen", "sil\u00b7ber\u00b7hel\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["KON", "PAV", "VVF