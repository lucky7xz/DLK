{"dta.poem.8021": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Das Schifflein .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Schifflein ziehet leise", "tokens": ["Ein", "Schif\u00b7flein", "zie\u00b7het", "lei\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Den Strom hin seine Gleise.", "tokens": ["Den", "Strom", "hin", "sei\u00b7ne", "Glei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Es schweigen, die drin wandern,", "tokens": ["Es", "schwei\u00b7gen", ",", "die", "drin", "wan\u00b7dern", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Denn Keiner kennt den Andern.", "tokens": ["Denn", "Kei\u00b7ner", "kennt", "den", "An\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "ART", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Was zieht hier aus dem Felle", "tokens": ["Was", "zieht", "hier", "aus", "dem", "Fel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der braune Waidgeselle?", "tokens": ["Der", "brau\u00b7ne", "Waid\u00b7ge\u00b7sel\u00b7le", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Horn, das sanft erschallet;", "tokens": ["Ein", "Horn", ",", "das", "sanft", "er\u00b7schal\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Ufer wiederhallet.", "tokens": ["Das", "U\u00b7fer", "wie\u00b7der\u00b7hal\u00b7let", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Von seinem Wanderstabe", "tokens": ["Von", "sei\u00b7nem", "Wan\u00b7der\u00b7sta\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schraubt Jener Stift und Habe,", "tokens": ["Schraubt", "Je\u00b7ner", "Stift", "und", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und mischt mit Fl\u00f6tent\u00f6nen", "tokens": ["Und", "mischt", "mit", "Fl\u00f6\u00b7ten\u00b7t\u00f6\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sich in des Hornes Dr\u00f6hnen.", "tokens": ["Sich", "in", "des", "Hor\u00b7nes", "Dr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Das M\u00e4dchen sa\u00df so bl\u00f6de,", "tokens": ["Das", "M\u00e4d\u00b7chen", "sa\u00df", "so", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Als fehlt\u2019 ihr gar die Rede,", "tokens": ["Als", "fehlt'", "ihr", "gar", "die", "Re\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Jetzt stimmt sie mit Gesange", "tokens": ["Jetzt", "stimmt", "sie", "mit", "Ge\u00b7san\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu Horn und Fl\u00f6tenklange.", "tokens": ["Zu", "Horn", "und", "Fl\u00f6\u00b7ten\u00b7klan\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "KON", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Rudrer auch sich regen", "tokens": ["Die", "Rud\u00b7rer", "auch", "sich", "re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PRF", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mit taktgem\u00e4\u00dfen Schl\u00e4gen.", "tokens": ["Mit", "takt\u00b7ge\u00b7m\u00e4\u00b7\u00dfen", "Schl\u00e4\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das Schiff hinunter flieget", "tokens": ["Das", "Schiff", "hin\u00b7un\u00b7ter", "flie\u00b7get"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "APPO", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Von Melodie gewieget.", "tokens": ["Von", "Me\u00b7lo\u00b7die", "ge\u00b7wie\u00b7get", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Hart st\u00f6\u00dft es auf am Strande,", "tokens": ["Hart", "st\u00f6\u00dft", "es", "auf", "am", "Stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Man trennt sich in die Lande.", "tokens": ["Man", "trennt", "sich", "in", "die", "Lan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wann treffen wir uns, Br\u00fcder!", "tokens": ["Wann", "tref\u00b7fen", "wir", "uns", ",", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$,", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf Einem Schifflein wieder?", "tokens": ["Auf", "Ei\u00b7nem", "Schif\u00b7flein", "wie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}