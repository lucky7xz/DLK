{"dta.poem.7680": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "52.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Zu lehren glaubt' ich oft, was ich an mir erfuhr,", "tokens": ["Zu", "leh\u00b7ren", "glaubt'", "ich", "oft", ",", "was", "ich", "an", "mir", "er\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "VVFIN", "PPER", "ADV", "$,", "PWS", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sah dann: ich umschrieb ein altes Sprichwort nur.", "tokens": ["Und", "sah", "dann", ":", "ich", "um\u00b7schrieb", "ein", "al\u00b7tes", "Sprich\u00b7wort", "nur", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das eben ist die Art des Sprichworts: wir gewahren", "tokens": ["Das", "e\u00b7ben", "ist", "die", "Art", "des", "Sprich\u00b7worts", ":", "wir", "ge\u00b7wah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "ADV", "VAFIN", "ART", "NN", "ART", "NN", "$.", "PPER", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Erst seinen Sinn, wenn wir ihn an uns selbst erfahren.", "tokens": ["Erst", "sei\u00b7nen", "Sinn", ",", "wenn", "wir", "ihn", "an", "uns", "selbst", "er\u00b7fah\u00b7ren", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT"