{"textgrid.poem.56769": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Mann mit einer Sense tritt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Mann mit einer Sense tritt", "tokens": ["Ein", "Mann", "mit", "ei\u00b7ner", "Sen\u00b7se", "tritt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "zur D\u00e4mmerzeit beim Dorfschmied ein.", "tokens": ["zur", "D\u00e4m\u00b7mer\u00b7zeit", "beim", "Dorf\u00b7schmied", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der schl\u00e4gt sie fester an den Stiel", "tokens": ["Der", "schl\u00e4gt", "sie", "fes\u00b7ter", "an", "den", "Stiel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "und dengelt sie und schleift sie scharf", "tokens": ["und", "den\u00b7gelt", "sie", "und", "schleift", "sie", "scharf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "KON", "VVFIN", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "und gibt sie frohen Spruchs zur\u00fcck", "tokens": ["und", "gibt", "sie", "fro\u00b7hen", "Spruchs", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "und fr\u00e4gt sein wer? woher? wohin?", "tokens": ["und", "fr\u00e4gt", "sein", "wer", "?", "wo\u00b7her", "?", "wo\u00b7hin", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "PWS", "$.", "PWAV", "$.", "PWAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "und lauscht dem Fremden offnen Munds,", "tokens": ["und", "lauscht", "dem", "Frem\u00b7den", "off\u00b7nen", "Munds", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "als der ihm dies und das erz\u00e4hlt.", "tokens": ["als", "der", "ihm", "dies", "und", "das", "er\u00b7z\u00e4hlt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPER", "PDS", "KON", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und wie die Rede irrt und kreist,", "tokens": ["Und", "wie", "die", "Re\u00b7de", "irrt", "und", "kreist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "ber\u00fchrt sie auch das letzte Los,", "tokens": ["be\u00b7r\u00fchrt", "sie", "auch", "das", "letz\u00b7te", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "das jedem f\u00e4llt, und \u2013 \u00bbUnverhofft!", "tokens": ["das", "je\u00b7dem", "f\u00e4llt", ",", "und", "\u2013", "\u00bb", "Un\u00b7ver\u00b7hofft", "!"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "punct"], "pos": ["PDS", "PIS", "VVFIN", "$,", "KON", "$(", "$(", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12":