{"textgrid.poem.49459": {"metadata": {"author": {"name": "Verlaine, Paul-Marie", "birth": "N.A.", "death": "N.A."}, "title": "1L: Mit hohlen Augen, wild, mit starren Br\u00fcsten,", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mit hohlen Augen, wild, mit starren Br\u00fcsten,", "tokens": ["Mit", "hoh\u00b7len", "Au\u00b7gen", ",", "wild", ",", "mit", "star\u00b7ren", "Br\u00fcs\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Eilt Sappho, die die Gluten ihres Wunschs verzehren,", "tokens": ["Eilt", "Sap\u00b7pho", ",", "die", "die", "Glu\u00b7ten", "ih\u00b7res", "Wunschs", "ver\u00b7zeh\u00b7ren", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "$,", "PRELS", "ART", "NN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Gleich einer W\u00f6lfin l\u00e4ngs der eis'gen K\u00fcsten.", "tokens": ["Gleich", "ei\u00b7ner", "W\u00f6l\u00b7fin", "l\u00e4ngs", "der", "eis'\u00b7gen", "K\u00fcs\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Von Phaon tr\u00e4umt sie, nicht von den Alt\u00e4ren,", "tokens": ["Von", "Phaon", "tr\u00e4umt", "sie", ",", "nicht", "von", "den", "Al\u00b7t\u00e4\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$,", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und da verschm\u00e4ht sie sieht der Sehnsucht bittre Tr\u00e4nen,", "tokens": ["Und", "da", "ver\u00b7schm\u00e4ht", "sie", "sieht", "der", "Sehn\u00b7sucht", "bitt\u00b7re", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Reisst sie ihr Haar sich aus in langen, \u00fcpp'gen Str\u00e4hnen.", "tokens": ["Reisst", "sie", "ihr", "Haar", "sich", "aus", "in", "lan\u00b7gen", ",", "\u00fcpp'\u00b7gen", "Str\u00e4h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "PRF", "APPR", "APPR", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In sehnsuchtsvoller Reue ruft sie schmerzlich jene", "tokens": ["In", "sehn\u00b7suchts\u00b7vol\u00b7ler", "Reu\u00b7e", "ruft", "sie", "schmerz\u00b7lich", "je\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "PDAT"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entschwundnen Tage junger Glut, die allzuschnellen,", "tokens": ["Ent\u00b7schwund\u00b7nen", "Ta\u00b7ge", "jun\u00b7ger", "Glut", ",", "die", "all\u00b7zu\u00b7schnel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "$,", "PRELS", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die sie in s\u00fcssen Liedern sang, die sich gesellen", "tokens": ["Die", "sie", "in", "s\u00fcs\u00b7sen", "Lie\u00b7dern", "sang", ",", "die", "sich", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "ADJA", "NN", "VVFIN", "$,", "PRELS", "PRF", "VVPP"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Jungfrau reinem Traum, dass sie sich selig w\u00e4hne.", "tokens": ["Der", "Jung\u00b7frau", "rei\u00b7nem", "Traum", ",", "dass", "sie", "sich", "se\u00b7lig", "w\u00e4h\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KOUS", "PPER", "PRF", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ihr tiefumschattet Augenlid verbirgt die Tr\u00e4ne,", "tokens": ["Ihr", "tie\u00b7fum\u00b7schat\u00b7tet", "Au\u00b7gen\u00b7lid", "ver\u00b7birgt", "die", "Tr\u00e4\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und auf der Moira Ruf st\u00fcrzt sie sich in die Wellen,", "tokens": ["Und", "auf", "der", "Moi\u00b7ra", "Ruf", "st\u00fcrzt", "sie", "sich", "in", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "PPER", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und silbern gl\u00e4nzt, die schwarzen Wasser zu erhellen,", "tokens": ["Und", "sil\u00b7bern", "gl\u00e4nzt", ",", "die", "schwar\u00b7zen", "Was\u00b7ser", "zu", "er\u00b7hel\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "$,", "ART", "AD