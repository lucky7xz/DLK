ord"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr gantzes Himmelreich,", "tokens": ["Ihr", "gant\u00b7zes", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag hie gekr\u00f6hnet werden,", "tokens": ["Mag", "hie", "ge\u00b7kr\u00f6h\u00b7net", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Frommen gilt es gleich,", "tokens": ["Den", "From\u00b7men", "gilt", "es", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie lassen Gott stets walten", "tokens": ["Sie", "las\u00b7sen", "Gott", "stets", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Tr\u00fcbsal, Schmach vnd Hohn,", "tokens": ["In", "Tr\u00fcb\u00b7sal", ",", "Schmach", "vnd", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn sie nur dort erhalten", "tokens": ["Wenn", "sie", "nur", "dort", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die ewig' Ehren-Krohn.", "tokens": ["Die", "e\u00b7wig'", "Eh\u00b7ren\u00b7Krohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}