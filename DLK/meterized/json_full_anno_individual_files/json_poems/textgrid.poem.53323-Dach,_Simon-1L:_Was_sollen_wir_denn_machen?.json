{"textgrid.poem.53323": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was sollen wir denn machen?", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was sollen wir denn machen?", "tokens": ["Was", "sol\u00b7len", "wir", "denn", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hie hilfft kein wiederstandt.", "tokens": ["Hie", "hilfft", "kein", "wie\u00b7der\u00b7standt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir, Welt vnd alle Sachen", "tokens": ["Wir", ",", "Welt", "vnd", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sind vnter Gottes Handt.", "tokens": ["Sind", "vn\u00b7ter", "Got\u00b7tes", "Handt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Mit Gotte wollen rechten", "tokens": ["Mit", "Got\u00b7te", "wol\u00b7len", "rech\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ist vnvernunfft vnd Schuld,", "tokens": ["Ist", "vn\u00b7ver\u00b7nunfft", "vnd", "Schuld", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wer wieder Ihn wil fechten,", "tokens": ["Wer", "wie\u00b7der", "Ihn"