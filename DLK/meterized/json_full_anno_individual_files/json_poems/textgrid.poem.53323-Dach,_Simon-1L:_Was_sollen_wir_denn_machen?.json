 ["Bey", "sei\u00b7ner", "G\u00fc\u00b7te", "stat", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Da\u00df wir noch fr\u00f6lich schneiden", "tokens": ["Da\u00df", "wir", "noch", "fr\u00f6\u00b7lich", "schnei\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die heisse Thr\u00e4nen-Saat.", "tokens": ["Die", "heis\u00b7se", "Thr\u00e4\u00b7nen\u00b7Saat", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Er schl\u00e4gt vns nie von Hertzen", "tokens": ["Er", "schl\u00e4gt", "vns", "nie", "von", "Hert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vnd sieht aus lieber Trew,", "tokens": ["Vnd", "sieht", "aus", "lie\u00b7ber", "Trew", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Da\u00df vnsre Krafft den Schmertzen", "tokens": ["Da\u00df", "vns\u00b7re", "Krafft", "den", "Schmert\u00b7zen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch gnug gewachsen sey,", "tokens": ["Auch", "gnug", "ge\u00b7wach\u00b7sen", "sey", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Er wird vns Rettung senden:", "tokens": ["Er", "wird", "vns", "Ret\u00b7tung", "sen\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Das Leiden dieser Zeit,", "tokens": ["Das", "Lei\u00b7den", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wie lang es w\u00e4hret, wenden", "tokens": ["Wie", "lang", "es", "w\u00e4h\u00b7ret", ",", "wen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "ADJD", "PPER", "VVFIN", "$,", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In ewig' Herrlicheit.", "tokens": ["In", "e\u00b7wig'", "Herr\u00b7li\u00b7cheit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Die Bo\u00dfheit hab' auff Erden", "tokens": ["Die", "Bo\u00df\u00b7heit", "hab'", "auff", "Er\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ihr gantzes Himmelreich,", "tokens": ["Ihr", "gant\u00b7zes", "Him\u00b7mel\u00b7reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mag hie gekr\u00f6hnet werden,", "tokens": ["Mag", "hie", "ge\u00b7kr\u00f6h\u00b7net", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "VVPP", "VAINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Frommen gilt es gleich,", "tokens": ["Den", "From\u00b7men", "gilt", "es", "gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Sie lassen Gott stets walten", "tokens": ["Sie", "las\u00b7sen", "Gott", "stets", "wal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "In Tr\u00fcbsal, Schmach vnd Hohn,", "tokens": ["In", "Tr\u00fcb\u00b7sal", ",", "Schmach", "vnd", "Hohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Wenn sie nur dort erhalten", "tokens": ["Wenn", "sie", "nur", "dort", "er\u00b7hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Die ewig' Ehren-Krohn.", "tokens": ["Die", "e\u00b7wig'", "Eh\u00b7ren\u00b7Krohn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Was sollen wir denn machen?", "tokens": ["Was", "sol\u00b7len", "wir", "denn", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Hie hilfft kein wiederstandt.", "tokens": ["Hie", "hilfft", "kein", "wie\u00b7der\u00b7standt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir, Welt vnd alle Sachen", "tokens": ["Wir", ",", "Welt", "vnd", "al\u00b7le", "Sa\u00b7chen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"te