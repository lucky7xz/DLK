{"textgrid.poem.63943": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der abgebrannte Bauer", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Dem feindlichen Geschick zum Trutz", "tokens": ["Dem", "feind\u00b7li\u00b7chen", "Ge\u00b7schick", "zum", "Trutz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mach' selbst das Ungl\u00fcck dir zu Nutz!", "tokens": ["Mach'", "selbst", "das", "Un\u00b7gl\u00fcck", "dir", "zu", "Nutz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Bei einem starken Winterfrost", "tokens": ["Bei", "ei\u00b7nem", "star\u00b7ken", "Win\u00b7ter\u00b7frost"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bei geringer schmaler Kost", "tokens": ["Und", "bei", "ge\u00b7rin\u00b7ger", "schma\u00b7ler", "Kost"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Behalf ein armer Bauer sich", "tokens": ["Be\u00b7half", "ein", "ar\u00b7mer", "Bau\u00b7er", "sich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Gar elend und gar j\u00e4mmerlich.", "tokens": ["Gar", "e\u00b7lend", "und", "gar", "j\u00e4m\u00b7mer\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem ward von B\u00f6sewichtes Hand", "tokens": ["Dem", "ward", "von", "B\u00f6\u00b7se\u00b7wich\u00b7tes", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sein kleines H\u00e4uschen angebrannt.", "tokens": ["Sein", "klei\u00b7nes", "H\u00e4usc\u00b7hen", "an\u00b7ge\u00b7brannt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Er lief hinaus. Die helle Gluth", "tokens": ["Er", "lief", "hin\u00b7aus", ".", "Die", "hel\u00b7le", "Gluth"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Nahm \u00fcberhand. Der Nachbarn Muth", "tokens": ["Nahm", "\u00fc\u00b7ber\u00b7hand", ".", "Der", "Nach\u00b7barn", "Muth"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "VVFIN", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Half ihm zwar treulich; doch zuletzt", "tokens": ["Half", "ihm", "zwar", "treu\u00b7lich", ";", "doch", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ADJD", "$.", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ward alles L\u00f6schen ausgesetzt,", "tokens": ["Ward", "al\u00b7les", "L\u00f6\u00b7schen", "aus\u00b7ge\u00b7setzt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Da bei stets wachsender Gefahr", "toke