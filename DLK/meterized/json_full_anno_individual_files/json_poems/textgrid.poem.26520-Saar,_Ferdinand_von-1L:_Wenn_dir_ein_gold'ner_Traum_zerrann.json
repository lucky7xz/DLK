{"textgrid.poem.26520": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wenn dir ein gold'ner Traum zerrann", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir ein gold'ner Traum zerrann", "tokens": ["Wenn", "dir", "ein", "gold'\u00b7ner", "Traum", "zer\u00b7rann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und rauh die Wirklichkeit dein Herz zerspaltet,", "tokens": ["Und", "rauh", "die", "Wirk\u00b7lich\u00b7keit", "dein", "Herz", "zer\u00b7spal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Nicht mit dem Schicksal had're dann,", "tokens": ["Nicht", "mit", "dem", "Schick\u00b7sal", "ha\u00b7d'\u00b7re", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Das doch nur in dir selber lebt und waltet.", "tokens": ["Das", "doch", "nur", "in", "dir", "sel\u00b7ber", "lebt", "und", "wal\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "APPR", "PPER", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wie sehr man dir auch weh' gethan,", "tokens": ["Wie", "sehr", "man", "dir", "auch", "weh'", "ge\u00b7than", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PIS", "PP