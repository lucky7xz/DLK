{"dta.poem.21514": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Emp\u00f6rung .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Manchmal ist\u2019s mir, als packte mich ein Krampf,", "tokens": ["Manch\u00b7mal", "ist's", "mir", ",", "als", "pack\u00b7te", "mich", "ein", "Krampf", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Wenn ich halbm\u00fcde, halbverdrossen,", "tokens": ["Wenn", "ich", "halb\u00b7m\u00fc\u00b7de", ",", "halb\u00b7ver\u00b7dros\u00b7sen", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vertr\u00e4umt, mechanisch dem Gew\u00f6lk nachstarre,", "tokens": ["Ver\u00b7tr\u00e4umt", ",", "me\u00b7cha\u00b7nisch", "dem", "Ge\u00b7w\u00f6lk", "nach\u00b7star\u00b7re", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das sich in zarten, duftig blauen Ringen", "tokens": ["Das", "sich", "in", "zar\u00b7ten", ",", "duf\u00b7tig", "blau\u00b7en", "Rin\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "PRF", "APPR", "ADJA", "$,", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Von der Cigarre m\u00e4hlich l\u00f6st . . . . \u2014 \u2014 \u2014:", "tokens": ["Von", "der", "Ci\u00b7gar\u00b7re", "m\u00e4h\u00b7lich", "l\u00f6st", ".", ".", ".", ".", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$.", "$.", "$.", "$.", "$(", "$(", "$(", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ist es mir, als packte mich ein Krampf \u2014", "tokens": ["Da", "ist", "es", "mir", ",", "als", "pack\u00b7te", "mich", "ein", "Krampf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "$,", "KOUS", "VVFIN", "PPER", "ART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als schl\u00fcg\u2019 an\u2019s Ohr mir dr\u00f6hnend Ro\u00dfgestampf \u2014", "tokens": ["Als", "schl\u00fcg'", "an's", "Ohr", "mir", "dr\u00f6h\u00b7nend", "Ro\u00df\u00b7ge\u00b7stampf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PPER", "ADJD", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Als schl\u00fcg\u2019 an\u2019s Ohr mir gellend Horngeschmetter \u2014", "tokens": ["Als", "schl\u00fcg'", "an's", "Ohr", "mir", "gel\u00b7lend", "Horn\u00b7ge\u00b7schmet\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPRART", "NN", "PPER", "VVPP", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Als riefe mich Posaunenton zum Kampf", "tokens": ["Als", "rie\u00b7fe", "mich", "Po\u00b7sau\u00b7nen\u00b7ton", "zum", "Kampf"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VVFIN", "PPER", "NE", "APPRART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "F\u00fcr einen neuen Heiland \u2014 einen neuen Retter!", "tokens": ["F\u00fcr", "ei\u00b7nen", "neu\u00b7en", "Hei\u00b7land", "ei\u00b7nen", "neu\u00b7en", "Ret\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$(", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "In wilden Rhythmen pulst mein Blut \u2014", "tokens": ["In", "wil\u00b7den", "Rhyth\u00b7men", "pulst", "mein", "Blut"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aufschwillt mir jauchzender Titanenmuth \u2014", "tokens": ["Auf\u00b7schwillt", "mir", "jauch\u00b7zen\u00b7der", "Ti\u00b7ta\u00b7nen\u00b7muth"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Erstickt liegt der ", "tokens": ["Er\u00b7stickt", "liegt", "der"], "token_info": ["word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART"], "meter": "-+--", "measure": "dactylic.init"}, "line.4": {"text": "Und wirbelt auseinander wie der Bl\u00e4tter", "tokens": ["Und", "wir\u00b7belt", "aus\u00b7ein\u00b7an\u00b7der", "wie", "der", "Bl\u00e4t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PTKVZ", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Zerm\u00fcrbte Spreu im Herbststurmtosen! . . . . .", "tokens": ["Zer\u00b7m\u00fcrb\u00b7te", "Spreu", "im", "Herbst\u00b7sturm\u00b7to\u00b7sen", "!", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["VVFIN", "NN", "APPRART", "NN", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich lebe nur der ", "tokens": ["Ich", "le\u00b7be", "nur", "der"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Und ihre Rosen", "tokens": ["Und", "ih\u00b7re", "Ro\u00b7sen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Bl\u00fch\u2019n auf in meiner qualzerspaltenen Brust . . . . . .", "tokens": ["Bl\u00fch'n", "auf", "in", "mei\u00b7ner", "qual\u00b7zer\u00b7spal\u00b7te\u00b7nen", "Brust", ".", ".", ".", ".", ".", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["NN", "APPR", "APPR", "PPOSAT", "ADJA", "NN", "$.", "$.", "$.", "$.", "$.", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.9": {"text": "Hei! Wilde G\u00f6tterlust,", "tokens": ["Hei", "!", "Wil\u00b7de", "G\u00f6t\u00b7ter\u00b7lust", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Auf d\u00fcrrem Haidepfad", "tokens": ["Auf", "d\u00fcr\u00b7rem", "Hai\u00b7de\u00b7pfad"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Dahinzufliegen!", "tokens": ["Da\u00b7hin\u00b7zu\u00b7flie\u00b7gen", "!"], "token_info": ["word", "punct"], "pos": ["PAV", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Es dampft das Ro\u00df \u2014 und in die Locken w\u00fchlt", "tokens": ["Es", "dampft", "das", "Ro\u00df", "und", "in", "die", "Lo\u00b7cken", "w\u00fchlt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "KON", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.13": {"text": "Der Sturm sich ein \u2014 \u2014", "tokens": ["Der", "Sturm", "sich", "ein"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PRF", "ART", "$(", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.14": {"text": "Gespenstisch liegen", "tokens": ["Ge\u00b7spens\u00b7tisch", "lie\u00b7gen"], "token_info": ["word", "word"], "pos": ["NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Des Mondes glei\u00dfend wei\u00dfe Silberschleier", "tokens": ["Des", "Mon\u00b7des", "glei\u00b7\u00dfend", "wei\u00b7\u00dfe", "Sil\u00b7ber\u00b7schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "In fahl cristall\u2019nem Schein", "tokens": ["In", "fahl", "cris\u00b7tall'\u00b7nem", "Schein"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADV", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Weit ausgespannt", "tokens": ["Weit", "aus\u00b7ge\u00b7spannt"], "token_info": ["word", "word"], "pos": ["ADJD", "VVPP"], "meter": "-+-+", "measure": "iambic.di"}, "line.18": {"text": "Auf dem Haideland . . . .", "tokens": ["Auf", "dem", "Hai\u00b7de\u00b7land", ".", ".", ".", "."], "token_info": ["word", "word", "word", "punct", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "$.", "$.", "$.", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.19": {"text": "Hei! Wie hinweggesp\u00fclt", "tokens": ["Hei", "!", "Wie", "hin\u00b7weg\u00b7ge\u00b7sp\u00fclt"], "token_info": ["word", "punct", "word", "word"], "pos": ["NE", "$.", "PWAV", "VVPP"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.20": {"text": "Wird da des Zweifels leichenfarbner Dunst! \u2014", "tokens": ["Wird", "da", "des", "Zwei\u00b7fels", "lei\u00b7chen\u00b7farb\u00b7ner", "Dunst", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "Es athmet freier auf und freier", "tokens": ["Es", "ath\u00b7met", "frei\u00b7er", "auf", "und", "frei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "PTKVZ", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.22": {"text": "Die erl\u00f6ste Brust \u2014", "tokens": ["Die", "er\u00b7l\u00f6s\u00b7te", "Brust"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.23": {"text": "Und in allm\u00e4cht\u2019ger Brunst,", "tokens": ["Und", "in", "all\u00b7m\u00e4cht'\u00b7ger", "Brunst", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "In neugeborner Werdelust,", "tokens": ["In", "neu\u00b7ge\u00b7bor\u00b7ner", "Wer\u00b7de\u00b7lust", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Umfa\u00dft sie tief und voll", "tokens": ["Um\u00b7fa\u00dft", "sie", "tief", "und", "voll"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.26": {"text": "Des Lebens ganzes Sein", "tokens": ["Des", "Le\u00b7bens", "gan\u00b7zes", "Sein"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Und die lebend\u2019ge ", "tokens": ["Und", "die", "le\u00b7ben\u00b7d'\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "ADJA"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.28": {"text": "Ein hei\u00dfer Groll", "tokens": ["Ein", "hei\u00b7\u00dfer", "Groll"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.29": {"text": "Flammt auf wie greller blut\u2019ger Nordlichtschein,", "tokens": ["Flammt", "auf", "wie", "grel\u00b7ler", "blut'\u00b7ger", "Nord\u00b7licht\u00b7schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "KOKOM", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Da\u00df ", "tokens": ["Da\u00df"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Am Heiligsten begangen ward!", "tokens": ["Am", "Hei\u00b7ligs\u00b7ten", "be\u00b7gan\u00b7gen", "ward", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVPP", "VAFIN", "$."], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.32": {"text": "Verblendet und genarrt", "tokens": ["Ver\u00b7blen\u00b7det", "und", "ge\u00b7narrt"], "token_info": ["word", "word", "word"], "pos": ["VVPP", "KON", "VVPP"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.33": {"text": "Hab\u2019 ich gefr\u00f6hnt nur bl\u00f6dem ", "tokens": ["Hab'", "ich", "ge\u00b7fr\u00f6hnt", "nur", "bl\u00f6\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "VVPP", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "\u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014", "tokens": [], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.35": {"text": "\u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014 \u2014", "tokens": [], "token_info": ["punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct", "punct"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("]}, "line.36": {"text": "Hei! Wie der Sturm in gellender Melodei,", "tokens": ["Hei", "!", "Wie", "der", "Sturm", "in", "gel\u00b7len\u00b7der", "Me\u00b7lo\u00b7dei", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "PWAV", "ART", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.37": {"text": "Mit dr\u00f6hnend heis\u2019rem Schrei,", "tokens": ["Mit", "dr\u00f6h\u00b7nend", "heis'\u00b7rem", "Schrei", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Mir um das Haupt braust!", "tokens": ["Mir", "um", "das", "Haupt", "braust", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.39": {"text": "Wie die Wolken flattern", "tokens": ["Wie", "die", "Wol\u00b7ken", "flat\u00b7tern"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "VVINF"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.40": {"text": "Und windgehetzt,", "tokens": ["Und", "wind\u00b7ge\u00b7hetzt", ","], "token_info": ["word", "word", "punct"], "pos": ["KON", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.41": {"text": "Zerrissen und zerfetzt,", "tokens": ["Zer\u00b7ris\u00b7sen", "und", "zer\u00b7fetzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Zu R