{"dta.poem.20668": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Bey  \n  dem Eheverb\u00fcndni\u00df  \n meines j\u00fcngsten Bruders  \n  Ernst Daniel Hempel .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du trauest, mein geliebter Bruder,                 ", "tokens": ["Du", "trau\u00b7est", ",", "mein", "ge\u00b7lieb\u00b7ter", "Bru\u00b7der", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dem Herrscher, der die Fluren tr\u00e4nkt", "tokens": ["Dem", "Herr\u00b7scher", ",", "der", "die", "Flu\u00b7ren", "tr\u00e4nkt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus seinem Himmel, und das gro\u00dfe Steuerruder", "tokens": ["Aus", "sei\u00b7nem", "Him\u00b7mel", ",", "und", "das", "gro\u00b7\u00dfe", "Steu\u00b7er\u00b7ru\u00b7der"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von aller Menschen Schicksal lenkt.", "tokens": ["Von", "al\u00b7ler", "Men\u00b7schen", "Schick\u00b7sal", "lenkt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Du denkst in feierlicher Stunde", "tokens": ["Du", "denkst", "in", "fei\u00b7er\u00b7li\u00b7cher", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zur\u00fcck an Deiner Jugend Pfad;", "tokens": ["Zu\u00b7r\u00fcck", "an", "Dei\u00b7ner", "Ju\u00b7gend", "Pfad", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Dornen machten Dir schon eine tiefe Wunde,", "tokens": ["Die", "Dor\u00b7nen", "mach\u00b7ten", "Dir", "schon", "ei\u00b7ne", "tie\u00b7fe", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als kaum Dein Fu\u00df die Welt betrat.", "tokens": ["Als", "kaum", "Dein", "Fu\u00df", "die", "Welt", "be\u00b7trat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als sich Dein Arm um meinen Nacken", "tokens": ["Als", "sich", "Dein", "Arm", "um", "mei\u00b7nen", "Na\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch kindisch wand, als Du den Sarg", "tokens": ["Noch", "kin\u00b7disch", "wand", ",", "als", "Du", "den", "Sarg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Des Vaters nicht gekannt, wo ihre nasse Backen", "tokens": ["Des", "Va\u00b7ters", "nicht", "ge\u00b7kannt", ",", "wo", "ih\u00b7re", "nas\u00b7se", "Ba\u00b7cken"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKNEG", "VVPP", "$,", "PWAV", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Mutter unterm Flor verbarg.", "tokens": ["Die", "Mut\u00b7ter", "un\u00b7term", "Flor", "ver\u00b7barg", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Du wuchsest mancher Noth entgegen,", "tokens": ["Du", "wuch\u00b7sest", "man\u00b7cher", "Noth", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu fr\u00fch verwaiset warst Du so", "tokens": ["Zu", "fr\u00fch", "ver\u00b7wai\u00b7set", "warst", "Du", "so"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "VVPP", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zur Sklaverei bestimmt, wie Israel zu Schl\u00e4gen", "tokens": ["Zur", "Skla\u00b7ve\u00b7rei", "be\u00b7stimmt", ",", "wie", "Is\u00b7rael", "zu", "Schl\u00e4\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVPP", "$,", "PWAV", "NE", "APPR", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Im Frohndienst eines Pharao.", "tokens": ["Im", "Frohn\u00b7dienst", "ei\u00b7nes", "Pha\u00b7rao", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.5": {"line.1": {"text": "Dich armen lastbeladnen Knaben", "tokens": ["Dich", "ar\u00b7men", "last\u00b7be\u00b7lad\u00b7nen", "Kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zog oftmahls die Melancholie", "tokens": ["Zog", "oft\u00b7mahls", "die", "Me\u00b7lan\u00b7cho\u00b7lie"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zum Gottesacker, wo Dein Vater ward begraben,", "tokens": ["Zum", "Got\u00b7te\u00b7sa\u00b7cker", ",", "wo", "Dein", "Va\u00b7ter", "ward", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da seufztest Du: \u201eich kannt\u2019 ihn nie.\u201e", "tokens": ["Da", "seufz\u00b7test", "Du", ":", "\u201e", "ich", "kannt'", "ihn", "nie", ".", "\u201e"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$.", "$(", "PPER", "VVFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Da sankst Du traurig auf den H\u00fcgel,", "tokens": ["Da", "sankst", "Du", "trau\u00b7rig", "auf", "den", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Deiner Mutter Staub bedeckt,", "tokens": ["Der", "Dei\u00b7ner", "Mut\u00b7ter", "Staub", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bis ein Posaunenschall des Grabes Thor und Riegel", "tokens": ["Bis", "ein", "Po\u00b7sau\u00b7nen\u00b7schall", "des", "Gra\u00b7bes", "Thor", "und", "Rie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zersprenget, und die Todten weckt.", "tokens": ["Zer\u00b7spren\u00b7get", ",", "und", "die", "Tod\u00b7ten", "weckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Du weintest laut auf jenem Sande,", "tokens": ["Du", "wein\u00b7test", "laut", "auf", "je\u00b7nem", "San\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der unsern Herzen heilig hei\u00dft,", "tokens": ["Der", "un\u00b7sern", "Her\u00b7zen", "hei\u00b7lig", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Nahmst einen Stab und giengst in unbekannte Lande", "tokens": ["Nahmst", "ei\u00b7nen", "Stab", "und", "giengst", "in", "un\u00b7be\u00b7kann\u00b7te", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "KON", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "M\u00fchselig, wie ein Wandrer reist.", "tokens": ["M\u00fch\u00b7se\u00b7lig", ",", "wie", "ein", "Wand\u00b7rer", "reist", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ART", "NN", "VVFIN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.8": {"line.1": {"text": "Der Mutter sterbendes Gebete,", "tokens": ["Der", "Mut\u00b7ter", "ster\u00b7ben\u00b7des", "Ge\u00b7be\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr lezter Seufzer ging mit Dir", "tokens": ["Ihr", "lez\u00b7ter", "Seuf\u00b7zer", "ging", "mit", "Dir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Durch Labyrinthe fort, durch W\u00e4lder, Meer und St\u00e4dte,", "tokens": ["Durch", "La\u00b7by\u00b7rin\u00b7the", "fort", ",", "durch", "W\u00e4l\u00b7der", ",", "Meer", "und", "St\u00e4d\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PTKVZ", "$,", "APPR", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ihr Engel brachte Dich zu mir,", "tokens": ["Ihr", "En\u00b7gel", "brach\u00b7te", "Dich", "zu", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Zu einer Schwester, die den Finger", "tokens": ["Zu", "ei\u00b7ner", "Schwes\u00b7ter", ",", "die", "den", "Fin\u00b7ger"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Des h\u00f6chsten Weltregierers kennt,", "tokens": ["Des", "h\u00f6chs\u00b7ten", "Welt\u00b7re\u00b7gie\u00b7rers", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und seine F\u00fchrung preist, und t\u00e4glich sich geringer", "tokens": ["Und", "sei\u00b7ne", "F\u00fch\u00b7rung", "preist", ",", "und", "t\u00e4g\u00b7lich", "sich", "ge\u00b7rin\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "KON", "ADJD", "PRF", "ADJD"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als tausend andre Menschen nennt.", "tokens": ["Als", "tau\u00b7send", "and\u00b7re", "Men\u00b7schen", "nennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "CARD", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "O Bruder, tr\u00fcge meine T\u00f6ne", "tokens": ["O", "Bru\u00b7der", ",", "tr\u00fc\u00b7ge", "mei\u00b7ne", "T\u00f6\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Nachhall bis zur Sternenwelt,", "tokens": ["Der", "Nach\u00b7hall", "bis", "zur", "Ster\u00b7nen\u00b7welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "W\u00fcrd\u2019 unsrer Mutter jezt das Bildni\u00df ihrer S\u00f6hne", "tokens": ["W\u00fcrd'", "uns\u00b7rer", "Mut\u00b7ter", "jezt", "das", "Bild\u00b7ni\u00df", "ih\u00b7rer", "S\u00f6h\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Mit Blumenkr\u00e4nzen vorgestellt,", "tokens": ["Mit", "Blu\u00b7men\u00b7kr\u00e4n\u00b7zen", "vor\u00b7ge\u00b7stellt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Und dieses Beispiel einer \u00e4chten", "tokens": ["Und", "die\u00b7ses", "Bei\u00b7spiel", "ei\u00b7ner", "\u00e4ch\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PDAT", "NN", "ART", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Getreuen Liebe, Deine Braut:", "tokens": ["Ge\u00b7treu\u00b7en", "Lie\u00b7be", ",", "Dei\u00b7ne", "Braut", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann f\u00fchlte sie noch mehr die Wonne des Gerechten,", "tokens": ["Dann", "f\u00fchl\u00b7te", "sie", "noch", "mehr", "die", "Won\u00b7ne", "des", "Ge\u00b7rech\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Gottes Angesichte schaut.", "tokens": ["Der", "Got\u00b7tes", "An\u00b7ge\u00b7sich\u00b7te", "schaut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Ich h\u00f6re Fl\u00fcgel sich bewegen,", "tokens": ["Ich", "h\u00f6\u00b7re", "Fl\u00fc\u00b7gel", "sich", "be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mein Bruder, horche doch, es bringt", "tokens": ["Mein", "Bru\u00b7der", ",", "hor\u00b7che", "doch", ",", "es", "bringt"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "VVFIN", "ADV", "$,", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vielleicht ein Engel von dem Himmel einen Seegen,", "tokens": ["Viel\u00b7leicht", "ein", "En\u00b7gel", "von", "dem", "Him\u00b7mel", "ei\u00b7nen", "See\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Den unsre Mutter jezt erringt.", "tokens": ["Den", "uns\u00b7re", "Mut\u00b7ter", "jezt", "er\u00b7ringt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Dein Herze f\u00fchlet st\u00e4rkre Schl\u00e4ge", "tokens": ["Dein", "Her\u00b7ze", "f\u00fch\u00b7let", "st\u00e4r\u00b7kre", "Schl\u00e4\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von Hoffnung und von Zuversicht,", "tokens": ["Von", "Hoff\u00b7nung", "und", "von", "Zu\u00b7ver\u00b7sicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Du befiehlst getrost dem Herren Deine Wege.", "tokens": ["Und", "Du", "be\u00b7fiehlst", "ge\u00b7trost", "dem", "Her\u00b7ren", "Dei\u00b7ne", "We\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJD", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.