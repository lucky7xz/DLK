{"textgrid.poem.65111": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Als heilig ist des Dichters Schmerz zu ehren,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Als heilig ist des Dichters Schmerz zu ehren,", "tokens": ["Als", "hei\u00b7lig", "ist", "des", "Dich\u00b7ters", "Schmerz", "zu", "eh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "VAFIN", "ART", "NN", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn bang er schmachtet nach der Wahrheit Strahle,", "tokens": ["Wenn", "bang", "er", "schmach\u00b7tet", "nach", "der", "Wahr\u00b7heit", "Strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wenn er verwais't sich f\u00fchlt im Erdenthale,", "tokens": ["Wenn", "er", "ver\u00b7wais't", "sich", "f\u00fchlt", "im", "Er\u00b7dent\u00b7ha\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "PRF", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und nach der Heimath Gl\u00fcck verlangt mit Z\u00e4hren.", "tokens": ["Und", "nach", "der", "Hei\u00b7math", "Gl\u00fcck", "ver\u00b7langt", "mit", "Z\u00e4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Allein bei euch, die jammernd sich beschweren,", "tokens": [