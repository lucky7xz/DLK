{"dta.poem.415": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Die Brust.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Was ist die sch\u00f6ne Brust/ was liegt daselbst verh\u00fcllet?", "tokens": ["Was", "ist", "die", "sch\u00f6\u00b7ne", "Brust", "/", "was", "liegt", "da\u00b7selbst", "ver\u00b7h\u00fcl\u00b7let", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "$(", "PWS", "VVFIN", "PAV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein lieblich Schnee-Geb\u00fcrg mit ", "tokens": ["Ein", "lieb\u00b7lich", "Schnee\u00b7Ge\u00b7b\u00fcrg", "mit"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "APPR"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Schatz/ den ", "tokens": ["Ein", "Schatz", "/", "den"], "token_info": ["word", "word", "punct", "word"], "pos": ["ART", "NN", "$(", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.4": {"text": "Wenn ein verliebter Sinn sich zu der Liebsten neigt.", "tokens": ["Wenn", "ein", "ver\u00b7lieb\u00b7ter", "Sinn", "sich", "zu", "der", "Liebs\u00b7ten", "neigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein ", "tokens": ["Ein"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.6": {"text": "Ob man von aussen gleich nur Ey\u00df und K\u00e4lte f\u00fchlet.", "tokens": ["Ob", "man", "von", "aus\u00b7sen", "gleich", "nur", "Ey\u00df", "und", "K\u00e4l\u00b7te", "f\u00fch\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NE", "ADV", "ADV", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ein Eingang/ welcher f\u00fchrt in das Gelobte-Land/", "tokens": ["Ein", "Ein\u00b7gang", "/", "wel\u00b7cher", "f\u00fchrt", "in", "das", "Ge\u00b7lob\u00b7te\u00b7Land", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWS", "VVFIN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo man die Geister k\u00fchlt von ihren Liebes-Brand.", "tokens": ["Wo", "man", "die", "Geis\u00b7ter", "k\u00fchlt", "von", "ih\u00b7ren", "Lie\u00b7bes\u00b7Brand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}