{"textgrid.poem.56518": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Spitzk\u00f6pfig kommt er", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Spitzk\u00f6pfig kommt er \u00fcber die D\u00e4cher hoch", "tokens": ["Spitz\u00b7k\u00f6p\u00b7fig", "kommt", "er", "\u00fc\u00b7ber", "die", "D\u00e4\u00b7cher", "hoch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und schleppt seine gelben Haare nach,", "tokens": ["Und", "schleppt", "sei\u00b7ne", "gel\u00b7ben", "Haa\u00b7re", "nach", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "ADJA", "NN", "PTKVZ", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "Der Zauberer, der still in die Himmelszimmer steigt", "tokens": ["Der", "Zau\u00b7be\u00b7rer", ",", "der", "still", "in", "die", "Him\u00b7mels\u00b7zim\u00b7mer", "steigt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "In vieler Gestirne gewundenem Blumenpfad.", "tokens": ["In", "vie\u00b7ler", "Ge\u00b7stir\u00b7ne", "ge\u00b7wun\u00b7de\u00b7nem", "Blu\u00b7men\u00b7pfad", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "ADJA", "NN", "$."], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}}, "stanza.2": {"line.1": {"text": "Alle Tiere unten im Wald und Gestr\u00fcpp", "tokens": ["Al\u00b7le", "Tie\u00b7re", "un\u00b7ten", "im", "Wald", "und", "Ge\u00b7str\u00fcpp"], "token_info":