{"dta.poem.15831": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "36.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Verw\u00f6hnen werden dich geschenkte Leckerbissen,", "tokens": ["Ver\u00b7w\u00f6h\u00b7nen", "wer\u00b7den", "dich", "ge\u00b7schenk\u00b7te", "Le\u00b7cker\u00b7bis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df du einst kaufen mu\u00dft, was du nicht mehr kannst missen.", "tokens": ["Da\u00df", "du", "einst", "kau\u00b7fen", "mu\u00dft", ",", "was", "du", "nicht", "mehr", "kannst", "mis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VVINF", "VMFIN", "$,", "PWS", "PPER", "PTKNEG", "ADV", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Die Schenker haben nicht die Sache recht bedacht,", "tokens": ["Die", "Schen\u00b7ker", "ha\u00b7ben", "nicht", "die", "Sa\u00b7che", "recht", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PTKNEG", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die etwas schenken, das, statt reicher, \u00e4rmer macht.", "tokens": ["Die", "et\u00b7was", "schen\u00b7ken", ",", "das", ",", "statt", "rei\u00b7cher", ",", "\u00e4r\u00b7mer", "macht", "."], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "PIS", "VVINF", "$