{"textgrid.poem.42957": {"metadata": {"author": {"name": "Ringelnatz, Joachim", "birth": "N.A.", "death": "N.A."}, "title": "Du und die Nacht", "genre": "verse", "period": "N.A.", "pub_year": 1908, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gib du dem Tag, was aus dir will.", "tokens": ["Gib", "du", "dem", "Tag", ",", "was", "aus", "dir", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "$,", "PRELS", "APPR", "PPER", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Nacht ist still.", "tokens": ["Die", "Nacht", "ist", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Auch wenn in einem Nachtlokal", "tokens": ["Auch", "wenn", "in", "ei\u00b7nem", "Nacht\u00b7lo\u00b7kal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du welche Leute, die Skandal", "tokens": ["Du", "wel\u00b7che", "Leu\u00b7te", ",", "die", "Skan\u00b7dal"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "PWAT", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Begeistert, siehst.", "tokens": ["Be\u00b7geis\u00b7tert", ",", "siehst", "."], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Nicht, da\u00df du fliehst!", "tokens": ["Nicht", ",", "da\u00df", "du", "fliehst", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Auch wenn ein Raufbold dich berennt,", "tokens": ["Auch", "wenn", "ein", "Rauf\u00b7bold", "dich", "be\u00b7rennt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Oder ein bl\u00f6der Korps-Student", "tokens": ["O\u00b7der", "ein", "bl\u00f6\u00b7der", "Korps\u00b7Stu\u00b7dent"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit Gott \u2013 dem's einfiel, dort zu wandeln \u2013", "tokens": ["Mit", "Gott", "\u2013", "dem's", "ein\u00b7fiel", ",", "dort", "zu", "wan\u00b7deln", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PDS", "VVFIN", "$,", "ADV", "PTKZU", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Will anbandeln.", "tokens": ["Will", "an\u00b7ban\u00b7deln", "."], "token_info": ["word", "word", "punct"], "pos": ["VMFIN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Dieweil das meiste schl\u00e4ft, baut aus Gestirnen", "tokens": ["Die\u00b7weil", "das", "meis\u00b7te", "schl\u00e4ft", ",", "baut", "aus", "Ge\u00b7stir\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "ART", "PIS", "VVFIN", "$,", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Sich Unkenm\u00e4rchenhaftes. Gruslig schiebt", "tokens": ["Sich", "Un\u00b7ken\u00b7m\u00e4r\u00b7chen\u00b7haf\u00b7tes", ".", "Grus\u00b7lig", "schiebt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PRF", "NN", "$.", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schlechtes Gewissen seine Heimlichkeiten,", "tokens": 