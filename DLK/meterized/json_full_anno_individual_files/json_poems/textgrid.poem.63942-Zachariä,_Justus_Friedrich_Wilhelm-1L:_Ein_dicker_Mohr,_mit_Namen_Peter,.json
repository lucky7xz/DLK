{"textgrid.poem.63942": {"metadata": {"author": {"name": "Zachari\u00e4, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein dicker Mohr, mit Namen Peter,", "genre": "verse", "period": "N.A.", "pub_year": 1751, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein dicker Mohr, mit Namen Peter,", "tokens": ["Ein", "di\u00b7cker", "Mohr", ",", "mit", "Na\u00b7men", "Pe\u00b7ter", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "NN", "NE", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward bei der Reiterei Trompeter,", "tokens": ["Ward", "bei", "der", "Rei\u00b7te\u00b7rei", "Trom\u00b7pe\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und bald darauf in einer Schlacht", "tokens": ["Und", "bald", "da\u00b7rauf", "in", "ei\u00b7ner", "Schlacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "PAV", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit zum Gefangenen gemacht.", "tokens": ["Mit", "zum", "Ge\u00b7fan\u00b7ge\u00b7nen", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPRART", "NN", "VVPP", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Man gab ihm manchen Rippensto\u00df,", "tokens": ["Man", "gab", "ihm", "man\u00b7chen", "Rip\u00b7pen\u00b7sto\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Er aber rief: La\u00dft mich doch los!", "tokens": ["Er", "a\u00b7ber", "rief", ":", "La\u00dft