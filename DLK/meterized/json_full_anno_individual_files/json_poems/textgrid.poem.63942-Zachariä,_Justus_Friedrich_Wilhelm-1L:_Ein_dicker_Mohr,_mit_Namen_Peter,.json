info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADJD", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Warum nicht? Schurke! (fing man an)", "tokens": ["Wa\u00b7rum", "nicht", "?", "Schur\u00b7ke", "!", "(", "fing", "man", "an", ")"], "token_info": ["word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$.", "NN", "$.", "$(", "VVFIN", "PIS", "PTKVZ", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Dein Blasen eben hat's gethan!", "tokens": ["Dein", "Bla\u00b7sen", "e\u00b7ben", "hat's", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Du machtest unsern Feinden Muth,", "tokens": ["Du", "mach\u00b7test", "un\u00b7sern", "Fein\u00b7den", "Muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und setztest sie dadurch in Wuth.", "tokens": ["Und", "setz\u00b7test", "sie", "da\u00b7durch", "in", "Wuth", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PAV", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.17": {"text": "Wer zu der That Ermunt'rung gibt,", "tokens": ["Wer", "zu", "der", "That", "Er\u00b7munt'\u00b7rung", "gibt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Hat selber sie mit ausge\u00fcbt.", "tokens": ["Hat", "sel\u00b7ber", "sie", "mit", "aus\u00b7ge\u00b7\u00fcbt", "."], "token_info": ["word", "word", "word", "word", "wor