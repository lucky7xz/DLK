{"dta.poem.5685": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von GOTT.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es bleibt die Wahrheit immer,", "tokens": ["Es", "bleibt", "die", "Wahr\u00b7heit", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.2": {"text": "Obgleich verh\u00fcllet und verstecket,", "tokens": ["Ob\u00b7gleich", "ver\u00b7h\u00fcl\u00b7let", "und", "ver\u00b7ste\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wird sie von uns gleich nicht entdecket,", "tokens": ["Wird", "sie", "von", "uns", "gleich", "nicht", "ent\u00b7de\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "PPER", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In ihrer Krafft und reinem Schimmer.", "tokens": ["In", "ih\u00b7rer", "Krafft", "und", "rei\u00b7nem", "Schim\u00b7mer", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Sie sey auch, wo sie sey; so ist sie wesentlich,", "tokens": ["Sie", "sey", "auch", ",", "wo", "sie", "sey", ";", "so", "ist", "sie", "we\u00b7sent\u00b7lich", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "PWAV", "PPER", "VAFIN", "$.", "ADV", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Lebendig, und es zeiget sich,", "tokens": ["Le\u00b7ben\u00b7dig", ",", "und", "es", "zei\u00b7get", "sich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "KON", "PPER", "VVFIN", "PRF", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Da\u00df unser aller Witz ein F\u00fcncklein nur aus Jhr.", "tokens": ["Da\u00df", "un\u00b7ser", "al\u00b7ler", "Witz", "ein", "F\u00fcnc\u00b7klein", "nur", "aus", "Ihr", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "PIAT", "NN", "ART", "NN", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Sie war von Ewigkeit, und wird auch ewig seyn.", "tokens": ["Sie", "war", "von", "E\u00b7wig\u00b7keit", ",", "und", "wird", "auch", "e\u00b7wig", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "$,", "KON", "VAFIN", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wenn unser schwacher Geist nun hier", "tokens": ["Wenn", "un\u00b7ser", "schwa\u00b7cher", "Geist", "nun", "hier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In Finsternissen stecket; so lasset uns den Schein", "tokens": ["In", "Fins\u00b7ter\u00b7nis\u00b7sen", "ste\u00b7cket", ";", "so", "las\u00b7set", "uns", "den", "Schein"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "$.", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Von Jhrem ", "tokens": ["Von", "Ih\u00b7rem"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.12": {"text": "Wenn wir die Augen lencken", "tokens": ["Wenn", "wir", "die", "Au\u00b7gen", "len\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Auf ihre wunderbahre Klarheit:", "tokens": ["Auf", "ih\u00b7re", "wun\u00b7der\u00b7bah\u00b7re", "Klar\u00b7heit", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}