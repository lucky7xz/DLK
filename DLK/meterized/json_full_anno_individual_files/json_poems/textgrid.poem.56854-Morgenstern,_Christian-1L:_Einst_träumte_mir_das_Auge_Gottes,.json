{"textgrid.poem.56854": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Einst tr\u00e4umte mir das Auge Gottes,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Einst tr\u00e4umte mir das Auge Gottes,", "tokens": ["Einst", "tr\u00e4um\u00b7te", "mir", "das", "Au\u00b7ge", "Got\u00b7tes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "und Grausen \u00fcberfiel mich.", "tokens": ["und", "Grau\u00b7sen", "\u00fc\u00b7berf\u00b7iel", "mich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Entsch\u00fcrzt, entzaubert lag die Welt vor ihm,", "tokens": ["Ent\u00b7sch\u00fcrzt", ",", "ent\u00b7zau\u00b7bert", "lag", "die", "Welt", "vor", "ihm", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "VVPP", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "entwirrt, entbl\u00f6\u00dft, bis in den letzten Winkel", "tokens": ["ent\u00b7wirrt", ",", "ent\u00b7bl\u00f6\u00dft", ",", "bis", "in", "den", "letz\u00b7ten", "Win\u00b7kel"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VVFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "entheimlicht, n\u00fcchtern, reiz- und r\u00e4tsellos.", "tokens": ["en\u00b7theim\u00b7licht", ",", "n\u00fcch\u00b7tern", ",", "rei\u00b7z", "und", "r\u00e4t\u00b7sel\u00b7los", "."], "token_info": ["word", "punct", "