{"textgrid.poem.33557": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "13.", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich f\u00fchl's mit Stolz, da\u00df ich nicht wohlverwahrt", "tokens": ["Ich", "f\u00fchl's", "mit", "Stolz", ",", "da\u00df", "ich", "nicht", "wohl\u00b7ver\u00b7wahrt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KOUS", "PPER", "PTKNEG", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wie Jene bin, die stets verschont geblieben", "tokens": ["Wie", "Je\u00b7ne", "bin", ",", "die", "stets", "ver\u00b7schont", "ge\u00b7blie\u00b7ben"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "PDS", "VAFIN", "$,", "PRELS", "ADV", "VVPP", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Im Leben, wie im Lieben,", "tokens": ["Im", "Le\u00b7ben", ",", "wie", "im", "Lie\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PWAV", "APPRART", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df keinen Schmerz das Schicksal mir erspart.", "tokens": ["Da\u00df", "kei\u00b7nen", "Schmerz", "das", "Schick\u00b7sal", "mir", "er\u00b7spart", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Erfinderisch, mit ausgesuchten Qualen", "tokens": ["Er\u00b7fin\u00b7de\u00b7risch", ",", "mit", "aus\u00b7ge\u00b7such\u00b7ten", "Qua\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Hat mich's verfolgt noch bis zuletzt", "tokens": ["Hat", "mich's", "ver\u00b7folgt", "noch", "bis", "zu\u00b7letzt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PIS", "VVFIN", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und immer dann am tiefsten mich verletzt,", "tokens": ["Und", "im\u00b7mer", "dann", "am", "tiefs\u00b7ten", "mich", "ver\u00b7letzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "APPRART", "ADJA", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wenn's mir gelacht mit seinen hellsten Strahlen.", "tokens": ["Wenn's", "mir", "ge\u00b7lacht", "mit", "sei\u00b7nen", "hells\u00b7ten", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich richte k\u00fchn mich vor den Blitzen auf,", "tokens": ["Ich", "rich\u00b7te", "k\u00fchn", "mich", "vor", "den", "Blit\u00b7zen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "PRF", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und sage: trefft! und zu den St\u00fcrmen:", "tokens": ["Und", "sa\u00b7ge", ":", "trefft", "!", "und", "zu", "den", "St\u00fcr\u00b7men", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "VVFIN", "$.", "KON", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "La\u00dft eure Wogen t\u00fcrmen!", "tokens": ["La\u00dft", "eu\u00b7re", "Wo\u00b7gen", "t\u00fcr\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "St\u00fcrmt fort, ras't fort, ihr haltet mich nicht auf!", "tokens": ["St\u00fcrmt", "fort", ",", "ras't", "fort", ",", "ihr", "hal\u00b7tet", "mich", "nicht", "auf", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$,", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und zu den Augen, die so stolz und gro\u00df", "tokens": ["Und", "zu", "den", "Au\u00b7gen", ",", "die", "so", "stolz", "und", "gro\u00df"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mein Herz bedrohn mit t\u00f6dlichem Verderben,", "tokens": ["Mein", "Herz", "be\u00b7drohn", "mit", "t\u00f6d\u00b7li\u00b7chem", "Ver\u00b7der\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Zu deinen Augen sag' ich: sch\u00f6nes Los,", "tokens": ["Zu", "dei\u00b7nen", "Au\u00b7gen", "sag'", "ich", ":", "sch\u00f6\u00b7nes", "Los", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "PPER", "$.", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Von eurer Glut versengt dahinzusterben!", "tokens": ["Von", "eu\u00b7rer", "Glut", "ver\u00b7sengt", "da\u00b7hin\u00b7zu\u00b7ster\u00b7ben", "!"], "token_inf