bic.tri"}, "line.8": {"text": "Umschlo\u00df ich mit dem Meer.\u00ab", "tokens": ["Um\u00b7schlo\u00df", "ich", "mit", "dem", "Meer", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und dennoch wandern pl\u00fcndernd sie, berauben", "tokens": ["Und", "den\u00b7noch", "wan\u00b7dern", "pl\u00fcn\u00b7dernd", "sie", ",", "be\u00b7rau\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADV", "VVFIN", "ADJD", "PPER", "$,", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der Musen und der K\u00fcnste Vaterland,", "tokens": ["Der", "Mu\u00b7sen", "und", "der", "K\u00fcns\u00b7te", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Begraben ihren Raub, die Th\u00f6richten! und glauben,", "tokens": ["Be\u00b7gra\u00b7ben", "ih\u00b7ren", "Raub", ",", "die", "Th\u00f6\u00b7rich\u00b7ten", "!", "und", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "ART", "NN", "$.", "KON", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Sie h\u00e4tten jetzt der Griechen Kunstverstand.", "tokens": ["Sie", "h\u00e4t\u00b7ten", "jetzt", "der", "Grie\u00b7chen", "Kunst\u00b7ver\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Weichling siechet an der Quelle", "tokens": ["Der", "Weich\u00b7ling", "sie\u00b7chet", "an", "der", "Quel\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Hygea's in dem rauhen Thal", "tokens": ["Hy\u00b7ge\u00b7a's", "in", "dem", "rau\u00b7hen", "Thal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.7": {"text": "Und suchet in der H\u00f6lle", "tokens": ["Und", "su\u00b7chet", "in", "der", "H\u00f6l\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Das Gold zu seiner Qual.", "tokens": ["Das", "Gold", "zu", "sei\u00b7ner", "Qual", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Warum verz\u00e4untest Du, Natur, mit Alpenh\u00f6hen", "tokens": ["Wa\u00b7rum", "ver\u00b7z\u00e4un\u00b7test", "Du", ",", "Na\u00b7tur", ",", "mit", "Al\u00b7pen\u00b7h\u00f6\u00b7hen"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "$,", "NN", "$,", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Musen und der K\u00fcnste Mutterland?", "tokens": ["Der", "Mu\u00b7sen", "und", "der", "K\u00fcns\u00b7te", "Mut\u00b7ter\u00b7land", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Warum umschlossest Du mit wilden Pyren\u00e4en,", "tokens": ["Wa\u00b7rum", "um\u00b7schlos\u00b7sest", "Du", "mit", "wil\u00b7den", "Py\u00b7re\u00b7n\u00e4\u00b7en", ","], "token_info": ["word", "word", "word", "wo