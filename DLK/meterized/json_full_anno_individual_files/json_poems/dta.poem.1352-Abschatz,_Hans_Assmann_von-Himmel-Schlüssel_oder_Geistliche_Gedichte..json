{"dta.poem.1352": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Zwey St\u00fccke bitt ich HErr von dir/", "tokens": ["Zwey", "St\u00fc\u00b7cke", "bitt", "ich", "Herr", "von", "dir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VVFIN", "PPER", "NN", "APPR", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die wollestu nicht wegern mir/", "tokens": ["Die", "wol\u00b7les\u00b7tu", "nicht", "we\u00b7gern", "mir", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VVFIN", "PPER", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Eh ich von hinnen scheide:", "tokens": ["Eh", "ich", "von", "hin\u00b7nen", "schei\u00b7de", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADV", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Beh\u00fctte mich vor Uberflu\u00df/", "tokens": ["Be\u00b7h\u00fct\u00b7te", "mich", "vor", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch gieb/ da\u00df ich nicht darben mu\u00df/", "tokens": ["Doch", "gieb", "/", "da\u00df", "ich", "nicht", "dar\u00b7ben", "mu\u00df", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "$(", "KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und schweres Armutt leyde.", "tokens": ["Und", "schwe\u00b7res", "Ar\u00b7mutt", "ley\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Lebt ich ohn alle Sorg und Noth/", "tokens": ["Lebt", "ich", "ohn", "al\u00b7le", "Sorg", "und", "Noth", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PIAT", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So m\u00f6cht ich sagen: Wer ist GOtt/", "tokens": ["So", "m\u00f6cht", "ich", "sa\u00b7gen", ":", "Wer", "ist", "Gott", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "VVINF", "$.", "PWS", "VAFIN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Vor dem ich mich soll schmiegen?", "tokens": ["Vor", "dem", "ich", "mich", "soll", "schmie\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPER", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und kehrte Mangel bey mir ein/", "tokens": ["Und", "kehr\u00b7te", "Man\u00b7gel", "bey", "mir", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "PPER", "ART", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So m\u00f6cht ich voller Kummer seyn/", "tokens": ["So", "m\u00f6cht", "ich", "vol\u00b7ler", "Kum\u00b7mer", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADJA", "NN", "VAINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Unrechtes Gutt zu kriegen.", "tokens": ["Un\u00b7rech\u00b7tes", "Gutt", "zu", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Drum la\u00df mich mein bescheiden Theil", "tokens": ["Drum", "la\u00df", "mich", "mein", "be\u00b7schei\u00b7den", "Theil"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Leibes Noth/ zum Seelen-Heyl", "tokens": ["Zur", "Lei\u00b7bes", "Noth", "/", "zum", "See\u00b7len\u00b7Heyl"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "NN", "$(", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus deiner Hand empfangen/", "tokens": ["Aus", "dei\u00b7ner", "Hand", "emp\u00b7fan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So r\u00fchm ich deine G\u00fcttigkeit/", "tokens": ["So", "r\u00fchm", "ich", "dei\u00b7ne", "G\u00fct\u00b7tig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wenn ich nach wohlgeschlo\u00dfner Zeit", "tokens": ["Wenn", "ich", "nach", "wohl\u00b7ge\u00b7schlo\u00df\u00b7ner", "Zeit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "In Himmel eingegangen.", "tokens": ["In", "Him\u00b7mel", "ein\u00b7ge\u00b7gan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}}}}