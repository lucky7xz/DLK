{"textgrid.poem.64563": {"metadata": {"author": {"name": "K\u00e4stner, Abraham Gotthelf", "birth": "N.A.", "death": "N.A."}, "title": "187. An Hrn. Professor Lichtenberg", "genre": "verse", "period": "N.A.", "pub_year": 1759, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du ladest zwanzig Mann, und dem ", "tokens": ["Du", "la\u00b7dest", "zwan\u00b7zig", "Mann", ",", "und", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$,", "KON", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh' ich sie manches Glas und manche Sch\u00fcssel leeren,", "tokens": ["Seh'", "ich", "sie", "man\u00b7ches", "Glas", "und", "man\u00b7che", "Sch\u00fcs\u00b7sel", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er, als w\u00e4r der Mund zum Reden nur bestimmt,", "tokens": ["Wenn", "er", ",", "als", "w\u00e4r", "der", "Mund", "zum", "Re\u00b7den", "nur", "be\u00b7stimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "KOKOM", "VAFIN", "ART", "NN", "APPRART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Flasche ruhig l\u00e4\u00dft, und keinen Teller nimmt.", "tokens": ["Die", "Fla\u00b7sche", "ru\u00b7hig", "l\u00e4\u00dft", ",", "und", "kei\u00b7nen", "Tel\u00b7ler", "nimmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVFIN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So war ein Opfermahl nach frommer Alten Weise,", "tokens": ["So", "war", "ein", "Op\u00b7fer\u00b7mahl", "nach", "from\u00b7mer", "Al\u00b7ten", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dampf f\u00fcr die Gottheit nur, und f\u00fcr die Priester Speise.", "tokens": ["Dampf", "f\u00fcr", "die", "Got\u00b7theit", "nur", ",", "und", "f\u00fcr", "die", "Pries\u00b7ter", "Spei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ADV", "$,", "KON", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}, "stanza.2": {"line.1": {"text": "Du ladest zwanzig Mann, und dem ", "tokens": ["Du", "la\u00b7dest", "zwan\u00b7zig", "Mann", ",", "und", "dem"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "CARD", "NN", "$,", "KON", "ART"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Seh' ich sie manches Glas und manche Sch\u00fcssel leeren,", "tokens": ["Seh'", "ich", "sie", "man\u00b7ches", "Glas", "und", "man\u00b7che", "Sch\u00fcs\u00b7sel", "lee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPER", "PIAT", "NN", "KON", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wenn er, als w\u00e4r der Mund zum Reden nur bestimmt,", "tokens": ["Wenn", "er", ",", "als", "w\u00e4r", "der", "Mund", "zum", "Re\u00b7den", "nur", "be\u00b7stimmt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "pu