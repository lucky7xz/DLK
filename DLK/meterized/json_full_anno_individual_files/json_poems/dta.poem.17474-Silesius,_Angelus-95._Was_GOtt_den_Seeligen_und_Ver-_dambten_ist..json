{"dta.poem.17474": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "95.  Was GOtt den Seeligen und Ver-  \n dambten ist.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.85", "nl:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott ist den Seeligen ein ewger freuden Gast/", "tokens": ["Gott", "ist", "den", "See\u00b7li\u00b7gen", "ein", "ew\u00b7ger", "freu\u00b7den", "Gast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.2": {"text": "Und den Verdammeten ein\u2019ewge \u00fcberlast.", "tokens": ["Und", "den", "Ver\u00b7dam\u00b7me\u00b7ten", "ein'\u00b7e\u00b7wge", "\u00fc\u00b7berl\u00b7ast", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$."], "meter": "+--+---+-+-+", "measure": "dactylic.di.plus"}}}}}