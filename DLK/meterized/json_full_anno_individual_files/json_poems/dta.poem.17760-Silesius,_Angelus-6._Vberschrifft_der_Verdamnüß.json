{"dta.poem.17760": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "6.  Vberschrifft der Verdamn\u00fc\u00df/", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Hter ist ein\u2019 Ewge Nacht: man wei\u00df von keinem", "tokens": ["Hter", "ist", "ein'", "Ew\u00b7ge", "Nacht", ":", "man", "wei\u00df", "von", "kei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$.", "PIS", "VVFIN", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "lachen/", "tokens": ["la\u00b7chen", "/"], "token_info": ["word", "punct"], "pos": ["VVINF", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ein Jam\u0304er Ach und Weh/ ach ewig seyn verlohrn!", "tokens": ["Ein", "Jam\u0304er", "Ach", "und", "Weh", "/", "ach", "e\u00b7wig", "seyn", "ver\u00b7lohrn", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "$(", "ADV", "ADJD", "VAINF", "VVINF", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "Wird jm\u0304er fort geschriehn/ und w\u00e4rn wir nie gebohrn!", "tokens": ["Wird", "jm\u0304er", "fort", "ge\u00b7schriehn", "/", "und", "w\u00e4rn", "wir", "nie", "ge\u00b7bohrn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "PTKVZ", "VVPP", "$(", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}}}}