{"textgrid.poem.64073": {"metadata": {"author": {"name": "Ahlefeld, Charlotte von", "birth": "N.A.", "death": "N.A."}, "title": "Thr\u00e4nen", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie sanft naht jene Trauermelodie", "tokens": ["Wie", "sanft", "naht", "je\u00b7ne", "Trau\u00b7er\u00b7me\u00b7lo\u00b7die"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mir durch des Abends leise D\u00e4mmerh\u00fclle.", "tokens": ["Mir", "durch", "des", "A\u00b7bends", "lei\u00b7se", "D\u00e4m\u00b7mer\u00b7h\u00fcl\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aus Deines Tones wunderbarer F\u00fclle", "tokens": ["Aus", "Dei\u00b7nes", "To\u00b7nes", "wun\u00b7der\u00b7ba\u00b7rer", "F\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Entquillt mir Trost in s\u00fcsser Harmonie.", "tokens": ["Ent\u00b7quillt", "mir", "Trost", "in", "s\u00fcs\u00b7ser", "Har\u00b7mo\u00b7nie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Es schwebt die Stimme meiner innern Schmerzen", "tokens": ["Es", "schwebt", "die", "Stim\u00b7me", "mei\u00b7ner", "in\u00b7nern", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iam