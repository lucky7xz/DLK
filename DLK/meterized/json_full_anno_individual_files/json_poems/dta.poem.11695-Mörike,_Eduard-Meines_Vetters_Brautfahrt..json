{"dta.poem.11695": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Meines Vetters Brautfahrt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "\u2014 Aber wo bleibt er so lang? Sagt ihm, die Kutsche", "tokens": ["A\u00b7ber", "wo", "bleibt", "er", "so", "lang", "?", "Sagt", "ihm", ",", "die", "Kut\u00b7sche"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "KON", "PWAV", "VVFIN", "PPER", "ADV", "ADJD", "$.", "VVFIN", "PPER", "$,", "ART", "NN"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Droben liegt er im Bett, verdrie\u00dflich, und lieset in Schellers", "tokens": ["Dro\u00b7ben", "liegt", "er", "im", "Bett", ",", "ver\u00b7drie\u00df\u00b7lich", ",", "und", "lie\u00b7set", "in", "Schel\u00b7lers"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPRART", "NN", "$,", "ADJD", "$,", "KON", "VVFIN", "APPR", "NN"], "meter": "+-+--+-+--++-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Lexikon! als ich ihn schalt, rief er halb grimmig:", "tokens": ["Le\u00b7xi\u00b7kon", "!", "als", "ich", "ihn", "schalt", ",", "rief", "er", "halb", "grim\u00b7mig", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADJD", "ADJD", "$."], "meter": "+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.4": {"text": "Gebt mir andere Str\u00fcmpf'! die haben L\u00f6cher \u2014 ach freilich", "tokens": ["Gebt", "mir", "an\u00b7de\u00b7re", "Str\u00fcmpf'", "!", "die", "ha\u00b7ben", "L\u00f6\u00b7cher", "ach", "frei\u00b7lich"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "ADJA", "NN", "$.", "ART", "ADJA", "NN", "$(", "ITJ", "ADV"], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Eine Frau mu\u00df in's Haus, die mich von Fu\u00df auf", "tokens": ["Ei\u00b7ne", "Frau", "mu\u00df", "in's", "Haus", ",", "die", "mich", "von", "Fu\u00df", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "$,", "PRELS", "PRF", "APPR", "NN", "APPR"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}