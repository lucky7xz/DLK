-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der grasse Schein,", "tokens": ["Der", "gras\u00b7se", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zernichtender Wetterschlag", "tokens": ["Zer\u00b7nich\u00b7ten\u00b7der", "Wet\u00b7ter\u00b7schlag"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schaffet wohl endlichen Tag \u2013", "tokens": ["Schaf\u00b7fet", "wohl", "end\u00b7li\u00b7chen", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Aber nur dieser,", "tokens": ["A\u00b7ber", "nur", "die\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Leuchtender, freundlicher Tag", "tokens": ["Leuch\u00b7ten\u00b7der", ",", "freund\u00b7li\u00b7cher", "Tag"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Ist auch der dauernde.", "tokens": ["Ist", "auch", "der", "dau\u00b7ern\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.20": {"line.1": {"text": "Stark allein und m\u00e4chtig", "tokens": ["Stark", "al\u00b7lein", "und", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Machet das Gl\u00fcck!", "tokens": ["Ma\u00b7chet", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nur der fr\u00f6hliche Geist", "tokens": ["Nur", "der", "fr\u00f6h\u00b7li\u00b7che", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wird das Unsterbliche thun,", "tokens": ["Wird", "das", "U\u00b7nsterb\u00b7li\u00b7che", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Weltentz\u00fcckende,", "tokens": ["Das", "Wel\u00b7tent\u00b7z\u00fc\u00b7cken\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Edle und Ewige.", "tokens": ["Ed\u00b7le", "und", "E\u00b7wi\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Segen dem Wein.", "tokens": ["Se\u00b7gen", "dem", "Wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Segen der Liebe!", "tokens": ["Se\u00b7gen", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Wer sie vertriebe,", "tokens": ["Wer", "sie", "ver\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "L\u00f6schte den Sonnenschein.", "tokens": ["L\u00f6schte", "den", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.21": {"line.1": {"text": "Komm 