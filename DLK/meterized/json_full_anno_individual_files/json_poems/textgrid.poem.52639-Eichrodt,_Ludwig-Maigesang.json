{"textgrid.poem.52639": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Maigesang", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihn will ich schauen", "tokens": ["Ihn", "will", "ich", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den tiefen blauen", "tokens": ["Den", "tie\u00b7fen", "blau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "S\u00fcdlichen Himmel!", "tokens": ["S\u00fcd\u00b7li\u00b7chen", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das reine, ewige Azur,", "tokens": ["Das", "rei\u00b7ne", ",", "e\u00b7wi\u00b7ge", "A\u00b7zur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das blaue Meer der Wonne,", "tokens": ["Das", "blau\u00b7e", "Meer", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darinnen pr\u00e4chtig blitzt", "tokens": ["Da\u00b7rin\u00b7nen", "pr\u00e4ch\u00b7tig", "blitzt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die goldne Mittagssonne.", "tokens": ["Die", "gold\u00b7ne", "Mit\u00b7tags\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Wonn, o Wonne!", "tokens": ["O", "Wonn", ",", "o", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ich ruht im k\u00fchlen Schattengr\u00fcn", "tokens": ["Ich", "ruht", "im", "k\u00fch\u00b7len", "Schat\u00b7ten\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Freund und Liebchen und Gesang,", "tokens": ["Bei", "Freund", "und", "Lieb\u00b7chen", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Worte, geistesk\u00fchn,", "tokens": ["Und", "Wor\u00b7te", ",", "geis\u00b7tes\u00b7k\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ku\u00df und Klang", "tokens": ["Ku\u00df", "und", "Klang"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sprudelten durch die Blumen hin.", "tokens": ["Spru\u00b7del\u00b7ten", "durch", "die", "Blu\u00b7men", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Gr\u00fc\u00dfende Augen,", "tokens": ["Gr\u00fc\u00b7\u00dfen\u00b7de", "Au\u00b7gen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Gr\u00fc\u00dfender Wein", "tokens": ["Gr\u00fc\u00b7\u00dfen\u00b7der", "Wein"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Goldener kreiset im holden Verein!", "tokens": ["Gol\u00b7de\u00b7ner", "krei\u00b7set", "im", "hol\u00b7den", "Ver\u00b7ein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Buche weht lieblichen Duft,", "tokens": ["Bu\u00b7che", "weht", "lieb\u00b7li\u00b7chen", "Duft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.2": {"text": "Linde w\u00fcrzt schmeichelnde Luft,", "tokens": ["Lin\u00b7de", "w\u00fcrzt", "schmei\u00b7cheln\u00b7de", "Luft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADJA", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Waldv\u00f6gelein ruft,", "tokens": ["Wald\u00b7v\u00f6\u00b7ge\u00b7lein", "ruft", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "++--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Die Wasser rauschen durch Berg und Kluft.", "tokens": ["Die", "Was\u00b7ser", "rau\u00b7schen", "durch", "Berg", "und", "Kluft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "KON", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "O Wonn, o Wonne!", "tokens": ["O", "Wonn", ",", "o", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Seele, du freue dich,", "tokens": ["See\u00b7le", ",", "du", "freu\u00b7e", "dich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Lachendes Herz, erquicke dich!", "tokens": ["La\u00b7chen\u00b7des", "Herz", ",", "er\u00b7qui\u00b7cke", "dich", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Und du vers\u00f6hnter,", "tokens": ["Und", "du", "ver\u00b7s\u00f6hn\u00b7ter", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Neuaufathmender Geist,", "tokens": ["Neu\u00b7auf\u00b7ath\u00b7men\u00b7der", "Geist", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Herrlich entfalte dich!", "tokens": ["Herr\u00b7lich", "ent\u00b7fal\u00b7te", "dich", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.5": {"line.1": {"text": "Die Freud allein,", "tokens": ["Die", "Freud", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.2": {"text": "Die Lust und Lieb allein,", "tokens": ["Die", "Lust", "und", "Lieb", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Strahlende Heiterkeit", "tokens": ["Strah\u00b7len\u00b7de", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ist der bewegende Gott", "tokens": ["Ist", "der", "be\u00b7we\u00b7gen\u00b7de", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "In Menschenbrust \u2013", "tokens": ["In", "Men\u00b7schen\u00b7brust", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Machet sie weit,", "tokens": ["Ma\u00b7chet", "sie", "weit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Machet sie stark,", "tokens": ["Ma\u00b7chet", "sie", "stark", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Hier nur ist Leben,", "tokens": ["Hier", "nur", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Hier Gedeihn.", "tokens": ["Hier", "Ge\u00b7deihn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.6": {"line.1": {"text": "Zum tiefen blauen", "tokens": ["Zum", "tie\u00b7fen", "blau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Azurnen Himmel", "tokens": ["A\u00b7zur\u00b7nen", "Him\u00b7mel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lasset uns schauen!", "tokens": ["Las\u00b7set", "uns", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Also von Wolken rein,", "tokens": ["Al\u00b7so", "von", "Wol\u00b7ken", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Rein vom Nebel der Sorge,", "tokens": ["Rein", "vom", "Ne\u00b7bel", "der", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Rein vom feuchten Kummer,", "tokens": ["Rein", "vom", "feuch\u00b7ten", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Von d\u00fcstrer Entsagung", "tokens": ["Von", "d\u00fcst\u00b7rer", "Ent\u00b7sa\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sei die Seele, sie sei", "tokens": ["Sei", "die", "See\u00b7le", ",", "sie", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PPER", "VAFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Heiter in lachender Menschenbrust!", "tokens": ["Hei\u00b7ter", "in", "la\u00b7chen\u00b7der", "Men\u00b7schen\u00b7brust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Jammer und Noth und Qual", "tokens": ["Jam\u00b7mer", "und", "Noth", "und", "Qual"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "NN", "KON", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wirket das Widrige,", "tokens": ["Wir\u00b7ket", "das", "Wid\u00b7ri\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Kraft der Verzweiflung", "tokens": ["Die", "Kraft", "der", "Ver\u00b7zwei\u00b7flung"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "F\u00fcrchterlich nur,", "tokens": ["F\u00fcrch\u00b7ter\u00b7lich", "nur", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Grausam nur ist sie,", "tokens": ["Grau\u00b7sam", "nur", "ist", "sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "VAFIN", "PPER", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Zeuget das Schreckliche.", "tokens": ["Zeu\u00b7get", "das", "Schreck\u00b7li\u00b7che", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Der Wolken grollen,", "tokens": ["Der", "Wol\u00b7ken", "grol\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Donnerrollen,", "tokens": ["Don\u00b7ner\u00b7rol\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Der grasse Schein,", "tokens": ["Der", "gras\u00b7se", "Schein", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Zernichtender Wetterschlag", "tokens": ["Zer\u00b7nich\u00b7ten\u00b7der", "Wet\u00b7ter\u00b7schlag"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Schaffet wohl endlichen Tag \u2013", "tokens": ["Schaf\u00b7fet", "wohl", "end\u00b7li\u00b7chen", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJA", "NN", "$("], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.6": {"text": "Aber nur dieser,", "tokens": ["A\u00b7ber", "nur", "die\u00b7ser", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADV", "PDAT", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Leuchtender, freundlicher Tag", "tokens": ["Leuch\u00b7ten\u00b7der", ",", "freund\u00b7li\u00b7cher", "Tag"], "token_info": ["word", "punct", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.8": {"text": "Ist auch der dauernde.", "tokens": ["Ist", "auch", "der", "dau\u00b7ern\u00b7de", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Stark allein und m\u00e4chtig", "tokens": ["Stark", "al\u00b7lein", "und", "m\u00e4ch\u00b7tig"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "ADV", "KON", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Machet das Gl\u00fcck!", "tokens": ["Ma\u00b7chet", "das", "Gl\u00fcck", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Nur der fr\u00f6hliche Geist", "tokens": ["Nur", "der", "fr\u00f6h\u00b7li\u00b7che", "Geist"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Wird das Unsterbliche thun,", "tokens": ["Wird", "das", "U\u00b7nsterb\u00b7li\u00b7che", "thun", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Das Weltentz\u00fcckende,", "tokens": ["Das", "Wel\u00b7tent\u00b7z\u00fc\u00b7cken\u00b7de", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-+--", "measure": "unknown.measure.di"}, "line.6": {"text": "Edle und Ewige.", "tokens": ["Ed\u00b7le", "und", "E\u00b7wi\u00b7ge", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "KON", "ADJA", "$."], "meter": "+--+--", "measure": "dactylic.di.plus"}, "line.7": {"text": "Segen dem Wein.", "tokens": ["Se\u00b7gen", "dem", "Wein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Segen der Liebe!", "tokens": ["Se\u00b7gen", "der", "Lie\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.9": {"text": "Wer sie vertriebe,", "tokens": ["Wer", "sie", "ver\u00b7trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.10": {"text": "L\u00f6schte den Sonnenschein.", "tokens": ["L\u00f6schte", "den", "Son\u00b7nen\u00b7schein", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Komm Freund,", "tokens": ["Komm", "Freund", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Wein, Liebchen, komm Gesang!", "tokens": ["Wein", ",", "Lieb\u00b7chen", ",", "komm", "Ge\u00b7sang", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ku\u00df und Klang!", "tokens": ["Ku\u00df", "und", "Klang", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Froh rausche, Strom, das Thal entlang!", "tokens": ["Froh", "rau\u00b7sche", ",", "Strom", ",", "das", "Thal", "ent\u00b7lang", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "$,", "NN", "$,", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Wo sind sie, die Schmerzen?", "tokens": ["Wo", "sind", "sie", ",", "die", "Schmer\u00b7zen", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "ART", "NN", "$."], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Thut auf euch, ihr Herzen,", "tokens": ["Thut", "auf", "euch", ",", "ihr", "Her\u00b7zen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Thut auf euch, ihr Augen,", "tokens": ["Thut", "auf", "euch", ",", "ihr", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPR", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu spiegeln, zu saugen", "tokens": ["Zu", "spie\u00b7geln", ",", "zu", "sau\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Das s\u00fc\u00dfe Dasein nur!", "tokens": ["Das", "s\u00fc\u00b7\u00dfe", "Da\u00b7sein", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Der Wein verm\u00e4hle", "tokens": ["Der", "Wein", "ver\u00b7m\u00e4h\u00b7le"], "token_info": ["word", "word", "word"], "pos": ["ART", "NN", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Geist, Herz, Natur", "tokens": ["Geist", ",", "Herz", ",", "Na\u00b7tur"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["NN", "$,", "NN", "$,", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.8": {"text": "Zu einer Seele..", "tokens": ["Zu", "ei\u00b7ner", "See\u00b7le", ".."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "O Wonn, o Wonne!", "tokens": ["O", "Wonn", ",", "o", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.12": {"line.1": {"text": "Ihn will ich schauen", "tokens": ["Ihn", "will", "ich", "schau\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Den tiefen blauen", "tokens": ["Den", "tie\u00b7fen", "blau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "S\u00fcdlichen Himmel!", "tokens": ["S\u00fcd\u00b7li\u00b7chen", "Him\u00b7mel", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Das reine, ewige Azur,", "tokens": ["Das", "rei\u00b7ne", ",", "e\u00b7wi\u00b7ge", "A\u00b7zur", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Das blaue Meer der Wonne,", "tokens": ["Das", "blau\u00b7e", "Meer", "der", "Won\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Darinnen pr\u00e4chtig blitzt", "tokens": ["Da\u00b7rin\u00b7nen", "pr\u00e4ch\u00b7tig", "blitzt"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die goldne Mittagssonne.", "tokens": ["Die", "gold\u00b7ne", "Mit\u00b7tags\u00b7son\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "O Wonn, o Wonne!", "tokens": ["O", "Wonn", ",", "o", "Won\u00b7ne", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "FM", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.13": {"line.1": {"text": "Ich ruht im k\u00fchlen Schattengr\u00fcn", "tokens": ["Ich", "ruht", "im", "k\u00fch\u00b7len", "Schat\u00b7ten\u00b7gr\u00fcn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Bei Freund und Liebchen und Gesang,", "tokens": ["Bei", "Freund", "und", "Lieb\u00b7chen", "und", "Ge\u00b7sang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Worte, geistesk\u00fchn,", "tokens": ["Und", "Wor\u00b7te", ",", "geis\u00b7tes\u00b7k\u00fchn", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["KON", "NN", "$,", "VVPP", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ku\u00df und Klang", "tokens": ["Ku\u00df", "und", "Klang"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Sprudelten durch die Blumen hin.", "