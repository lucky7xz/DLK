"}, "line.2": {"text": "Die Lust und Lieb allein,", "tokens": ["Die", "Lust", "und", "Lieb", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Strahlende Heiterkeit", "tokens": ["Strah\u00b7len\u00b7de", "Hei\u00b7ter\u00b7keit"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Ist der bewegende Gott", "tokens": ["Ist", "der", "be\u00b7we\u00b7gen\u00b7de", "Gott"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "In Menschenbrust \u2013", "tokens": ["In", "Men\u00b7schen\u00b7brust", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["APPR", "NN", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.6": {"text": "Machet sie weit,", "tokens": ["Ma\u00b7chet", "sie", "weit", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Machet sie stark,", "tokens": ["Ma\u00b7chet", "sie", "stark", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.8": {"text": "Hier nur ist Leben,", "tokens": ["Hier", "nur", "ist", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.9": {"text": "Hier Gedeihn.", "tokens": ["Hier", "Ge\u00b7deihn", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "NN", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.17": {"line.1": {"text": "Zum tiefen blauen", "tokens": ["Zum", "tie\u00b7fen", "blau\u00b7en"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "ADJA", "ADJA"], "meter": "-+-+-", "measure": "iambic.di"}, "line.2": {"text": "Azurnen Himmel", "tokens": ["A\u00b7zur\u00b7nen", "Him\u00b7mel"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.3": {"text": "Lasset uns schauen!", "tokens": ["Las\u00b7set", "uns", "schau\u00b7en", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Also von Wolken rein,", "tokens": ["Al\u00b7so", "von", "Wol\u00b7ken", "rein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "ADJD", "$,"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.5": {"text": "Rein vom Nebel der Sorge,", "tokens": ["Rein", "vom", "Ne\u00b7bel", "der", "Sor\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.6": {"text": "Rein vom feuchten Kummer,", "tokens": ["Rein", "vom", "feuch\u00b7ten", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.7": {"text": "Von d\u00fcstrer Entsagung", "tokens": ["Von", "d\u00fcst\u00b7rer", "Ent\u00b7sa\u00b7gung"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Sei die Seele, sie sei", "tokens": ["Sei", "die", "See\u00b7le", ",", "sie", "sei"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "$,", "PPER", "VAFIN"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Heiter in lachender Menschenbrust!", "tokens": ["Hei\u00b7ter", "in", "la\u00b7chen\u00b7der", "Men\u00b7schen\u00b7brust", "!"], "token_info": ["word", "word", "wo