{"dta.poem.1565": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Frauen Eva von Nimtschin gebohrner von  \n Kottwitzin.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Ausbund aller Fr\u00f6mmigkeit/ ", "tokens": ["Ein", "Aus\u00b7bund", "al\u00b7ler", "Fr\u00f6m\u00b7mig\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Schmuck vom keuschen Frauen-Stande/", "tokens": ["Der", "Schmuck", "vom", "keu\u00b7schen", "Frau\u00b7en\u00b7Stan\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vertraute diesem schlechten Sande", "tokens": ["Ver\u00b7trau\u00b7te", "die\u00b7sem", "schlech\u00b7ten", "San\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der edlen Seele d\u00fcrfftigs Kleid.", "tokens": ["Der", "ed\u00b7len", "See\u00b7le", "d\u00fcr\u00b7ff\u00b7tigs", "Kleid", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Kan wohl jemand ein sch\u00f6ner Grabmahl haben?", "tokens": ["Kan", "wohl", "je\u00b7mand", "ein", "sch\u00f6\u00b7ner", "Grab\u00b7mahl", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PIS", "ART", "ADJA", "NN", "VAFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Die Tugend selbst hat sich mit ihr begraben.", "tokens": ["Die", "Tu\u00b7gend", "selbst", "hat", "sich", "mit", "ihr", "be\u00b7gra\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "PRF", "APPR", "PPOSAT", "VVPP", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Verwest der Leib; der Nachruhm w\u00e4hrt/", "tokens": ["Ver\u00b7west", "der", "Leib", ";", "der", "Nach\u00b7ruhm", "w\u00e4hrt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$.", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wird/ wie schlecht er hier geschienen/", "tokens": ["Und", "wird", "/", "wie", "schlecht", "er", "hier", "ge\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$(", "PWAV", "ADJD", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Geist und Welt die Wette gr\u00fcnen", "tokens": ["Mit", "Geist", "und", "Welt", "die", "Wet\u00b7te", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von Sorg\u2019 und Siechthum unversehrt.", "tokens": ["Von", "Sor\u00b7g'", "und", "Siech\u00b7thum", "un\u00b7ver\u00b7sehrt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ADJD", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "GoTT hat ihm selbst den Schatz zur Braut erlesen/", "tokens": ["GoTT", "hat", "ihm", "selbst", "den", "Schatz", "zur", "Braut", "er\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PPER", "ADV", "ART", "NN", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "De\u00df diese Welt nicht l\u00e4nger werth gewesen.", "tokens": ["De\u00df", "die\u00b7se", "Welt", "nicht", "l\u00e4n\u00b7ger", "werth", "ge\u00b7we\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PDAT", "NN", "PTKNEG", "ADJD", "ADJD", "VAPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Auff Frost und Mertzen folgt der M\u00e4y/", "tokens": ["Auff", "Frost", "und", "Mert\u00b7zen", "folgt", "der", "M\u00e4y", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Auff Wind und Schnee der Sonne Blicken;", "tokens": ["Auff", "Wind", "und", "Schnee", "der", "Son\u00b7ne", "Bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie tr\u00f6stet ewiges Erquicken/", "tokens": ["Sie", "tr\u00f6s\u00b7tet", "e\u00b7wi\u00b7ges", "Er\u00b7qui\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nachdem der Erde Sturm vorbey.", "tokens": ["Nach\u00b7dem", "der", "Er\u00b7de", "Sturm", "vor\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wir sehn ihr nach; Ihr steht schon frey und offen/", "tokens": ["Wir", "sehn", "ihr", "nach", ";", "Ihr", "steht", "schon", "frey", "und", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was wir zum Trost nach so viel Thr\u00e4nen hoffen.", "tokens": ["Was", "wir", "zum", "Trost", "nach", "so", "viel", "Thr\u00e4\u00b7nen", "hof\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "APPR", "ADV", "PIAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}