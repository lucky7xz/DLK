{"dta.poem.13435": {"metadata": {"author": {"name": "Suppius, Christoph Eusebius", "birth": "N.A.", "death": "N.A."}, "title": "Christiane.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1749", "urn": "urn:nbn:de:kobv:b4-20594-7", "language": ["de:0.99"], "booktitle": "Suppius, Christoph Eusebius: Oden und Lieder. Gotha, 1749."}, "poem": {"stanza.1": {"line.1": {"text": "Von Christianen will ich singen,", "tokens": ["Von", "Chris\u00b7ti\u00b7a\u00b7nen", "will", "ich", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die selbst der Tugend wohlgefiel,", "tokens": ["Die", "selbst", "der", "Tu\u00b7gend", "wohl\u00b7ge\u00b7fiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von dieser soll mein Saitenspiel", "tokens": ["Von", "die\u00b7ser", "soll", "mein", "Sai\u00b7ten\u00b7spiel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In abgelegnen Fluhren klingen,", "tokens": ["In", "ab\u00b7ge\u00b7leg\u00b7nen", "Fluh\u00b7ren", "klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Wo Echo denn gedoppelt sagt:", "tokens": ["Wo", "E\u00b7cho", "denn", "ge\u00b7dop\u00b7pelt", "sagt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "KON", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie sehr ich ihren Tod beklagt!", "tokens": ["Wie", "sehr", "ich", "ih\u00b7ren", "Tod", "be\u00b7klagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Zu euch, ihr Berge, Th\u00e4ler, Auen,", "tokens": ["Zu", "euch", ",", "ihr", "Ber\u00b7ge", ",", "Th\u00e4\u00b7ler", ",", "Au\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "PPOSAT", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Zu euch flieh ich! betr\u00fcbni\u00dfvoll!", "tokens": ["Zu", "euch", "flieh", "ich", "!", "be\u00b7tr\u00fcb\u00b7ni\u00df\u00b7voll", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$.", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil ich sie \u00fcberleben soll,", "tokens": ["Weil", "ich", "sie", "\u00fc\u00b7berl\u00b7e\u00b7ben", "soll", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das Muster tugendhafter Frauen,", "tokens": ["Das", "Mus\u00b7ter", "tu\u00b7gend\u00b7haf\u00b7ter", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Euch sey der Kummer, der mich qv\u00e4lt,", "tokens": ["Euch", "sey", "der", "Kum\u00b7mer", ",", "der", "mich", "qv\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch sey mein Herzeleid erzehlt!", "tokens": ["Euch", "sey", "mein", "Her\u00b7ze\u00b7leid", "er\u00b7zehlt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie? trau ich dem betr\u00fcbten Wahne,", "tokens": ["Wie", "?", "trau", "ich", "dem", "be\u00b7tr\u00fcb\u00b7ten", "Wah\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "VVFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der meinen Jammer aufgebracht?", "tokens": ["Der", "mei\u00b7nen", "Jam\u00b7mer", "auf\u00b7ge\u00b7bracht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ist wohl die Nachricht nur erdacht?", "tokens": ["Ist", "wohl", "die", "Nach\u00b7richt", "nur", "er\u00b7dacht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was? lebt sie, meine Christiane,", "tokens": ["Was", "?", "lebt", "sie", ",", "mei\u00b7ne", "Chris\u00b7ti\u00b7a\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "$.", "VVFIN", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die jeder liebensw\u00fcrdig fand,", "tokens": ["Die", "je\u00b7der", "lie\u00b7bens\u00b7w\u00fcr\u00b7dig", "fand", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der ihre Seltenheit gekannt?", "tokens": ["Der", "ih\u00b7re", "Sel\u00b7ten\u00b7heit", "ge\u00b7kannt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "O Himmel! d\u00fcrft ich nur noch hoffen!", "tokens": ["O", "Him\u00b7mel", "!", "d\u00fcrft", "ich", "nur", "noch", "hof\u00b7fen", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Jedoch mein Herz ist allzuschwer,", "tokens": ["Je\u00b7doch", "mein", "Herz", "ist", "all\u00b7zu\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Das saget mir: sie lebt nicht mehr!", "tokens": ["Das", "sa\u00b7get", "mir", ":", "sie", "lebt", "nicht", "mehr", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Todes Hand hat sie getroffen!", "tokens": ["Des", "To\u00b7des", "Hand", "hat", "sie", "ge\u00b7trof\u00b7fen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und heut ist ihr Erblassungs-Tag,", "tokens": ["Und", "heut", "ist", "ihr", "Erb\u00b7las\u00b7sungs\u00b7Tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Woran ich kaum gedenken mag!", "tokens": ["Wo\u00b7ran", "ich", "kaum", "ge\u00b7den\u00b7ken", "mag", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ist der Verlust wohl auszustehen?", "tokens": ["Ist", "der", "Ver\u00b7lust", "wohl", "aus\u00b7zu\u00b7ste\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich sah sie, von mir abgewand,", "tokens": ["Ich", "sah", "sie", ",", "von", "mir", "ab\u00b7ge\u00b7wand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$,", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wohl wissend wie? in fremder Hand,", "tokens": ["Wohl", "wis\u00b7send", "wie", "?", "in", "frem\u00b7der", "Hand", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KOKOM", "$.", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nun soll ich sie gar nicht mehr sehen!", "tokens": ["Nun", "soll", "ich", "sie", "gar", "nicht", "mehr", "se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "ADV", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Verlust! der mir gedoppelt wird:", "tokens": ["Ver\u00b7lust", "!", "der", "mir", "ge\u00b7dop\u00b7pelt", "wird", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "ART", "PPER", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie stirbt! und ist mir auch entf\u00fchrt!", "tokens": ["Sie", "stirbt", "!", "und", "ist", "mir", "auch", "ent\u00b7f\u00fchrt", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ach Christiane! holde Seele!", "tokens": ["Ach", "Chris\u00b7ti\u00b7a\u00b7ne", "!", "hol\u00b7de", "See\u00b7le", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NE", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du mir noch in dem Grabe werth!", "tokens": ["Du", "mir", "noch", "in", "dem", "Gra\u00b7be", "werth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "ADV", "APPR", "ART", "NN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sieh, wie von Harm und Leid verzehrt", "tokens": ["Sieh", ",", "wie", "von", "Harm", "und", "Leid", "ver\u00b7zehrt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "PWAV", "APPR", "NN", "KON", "NN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich mich bey deinem Sterben qv\u00e4le!", "tokens": ["Ich", "mich", "bey", "dei\u00b7nem", "Ster\u00b7ben", "qv\u00e4\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PRF", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Der nun erst zu erkennen giebt,", "tokens": ["Der", "nun", "erst", "zu", "er\u00b7ken\u00b7nen", "giebt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df er dich bis im Tod geliebt!", "tokens": ["Da\u00df", "er", "dich", "bis", "im", "Tod", "ge\u00b7liebt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Nachwelt! sollten meine Lieder", "tokens": ["O", "Nach\u00b7welt", "!", "soll\u00b7ten", "mei\u00b7ne", "Lie\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NN", "$.", "VMFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Noch deiner Zukunft w\u00fcrdig seyn,", "tokens": ["Noch", "dei\u00b7ner", "Zu\u00b7kunft", "w\u00fcr\u00b7dig", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "ADJD", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist ihr Nutz gewi\u00df nicht klein,", "tokens": ["So", "ist", "ihr", "Nutz", "ge\u00b7wi\u00df", "nicht", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie schaffen Christianen wieder,", "tokens": ["Sie", "schaf\u00b7fen", "Chris\u00b7ti\u00b7a\u00b7nen", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "ADV", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und stellen dir ein Muster dar,", "tokens": ["Und", "stel\u00b7len", "dir", "ein", "Mus\u00b7ter", "dar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das unsern Zeiten eigen war.", "tokens": ["Das", "un\u00b7sern", "Zei\u00b7ten", "ei\u00b7gen", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Steigt denn in deinen g\u00fcldnen Tagen", "tokens": ["Steigt", "denn", "in", "dei\u00b7nen", "g\u00fcld\u00b7nen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Werth der deutschen Poesie,", "tokens": ["Der", "Werth", "der", "deut\u00b7schen", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Gut! wahre Tugend altert nie,", "tokens": ["Gut", "!", "wah\u00b7re", "Tu\u00b7gend", "al\u00b7tert", "nie", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$.", "ADJA", "NN", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will sie dir entgegen tragen,", "tokens": ["Ich", "will", "sie", "dir", "ent\u00b7ge\u00b7gen", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PPER", "PTKVZ", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "An Sch\u00f6nheit ist sie immer reich,", "tokens": ["An", "Sch\u00f6n\u00b7heit", "ist", "sie", "im\u00b7mer", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und meiner Christiane gleich.", "tokens": ["Und", "mei\u00b7ner", "Chris\u00b7ti\u00b7a\u00b7ne", "gleich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Jhr K\u00f6rper war so wohlgebauet,", "tokens": ["Ihr", "K\u00f6r\u00b7per", "war", "so", "wohl\u00b7ge\u00b7bau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ja selbst der Reizung Augenmerk,", "tokens": ["Ja", "selbst", "der", "Rei\u00b7zung", "Au\u00b7gen\u00b7merk", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df dieses letzte Tagewerk", "tokens": ["Da\u00df", "die\u00b7ses", "letz\u00b7te", "Ta\u00b7ge\u00b7werk"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PDAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die Liebe ganz entz\u00fcckt geschauet,", "tokens": ["Die", "Lie\u00b7be", "ganz", "ent\u00b7z\u00fcckt", "ge\u00b7schau\u00b7et", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Dabey die Meynung festgestellt:", "tokens": ["Da\u00b7bey", "die", "Mey\u00b7nung", "fest\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Sie ist ein Engel auf der Welt.", "tokens": ["Sie", "ist", "ein", "En\u00b7gel", "auf", "der", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Poeten! wollt ihr Daphnen schildern,", "tokens": ["Po\u00b7et\u00b7en", "!", "wollt", "ihr", "Daph\u00b7nen", "schil\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und l\u00e4sst ein K\u00fcnstler von Athen", "tokens": ["Und", "l\u00e4sst", "ein", "K\u00fcnst\u00b7ler", "von", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Einst wieder eine Venus sehn,", "tokens": ["Einst", "wie\u00b7der", "ei\u00b7ne", "Ve\u00b7nus", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Euch fehlt noch was an euren Bildern,", "tokens": ["Euch", "fehlt", "noch", "was", "an", "eu\u00b7ren", "Bil\u00b7dern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PWS", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Gnug, der Beweis wird mir nicht schwer,", "tokens": ["Gnug", ",", "der", "Be\u00b7weis", "wird", "mir", "nicht", "schwer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ART", "NN", "VAFIN", "PPER", "PTKNEG", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Bey Christianen war noch mehr.", "tokens": ["Bey", "Chris\u00b7ti\u00b7a\u00b7nen", "war", "noch", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Es schien ihr holdes Angesichte,", "tokens": ["Es", "schien", "ihr", "hol\u00b7des", "An\u00b7ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PP