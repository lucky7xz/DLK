tetra"}, "line.3": {"text": "An K\u00fcssen unersch\u00f6pft, nie leer,", "tokens": ["An", "K\u00fcs\u00b7sen", "un\u00b7er\u00b7sch\u00f6pft", ",", "nie", "leer", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "War klein, so zierlich abgemessen,", "tokens": ["War", "klein", ",", "so", "zier\u00b7lich", "ab\u00b7ge\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df, wenn sie solchen nur geregt,", "tokens": ["Da\u00df", ",", "wenn", "sie", "sol\u00b7chen", "nur", "ge\u00b7regt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$,", "KOUS", "PPER", "PIAT", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mein Herze sich zugleich bewegt.", "tokens": ["Mein", "Her\u00b7ze", "sich", "zu\u00b7gleich", "be\u00b7wegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVFIN", "PRF", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Seht, welche wohlgezogne Minen,", "tokens": ["Seht", ",", "wel\u00b7che", "wohl\u00b7ge\u00b7zog\u00b7ne", "Mi\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die immer in dem vollen Licht,", "tokens": ["Die", "im\u00b7mer", "in", "dem", "vol\u00b7len", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"tex