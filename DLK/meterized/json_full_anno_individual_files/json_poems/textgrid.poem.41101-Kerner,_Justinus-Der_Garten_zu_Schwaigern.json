{"textgrid.poem.41101": {"metadata": {"author": {"name": "Kerner, Justinus", "birth": "N.A.", "death": "N.A."}, "title": "Der Garten zu Schwaigern", "genre": "verse", "period": "N.A.", "pub_year": 1824, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Schwaigern steht ein sch\u00f6ner Garten,", "tokens": ["Zu", "Schwai\u00b7gern", "steht", "ein", "sch\u00f6\u00b7ner", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ich schau' ihn stets mit Freuden nur,", "tokens": ["Ich", "schau'", "ihn", "stets", "mit", "Freu\u00b7den", "nur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit Lust bem\u00fcht ist ihn zu warten", "tokens": ["Mit", "Lust", "be\u00b7m\u00fcht", "ist", "ihn", "zu", "war\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "VAFIN", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein edler Liebling der Natur.", "tokens": ["Ein", "ed\u00b7ler", "Lieb\u00b7ling", "der", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Der Rosenflor, den er gezogen,", "tokens": ["Der", "Ro\u00b7sen\u00b7flor", ",", "den", "er", "ge\u00b7zo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der Georginen bunte Zahl", "tokens": ["Der", "Ge\u00b7or\u00b7gi\u00b7nen", "bun\u00b7te", "Zahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gleicht einem farb'gen Regenbogen,", "tokens": ["Gleicht", "ei\u00b7nem", "fa\u00b7rb'\u00b7gen", "Re\u00b7gen\u00b7bo\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der von dem Himmel sank zu Tal.", "tokens": ["Der", "von", "dem", "Him\u00b7mel", "sank", "zu", "Tal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Platanen auch und Pinien heben", "tokens": ["Pla\u00b7ta\u00b7nen", "auch", "und", "Pi\u00b7ni\u00b7en", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "KON", "NN", "VVINF"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ihr gr\u00fcnend Haupt zum Himmel fromm,", "tokens": ["Ihr", "gr\u00fc\u00b7nend", "Haupt", "zum", "Him\u00b7mel", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein Zeuge von vergangnem Leben", "tokens": ["Ein", "Zeu\u00b7ge", "von", "ver\u00b7gang\u00b7nem", "Le\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Schaut ernst durch sie der alte Dom.", "tokens": ["Schaut", "ernst", "durch", "sie", "der", "al\u00b7te", "Dom", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ein Schlo\u00df, von Efeu gr\u00fcn umfangen,", "tokens": ["Ein", "Schlo\u00df", ",", "von", "E\u00b7feu", "gr\u00fcn", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "NE", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Begrenzet diese bunte Flur,", "tokens": ["Be\u00b7gren\u00b7zet", "die\u00b7se", "bun\u00b7te", "Flur", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00dcppig in warmen Beeten prangen", "tokens": ["\u00dcp\u00b7pig", "in", "war\u00b7men", "Bee\u00b7ten", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die Kinder s\u00fcdlicher Natur.", "tokens": ["Die", "Kin\u00b7der", "s\u00fcd\u00b7li\u00b7cher", "Na\u00b7tur", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Hier h\u00e4ngen der Hortensia Dolden", "tokens": ["Hier", "h\u00e4n\u00b7gen", "der", "Hor\u00b7ten\u00b7sia", "Dol\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NE", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Herab in schwerer Bl\u00fcten Wucht,", "tokens": ["Her\u00b7ab", "in", "schwe\u00b7rer", "Bl\u00fc\u00b7ten", "Wucht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dort gl\u00e4nzt aus dunklem Laube golden", "tokens": ["Dort", "gl\u00e4nzt", "aus", "dunk\u00b7lem", "Lau\u00b7be", "gol\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Italiens Orangenfrucht.", "tokens": ["I\u00b7ta\u00b7li\u00b7ens", "O\u00b7ran\u00b7gen\u00b7frucht", "."], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Noch dunkler Laub! noch sch\u00f6nre Bl\u00fcten,", "tokens": ["Noch", "dunk\u00b7ler", "Laub", "!", "noch", "sch\u00f6n\u00b7re", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie sie nur Edens Garten sah!", "tokens": ["Wie", "sie", "nur", "E\u00b7dens", "Gar\u00b7ten", "sah", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Feurig, wie kaum je Rosen gl\u00fchten,", "tokens": ["Feu\u00b7rig", ",", "wie", "kaum", "je", "Ro\u00b7sen", "gl\u00fch\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "PWAV", "ADV", "ADV", "NN", "VVFIN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Ergl\u00fcht hier die Kamelia.", "tokens": ["Er\u00b7gl\u00fcht", "hier", "die", "Ka\u00b7me\u00b7lia", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NE", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.7": {"line.1": {"text": "Des Mittlers Leidenspflanze s\u00e4umet", "tokens": ["Des", "Mitt\u00b7lers", "Lei\u00b7dens\u00b7pflan\u00b7ze", "s\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mit heil'ger Bl\u00fct' des Hauses Wand,", "tokens": ["Mit", "heil'\u00b7ger", "Bl\u00fct'", "des", "Hau\u00b7ses", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dran eine Palme lehnt und tr\u00e4umet", "tokens": ["Dran", "ei\u00b7ne", "Pal\u00b7me", "lehnt", "und", "tr\u00e4u\u00b7met"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ART", "NN", "VVFIN", "KON", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von ihrer Heimat fernem Land.", "tokens": ["Von", "ih\u00b7rer", "Hei\u00b7mat", "fer\u00b7nem", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Pfleger des Gartens! la\u00df mich weilen", "tokens": ["Pfle\u00b7ger", "des", "Gar\u00b7tens", "!", "la\u00df", "mich", "wei\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "VVIMP", "PPER", "VVFIN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Bei ihr! mittr\u00e4umen ihren Traum,", "tokens": ["Bei", "ihr", "!", "mit\u00b7tr\u00e4u\u00b7men", "ih\u00b7ren", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "$.", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Glut jenes Himmels mit ihr teilen,", "tokens": ["Glut", "je\u00b7nes", "Him\u00b7mels", "mit", "ihr", "tei\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "An Euphrats Strand ein heil'ger Baum.", "tokens": ["An", "Eu\u00b7ph\u00b7rats", "Strand", "ein", "heil'\u00b7ger", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "ART", "ADJA", "NN", "$."], "meter": "----+-+-+", "measure": "unknown.measure.tri"}}, "stanza.9": {"line.1": {"text": "Da sieht sie hoch den Ibis fliegen", "tokens": ["Da", "sieht", "sie", "hoch", "den", "I\u00b7bis", "flie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "ART", "NE", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von heil'gen St\u00e4tten hergeweht,", "tokens": ["Von", "heil'\u00b7gen", "St\u00e4t\u00b7ten", "her\u00b7ge\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zephire ihre Bl\u00e4tter wiegen,", "tokens": ["Ze\u00b7phi\u00b7re", "ih\u00b7re", "Bl\u00e4t\u00b7ter", "wie\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die s\u00e4useln wie ein still Gebet.", "tokens": ["Die", "s\u00e4u\u00b7seln", "wie", "ein", "still", "Ge\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "KOKOM", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ein Singen rings um sie und D\u00fcften", "tokens": ["Ein", "Sin\u00b7gen", "rings", "um", "sie", "und", "D\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "APPR", "PPER", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Von bunten V\u00f6geln, Bl\u00fcten viel,", "tokens": ["Von", "bun\u00b7ten", "V\u00f6\u00b7geln", ",", "Bl\u00fc\u00b7ten", "viel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und vor ihr in azurnen L\u00fcften", "tokens": ["Und", "vor", "ihr", "in", "a\u00b7zur\u00b7nen", "L\u00fcf\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Der Fee Morgana Zauberspiel.", "tokens": ["Der", "Fee", "Mor\u00b7ga\u00b7na", "Zau\u00b7ber\u00b7spiel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "So tr\u00e4umt die Palme, kennt die Ferne", "tokens": ["So", "tr\u00e4umt", "die", "Pal\u00b7me", ",", "kennt", "die", "Fer\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht, die sie von der Heimat trennt,", "tokens": ["Nicht", ",", "die", "sie", "von", "der", "Hei\u00b7mat", "trennt", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Weil sie nicht Nordens kalte Sterne", "tokens": ["Weil", "sie", "nicht", "Nor\u00b7dens", "kal\u00b7te", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PTKNEG", "NE", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Bei dieser milden Pflege kennt.", "tokens": ["Bei", "die\u00b7ser", "mil\u00b7den", "Pfle\u00b7ge", "kennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Oft ist's auch mir schon vorgekommen,", "tokens": ["Oft", "ist's", "auch", "mir", "schon", "vor\u00b7ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als sei aus einem w\u00e4rmern Land", "tokens": ["Als", "sei", "aus", "ei\u00b7nem", "w\u00e4r\u00b7mern", "Land"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich auf die kalte Flur gekommen,", "tokens": ["Ich", "auf", "die", "kal\u00b7te", "Flur", "ge\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Dir, S\u00fcdens Palme, so verwandt.", "tokens": ["Dir", ",", "S\u00fc\u00b7dens", "Pal\u00b7me", ",", "so", "ver\u00b7wandt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "$,", "NN", "NN", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Es dringt das Eis von Deutschlands Fluren", "tokens": ["Es", "dringt", "das", "Eis", "von", "Deutschlands", "Flu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NE", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Gar schmerzlich in das Herze mir,", "tokens": ["Gar", "schmerz\u00b7lich", "in", "das", "Her\u00b7ze", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "PDS", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Dann treibt's zu sonnigern Naturen", "tokens": ["Dann", "treibt's", "zu", "son\u00b7ni\u00b7gern", "Na\u00b7tu\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mich oft in Tr\u00e4umen weit von hier.", "tokens": ["Mich", "oft", "in", "Tr\u00e4u\u00b7men", "weit", "von", "hier", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Doch wie hier w\u00e4rmrer Zonen Kinder", "tokens": ["Doch", "wie", "hier", "w\u00e4rm\u00b7rer", "Zo\u00b7nen", "Kin\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ADV", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Treu deine Hand, du Edler, pflegt,", "tokens": ["Treu", "dei\u00b7ne", "Hand", ",", "du", "Ed\u00b7ler", ",", "pflegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,", "PPER", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "So hast du sch\u00fctzend mich nicht minder", "tokens": ["So", "hast", "du", "sch\u00fct\u00b7zend", "mich", "nicht", "min\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "PPER", "PTKNEG",