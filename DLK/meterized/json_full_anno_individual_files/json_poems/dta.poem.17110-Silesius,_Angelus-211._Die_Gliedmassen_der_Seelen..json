{"dta.poem.17110": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "211.  Die Gliedmassen der Seelen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.71", "nl:0.14", "af:0.14"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Die Seel steht mit verstand/ geht mit begierden fort/", "tokens": ["Die", "Seel", "steht", "mit", "ver\u00b7stand", "/", "geht", "mit", "be\u00b7gier\u00b7den", "fort", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "VVFIN", "$(", "VVFIN", "APPR", "ADJA", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Mit Andacht redet sie/ kombt mit Verharrn ann Port.", "tokens": ["Mit", "An\u00b7dacht", "re\u00b7det", "sie", "/", "kombt", "mit", "Ver\u00b7harrn", "ann", "Port", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "$(", "VVFIN", "APPR", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}