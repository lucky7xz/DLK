{"dta.poem.13897": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "V .  \n  Die Heymonskinder .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Den Dichter erbarmet der Rebensaft,", "tokens": ["Den", "Dich\u00b7ter", "er\u00b7bar\u00b7met", "der", "Re\u00b7ben\u00b7saft", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "In den B\u00fcgel er k\u00fchn sich stellt", "tokens": ["In", "den", "B\u00fc\u00b7gel", "er", "k\u00fchn", "sich", "stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "ADJD", "PRF", "VVFIN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Und trinkt mit dem Gotte Br\u00fcderschaft \u2014", "tokens": ["Und", "trinkt", "mit", "dem", "Got\u00b7te", "Br\u00fc\u00b7der\u00b7schaft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "NN", "$("], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nun geht's erst, als ging's aus der Welt!", "tokens": ["Nun", "geht's", "erst", ",", "als", "ging's", "aus", "der", "Welt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "$,", "KOUS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}}}}