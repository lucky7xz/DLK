{"dta.poem.13063": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Siebendes Buch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem sie den Befehl bescheiden ausgesprochen;", "tokens": ["In\u00b7dem", "sie", "den", "Be\u00b7fehl", "be\u00b7schei\u00b7den", "aus\u00b7ge\u00b7spro\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "580Ist ihr aus dem Gesicht die R\u00f6the vorgebrochen.", "tokens": ["ihr", "aus", "dem", "Ge\u00b7sicht", "die", "R\u00f6\u00b7the", "vor\u00b7ge\u00b7bro\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ART", "NN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.3": {"text": "Der Krei\u00df stund aber auf. Man lispelte zugleich,", "tokens": ["Der", "Krei\u00df", "stund", "a\u00b7ber", "auf", ".", "Man", "lis\u00b7pel\u00b7te", "zu\u00b7gleich", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "PTKVZ", "$.", "PIS", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verschiedne wurden still, die roth und andre bleich.", "tokens": ["Ver\u00b7schied\u00b7ne", "wur\u00b7den", "still", ",", "die", "roth", "und", "and\u00b7re", "bleich", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ADJD", "$,", "PRELS", "ADJD", "KON", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ewohlan! \u201e fuhr jemand auf, wer hat den Prei\u00df gewonnen?", "tokens": ["\u201e", "wo\u00b7hlan", "!", "\u201e", "fuhr", "je\u00b7mand", "auf", ",", "wer", "hat", "den", "Prei\u00df", "ge\u00b7won\u00b7nen", "?"], "token_info": ["punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "$.", "$(", "VVFIN", "PIS", "PTKVZ", "$,", "PWS", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "\u201ewarum hat man so lang auf einen Schlu\u00df gesonnen?", "tokens": ["\u201e", "wa\u00b7rum", "hat", "man", "so", "lang", "auf", "ei\u00b7nen", "Schlu\u00df", "ge\u00b7son\u00b7nen", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "VAFIN", "PIS", "ADV", "ADJD", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+---+-+-+-+-", "measure": "dactylic.init"}}}}}