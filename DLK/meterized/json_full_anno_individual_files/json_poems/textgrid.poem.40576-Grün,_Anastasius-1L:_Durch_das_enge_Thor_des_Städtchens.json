en", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7gin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprachen da in stillem Gram:", "tokens": ["Spra\u00b7chen", "da", "in", "stil\u00b7lem", "Gram", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist wohl der arme Alte,", "tokens": ["Wer", "ist", "wohl", "der", "ar\u00b7me", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so elend hier verkam?", "tokens": ["Der", "so", "e\u00b7lend", "hier", "ver\u00b7kam", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.20": {"line.1": {"text": "Doch Natur mit lichtem Auge", "tokens": ["Doch", "Na\u00b7tur", "mit", "lich\u00b7tem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat den Schl\u00e4fer wohl erkannt,", "tokens": ["Hat", "den", "Schl\u00e4\u00b7fer", "wohl", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein feierlich Beg\u00e4ngni\u00df,", "tokens": ["Und", "ein", "fei\u00b7er\u00b7lich", "Be\u00b7g\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "