{"textgrid.poem.40576": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Durch das enge Thor des St\u00e4dtchens", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Durch das enge Thor des St\u00e4dtchens", "tokens": ["Durch", "das", "en\u00b7ge", "Thor", "des", "St\u00e4dt\u00b7chens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht ein alter Bettler fort,", "tokens": ["Zieht", "ein", "al\u00b7ter", "Bett\u00b7ler", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand spendet ihm Geleite,", "tokens": ["Nie\u00b7mand", "spen\u00b7det", "ihm", "Ge\u00b7lei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lebewohl und Abschiedswort.", "tokens": ["Le\u00b7be\u00b7wohl", "und", "Ab\u00b7schieds\u00b7wort", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Nicht verr\u00e4th die graue Wolke,", "tokens": ["Nicht", "ver\u00b7r\u00e4\u00b7th", "die", "grau\u00b7e", "Wol\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Da\u00df sie Botschaft Gottes tr\u00e4gt;", "tokens": ["Da\u00df", "sie", "Bot\u00b7schaft", "Got\u00b7tes", "tr\u00e4gt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Nicht verr\u00e4th der graue Felsen,", "tokens": ["Nicht", "ver\u00b7r\u00e4\u00b7th", "der", "grau\u00b7e", "Fel\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Da\u00df er Schachte Goldes hegt.", "tokens": ["Da\u00df", "er", "Schach\u00b7te", "Gol\u00b7des", "hegt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dem kahlen Baum im Winter", "tokens": ["Und", "dem", "kah\u00b7len", "Baum", "im", "Win\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Seht ihr's auch nicht an sogleich,", "tokens": ["Seht", "ih\u00b7r's", "auch", "nicht", "an", "sog\u00b7leich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "APPR", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er einst so fr\u00f6hlich gr\u00fcnte", "tokens": ["Da\u00df", "er", "einst", "so", "fr\u00f6h\u00b7lich", "gr\u00fcn\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und an Bl\u00fcth' und Frucht so reich.", "tokens": ["Und", "an", "Bl\u00fcth'", "und", "Frucht", "so", "reich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "ADV", "ADJD", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Von dem Mann am Bettelstabe", "tokens": ["Von", "dem", "Mann", "am", "Bet\u00b7tel\u00b7sta\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "H\u00e4tt' es Keiner wohl geglaubt,", "tokens": ["H\u00e4tt'", "es", "Kei\u00b7ner", "wohl", "ge\u00b7glaubt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIS", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df er einst im Purpur strahlte", "tokens": ["Da\u00df", "er", "einst", "im", "Pur\u00b7pur", "strahl\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kronumgl\u00e4nzt sein Lockenhaupt!", "tokens": ["Kro\u00b7num\u00b7gl\u00e4nzt", "sein", "Lo\u00b7cken\u00b7haupt", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Meuter rissen ihm die Krone", "tokens": ["Meu\u00b7ter", "ris\u00b7sen", "ihm", "die", "Kro\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und den lichten Purpur ab,", "tokens": ["Und", "den", "lich\u00b7ten", "Pur\u00b7pur", "ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Reichten ihm, anstatt des Zepters,", "tokens": ["Reich\u00b7ten", "ihm", ",", "an\u00b7statt", "des", "Zep\u00b7ters", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "KOUI", "ART", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Einen morschen Wanderstab.", "tokens": ["Ei\u00b7nen", "mor\u00b7schen", "Wan\u00b7der\u00b7stab", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und so wallt er schon seit Jahren,", "tokens": ["Und", "so", "wallt", "er", "schon", "seit", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ungegr\u00fc\u00dft und ungekannt,", "tokens": ["Un\u00b7ge\u00b7gr\u00fc\u00dft", "und", "un\u00b7ge\u00b7kannt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem schwergebeugten Haupte", "tokens": ["Mit", "dem", "schwer\u00b7ge\u00b7beug\u00b7ten", "Haup\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch so manches fremde Land.", "tokens": ["Durch", "so", "man\u00b7ches", "frem\u00b7de", "Land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "M\u00fcde, todesm\u00fcde sinkt er", "tokens": ["M\u00fc\u00b7de", ",", "to\u00b7des\u00b7m\u00fc\u00b7de", "sinkt", "er"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "ADJD", "VVFIN", "PPER"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unter einen Bl\u00fcthenbaum,", "tokens": ["Un\u00b7ter", "ei\u00b7nen", "Bl\u00fc\u00b7then\u00b7baum", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Von den Zweigen eingesungen", "tokens": ["Von", "den", "Zwei\u00b7gen", "ein\u00b7ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In den tiefen, ew'gen Traum.", "tokens": ["In", "den", "tie\u00b7fen", ",", "ew'\u00b7gen", "Traum", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Menschen, die vor\u00fcbergingen,", "tokens": ["Men\u00b7schen", ",", "die", "vor\u00b7\u00fc\u00b7ber\u00b7gin\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprachen da in stillem Gram:", "tokens": ["Spra\u00b7chen", "da", "in", "stil\u00b7lem", "Gram", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wer ist wohl der arme Alte,", "tokens": ["Wer", "ist", "wohl", "der", "ar\u00b7me", "Al\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Der so elend hier verkam?", "tokens": ["Der", "so", "e\u00b7lend", "hier", "ver\u00b7kam", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Doch Natur mit lichtem Auge", "tokens": ["Doch", "Na\u00b7tur", "mit", "lich\u00b7tem", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Hat den Schl\u00e4fer wohl erkannt,", "tokens": ["Hat", "den", "Schl\u00e4\u00b7fer", "wohl", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein feierlich Beg\u00e4ngni\u00df,", "tokens": ["Und", "ein", "fei\u00b7er\u00b7lich", "Be\u00b7g\u00e4ng\u00b7ni\u00df", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie's dem K\u00f6nig ziemt, gesandt.", "tokens": ["Wie's", "dem", "K\u00f6\u00b7nig", "ziemt", ",", "ge\u00b7sandt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "ART", "NN", "VVFIN", "$,", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Bl\u00fcthenkr\u00e4nze wehn vom Baume", "tokens": ["Bl\u00fc\u00b7then\u00b7kr\u00e4n\u00b7ze", "wehn", "vom", "Bau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihm als Kron' aufs Haupt herab,", "tokens": ["Ihm", "als", "Kron'", "aufs", "Haupt", "her\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "KOUS", "NN", "APPRART", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Zepter \u00fcbergoldet", "tokens": ["Und", "zum", "Zep\u00b7ter", "\u00fc\u00b7ber\u00b7gol\u00b7det"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sonne ihm den Bettelstab.", "tokens": ["Son\u00b7ne", "ihm", "den", "Bet\u00b7tel\u00b7stab", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Rauschend w\u00f6lben sich die Zweige", "tokens": ["Rau\u00b7schend", "w\u00f6l\u00b7ben", "sich", "die", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ueber ihm als Baldachin,", "tokens": ["Ue\u00b7ber", "ihm", "als", "Bal\u00b7da\u00b7ch\u00b7in", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KOUS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und den k\u00f6niglichen Purpur", "tokens": ["Und", "den", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Pur\u00b7pur"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Legt das Abendroth auf ihn.", "tokens": ["Legt", "das", "A\u00b7ben\u00b7droth", "auf", "ihn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Durch das enge Thor des St\u00e4dtchens", "tokens": ["Durch", "das", "en\u00b7ge", "Thor", "des", "St\u00e4dt\u00b7chens"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht ein alter Bettler fort,", "tokens": ["Zieht", "ein", "al\u00b7ter", "Bett\u00b7ler", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART"