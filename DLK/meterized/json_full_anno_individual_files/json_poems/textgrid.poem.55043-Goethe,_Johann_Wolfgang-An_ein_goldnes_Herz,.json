{"textgrid.poem.55043": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "An ein goldnes Herz,", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Angedenken du verklungner Freude,", "tokens": ["An\u00b7ge\u00b7den\u00b7ken", "du", "ver\u00b7klung\u00b7ner", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Das ich immer noch am Halse trage,", "tokens": ["Das", "ich", "im\u00b7mer", "noch", "am", "Hal\u00b7se", "tra\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "H\u00e4ltst du l\u00e4nger als das Seelenband uns beide?", "tokens": ["H\u00e4ltst", "du", "l\u00e4n\u00b7ger", "als", "das", "See\u00b7len\u00b7band", "uns", "bei\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "KOKOM", "ART", "NN", "PPER", "PIS", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "Verl\u00e4ngerst du der Liebe kurze Tage?", "tokens": ["Ver\u00b7l\u00e4n\u00b7gerst", "du", "der", "Lie\u00b7be", "kur\u00b7ze", "Ta\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Flieh ich, Lili, vor dir! Mu\u00df noch an deinem Bande", "tokens": ["Flieh", "ich", ",", "Li\u00b7li", ",", "vor", "dir", "!", "Mu\u00df", "noch", "an", "dei\u00b7nem", "Ban\u00b7de"], "token_info": ["word", "word", "punct", "wor