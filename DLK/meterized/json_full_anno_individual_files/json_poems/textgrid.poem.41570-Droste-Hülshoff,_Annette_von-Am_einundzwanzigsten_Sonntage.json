word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.14": {"line.1": {"text": "Gro\u00df ist die Zahl, das hab' ich erst erfahren,", "tokens": ["Gro\u00df", "ist", "die", "Zahl", ",", "das", "hab'", "ich", "erst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit mich die Wellen unter Menschen trieben.", "tokens": ["Seit", "mich", "die", "Wel\u00b7len", "un\u00b7ter", "Men\u00b7schen", "trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meiner Heimat noch, der frommen, klaren,", "tokens": ["In", "mei\u00b7ner", "Hei\u00b7mat", "noch", ",", "der", "from\u00b7men", ",", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da mu\u00dfte Einsamkeit mich sehr betr\u00fcben,", "tokens": ["Da", "mu\u00df\u00b7te", "Ein\u00b7sam\u00b7keit", "mich", "sehr", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch als ich in die Fremde nun getreten,", "tokens": ["Doch", "als", "ich", "in", "die", "Frem\u00b7de", "nun", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie schauderte mir vor Genossenschaft!", "tokens": ["Wie", "schau\u00b7der\u00b7te", "mir", "vor", "Ge\u00b7nos\u00b7sen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Pilze hingen sie am d\u00fcrren Schaft,", "tokens": ["Wie", "Pil\u00b7ze", "hin\u00b7gen", "sie", "am", "d\u00fcr\u00b7ren", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie Nesseln schossen sie aus allen Beeten.", "tokens": ["Wie", "Nes\u00b7seln", "schos\u00b7sen", "sie", "aus", "al\u00b7len", "Bee\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.15": {"line.1": {"text": "Da sah ich auch, wohin es konnte f\u00fchren", "tokens": ["Da", "sah", "ich", "auch", ",", "wo\u00b7hin", "es", "konn\u00b7te", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mutlos zu stehn auf unterh\u00f6hltem Grunde;", "tokens": ["Mut\u00b7los", "zu", "stehn", "auf", "un\u00b7ter\u00b7h\u00f6hl\u00b7tem", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch durfte meine Hand das Kreuz ber\u00fchren,", "tokens": ["Noch", "durf\u00b7te", "mei\u00b7ne", "Hand", "das", "Kreuz", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch andre h\u00f6rt' ich jubeln tief im Schlunde.", "tokens": ["Doch", "and\u00b7re", "h\u00f6rt'", "ich", "ju\u00b7beln", "tief", "im", "Schlun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da sah ich, wem sich meine Augen wandten,", "tokens": ["Da", "sah", "ich", ",", "wem", "sich", "mei\u00b7ne", "Au\u00b7gen", "wand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da h\u00f6rte ich, was ich vergessen will,", "tokens": ["Da", "h\u00f6r\u00b7te", "ich", ",", "was", "ich", "ver\u00b7ges\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch sprach in mir ein Laut, o steh nicht still!", "tokens": ["Noch", "sprach", "in", "mir", "ein", "Laut", ",", "o", "steh", "nicht", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "FM", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schau jene an, sie sind nur still gestanden!", "tokens": ["Schau", "je\u00b7ne", "an", ",", "sie", "sind", "nur", "still", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.16": {"line.1": {"text": "Seitdem auch wei\u00df ich, wem ich bin gesendet;", "tokens": ["Seit\u00b7dem", "auch", "wei\u00df", "ich", ",", "wem", "ich", "bin", "ge\u00b7sen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem der da steht, wo ich nicht durfte weilen.", "tokens": ["Dem", "der", "da", "steht", ",", "wo", "ich", "nicht", "durf\u00b7te", "wei\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "ADV", "VVFIN", "$,", "PWAV", "PPER", "PTKNEG", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Kein Licht hab' ich was leuchtet oder blendet,", "tokens": ["Kein", "Licht", "hab'", "ich", "was", "leuch\u00b7tet", "o\u00b7der", "blen\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "PPER", "PIS", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nur eine Stimme! die da treibt zu eilen,", "tokens": ["Nur", "ei\u00b7ne", "Stim\u00b7me", "!", "die", "da", "treibt", "zu", "ei\u00b7len", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$.", "ART", "ADV", "VVFIN", "PTKZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O eile! eile! nur die Schritte wende!", "tokens": ["O", "ei\u00b7le", "!", "ei\u00b7le", "!", "nur", "die", "Schrit\u00b7te", "wen\u00b7de", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "$.", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und ob kein Schimmer durch die Wolke bricht,", "tokens": ["Und", "ob", "kein", "Schim\u00b7mer", "durch", "die", "Wol\u00b7ke", "bricht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIAT", "NN", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So denk \u00bbEr herrscht im Dunkel wie im Licht.\u00ab", "tokens": ["So", "denk", "\u00bb", "Er", "herrscht", "im", "Dun\u00b7kel", "wie", "im", "Licht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "$(", "PPER", "VVFIN", "APPRART", "NN", "KOKOM", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und falte nur im Finstern deine H\u00e4nde!", "tokens": ["Und", "fal\u00b7te", "nur", "im", "Fins\u00b7tern", "dei\u00b7ne", "H\u00e4n\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "APPRART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}