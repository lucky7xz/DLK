{"textgrid.poem.41570": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Am einundzwanzigsten Sonntage", "genre": "verse", "period": "N.A.", "pub_year": 1822, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "An manchem Tag mein Hirn wie w\u00fcst und \u00f6de!", "tokens": ["An", "man\u00b7chem", "Tag", "mein", "Hirn", "wie", "w\u00fcst", "und", "\u00f6\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PPOSAT", "NN", "KOKOM", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie eingesargt mein Herz zu manchen Zeiten!", "tokens": ["Wie", "ein\u00b7ge\u00b7sargt", "mein", "Herz", "zu", "man\u00b7chen", "Zei\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Vor \u00fcbergro\u00dfer Schw\u00e4che schein' ich bl\u00f6de,", "tokens": ["Vor", "\u00fc\u00b7ber\u00b7gro\u00b7\u00dfer", "Schw\u00e4\u00b7che", "schein'", "ich", "bl\u00f6\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "PPER", "ADJA", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Bewu\u00dftlos starrt mein Auge durch die Weiten,", "tokens": ["Be\u00b7wu\u00dft\u00b7los", "starrt", "mein", "Au\u00b7ge", "durch", "die", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "O welch ein Bild verschuldeten Verfalles!", "tokens": ["O", "welch", "ein", "Bild", "ver\u00b7schul\u00b7de\u00b7ten", "Ver\u00b7fal\u00b7les", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "O welch ein kl\u00e4glich Bild der Niedrigkeit!", "tokens": ["O", "welch", "ein", "kl\u00e4g\u00b7lich", "Bild", "der", "Nied\u00b7rig\u00b7keit", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "ART", "ADJD", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie f\u00fchl' ich es! doch nicht zu jener Zeit,", "tokens": ["Wie", "f\u00fchl'", "ich", "es", "!", "doch", "nicht", "zu", "je\u00b7ner", "Zeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PPER", "$.", "ADV", "PTKNEG", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wo neblig mir und unverst\u00e4ndlich alles.", "tokens": ["Wo", "neb\u00b7lig", "mir", "und", "un\u00b7ver\u00b7st\u00e4nd\u00b7lich", "al\u00b7les", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "KON", "ADJD", "PIS", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Soll ich es Leichtsinn nennen? O mitnichten!", "tokens": ["Soll", "ich", "es", "Leicht\u00b7sinn", "nen\u00b7nen", "?", "O", "mit\u00b7nich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPER", "NN", "VVINF", "$.", "NE", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie Zentner f\u00fchl' ich es am Herzen liegen.", "tokens": ["Wie", "Zent\u00b7ner", "f\u00fchl'", "ich", "es", "am", "Her\u00b7zen", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Soll ich verstecktem Trotze gleich es richten?", "tokens": ["Soll", "ich", "ver\u00b7steck\u00b7tem", "Trot\u00b7ze", "gleich", "es", "rich\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJA", "NN", "ADV", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dann wahrlich m\u00fc\u00dft' ich mich zum Meister l\u00fcgen!", "tokens": ["Dann", "wahr\u00b7lich", "m\u00fc\u00dft'", "ich", "mich", "zum", "Meis\u00b7ter", "l\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VMFIN", "PPER", "PRF", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Des Trotzes Kraft, des Leichtsinns heiter Prangen,", "tokens": ["Des", "Trot\u00b7zes", "Kraft", ",", "des", "Leicht\u00b7sinns", "hei\u00b7ter", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die sind gebrochen mit dem gleichen Streich;", "tokens": ["Die", "sind", "ge\u00b7bro\u00b7chen", "mit", "dem", "glei\u00b7chen", "Streich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "VVPP", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Nein! einem morschen Stamme bin ich gleich,", "tokens": ["Nein", "!", "ei\u00b7nem", "mor\u00b7schen", "Stam\u00b7me", "bin", "ich", "gleich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "ART", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "An dem die Bl\u00e4tter halbverhungert hangen.", "tokens": ["An", "dem", "die", "Bl\u00e4t\u00b7ter", "halb\u00b7ver\u00b7hun\u00b7gert", "han\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn Nervenspiel mir einmal m\u00f6chte hellen", "tokens": ["Wenn", "Ner\u00b7ven\u00b7spiel", "mir", "ein\u00b7mal", "m\u00f6ch\u00b7te", "hel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPER", "ADV", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Der dumpfen Stirne fieberisch Umgeben,", "tokens": ["Der", "dum\u00b7pfen", "Stir\u00b7ne", "fie\u00b7be\u00b7risch", "Um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Aufsprudeln m\u00f6chten alter Wunden Quellen", "tokens": ["Auf\u00b7spru\u00b7deln", "m\u00f6ch\u00b7ten", "al\u00b7ter", "Wun\u00b7den", "Quel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und sto\u00dfen vor der Worte sengend Leben:", "tokens": ["Und", "sto\u00b7\u00dfen", "vor", "der", "Wor\u00b7te", "sen\u00b7gend", "Le\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Wie zittert meine Hand! wie bricht zusammen", "tokens": ["Wie", "zit\u00b7tert", "mei\u00b7ne", "Hand", "!", "wie", "bricht", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "PWAV", "VVFIN", "PTKVZ"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die K\u00f6rperkraft in solchem Augenblick!", "tokens": ["Die", "K\u00f6r\u00b7per\u00b7kraft", "in", "sol\u00b7chem", "Au\u00b7gen\u00b7blick", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und eine harte Faust st\u00f6\u00dft mich zur\u00fcck", "tokens": ["Und", "ei\u00b7ne", "har\u00b7te", "Faust", "st\u00f6\u00dft", "mich", "zu\u00b7r\u00fcck"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein nutzlos Opfer in die eignen Flammen.", "tokens": ["Ein", "nutz\u00b7los", "Op\u00b7fer", "in", "die", "eig\u00b7nen", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Weh mir, ist dies ein hochzeitliches Kleid,", "tokens": ["Weh", "mir", ",", "ist", "dies", "ein", "hoch\u00b7zeit\u00b7li\u00b7ches", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Worin ich deinen G\u00e4sten mich gesellen", "tokens": ["Wo\u00b7rin", "ich", "dei\u00b7nen", "G\u00e4s\u00b7ten", "mich", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meine arme Lampe lehrbereit", "tokens": ["Und", "mei\u00b7ne", "ar\u00b7me", "Lam\u00b7pe", "lehr\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Herr! an deinen heil'gen Schrein darf stellen?", "tokens": ["O", "Herr", "!", "an", "dei\u00b7nen", "heil'\u00b7gen", "Schrein", "darf", "stel\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein halb Ertrunkner deut' ich nach der K\u00fcste", "tokens": ["Ein", "halb", "Er\u00b7trunk\u00b7ner", "deut'", "ich", "nach", "der", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und aufw\u00e4rts deut' ich schwindelnd, wie verwirrt;", "tokens": ["Und", "auf\u00b7w\u00e4rts", "deut'", "ich", "schwin\u00b7delnd", ",", "wie", "ver\u00b7wirrt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ADJD", "$,", "PWAV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So Israel durch vierzig Jahre irrt'", "tokens": ["So", "Is\u00b7rael", "durch", "vier\u00b7zig", "Jah\u00b7re", "irrt'"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NE", "APPR", "CARD", "NN", "VVFIN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.8": {"text": "Und sucht' und sucht', und fand ein Grab der W\u00fcste.", "tokens": ["Und", "sucht'", "und", "sucht'", ",", "und", "fand", "ein", "Grab", "der", "W\u00fcs\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Doch wei\u00dft du auch, mein Herr und milder Richter!", "tokens": ["Doch", "wei\u00dft", "du", "auch", ",", "mein", "Herr", "und", "mil\u00b7der", "Rich\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es war nicht Eitelkeit, was mich geleitet.", "tokens": ["Es", "war", "nicht", "Ei\u00b7tel\u00b7keit", ",", "was", "mich", "ge\u00b7lei\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "NN", "$,", "PWS", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der z\u00fcndet nicht dem eignen Moder Lichter,", "tokens": ["Der", "z\u00fcn\u00b7det", "nicht", "dem", "eig\u00b7nen", "Mo\u00b7der", "Lich\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wer noch um ird'scher Ehre Kr\u00e4nze streitet!", "tokens": ["Wer", "noch", "um", "ird'\u00b7scher", "Eh\u00b7re", "Kr\u00e4n\u00b7ze", "strei\u00b7tet", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "APPR", "ADJA", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Der l\u00e4\u00dft des Sarges Deckel gern geschlossen.", "tokens": ["Der", "l\u00e4\u00dft", "des", "Sar\u00b7ges", "De\u00b7ckel", "gern", "ge\u00b7schlos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "ADJA", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Doch eben jetzt, all deiner Pfunde bar,", "tokens": ["Doch", "e\u00b7ben", "jetzt", ",", "all", "dei\u00b7ner", "Pfun\u00b7de", "bar", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "$,", "PIAT", "PPOSAT", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Jetzt br\u00e4cht' ich gerne noch ein Scherflein dar", "tokens": ["Jetzt", "br\u00e4cht'", "ich", "ger\u00b7ne", "noch", "ein", "Scher\u00b7flein", "dar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "F\u00fcr alle meines eignen Leids Genossen.", "tokens": ["F\u00fcr", "al\u00b7le", "mei\u00b7nes", "eig\u00b7nen", "Leids", "Ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Gro\u00df ist die Zahl, das hab' ich erst erfahren,", "tokens": ["Gro\u00df", "ist", "die", "Zahl", ",", "das", "hab'", "ich", "erst", "er\u00b7fah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "PDS", "VAFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Seit mich die Wellen unter Menschen trieben.", "tokens": ["Seit", "mich", "die", "Wel\u00b7len", "un\u00b7ter", "Men\u00b7schen", "trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "In meiner Heimat noch, der frommen, klaren,", "tokens": ["In", "mei\u00b7ner", "Hei\u00b7mat", "noch", ",", "der", "from\u00b7men", ",", "kla\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,", "ART", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da mu\u00dfte Einsamkeit mich sehr betr\u00fcben,", "tokens": ["Da", "mu\u00df\u00b7te", "Ein\u00b7sam\u00b7keit", "mich", "sehr", "be\u00b7tr\u00fc\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "NN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch als ich in die Fremde nun getreten,", "tokens": ["Doch", "als", "ich", "in", "die", "Frem\u00b7de", "nun", "ge\u00b7tre\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wie schauderte mir vor Genossenschaft!", "tokens": ["Wie", "schau\u00b7der\u00b7te", "mir", "vor", "Ge\u00b7nos\u00b7sen\u00b7schaft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Wie Pilze hingen sie am d\u00fcrren Schaft,", "tokens": ["Wie", "Pil\u00b7ze", "hin\u00b7gen", "sie", "am", "d\u00fcr\u00b7ren", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Wie Nesseln schossen sie aus allen Beeten.", "tokens": ["Wie", "Nes\u00b7seln", "schos\u00b7sen", "sie", "aus", "al\u00b7len", "Bee\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Da sah ich auch, wohin es konnte f\u00fchren", "tokens": ["Da", "sah", "ich", "auch", ",", "wo\u00b7hin", "es", "konn\u00b7te", "f\u00fch\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$,", "PWAV", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mutlos zu stehn auf unterh\u00f6hltem Grunde;", "tokens": ["Mut\u00b7los", "zu", "stehn", "auf", "un\u00b7ter\u00b7h\u00f6hl\u00b7tem", "Grun\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch durfte meine Hand das Kreuz ber\u00fchren,", "tokens": ["Noch", "durf\u00b7te", "mei\u00b7ne", "Hand", "das", "Kreuz", "be\u00b7r\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPOSAT", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Doch andre h\u00f6rt' ich jubeln tief im Schlunde.", "tokens": ["Doch", "and\u00b7re", "h\u00f6rt'", "ich", "ju\u00b7beln", "tief", "im", "Schlun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "PPER", "VVFIN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Da sah ich, wem sich meine Augen wandten,", "tokens": ["Da", "sah", "ich", ",", "wem", "sich", "mei\u00b7ne", "Au\u00b7gen", "wand\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PRF", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da h\u00f6rte ich, was ich vergessen will,", "tokens": ["Da", "h\u00f6r\u00b7te", "ich", ",", "was", "ich", "ver\u00b7ges\u00b7sen", "will", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Noch sprach in mir ein Laut, o steh nicht still!", "tokens": ["Noch", "sprach", "in", "mir", "ein", "Laut", ",", "o", "steh", "nicht", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "$,", "FM", "VVFIN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Schau jene an, sie sind nur still gestanden!", "tokens": ["Schau", "je\u00b7ne", "an", ",", "sie", "sind", "nur", "still", "ge\u00b7stan\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDS", "PTKVZ", "$,", "PPER", "VAFIN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.8": {"line.1": {"text": "Seitdem auch wei\u00df ich, wem ich bin gesendet;", "tokens": ["Seit\u00b7dem", "auch", "wei\u00df", "ich", ",", "wem", "ich", "bin", "ge\u00b7sen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "w