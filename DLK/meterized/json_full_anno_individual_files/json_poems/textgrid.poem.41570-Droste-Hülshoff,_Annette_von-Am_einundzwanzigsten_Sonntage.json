": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.12": {"line.1": {"text": "Weh mir, ist dies ein hochzeitliches Kleid,", "tokens": ["Weh", "mir", ",", "ist", "dies", "ein", "hoch\u00b7zeit\u00b7li\u00b7ches", "Kleid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "$,", "VAFIN", "PDS", "ART", "ADJA", "NN", "$,"], "meter": "+-+--++--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Worin ich deinen G\u00e4sten mich gesellen", "tokens": ["Wo\u00b7rin", "ich", "dei\u00b7nen", "G\u00e4s\u00b7ten", "mich", "ge\u00b7sel\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PPOSAT", "NN", "PPER", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und meine arme Lampe lehrbereit", "tokens": ["Und", "mei\u00b7ne", "ar\u00b7me", "Lam\u00b7pe", "lehr\u00b7be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "O Herr! an deinen heil'gen Schrein darf stellen?", "tokens": ["O", "Herr", "!", "an", "dei\u00b7nen", "heil'\u00b7gen", "Schrein", "darf", "stel\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$.", "APPR", "PPOSAT", "ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ein halb Ertrunkner deut' ich nach der K\u00fcste", "tokens": ["Ein", "halb", "Er\u00b7trunk\u00b7ner", "deut'", "ich", "nach", "der", "K\u00fcs\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und aufw\u00e4rts deut' ich schwindelnd, wie verwirrt;", "tokens": ["Und", "auf\u00b7w\u00e4rts", "