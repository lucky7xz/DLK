{"textgrid.poem.57587": {"metadata": {"author": {"name": "L\u00f6ns, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "M\u00e4rz", "genre": "verse", "period": "N.A.", "pub_year": 1890, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Jetzt zieht ein s\u00fc\u00dfes, banges Wonneahnen", "tokens": ["Jetzt", "zieht", "ein", "s\u00fc\u00b7\u00dfes", ",", "ban\u00b7ges", "Won\u00b7neah\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Heimlich erschauernd \u00fcber die Natur,", "tokens": ["Heim\u00b7lich", "er\u00b7schau\u00b7ernd", "\u00fc\u00b7ber", "die", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.3": {"text": "Ein unbewu\u00dftes traulich-leises Mahnen", "tokens": ["Ein", "un\u00b7be\u00b7wu\u00df\u00b7tes", "trau\u00b7lich\u00b7lei\u00b7ses", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Des nahen Lenzes erste Werdespur.", "tokens": ["Des", "na\u00b7hen", "Len\u00b7zes", "ers\u00b7te", "Wer\u00b7de\u00b7spur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Am Weidenbusch die Silberk\u00e4tzchen schwellen,", "tokens": ["Am", "Wei\u00b7den\u00b7busch", "die", "Sil\u00b7ber\u00b7k\u00e4tz\u00b7chen", "schwel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es fliegt der erste gelbe Schmetterling,", "tokens": ["Es", "fliegt", "der", "ers\u00b7te", "gel\u00b7be", "Schmet\u00b7ter\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Es murmeln leise die befreiten Wellen,", "tokens": ["Es", "mur\u00b7meln", "lei\u00b7se", "die", "be\u00b7frei\u00b7ten", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Im kahlen Apfelbaum studiert der Fink.", "tokens": ["Im", "kah\u00b7len", "Ap\u00b7fel\u00b7baum", "stu\u00b7diert", "der", "Fink", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Der Winter flieht, der alles kalt und tr\u00fcbe", "tokens": ["Der", "Win\u00b7ter", "flieht", ",", "der", "al\u00b7les", "kalt", "und", "tr\u00fc\u00b7be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "PIS", "ADJD", "KON", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Verschlossen hielt, erk\u00e4ltend jede Glut,", "tokens": ["Ver\u00b7schlos\u00b7sen", "hielt", ",", "er\u00b7k\u00e4l\u00b7tend", "je\u00b7de", "Glut", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "VVPP", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein jedes Herzchen denkt an neue Liebe,", "tokens": ["Ein", "je\u00b7des", "Herz\u00b7chen", "denkt", "an", "neu\u00b7e", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "An helle Kleider und den Sommerhut.", "tokens": ["An", "hel\u00b7le", "Klei\u00b7der", "und", "den", "Som\u00b7mer\u00b7hut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Es kommen jetzt die holden Weihetage,", "tokens": ["Es", "kom\u00b7men", "jetzt", "die", "hol\u00b7den", "Wei\u00b7he\u00b7ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Jedweden Dichter k\u00fc\u00dft der Genius,", "tokens": ["Jed\u00b7we\u00b7den", "Dich\u00b7ter", "k\u00fc\u00dft", "der", "Ge\u00b7nius", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Nach rosa Briefpapier ist gro\u00dfe Frage", "tokens": ["Nach", "ro\u00b7sa", "Brief\u00b7pa\u00b7pier", "ist", "gro\u00b7\u00dfe", "Fra\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "VAFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und der Papierkorb sch\u00e4umt von \u00dcberflu\u00df.", "tokens": ["Und", "der", "Pa\u00b7pier\u00b7korb", "sch\u00e4umt", "von", "\u00dc\u00b7berf\u00b7lu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Nun ruhe, Hand, du hast genug geschrieben \