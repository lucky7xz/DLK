{"dta.poem.21146": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "An Herrn Fleischer,  \n einen Virtuosen auf dem Clavier.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "O Fleischer, umstr\u00f6me mein Herz mit Meeren sera-\nphischer T\u00f6ne;", "tokens": ["O", "Flei\u00b7scher", ",", "um\u00b7str\u00f6\u00b7me", "mein", "Herz", "mit", "Mee\u00b7ren", "se\u00b7ra", "phi\u00b7scher", "T\u00f6\u00b7ne", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "VVFIN", "PPOSAT", "NN", "APPR", "NN", "TRUNC", "ADJA", "NN", "$."], "meter": "-+--+--+-+-+-+-+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Rei\u00df mich zu s\u00fc\u00dfen Entz\u00fcckungen hin!", "tokens": ["Rei\u00df", "mich", "zu", "s\u00fc\u00b7\u00dfen", "Ent\u00b7z\u00fc\u00b7ckun\u00b7gen", "hin", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "+--+---+-+", "measure": "dactylic.di.plus"}, "line.3": {"text": "Du spielst; wie pr\u00e4chtig ert\u00f6nt, die Stimme der m\u00e4ch-", "tokens": ["Du", "spielst", ";", "wie", "pr\u00e4ch\u00b7tig", "er\u00b7t\u00f6nt", ",", "die", "Stim\u00b7me", "der", "m\u00e4ch"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "PWAV", "ADJD", "VVFIN", "$,", "ART", "NN", "ART", "TRUNC"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Durch Silbersaiten des hohen Claviers.", "tokens": ["Durch", "Sil\u00b7ber\u00b7sai\u00b7ten", "des", "ho\u00b7hen", "Cla\u00b7viers", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "So wie im Tempel, das Chor der unentheiligten S\u00e4n-", "tokens": ["So", "wie", "im", "Tem\u00b7pel", ",", "das", "Chor", "der", "un\u00b7en\u00b7thei\u00b7lig\u00b7ten", "S\u00e4n"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOKOM", "APPRART", "NN", "$,", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Ein Fest mit Halleluja begr\u00fc\u00dft;", "tokens": ["Ein", "Fest", "mit", "Hal\u00b7le\u00b7lu\u00b7ja", "be\u00b7gr\u00fc\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NE", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Und in dem Dom der Triumph der majest\u00e4tischen Or-", "tokens": ["Und", "in", "dem", "Dom", "der", "Tri\u00b7umph", "der", "ma\u00b7jes\u00b7t\u00e4\u00b7ti\u00b7schen", "Or"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "ART", "NN", "ART", "ADJA", "TRUNC"], "meter": "---+---+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von heiligen Tagen die Feyer anhebt:", "tokens": ["Von", "hei\u00b7li\u00b7gen", "Ta\u00b7gen", "die", "Fe\u00b7yer", "an\u00b7hebt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "So rauscht Accord durch Accord; doch schnell gehn rie-", "tokens": ["So", "rauscht", "Ac\u00b7cord", "durch", "Ac\u00b7cord", ";", "doch", "schnell", "gehn", "rie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "NN", "APPR", "NN", "$.", "ADV", "ADJD", "VVINF", "TRUNC"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und zarte Triller die Saiten hinauf.", "tokens": ["Und", "zar\u00b7te", "Tril\u00b7ler", "die", "Sai\u00b7ten", "hin\u00b7auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wie \u00e4ngstlich zittert mein Herz vom Winseln der kl\u00e4g-", "tokens": ["Wie", "\u00e4ngst\u00b7lich", "zit\u00b7tert", "mein", "Herz", "vom", "Win\u00b7seln", "der", "kl\u00e4g"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "TRUNC"], "meter": "-+-+--+-+--+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die unter dem schaffenden Finger erseufzt.", "tokens": ["Die", "un\u00b7ter", "dem", "schaf\u00b7fen\u00b7den", "Fin\u00b7ger", "er\u00b7seufzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "So weint im horchenden Wald die Nachtigall z\u00e4rtliche", "tokens": ["So", "weint", "im", "hor\u00b7chen\u00b7den", "Wald", "die", "Nach\u00b7ti\u00b7gall", "z\u00e4rt\u00b7li\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPRART", "ADJA", "NN", "ART", "NN", "ADJA"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "So sang die Colizzi dem lauschenden Ohr;", "tokens": ["So", "sang", "die", "Co\u00b7liz\u00b7zi", "dem", "lau\u00b7schen\u00b7den", "Ohr", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "Und so weint auch ein Poet in Elegien voll Wehmuth", "tokens": ["Und", "so", "weint", "auch", "ein", "Po\u00b7et", "in", "E\u00b7le\u00b7gi\u00b7en", "voll", "Weh\u00b7muth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "APPR", "NE", "ADJD", "NN"], "meter": "+-+-+-+-+-+-+++", "measure": "unknown.measure.octa.plus"}, "line.4": {"text": "Um seiner Sch\u00f6ne fr\u00fchzeitiges Grab.", "tokens": ["Um", "sei\u00b7ner", "Sch\u00f6\u00b7ne", "fr\u00fch\u00b7zei\u00b7ti\u00b7ges", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Unwillig murret der Ba\u00df, da\u00df im Diskante die Sai-", "tokens": ["Un\u00b7wil\u00b7lig", "mur\u00b7ret", "der", "Ba\u00df", ",", "da\u00df", "im", "Dis\u00b7kan\u00b7te", "die", "Sai"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "KOUS", "APPRART", "NN", "ART", "TRUNC"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Die schnelle Rechte heller belebt.", "tokens": ["Die", "schnel\u00b7le", "Rech\u00b7te", "hel\u00b7ler", "be\u00b7lebt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Doch pl\u00f6tzlich brausest du auch mit deiner Linken hin-", "tokens": ["Doch", "pl\u00f6tz\u00b7lich", "brau\u00b7sest", "du", "auch", "mit", "dei\u00b7ner", "Lin\u00b7ken", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+--+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und herrschest zur Oberstimme den Ba\u00df.", "tokens": ["Und", "herr\u00b7schest", "zur", "O\u00b7bers\u00b7tim\u00b7me", "den", "Ba\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Nun jauchzt das ganze Clavier, und feyert hohe Ge-", "tokens": ["Nun", "jauchzt", "das", "gan\u00b7ze", "Cla\u00b7vier", ",", "und", "fe\u00b7yert", "ho\u00b7he", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "KON", "ADJD", "ADJA", "TRUNC"], "meter": "-+-+--+-+-+--", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In Phantasien voll Anmuth und Pracht.", "tokens": ["In", "Phan\u00b7ta\u00b7si\u00b7en", "voll", "An\u00b7muth", "und", "Pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "O Fleischer, folgen dir nicht die m\u00e4chtig bezauberten", "tokens": ["O", "Flei\u00b7scher", ",", "fol\u00b7gen", "dir", "nicht", "die", "m\u00e4ch\u00b7tig", "be\u00b7zau\u00b7ber\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "ART", "ADJD", "VVFIN"], "meter": "-+-+--+-+--+--", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie sonst dem Thrazier W\u00e4lder gefolgt?", "tokens": ["Wie", "sonst", "dem", "Thra\u00b7zier", "W\u00e4l\u00b7der", "ge\u00b7folgt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "NN", "VVPP", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}}}}}