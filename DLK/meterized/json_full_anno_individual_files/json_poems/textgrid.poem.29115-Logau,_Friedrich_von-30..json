{"textgrid.poem.29115": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Pica nam ihr einen G\u00e4rber; selten g\u00e4rbt er oder nie,", "tokens": ["Pi\u00b7ca", "nam", "ihr", "ei\u00b7nen", "G\u00e4r\u00b7ber", ";", "sel\u00b7ten", "g\u00e4rbt", "er", "o\u00b7der", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER", "KON", "ADV", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Trieb vielmehr als wie ein B\u00fctner St\u00e4b und Pr\u00fcgel \u00fcber sie.", "tokens": ["Trieb", "viel\u00b7mehr", "als", "wie", "ein", "B\u00fct\u00b7ner", "St\u00e4b", "und", "Pr\u00fc\u00b7gel", "\u00fc\u00b7ber", "sie", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "KOUS", "KOKOM", "ART", "ADJA", "NN", "KON", "NN", "APPR", "PPER", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Sie besprach das Mittel drum, da\u00df er Handwercks Recht nicht hielte,", "tokens": ["Sie", "be\u00b7sprach", "das", "Mit\u00b7tel", "drum", ",", "da\u00df", "er", "Hand\u00b7wercks", "Recht", "nicht", "hiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PAV", "$,", "KOUS", "PPER", "NN", "NN", "PTKNEG", "VVFIN", "$,"], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.4": {"text": "Da\u00df er G\u00e4rber solte seyn, aber als ein B\u00fctner gilte;", "tokens": ["Da\u00df", "er", "G\u00e4r\u00b7ber", "sol\u00b7te", "seyn", ",", "a\u00b7ber", "als", "ein", "B\u00fct\u00b7ner", "gil\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "VMFIN", "VAINF", "$,", "ADV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.5": {"text": "Doch, so sey er, sprach sie, B\u00fctner; doch er thu, was hier geb\u00fchrt;", "tokens": ["Doch", ",", "so", "sey", "er", ",", "sprach", "sie", ",", "B\u00fct\u00b7ner", ";", "doch", "er", "thu", ",", "was", "hier", "ge\u00b7b\u00fchrt", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ADV", "VAFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "NN", "$.", "KON", "PPER", "VVFIN", "$,", "PRELS", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Da\u00df er Fasse nur nicht bindet, sondern da\u00df er sie auch sch\u00fcrt.", "tokens": ["Da\u00df", "er", "Fas\u00b7se", "nur", "nicht", "bin\u00b7det", ",", "son\u00b7dern", "da\u00df", "er", "sie", "auch", "sch\u00fcrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "ADV", "PTKNEG", "VVFIN", "$,", "KON", "KOUS", "PPER", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Pica nam ihr einen G\u00e4rber; selten g\u00e4rbt er oder nie,", "tokens": ["Pi\u00b7ca", "nam", "ihr", "ei\u00b7nen", "G\u00e4r\u00b7ber", ";", "sel\u00b7ten", "g\u00e4rbt", "er", "o\u00b7der", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ART", "NN", "$.", "ADJD", "VVFIN", "PPER"