{"textgrid.poem.49209": {"metadata": {"author": {"name": "Opitz, Martin", "birth": "N.A.", "death": "N.A."}, "title": "9. Auff einen Ku\u00df", "genre": "verse", "period": "N.A.", "pub_year": 1618, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auff alle meine Noth, auff so viel Angst und Klagen,", "tokens": ["Auff", "al\u00b7le", "mei\u00b7ne", "Noth", ",", "auff", "so", "viel", "Angst", "und", "Kla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "PPOSAT", "NN", "$,", "APPR", "ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Auff Seufftzen, Ach und Weh, auff h\u00f6chste Traurigkeit,", "tokens": ["Auff", "Seufft\u00b7zen", ",", "Ach", "und", "Weh", ",", "auff", "h\u00f6chs\u00b7te", "Trau\u00b7rig\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "ITJ", "KON", "NN", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Auff das, wodurch mein Hertz' empfandt sein tieffes Leid,", "tokens": ["Auff", "das", ",", "wo\u00b7durch", "mein", "Hertz'", "emp\u00b7fandt", "sein", "tief\u00b7fes", "Leid", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PWAV", "PPOSAT", "NN", "VVFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wird doch mein Lieb bewegt mir eins nicht abzuschlagen.", "tokens": ["Wird", "doch", "mein", "Lieb", "be\u00b7wegt", "mir", "eins", "nicht", "ab\u00b7zu\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos":