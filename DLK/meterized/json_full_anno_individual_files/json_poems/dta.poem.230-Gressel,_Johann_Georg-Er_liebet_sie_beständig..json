{"dta.poem.230": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Er liebet sie best\u00e4ndig.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Hertz und Sinn", "tokens": ["Hertz", "und", "Sinn"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Hast du hin/", "tokens": ["Hast", "du", "hin", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKVZ", "$("], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Weiter kan ich dir nichts geben/", "tokens": ["Wei\u00b7ter", "kan", "ich", "dir", "nichts", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PPER", "PIS", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Selbst mein Leben", "tokens": ["Selbst", "mein", "Le\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Ist durch ander stille Macht", "tokens": ["Ist", "durch", "an\u00b7der", "stil\u00b7le", "Macht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Dir auf ewig zugedacht.", "tokens": ["Dir", "auf", "e\u00b7wig", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "ADJD", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich will dein", "tokens": ["Ich", "will", "dein"], "token_info": ["word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Ewig seyn/", "tokens": ["E\u00b7wig", "seyn", "/"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VAINF", "$("], "meter": "+--", "measure": "dactylic.init"}, "line.3": {"text": "Nichtes soll mich ", "tokens": ["Nich\u00b7tes", "soll", "mich"], "token_info": ["word", "word", "word"], "pos": ["PIS", "VMFIN", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.4": {"text": "Hohe Sachen", "tokens": ["Ho\u00b7he", "Sa\u00b7chen"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Sind aus meiner Brust verbannt/", "tokens": ["Sind", "aus", "mei\u00b7ner", "Brust", "ver\u00b7bannt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Wo dein Bild nur ist bekand.", "tokens": ["Wo", "dein", "Bild", "nur", "ist", "be\u00b7kand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ADV", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Denn mein Hertz", "tokens": ["Denn", "mein", "Hertz"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.2": {"text": "Sucht im Schmertz", "tokens": ["Sucht", "im", "Schmertz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "APPRART", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Nur auf Gott und dich zu setzen", "tokens": ["Nur", "auf", "Gott", "und", "dich", "zu", "set\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "NN", "KON", "PPER", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sein Erg\u00f6tzen:", "tokens": ["Sein", "Er\u00b7g\u00f6t\u00b7zen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Drum mein Licht bleib mir getreu/", "tokens": ["Drum", "mein", "Licht", "bleib", "mir", "ge\u00b7treu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Da\u00df ich nicht des Todes sey.", "tokens": ["Da\u00df", "ich", "nicht", "des", "To\u00b7des", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}