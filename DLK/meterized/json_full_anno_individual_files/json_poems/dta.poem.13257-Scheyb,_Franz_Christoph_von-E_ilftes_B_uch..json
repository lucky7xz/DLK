{"dta.poem.13257": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "E ilftes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eich wu\u00dfte nicht was mich fast aus mir selber bracht;", "tokens": ["\u201e", "ich", "wu\u00df\u00b7te", "nicht", "was", "mich", "fast", "aus", "mir", "sel\u00b7ber", "bracht", ";"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PTKNEG", "PWS", "PPER", "ADV", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "330\u201dIch hatte sonderbar auf die Bedeutung acht,", "tokens": ["\"", "Ich", "hat\u00b7te", "son\u00b7der\u00b7bar", "auf", "die", "Be\u00b7deu\u00b7tung", "acht", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VAFIN", "ADJD", "APPR", "ART", "NN", "CARD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "\u201evon der das frohe Volck verschiedne Reden f\u00fchrte,", "tokens": ["\u201e", "von", "der", "das", "fro\u00b7he", "Volck", "ver\u00b7schied\u00b7ne", "Re\u00b7den", "f\u00fchr\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPR", "ART", "ART", "ADJA", "NN", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u201eund mein Gem\u00fcth sowohl, als Aug und Ohren r\u00fchrte.", "tokens": ["\u201e", "und", "mein", "Ge\u00b7m\u00fcth", "so\u00b7wohl", ",", "als", "Aug", "und", "Oh\u00b7ren", "r\u00fchr\u00b7te", "."], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "PPOSAT", "NN", "KON", "$,", "KOUS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "\u201ees leb ", "tokens": ["\u201e", "es", "leb"], "token_info": ["punct", "word", "word"], "pos": ["$(", "PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.6": {"text": "\u201edenselben trifft gewi\u00df die n\u00e4chste Kayser-Wahl!", "tokens": ["\u201e", "den\u00b7sel\u00b7ben", "trifft", "ge\u00b7wi\u00df", "die", "n\u00e4chs\u00b7te", "Kay\u00b7ser\u00b7Wahl", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PDS", "VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "335\u201dHier ist ihr Aufenthalt! hier seynd der Adler Nester!", "tokens": ["\"", "Hier", "ist", "ihr", "Auf\u00b7ent\u00b7halt", "!", "hier", "seynd", "der", "Ad\u00b7ler", "Nes\u00b7ter", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "PPOSAT", "NN", "$.", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "\u201ejezt ist schon wiederum der Wohlfart Hoffnung fester.", "tokens": ["\u201e", "jezt", "ist", "schon", "wie\u00b7de\u00b7rum", "der", "Wohl\u00b7fart", "Hoff\u00b7nung", "fes\u00b7ter", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VAFIN", "ADV", "ADV", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "\u201edergleichen war, was ich mit Wunder angeh\u00f6rt,", "tokens": ["\u201e", "derg\u00b7lei\u00b7chen", "war", ",", "was", "ich", "mit", "Wun\u00b7der", "an\u00b7ge\u00b7h\u00f6rt", ","], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PIS", "VAFIN", "$,", "PWS", "PPER", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "\u201eindem des Volcks Gel\u00e4uf und Jauchzen sich gemehrt.", "tokens": ["\u201e", "in\u00b7dem", "des", "Volcks", "Ge\u00b7l\u00e4uf", "und", "Jauch\u00b7zen", "sich", "ge\u00b7mehrt", "."], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KOUS", "ART", "NN", "NN", "KON", "NN", "PRF", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "\u201ewie viele sah\u2019 ich nicht gar auf die D\u00e4cher steigen,", "tokens": ["\u201e", "wie", "vie\u00b7le", "sah'", "ich", "nicht", "gar", "auf", "die", "D\u00e4\u00b7cher", "stei\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWAV", "PIS", "VVFIN", "PPER", "PTKNEG", "ADV", "APPR", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "340\u201dEinander diese Schaar, die Wunder-Schaar zu zeigen!", "tokens": ["\"", "Ein\u00b7an\u00b7der", "die\u00b7se", "Schaar", ",", "die", "Wun\u00b7der\u00b7Schaar", "zu", "zei\u00b7gen", "!"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "PDAT", "NN", "$,", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}