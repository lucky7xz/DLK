{"dta.poem.1371": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Himmel-Schl\u00fcssel  \n oder  \n Geistliche Gedichte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "GoTT lebt: was trauren wir? Er lebet uns zum Besten;", "tokens": ["GoTT", "lebt", ":", "was", "trau\u00b7ren", "wir", "?", "Er", "le\u00b7bet", "uns", "zum", "Bes\u00b7ten", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie sehr uns Hertzeleid/ Furcht/ Sorg\u2019 und S\u00fcnde", "tokens": ["Wie", "sehr", "uns", "Hert\u00b7ze\u00b7leid", "/", "Furcht", "/", "Sor\u00b7g'", "und", "S\u00fcn\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "PPER", "NN", "$(", "NN", "$(", "NN", "KON", "NN"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "questen/", "tokens": ["ques\u00b7ten", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.4": {"text": "So lebet seine Gnad/ und \u00f6ffnet uns die Th\u00fcr", "tokens": ["So", "le\u00b7bet", "sei\u00b7ne", "Gnad", "/", "und", "\u00f6ff\u00b7net", "uns", "die", "Th\u00fcr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Aus Angst zu Freud und Ruh: GOtt lebt; was trauren", "tokens": ["Aus", "Angst", "zu", "Freud", "und", "Ruh", ":", "Gott", "lebt", ";", "was", "trau\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "APPR", "NN", "KON", "NN", "$.", "NN", "VVFIN", "$.", "PWS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "GoTT liebt: was trauren wir? Wir f\u00fchlen manche Pla-", "tokens": ["GoTT", "liebt", ":", "was", "trau\u00b7ren", "wir", "?", "Wir", "f\u00fch\u00b7len", "man\u00b7che", "Pla"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PWS", "VVFIN", "PPER", "$.", "PPER", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Es wird uns Hertz und Geist und Bein und Marck zerschla-", "tokens": ["Es", "wird", "uns", "Hertz", "und", "Geist", "und", "Bein", "und", "Marck", "zer\u00b7schla"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "NN", "KON", "NN", "KON", "NN", "KON", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Aus Liebe/ nicht aus Ha\u00df; was schreiben wir ihm f\u00fcr!", "tokens": ["Aus", "Lie\u00b7be", "/", "nicht", "aus", "Ha\u00df", ";", "was", "schrei\u00b7ben", "wir", "ihm", "f\u00fcr", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$(", "PTKNEG", "APPR", "NN", "$.", "PWS", "VVFIN", "PPER", "PPER", "APPR", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er hilfft uns/ wenn es gutt/ GOTT liebt: was trauren", "tokens": ["Er", "hilfft", "uns", "/", "wenn", "es", "gutt", "/", "GoTT", "liebt", ":", "was", "trau\u00b7ren"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$(", "KOUS", "PPER", "ADJD", "$(", "NE", "VVFIN", "$.", "PWS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}