{"dta.poem.1858": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Ubersezte Spruch-Reimen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00df deinen Sinn das Gl\u00fccke nicht verdrehn/", "tokens": ["La\u00df", "dei\u00b7nen", "Sinn", "das", "Gl\u00fc\u00b7cke", "nicht", "ver\u00b7drehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "NN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das selten lang auff ebnem Fusse steht;", "tokens": ["Das", "sel\u00b7ten", "lang", "auff", "eb\u00b7nem", "Fus\u00b7se", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJD", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bleib unbewegt/ und lerne wie es geht/", "tokens": ["Bleib", "un\u00b7be\u00b7wegt", "/", "und", "ler\u00b7ne", "wie", "es", "geht", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "$(", "KON", "VVFIN", "KOKOM", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Gutt oder schlimm/ mit gleichem Auge sehn.", "tokens": ["Gutt", "o\u00b7der", "schlimm", "/", "mit", "glei\u00b7chem", "Au\u00b7ge", "sehn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$(", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}