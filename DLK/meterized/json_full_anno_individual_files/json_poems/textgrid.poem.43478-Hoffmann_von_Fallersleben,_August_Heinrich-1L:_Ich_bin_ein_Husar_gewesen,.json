], "pos": ["KOUS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Und unsers K\u00f6nigs Aufruf", "tokens": ["Und", "un\u00b7sers", "K\u00f6\u00b7nigs", "Auf\u00b7ruf"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Blieb uns ein heilig Wort:", "tokens": ["Blieb", "uns", "ein", "hei\u00b7lig", "Wort", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wir jagten die Franzosen", "tokens": ["Wir", "jag\u00b7ten", "die", "Fran\u00b7zo\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Aus unserm Lande fort.", "tokens": ["Aus", "un\u00b7serm", "Lan\u00b7de", "fort", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Und ruft der K\u00f6nig wieder,", "tokens": ["Und", "ruft", "der", "K\u00f6\u00b7nig", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ich schw\u00f6r's bei Bart und Haar,", "tokens": ["Ich", "schw\u00f6r's", "bei", "Bart", "und", "Haar", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch heute werd' ich wieder", "tokens": ["Noch", "heu\u00b7te", "werd'", "ich", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+-"