{"textgrid.poem.37249": {"metadata": {"author": {"name": "Busch, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "[ihr kennt ihn doch schon manches Jahr]", "genre": "verse", "period": "N.A.", "pub_year": 1870, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr kennt ihn doch schon manches Jahr,", "tokens": ["Ihr", "kennt", "ihn", "doch", "schon", "man\u00b7ches", "Jahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wi\u00dft, was es f\u00fcr ein Vogel war;", "tokens": ["Wi\u00dft", ",", "was", "es", "f\u00fcr", "ein", "Vo\u00b7gel", "war", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWS", "PPER", "APPR", "ART", "NE", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie er in allen Gartenr\u00e4umen", "tokens": ["Wie", "er", "in", "al\u00b7len", "Gar\u00b7ten\u00b7r\u00e4u\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Herumgeflattert auf den B\u00e4umen;", "tokens": ["Her\u00b7um\u00b7ge\u00b7flat\u00b7tert", "auf", "den", "B\u00e4u\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie er die h\u00fcbschen roten Beeren,", "tokens": ["Wie", "er", "die", "h\u00fcb\u00b7schen", "ro\u00b7ten", "Bee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die ander