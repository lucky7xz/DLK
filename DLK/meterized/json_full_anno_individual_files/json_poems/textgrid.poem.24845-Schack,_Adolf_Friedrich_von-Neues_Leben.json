{"textgrid.poem.24845": {"metadata": {"author": {"name": "Schack, Adolf Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "Neues Leben", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Heil, goldener Morgen! Erschlie\u00df mir das Thor", "tokens": ["Heil", ",", "gol\u00b7de\u00b7ner", "Mor\u00b7gen", "!", "Er\u00b7schlie\u00df", "mir", "das", "Thor"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "VVFIN", "PPER", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Des neuen Lebenstages!", "tokens": ["Des", "neu\u00b7en", "Le\u00b7bens\u00b7ta\u00b7ges", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Noch nie begr\u00fc\u00dft' ich dein Licht zuvor", "tokens": ["Noch", "nie", "be\u00b7gr\u00fc\u00dft'", "ich", "dein", "Licht", "zu\u00b7vor"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "So freudigen Herzenschlages.", "tokens": ["So", "freu\u00b7di\u00b7gen", "Her\u00b7zen\u00b7schla\u00b7ges", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Wir haben geatmet Mund an Mund,", "tokens": ["Wir", "ha\u00b7ben", "ge\u00b7at\u00b7met", "Mund", "an", "Mund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Uns Aug' in Auge gespiegelt,", "tokens": ["Uns", "Aug'", "in", "Au\u00b7ge", "ge\u00b7spie\u00b7gelt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Indessen die Lippen den gro\u00dfen Bund", "tokens": ["In\u00b7des\u00b7sen", "die", "Lip\u00b7pen", "den", "gro\u00b7\u00dfen", "Bund"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "ART", "ADJA", "NN"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Im heiligen Ku\u00df besiegelt.", "tokens": ["Im", "hei\u00b7li\u00b7gen", "Ku\u00df", "be\u00b7sie\u00b7gelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Mein darf ich, mein f\u00fcr Leben und Tod,", "tokens": ["Mein", "darf", "ich", ",", "mein", "f\u00fcr", "Le\u00b7ben", "und", "Tod", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VMFIN", "PPER", "$,", "PPOSAT", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "F\u00fcr hier und dr\u00fcben sie hei\u00dfen;", "tokens": ["F\u00fcr", "hier", "und", "dr\u00fc\u00b7ben", "sie", "hei\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "VVFIN", "PPER", "VVINF", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Und ob die ganze Welt uns bedroht,", "tokens": ["Und", "ob", "die", "gan\u00b7ze", "Welt", "uns", "be\u00b7droht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Wer will auseinander