{"textgrid.poem.61176": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Lied", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich habe wohl in jungen Tagen", "tokens": ["Ich", "ha\u00b7be", "wohl", "in", "jun\u00b7gen", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich stark in mir geglaubt und fest", "tokens": ["Mich", "stark", "in", "mir", "ge\u00b7glaubt", "und", "fest"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADJD", "APPR", "PPER", "VVPP", "KON", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und keck der Sorgen mich entschlagen,", "tokens": ["Und", "keck", "der", "Sor\u00b7gen", "mich", "ent\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sah ich den Vogel baun sein Nest.", "tokens": ["Sah", "ich", "den", "Vo\u00b7gel", "baun", "sein", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NE", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch kommt die Zeit, wo auch den S\u00e4nger", "tokens": ["Doch", "kommt", "die", "Zeit", ",", "wo", "auch", "den", "S\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Die Sehnsucht fasset bang und b\u00e4nger,", "tokens": ["Die", "Sehn\u00b7sucht", "fas\u00b7set", "bang", "und", "b\u00e4n\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Und wo das m\u00fcde Herz nicht l\u00e4nger", "tokens": ["Und", "wo", "das", "m\u00fc\u00b7de", "Herz", "nicht", "l\u00e4n\u00b7ger"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "PTKNEG", "ADJD"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Sich um sein Recht betr\u00fcgen l\u00e4\u00dft.", "tokens": ["Sich", "um", "sein", "Recht", "be\u00b7tr\u00fc\u00b7gen", "l\u00e4\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "APPR", "PPOSAT", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun bl\u00fcht um mich das Land der Reben,", "tokens": ["Nun", "bl\u00fcht", "um", "mich", "das", "Land", "der", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PRF", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und Burgen winken \u00fcberm Rhein;", "tokens": ["Und", "Bur\u00b7gen", "win\u00b7ken", "\u00fc\u00b7berm", "Rhein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mich tr\u00e4gt der Kahn mit leisem Schweben", "tokens": ["Mich", "tr\u00e4gt", "der", "Kahn", "mit", "lei\u00b7sem", "Schwe\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "lin