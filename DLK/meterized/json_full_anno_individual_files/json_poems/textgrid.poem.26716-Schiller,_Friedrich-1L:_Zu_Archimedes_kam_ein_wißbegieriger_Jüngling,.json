{"textgrid.poem.26716": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Zu Archimedes kam ein wi\u00dfbegieriger J\u00fcngling,", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zu Archimedes kam ein wi\u00dfbegieriger J\u00fcngling,", "tokens": ["Zu", "Ar\u00b7chi\u00b7me\u00b7des", "kam", "ein", "wi\u00df\u00b7be\u00b7gie\u00b7ri\u00b7ger", "J\u00fcng\u00b7ling", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+-+--+-", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "\u00bbweihe mich,\u00ab sprach er zu ihm, \u00bbein in die g\u00f6ttliche Kunst,", "tokens": ["\u00bb", "wei\u00b7he", "mich", ",", "\u00ab", "sprach", "er", "zu", "ihm", ",", "\u00bb", "ein", "in", "die", "g\u00f6tt\u00b7li\u00b7che", "Kunst", ","], "token_info": ["punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVFIN", "PPER", "$,", "$(", "VVFIN", "PPER", "APPR", "PPER", "$,", "$(", "ART", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Die so herrliche Frucht dem Vaterlande getragen", "tokens": ["Die", "so", "herr\u00b7li\u00b7che", "Frucht", "dem", "Va\u00b7ter\u00b7lan\u00b7de", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADV", "ADJA", "NN", "ART", "NN", "VVPP"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.4": {"text": "Und die Mauren der Stadt vor der Sambuca", "tokens": ["Und", "die", "Mau\u00b7ren", "der", "Stadt", "vor", "der", "Sam\u00b7bu\u00b7ca"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["K