{"dta.poem.2137": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "4. Des Geschmacks.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Was k\u00f6nnten wir vor Lust im Schmecken,", "tokens": ["Was", "k\u00f6nn\u00b7ten", "wir", "vor", "Lust", "im", "Schme\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn man daran nur d\u00e4cht\u2019, entdecken!", "tokens": ["Wenn", "man", "da\u00b7ran", "nur", "d\u00e4cht'", ",", "ent\u00b7de\u00b7cken", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "PAV", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es sind die Kr\u00e4fte nicht zu z\u00e4hlen, die in den e\u00dfbarn", "tokens": ["Es", "sind", "die", "Kr\u00e4f\u00b7te", "nicht", "zu", "z\u00e4h\u00b7len", ",", "die", "in", "den", "e\u00df\u00b7barn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "PTKNEG", "PTKZU", "VVINF", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "K\u00f6rpern stecken;", "tokens": ["K\u00f6r\u00b7pern", "ste\u00b7cken", ";"], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Doch w\u00e4ren sie f\u00fcr uns nicht da, wofern nicht eine", "tokens": ["Doch", "w\u00e4\u00b7ren", "sie", "f\u00fcr", "uns", "nicht", "da", ",", "wo\u00b7fern", "nicht", "ei\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VAFIN", "PPER", "APPR", "PPER", "PTKNEG", "ADV", "$,", "KOUS", "PTKNEG", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wunderkraft", "tokens": ["Wun\u00b7der\u00b7kraft"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "In unsrer Zungen Bau gesenket, und die empfindend\u2019", "tokens": ["In", "uns\u00b7rer", "Zun\u00b7gen", "Bau", "ge\u00b7sen\u00b7ket", ",", "und", "die", "emp\u00b7fin\u00b7dend'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN", "VVPP", "$,", "KON", "ART", "NN"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.8": {"text": "Eigenschaft,", "tokens": ["Ei\u00b7gen\u00b7schaft", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "So manchen Saft zu unterscheiden. Bewundert doch,", "tokens": ["So", "man\u00b7chen", "Saft", "zu", "un\u00b7ter\u00b7schei\u00b7den", ".", "Be\u00b7wun\u00b7dert", "doch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "PTKZU", "VVINF", "$.", "VVFIN", "ADV", "$,"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.10": {"text": "wie unsre Seelen", "tokens": ["wie", "uns\u00b7re", "See\u00b7len"], "token_info": ["word", "word", "word"], "pos": ["PWAV", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Mit so viel Wesen dieser Welt, durch dieses Werk-", "tokens": ["Mit", "so", "viel", "We\u00b7sen", "die\u00b7ser", "Welt", ",", "durch", "die\u00b7ses", "Werk"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ADV", "PIAT", "NN", "PDAT", "NN", "$,", "APPR", "PDAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "zeug, sich verm\u00e4hlen.", "tokens": ["zeug", ",", "sich", "ver\u00b7m\u00e4h\u00b7len", "."], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRF", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "Auf! la\u00dft uns denn mit Freuden trinken, auf! la\u00dft", "tokens": ["Auf", "!", "la\u00dft", "uns", "denn", "mit", "Freu\u00b7den", "trin\u00b7ken", ",", "auf", "!", "la\u00dft"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "$.", "VVIMP", "PPER", "ADV", "APPR", "NN", "VVINF", "$,", "PTKVZ", "$.", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "uns mit Vergn\u00fcgen essen,", "tokens": ["uns", "mit", "Ver\u00b7gn\u00fc\u00b7gen", "es\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Die\u00df ist des Sch\u00f6pfers Will\u2019 und Absicht. Wenn", "tokens": ["Die\u00df", "ist", "des", "Sch\u00f6p\u00b7fers", "Will'", "und", "Ab\u00b7sicht", ".", "Wenn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "VAFIN", "ART", "NN", "NE", "KON", "NN", "$.", "KOUS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "wir dabey nun auch ermessen,", "tokens": ["wir", "da\u00b7bey", "nun", "auch", "er\u00b7mes\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PAV", "ADV", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Da\u00df uns, ein liebreich weises Wesen, mit einer sol-", "tokens": ["Da\u00df", "uns", ",", "ein", "lieb\u00b7reich", "wei\u00b7ses", "We\u00b7sen", ",", "mit", "ei\u00b7ner", "sol"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJD", "ADJA", "NN", "$,", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "chen Wundergabe,", "tokens": ["chen", "Wun\u00b7der\u00b7ga\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.19": {"text": "Der Erden Sch\u00e4tze zu genie\u00dfen, aus lauter Huld be-", "tokens": ["Der", "Er\u00b7den", "Sch\u00e4t\u00b7ze", "zu", "ge\u00b7nie\u00b7\u00dfen", ",", "aus", "lau\u00b7ter", "Huld", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "PTKZU", "VVINF", "$,", "APPR", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.20": {"text": "schenket habe,", "tokens": ["schen\u00b7ket", "ha\u00b7be", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VAFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Sein Freundlich-seyn im Schmecken schmecken; so ha-", "tokens": ["Sein", "Freund\u00b7lich\u00b7seyn", "im", "Schme\u00b7cken", "schme\u00b7cken", ";", "so", "ha"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVINF", "$.", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.22": {"text": "ben wir mit unsern Zungen,", "tokens": ["ben", "wir", "mit", "un\u00b7sern", "Zun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Auch wenn wir essen, ihm gedient, und Gott, in", "tokens": ["Auch", "wenn", "wir", "es\u00b7sen", ",", "ihm", "ge\u00b7dient", ",", "und", "Gott", ",", "in"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "KOUS", "PPER", "VVINF", "$,", "PPER", "VVPP", "$,", "KON", "NN", "$,", "APPR"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "unsrer Lust, besungen.", "tokens": ["uns\u00b7rer", "Lust", ",", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}}}}