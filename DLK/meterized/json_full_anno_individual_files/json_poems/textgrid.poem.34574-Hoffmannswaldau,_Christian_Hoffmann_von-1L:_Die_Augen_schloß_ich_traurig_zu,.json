ure": "iambic.tetra"}, "line.4": {"text": "Ich w\u00fcnsche, von der Welt zu fliehen.", "tokens": ["Ich", "w\u00fcn\u00b7sche", ",", "von", "der", "Welt", "zu", "flie\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ich mag nicht Scepter, mag nicht Gold;", "tokens": ["Ich", "mag", "nicht", "Scep\u00b7ter", ",", "mag", "nicht", "Gold", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "NN", "$,", "VMFIN", "PTKNEG", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Man sei mir feind, man sei mir hold,", "tokens": ["Man", "sei", "mir", "feind", ",", "man", "sei", "mir", "hold", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VAFIN", "PPER", "NN", "$,", "PIS", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es soll mich Beides gleich erquicken.", "tokens": ["Es", "soll", "mich", "Bei\u00b7des", "gleich", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die Liebe, so uns n\u00e4rrisch macht", "tokens": ["Die", "Lie\u00b7be", ",", "so", "uns", "n\u00e4r\u00b7risch", "macht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und uns bezwingt mit dicker Nacht,", "tokens": ["Und", "uns", "be\u00b7zwingt", "mit", "di\u00b7cker", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Soll mir den Compa\u00df nicht verr\u00fccken.", "tokens": ["Soll", "mir", "den", "Com\u00b7pa\u00df", "nicht", "ver\u00b7r\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ich lache, wenn ich \u00fcberhin", "tokens": ["Ich", "la\u00b7che", ",", "wenn", "ich", "\u00fc\u00b7berh\u00b7in"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPER", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mein dummes Leben \u00fcberlege", "tokens": ["Mein", "dum\u00b7mes", "Le\u00b7ben", "\u00fc\u00b7berl\u00b7e\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und das, worauf ich kommen bin,", "tokens": ["Und", "das", ",", "wo\u00b7rauf", "ich", "kom\u00b7men", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "$,", "PWAV", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In den Gedanken recht erw\u00e4ge;", "tokens": ["In", "den", "Ge\u00b7dan\u00b7ken", "recht", "er\u00b7w\u00e4\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mir zittern Beides, Mark und Bein,", "tokens": ["Mir", "zit\u00b7tern", "Bei\u00b7des", ",", "Mark", "und", "Bein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Stirne wird wie Eis und Stein;", "tokens": ["Die", "Stir\u00b7ne", "wird", "wie", "Eis", "und", "Stein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es will Gebl\u00fct und Geist erstarren. \u2013", "tokens": ["Es", "will", "Ge\u00b7bl\u00fct", "und", "Geist", "er\u00b7star\u00b7ren", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VMFIN", "NN", "KON", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Genug geirrt, genug geklagt!", "tokens": ["Ge\u00b7nug", "ge\u00b7irrt", ",", "ge\u00b7nug", "ge\u00b7klagt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Den Irrthum hat die Zeit verjagt;", "tokens": ["Den", "Irr\u00b7thum", "hat", "die", "Zeit", "ver\u00b7jagt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Ich will nicht l\u00e4nger hier verharren.", "tokens": ["Ich", "will", "nicht", "l\u00e4n\u00b7ger", "hier", "ver\u00b7har\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJD", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Und da\u00df die Feder nicht zu viel", "tokens": ["Und", "da\u00df", "die", "Fe\u00b7der", "nicht", "zu", "viel"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "ART", "NN", "PTKNEG", "PTKA", "PIS"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von meinem b\u00f6sen Leben sage,", "tokens": ["Von", "mei\u00b7nem", "b\u00f6\u00b7sen", "Le\u00b7ben", "sa\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "So habe sie hiermit ihr Ziel;", "tokens": ["So", "ha\u00b7be", "sie", "hier\u00b7mit", "ihr", "Ziel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PAV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich will nicht, da\u00df sie ferner klage.", "tokens": ["Ich", "will", "nicht", ",", "da\u00df", "sie", "fer\u00b7ner", "kla\u00b7ge", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "$,", "KOUS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit diesem geht mein Wallen an.", "tokens": ["Mit", "die\u00b7sem", "geht", "mein", "Wal\u00b7len", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl jedem, der da bleiben kann!", "tokens": ["Wohl", "je\u00b7dem", ",", "der", "da", "blei\u00b7ben", "kann", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Mein Wohlsein such' ich im Verderben.", "tokens": ["Mein", "Wohl\u00b7sein", "such'", "ich", "im", "Ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ihr guten Freunde, gute Nacht!", "tokens": ["Ihr", "gu\u00b7ten", "Freun\u00b7de", ",", "gu\u00b7te", "Nacht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Leben mag mein Feind ererben.", "tokens": ["Mein", "Le\u00b7ben", "mag", "mein", "Feind", "er\u00b7er\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}