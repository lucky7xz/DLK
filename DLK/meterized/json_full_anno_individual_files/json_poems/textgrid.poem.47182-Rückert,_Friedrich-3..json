ickt,", "tokens": ["Mit", "bre\u00b7chen\u00b7den", "Au\u00b7gen", "ihn", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit sterbenden H\u00e4ndchen gestreichelt.", "tokens": ["Mit", "ster\u00b7ben\u00b7den", "H\u00e4nd\u00b7chen", "ge\u00b7strei\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.19": {"line.1": {"text": "Was hat mir gesagt die streichelnde Hand,", "tokens": ["Was", "hat", "mir", "ge\u00b7sagt", "die", "strei\u00b7cheln\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schon die Rede dir fehlte?", "tokens": ["Da", "schon", "die", "Re\u00b7de", "dir", "fehl\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df du verziehest den Unverstand,", "tokens": ["Da\u00df", "du", "ver\u00b7zie\u00b7hest", "den", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der dich gutmeinend qu\u00e4lte.", "tokens": ["Der", "dich", "gut\u00b7mei\u00b7nend", "qu\u00e4l\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Nun bitt' ich dir ab jedes harte Wort,", "tokens": ["Nun", "bitt'", "ich", "dir", "ab", "je\u00b7des", "har\u00b7te", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Worte, die dich bedr\u00e4uten,", "tokens": ["Die", "Wor\u00b7te", ",", "die", "dich", "be\u00b7dr\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du wirst sie haben vergessen dort", "tokens": ["Du", "wirst", "sie", "ha\u00b7ben", "ver\u00b7ges\u00b7sen", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VAFIN", "VVPP", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Oder wei\u00dft sie zu deuten.", "tokens": ["O\u00b7der", "wei\u00dft", "sie", "zu", "deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}}}}