{"textgrid.poem.47182": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich hatte dich lieb, mein T\u00f6chterlein!", "tokens": ["Ich", "hat\u00b7te", "dich", "lieb", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nun ich dich habe begraben,", "tokens": ["Und", "nun", "ich", "dich", "ha\u00b7be", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mach' ich mir Vorw\u00fcrf', ich h\u00e4tte fein", "tokens": ["Mach'", "ich", "mir", "Vor\u00b7w\u00fcr\u00b7f'", ",", "ich", "h\u00e4t\u00b7te", "fein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch lieber dich k\u00f6nnen haben.", "tokens": ["Noch", "lie\u00b7ber", "dich", "k\u00f6n\u00b7nen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich habe dich lieber, viel lieber gehabt,", "tokens": ["Ich", "ha\u00b7be", "dich", "lie\u00b7ber", ",", "viel", "lie\u00b7ber", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "VAPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als ich dir's mochte zeigen;", "tokens": ["Als", "ich", "dir's", "moch\u00b7te", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu selten mit Liebeszeichen begabt", "tokens": ["Zu", "sel\u00b7ten", "mit", "Lie\u00b7bes\u00b7zei\u00b7chen", "be\u00b7gabt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKA", "ADJD", "APPR", "NN", "ADJD"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Hat dich mein ernstes Schweigen.", "tokens": ["Hat", "dich", "mein", "erns\u00b7tes", "Schwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Ich habe dich lieb gehabt, so lieb,", "tokens": ["Ich", "ha\u00b7be", "dich", "lieb", "ge\u00b7habt", ",", "so", "lieb", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VAPP", "$,", "ADV", "ADJD", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Auch wenn ich dich streng gescholten;", "tokens": ["Auch", "wenn", "ich", "dich", "streng", "ge\u00b7schol\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "PPER", "PRF", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich von Liebe dir schuldig blieb,", "tokens": ["Was", "ich", "von", "Lie\u00b7be", "dir", "schul\u00b7dig", "blieb", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPR", "NN", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Sei zwiefach dir jetzt vergolten!", "tokens": ["Sei", "zwie\u00b7fach", "dir", "jetzt", "ver\u00b7gol\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Zu oft verbarg sich hinter der Zucht", "tokens": ["Zu", "oft", "ver\u00b7barg", "sich", "hin\u00b7ter", "der", "Zucht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADV", "VVFIN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Die Vaterlieb' im Gem\u00fcte;", "tokens": ["Die", "Va\u00b7ter\u00b7lieb'", "im", "Ge\u00b7m\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Ich hatte schon im Auge die Frucht,", "tokens": ["Ich", "hat\u00b7te", "schon", "im", "Au\u00b7ge", "die", "Frucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Anstatt mich zu freuen der Bl\u00fcte.", "tokens": ["An\u00b7statt", "mich", "zu", "freu\u00b7en", "der", "Bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "PTKZU", "VVINF", "ART", "NN", "$."], "meter": "+---+--+-", "measure": "trochaic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "O h\u00e4tt' ich gewu\u00dft, wie bald der Wind", "tokens": ["O", "h\u00e4tt'", "ich", "ge\u00b7wu\u00dft", ",", "wie", "bald", "der", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "PPER", "VVPP", "$,", "PWAV", "ADV", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.2": {"text": "Die Bl\u00fct' entbl\u00e4ttern sollte!", "tokens": ["Die", "Bl\u00fct'", "ent\u00b7bl\u00e4t\u00b7tern", "soll\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Thun h\u00e4tt' ich sollen meinem Kind,", "tokens": ["Thun", "h\u00e4tt'", "ich", "sol\u00b7len", "mei\u00b7nem", "Kind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "VMFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Was alles sein Herzchen wollte.", "tokens": ["Was", "al\u00b7les", "sein", "Herz\u00b7chen", "woll\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PIS", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Da solltest du, was ich wollte, thun,", "tokens": ["Da", "soll\u00b7test", "du", ",", "was", "ich", "woll\u00b7te", ",", "thun", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PWS", "PPER", "VMFIN", "$,", "VVINF", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und thatst es auf meine Winke.", "tokens": ["Und", "thatst", "es", "auf", "mei\u00b7ne", "Win\u00b7ke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du trankst das Bittre, wie reut mich's nun,", "tokens": ["Du", "trankst", "das", "Bitt\u00b7re", ",", "wie", "reut", "mich's", "nun", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PWAV", "VVFIN", "PIS", "ADV", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Weil ich dir sagte: Trinke!", "tokens": ["Weil", "ich", "dir", "sag\u00b7te", ":", "Trin\u00b7ke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Dein Mund, geschlossen von Todeskrampf,", "tokens": ["Dein", "Mund", ",", "ge\u00b7schlos\u00b7sen", "von", "To\u00b7des\u00b7krampf", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "VVPP", "APPR", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Hat meinem Gebot sich erschlossen;", "tokens": ["Hat", "mei\u00b7nem", "Ge\u00b7bot", "sich", "er\u00b7schlos\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PRF", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ach! nur zu verl\u00e4ngern den Todeskampf,", "tokens": ["Ach", "!", "nur", "zu", "ver\u00b7l\u00e4n\u00b7gern", "den", "To\u00b7des\u00b7kampf", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ADV", "PTKZU", "VVINF", "ART", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Hat man dir's eingegossen.", "tokens": ["Hat", "man", "dir's", "ein\u00b7ge\u00b7gos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "PIS", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Du aber hast, vom Tod umstrickt,", "tokens": ["Du", "a\u00b7ber", "hast", ",", "vom", "Tod", "um\u00b7strickt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VAFIN", "$,", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Noch deinem Vater geschmeichelt,", "tokens": ["Noch", "dei\u00b7nem", "Va\u00b7ter", "ge\u00b7schmei\u00b7chelt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Mit brechenden Augen ihn angeblickt,", "tokens": ["Mit", "bre\u00b7chen\u00b7den", "Au\u00b7gen", "ihn", "an\u00b7ge\u00b7blickt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PPER", "VVPP", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Mit sterbenden H\u00e4ndchen gestreichelt.", "tokens": ["Mit", "ster\u00b7ben\u00b7den", "H\u00e4nd\u00b7chen", "ge\u00b7strei\u00b7chelt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}, "stanza.9": {"line.1": {"text": "Was hat mir gesagt die streichelnde Hand,", "tokens": ["Was", "hat", "mir", "ge\u00b7sagt", "die", "strei\u00b7cheln\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da schon die Rede dir fehlte?", "tokens": ["Da", "schon", "die", "Re\u00b7de", "dir", "fehl\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da\u00df du verziehest den Unverstand,", "tokens": ["Da\u00df", "du", "ver\u00b7zie\u00b7hest", "den", "Un\u00b7ver\u00b7stand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der dich gutmeinend qu\u00e4lte.", "tokens": ["Der", "dich", "gut\u00b7mei\u00b7nend", "qu\u00e4l\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Nun bitt' ich dir ab jedes harte Wort,", "tokens": ["Nun", "bitt'", "ich", "dir", "ab", "je\u00b7des", "har\u00b7te", "Wort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Worte, die dich bedr\u00e4uten,", "tokens": ["Die", "Wor\u00b7te", ",", "die", "dich", "be\u00b7dr\u00e4u\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Du wirst sie haben vergessen dort", "tokens": ["Du", "wirst", "sie", "ha\u00b7ben", "ver\u00b7ges\u00b7sen", "dort"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "VAFIN", "VVPP", "ADV"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Oder wei\u00dft sie zu deuten.", "tokens": ["O\u00b7der", "wei\u00dft", "sie", "zu", "deu\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}}, "stanza.11": {"line.1": {"text": "Ich hatte dich lieb, mein T\u00f6chterlein!", "tokens": ["Ich", "hat\u00b7te", "dich", "lieb", ",", "mein", "T\u00f6ch\u00b7ter\u00b7lein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nun ich dich habe begraben,", "tokens": ["Und", "nun", "ich", "dich", "ha\u00b7be", "be\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPER", "PPER", "VAFIN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Mach' ich mir Vorw\u00fcrf', ich h\u00e4tte fein", "tokens": ["Mach'", "ich", "mir", "Vor\u00b7w\u00fcr\u00b7f'", ",", "ich", "h\u00e4t\u00b7te", "fein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPER", "NN", "$,", "PPER", "VAFIN", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Noch lieber dich k\u00f6nnen haben.", "tokens": ["Noch", "lie\u00b7ber", "dich", "k\u00f6n\u00b7nen", "ha\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VMFIN", "VAINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.12": {"line.1": {"text": "Ich habe dich lieber, viel lieber gehabt,", "tokens": ["Ich", "ha\u00b7be", "dich", "lie\u00b7ber", ",", "viel", "lie\u00b7ber", "ge\u00b7habt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "$,", "ADV", "ADV", "VAPP", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Als ich dir's mochte zeigen;", "tokens": ["Als", "ich", "dir's", "moch\u00b7te", "zei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIS", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Zu selten mit Liebeszeichen begabt", "tokens": ["Zu", "sel\u00b7ten", "mit", "Lie\u00b7bes\u00b7zei\u00b7chen", "b