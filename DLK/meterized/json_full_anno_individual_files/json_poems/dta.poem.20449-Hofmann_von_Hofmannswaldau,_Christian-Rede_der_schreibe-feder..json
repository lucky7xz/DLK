{"dta.poem.20449": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Rede der schreibe-feder.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mich hat ein schwaches thier zwar zu der welt gebracht/", "tokens": ["Mich", "hat", "ein", "schwa\u00b7ches", "thier", "zwar", "zu", "der", "welt", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch kan ich thron und kron durch meine kunst besiegen/", "tokens": ["Doch", "kan", "ich", "thron", "und", "kron", "durch", "mei\u00b7ne", "kunst", "be\u00b7sie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "NE", "KON", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es wird des scepters stab zu meinen f\u00fcssen liegen/", "tokens": ["Es", "wird", "des", "scep\u00b7ters", "stab", "zu", "mei\u00b7nen", "f\u00fcs\u00b7sen", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wo ihn der kluge kiel durch sich nicht sch\u00e4tzbar macht.", "tokens": ["Wo", "ihn", "der", "klu\u00b7ge", "kiel", "durch", "sich", "nicht", "sch\u00e4tz\u00b7bar", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADJA", "NN", "APPR", "PRF", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Rom war bey aller welt durch mich so gro\u00df geacht/", "tokens": ["Rom", "war", "bey", "al\u00b7ler", "welt", "durch", "mich", "so", "gro\u00df", "ge\u00b7acht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "APPR", "PIAT", "NN", "APPR", "PPER", "ADV", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df/ wenn sich k\u00f6nige und f\u00fcrsten musten biegen/", "tokens": ["Da\u00df", "/", "wenn", "sich", "k\u00f6\u00b7ni\u00b7ge", "und", "f\u00fcrs\u00b7ten", "mus\u00b7ten", "bie\u00b7gen", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "$(", "KOUS", "PRF", "ADJA", "KON", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So stieg ich \u00fcber di\u00df. Den lorbeer-krantz von kriegen", "tokens": ["So", "stieg", "ich", "\u00fc\u00b7ber", "di\u00df", ".", "Den", "lor\u00b7beer\u00b7krantz", "von", "krie\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PDS", "$.", "ART", "NN", "APPR", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Hat eintzig und allein vermehret meine pracht.", "tokens": ["Hat", "eint\u00b7zig", "und", "al\u00b7lein", "ver\u00b7meh\u00b7ret", "mei\u00b7ne", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "KON", "ADV", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der himmlische Virgil sa\u00df in Augustus schoo\u00df/", "tokens": ["Der", "himm\u00b7li\u00b7sche", "Vir\u00b7gil", "sa\u00df", "in", "Au\u00b7gus\u00b7tus", "schoo\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "NE", "VVFIN", "$("], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.10": {"text": "Und Cicero hat offt durch reden Rom beweget.", "tokens": ["Und", "Ci\u00b7ce\u00b7ro", "hat", "offt", "durch", "re\u00b7den", "Rom", "be\u00b7we\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VAFIN", "ADV", "APPR", "ADJA", "NE", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Itzt wird Germanien noch tausendmahl so gro\u00df/", "tokens": ["Itzt", "wird", "Ger\u00b7ma\u00b7ni\u00b7en", "noch", "tau\u00b7send\u00b7mahl", "so", "gro\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Weil es den helden-muth auff freye k\u00fcnste leget.", "tokens": ["Weil", "es", "den", "hel\u00b7den\u00b7muth", "auff", "frey\u00b7e", "k\u00fcns\u00b7te", "le\u00b7get", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJA", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Manch hut/ der mich zwar tr\u00e4gt/ wird nur durch mich ver-", "tokens": ["Manch", "hut", "/", "der", "mich", "zwar", "tr\u00e4gt", "/", "wird", "nur", "durch", "mich", "ver"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "$(", "PRELS", "PPER", "ADV", "VVFIN", "$(", "VAFIN", "ADV", "APPR", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "stellt/", "tokens": ["stellt", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Weil sich nicht kunst und witz zu seinem strau\u00df gesellt.", "tokens": ["Weil", "sich", "nicht", "kunst", "und", "witz", "zu", "sei\u00b7nem", "strau\u00df", "ge\u00b7sellt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PTKNEG", "PTKVZ", "KON", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}