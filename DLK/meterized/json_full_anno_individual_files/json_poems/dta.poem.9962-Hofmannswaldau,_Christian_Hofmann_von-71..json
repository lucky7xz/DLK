{"dta.poem.9962": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "71.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bin ich gleich gefangen nicht/ so bin dennoch ich gebunden/", "tokens": ["Bin", "ich", "gleich", "ge\u00b7fan\u00b7gen", "nicht", "/", "so", "bin", "den\u00b7noch", "ich", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "PTKNEG", "$(", "ADV", "VAFIN", "ADV", "PPER", "VVPP", "$("], "meter": "+-+-+-+-+--+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Bin ich gleich kein wasserflu\u00df/ wird ein rand bey mir gefunden/", "tokens": ["Bin", "ich", "gleich", "kein", "was\u00b7ser\u00b7flu\u00df", "/", "wird", "ein", "rand", "bey", "mir", "ge\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PIAT", "NN", "$(", "VAFIN", "ART", "NN", "APPR", "PPER", "VVPP", "$("], "meter": "+-+-+-+--+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Komm ich schon von schlechten eltern/ so geh \u00fcber f\u00fcrsten ich/", "tokens": ["Komm", "ich", "schon", "von", "schlech\u00b7ten", "el\u00b7tern", "/", "so", "geh", "\u00fc\u00b7ber", "f\u00fcrs\u00b7ten", "ich", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "$(", "ADV", "VVFIN", "APPR", "VVFIN", "PPER", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und die helffte von den menschen/ die bewirbet sich um mich.", "tokens": ["Und", "die", "helff\u00b7te", "von", "den", "men\u00b7schen", "/", "die", "be\u00b7wir\u00b7bet", "sich", "um", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "APPR", "ART", "NN", "$(", "ART", "VVFIN", "PRF", "APPR", "PPER", "$."], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}}}}}