{"textgrid.poem.36195": {"metadata": {"author": {"name": "Schlegel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Elfte Romanze", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eben las die Seelenmesse", "tokens": ["E\u00b7ben", "las", "die", "See\u00b7len\u00b7mes\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Christenk\u00e4mpfer Ehre", "tokens": ["Zu", "der", "Chris\u00b7ten\u00b7k\u00e4mp\u00b7fer", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Turpin dort im Kriegesfelde,", "tokens": ["Tur\u00b7pin", "dort", "im", "Krie\u00b7ges\u00b7fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaiser Karol stand daneben;", "tokens": ["Kai\u00b7ser", "Ka\u00b7rol", "stand", "da\u00b7ne\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VVFIN", "PAV", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.5": {"text": "Eh' das Hochamt noch vollendet,", "tokens": ["Eh'", "das", "Hoc\u00b7hamt", "noch", "voll\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wird entr\u00fcckt des Bischofs Seele.", "tokens": ["Wird", "ent\u00b7r\u00fcckt", "des", "Bi\u00b7schofs", "See\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "ART", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Singen h\u00f6rt er pl\u00f6tzlich Engel,", "tokens": ["Sin\u00b7gen", "h\u00f6rt", "er", "pl\u00f6tz\u00b7lich", "En\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die im Chor gen Himmel kehren;", "tokens": ["Die", "im", "Chor", "gen", "Him\u00b7mel", "keh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "APPR", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Da die seinem Blick entschwebet,", "tokens": ["Da", "die", "sei\u00b7nem", "Blick", "ent\u00b7schwe\u00b7bet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Folgt ein wilder Haufe denen", "tokens": ["Folgt", "ein", "wil\u00b7der", "Hau\u00b7fe", "de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PDS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Dunkler H\u00f6llenrichter schnelle,", "tokens": ["Dunk\u00b7ler", "H\u00f6l\u00b7len\u00b7rich\u00b7ter", "schnel\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "F\u00fchren einen Mann gefesselt,", "tokens": ["F\u00fch\u00b7ren", "ei\u00b7nen", "Mann", "ge\u00b7fes\u00b7selt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wie zur H\u00f6lle R\u00e4uber gehen,", "tokens": ["Wie", "zur", "H\u00f6l\u00b7le", "R\u00e4u\u00b7ber", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Mit dem Raub in frechen H\u00e4nden.", "tokens": ["Mit", "dem", "Raub", "in", "fre\u00b7chen", "H\u00e4n\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Wen sie f\u00fchren, fragt er; jene", "tokens": ["Wen", "sie", "f\u00fch\u00b7ren", ",", "fragt", "er", ";", "je\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["PWS", "PPER", "VVINF", "$,", "VVFIN", "PPER", "$.", "PDS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Zu dem frommen Bischof sprechen:", "tokens": ["Zu", "dem", "from\u00b7men", "Bi\u00b7schof", "spre\u00b7chen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "\u00bbden Marsir zur H\u00f6ll' in Ketten,", "tokens": ["\u00bb", "den", "Mar\u00b7sir", "zur", "H\u00f6ll'", "in", "Ket\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.18": {"text": "Aber den vom Horn, den Helden,", "tokens": ["A\u00b7ber", "den", "vom", "Horn", ",", "den", "Hel\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ART", "APPRART", "NN", "$,", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Michael zur Himmelsveste.\u00ab \u2013", "tokens": ["Mic\u00b7hael", "zur", "Him\u00b7mels\u00b7ves\u00b7te", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "punct"], "pos": ["NE", "APPRART", "NN", "$.", "$(", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.20": {"text": "Da die Messe nun geendet,", "tokens": ["Da", "die", "Mes\u00b7se", "nun", "ge\u00b7en\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Vor den Kaiser Karol tretend,", "tokens": ["Vor", "den", "Kai\u00b7ser", "Ka\u00b7rol", "tre\u00b7tend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NE", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Hat der Bischof so geredet,", "tokens": ["Hat", "der", "Bi\u00b7schof", "so", "ge\u00b7re\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.23": {"text": "Mit dem Kreuz zuvor sich segnend:", "tokens": ["Mit", "dem", "Kreuz", "zu\u00b7vor", "sich", "seg\u00b7nend", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADV", "PRF", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "\u00bbkund mu\u00df dir, o Kaiser, werden,", "tokens": ["\u00bb", "kund", "mu\u00df", "dir", ",", "o", "Kai\u00b7ser", ",", "wer\u00b7den", ","], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["$(", "PTKVZ", "VMFIN", "PPER", "$,", "FM", "NN", "$,", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.25": {"text": "Roland ist nicht mehr am Leben.", "tokens": ["Ro\u00b7land", "ist", "nicht", "mehr", "am", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VAFIN", "PTKNEG", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Michael f\u00fchrte seine Seele", "tokens": ["Mic\u00b7hael", "f\u00fchr\u00b7te", "sei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Mit viel andern Christenseelen", "tokens": ["Mit", "viel", "an\u00b7dern", "Chris\u00b7ten\u00b7see\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PIAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Zu des Himmels lichten Welten.", "tokens": ["Zu", "des", "Him\u00b7mels", "lich\u00b7ten", "Wel\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.29": {"text": "Den Marsirus aber werfen", "tokens": ["Den", "Mar\u00b7si\u00b7rus", "a\u00b7ber", "wer\u00b7fen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NE", "ADV", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.30": {"text": "H\u00f6ll'sche Geister, hart gefesselt,", "tokens": ["H\u00f6ll'\u00b7sche", "Geis\u00b7ter", ",", "hart", "ge\u00b7fes\u00b7selt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.31": {"text": "In des Flammenpfuhles Wellen.\u00ab \u2013", "tokens": ["In", "des", "Flam\u00b7men\u00b7pfuh\u00b7les", "Wel\u00b7len", ".", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$.", "$(", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "So noch sprach er; und da sehen", "tokens": ["So", "noch", "sprach", "er", ";", "und", "da", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$.", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.33": {"text": "Balduin sie durch die Felder,", "tokens": ["Bal\u00b7du\u00b7in", "sie", "durch", "die", "Fel\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.34": {"text": "Der in Eil absprang vom Pferde,", "tokens": ["Der", "in", "Eil", "ab\u00b7sprang", "vom", "Pfer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "---+--+-", "measure": "iambic.di.relaxed"}, "line.35": {"text": "Alles treu dem Kaiser meldet,", "tokens": ["Al\u00b7les", "treu", "dem", "Kai\u00b7ser", "mel\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Wie er Roland lie\u00df beim Felsen", "tokens": ["Wie", "er", "Ro\u00b7land", "lie\u00df", "beim", "Fel\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "NE", "VVFIN", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.37": {"text": "Schon im Todeskampfe sterbend.", "tokens": ["Schon", "im", "To\u00b7des\u00b7kamp\u00b7fe", "ster\u00b7bend", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Ein Geschrei wird da im Heere,", "tokens": ["Ein", "Ge\u00b7schrei", "wird", "da", "im", "Hee\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Wie sie hiehin, dorthin gehen,", "tokens": ["Wie", "sie", "hie\u00b7hin", ",", "dor\u00b7thin", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PAV", "$,", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "Bis der Kaiser Karl den Helden", "tokens": ["Bis", "der", "Kai\u00b7ser", "Karl", "den", "Hel\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NE", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.41": {"text": "Liegen fand bleich und enseelet,", "tokens": ["Lie\u00b7gen", "fand", "bleich", "und", "en\u00b7see\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "KON", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.42": {"text": "Kreuzweis auf die Brust geleget", "tokens": ["Kreuz\u00b7weis", "auf", "die", "Brust", "ge\u00b7le\u00b7get"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Seine H\u00e4nde zum Gebete.", "tokens": ["Sei\u00b7ne", "H\u00e4n\u00b7de", "zum", "Ge\u00b7be\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.44": {"text": "Da begann mit tiefem Wehe,", "tokens": ["Da", "be\u00b7gann", "mit", "tie\u00b7fem", "We\u00b7he", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.45": {"text": "Klagevoll am Leichnam stehend,", "tokens": ["Kla\u00b7ge\u00b7voll", "am", "Leich\u00b7nam", "ste\u00b7hend", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.46": {"text": "Weinend, seufzend, ohne Ende,", "tokens": ["Wei\u00b7nend", ",", "seuf\u00b7zend", ",", "oh\u00b7ne", "En\u00b7de", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVPP", "$,", "KOUI", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.47": {"text": "Laut vergie\u00dfend hei\u00dfe Tr\u00e4nen,", "tokens": ["Laut", "ver\u00b7gie\u00b7\u00dfend", "hei\u00b7\u00dfe", "Tr\u00e4\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.48": {"text": "H\u00e4nderingend und im Schmerze", "tokens": ["H\u00e4n\u00b7de\u00b7rin\u00b7gend", "und", "im", "Schmer\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["VVPP", "KON", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.49": {"text": "Haar und Wange sich verletzend,", "tokens": ["Haar", "und", "Wan\u00b7ge", "sich", "ver\u00b7let\u00b7zend", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PRF", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.50": {"text": "Karol diese Klagerede:", "tokens": ["Ka\u00b7rol", "die\u00b7se", "Kla\u00b7ge\u00b7re\u00b7de", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "PDAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "\u00bbo du meines Leibes Rechte,", "tokens": ["\u00bb", "o", "du", "mei\u00b7nes", "Lei\u00b7bes", "Rech\u00b7te", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "FM", "PPER", "PPOSAT", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ruhm und hohe Zier der Franken,", "tokens": ["Ruhm", "und", "ho\u00b7he", "Zier", "der", "Fran\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ADJA", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwert des Rechtes, Schirm des Heiles,", "tokens": ["Schwert", "des", "Rech\u00b7tes", ",", "Schirm", "des", "Hei\u00b7les", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Nie bezwungne Heldenlanze!", "tokens": ["Nie", "be\u00b7zwung\u00b7ne", "Hel\u00b7den\u00b7lan\u00b7ze", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du, dem Judas Makkab\u00e4us", "tokens": ["Du", ",", "dem", "Ju\u00b7das", "Mak\u00b7ka\u00b7b\u00e4us"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["PPER", "$,", "ART", "NN", "NE"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "\u00c4hnlich durch der Tugend Taten,", "tokens": ["\u00c4hn\u00b7lich", "durch", "der", "Tu\u00b7gend", "Ta\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Saul und Jonathan im Tode,", "tokens": ["Saul", "und", "Jo\u00b7na\u00b7than", "im", "To\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Simson gleich an Kraft des Armes;", "tokens": ["Sim\u00b7son", "gleich", "an", "Kraft", "des", "Ar\u00b7mes", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPR", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weh, da\u00df du erschlagen!", "tokens": ["Weh", ",", "da\u00df", "du", "er\u00b7schla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "O du rastlos wackrer K\u00e4mpfer,", "tokens": ["O", "du", "rast\u00b7los", "wack\u00b7rer", "K\u00e4mp\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rkster unter allen Tapfern,", "tokens": ["St\u00e4rks\u00b7ter", "un\u00b7ter", "al\u00b7len", "Tap\u00b7fern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Tod der Heiden, Schirm der Christen,", "tokens": ["Tod", "der", "Hei\u00b7den", ",", "Schirm", "der", "Chris\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "K\u00f6niglich von Sinn und Adel!", "tokens": ["K\u00f6\u00b7nig\u00b7lich", "von", "Sinn", "und", "A\u00b7del", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Du des Klerus hohe Mauer,", "tokens": ["Du", "des", "Kle\u00b7rus", "ho\u00b7he", "Mau\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Stab der Waisen und der Armen,", "tokens": ["Stab", "der", "Wai\u00b7sen", "und", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Allen h\u00fclfreich, Schild der Witwen,", "tokens": ["Al\u00b7len", "h\u00fclf\u00b7reich", ",", "Schild", "der", "Wit\u00b7wen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Der nicht Trug noch L\u00fcge kannte;", "tokens": ["Der", "nicht", "Trug", "noch", "L\u00fc\u00b7ge", "kann\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "ADV", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Weh, da\u00df du erschlagen!", "tokens": ["Weh", ",", "da\u00df", "du", "er\u00b7schla\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.10": {"text": "Warum mu\u00dft' ich her dich f\u00fchren,", "tokens": ["Wa\u00b7rum", "mu\u00dft'", "ich", "her", "dich", "f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "ADV", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wo dich tot mein Auge sahe?", "tokens": ["Wo", "dich", "tot", "mein", "Au\u00b7ge", "sa\u00b7he", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Konnt' ich denn mit dir nicht sterben?", "tokens": ["Konnt'", "ich", "denn", "mit", "dir", "nicht", "ster\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PPER", "PTKNEG", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Warum bleib' ich hier verlassen?", "tokens": ["Wa\u00b7rum", "bleib'", "ich", "hier", "ver\u00b7las\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Du zwar magst nun immer selig", "tokens": ["Du", "zwar", "magst", "nun", "im\u00b7mer", "se\u00b7lig"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "In des M\u00e4rtertumes Kranze", "tokens": ["In", "des", "M\u00e4r\u00b7ter\u00b7tu\u00b7mes", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Dich des Paradieses freuen", "tokens": ["Dich", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "freu\u00b7en"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.17": {"text": "Mit der heil'gen Engel Scharen.", "tokens": ["Mit", "der", "heil'\u00b7gen", "En\u00b7gel", "Scha\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.18": {"text": "Aber wir, so wie die Seinen", "tokens": ["A\u00b7ber", "wir", ",", "so", "wie", "die", "Sei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "$,", "ADV", "KOKOM", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "K\u00f6nig David mu\u00df bejammern,", "tokens": ["K\u00f6\u00b7nig", "Da\u00b7vid", "mu\u00df", "be\u00b7jam\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "VMFIN", "VVINF", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.20": {"text": "Also wir auch ohne Ende,", "tokens": ["Al\u00b7so", "wir", "auch", "oh\u00b7ne", "En\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.21": {"text": "Roland, m\u00fcssen um dich klagen;", "tokens": ["Ro\u00b7land", ",", "m\u00fcs\u00b7sen", "um", "dich", "kla\u00b7gen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VMFIN", "APPR", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.22": {"text": "Weh, da\u00df du erschlagen!\u00ab", "tokens": ["Weh", ",", "da\u00df", "du", "er\u00b7schla\u00b7gen", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "KOUS", "PPER", "VVINF", "$.", "$("], "meter": "+-+-+-", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Eben las die Seelenmesse", "tokens": ["E\u00b7ben", "las", "die", "See\u00b7len\u00b7mes\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zu der Christenk\u00e4mpfer Ehre", "tokens": ["Zu", "der", "Chris\u00b7ten\u00b7k\u00e4mp\u00b7fer", "Eh\u00b7re"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Turpin dort im Kriegesfelde,", "tokens": ["Tur\u00b7pin", "dort", "im", "Krie\u00b7ges\u00b7fel\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Kaiser Karol stand daneben;", "tokens": ["Kai\u0