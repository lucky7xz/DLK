{"textgrid.poem.52770": {"metadata": {"author": {"name": "Eichrodt, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Ros' ergl\u00fchte in Flammen,", "genre": "verse", "period": "N.A.", "pub_year": 1859, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Ros' ergl\u00fchte in Flammen,", "tokens": ["Die", "Ros'", "er\u00b7gl\u00fch\u00b7te", "in", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die Nachtigall war kein Amphib',", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "war", "kein", "Am\u00b7phib'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie setzten sich fr\u00f6hlich beisammen,", "tokens": ["Sie", "setz\u00b7ten", "sich", "fr\u00f6h\u00b7lich", "bei\u00b7sam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Und hatten einander so lieb.", "tokens": ["Und", "hat\u00b7ten", "ein\u00b7an\u00b7der", "so", "lieb", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Suleima's Teint, klagt Rose,", "tokens": ["Su\u00b7lei\u00b7ma's", "Teint", ",", "klagt", "Ro\u00b7se", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVFIN", "NE", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist rosiger immer als ich,", "tokens": ["Ist", "ro\u00b7si\u00b7ger", "im\u00b7mer", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "KOUS", "PPER", "$,"], "meter": "-+---+-+", "measure": "dactylic.init"}, "line.3": {"text": "Auch giebt bei s\u00fc\u00dfem Gekose", "tokens": ["Auch", "giebt", "bei", "s\u00fc\u00b7\u00dfem", "Ge\u00b7ko\u00b7se"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Ihr Z\u00fcnglein feineren Stich.", "tokens": ["Ihr", "Z\u00fcn\u00b7glein", "fei\u00b7ne\u00b7ren", "Stich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Und Nachtigall klagt mit Aechzen:", "tokens": ["Und", "Nach\u00b7ti\u00b7gall", "klagt", "mit", "A\u00b7ech\u00b7zen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Nicht sei mir das Schweigen verargt,", "tokens": ["Nicht", "sei", "mir", "das", "Schwei\u00b7gen", "ver\u00b7argt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mein Singen ist rabenhaft Kr\u00e4chzen,", "tokens": ["Mein", "Sin\u00b7gen", "ist", "ra\u00b7ben\u00b7haft", "Kr\u00e4ch\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Sobald sie nur seufzt oder schnarcht.", "tokens": ["So\u00b7bald", "sie", "nur", "seufzt", "o\u00b7der", "schnarcht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "KON", "VVFIN"