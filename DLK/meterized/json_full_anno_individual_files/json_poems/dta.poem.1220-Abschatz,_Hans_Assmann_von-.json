{"dta.poem.1220": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.57", "en:0.42"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Amaranthens braune Wangen", "tokens": ["A\u00b7ma\u00b7ran\u00b7thens", "brau\u00b7ne", "Wan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Haben meinen Geist besiegt.", "tokens": ["Ha\u00b7ben", "mei\u00b7nen", "Geist", "be\u00b7siegt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "K\u00f6nt ich ihre Gunst erlangen/", "tokens": ["K\u00f6nt", "ich", "ih\u00b7re", "Gunst", "er\u00b7lan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach wie w\u00e4r ich so vergn\u00fcgt!", "tokens": ["Ach", "wie", "w\u00e4r", "ich", "so", "ver\u00b7gn\u00fcgt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Neue Glutt f\u00fchl ich im Hertzen;", "tokens": ["Neu\u00b7e", "Glutt", "f\u00fchl", "ich", "im", "Hert\u00b7zen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PPER", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Lieb ich nimmer ohne Schmertzen.", "tokens": ["Lieb", "ich", "nim\u00b7mer", "oh\u00b7ne", "Schmert\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Tugend-voll ist ihr Beginnen/", "tokens": ["Tu\u00b7gen\u00b7dvoll", "ist", "ihr", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$("], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Da\u00df man nichts zu klagen wei\u00df/", "tokens": ["Da\u00df", "man", "nichts", "zu", "kla\u00b7gen", "wei\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PIS", "PTKZU", "VVINF", "VVFIN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die allzuharten Sinnen/", "tokens": ["Als", "die", "all\u00b7zu\u00b7har\u00b7ten", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und das Hertze voller Ei\u00df.", "tokens": ["Und", "das", "Hert\u00b7ze", "vol\u00b7ler", "Ei\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Lieben und nicht Lieb erwerben", "tokens": ["Lie\u00b7ben", "und", "nicht", "Lieb", "er\u00b7wer\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJA", "KON", "PTKNEG", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Macht uns offt und nimmer sterben.", "tokens": ["Macht", "uns", "offt", "und", "nim\u00b7mer", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "KON", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Reist sich gleich von ihrem Stricke", "tokens": ["Reist", "sich", "gleich", "von", "ih\u00b7rem", "Stri\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein gesangnes Hertze frey/", "tokens": ["Mein", "ge\u00b7sang\u00b7nes", "Hert\u00b7ze", "frey", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADJD", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Bringt sie doch mit einem Blicke/", "tokens": ["Bringt", "sie", "doch", "mit", "ei\u00b7nem", "Bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Solches auff das neu herbey.", "tokens": ["Sol\u00b7ches", "auff", "das", "neu", "her\u00b7bey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPR", "ART", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer kan f\u00fcr der Augen Blitzen", "tokens": ["Wer", "kan", "f\u00fcr", "der", "Au\u00b7gen", "Blit\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Seiner Freyheit Recht besch\u00fctzen?", "tokens": ["Sei\u00b7ner", "Frey\u00b7heit", "Recht", "be\u00b7sch\u00fct\u00b7zen", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ich gedachte mir zu leben/", "tokens": ["Ich", "ge\u00b7dach\u00b7te", "mir", "zu", "le\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ohn der Liebe Joch zu seyn:", "tokens": ["Ohn", "der", "Lie\u00b7be", "Joch", "zu", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich ihr nicht wolte geben/", "tokens": ["Was", "ich", "ihr", "nicht", "wol\u00b7te", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "PTKNEG", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hat sie selbst genommen ein:", "tokens": ["Hat", "sie", "selbst", "ge\u00b7nom\u00b7men", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Besser ist sich leicht entschl\u00fcssen", "tokens": ["Bes\u00b7ser", "ist", "sich", "leicht", "ent\u00b7schl\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VAFIN", "PRF", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Als gezwungen lieben m\u00fcssen.", "tokens": ["Als", "ge\u00b7zwun\u00b7gen", "lie\u00b7ben", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVPP", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Man mag streiten/ man mag klagen/", "tokens": ["Man", "mag", "strei\u00b7ten", "/", "man", "mag", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "VVINF", "$(", "PIS", "VMFIN", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mag ihr kr\u00e4fftig widerstehn;", "tokens": ["Mag", "ihr", "kr\u00e4ff\u00b7tig", "wi\u00b7der\u00b7stehn", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Niemand wird doch ihren Plagen", "tokens": ["Nie\u00b7mand", "wird", "doch", "ih\u00b7ren", "Pla\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "ADV", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zu bestimmter Zeit entgehn.", "tokens": ["Zu", "be\u00b7stimm\u00b7ter", "Zeit", "ent\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer sich ihrer will befreyen/", "tokens": ["Wer", "sich", "ih\u00b7rer", "will", "be\u00b7fre\u00b7yen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PRF", "PPOSAT", "VMFIN", "VVINF", "$("], "meter": "-+--+---", "measure": "iambic.di.relaxed"}, "line.6": {"text": "F\u00e4ngt offt erst recht an von neuen.", "tokens": ["F\u00e4ngt", "offt", "erst", "recht", "an", "von", "neu\u00b7en", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADV", "APPR", "APPR", "ADJA", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ich/ von k\u00fchner Lust getrieben/", "tokens": ["Ich", "/", "von", "k\u00fch\u00b7ner", "Lust", "ge\u00b7trie\u00b7ben", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$(", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wolte wissen/ was die Zier", "tokens": ["Wol\u00b7te", "wis\u00b7sen", "/", "was", "die", "Zier"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "VVINF", "$(", "PWS", "ART", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sch\u00f6ner Augen kan ver\u00fcben;", "tokens": ["Sch\u00f6\u00b7ner", "Au\u00b7gen", "kan", "ver\u00b7\u00fc\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Izo b\u00fc\u00df ich nun daf\u00fcr.", "tokens": ["I\u00b7zo", "b\u00fc\u00df", "ich", "nun", "da\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wer wei\u00df/ was er sich erk\u00fchnet/", "tokens": ["Wer", "wei\u00df", "/", "was", "er", "sich", "er\u00b7k\u00fch\u00b7net", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$(", "PWS", "PPER", "PRF", "VVFIN", "$("], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Wenn er/ Nimphe/ dich bedienet?", "tokens": ["Wenn", "er", "/", "Nim\u00b7phe", "/", "dich", "be\u00b7die\u00b7net", "?"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$(", "NE", "$(", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}