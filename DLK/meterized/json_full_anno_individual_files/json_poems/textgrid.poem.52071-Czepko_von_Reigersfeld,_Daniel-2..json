{"textgrid.poem.52071": {"metadata": {"author": {"name": "Czepko von Reigersfeld, Daniel", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann du mir Geld solst leihn, sprichst du: Geld hab ich nicht,", "tokens": ["Wann", "du", "mir", "Geld", "solst", "leihn", ",", "sprichst", "du", ":", "Geld", "hab", "ich", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PPER", "NN", "VMFIN", "VVINF", "$,", "VVFIN", "PPER", "$.", "NN", "VAFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Geld hast du: wann ich dir wil Haus und Gut verpf\u00e4nden,", "tokens": ["Geld", "hast", "du", ":", "wann", "ich", "dir", "wil", "Haus", "und", "Gut", "ver\u00b7pf\u00e4n\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "$.", "PWAV", "PPER", "PPER", "VMFIN", "NN", "KON", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Was du nicht Freunden traust, das traust du Kalck und W\u00e4nden:", "tokens": ["Was", "du", "nicht", "Freun\u00b7den", "traust", ",", "das", "traust", "du", "Kalck", "und", "W\u00e4n\u00b7den", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PTKNEG", "NN", "VVFIN", "$,", "PDS", "VVFIN", "PPER", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der Mist ist mehr als ich in deiner Treu und Pflicht.", "tokens": ["Der", "Mist", "ist", "mehr", "als", "ich", "in", "dei\u00b7ner", "Treu", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KOUS", "PPER", "APPR", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du solt hin vor das Ammt, dein Hau\u00df mag mit dir gehn,", "tokens": ["Du", "solt", "hin", "vor", "das", "Ammt", ",", "dein", "Hau\u00df", "mag", "mit", "dir", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "VMFIN", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du solt hin in die Flucht, dein Gutt mag dich begleiten,", "tokens": ["Du", "solt", "hin", "in", "die", "Flucht", ",", "dein", "Gutt", "mag", "dich", "be\u00b7glei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "ART", "NN", "$,", "PPOSAT", "NN", "VMFIN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ein M\u00f6rder f\u00e4llt dich an: Der Kalck mag vor dich streiten,", "tokens": ["Ein", "M\u00f6r\u00b7der", "f\u00e4llt", "dich", "an", ":", "Der", "Kalck", "mag", "vor", "dich", "strei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$.", "ART", "NN", "VMFIN", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Du kranckst und stirbst: Der Mist mag deinen Ruhm erh\u00f6hn.", "tokens": ["Du", "kranckst", "und", "stirbst", ":", "Der", "Mist", "mag", "dei