{"textgrid.poem.27587": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "verse", "period": "N.A.", "pub_year": 1637, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ihr klugen, derer Faust die Feder embsig f\u00fchret,", "tokens": ["Ihr", "klu\u00b7gen", ",", "de\u00b7rer", "Faust", "die", "Fe\u00b7der", "emb\u00b7sig", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PDS", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu klagen dessen Tod, der an die Wolken r\u00fchret", "tokens": ["Zu", "kla\u00b7gen", "des\u00b7sen", "Tod", ",", "der", "an", "die", "Wol\u00b7ken", "r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PRELAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Thaten ohne gleich, durch Thaten, die der Welt", "tokens": ["Durch", "Tha\u00b7ten", "oh\u00b7ne", "gleich", ",", "durch", "Tha\u00b7ten", ",", "die", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ADV", "$,", "APPR", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "De\u00df Himmels kurtze Gunst hat einig f\u00fcrgestellt", "tokens": ["De\u00df", "Him\u00b7mels", "kurt\u00b7ze", "Gunst", "hat", "ei\u00b7nig", "f\u00fcr\u00b7ge\u00b7stellt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Zum Eigenthum zwar nicht, zum Wunder aber allen,", "tokens": ["Zum", "Ei\u00b7gen\u00b7thum", "zwar", "nicht", ",", "zum", "Wun\u00b7der", "a\u00b7ber", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADV", "PTKNEG", "$,", "APPRART", "NN", "ADV", "PIS", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So weit der Titan leucht; der Mut mag euch entfallen,", "tokens": ["So", "weit", "der", "Ti\u00b7tan", "leucht", ";", "der", "Mut", "mag", "euch", "ent\u00b7fal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ART", "NN", "VVFIN", "$.", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df di\u00df, wo ewig Ding genug zu schaffen hat,", "tokens": ["Da\u00df", "di\u00df", ",", "wo", "e\u00b7wig", "Ding", "ge\u00b7nug", "zu", "schaf\u00b7fen", "hat", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "$,", "PWAV", "ADJD", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die Feder enden soll und ein papiernes Blat.", "tokens": ["Die", "Fe\u00b7der", "en\u00b7den", "soll", "und", "ein", "pa\u00b7pier\u00b7nes", "Blat", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "KON", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Weicht ab von da, wo Flei\u00df gar schwerlich Frucht gewinnet;", "tokens": ["Weicht", "ab", "von", "da", ",", "wo", "Flei\u00df", "gar", "schwer\u00b7lich", "Frucht", "ge\u00b7win\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "ADV", "$,", "PWAV", "NN", "ADV", "ADJD", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Klagt nichts so sehr als di\u00df, da\u00df klagen ihr nicht k\u00f6nnet.", "tokens": ["Klagt", "nichts", "so", "sehr", "als", "di\u00df", ",", "da\u00df", "kla\u00b7gen", "ihr", "nicht", "k\u00f6n\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ADV", "KOKOM", "PDS", "$,", "KOUS", "VVFIN", "PPER", "PTKNEG", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ihr klugen, derer Faust die Feder embsig f\u00fchret,", "tokens": ["Ihr", "klu\u00b7gen", ",", "de\u00b7rer", "Faust", "die", "Fe\u00b7der", "emb\u00b7sig", "f\u00fch\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "PDS", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zu klagen dessen Tod, der an die Wolken r\u00fchret", "tokens": ["Zu", "kla\u00b7gen", "des\u00b7sen", "Tod", ",", "der", "an", "die", "Wol\u00b7ken", "r\u00fch\u00b7ret"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "PRELAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch Thaten ohne gleich, durch Thaten, die der Welt", "tokens": ["Durch", "Tha\u00b7ten", "oh\u00b7ne", "gleich", ",", "durch", "Tha\u00b7ten", ",", "die", "der", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "pun