{"dta.poem.20442": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Unum discamus mori.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn wir die gantze welt in unsern kopff gefast/", "tokens": ["Wenn", "wir", "die", "gant\u00b7ze", "welt", "in", "un\u00b7sern", "kopff", "ge\u00b7fast", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des himmels lauff gesehn/ der erden ziel gemessen/", "tokens": ["Des", "him\u00b7mels", "lauff", "ge\u00b7sehn", "/", "der", "er\u00b7den", "ziel", "ge\u00b7mes\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVPP", "$(", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bey fr\u00fchem morgen-licht/ und auch bey nacht gesessen/", "tokens": ["Bey", "fr\u00fc\u00b7hem", "mor\u00b7gen\u00b7licht", "/", "und", "auch", "bey", "nacht", "ge\u00b7ses\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$(", "KON", "ADV", "APPR", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und alles durchgesucht/ so kommt ein fremder gast/", "tokens": ["Und", "al\u00b7les", "durch\u00b7ge\u00b7sucht", "/", "so", "kommt", "ein", "frem\u00b7der", "gast", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVPP", "$(", "ADV", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Weist uns das stunden-gla\u00df/ und spricht: Mensch lerne sterben/", "tokens": ["Weist", "uns", "das", "stun\u00b7den\u00b7gla\u00df", "/", "und", "spricht", ":", "Mensch", "ler\u00b7ne", "ster\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$(", "KON", "VVFIN", "$.", "NN", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wo du nicht ewig wilst an leib und seel verderben.", "tokens": ["Wo", "du", "nicht", "e\u00b7wig", "wilst", "an", "leib", "und", "seel", "ver\u00b7der\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ADJD", "VMFIN", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach wunder-volle kunst/ und unergr\u00fcndtes werck!", "tokens": ["Ach", "wun\u00b7der\u00b7vol\u00b7le", "kunst", "/", "und", "un\u00b7er\u00b7gr\u00fcnd\u00b7tes", "werck", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "ADJA", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die wei\u00dfheit/ so zuvor ein gantzes land geehret/", "tokens": ["Die", "wei\u00df\u00b7heit", "/", "so", "zu\u00b7vor", "ein", "gant\u00b7zes", "land", "ge\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "ADV", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wird da zum kinder-spiel. Was Plato hat gelehret/", "tokens": ["Wird", "da", "zum", "kin\u00b7der\u00b7spiel", ".", "Was", "Pla\u00b7to", "hat", "ge\u00b7leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "APPRART", "NN", "$.", "PWS", "NE", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Was Socrates gesagt/ und was der k\u00fcnste berg", "tokens": ["Was", "So\u00b7cra\u00b7tes", "ge\u00b7sagt", "/", "und", "was", "der", "k\u00fcns\u00b7te", "berg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWS", "NE", "VVPP", "$(", "KON", "PWS", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Von klugheit bey sich hat/ das wird allhier zum thoren.", "tokens": ["Von", "klug\u00b7heit", "bey", "sich", "hat", "/", "das", "wird", "all\u00b7hier", "zum", "tho\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "VAFIN", "$(", "PDS", "VAFIN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Wer nicht recht sterben lernt/ ist ewiglich verlohren.", "tokens": ["Wer", "nicht", "recht", "ster\u00b7ben", "lernt", "/", "ist", "e\u00b7wig\u00b7lich", "ver\u00b7loh\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "ADJD", "VVINF", "VVFIN", "$(", "VAFIN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Und weil ich denn gewi\u00df/ da\u00df iede stunde mich", "tokens": ["Und", "weil", "ich", "denn", "ge\u00b7wi\u00df", "/", "da\u00df", "ie\u00b7de", "stun\u00b7de", "mich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "ADV", "ADV", "$(", "KOUS", "PIAT", "ADJA", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Aus diesem leben rufft/ da\u00df tag und nacht bezeuget/", "tokens": ["Aus", "die\u00b7sem", "le\u00b7ben", "rufft", "/", "da\u00df", "tag", "und", "nacht", "be\u00b7zeu\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVINF", "VVFIN", "$(", "KOUS", "NN", "KON", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Wienach der arme mensch zu seinem grabe steiget;", "tokens": ["Wie\u00b7nach", "der", "ar\u00b7me", "mensch", "zu", "sei\u00b7nem", "gra\u00b7be", "stei\u00b7get", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "So mach ich mich bereit/ und trachte br\u00fcnstiglich", "tokens": ["So", "mach", "ich", "mich", "be\u00b7reit", "/", "und", "trach\u00b7te", "br\u00fcns\u00b7tig\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADJD", "$(", "KON", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "In dieser h\u00f6chsten kunst nur dieses zu begreiffen/", "tokens": ["In", "die\u00b7ser", "h\u00f6chs\u00b7ten", "kunst", "nur", "die\u00b7ses", "zu", "be\u00b7greif\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADV", "PDS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Wie meine seele m\u00f6g in tods-gedancken reifen.", "tokens": ["Wie", "mei\u00b7ne", "see\u00b7le", "m\u00f6g", "in", "tods\u00b7ge\u00b7dan\u00b7cken", "rei\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VMFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}