{"dta.poem.7252": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Die Stadt und der Dom.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "\u201eder Dom! der Dom! der deutsche Dom!", "tokens": ["\u201e", "der", "Dom", "!", "der", "Dom", "!", "der", "deut\u00b7sche", "Dom", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wer hilft den C\u00f6lner Dom uns baun!\u201c", "tokens": ["Wer", "hilft", "den", "C\u00f6l\u00b7ner", "Dom", "uns", "baun", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "ART", "ADJA", "NN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So fern und nah der Zeitenstrom", "tokens": ["So", "fern", "und", "nah", "der", "Zei\u00b7ten\u00b7strom"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erdonnert durch die deutschen Gaun.", "tokens": ["Er\u00b7don\u00b7nert", "durch", "die", "deut\u00b7schen", "Gaun", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es ist ein Zug, es ist ein Schall", "tokens": ["Es", "ist", "ein", "Zug", ",", "es", "ist", "ein", "Schall"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "PPER", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie ein gewaltger Wogenschwall.", "tokens": ["Wie", "ein", "ge\u00b7walt\u00b7ger", "Wo\u00b7gen\u00b7schwall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer z\u00e4hlt der H\u00e4nde Legion", "tokens": ["Wer", "z\u00e4hlt", "der", "H\u00e4n\u00b7de", "Le\u00b7gi\u00b7on"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In denen Opferheller gl\u00e4nzt?", "tokens": ["In", "de\u00b7nen", "Op\u00b7fer\u00b7hel\u00b7ler", "gl\u00e4nzt", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Die Liederkl\u00e4nge wer, die schon", "tokens": ["Die", "Lie\u00b7der\u00b7kl\u00e4n\u00b7ge", "wer", ",", "die", "schon"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "PWS", "$,", "PRELS", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Das Echo dieses Rufs erg\u00e4nzt?", "tokens": ["Das", "E\u00b7cho", "die\u00b7ses", "Rufs", "er\u00b7g\u00e4nzt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und wieder schallt's vom Elbestrand:", "tokens": ["Und", "wie\u00b7der", "schallt's", "vom", "El\u00b7be\u00b7strand", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "APPRART", "NE", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "\u201edie Stadt! die Stadt! der deutsche Port!\u201c", "tokens": ["\u201e", "die", "Stadt", "!", "die", "Stadt", "!", "der", "deut\u00b7sche", "Port", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "NN", "$.", "ART", "NN", "$.", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und wieder zieht von Land zu Land", "tokens": ["Und", "wie\u00b7der", "zieht", "von", "Land", "zu", "Land"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "APPR", "NN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Gabespendend Klingeln fort;", "tokens": ["Ein", "Ga\u00b7be\u00b7spen\u00b7dend", "Klin\u00b7geln", "fort", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Schiffe kommen Mast an Mast,", "tokens": ["Die", "Schif\u00b7fe", "kom\u00b7men", "Mast", "an", "Mast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Goldregen sch\u00fcttet der Pallast,", "tokens": ["Gold\u00b7re\u00b7gen", "sch\u00fct\u00b7tet", "der", "Pal\u00b7last", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Wem nie ein eignes Dach bescheert,", "tokens": ["Wem", "nie", "ein", "eig\u00b7nes", "Dach", "be\u00b7scheert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der w\u00f6lbt es \u00fcber fremde Noth,", "tokens": ["Der", "w\u00f6lbt", "es", "\u00fc\u00b7ber", "frem\u00b7de", "Noth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Wem nie geraucht der eigne Heerd,", "tokens": ["Wem", "nie", "ge\u00b7raucht", "der", "eig\u00b7ne", "Heerd", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVPP", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der theilt sein schwei\u00dfbenetztes Brod.", "tokens": ["Der", "theilt", "sein", "schwei\u00df\u00b7be\u00b7netz\u00b7tes", "Brod", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn eines ganzen Volkes Kraft", "tokens": ["Wenn", "ei\u00b7nes", "gan\u00b7zen", "Vol\u00b7kes", "Kraft"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr seines Gottes Heiligthum", "tokens": ["F\u00fcr", "sei\u00b7nes", "Got\u00b7tes", "Hei\u00b7lig\u00b7thum"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lanze hebt so Schaft an Schaft,", "tokens": ["Die", "Lan\u00b7ze", "hebt", "so", "Schaft", "an", "Schaft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wer gl\u00fchte nicht dem sch\u00f6nsten Ruhm?", "tokens": ["Wer", "gl\u00fch\u00b7te", "nicht", "dem", "sch\u00f6ns\u00b7ten", "Ruhm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und wem, wem rollte nicht wie Brand", "tokens": ["Und", "wem", ",", "wem", "roll\u00b7te", "nicht", "wie", "Brand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PWS", "$,", "PWS", "VVFIN", "PTKNEG", "KOKOM", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Blut an seiner Adern Wand,", "tokens": ["Das", "Blut", "an", "sei\u00b7ner", "A\u00b7dern", "Wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wenn eines ganzen Volkes Schwei\u00df", "tokens": ["Wenn", "ei\u00b7nes", "gan\u00b7zen", "Vol\u00b7kes", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gleich edlem Regen niedertr\u00e4uft,", "tokens": ["Gleich", "ed\u00b7lem", "Re\u00b7gen", "nie\u00b7der\u00b7tr\u00e4uft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Bis in der Aschensteppe hei\u00df", "tokens": ["Bis", "in", "der", "A\u00b7schens\u00b7tep\u00b7pe", "hei\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Viel Tausenden die Garbe reift?", "tokens": ["Viel", "Tau\u00b7sen\u00b7den", "die", "Gar\u00b7be", "reift", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Man meint, ein Volk von Heil'gen sey", "tokens": ["Man", "meint", ",", "ein", "Volk", "von", "Heil'\u00b7gen", "sey"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "$,", "ART", "NN", "APPR", "NN", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Herabgestiegen \u00fcber Nacht,", "tokens": ["Her\u00b7ab\u00b7ge\u00b7stie\u00b7gen", "\u00fc\u00b7ber", "Nacht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In ihrem Eichensarg aufs neu", "tokens": ["In", "ih\u00b7rem", "Ei\u00b7chen\u00b7sarg", "aufs", "neu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte deutsche Treu' erwacht.", "tokens": ["Die", "al\u00b7te", "deut\u00b7sche", "Treu'", "er\u00b7wacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "O werthe Einheit, bist du Eins \u2014", "tokens": ["O", "wert\u00b7he", "Ein\u00b7heit", ",", "bist", "du", "Eins"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "NN", "$,", "VAFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wer st\u00e4nde dann des Heilgenscheins,", "tokens": ["Wer", "st\u00e4n\u00b7de", "dann", "des", "Heil\u00b7gen\u00b7scheins", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Des Kranzes w\u00fcrdiger als du,", "tokens": ["Des", "Kran\u00b7zes", "w\u00fcr\u00b7di\u00b7ger", "als", "du", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "KOKOM", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Gesegnete, auf deutschem Grund!", "tokens": ["Ge\u00b7se\u00b7gne\u00b7te", ",", "auf", "deut\u00b7schem", "Grund", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ADJA", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.9": {"text": "Du trugst den goldnen Schl\u00fcssel zu", "tokens": ["Du", "trugst", "den", "gold\u00b7nen", "Schl\u00fcs\u00b7sel", "zu"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "PTKZU"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Des Himmels Hort in deinem Bund.", "tokens": ["Des", "Him\u00b7mels", "Hort", "in", "dei\u00b7nem", "Bund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Wohlan ihr K\u00e4mpen denn, wohlan", "tokens": ["Wo\u00b7hlan", "ihr", "K\u00e4m\u00b7pen", "denn", ",", "wo\u00b7hlan"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$,", "PWAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du werthe Kreuzesmassoney,", "tokens": ["Du", "wert\u00b7he", "Kreu\u00b7zes\u00b7mas\u00b7so\u00b7ney", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So gebt mir eure Zeichen dann", "tokens": ["So", "gebt", "mir", "eu\u00b7re", "Zei\u00b7chen", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PPOSAT", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und euer edles Feldgeschrei!", "tokens": ["Und", "eu\u00b7er", "ed\u00b7les", "Feld\u00b7ge\u00b7schrei", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da, horch! da stie\u00df vom n\u00e4chsten Schiff", "tokens": ["Da", ",", "horch", "!", "da", "stie\u00df", "vom", "n\u00e4chs\u00b7ten", "Schiff"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVIMP", "$.", "ADV", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Bootmannspfeife grellen Pfiff,", "tokens": ["Die", "Boot\u00b7manns\u00b7pfei\u00b7fe", "grel\u00b7len", "Pfiff", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da stiegen Flaggen ungez\u00e4hlt,", "tokens": ["Da", "stie\u00b7gen", "Flag\u00b7gen", "un\u00b7ge\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Cantate summte und Gedicht,", "tokens": ["Can\u00b7ta\u00b7te", "summ\u00b7te", "und", "Ge\u00b7dicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "NN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.9": {"text": "Der Demuth Braun nur hat gefehlt,", "tokens": ["Der", "De\u00b7muth", "Braun", "nur", "hat", "ge\u00b7fehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADV", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Jehova's Namen h\u00f6rt ich nicht.", "tokens": ["Je\u00b7ho\u00b7va's", "Na\u00b7men", "h\u00f6rt", "ich", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Wo deine Legion, o Herr,", "tokens": ["Wo", "dei\u00b7ne", "Le\u00b7gi\u00b7on", ",", "o", "Herr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "$,", "FM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die knieend am Altare baut?", "tokens": ["Die", "kni\u00b7e\u00b7end", "am", "Al\u00b7ta\u00b7re", "baut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Wo, wo dein Samariter, der", "tokens": ["Wo", ",", "wo", "dein", "Sa\u00b7ma\u00b7ri\u00b7ter", ",", "der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["PWAV", "$,", "PWAV", "PPOSAT", "NN", "$,", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In Wunden seine Thr\u00e4ne thaut?", "tokens": ["In", "Wun\u00b7den", "sei\u00b7ne", "Thr\u00e4\u00b7ne", "thaut", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, was ich fragte und gelauscht,", "tokens": ["Ach", ",", "was", "ich", "frag\u00b7te", "und", "ge\u00b7lauscht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "PWS", "PPER", "VVFIN", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der deutsche Strom hat mir gerauscht,", "tokens": ["Der", "deut\u00b7sche", "Strom", "hat", "mir", "ge\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+", "meas