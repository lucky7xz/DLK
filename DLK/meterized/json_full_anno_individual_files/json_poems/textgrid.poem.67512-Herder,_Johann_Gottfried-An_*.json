{"textgrid.poem.67512": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "An *", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auch Dir leuchtet der Kranz Ariadnens unter den Sternen,", "tokens": ["Auch", "Dir", "leuch\u00b7tet", "der", "Kranz", "A\u00b7riad\u00b7nens", "un\u00b7ter", "den", "Ster\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "ART", "NN", "NE", "APPR", "ART", "NN", "$,"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Den mit gro\u00dfem Gem\u00fcth G\u00fcte der Frauen erstrebt;", "tokens": ["Den", "mit", "gro\u00b7\u00dfem", "Ge\u00b7m\u00fcth", "G\u00fc\u00b7te", "der", "Frau\u00b7en", "er\u00b7strebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ADJA", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Denn von den Sternen hinab flo\u00df manche himmlische Gabe,", "tokens": ["Denn", "von", "den", "Ster\u00b7nen", "hin\u00b7ab", "flo\u00df", "man\u00b7che", "himm\u00b7li\u00b7sche", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "ADV", "VVFIN", "PIAT", "ADJA", "NN", "$,"], "meter": "---+--+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Reifer Verstand in Dich, Gr\u00f6\u00dfe des Herzens und Huld.", "tokens": ["Rei\u00b7fer", "Ver\u00b7stand", "in", "Dich", ",", "Gr\u00f6\u00b7\u00dfe", "des", "Her\u00b7zens", "und", "Huld", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "APPR", "PPER",