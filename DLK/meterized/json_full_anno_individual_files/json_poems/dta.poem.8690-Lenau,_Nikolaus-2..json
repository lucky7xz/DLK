{"dta.poem.8690": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1832", "urn": "urn:nbn:de:kobv:b4-200905193572", "language": ["de:0.99"], "booktitle": "Lenau, Nikolaus: Gedichte. Stuttgart, 1832."}, "poem": {"stanza.1": {"line.1": {"text": "Horch! pl\u00f6tzlich st\u00f6rt ein Ruf die Einsamkeit:", "tokens": ["Horch", "!", "pl\u00f6tz\u00b7lich", "st\u00f6rt", "ein", "Ruf", "die", "Ein\u00b7sam\u00b7keit", ":"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "ADJD", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Klang's nicht aus der Kapelle \u00f6den Mauern?", "tokens": ["Klang's", "nicht", "aus", "der", "Ka\u00b7pel\u00b7le", "\u00f6\u00b7den", "Mau\u00b7ern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wer ist es, der so wunderlich dort schreit,", "tokens": ["Wer", "ist", "es", ",", "der", "so", "wun\u00b7der\u00b7lich", "dort", "schreit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "$,", "PRELS", "ADV", "ADJD", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df mich's unheimlich fa\u00dft mit kaltem Schauern?!", "tokens": ["Da\u00df", "mich's", "un\u00b7heim\u00b7lich", "fa\u00dft", "mit", "kal\u00b7tem", "Schau\u00b7ern", "?!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "\u201eherr Gott! wir loben dich \u2014 ha, ha, ha, ha!\u201c", "tokens": ["\u201e", "herr", "Gott", "!", "wir", "lo\u00b7ben", "dich", "ha", ",", "ha", ",", "ha", ",", "ha", "!", "\u201c"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["$(", "ADJD", "NN", "$.", "PPER", "VVFIN", "PPER", "$(", "ITJ", "$,", "ITJ", "$,", "ITJ", "$,", "ITJ", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Nun schweigt er still, der grause Gottver\u00e4chter;", "tokens": ["Nun", "schweigt", "er", "still", ",", "der", "grau\u00b7se", "Gott\u00b7ver\u00b7\u00e4ch\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und donnernd ruft er nun: \u201eAlleluiah!\u201c", "tokens": ["Und", "don\u00b7nernd", "ruft", "er", "nun", ":", "\u201e", "Al\u00b7le\u00b7lu\u00b7i\u00b7ah", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PPER", "ADV", "$.", "$(", "ITJ", "$.", "$("], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und \u00fcberdonnernd folgt sein Hohngel\u00e4chter.", "tokens": ["Und", "\u00fc\u00b7ber\u00b7don\u00b7nernd", "folgt", "sein", "Hohn\u00b7ge\u00b7l\u00e4ch\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVPP", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da st\u00fcrzt er sich vorbei voll scheuer Hast,", "tokens": ["Da", "st\u00fcrzt", "er", "sich", "vor\u00b7bei", "voll", "scheu\u00b7er", "Hast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das wirre Haar von bleicher Wange streifend,", "tokens": ["Das", "wir\u00b7re", "Haar", "von", "blei\u00b7cher", "Wan\u00b7ge", "strei\u00b7fend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Augen wild bewegt und ohne Rast,", "tokens": ["Die", "Au\u00b7gen", "wild", "be\u00b7wegt", "und", "oh\u00b7ne", "Rast", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "VVPP", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Irrlichter, in der Nacht des Wahnsinns schweifend.", "tokens": ["Irr\u00b7lich\u00b7ter", ",", "in", "der", "Nacht", "des", "Wahn\u00b7sinns", "schwei\u00b7fend", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Er eilt waldein, von seinem Tritte rauscht", "tokens": ["Er", "eilt", "wal\u00b7dein", ",", "von", "sei\u00b7nem", "Trit\u00b7te", "rauscht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das d\u00fcrre Laub im dunkeln Eichenhaine;", "tokens": ["Das", "d\u00fcr\u00b7re", "Laub", "im", "dun\u00b7keln", "Ei\u00b7chen\u00b7hai\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie sinnend bleibt er pl\u00f6tzlich stehn und lauscht,", "tokens": ["Wie", "sin\u00b7nend", "bleibt", "er", "pl\u00f6tz\u00b7lich", "stehn", "und", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VVFIN", "PPER", "ADJD", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und leise h\u00f6r' ich's nun, als ob er weine.", "tokens": ["Und", "lei\u00b7se", "h\u00f6r'", "ich's", "nun", ",", "als", "ob", "er", "wei\u00b7ne", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "PIS", "ADV", "$,", "KOKOM", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Mitleidig rauscht ihr ihm, \u2014 o rauschet nur! \u2014", "tokens": ["Mit\u00b7lei\u00b7dig", "rauscht", "ihr", "ihm", ",", "o", "rau\u00b7schet", "nur", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PPER", "$,", "$(", "FM", "VVFIN", "ADV", "$.", "$("], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den Trost: \u201eVerg\u00e4nglichkeit!\u201c ihr welken Bl\u00e4tter!", "tokens": ["Den", "Trost", ":", "\u201e", "Ver\u00b7g\u00e4ng\u00b7lich\u00b7keit", "!", "\u201c", "ihr", "wel\u00b7ken", "Bl\u00e4t\u00b7ter", "!"], "token_info": ["word", "word", "punct", "punct", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "$(", "NN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O locket seine Seele auf die Spur", "tokens": ["O", "lo\u00b7cket", "sei\u00b7ne", "See\u00b7le", "auf", "die", "Spur"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Des milden Todes, nennt ihm seinen Retter! \u2014", "tokens": ["Des", "mil\u00b7den", "To\u00b7des", ",", "nennt", "ihm", "sei\u00b7nen", "Ret\u00b7ter", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "VVFIN", "PPER", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Zur sanften Wehmuth lichtet sich das Thal,", "tokens": ["Zur", "sanf\u00b7ten", "Weh\u00b7muth", "lich\u00b7tet", "sich", "das", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Dort kommt der Mond zum stillen Abschiedsfeste,", "tokens": ["Dort", "kommt", "der", "Mond", "zum", "stil\u00b7len", "Ab\u00b7schieds\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es will sein Silberschimmer noch einmal", "tokens": ["Es", "will", "sein", "Sil\u00b7ber\u00b7schim\u00b7mer", "noch", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "ADV", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sich schmiegen an des Sommers karge Reste.", "tokens": ["Sich", "schmie\u00b7gen", "an", "des", "Som\u00b7mers", "kar\u00b7ge", "Res\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Wie schwach ist schon der Eiche fahles Laub!", "tokens": ["Wie", "schwach", "ist", "schon", "der", "Ei\u00b7che", "fah\u00b7les", "Laub", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Den leichten Mondstrahl kann es nicht mehr tragen;", "tokens": ["Den", "leich\u00b7ten", "Mond\u00b7strahl", "kann", "es", "nicht", "mehr", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "PPER", "PTKNEG", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es bricht und zittert unter ihm in Staub,", "tokens": ["Es", "bricht", "und", "zit\u00b7tert", "un\u00b7ter", "ihm", "in", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "APPR", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und l\u00e4\u00dft die kahlen Aeste traurig ragen. \u2014", "tokens": ["Und", "l\u00e4\u00dft", "die", "kah\u00b7len", "A\u00b7es\u00b7te", "trau\u00b7rig", "ra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "ADJD", "VVFIN", "$.", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.8": {"line.1": {"text": "Da steht der Irre, bleich und stumm, den Blick,", "tokens": ["Da", "steht", "der", "Ir\u00b7re", ",", "bleich", "und", "stumm", ",", "den", "Blick", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Das bittre L\u00e4cheln auf den Mond gerichtet,", "tokens": ["Das", "bitt\u00b7re", "L\u00e4\u00b7cheln", "auf", "den", "Mond", "ge\u00b7rich\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Es prallt das Mondlicht scheu von ihm zur\u00fcck,", "tokens": ["Es", "prallt", "das", "Mond\u00b7licht", "scheu", "von", "ihm", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADJD", "APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und scheu der Wind an ihm vor\u00fcberfl\u00fcchtet.", "tokens": ["Und", "scheu", "der", "Wind", "an", "ihm", "vor\u00b7\u00fc\u00b7berf\u00b7l\u00fcch\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ART", "NN", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.9": {"line.1": {"text": "Starrt so des Wahnsinns Auge wild hinauf", "tokens": ["Starrt", "so", "des", "Wahn\u00b7sinns", "Au\u00b7ge", "wild", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "PTKVZ"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.2": {"text": "Zum stillen, klaren, ewiggleichen Frieden,", "tokens": ["Zum", "stil\u00b7len", ",", "kla\u00b7ren", ",", "e\u00b7wig\u00b7glei\u00b7chen", "Frie\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit dem die Sterne wandeln ihren Lauf:", "tokens": ["Mit", "dem", "die", "Ster\u00b7ne", "wan\u00b7deln", "ih\u00b7ren", "Lauf", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ein Anblick ist's der traurigsten hienieden. \u2014", "tokens": ["Ein", "An\u00b7blick", "ist's", "der", "trau\u00b7rigs\u00b7ten", "hien\u00b7ie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "ART", "ADJA", "VVFIN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.10": {"line.1": {"text": "Was hat, o Schicksal, dieser Mensch gethan,", "tokens": ["Was", "hat", ",", "o", "Schick\u00b7sal", ",", "die\u00b7ser", "Mensch", "ge\u00b7than", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "$,", "FM", "NN", "$,", "PDAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df mit des Wahnsinns bangen Finsternissen", "tokens": ["Da\u00df", "mit", "des", "Wahn\u00b7sinns", "ban\u00b7gen", "Fins\u00b7ter\u00b7nis\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du ihm versch\u00fcttet hast die Lebensbahn,", "tokens": ["Du", "ihm", "ver\u00b7sch\u00fct\u00b7tet", "hast", "die", "Le\u00b7bens\u00b7bahn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VVPP", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Aus seiner Seele seinen Gott gerissen?", "tokens": ["Aus", "sei\u00b7ner", "See\u00b7le", "sei\u00b7nen", "Gott", "ge\u00b7ris\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}