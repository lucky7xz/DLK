{"dta.poem.2976": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905195108", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wo nicht, wie Mosis Stab die andern St\u00e4be fra\u00df,", "tokens": ["Wo", "nicht", ",", "wie", "Mo\u00b7sis", "Stab", "die", "an\u00b7dern", "St\u00e4\u00b7be", "fra\u00df", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PTKNEG", "$,", "PWAV", "NE", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Womit sich gegen ihn die Gaukelei verma\u00df,", "tokens": ["Wo\u00b7mit", "sich", "ge\u00b7gen", "ihn", "die", "Gau\u00b7ke\u00b7lei", "ver\u00b7ma\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PRF", "APPR", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wo so nicht ein Profet jetzt auch die andern fri\u00dft,", "tokens": ["Wo", "so", "nicht", "ein", "Pro\u00b7fet", "jetzt", "auch", "die", "an\u00b7dern", "fri\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "PTKNEG", "ART", "NN", "ADV", "ADV", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Damit ihr, welchem ihr zu glauben habet, wi\u00dft;", "tokens": ["Da\u00b7mit", "ihr", ",", "wel\u00b7chem", "ihr", "zu", "glau\u00b7ben", "ha\u00b7bet", ",", "wi\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PRELS", "PPER", "PTKZU", "VVINF", "VAFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "So werdet ihr im Lerm erblinden und ertauben,", "tokens": ["So", "wer\u00b7det", "ihr", "im", "Lerm", "er\u00b7blin\u00b7den", "und", "er\u00b7tau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "APPRART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df ihr am Ende nichts und alles werdet glauben.", "tokens": ["Da\u00df", "ihr", "am", "En\u00b7de", "nichts", "und", "al\u00b7les", "wer\u00b7det", "glau\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PIS", "KON", "PIS", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}