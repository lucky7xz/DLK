{"textgrid.poem.34031": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Ph\u00f6nix", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Bist du selber, o Mensch, der Ph\u00f6nix, von welchem du tr\u00e4umtest,", "tokens": ["Bist", "du", "sel\u00b7ber", ",", "o", "Mensch", ",", "der", "Ph\u00f6\u00b7nix", ",", "von", "wel\u00b7chem", "du", "tr\u00e4um\u00b7test", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "$,", "FM", "NN", "$,", "ART", "NN", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "--+--+-+--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da\u00df ihn die Flamme verj\u00fcngt? Innig beklag' ich dich dann,", "tokens": ["Da\u00df", "ihn", "die", "Flam\u00b7me", "ver\u00b7j\u00fcngt", "?", "In\u00b7nig", "be\u00b7klag'", "ich", "dich", "dann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$.", "NN", "VVFIN", "PPER", "PRF", "ADV", "$,"], "meter": "-+-+--++--+--+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Da\u00df man aus feuchtem Holz den Scheiterhaufen dir th\u00fcrmte", "tokens": ["Da\u00df", "man", "aus", "feuch\u00b7tem", "Holz", "den", "Schei\u00b7ter\u00b7hau\u00b7fen", "dir", "th\u00fcrm\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "APPR", "ADJA", "NN", "ART", "NN", "PPER", "VVFIN"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Und in regnigter Nacht gar in den Brand ihn gesteckt.", "tokens": ["Und", "in", "reg\u00b7nig\u00b7ter", "Nacht", "gar", "in", "den", "Brand", "ihn", "ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADV", "APPR", "ART", "NN", "PPER", "VVPP", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Anfangs zwar sch\u00fcrt Amor das Feuer, er hat es entz\u00fcndet,", "tokens": ["An\u00b7fangs", "zwar", "sch\u00fcrt", "A\u00b7mor", "das", "Feu\u00b7er", ",", "er", "hat", "es", "ent\u00b7z\u00fcn\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "NE", "ART", "NN", "$,", "PPER", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+--++--+--+--+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Lustig prasselt es auf, doch er vers\u00e4umt es zu bald,", "tokens": ["Lus\u00b7tig", "pras\u00b7selt", "es", "auf", ",", "doch", "er", "ver\u00b7s\u00e4umt", "es", "zu", "bald", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPER", "PTKVZ", "$,", "KON", "PPER", "VVFIN", "PPER", "APPR", "ADV", "$,"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Nun erlischt es, du liegst auf todten Kohlen, die Winde", "tokens": ["Nun", "er\u00b7lischt", "es", ",", "du", "liegst", "auf", "tod\u00b7ten", "Koh\u00b7len", ",", "die", "Win\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN"], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.8": {"text": "Sausen, der Regen tropft, und du erstarrst und erfrierst.", "tokens": ["Sau\u00b7sen", ",", "der", "Re\u00b7gen", "tropft", ",", "und", "du", "er\u00b7starrst", "und", "er\u00b7frierst", "."], "token_info": ["word", "punct", "word", "word", "wo