{"dta.poem.1239": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Mit was vor S\u00fc\u00dfigkeit/ o zarter Mund/", "tokens": ["Mit", "was", "vor", "S\u00fc\u00b7\u00dfig\u00b7keit", "/", "o", "zar\u00b7ter", "Mund", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "$(", "FM", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bek\u00fc\u00df ich den Rubinen-Grund!", "tokens": ["Be\u00b7k\u00fc\u00df", "ich", "den", "Ru\u00b7bi\u00b7nen\u00b7Grund", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Mit was vor S\u00fc\u00dfigkeit h\u00f6r ich die Lippen sprechen/", "tokens": ["Mit", "was", "vor", "S\u00fc\u00b7\u00dfig\u00b7keit", "h\u00f6r", "ich", "die", "Lip\u00b7pen", "spre\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "APPR", "NN", "VVFIN", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die voller Honig-Worte seyn!", "tokens": ["Die", "vol\u00b7ler", "Ho\u00b7nig\u00b7Wor\u00b7te", "seyn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach aber/ sch\u00f6pff ich ein Vergn\u00fcgen ein/", "tokens": ["Ach", "a\u00b7ber", "/", "sch\u00f6pff", "ich", "ein", "Ver\u00b7gn\u00fc\u00b7gen", "ein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "ADV", "$(", "VVFIN", "PPER", "ART", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So mu\u00df ich unterde\u00df des andern mich entbrechen.", "tokens": ["So", "mu\u00df", "ich", "un\u00b7ter\u00b7de\u00df", "des", "an\u00b7dern", "mich", "ent\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ART", "ADJA", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Dein Himmels-Geist belebt der Worte Flu\u00df/", "tokens": ["Dein", "Him\u00b7mels\u00b7Geist", "be\u00b7lebt", "der", "Wor\u00b7te", "Flu\u00df", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Seelen Seele deinen Ku\u00df.", "tokens": ["Der", "See\u00b7len", "See\u00b7le", "dei\u00b7nen", "Ku\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wie soll ich mich der Wahl/ der schweren Wahl entbrechen?", "tokens": ["Wie", "soll", "ich", "mich", "der", "Wahl", "/", "der", "schwe\u00b7ren", "Wahl", "ent\u00b7bre\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "PRF", "ART", "NN", "$(", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach/ k\u00f6nte doch dein edler Mund/", "tokens": ["Ach", "/", "k\u00f6n\u00b7te", "doch", "dein", "ed\u00b7ler", "Mund", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VMFIN", "ADV", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dem so viel Gunst der Himmel hat vergunnt/", "tokens": ["Dem", "so", "viel", "Gunst", "der", "Him\u00b7mel", "hat", "ver\u00b7gunnt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PIAT", "NN", "ART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Mit Reden k\u00fcssen/ und mit K\u00fcssen sprechen!", "tokens": ["Mit", "Re\u00b7den", "k\u00fcs\u00b7sen", "/", "und", "mit", "K\u00fcs\u00b7sen", "spre\u00b7chen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVINF", "$(", "KON", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}