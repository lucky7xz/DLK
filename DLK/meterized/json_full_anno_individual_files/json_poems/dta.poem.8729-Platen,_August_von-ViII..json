{"dta.poem.8729": {"metadata": {"author": {"name": "Platen, August von", "birth": "N.A.", "death": "N.A."}, "title": "ViII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1828", "urn": "urn:nbn:de:kobv:b4-200905194597", "language": ["de:0.99"], "booktitle": "Platen, August von: Gedichte. Stuttgart, 1828."}, "poem": {"stanza.1": {"line.1": {"text": "Schimmer sich vom See verlor,", "tokens": ["Schim\u00b7mer", "sich", "vom", "See", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Steigen mir Ged\u00e4chtni\u00dfbilder", "tokens": ["Stei\u00b7gen", "mir", "Ge\u00b7d\u00e4cht\u00b7ni\u00df\u00b7bil\u00b7der"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Wellen Nacht empor:", "tokens": ["Aus", "der", "Wel\u00b7len", "Nacht", "em\u00b7por", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "PTKVZ", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.2": {"line.1": {"text": "Malen mir des Kahnes Schwanken", "tokens": ["Ma\u00b7len", "mir", "des", "Kah\u00b7nes", "Schwan\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Den gefurchten Pfad entlang,", "tokens": ["Den", "ge\u00b7furch\u00b7ten", "Pfad", "ent\u00b7lang", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Als die Morgenl\u00fcfte tranken", "tokens": ["Als", "die", "Mor\u00b7gen\u00b7l\u00fcf\u00b7te", "tran\u00b7ken"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Zauberischen Liederklang.", "tokens": ["Zau\u00b7be\u00b7ri\u00b7schen", "Lie\u00b7der\u00b7klang", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Malen mir, von Berges Kuppe", "tokens": ["Ma\u00b7len", "mir", ",", "von", "Ber\u00b7ges", "Kup\u00b7pe"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NN", "PPER", "$,", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schweifend den erg\u00f6zten Sinn,", "tokens": ["Schwei\u00b7fend", "den", "er\u00b7g\u00f6z\u00b7ten", "Sinn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die l\u00e4ndlichsch\u00f6ne Gruppe", "tokens": ["Und", "die", "l\u00e4nd\u00b7lich\u00b7sch\u00f6\u00b7ne", "Grup\u00b7pe"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Um den Heerd der Sennerin.", "tokens": ["Um", "den", "Heerd", "der", "Sen\u00b7ne\u00b7rin", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Malen mir die Felsgehege,", "tokens": ["Ma\u00b7len", "mir", "die", "Fels\u00b7ge\u00b7he\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die Alpenrose hangt,", "tokens": ["Wo", "die", "Al\u00b7pen\u00b7ro\u00b7se", "hangt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Welche nicht durch Menschenpflege", "tokens": ["Wel\u00b7che", "nicht", "durch", "Men\u00b7schen\u00b7pfle\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "PTKNEG", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Thales G\u00e4rten prangt.", "tokens": ["In", "des", "Tha\u00b7les", "G\u00e4r\u00b7ten", "prangt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "N\u00e4chtlich f\u00fchl' ich jezt ein Bangen,", "tokens": ["N\u00e4cht\u00b7lich", "f\u00fchl'", "ich", "jezt", "ein", "Ban\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wann der See gehoben wallt,", "tokens": ["Wann", "der", "See", "ge\u00b7ho\u00b7ben", "wallt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Jene Tage sind vergangen,", "tokens": ["Je\u00b7ne", "Ta\u00b7ge", "sind", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Jene Stimmen sind verhallt.", "tokens": ["Je\u00b7ne", "Stim\u00b7men", "sind", "ver\u00b7hallt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VAFIN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Frost'ge Nebel steigen, welche", "tokens": ["Frost'\u00b7ge", "Ne\u00b7bel", "stei\u00b7gen", ",", "wel\u00b7che"], "token_info": ["word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "VVINF", "$,", "PRELS"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Berg und Kuppe tr\u00fcb' umziehn,", "tokens": ["Berg", "und", "Kup\u00b7pe", "tr\u00fcb'", "um\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "VVIZU", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die rothen Alpenkelche", "tokens": ["Und", "die", "ro\u00b7then", "Al\u00b7pen\u00b7kel\u00b7che"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Werden mit dem Sommer fliehn.", "tokens": ["Wer\u00b7den", "mit", "dem", "Som\u00b7mer", "fliehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Bald, verjagt von Sturm und Flocken", "tokens": ["Bald", ",", "ver\u00b7jagt", "von", "Sturm", "und", "Flo\u00b7cken"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "VVPP", "APPR", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zieht die Hirtin froh in's Thal,", "tokens": ["Zieht", "die", "Hir\u00b7tin", "froh", "in's", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und es t\u00f6nt der Hall der Glocken", "tokens": ["Und", "es", "t\u00f6nt", "der", "Hall", "der", "Glo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Von der H\u00f6h' zum lezten Mal.", "tokens": ["Von", "der", "H\u00f6h'", "zum", "lez\u00b7ten", "Mal", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}}}}