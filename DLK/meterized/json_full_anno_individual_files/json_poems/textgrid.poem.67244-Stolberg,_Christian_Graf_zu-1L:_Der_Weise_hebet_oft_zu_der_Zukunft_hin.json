{"textgrid.poem.67244": {"metadata": {"author": {"name": "Stolberg, Christian Graf zu", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Weise hebet oft zu der Zukunft hin", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Weise hebet oft zu der Zukunft hin", "tokens": ["Der", "Wei\u00b7se", "he\u00b7bet", "oft", "zu", "der", "Zu\u00b7kunft", "hin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "APPR", "ART", "NN", "PTKVZ"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Blick und senkt ihn oft zur Vergangenheit,", "tokens": ["Den", "Blick", "und", "senkt", "ihn", "oft", "zur", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nur Er genie\u00dft in heiterm Gleichmuth,", "tokens": ["Nur", "Er", "ge\u00b7nie\u00dft", "in", "hei\u00b7term", "Gleich\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Stunde der Gegenwart, deine Segen.", "tokens": ["Stun\u00b7de", "der", "Ge\u00b7gen\u00b7wart", ",", "dei\u00b7ne", "Se\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.2": {"line.1": {"text": "Ist sie's denn nicht, die ehmahl uns Zukunft war,", "tokens": ["Ist", "sie's", "denn", "nicht", ",", "die", "eh\u00b7mahl", "uns", "Zu\u00b7kunft", "war", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "PTKNEG", "$,", "PRELS", "ADV", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Und wird sie einst nicht seyn uns Vergangenheit?", "tokens": ["Und", "wird", "sie", "einst", "nicht", "seyn", "uns", "Ver\u00b7gan\u00b7gen\u00b7heit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "PTKNEG", "VAINF", "PPER", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Umschwebt sie mich nicht n\u00e4her, als was", "tokens": ["Um\u00b7schwebt", "sie", "mich", "nicht", "n\u00e4\u00b7her", ",", "als", "was"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "PTKNEG", "ADJD", "$,", "KOUS", "PIS"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "War, und als alles was einst noch seyn wird?", "tokens": ["War", ",", "und", "als", "al\u00b7les", "was", "einst", "noch", "seyn", "wird", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KON", "KOUS", "PIS", "PWS", "ADV", "ADV", "VAINF", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Tr\u00fcbt oft nicht Reu', umw\u00f6lkt nicht Erinnerung", "tokens": ["Tr\u00fcbt", "oft", "nicht", "Reu'", ",", "um\u00b7w\u00f6lkt", "nicht", "E\u00b7rin\u00b7ne\u00b7rung"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "PTKNEG", "NE", "$,", "VVFIN", "PTKNEG", "NN"], "meter": "+--+-+--+--", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Ein ernstes Auge, wenn's in die Vorzeit schaut,", "tokens": ["Ein", "erns\u00b7tes", "Au\u00b7ge", ",", "wenn's", "in", "die", "Vor\u00b7zeit", "schaut", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "KOUS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "F\u00e4rbt Aberwitz und Thorheit oftmahl", "tokens": ["F\u00e4rbt", "A\u00b7ber\u00b7witz", "und", "Thor\u00b7heit", "oft\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Nicht der Erwartungen buntes Sp\u00e4hglas?", "tokens": ["Nicht", "der", "Er\u00b7war\u00b7tun\u00b7gen", "bun\u00b7tes", "Sp\u00e4h\u00b7glas", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "O Labyrinth des Irrsals, du Menschenherz!", "tokens": ["O", "La\u00b7by\u00b7rinth", "des", "Irr\u00b7sals", ",", "du", "Men\u00b7schen\u00b7herz", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "$,", "PPER", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "O Labyrinth des Irrsals mein eignes Herz!", "tokens": ["O", "La\u00b7by\u00b7rinth", "des", "Irr\u00b7sals", "mein", "eig\u00b7nes", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ART", "NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Das f\u00fcr gehoffte, ferne Freuden", "tokens": ["Das", "f\u00fcr", "ge\u00b7hoff\u00b7te", ",", "fer\u00b7ne", "Freu\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oft die gewissen, die nahen, wegwarf!", "tokens": ["Oft", "die", "ge\u00b7wis\u00b7sen", ",", "die", "na\u00b7hen", ",", "weg\u00b7warf", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "$,", "ART", "ADJA", "$,", "VVFIN", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Da\u00df ich mir \u00e4msig oft mit der M\u00fche Schwei\u00df", "tokens": ["Da\u00df", "ich", "mir", "\u00e4m\u00b7sig", "oft", "mit", "der", "M\u00fc\u00b7he", "Schwei\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Den Wermuthtrank der Reue bereitete,", "tokens": ["Den", "Wer\u00b7muth\u00b7trank", "der", "Reu\u00b7e", "be\u00b7rei\u00b7te\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+-+--+--+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Oft, die zum Kranz ich flocht, entbl\u00e4ttert", "tokens": ["Oft", ",", "die", "zum", "Kranz", "ich", "flocht", ",", "ent\u00b7bl\u00e4t\u00b7tert"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PRELS", "APPRART", "NN", "PPER", "VVFIN", "$,", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welkte die Rose, nur Dornen blieben.", "tokens": ["Welk\u00b7te", "die", "Ro\u00b7se", ",", "nur", "Dor\u00b7nen", "blie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "ADV", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Der Freude wallt das strebende Herz empor,", "tokens": ["Der", "Freu\u00b7de", "wallt", "das", "stre\u00b7ben\u00b7de", "Herz", "em\u00b7por", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Doch dies verzagte, trotzige Mi\u00dfgesch\u00f6pf", "tokens": ["Doch", "dies", "ver\u00b7zag\u00b7te", ",", "trot\u00b7zi\u00b7ge", "Mi\u00df\u00b7ge\u00b7sch\u00f6pf"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PDS", "VVFIN", "$,", "ADJA", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Pflanzt sich oft selbst des Kummers Keime,", "tokens": ["Pflanzt", "sich", "oft", "selbst", "des", "Kum\u00b7mers", "Kei\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "ADV", "ART", "NN", "NN", "$,"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Erndtet mit Schaam, was es m\u00fchsam s\u00e4ete.", "tokens": ["Ernd\u00b7tet", "mit", "Schaam", ",", "was", "es", "m\u00fch\u00b7sam", "s\u00e4e\u00b7te", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "NN", "$,", "PWS", "PPER", "ADJD", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Mit Bergstroms Eile fliehet die Stund' hinweg,", "tokens": ["Mit", "Berg\u00b7stroms", "Ei\u00b7le", "flie\u00b7het", "die", "Stund'", "hin\u00b7weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "St\u00fcrzt j\u00e4hling sich in's Meer der Vergangenheit,", "tokens": ["St\u00fcrzt", "j\u00e4h\u00b7ling", "sich", "in's", "Meer", "der", "Ver\u00b7gan\u00b7gen\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PRF", "APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Dort schifft in stiller Mondnacht oft, und", "tokens": ["Dort", "schifft", "in", "stil\u00b7ler", "Mond\u00b7nacht", "oft", ",", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "ADV", "$,", "KON"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Oft die Erinnrung im Klippenstrudel.", "tokens": ["Oft", "die", "E\u00b7rinn\u00b7rung", "im", "Klip\u00b7pen\u00b7stru\u00b7del", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "APPRART", "NN", "$."], "meter": "---+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.8": {"line.1": {"text": "Da\u00df sie im Klippenstrudel nicht strande, da\u00df", "tokens": ["Da\u00df", "sie", "im", "Klip\u00b7pen\u00b7stru\u00b7del", "nicht", "stran\u00b7de", ",", "da\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "PTKNEG", "VVFIN", "$,", "KOUS"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Sie schiff' in stiller, lieblicher Mondesnacht,", "tokens": ["Sie", "schiff'", "in", "stil\u00b7ler", ",", "lieb\u00b7li\u00b7cher", "Mon\u00b7des\u00b7nacht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Da\u00df sanft der Nachen gleite, Sterne", "tokens": ["Da\u00df", "sanft", "der", "Na\u00b7chen", "glei\u00b7te", ",", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "ADJD", "ART", "NN", "VVFIN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Unter ihm spiegeln und Fische scherzen,", "tokens": ["Un\u00b7ter", "ihm", "spie\u00b7geln", "und", "Fi\u00b7sche", "scher\u00b7zen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVINF", "KON", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.9": {"line.1": {"text": "Dies harret dein, wenn Weisheit die Blume des", "tokens": ["Dies", "har\u00b7ret", "dein", ",", "wenn", "Weis\u00b7heit", "die", "Blu\u00b7me", "des"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "$,", "KOUS", "NN", "ART", "NN", "ART"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Genusses pfl\u00fccket, die dir im Pfade spro\u00dft;", "tokens": ["Ge\u00b7nus\u00b7ses", "pfl\u00fc\u00b7cket", ",", "die", "dir", "im", "Pfa\u00b7de", "spro\u00dft", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Den, der sie pfl\u00fcckt, den kr\u00e4nzt sie, hold ist", "tokens": ["Den", ",", "der", "sie", "pfl\u00fcckt", ",", "den", "kr\u00e4nzt", "sie", ",", "hold", "ist"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPER", "VVFIN", "$,", "ART", "VVFIN", "PPER", "$,", "ADJD", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ihm die Vergangenheit, ihm die Zukunft!", "tokens": ["Ihm", "die", "Ver\u00b7gan\u00b7gen\u00b7heit", ",", "ihm", "die", "Zu\u00b7kunft", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "$,", "PPER", 