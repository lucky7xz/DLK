{"textgrid.poem.54533": {"metadata": {"author": {"name": "Gryphius, Andreas", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1640, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die edle Sonn' ersucht nunmehr ", "tokens": ["Die", "ed\u00b7le", "Sonn'", "er\u00b7sucht", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und theilt die Stunden gleich dem Tag aus und der Nacht/", "tokens": ["Und", "theilt", "die", "Stun\u00b7den", "gleich", "dem", "Tag", "aus", "und", "der", "Nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "ADV", "ART", "NN", "PTKVZ", "KON", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie bringt mir diese Zeit/ die mich ans Licht gebracht/", "tokens": ["Sie", "bringt", "mir", "die\u00b7se", "Zeit", "/", "die", "mich", "ans", "Licht", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PDAT", "NN", "$(", "PRELS", "PRF", "APPRART", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Als sie den neundten Theil erreicht mit goldnen Strahlen.", "tokens": ["Als", "sie", "den", "neund\u00b7ten", "Theil", "er\u00b7reicht", "mit", "gold\u00b7nen", "Strah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "O ewiglichte- Sonn! die du die Himmel mahlen/", "tokens": ["O", "e\u00b7wig\u00b7lich\u00b7te", "Sonn", "!", "die", "du", "die", "Him\u00b7mel", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "TRUNC", "NN", "$.", "PRELS", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die Erden schmincken kanst; die meine Seel anlacht/", "tokens": ["Die", "Er\u00b7den", "schmin\u00b7cken", "kanst", ";", "die", "mei\u00b7ne", "Seel", "an\u00b7lacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "VMFIN", "$.", "ART", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die da ich tod in Schuld mich gn\u00e4digst lebend macht/", "tokens": ["Die", "da", "ich", "tod", "in", "Schuld", "mich", "gn\u00e4\u00b7digst", "le\u00b7bend", "macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "KOUS", "PPER", "NN", "APPR", "NN", "PPER", "VVFIN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wie werd ich deine Huld/ und mit was Danck bezahlen!", "tokens": ["Wie", "werd", "ich", "dei\u00b7ne", "Huld", "/", "und", "mit", "was", "Danck", "be\u00b7zah\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "PPOSAT", "NN", "$(", "KON", "APPR", "PRELS", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Hilff da\u00df ich meine Zeit genau und wol abtheil/", "tokens": ["Hilff", "da\u00df", "ich", "mei\u00b7ne", "Zeit", "ge\u00b7nau", "und", "wol", "ab\u00b7theil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PPOSAT", "NN", "ADJD", "KON", "ADV", "KOUS", "$("], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Gerechtigkeit Ach hilff! Hilff da\u00df ich nimmer feil/", "tokens": ["Ge\u00b7rech\u00b7tig\u00b7keit", "Ach", "hilff", "!", "Hilff", "da\u00df", "ich", "nim\u00b7mer", "feil", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NN", "$.", "NN", "KOUS", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und leben mag an dir wann du wirst Urtheil hegen.", "tokens": ["Und", "le\u00b7ben", "mag", "an", "dir", "wann", "du", "wirst", "Ur\u00b7theil", "he\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "APPR", "PPER", "PWAV", "PPER", "VAFIN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "In dessen gib da\u00df ich das Pfand so mir vertraut", "tokens": ["In", "des\u00b7sen", "gib", "da\u00df", "ich", "das", "Pfand", "so", "mir", "ver\u00b7traut"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PRELAT", "VVIMP", "KOUS", "PPER", "ART", "NN", "ADV", "PPER", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu deiner Ehr und Nutz/ der die auf dich erbaut/", "tokens": ["Zu", "dei\u00b7ner", "Ehr", "und", "Nutz", "/", "der", "die", "auf", "dich", "er\u00b7baut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$(", "ART", "ART", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "M\u00f6g embsi