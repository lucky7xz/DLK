{"dta.poem.9106": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1898", "urn": "urn:nbn:de:kobv:b4-200905192685", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unten im Dorf", "tokens": ["Un\u00b7ten", "im", "Dorf"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "hinter der Kirchhofsmauer", "tokens": ["hin\u00b7ter", "der", "Kirch\u00b7hofs\u00b7mau\u00b7er"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ART", "NN"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.3": {"text": "schl\u00e4ft der M\u00fcller.", "tokens": ["schl\u00e4ft", "der", "M\u00fcl\u00b7ler", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.2": {"line.1": {"text": "Die M\u00fchle steht still.", "tokens": ["Die", "M\u00fch\u00b7le", "steht", "still", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.3": {"line.1": {"text": "Auf ihrem morschen Geb\u00e4lk kriechen Marieenk\u00e4ferchen,", "tokens": ["Auf", "ih\u00b7rem", "mor\u00b7schen", "Ge\u00b7b\u00e4lk", "krie\u00b7chen", "Ma\u00b7ri\u00b7e\u00b7en\u00b7k\u00e4\u00b7fer\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "\u00fcber sie fliegt ein Kukuk hin.", "tokens": ["\u00fc\u00b7ber", "sie", "fliegt", "ein", "Ku\u00b7kuk", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.4": {"line.1": {"text": ". . . Kukuk . . . Kukuk . . .", "tokens": [".", ".", ".", "Ku\u00b7kuk", ".", ".", ".", "Ku\u00b7kuk", ".", ".", "."], "token_info": ["punct", "punct", "punct", "word", "punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "NN", "$.", "$.", "$.", "NN", "$.", "$.", "$."], "meter": "+-+-", "measure": "trochaic.di"}}, "stanza.5": {"line.1": {"text": "Den steilen Weg durchs Korn her kommen Kinder,", "tokens": ["Den", "stei\u00b7len", "Weg", "durchs", "Korn", "her", "kom\u00b7men", "Kin\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "APZR", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "lachen, schwatzen und stopfen Gras durch die Ritzen.", "tokens": ["la\u00b7chen", ",", "schwat\u00b7zen", "und", "stop\u00b7fen", "Gras", "durch", "die", "Rit\u00b7zen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVINF", "$,", "VVFIN", "KON", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+--+-+--+-", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Eins kuckt durch.", "tokens": ["Eins", "kuckt", "durch", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}}, "stanza.7": {"line.1": {"text": ". . . Kukuk . . .", "tokens": [".", ".", ".", "Ku\u00b7kuk", ".", ".", "."], "token_info": ["punct", "punct", "punct", "word", "punct", "punct", "punct"], "pos": ["$.", "$.", "$.", "NN", "$.", "$.", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.8": {"line.1": {"text": "Innen:", "tokens": ["In\u00b7nen", ":"], "token_info": ["word", "punct"], "pos": ["ADV", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.2": {"text": "Sonnenstrahlen und Schmetterlinge!", "tokens": ["Son\u00b7nen\u00b7strah\u00b7len", "und", "Schmet\u00b7ter\u00b7lin\u00b7ge", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}}}}}