{"dta.poem.20508": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Verliebte Arien.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Komm/ Philirose/ schau die nacht/", "tokens": ["Komm", "/", "Phi\u00b7li\u00b7ro\u00b7se", "/", "schau", "die", "nacht", "/"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$(", "NE", "$(", "ADJD", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Du julep halbverwundter hertzen/", "tokens": ["Du", "ju\u00b7lep", "halb\u00b7ver\u00b7wund\u00b7ter", "hert\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die zeit/ wo wir mit liebe schertzen/", "tokens": ["Die", "zeit", "/", "wo", "wir", "mit", "lie\u00b7be", "schert\u00b7zen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PPER", "APPR", "ADJA", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Hat sich nunmehr zu uns gemacht/", "tokens": ["Hat", "sich", "nun\u00b7mehr", "zu", "uns", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PRF", "ADV", "APPR", "PPER", "VVPP", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das bleiche licht ist uns erschienen/", "tokens": ["Das", "blei\u00b7che", "licht", "ist", "uns", "er\u00b7schie\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "PPER", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will uns zur liebes-fackel dienen.", "tokens": ["Will", "uns", "zur", "lie\u00b7bes\u00b7fa\u00b7ckel", "die\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Entbl\u00f6sse jenes sch\u00f6ne feld/", "tokens": ["Ent\u00b7bl\u00f6s\u00b7se", "je\u00b7nes", "sch\u00f6\u00b7ne", "feld", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo meine seel die wahlstatt funden/", "tokens": ["Wo", "mei\u00b7ne", "seel", "die", "wahl\u00b7statt", "fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und meine krafft wie schnee verschwunden/", "tokens": ["Und", "mei\u00b7ne", "krafft", "wie", "schnee", "ver\u00b7schwun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KOKOM", "NN", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Als sich dein blick/ der starcke held/", "tokens": ["Als", "sich", "dein", "blick", "/", "der", "star\u00b7cke", "held", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "$(", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So tausend flammen mit sich brachte/", "tokens": ["So", "tau\u00b7send", "flam\u00b7men", "mit", "sich", "brach\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "APPR", "PRF", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zugleich aus deinen augen machte.", "tokens": ["Zu\u00b7gleich", "aus", "dei\u00b7nen", "au\u00b7gen", "mach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach Philirose komm geschwind/", "tokens": ["Ach", "Phi\u00b7li\u00b7ro\u00b7se", "komm", "ge\u00b7schwind", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "VVFIN", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "La\u00df mich das paradie\u00df besteigen/", "tokens": ["La\u00df", "mich", "das", "pa\u00b7ra\u00b7die\u00df", "be\u00b7stei\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Den ort/ wo sich die gaben zeigen/", "tokens": ["Den", "ort", "/", "wo", "sich", "die", "ga\u00b7ben", "zei\u00b7gen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PWAV", "PRF", "ART", "VVFIN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Die weisser/ als narcissen/ sind/", "tokens": ["Die", "weis\u00b7ser", "/", "als", "nar\u00b7cis\u00b7sen", "/", "sind", "/"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$(", "KOUS", "NN", "$(", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "La\u00df mich in den verliebten stellen/", "tokens": ["La\u00df", "mich", "in", "den", "ver\u00b7lieb\u00b7ten", "stel\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "APPR", "ART", "ADJA", "VVINF", "$("], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.6": {"text": "Dir meine seele zugesellen.", "tokens": ["Dir", "mei\u00b7ne", "see\u00b7le", "zu\u00b7ge\u00b7sel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Hier ist kein schrecken noch gefahr/", "tokens": ["Hier", "ist", "kein", "schre\u00b7cken", "noch", "ge\u00b7fahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "VVFIN", "ADV", "ADJD", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Entkleide deine brust und lenden.", "tokens": ["Ent\u00b7klei\u00b7de", "dei\u00b7ne", "brust", "und", "len\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPOSAT", "NN", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich will die glut auff diesen enden/", "tokens": ["Ich", "will", "die", "glut", "auff", "die\u00b7sen", "en\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "PDAT", "NN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was mir die erste flamm gebahr/", "tokens": ["Was", "mir", "die", "ers\u00b7te", "flamm", "ge\u00b7bahr", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es soll mein mund auff deinem liegen/", "tokens": ["Es", "soll", "mein", "mund", "auff", "dei\u00b7nem", "lie\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "APPR", "PPOSAT", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bi\u00df mich die ohnmacht wird besiegen.", "tokens": ["Bi\u00df", "mich", "die", "ohn\u00b7macht", "wird", "be\u00b7sie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "ADJD", "VAFIN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und wenn ich nun besieget bin/", "tokens": ["Und", "wenn", "ich", "nun", "be\u00b7sie\u00b7get", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADV", "VVPP", "VAFIN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn die entseelung mich gewonnen/", "tokens": ["Wenn", "die", "ent\u00b7see\u00b7lung", "mich", "ge\u00b7won\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und mir das leben gar entnommen/", "tokens": ["Und", "mir", "das", "le\u00b7ben", "gar", "ent\u00b7nom\u00b7men", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PDS", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So gib mir tausend k\u00fcsse hin;", "tokens": ["So", "gib", "mir", "tau\u00b7send", "k\u00fcs\u00b7se", "hin", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und solt ich neue krafft erwerben/", "tokens": ["Und", "solt", "ich", "neu\u00b7e", "krafft", "er\u00b7wer\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich noch zehnmahl also sterben.", "tokens": ["Will", "ich", "noch", "zehn\u00b7mahl", "al\u00b7so", "ster\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}