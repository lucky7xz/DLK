{"textgrid.poem.55284": {"metadata": {"author": {"name": "Goethe, Johann Wolfgang", "birth": "N.A.", "death": "N.A."}, "title": "Lust und Qual", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Knabe sa\u00df ich, Fischerknabe,", "tokens": ["Kna\u00b7be", "sa\u00df", "ich", ",", "Fi\u00b7scher\u00b7kna\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Auf dem schwarzen Fels im Meer,", "tokens": ["Auf", "dem", "schwar\u00b7zen", "Fels", "im", "Meer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und bereitend falsche Gabe,", "tokens": ["Und", "be\u00b7rei\u00b7tend", "fal\u00b7sche", "Ga\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sang ich, lauschend rings umher.", "tokens": ["Sang", "ich", ",", "lau\u00b7schend", "rings", "um\u00b7her", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "ADJD", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Angel schwebte lockend nieder;", "tokens": ["An\u00b7gel", "schweb\u00b7te", "lo\u00b7ckend", "nie\u00b7der", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Gleich ein Fischlein streift und schnappt,", "tokens": ["Gleich", "ein", "Fisc\u00b7hlein", "streift", "und", "schnappt", ","], "token_info": ["word", "word", "word",