{"dta.poem.17902": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "145.  Die seltzame Tugend.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Gott spricht/ wer sich versenckt der wird erhaben/", "tokens": ["Gott", "spricht", "/", "wer", "sich", "ver\u00b7senckt", "der", "wird", "er\u00b7ha\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$(", "PWS", "PRF", "VVFIN", "ART", "VAFIN", "ADJD", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "werden:", "tokens": ["wer\u00b7den", ":"], "token_info": ["word", "punct"], "pos": ["VAINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Und doch ist dieses thun das seltzamst\u2019 auf der Erden!", "tokens": ["Und", "doch", "ist", "die\u00b7ses", "thun", "das", "seltz\u00b7amst'", "auf", "der", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PDS", "VVFIN", "PDS", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}