{"textgrid.poem.33114": {"metadata": {"author": {"name": "B\u00fcchner, Luise", "birth": "N.A.", "death": "N.A."}, "title": "Vittoria Colonna", "genre": "verse", "period": "N.A.", "pub_year": 1849, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn Neid das Zeichen nicht gemeiner Seelen,", "tokens": ["Wenn", "Neid", "das", "Zei\u00b7chen", "nicht", "ge\u00b7mei\u00b7ner", "See\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "ART", "NN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Vor allen Frauen m\u00fc\u00dft' ich dich beneiden!", "tokens": ["Vor", "al\u00b7len", "Frau\u00b7en", "m\u00fc\u00dft'", "ich", "dich", "be\u00b7nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du lebtest in des Geistes heil'gen Zeiten,", "tokens": ["Du", "leb\u00b7test", "in", "des", "Geis\u00b7tes", "heil'\u00b7gen", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Da Poesie und Kunst sich neu verm\u00e4hlen;", "tokens": ["Da", "Poe\u00b7sie", "und", "Kunst", "sich", "neu", "ver\u00b7m\u00e4h\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "NN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Du durftest zwischen Myrth' und Lorbeer w\u00e4hlen,", "tokens": ["Du", "durf\u00b7test", "zwi\u00b7schen", "Myr\u00b7th'", "und", "Lor\u00b7beer", "w\u