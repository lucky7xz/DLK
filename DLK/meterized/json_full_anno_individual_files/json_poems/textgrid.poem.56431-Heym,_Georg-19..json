{"textgrid.poem.56431": {"metadata": {"author": {"name": "Heym, Georg", "birth": "N.A.", "death": "N.A."}, "title": "19.", "genre": "verse", "period": "N.A.", "pub_year": 1899, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Schiffe schwimmen durch der Riesen Leichen", "tokens": ["Die", "Schif\u00b7fe", "schwim\u00b7men", "durch", "der", "Rie\u00b7sen", "Lei\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aufs Meer hinaus. Die Segel fa\u00dft der Wind.", "tokens": ["Aufs", "Meer", "hin\u00b7aus", ".", "Die", "Se\u00b7gel", "fa\u00dft", "der", "Wind", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PTKVZ", "$.", "ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie bl\u00e4hen auf sich, und die Rudrer sind", "tokens": ["Sie", "bl\u00e4\u00b7hen", "auf", "sich", ",", "und", "die", "Rud\u00b7rer", "sind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "$,", "KON", "ART", "NN", "VAFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Im Raum gesch\u00e4ftig, durch die Flut zu streichen.", "tokens": ["Im", "Raum", "ge\u00b7sch\u00e4f\u00b7tig", ",", "durch", "die", "Flut", "zu", "strei\u00b7chen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ADJD", "$,", "APPR", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Volk sitzt wie die Fliegen matt im Rumpf,", "tokens": ["Das", "Volk", "sitzt", "wie", "die", "Flie\u00b7gen", "matt", 