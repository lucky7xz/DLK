{"dta.poem.10273": {"metadata": {"author": {"name": "Liliencron, Detlev von", "birth": "N.A.", "death": "N.A."}, "title": "I.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1883", "urn": "urn:nbn:de:kobv:b4-200905197184", "language": ["de:0.99"], "booktitle": "Liliencron, Detlev von: Adjutantenritte und andere Gedichte. Leipzig, [1883]."}, "poem": {"stanza.1": {"line.1": {"text": "Vorbei, vorbei, auf feuchter Spur               ", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7bei", ",", "auf", "feuch\u00b7ter", "Spur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PTKVZ", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Irrt trostlos nun mein Blick ins Weite.", "tokens": ["Irrt", "trost\u00b7los", "nun", "mein", "Blick", "ins", "Wei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ADV", "PPOSAT", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vorbei, vorbei, die M\u00f6we nur", "tokens": ["Vor\u00b7bei", ",", "vor\u00b7bei", ",", "die", "M\u00f6\u00b7we", "nur"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "PTKVZ", "$,", "ART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Giebt mir ein trauriges Geleite.", "tokens": ["Giebt", "mir", "ein", "trau\u00b7ri\u00b7ges", "Ge\u00b7lei\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Nun kehrt auch sie, fernab, fernab", "tokens": ["Nun", "kehrt", "auch", "sie", ",", "fern\u00b7ab", ",", "fern\u00b7ab"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "ADV", "PPER", "$,", "PTKVZ", "$,", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ist l\u00e4ngst mein Vaterland geblieben.", "tokens": ["Ist", "l\u00e4ngst", "mein", "Va\u00b7ter\u00b7land", "ge\u00b7blie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aus meiner Heimat, wo mein Grab", "tokens": ["Aus", "mei\u00b7ner", "Hei\u00b7mat", ",", "wo", "mein", "Grab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich schon gew\u00e4hlt, bin ich vertrieben.", "tokens": ["Ich", "schon", "ge\u00b7w\u00e4hlt", ",", "bin", "ich", "ver\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Als gestern ich im Abschiedszorn", "tokens": ["Als", "ge\u00b7stern", "ich", "im", "Ab\u00b7schieds\u00b7zorn"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "PPER", "APPRART", "NN"], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.2": {"text": "Voll Schmerz den Lindenzweig ger\u00fcttelt,", "tokens": ["Voll", "Schmerz", "den", "Lin\u00b7den\u00b7zweig", "ge\u00b7r\u00fct\u00b7telt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich den Rebhahn h\u00f6rt\u2019 im Korn,", "tokens": ["Als", "ich", "den", "Reb\u00b7hahn", "h\u00f6rt'", "im", "Korn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es hat ein Fieber mich gesch\u00fcttelt.", "tokens": ["Es", "hat", "ein", "Fie\u00b7ber", "mich", "ge\u00b7sch\u00fct\u00b7telt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Es wogt mein Schiff, es sinkt und hebt,", "tokens": ["Es", "wogt", "mein", "Schiff", ",", "es", "sinkt", "und", "hebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein Sturmlied singen die Matrosen.", "tokens": ["Ein", "Sturm\u00b7lied", "sin\u00b7gen", "die", "Mat\u00b7ro\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Es wogt mein Herz, es ringt und bebt,", "tokens": ["Es", "wogt", "mein", "Herz", ",", "es", "ringt", "und", "bebt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Es schl\u00e4gt der Sturm den Heimatlosen.", "tokens": ["Es", "schl\u00e4gt", "der", "Sturm", "den", "Hei\u00b7mat\u00b7lo\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}