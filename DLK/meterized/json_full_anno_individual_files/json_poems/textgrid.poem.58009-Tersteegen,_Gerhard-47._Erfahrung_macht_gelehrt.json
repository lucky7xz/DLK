{"textgrid.poem.58009": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "47. Erfahrung macht gelehrt", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die J\u00fcnger wurden einst des heil'gen Geistes voll;", "tokens": ["Die", "J\u00fcn\u00b7ger", "wur\u00b7den", "einst", "des", "heil'\u00b7gen", "Geis\u00b7tes", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ART", "ADJA", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ich spekuliere nicht, wie ich's begreifen soll.", "tokens": ["Ich", "spe\u00b7ku\u00b7lie\u00b7re", "nicht", ",", "wie", "ich's", "be\u00b7grei\u00b7fen", "soll", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "M\u00f6cht' ich mein Herz nur leer von mir und allem sehen,", "tokens": ["M\u00f6cht'", "ich", "mein", "Herz", "nur", "leer", "von", "mir", "und", "al\u00b7lem", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "NN", "ADV", "ADJD", "APPR", "PPER", "KON", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Bald w\u00fcrd' ich's seliglich erfahren und verstehen!", "tokens": ["Bald", "w\u00fcrd'", "ich's", "se\u00b7lig\u00b7lich", "er\u00b7fah\u00b7ren", "und", "ver\u00b7ste\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "VVINF", "KON", "VVINF", "$."], "me