{"dta.poem.15568": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wer unter Weisen ist nicht von den Ueberweisen,", "tokens": ["Wer", "un\u00b7ter", "Wei\u00b7sen", "ist", "nicht", "von", "den", "Ue\u00b7ber\u00b7wei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "NN", "VAFIN", "PTKNEG", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nur unterweisen will er dich, nicht \u00fcberweisen.", "tokens": ["Nur", "un\u00b7ter\u00b7wei\u00b7sen", "will", "er", "dich", ",", "nicht", "\u00fc\u00b7ber\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVINF", "VMFIN", "PPER", "PRF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Von dem, was \u00fcber dem Bereich der Sinne liegt,", "tokens": ["Von", "dem", ",", "was", "\u00fc\u00b7ber", "dem", "Be\u00b7reich", "der", "Sin\u00b7ne", "liegt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wohin der k\u00fchne Geist auf seinen Schl\u00fcssen fliegt,", "tokens": ["Wo\u00b7hin", "der", "k\u00fch\u00b7ne", "Geist", "auf", "sei\u00b7nen", "Schl\u00fcs\u00b7sen", "fliegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sagt er nur was er meint, sagt er nur was ihm scheint,", "tokens": ["Sagt", "er", "nur", "was", "er", "meint", ",", "sagt", "er", "nur", "was", "ihm", "scheint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PWS", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "ADV", "PWS", "PPER", "VVFIN", "$,"], "meter": "+-+--++-+--+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Wenn er entschieden auch bejahet und verneint.", "tokens": ["Wenn", "er", "ent\u00b7schie\u00b7den", "auch", "be\u00b7ja\u00b7het", "und", "ver\u00b7neint", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "ADV", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Sagt er auch nicht dazu: so mein' ich und so scheint es;", "tokens": ["Sagt", "er", "auch", "nicht", "da\u00b7zu", ":", "so", "mein'", "ich", "und", "so", "scheint", "es", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "PTKNEG", "PAV", "$.", "ADV", "VVFIN", "PPER", "KON", "ADV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von selbst versteht es sich: es scheint ihm und er meint es.", "tokens": ["Von", "selbst", "ver\u00b7steht", "es", "sich", ":", "es", "scheint", "ihm", "und", "er", "meint", "es", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "PPER", "PRF", "$.", "PPER", "VVFIN", "PPER", "KON", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Nimm davon an, was sich mag deinem Sinn vereinen,", "tokens": ["Nimm", "da\u00b7von", "an", ",", "was", "sich", "mag", "dei\u00b7nem", "Sinn", "ver\u00b7ei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "PTKVZ", "$,", "PRELS", "PRF", "VMFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und hab' im Uebrigen dein Scheinen selbst und Meinen.", "tokens": ["Und", "hab'", "im", "Ueb\u00b7ri\u00b7gen", "dein", "Schei\u00b7nen", "selbst", "und", "Mei\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "APPRART", "NN", "PPOSAT", "NN", "ADV", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}