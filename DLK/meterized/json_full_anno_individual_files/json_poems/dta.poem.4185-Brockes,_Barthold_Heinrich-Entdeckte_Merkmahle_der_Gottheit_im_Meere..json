{"dta.poem.4185": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entdeckte Merkmahle der Gottheit  \n im Meere.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Indem ich j\u00fcngst, bey starkem Sturm, allein am Meeres-", "tokens": ["In\u00b7dem", "ich", "j\u00fcngst", ",", "bey", "star\u00b7kem", "Sturm", ",", "al\u00b7lein", "am", "Mee\u00b7res"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "$,", "APPR", "ADJA", "NN", "$,", "ADV", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Strande stand,", "tokens": ["Stran\u00b7de", "stand", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Betrachtet\u2019 ich die rege Fl\u00e4che. Ich sah die Brandungen", "tokens": ["Be\u00b7trach\u00b7tet'", "ich", "die", "re\u00b7ge", "Fl\u00e4\u00b7che", ".", "Ich", "sah", "die", "Bran\u00b7dun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ART", "NN"], "meter": "-+--+--+--+--+-", "measure": "amphibrach.penta.plus"}, "line.4": {"text": "der Wellen", "tokens": ["der", "Wel\u00b7len"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Aus ihren Tiefen sich erh\u00f6hn, sich b\u00e4umen, wallen, brausen,", "tokens": ["Aus", "ih\u00b7ren", "Tie\u00b7fen", "sich", "er\u00b7h\u00f6hn", ",", "sich", "b\u00e4u\u00b7men", ",", "wal\u00b7len", ",", "brau\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PRF", "VVINF", "$,", "PRF", "VVINF", "$,", "VVINF", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "schwellen,", "tokens": ["schwel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Mit einem knirschenden Ger\u00e4usch, ja recht, mit einem heisern", "tokens": ["Mit", "ei\u00b7nem", "knir\u00b7schen\u00b7den", "Ge\u00b7r\u00e4usch", ",", "ja", "recht", ",", "mit", "ei\u00b7nem", "hei\u00b7sern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "ADV", "ADJD", "$,", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "Bellen,", "tokens": ["Bel\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Das Ufer zu verschlingen drohn, und auf den widersteh\u2019nden", "tokens": ["Das", "U\u00b7fer", "zu", "ver\u00b7schlin\u00b7gen", "drohn", ",", "und", "auf", "den", "wi\u00b7der\u00b7steh'n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,", "KON", "APPR", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Strand", "tokens": ["Strand"], "token_info": ["word"], "pos": ["NN"], "meter": "+", "measure": "single.up"}, "line.11": {"text": "Besch\u00e4umt von oben abwerts st\u00fcrzen, bald aber wieder", "tokens": ["Be\u00b7sch\u00e4umt", "von", "o\u00b7ben", "ab\u00b7werts", "st\u00fcr\u00b7zen", ",", "bald", "a\u00b7ber", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "APPR", "ADV", "ADV", "VVINF", "$,", "ADV", "ADV", "ADV"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "r\u00fcckwerts fliehn,", "tokens": ["r\u00fcck\u00b7werts", "fliehn", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.13": {"text": "Und, von der Tief\u2019 als eingeschlurft, oft selbst die Steine", "tokens": ["Und", ",", "von", "der", "Tie\u00b7f'", "als", "ein\u00b7ge\u00b7schlurft", ",", "oft", "selbst", "die", "Stei\u00b7ne"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "$,", "APPR", "ART", "NN", "KOKOM", "VVFIN", "$,", "ADV", "ADV", "ART", "NN"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "mit sich ziehn,", "tokens": ["mit", "sich", "ziehn", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PRF", "VVINF", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Den pl\u00f6tzlich \u00fcberschwemmten Sand entbl\u00f6\u00dft, ver\u00f6det,", "tokens": ["Den", "pl\u00f6tz\u00b7lich", "\u00fc\u00b7bersc\u00b7hwemm\u00b7ten", "Sand", "ent\u00b7bl\u00f6\u00dft", ",", "ver\u00b7\u00f6\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJD", "ADJA", "NN", "VVFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "schnell verlassen,", "tokens": ["schnell", "ver\u00b7las\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Bald aber, wie sie ihn aufs neu bedeckten, \u00fcberst\u00fcrzten,", "tokens": ["Bald", "a\u00b7ber", ",", "wie", "sie", "ihn", "aufs", "neu", "be\u00b7deck\u00b7ten", ",", "\u00fc\u00b7bers\u00b7t\u00fcrz\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PWAV", "PPER", "PPER", "APPRART", "ADJD", "ADJA", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.18": {"text": "frassen.", "tokens": ["fras\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Es schwamm, mit diesen regen Bergen, mein oft mit-", "tokens": ["Es", "schwamm", ",", "mit", "die\u00b7sen", "re\u00b7gen", "Ber\u00b7gen", ",", "mein", "oft", "mit"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PDAT", "ADJA", "NN", "$,", "PPOSAT", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "weggeri\u00dfner Blick,", "tokens": ["weg\u00b7ge\u00b7ri\u00df\u00b7ner", "Blick", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Vom Ufer ab und in die Tiefe, bald pl\u00f6tzlich wiederum", "tokens": ["Vom", "U\u00b7fer", "ab", "und", "in", "die", "Tie\u00b7fe", ",", "bald", "pl\u00f6tz\u00b7lich", "wie\u00b7de\u00b7rum"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "PTKVZ", "KON", "APPR", "ART", "NN", "$,", "ADV", "ADJD", "ADV"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.4": {"text": "zur\u00fcck.", "tokens": ["zu\u00b7r\u00fcck", "."], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}, "line.5": {"text": "Nachdem ich nun auf diesen Ufern, der wilden Fluht ver-", "tokens": ["Nach\u00b7dem", "ich", "nun", "auf", "die\u00b7sen", "U\u00b7fern", ",", "der", "wil\u00b7den", "Fluht", "ver"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PDAT", "NN", "$,", "ART", "ADJA", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "ha\u00dften Banden,", "tokens": ["ha\u00df\u00b7ten", "Ban\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Mit ganz vom Sturm zerzaustem Haar, bespr\u00fctzet, lange", "tokens": ["Mit", "ganz", "vom", "Sturm", "zer\u00b7zaus\u00b7tem", "Haar", ",", "be\u00b7spr\u00fct\u00b7zet", ",", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word"], "pos": ["APPR", "ADV", "APPRART", "NN", "ADJA", "NN", "$,", "VVFIN", "$,", "ADV"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "still gestanden,", "tokens": ["still", "ge\u00b7stan\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Und das entsetzliche Gew\u00fchl der aufgebrachten Wasser-", "tokens": ["Und", "das", "ent\u00b7setz\u00b7li\u00b7che", "Ge\u00b7w\u00fchl", "der", "auf\u00b7ge\u00b7brach\u00b7ten", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.10": {"text": "Welt,", "tokens": ["Welt", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}}, "stanza.3": {"line.1": {"text": "(die fl\u00fcchtgen Berge steiler Wellen, der wei\u00df-besch\u00e4umten", "tokens": ["(", "die", "fl\u00fccht\u00b7gen", "Ber\u00b7ge", "stei\u00b7ler", "Wel\u00b7len", ",", "der", "wei\u00df\u00b7be\u00b7sch\u00e4um\u00b7ten"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ART", "ADJA", "NN", "ADJA", "NN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Wogen Wanken,)", "tokens": ["Wo\u00b7gen", "Wan\u00b7ken", ",", ")"], "token_info": ["word", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "$("], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "Mir, durch das schwindlende Gesicht; den gleichfalls schwin-", "tokens": ["Mir", ",", "durch", "das", "schwind\u00b7len\u00b7de", "Ge\u00b7sicht", ";", "den", "gleich\u00b7falls", "schwin"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "APPR", "ART", "ADJA", "NN", "$.", "ART", "ADV", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "delnden Gedanken", "tokens": ["deln\u00b7den", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Hingegen, die verborgne Tiefen, und dunkle Schl\u00fcnde", "tokens": ["Hin\u00b7ge\u00b7gen", ",", "die", "ver\u00b7borg\u00b7ne", "Tie\u00b7fen", ",", "und", "dunk\u00b7le", "Schl\u00fcn\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN"], "meter": "+--+-+-+--+-+-", "measure": "iambic.hexa.invert"}, "line.6": {"text": "vorgestellt:", "tokens": ["vor\u00b7ge\u00b7stellt", ":"], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Entsetzte sich mein ganzes Wesen, des weiten Meeres Fl\u00e4ch\u2019", "tokens": ["Ent\u00b7setz\u00b7te", "sich", "mein", "gan\u00b7zes", "We\u00b7sen", ",", "des", "wei\u00b7ten", "Mee\u00b7res", "Fl\u00e4ch'"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "PPOSAT", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "und Schoo\u00df", "tokens": ["und", "Schoo\u00df"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "War, meinem \u00fcberwognen Sinn, zu weit, zu f\u00fcrchterlich,", "tokens": ["War", ",", "mei\u00b7nem", "\u00fc\u00b7ber\u00b7wog\u00b7nen", "Sinn", ",", "zu", "weit", ",", "zu", "f\u00fcrch\u00b7ter\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "$,", "PPOSAT", "ADJA", "NN", "$,", "PTKA", "ADJD", "$,", "PTKA", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.10": {"text": "zu gro\u00df.", "tokens": ["zu", "gro\u00df", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKA", "ADJD", "$."], "meter": "-+", "measure": "iambic.single"}, "line.11": {"text": "Doch da die ungeheure Last der Wasser meine Seele beugte,", "tokens": ["Doch", "da", "die", "un\u00b7ge\u00b7heu\u00b7re", "Last", "der", "Was\u00b7ser", "mei\u00b7ne", "See\u00b7le", "beug\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.12": {"text": "Erh\u00f6hete sie mich zugleich, indem sie mir den wahren Grund,", "tokens": ["Er\u00b7h\u00f6\u00b7he\u00b7te", "sie", "mich", "zu\u00b7gleich", ",", "in\u00b7dem", "sie", "mir", "den", "wah\u00b7ren", "Grund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$,", "KOUS", "PPER", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-+", "measure": "iambic.octa.plus"}, "line.13": {"text": "Den ew\u2019gen Urquell aller Fluhten, den Born, woraus das", "tokens": ["Den", "ew'\u00b7gen", "Ur\u00b7quell", "al\u00b7ler", "Fluh\u00b7ten", ",", "den", "Born", ",", "wo\u00b7raus", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$,", "ART", "NE", "$,", "PWAV", "ART"], "meter": "-+-+-+-+--+--+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Meer entstund,", "tokens": ["Meer", "ent\u00b7stund", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.15": {"text": "Der es im Schlauch gefa\u00dft, regiert und lenkt, die wahre", "tokens": ["Der", "es", "im", "Schlauch", "ge\u00b7fa\u00dft", ",", "re\u00b7giert", "und", "lenkt", ",", "die", "wah\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "APPRART", "NN", "VVPP", "$,", "VVFIN", "KON", "VVFIN", "$,", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Gottheit zeigte,", "tokens": ["Got\u00b7theit", "zeig\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Wobey doch mein erstaunter Geist nichts, als nur dieses,", "tokens": ["Wo\u00b7bey", "doch", "mein", "er\u00b7staun\u00b7ter", "Geist", "nichts", ",", "als", "nur", "die\u00b7ses", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PPOSAT", "ADJA", "NN", "PIS", "$,", "KOUS", "ADV", "PDAT", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "seufzen kunt':", "tokens": ["seuf\u00b7zen", "kunt'", ":"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.19": {"text": "Ew\u2019ger Urstand aller Dinge! HErr! vor welchem", "tokens": ["Ew'\u00b7ger", "Ur\u00b7stand", "al\u00b7ler", "Din\u00b7ge", "!", "Herr", "!", "vor", "wel\u00b7chem"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADJA", "NN", "PIAT", "NN", "$.", "NN", "$.", "APPR", "PWAT"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.20": {"text": "alle Meere", "tokens": ["al\u00b7le", "Mee\u00b7re"], "token_info": ["word", "word"], "pos": ["PIAT", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.21": {"text": "Wie ein Tropfen in dem Eimer, Dir allein sey Preis", "tokens": ["Wie", "ein", "Trop\u00b7fen", "in", "dem", "Ei\u00b7mer", ",", "Dir", "al\u00b7lein", "sey", "Preis"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART", "NN", "APPR", "ART", "NN", "$,", "PPER", "ADV", "VAFIN", "NN"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.22": {"text": "und Ehre!", "tokens": ["und", "Eh\u00b7re", "!"], "token_info": ["word", "word", "punct"], "pos": ["KON", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}}}}}