{"dta.poem.10914": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die L\u00fcchse.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Welche Schrecken-reiche H\u00f6hle, voller Spalten, voller Kl\u00fcfte,", "tokens": ["Wel\u00b7che", "Schre\u00b7cken\u00b7rei\u00b7che", "H\u00f6h\u00b7le", ",", "vol\u00b7ler", "Spal\u00b7ten", ",", "vol\u00b7ler", "Kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAT", "ADJA", "NN", "$,", "ADJA", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Welche Last gew\u00f6lbtert Felsen, voller tief - und dunklen", "tokens": ["Wel\u00b7che", "Last", "ge\u00b7w\u00f6lb\u00b7tert", "Fel\u00b7sen", ",", "vol\u00b7ler", "tief", "und", "dunk\u00b7len"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAT", "NN", "VVPP", "NN", "$,", "ADJA", "ADJD", "$(", "KON", "ADJA"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.3": {"text": "Welch ein w\u00fcrdigs Nest der Thiere, deren heissen Durst nach", "tokens": ["Welch", "ein", "w\u00fcr\u00b7digs", "Nest", "der", "Thie\u00b7re", ",", "de\u00b7ren", "heis\u00b7sen", "Durst", "nach"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "ART", "ADJA", "NN", "ART", "NN", "$,", "PRELAT", "ADJA", "NN", "APPR"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.4": {"text": "Grimm, Gefr\u00e4\u00dfigkeit und Bosheit, Hurtigkeit und List und", "tokens": ["Grimm", ",", "Ge\u00b7fr\u00e4\u00b7\u00dfig\u00b7keit", "und", "Bos\u00b7heit", ",", "Hur\u00b7tig\u00b7keit", "und", "List", "und"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "KON"], "meter": "+-+-+-+-+-+-+-", "measure": "trochaic.septa"}, "line.5": {"text": "Jhre ganze Stellung weist! Kann man doch, fast ohne Grauen,", "tokens": ["Ih\u00b7re", "gan\u00b7ze", "Stel\u00b7lung", "weist", "!", "Kann", "man", "doch", ",", "fast", "oh\u00b7ne", "Grau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$.", "VMFIN", "PIS", "ADV", "$,", "ADV", "APPR", "NN", "$,"], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.6": {"text": "Kaum die sichere Copie, weil auch sie fast lebet, schauen.", "tokens": ["Kaum", "die", "si\u00b7che\u00b7re", "Co\u00b7pie", ",", "weil", "auch", "sie", "fast", "le\u00b7bet", ",", "schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,", "KOUS", "ADV", "PPER", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.7": {"text": "Seht, wie die, bey ihren Jungen, grimmiglich die Z\u00e4hne bleckt,", "tokens": ["Seht", ",", "wie", "die", ",", "bey", "ih\u00b7ren", "Jun\u00b7gen", ",", "grim\u00b7mig\u00b7lich", "die", "Z\u00e4h\u00b7ne", "bleckt", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "$,", "APPR", "PPOSAT", "NN", "$,", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Wie ein andrer dort bequem seine Tatzen von sich streckt,", "tokens": ["Wie", "ein", "an\u00b7drer", "dort", "be\u00b7quem", "sei\u00b7ne", "Tat\u00b7zen", "von", "sich", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "ADV", "ADJD", "PPOSAT", "NN", "APPR", "PRF", "VVFIN", "$,"], "meter": "--+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Und, auch mit verdeckter Sch\u00e4rfe eingezogner Krallen, schreckt.", "tokens": ["Und", ",", "auch", "mit", "ver\u00b7deck\u00b7ter", "Sch\u00e4r\u00b7fe", "ein\u00b7ge\u00b7zog\u00b7ner", "Kral\u00b7len", ",", "schreckt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "$,", "ADV", "APPR", "ADJA", "NN", "ART", "NN", "$,", "VVFIN", "$."], "meter": "----+-+-+-+-+-+", "measure": "unknown.measure.hexa"}, "line.10": {"text": "Einer sieht sich grimmig um. Selber an der jungen Schaar,", "tokens": ["Ei\u00b7ner", "sieht", "sich", "grim\u00b7mig", "um", ".", "Sel\u00b7ber", "an", "der", "jun\u00b7gen", "Schaar", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "PTKVZ", "$.", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.11": {"text": "Wird man eine junge Bosheit, kleine T\u00fccke schon gewahr.", "tokens": ["Wird", "man", "ei\u00b7ne", "jun\u00b7ge", "Bos\u00b7heit", ",", "klei\u00b7ne", "T\u00fc\u00b7cke", "schon", "ge\u00b7wahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "ART", "ADJA", "NN", "$,", "ADJA", "NN", "ADV", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Seht, wie jener alle Muskeln, zu dem nahen Raub, bestimmt;", "tokens": ["Seht", ",", "wie", "je\u00b7ner", "al\u00b7le", "Mus\u00b7keln", ",", "zu", "dem", "na\u00b7hen", "Raub", ",", "be\u00b7stimmt", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "PDAT", "PIAT", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$,", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Wie, in seinem scharfen Blicke, recht ein wildes Feuer glimmt!", "tokens": ["Wie", ",", "in", "sei\u00b7nem", "schar\u00b7fen", "Bli\u00b7cke", ",", "recht", "ein", "wil\u00b7des", "Feu\u00b7er", "glimmt", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "ADV", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Sehet, wie der gro\u00dfe dort mordbegierge Klauen kr\u00fcmmt.", "tokens": ["Se\u00b7het", ",", "wie", "der", "gro\u00b7\u00dfe", "dort", "mord\u00b7be\u00b7gier\u00b7ge", "Klau\u00b7en", "kr\u00fcmmt", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "ADV", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.15": {"text": "Doch warum, macht etwa jemand mir vielleicht hier einen", "tokens": ["Doch", "wa\u00b7rum", ",", "macht", "et\u00b7wa", "je\u00b7mand", "mir", "viel\u00b7leicht", "hier", "ei\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "$,", "VVFIN", "ADV", "PIS", "PPER", "ADV", "ADV", "ART"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "Zeugt, in L\u00fcchsen, die Natur, gleichsam eingefleischte Teufel?", "tokens": ["Zeugt", ",", "in", "L\u00fcch\u00b7sen", ",", "die", "Na\u00b7tur", ",", "gleich\u00b7sam", "ein\u00b7ge\u00b7fleischte", "Teu\u00b7fel", "?"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "APPR", "NN", "$,", "ART", "NN", "$,", "ADJD", "ADJA", "NN", "$."], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.17": {"text": "Nicht umsonst. Sie dienen uns. Man kann sie zum Jagen n\u00fctzen,", "tokens": ["Nicht", "um\u00b7sonst", ".", "Sie", "die\u00b7nen", "uns", ".", "Man", "kann", "sie", "zum", "Ja\u00b7gen", "n\u00fct\u00b7zen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "$.", "PPER", "VVFIN", "PPER", "$.", "PIS", "VMFIN", "PPER", "APPRART", "NN", "VVINF", "$,"], "meter": "--+-+--++--+-+-", "measure": "iambic.hexa.relaxed"}, "line.18": {"text": "Und es m\u00fcssen ihre B\u00e4lge uns f\u00fcr scharfe K\u00e4lte sch\u00fctzen.", "tokens": ["Und", "es", "m\u00fcs\u00b7sen", "ih\u00b7re", "B\u00e4l\u00b7ge", "uns", "f\u00fcr", "schar\u00b7fe", "K\u00e4l\u00b7te", "sch\u00fct\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPOSAT", "NN", "PPER", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}}}}}