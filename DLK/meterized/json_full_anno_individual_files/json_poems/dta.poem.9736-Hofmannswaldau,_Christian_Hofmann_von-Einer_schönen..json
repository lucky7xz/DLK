{"dta.poem.9736": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Einer sch\u00f6nen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer liegt die/ welcher nichts an sch\u00f6nheit konte gleichen/", "tokens": ["Hjer", "liegt", "die", "/", "wel\u00b7cher", "nichts", "an", "sch\u00f6n\u00b7heit", "kon\u00b7te", "glei\u00b7chen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "$(", "PRELS", "PIS", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der die vollkommensten an tugend musten weichen/", "tokens": ["Der", "die", "voll\u00b7kom\u00b7men\u00b7sten", "an", "tu\u00b7gend", "mus\u00b7ten", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "APPR", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die alle hertzen zwang durch anmuths-volle blicke.", "tokens": ["Die", "al\u00b7le", "hert\u00b7zen", "zwang", "durch", "an\u00b7muths\u00b7vol\u00b7le", "bli\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der tod/ der nahm zu bald ihr und der welt dis gl\u00fccke.", "tokens": ["Der", "tod", "/", "der", "nahm", "zu", "bald", "ihr", "und", "der", "welt", "dis", "gl\u00fc\u00b7cke", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "VVFIN", "APPR", "ADV", "PPER", "KON", "ART", "NN", "PDS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}