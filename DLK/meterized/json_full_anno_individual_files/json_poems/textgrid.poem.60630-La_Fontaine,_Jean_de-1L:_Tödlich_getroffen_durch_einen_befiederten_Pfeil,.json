{"textgrid.poem.60630": {"metadata": {"author": {"name": "La Fontaine, Jean de", "birth": "N.A.", "death": "N.A."}, "title": "1L: T\u00f6dlich getroffen durch einen befiederten Pfeil,", "genre": "verse", "period": "N.A.", "pub_year": 1658, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "T\u00f6dlich getroffen durch einen befiederten Pfeil,", "tokens": ["T\u00f6d\u00b7lich", "ge\u00b7trof\u00b7fen", "durch", "ei\u00b7nen", "be\u00b7fie\u00b7der\u00b7ten", "Pfeil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVPP", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.2": {"text": "Klagte ein Vogel in blutendem Schmerz sein Geschick:", "tokens": ["Klag\u00b7te", "ein", "Vo\u00b7gel", "in", "blu\u00b7ten\u00b7dem", "Schmerz", "sein", "Ge\u00b7schick", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ADJA", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.3": {"text": "\u00bbmu\u00df man zum eignen Verderben beisteuern sein Teil!", "tokens": ["\u00bb", "mu\u00df", "man", "zum", "eig\u00b7nen", "Ver\u00b7der\u00b7ben", "bei\u00b7steu\u00b7ern", "sein", "Teil", "!"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "VMFIN", "PIS", "APPRART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "+--+--+--+--+", "measure": "dactylic.penta"}, "line.4": {"text": "Nimmt doch der Mensch aus unserm Gefieder das St\u00fcck,", "tokens": ["Nimmt", "doch", "der", "Mensch", "aus", "un\u00b7serm", "Ge\u00b7fie\u00b7der", "das", "St\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-