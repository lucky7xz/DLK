{"textgrid.poem.54177": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "Die Herren Eltern", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ist ein Schullehrer Pazifist", "tokens": ["Ist", "ein", "Schul\u00b7leh\u00b7rer", "Pa\u00b7zi\u00b7fist"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sagt, wie es in Wahrheit im Kriege ist \u2013:", "tokens": ["und", "sagt", ",", "wie", "es", "in", "Wahr\u00b7heit", "im", "Krie\u00b7ge", "ist", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "PWAV", "PPER", "APPR", "NN", "APPRART", "NN", "VAFIN", "$(", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "da\u00df Generale Kriegsinteressenten sind,", "tokens": ["da\u00df", "Ge\u00b7ne\u00b7ra\u00b7le", "Kriegs\u00b7in\u00b7ter\u00b7es\u00b7sen\u00b7ten", "sind", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "VAFIN", "$,"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.4": {"text": "ganz gleich, wer verliert; ganz gleich, wer gewinnt . . .", "tokens": ["ganz", "gleich", ",", "wer", "ver\u00b7liert", ";", "ganz", "gleich", ",", "wer", "ge\u00b7winnt", ".", ".", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct", "punct"], "pos": ["ADV", "ADV", "$,", "PWS", "VVFIN", "$.", "ADV", "ADV", "$,", "PWS", "VVFIN", "$.", "$.", "$."], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "dann \u2013 sollte man meinen \u2013 freun sich die Eltern f\u00fcr ihr Kind?", "tokens": ["dann", "\u2013", "soll\u00b7te", "man", "mei\u00b7nen", "\u2013", "freun", "sich", "die", "El\u00b7tern", "f\u00fcr", "ihr", "Kind", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$(", "VMFIN", "PIS", "PPOSAT", "$(", "VVFIN", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Jawoll!", "tokens": ["Ja\u00b7woll", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Dann erhebt sich ein ungeheures Elterngeschrei:", "tokens": ["Dann", "er\u00b7hebt", "sich", "ein", "un\u00b7ge\u00b7heu\u00b7res", "El\u00b7tern\u00b7ge\u00b7schrei", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PRF", "ART", "ADJA", "NN", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "\u00bbraus mit dem Kerl! Das ist Giftmischerei!", "tokens": ["\u00bb", "raus", "mit", "dem", "Kerl", "!", "Das", "ist", "Gift\u00b7mi\u00b7sche\u00b7rei", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "APPR", "ART", "NN", "$.", "PDS", "VAFIN", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Unser Junge soll lernen, wie sch\u00f6n die Kriege sind!", "tokens": ["Un\u00b7ser", "Jun\u00b7ge", "soll", "ler\u00b7nen", ",", "wie", "sch\u00f6n", "die", "Krie\u00b7ge", "sind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "VVINF", "$,", "PWAV", "ADJD", "ART", "NN", "VAFIN", "$."], "meter": "+-+--+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Wir warten schon drauf, wann wieder ein neuer beginnt \u2013", "tokens": ["Wir", "war\u00b7ten", "schon", "drauf", ",", "wann", "wie\u00b7der", "ein", "neu\u00b7er", "be\u00b7ginnt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PTKVZ", "$,", "PWAV", "ADV", "ART", "ADJA", "VVFIN", "$("], "meter": "-+--+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "und dazu liefern wir gratis und franko 1 Kind!", "tokens": ["und", "da\u00b7zu", "lie\u00b7fern", "wir", "gra\u00b7tis", "und", "fran\u00b7ko", "1", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "number", "word", "punct"], "pos": ["KON", "PAV", "VVFIN", "PPER", "ADJD", "KON", "ADV", "CARD", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jawoll!\u00ab", "tokens": ["Ja\u00b7woll", "!", "\u00ab"], "token_info": ["word", "punct", "punct"], "pos": ["ADJD", "$.", "$("], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Die Elternbegeisterung ist ganz enorm.", "tokens": ["Die", "El\u00b7tern\u00b7be\u00b7geis\u00b7te\u00b7rung", "ist", "ganz", "en\u00b7orm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die M\u00fctter: aus Liebe zur Uniform.", "tokens": ["Die", "M\u00fct\u00b7ter", ":", "aus", "Lie\u00b7be", "zur", "U\u00b7ni\u00b7form", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "APPR", "NN", "APPRART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.3": {"text": "Die V\u00e4ter, die Lieferanten f\u00fcr den Sch\u00fctzengraben,", "tokens": ["Die", "V\u00e4\u00b7ter", ",", "die", "Lie\u00b7fe\u00b7ran\u00b7ten", "f\u00fcr", "den", "Sch\u00fct\u00b7zen\u00b7gra\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "denken: warum sollen denn diese Knaben", "tokens": ["den\u00b7ken", ":", "wa\u00b7rum", "sol\u00b7len", "denn", "die\u00b7se", "Kna\u00b7ben"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVINF", "$.", "PWAV", "VMFIN", "ADV", "PDAT", "NN"], "meter": "+---+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.5": {"text": "es besser als unsereiner haben?", "tokens": ["es", "bes\u00b7ser", "als", "un\u00b7ser\u00b7ei\u00b7ner", "ha\u00b7ben", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KOKOM", "PPOSAT", "VAINF", "$."], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Nicht wahr?", "tokens": ["Nicht", "wahr", "?"], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.4": {"line.1": {"text": "Die Fabrikation eines Kindes ist nicht sehr teuer.", "tokens": ["Die", "Fab\u00b7ri\u00b7ka\u00b7ti\u00b7on", "ei\u00b7nes", "Kin\u00b7des", "ist", "nicht", "sehr", "teu\u00b7er", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+--+-+-++-+-", "measure": "iambic.septa.relaxed"}}, "stanza.5": {"line.1": {"text": "Aber erh\u00f6h mal ein bi\u00dfchen die Umsatzsteuer \u2013:", "tokens": ["A\u00b7ber", "er\u00b7h\u00f6h", "mal", "ein", "bi\u00df\u00b7chen", "die", "Um\u00b7satz\u00b7steu\u00b7er", "\u2013", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "PIS", "ART", "NN", "$(", "$."], "meter": "+--+-++--+-+-", "measure": "iambic.hexa.invert"}, "line.2": {"text": "dann kreischen die Herren Eltern, da\u00df der Ziegel vom Dache f\u00e4llt.", "tokens": ["dann", "krei\u00b7schen", "die", "Her\u00b7ren", "El\u00b7tern", ",", "da\u00df", "der", "Zie\u00b7gel", "vom", "Da\u00b7che", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "NN", "$,", "KOUS", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+--+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Man trennt sich leicht vom Kind.", "tokens": ["Man", "trennt", "sich", "leicht", "vom", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PRF", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Aber schwer vom Geld.", "tokens": ["A\u00b7ber", "schwer", "vom", "Geld", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.5": {"text": "Bekommt das Kind einen Bauchschu\u00df? Das macht ihnen keine Schmerzen.", "tokens": ["Be\u00b7kommt", "das", "Kind", "ei\u00b7nen", "Bauch\u00b7schu\u00df", "?", "Das", "macht", "ih\u00b7nen", "kei\u00b7ne", "Schmer\u00b7zen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$.", "PDS", "VVFIN", "PPER", "PIAT", "NN", "$."], "meter": "-+-+--+--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Doch ihr Geld \u2013 das lieben die Herren Eltern von Herzen.", "tokens": ["Doch", "ihr", "Geld", "\u2013", "das", "lie\u00b7ben", "die", "Her\u00b7ren", "El\u00b7tern", "von", "Her\u00b7zen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "$(", "PDS", "VVFIN", "ART", "NN", "NN", "APPR", "NN", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Jawoll!", "tokens": ["Ja\u00b7woll", "!"], "token_info": ["word", "punct"], "pos": ["ADJD", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.6": {"line.1": {"text": "Mitleid mit den Opfern, die da fallen f\u00fcr Petroleum, f\u00fcr Fahnen,", "tokens": ["Mit\u00b7leid", "mit", "den", "Op\u00b7fern", ",", "die", "da", "fal\u00b7len", "f\u00fcr", "Pet\u00b7ro\u00b7le\u00b7um", ",", "f\u00fcr", "Fah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "PRELS", "ADV", "VVFIN", "APPR", "NE", "$,", "APPR", "NN", "$,"], "meter": "-+--+-+-+-+-+-+-+-", "measure": "iambic.octa.plus.relaxed"}, "line.2": {"text": "f\u00fcr Gold \u2013?", "tokens": ["f\u00fcr", "Gold", "\u2013", "?"], "token_info": ["word", "word", "punct", "punct"], "pos": ["APPR", "NN", "$(", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Die Herren Eltern haben es so gewollt.", "tokens": ["Die", "Her\u00b7ren", "El\u00b7tern", "ha\u00b7ben", "es", "so", "ge\u00b7wollt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "PPER", "ADV", "VMPP", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.7": {"line.1": {"text": "Ist ein Schullehrer Pazifist", "tokens": ["Ist", "ein", "Schul\u00b7leh\u00b7rer", "Pa\u00b7zi\u00b7fist"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "und sagt, wie es in Wahrheit im Kriege ist \u2013:", "tokens": ["und", "sagt", ",", "wie", "es", "in", "Wahr\u00b7heit", "im", "Krie\u00b7ge", "ist", "\u2013", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "$,", "P