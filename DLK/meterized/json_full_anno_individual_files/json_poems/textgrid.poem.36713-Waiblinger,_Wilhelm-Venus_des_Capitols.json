{"textgrid.poem.36713": {"metadata": {"author": {"name": "Waiblinger, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Venus des Capitols", "genre": "verse", "period": "N.A.", "pub_year": 1817, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "G\u00f6tter steigen herab in menschliche H\u00fclle sich bergend,", "tokens": ["G\u00f6t\u00b7ter", "stei\u00b7gen", "her\u00b7ab", "in", "menschli\u00b7che", "H\u00fcl\u00b7le", "sich", "ber\u00b7gend", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ADV", "APPR", "ADJA", "NN", "PRF", "VVPP", "$,"], "meter": "+-+--+-+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Und dem Sterblichen mischt gern sich das Himmlische bei.", "tokens": ["Und", "dem", "Sterb\u00b7li\u00b7chen", "mischt", "gern", "sich", "das", "Himm\u00b7li\u00b7sche", "bei", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "PRF", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Sinnlicher F\u00fclle hast du, uranische geistige Sch\u00f6nheit,", "tokens": ["Sinn\u00b7li\u00b7cher", "F\u00fcl\u00b7le", "hast", "du", ",", "u\u00b7ra\u00b7ni\u00b7sche", "geis\u00b7ti\u00b7ge", "Sch\u00f6n\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VAFIN", "PPER", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-+--+-", "measure": "iambic.septa.invert"}, "line.4": {"text": "All' dein Wesen und Sein, all' dein Geheimni\u00df vertraut.", "tokens": ["All'", "dein", "We\u00b7sen", "und", "Sein", ",", "all'", "dein", "Ge\u00b7heim\u00b7ni\u00df", "ver\u00b7traut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": [