{"textgrid.poem.58757": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "5. Einig", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du, unser Gott, bist Gott allein,", "tokens": ["Du", ",", "un\u00b7ser", "Gott", ",", "bist", "Gott", "al\u00b7lein", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VAFIN", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und neben dir kann keiner sein,", "tokens": ["Und", "ne\u00b7ben", "dir", "kann", "kei\u00b7ner", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "VMFIN", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In dir beruhst du, hast's in Einem.", "tokens": ["In", "dir", "be\u00b7ruhst", "du", ",", "hast's", "in", "Ei\u00b7nem", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVFIN", "PPER", "$,", "VAFIN", "APPR", "PIS", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nur Eins ist not! rufst du mir zu,", "tokens": ["Nur", "Eins", "ist", "not", "!", "rufst", "du", "mir", "zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VAFIN", "ADJD", "$.", "VVFIN", "PPER", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der G\u00f6tzen Meng' bringt keine Ruh;", "tokens": ["Der", "G\u00f6t\u00b7zen", "Meng'", "bringt", "kei\u00b7ne", "Ruh", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VVFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "meas