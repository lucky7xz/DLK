{"dta.poem.9296": {"metadata": {"author": {"name": "Weise, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Das f\u00fcnffte Gespr\u00e4ch.", "genre": "Lyrik; Drama; Prosa", "period": "N.A.", "pub_year": "1701", "urn": "urn:nbn:de:kobv:b4-25043-0", "language": ["de:0.99"], "booktitle": "Weise, Christian: \u00dcberfl\u00fc\u00dfige Gedancken Der gr\u00fcnenden jugend. Leipzig, 1701."}, "poem": {"stanza.1": {"line.1": {"text": "Was will doch nun der himmel aus mir machen?", "tokens": ["Was", "will", "doch", "nun", "der", "him\u00b7mel", "aus", "mir", "ma\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein gl\u00fccke schl\u00e4fft und will noch nicht erwachen/", "tokens": ["Mein", "gl\u00fc\u00b7cke", "schl\u00e4fft", "und", "will", "noch", "nicht", "er\u00b7wa\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VMFIN", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mein leben stirbt und lebt nicht wo ich bin/", "tokens": ["Mein", "le\u00b7ben", "stirbt", "und", "lebt", "nicht", "wo", "ich", "bin", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "VVINF", "VVFIN", "KON", "VVFIN", "PTKNEG", "PWAV", "PPER", "VAFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Mein hertz vergeht und kan sich selbst nicht finden/", "tokens": ["Mein", "hertz", "ver\u00b7geht", "und", "kan", "sich", "selbst", "nicht", "fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "KON", "VMFIN", "PRF", "ADV", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und aller trost den ich hier soll empfinden/", "tokens": ["Und", "al\u00b7ler", "trost", "den", "ich", "hier", "soll", "emp\u00b7fin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "ART", "PPER", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Der ist dahin.", "tokens": ["Der", "ist", "da\u00b7hin", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PAV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "2. Es hilfft mich nicht/ da\u00df ich die sch\u00f6nste liebe/", "tokens": ["Es", "hilfft", "mich", "nicht", "/", "da\u00df", "ich", "die", "sch\u00f6ns\u00b7te", "lie\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "$(", "KOUS", "PPER", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wenn ich mich nur bi\u00df auf den tod betr\u00fcbe/", "tokens": ["Wenn", "ich", "mich", "nur", "bi\u00df", "auf", "den", "tod", "be\u00b7tr\u00fc\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich lasse mich zwar in gedancken ein/", "tokens": ["Ich", "las\u00b7se", "mich", "zwar", "in", "ge\u00b7dan\u00b7cken", "ein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "APPR", "NN", "ART", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und ehre sie mit heimlichen geberden/", "tokens": ["Und", "eh\u00b7re", "sie", "mit", "heim\u00b7li\u00b7chen", "ge\u00b7ber\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Doch kan ich nicht so sehr begnadigt werden/", "tokens": ["Doch", "kan", "ich", "nicht", "so", "sehr", "be\u00b7gna\u00b7digt", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADV", "ADV", "VVPP", "VAINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bey ihr zu seyn.", "tokens": ["Bey", "ihr", "zu", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKZU", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "3. Sol ich mein leid den stummen w\u00e4nden klagen/", "tokens": ["Sol", "ich", "mein", "leid", "den", "stum\u00b7men", "w\u00e4n\u00b7den", "kla\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PPOSAT", "ADJD", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Weil sie nicht wil nach meinen schmertzen fragen;", "tokens": ["Weil", "sie", "nicht", "wil", "nach", "mei\u00b7nen", "schmert\u00b7zen", "fra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sol ich verziehn in meiner Einsamkeit?", "tokens": ["Sol", "ich", "ver\u00b7ziehn", "in", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hier mag ich nicht; dort kan ich mich nicht laben", "tokens": ["Hier", "mag", "ich", "nicht", ";", "dort", "kan", "ich", "mich", "nicht", "la\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "PTKNEG", "$.", "ADV", "VMFIN", "PPER", "PRF", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Hier bin ich todt; und jene lebens-gaben", "tokens": ["Hier", "bin", "ich", "todt", ";", "und", "je\u00b7ne", "le\u00b7bens\u00b7ga\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "$.", "KON", "PDS", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Sind mir zu weit.", "tokens": ["Sind", "mir", "zu", "weit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKA", "ADJD", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "4. Erbarme dich du sonne meiner seelen/", "tokens": ["Er\u00b7bar\u00b7me", "dich", "du", "son\u00b7ne", "mei\u00b7ner", "see\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PPER", "VVFIN", "PPOSAT", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Was wilstu mich in meiner unschuld qv\u00e4len?", "tokens": ["Was", "wils\u00b7tu", "mich", "in", "mei\u00b7ner", "un\u00b7schuld", "qv\u00e4\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PRF", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich bin verliebt/ ist das die missethat?", "tokens": ["Ich", "bin", "ver\u00b7liebt", "/", "ist", "das", "die", "mis\u00b7se\u00b7that", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "$(", "VAFIN", "ART", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "So dencke nach und straffe deine wangen/", "tokens": ["So", "den\u00b7cke", "nach", "und", "straf\u00b7fe", "dei\u00b7ne", "wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "KON", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Dieweil ihr Glantz mein hoffen und verlangen", "tokens": ["Die\u00b7weil", "ihr", "Glantz", "mein", "hof\u00b7fen", "und", "ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "PPOSAT", "VVINF", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bezwungen hat.", "tokens": ["Be\u00b7zwun\u00b7gen", "hat", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VAFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "5. Kan ich daf\u00fcr? indem mir unter allen", "tokens": ["Kan", "ich", "da\u00b7f\u00fcr", "?", "in\u00b7dem", "mir", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "PAV", "$.", "KOUS", "PPER", "APPR", "PIAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die tugenden bey dir so wohl gefallen?", "tokens": ["Die", "tu\u00b7gen\u00b7den", "bey", "dir", "so", "wohl", "ge\u00b7fal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "ADV", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dein licht ist sch\u00f6n/ dein ruhm ist lobens wehrt:", "tokens": ["Dein", "licht", "ist", "sch\u00f6n", "/", "dein", "ruhm", "ist", "lo\u00b7bens", "wehrt", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$(", "PPOSAT", "NN", "VAFIN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Kein hertz kan hier bey deiner glut erkalten:", "tokens": ["Kein", "hertz", "kan", "hier", "bey", "dei\u00b7ner", "glut", "er\u00b7kal\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Drum wirstu auch die lieb entschuldigt halten", "tokens": ["Drum", "wirs\u00b7tu", "auch", "die", "lieb", "ent\u00b7schul\u00b7digt", "hal\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VAFIN", "ADV", "ART", "ADJD", "VVPP", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die dich begehrt.", "tokens": ["Die", "dich", "be\u00b7gehrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "6. Wilstu mich auch durch diese noth probieren/", "tokens": ["Wils\u00b7tu", "mich", "auch", "durch", "die\u00b7se", "noth", "pro\u00b7bie\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "PDAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00df mich nur die hoffnung nicht verlieren.", "tokens": ["So", "la\u00df", "mich", "nur", "die", "hoff\u00b7nung", "nicht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch gieb mir itz aus gnaden und zur lust/", "tokens": ["Doch", "gieb", "mir", "itz", "aus", "gna\u00b7den", "und", "zur", "lust", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "APPR", "ADJA", "KON", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Was du mir sonst nach abgelegten proben/", "tokens": ["Was", "du", "mir", "sonst", "nach", "ab\u00b7ge\u00b7leg\u00b7ten", "pro\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Au\u00df billigkeit geb\u00fchrlich angeloben", "tokens": ["Au\u00df", "bil\u00b7lig\u00b7keit", "ge\u00b7b\u00fchr\u00b7lich", "an\u00b7ge\u00b7lo\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "ADJD", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und leisten must.", "tokens": ["Und", "leis\u00b7ten", "must", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVINF", "VMFIN", "$."], "meter": "-+-+", "measure": "iambic.di"}}}}}