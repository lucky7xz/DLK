ambic.tri"}, "line.2": {"text": "Da wir am Feuer steh'n", "tokens": ["Da", "wir", "am", "Feu\u00b7er", "steh'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem hei\u00dfen Brodem", "tokens": ["Und", "in", "dem", "hei\u00b7\u00dfen", "Bro\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Langsam zu Grunde geh'n.\u00ab", "tokens": ["Lang\u00b7sam", "zu", "Grun\u00b7de", "geh'", "n.", "\u00ab"], "token_info": ["word", "word", "word", "word", "abbreviation", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "NE", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.17": {"line.1": {"text": "\u00bbder soll es noch erfahren,", "tokens": ["\u00bb", "der", "soll", "es", "noch", "er\u00b7fah\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie es dem Menschen thut,", "tokens": ["Wie", "es", "dem", "Men\u00b7schen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df er das Athmen zahlen", "tokens": ["Mu\u00df", "er", "das", "Ath\u00b7men", "zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem Schwei\u00df und Blut!\u00ab \u2013", "tokens": ["Mit", "sei\u00b7nem", "Schwei\u00df", "und", "Blut", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Verziehen sei dir Alles,", "tokens": ["Ver\u00b7zie\u00b7hen", "sei", "dir", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Womit du schwer mich kr\u00e4nkst,", "tokens": ["Wo\u00b7mit", "du", "schwer", "mich", "kr\u00e4nkst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verziehen sei dir's gerne:", "tokens": ["Ver\u00b7zie\u00b7hen", "sei", "dir's", "ger\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du wei\u00dft nicht, was du denkst.", "tokens": ["Du", "wei\u00dft", "nicht", ",", "was", "du", "denkst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Du hast ja nie erfahren", "tokens": ["Du", "hast", "ja", "nie", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Geistes tiefe M\u00fch'n,", "tokens": ["Des", "Geis\u00b7tes", "tie\u00b7fe", "M\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ahnst nicht, wie die Schl\u00e4fen", "tokens": ["Und", "ahnst", "nicht", ",", "wie", "die", "Schl\u00e4\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir hei\u00df vom Denken gl\u00fch'n;", "tokens": ["Mir", "hei\u00df", "vom", "Den\u00b7ken", "gl\u00fch'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.20": {"line.1": {"text": "Du ahnst nicht, wie ich h\u00e4mm're", "tokens": ["Du", "ahnst", "nicht", ",", "wie", "ich", "h\u00e4m\u00b7m'\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und feile Tag f\u00fcr Tag \u2013", "tokens": ["Und", "fei\u00b7le", "Tag", "f\u00fcr", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie ich mich verblute", "tokens": ["Und", "wie", "ich", "mich", "ver\u00b7blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit jedem Herzensschlag!", "tokens": ["Mit", "je\u00b7dem", "Her\u00b7zens\u00b7schlag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}