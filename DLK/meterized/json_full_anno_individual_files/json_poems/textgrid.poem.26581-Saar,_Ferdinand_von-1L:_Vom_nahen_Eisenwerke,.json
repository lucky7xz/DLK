{"textgrid.poem.26581": {"metadata": {"author": {"name": "Saar, Ferdinand von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Vom nahen Eisenwerke,", "genre": "verse", "period": "N.A.", "pub_year": 1869, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Vom nahen Eisenwerke,", "tokens": ["Vom", "na\u00b7hen", "Ei\u00b7sen\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beru\u00dft, mit schwerem Gang,", "tokens": ["Be\u00b7ru\u00dft", ",", "mit", "schwe\u00b7rem", "Gang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommt mir ein Mann entgegen,", "tokens": ["Kommt", "mir", "ein", "Mann", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Wiesenpfad entlang.", "tokens": ["Den", "Wie\u00b7sen\u00b7pfad", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mit trotzig finst'rer Miene,", "tokens": ["Mit", "trot\u00b7zig", "finst'\u00b7rer", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie mit sich selbst im Streit,", "tokens": ["Wie", "mit", "sich", "selbst", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Greift er an seine M\u00fctze \u2013", "tokens": ["Greift", "er", "an", "sei\u00b7ne", "M\u00fct\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewohnheit alter Zeit.", "tokens": ["Ge\u00b7wohn\u00b7heit", "al\u00b7ter", "Zeit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es blickt dabei sein Auge", "tokens": ["Es", "blickt", "da\u00b7bei", "sein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PAV", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir musternd auf den Rock,", "tokens": ["Mir", "mus\u00b7ternd", "auf", "den", "Rock", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann beim Weiterschreiten", "tokens": ["Und", "dann", "beim", "Wei\u00b7ter\u00b7schrei\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Schwingt er den Knotenstock.", "tokens": ["Schwingt", "er", "den", "Kno\u00b7ten\u00b7stock", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Ich ahne, was im Herzen", "tokens": ["Ich", "ah\u00b7ne", ",", "was", "im", "Her\u00b7zen"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PRELS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und was im Hirn ihm brennt:", "tokens": ["Und", "was", "im", "Hirn", "ihm", "brennt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbdas ist auch Einer,\u00ab denkt er,", "tokens": ["\u00bb", "das", "ist", "auch", "Ei\u00b7ner", ",", "\u00ab", "denkt", "er", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["$(", "PDS", "VAFIN", "ADV", "PIS", "$,", "$(", "VVFIN", "PPER", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "\u00bbder nicht die Arbeit kennt.\u00ab", "tokens": ["\u00bb", "der", "nicht", "die", "Ar\u00b7beit", "kennt", ".", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "ART", "PTKNEG", "ART", "NN", "VVFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bblustwandelnd hier im Freien,", "tokens": ["\u00bb", "lust\u00b7wan\u00b7delnd", "hier", "im", "Frei\u00b7en", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJD", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Verdaut er \u00fcpp'ges Mahl,", "tokens": ["Ver\u00b7daut", "er", "\u00fcpp'\u00b7ges", "Mahl", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Inde\u00df wir darbend schmieden", "tokens": ["In\u00b7de\u00df", "wir", "dar\u00b7bend", "schmie\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVPP", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das Eisen und den Stahl.\u00ab", "tokens": ["Das", "Ei\u00b7sen", "und", "den", "Stahl", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bber sucht den Waldesschatten,", "tokens": ["\u00bb", "er", "sucht", "den", "Wal\u00b7des\u00b7schat\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Da wir am Feuer steh'n", "tokens": ["Da", "wir", "am", "Feu\u00b7er", "steh'n"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVINF"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und in dem hei\u00dfen Brodem", "tokens": ["Und", "in", "dem", "hei\u00b7\u00dfen", "Bro\u00b7dem"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Langsam zu Grunde geh'n.\u00ab", "tokens": ["Lang\u00b7sam", "zu", "Grun\u00b7de", "geh'", "n.", "\u00ab"], "token_info": ["word", "word", "word", "word", "abbreviation", "punct"], "pos": ["ADJD", "APPR", "NN", "VVFIN", "NE", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.7": {"line.1": {"text": "\u00bbder soll es noch erfahren,", "tokens": ["\u00bb", "der", "soll", "es", "noch", "er\u00b7fah\u00b7ren", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie es dem Menschen thut,", "tokens": ["Wie", "es", "dem", "Men\u00b7schen", "thut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Mu\u00df er das Athmen zahlen", "tokens": ["Mu\u00df", "er", "das", "Ath\u00b7men", "zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit seinem Schwei\u00df und Blut!\u00ab \u2013", "tokens": ["Mit", "sei\u00b7nem", "Schwei\u00df", "und", "Blut", "!", "\u00ab", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN", "$.", "$(", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Verziehen sei dir Alles,", "tokens": ["Ver\u00b7zie\u00b7hen", "sei", "dir", "Al\u00b7les", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Womit du schwer mich kr\u00e4nkst,", "tokens": ["Wo\u00b7mit", "du", "schwer", "mich", "kr\u00e4nkst", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJD", "PPER", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Verziehen sei dir's gerne:", "tokens": ["Ver\u00b7zie\u00b7hen", "sei", "dir's", "ger\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PIS", "ADV", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du wei\u00dft nicht, was du denkst.", "tokens": ["Du", "wei\u00dft", "nicht", ",", "was", "du", "denkst", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Du hast ja nie erfahren", "tokens": ["Du", "hast", "ja", "nie", "er\u00b7fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des Geistes tiefe M\u00fch'n,", "tokens": ["Des", "Geis\u00b7tes", "tie\u00b7fe", "M\u00fch'n", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und ahnst nicht, wie die Schl\u00e4fen", "tokens": ["Und", "ahnst", "nicht", ",", "wie", "die", "Schl\u00e4\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "PTKNEG", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mir hei\u00df vom Denken gl\u00fch'n;", "tokens": ["Mir", "hei\u00df", "vom", "Den\u00b7ken", "gl\u00fch'n", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Du ahnst nicht, wie ich h\u00e4mm're", "tokens": ["Du", "ahnst", "nicht", ",", "wie", "ich", "h\u00e4m\u00b7m'\u00b7re"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und feile Tag f\u00fcr Tag \u2013", "tokens": ["Und", "fei\u00b7le", "Tag", "f\u00fcr", "Tag", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und wie ich mich verblute", "tokens": ["Und", "wie", "ich", "mich", "ver\u00b7blu\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "PRF", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit jedem Herzensschlag!", "tokens": ["Mit", "je\u00b7dem", "Her\u00b7zens\u00b7schlag", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Vom nahen Eisenwerke,", "tokens": ["Vom", "na\u00b7hen", "Ei\u00b7sen\u00b7wer\u00b7ke", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Beru\u00dft, mit schwerem Gang,", "tokens": ["Be\u00b7ru\u00dft", ",", "mit", "schwe\u00b7rem", "Gang", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Kommt mir ein Mann entgegen,", "tokens": ["Kommt", "mir", "ein", "Mann", "ent\u00b7ge\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Den Wiesenpfad entlang.", "tokens": ["Den", "Wie\u00b7sen\u00b7pfad", "ent\u00b7lang", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Mit trotzig finst'rer Miene,", "tokens": ["Mit", "trot\u00b7zig", "finst'\u00b7rer", "Mie\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie mit sich selbst im Streit,", "tokens": ["Wie", "mit", "sich", "selbst", "im", "Streit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Greift er an seine M\u00fctze \u2013", "tokens": ["Greift", "er", "an", "sei\u00b7ne", "M\u00fct\u00b7ze", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Gewohnheit alter Zeit.", "tokens": ["Ge\u00b7wohn\u00b7heit", "al\u00b7ter", "Zeit",