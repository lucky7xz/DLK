{"dta.poem.4209": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Anmuhtige Vorw\u00fcrfe im Walde.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nachdem ich den gefundnen Wald, mit noch vier G\u00e4ngen,", "tokens": ["Nach\u00b7dem", "ich", "den", "ge\u00b7fund\u00b7nen", "Wald", ",", "mit", "noch", "vier", "G\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$,", "APPR", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "zieren lassen,", "tokens": ["zie\u00b7ren", "las\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.3": {"text": "So, da\u00df wir immer drey Alleen,", "tokens": ["So", ",", "da\u00df", "wir", "im\u00b7mer", "drey", "Al\u00b7leen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PPER", "ADV", "CARD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn wir in eine treten, sehen;", "tokens": ["Wenn", "wir", "in", "ei\u00b7ne", "tre\u00b7ten", ",", "se\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und wir, an einem sch\u00f6nen Tag\u2019 im Fr\u00fchling, in demselben", "tokens": ["Und", "wir", ",", "an", "ei\u00b7nem", "sch\u00f6\u00b7nen", "Tag'", "im", "Fr\u00fch\u00b7ling", ",", "in", "dem\u00b7sel\u00b7ben"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "PPER", "$,", "APPR", "ART", "ADJA", "NN", "APPRART", "NN", "$,", "APPR", "PDAT"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "sassen,", "tokens": ["sas\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Das junge Laub entspriessen sah\u2019n, der V\u00f6gel s\u00fcsse Stimmen", "tokens": ["Das", "jun\u00b7ge", "Laub", "ent\u00b7spries\u00b7sen", "sah'n", ",", "der", "V\u00f6\u00b7gel", "s\u00fcs\u00b7se", "Stim\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP", "VAINF", "$,", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "h\u00f6rten,", "tokens": ["h\u00f6r\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Nicht minder der vergn\u00fcgten Fr\u00f6sche,", "tokens": ["Nicht", "min\u00b7der", "der", "ver\u00b7gn\u00fcg\u00b7ten", "Fr\u00f6\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Auch uns vergn\u00fcgendes Gew\u00e4sche,", "tokens": ["Auch", "uns", "ver\u00b7gn\u00fc\u00b7gen\u00b7des", "Ge\u00b7w\u00e4\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Womit sie, durch ihr sanftes ", "tokens": ["Wo\u00b7mit", "sie", ",", "durch", "ihr", "sanf\u00b7tes"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PPER", "$,", "APPR", "PPOSAT", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "gen, auch belehrten;", "tokens": ["gen", ",", "auch", "be\u00b7lehr\u00b7ten", ";"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "VVFIN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.13": {"text": "Rieth einer, damit wir das Gurgeln, wovon die ganze Luft", "tokens": ["Rieth", "ei\u00b7ner", ",", "da\u00b7mit", "wir", "das", "Gur\u00b7geln", ",", "wo\u00b7von", "die", "gan\u00b7ze", "Luft"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "ART", "$,", "KOUS", "PPER", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.14": {"text": "erf\u00fcllt,", "tokens": ["er\u00b7f\u00fcllt", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.15": {"text": "Noch deutlicher verstehen m\u00f6gten, und noch vernehmlicher", "tokens": ["Noch", "deut\u00b7li\u00b7cher", "ver\u00b7ste\u00b7hen", "m\u00f6g\u00b7ten", ",", "und", "noch", "ver\u00b7nehm\u00b7li\u00b7cher"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$,", "KON", "ADV", "ADJA"], "meter": "-+-+-+-+--+-+--", "measure": "iambic.hexa.relaxed"}, "line.16": {"text": "gen\u00f6ssen,", "tokens": ["ge\u00b7n\u00f6s\u00b7sen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "Da\u00df wir, um sch\u00e4rfer noch zu h\u00f6ren, auf kurze Zeit die Augen", "tokens": ["Da\u00df", "wir", ",", "um", "sch\u00e4r\u00b7fer", "noch", "zu", "h\u00f6\u00b7ren", ",", "auf", "kur\u00b7ze", "Zeit", "die", "Au\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUI", "ADJD", "ADV", "PTKZU", "VVINF", "$,", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.18": {"text": "schl\u00f6ssen.", "tokens": ["schl\u00f6s\u00b7sen", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.19": {"text": "Hiedurch nun sch\u00e4rft- und st\u00e4rkten sich", "tokens": ["Hie\u00b7durch", "nun", "sch\u00e4rft", "und", "st\u00e4rk\u00b7ten", "sich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PAV", "ADV", "TRUNC", "KON", "VVFIN", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Die Kr\u00e4fte des Geh\u00f6rs. Das sanfte Musiciren", "tokens": ["Die", "Kr\u00e4f\u00b7te", "des", "Ge\u00b7h\u00f6rs", ".", "Das", "sanf\u00b7te", "Mu\u00b7si\u00b7ci\u00b7ren"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$.", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "War merklich hell- und deutlicher zu sp\u00fchren.", "tokens": ["War", "merk\u00b7lich", "hell", "und", "deut\u00b7li\u00b7cher", "zu", "sp\u00fch\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "TRUNC", "KON", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.22": {"text": "Die Lieblichkeit des Tons schien ausserordentlich", "tokens": ["Die", "Lieb\u00b7lich\u00b7keit", "des", "Tons", "schien", "aus\u00b7ser\u00b7or\u00b7dent\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Zu wachsen, sich zu n\u00e4hren, sich zu mehren.", "tokens": ["Zu", "wach\u00b7sen", ",", "sich", "zu", "n\u00e4h\u00b7ren", ",", "sich", "zu", "meh\u00b7ren", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$,", "PRF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Ein angenehm Concert, ein s\u00fc\u00df-gemischter Schall,", "tokens": ["Ein", "an\u00b7ge\u00b7nehm", "Con\u00b7cert", ",", "ein", "s\u00fc\u00df\u00b7ge\u00b7mischter", "Schall", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NE", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.25": {"text": "War \u00fcberall,", "tokens": ["War", "\u00fc\u00b7be\u00b7rall", ","], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADV", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Als w\u00e4r die ganze Luft davon erf\u00fcllt, zu h\u00f6ren.", "tokens": ["Als", "w\u00e4r", "die", "gan\u00b7ze", "Luft", "da\u00b7von", "er\u00b7f\u00fcllt", ",", "zu", "h\u00f6\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "ADJA", "NN", "PAV", "VVPP", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Kaum war nun, durch die Lieblichkeit", "tokens": ["Kaum", "war", "nun", ",", "durch", "die", "Lieb\u00b7lich\u00b7keit"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Der auch, durchs Ohr, so holden Lenzen-Zeit,", "tokens": ["Der", "auch", ",", "durchs", "Ohr", ",", "so", "hol\u00b7den", "Len\u00b7zen\u00b7Zeit", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "$,", "APPRART", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das Herz der H\u00f6rer stark ger\u00fchret;", "tokens": ["Das", "Herz", "der", "H\u00f6\u00b7rer", "stark", "ge\u00b7r\u00fch\u00b7ret", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Als man von ungefehr ein neu Ergetzen sp\u00fchret.", "tokens": ["Als", "man", "von", "un\u00b7ge\u00b7fehr", "ein", "neu", "Er\u00b7get\u00b7zen", "sp\u00fch\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "ADJD", "ART", "ADJD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Wir h\u00f6rten allgemach ein sanft Bewegen,", "tokens": ["Wir", "h\u00f6r\u00b7ten", "all\u00b7ge\u00b7mach", "ein", "sanft", "Be\u00b7we\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In der bedeckten Luft, entstehn.", "tokens": ["In", "der", "be\u00b7deck\u00b7ten", "Luft", ",", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Es fiel ein lau- ein sanft- und angenehmer Regen", "tokens": ["Es", "fiel", "ein", "lau", "ein", "sanft", "und", "an\u00b7ge\u00b7neh\u00b7mer", "Re\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "ART", "TRUNC", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit einem rauschenden Get\u00f6n.", "tokens": ["Mit", "ei\u00b7nem", "rau\u00b7schen\u00b7den", "Ge\u00b7t\u00f6n", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Es tropft\u2019 ein unverhofftes Na\u00df", "tokens": ["Es", "tropft'", "ein", "un\u00b7ver\u00b7hoff\u00b7tes", "Na\u00df"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Auf das noch junge Laub und Gras,", "tokens": ["Auf", "das", "noch", "jun\u00b7ge", "Laub", "und", "Gras", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Das nicht nur blo\u00df, da man es rauschen h\u00f6rte,", "tokens": ["Das", "nicht", "nur", "blo\u00df", ",", "da", "man", "es", "rau\u00b7schen", "h\u00f6r\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PIS", "PPER", "VVINF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Die Anmuht durchs Geh\u00f6r vermehrte;", "tokens": ["Die", "An\u00b7muht", "durchs", "Ge\u00b7h\u00f6r", "ver\u00b7mehr\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.11": {"text": "Nein, welches durchs Gesicht zugleich, in unsrer Brust,", "tokens": ["Nein", ",", "wel\u00b7ches", "durchs", "Ge\u00b7sicht", "zu\u00b7gleich", ",", "in", "uns\u00b7rer", "Brust", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PRELS", "APPRART", "NN", "ADV", "$,", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Noch eine neue Lust,", "tokens": ["Noch", "ei\u00b7ne", "neu\u00b7e", "Lust", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Auf unterschiedne Weis\u2019, erregte,", "tokens": ["Auf", "un\u00b7ter\u00b7schied\u00b7ne", "Weis'", ",", "er\u00b7reg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Indem ein heller Glanz sich in die Feuchtigkeit", "tokens": ["In\u00b7dem", "ein", "hel\u00b7ler", "Glanz", "sich", "in", "die", "Feuch\u00b7tig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Der erst benetzten St\u00e4mm\u2019 und Zweige pr\u00e4gte,", "tokens": ["Der", "erst", "be\u00b7netz\u00b7ten", "St\u00e4mm'", "und", "Zwei\u00b7ge", "pr\u00e4g\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.16": {"text": "Wodurch sie, wo sie nicht bedeckt vom jungen Gr\u00fcnen,", "tokens": ["Wo\u00b7durch", "sie", ",", "wo", "sie", "nicht", "be\u00b7deckt", "vom", "jun\u00b7gen", "Gr\u00fc\u00b7nen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "PWAV", "PPER", "PTKNEG", "ADJD", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Als w\u00e4ren sie versilbert, schienen.", "tokens": ["Als", "w\u00e4\u00b7ren", "sie", "ver\u00b7sil\u00b7bert", ",", "schie\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Allein, was unsern Blick weit mehr annoch ergetzte,", "tokens": ["Al\u00b7lein", ",", "was", "un\u00b7sern", "Blick", "weit", "mehr", "an\u00b7noch", "er\u00b7getz\u00b7te", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PRELS", "PPOSAT", "NN", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Ja uns fast in Erstaunen setzte,", "tokens": ["Ja", "uns", "fast", "in", "Er\u00b7stau\u00b7nen", "setz\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PPER", "ADV", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "War, als ich linker Hand,", "tokens": ["War", ",", "als", "ich", "lin\u00b7ker", "Hand", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "KOUS", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Und etwas Westen-werts mich r\u00fcckwerts wandt,", "tokens": ["Und", "et\u00b7was", "Wes\u00b7ten\u00b7werts", "mich", "r\u00fcck\u00b7werts", "wandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "NN", "PPER", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und ein so herrliches Spectakel fand,", "tokens": ["Und", "ein", "so", "herr\u00b7li\u00b7ches", "Spec\u00b7ta\u00b7kel", "fand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADV", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+