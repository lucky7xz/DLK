ic.tri"}}, "stanza.6": {"line.1": {"text": "Wir waren all\u2019 hiedurch ganz eingenommen,", "tokens": ["Wir", "wa\u00b7ren", "all'", "hie\u00b7durch", "ganz", "ein\u00b7ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "PAV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und konnten uns nicht satt an allem Glanz,", "tokens": ["Und", "konn\u00b7ten", "uns", "nicht", "satt", "an", "al\u00b7lem", "Glanz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "PTKNEG", "ADJD", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "An aller Pracht der Gluht, wodurch die Fluht entglom-", "tokens": ["An", "al\u00b7ler", "Pracht", "der", "Gluht", ",", "wo\u00b7durch", "die", "Fluht", "ent\u00b7glom"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ART", "NN", "$,", "PWAV", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "men,", "tokens": ["men", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Und aller Sch\u00f6nheit sehn, als wir den bunten Kranz", "tokens": ["Und", "al\u00b7ler", "Sch\u00f6n\u00b7heit", "sehn", ",", "als", "wir", "den", "bun\u00b7ten", "Kranz"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "PIAT", "NN", "VVINF", "$,", "KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Firmaments, des Himmels bunten Bogen,", "tokens": ["Des", "Fir\u00b7ma\u00b7ments", ",", "des", "Him\u00b7mels", "bun\u00b7ten", "Bo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word