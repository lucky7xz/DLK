{"textgrid.poem.25466": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Gl\u00fccksg\u00f6ttin, wenn sie auch Ohren h\u00e4tte,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Gl\u00fccksg\u00f6ttin, wenn sie auch Ohren h\u00e4tte,", "tokens": ["Die", "Gl\u00fccks\u00b7g\u00f6t\u00b7tin", ",", "wenn", "sie", "auch", "Oh\u00b7ren", "h\u00e4t\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "ADV", "NN", "VAFIN", "$,"], "meter": "-+--++-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Kehrt sich an unsre W\u00fcnsche nicht,", "tokens": ["Kehrt", "sich", "an", "uns\u00b7re", "W\u00fcn\u00b7sche", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "PPOSAT", "NN", "PTKNEG", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sonst schleppte niemand das Gewicht", "tokens": ["Sonst", "schlepp\u00b7te", "nie\u00b7mand", "das", "Ge\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Zentnerschweren Sklavenkette,", "tokens": ["Der", "Zent\u00b7ner\u00b7schwe\u00b7ren", "Skla\u00b7ven\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die selten, selbst des Starken Arm, zerbricht.", "tokens": ["Die", "sel\u00b7ten", ",", "selbst", "des", "Star\u00b7ken", "Arm", ",", "zer\u00b7bricht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADV", "ART", "NN", "NN", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wer mit sich Eins ist, wer den Werth des Lebens kennt,", "tokens": ["Wer", "mit", "sich", "Eins", "ist", ",", "wer", "den", "Werth", "des", "Le\u00b7bens", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "PRF", "NN", "VAFIN", "$,", "PWS", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wird seine Zeit mit W\u00fcnschen nicht verlieren.", "tokens": ["Wird", "sei\u00b7ne", "Zeit", "mit", "W\u00fcn\u00b7schen", "nicht", "ver\u00b7lie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "APPR", "NN", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Nur wenig Tage sind uns hier verg\u00f6nnt,", "tokens": ["Nur", "we\u00b7nig", "Ta\u00b7ge", "sind", "uns", "hier", "ver\u00b7g\u00f6nnt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Und wozu sonst, als uns zu r\u00fchren?", "tokens": ["Und", "wo\u00b7zu", "sonst", ",", "als", "uns", "zu", "r\u00fch\u00b7ren", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ADV", "$,", "KOUS", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Die kluge Th\u00e4tigkeit vergi\u00dft", "tokens": ["Die", "klu\u00b7ge", "Th\u00e4\u00b7tig\u00b7keit", "ver\u00b7gi\u00dft"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Am ersten, was ihr etwa fehlet;", "tokens": ["Am", "ers\u00b7ten", ",", "was", "ihr", "et\u00b7wa", "feh\u00b7let", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PWS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.12": {"text": "Nicht so die Thorheit! diese z\u00e4hlet,", "tokens": ["Nicht", "so", "die", "Thor\u00b7heit", "!", "die\u00b7se", "z\u00e4h\u00b7let", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "ADV", "ART", "NN", "$.", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Nicht, was sie hat, nein! was sie noch vermi\u00dft.", "tokens": ["Nicht", ",", "was", "sie", "hat", ",", "nein", "!", "was", "sie", "noch", "ver\u00b7mi\u00dft", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "PRELS", "PPER", "VAFIN", "$,", "PTKANT", "$.", "PWS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "So w\u00fcnsch' ich dir denn nichts! Was hie\u00df' es weiter,", "tokens": ["So", "w\u00fcn\u00b7sch'", "ich", "dir", "denn", "nichts", "!", "Was", "hie\u00df'", "es", "wei\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "PIS", "$.", "PWS", "VVFIN", "PPER", "ADV", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.15": {"text": "Als Wasser sch\u00f6pfen durch ein Sieb?", "tokens": ["Als", "Was\u00b7ser", "sch\u00f6p\u00b7fen", "durch", "ein", "Sieb", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16":