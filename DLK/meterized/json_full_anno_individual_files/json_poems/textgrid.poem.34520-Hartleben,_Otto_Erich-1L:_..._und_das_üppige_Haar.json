{"textgrid.poem.34520": {"metadata": {"author": {"name": "Hartleben, Otto Erich", "birth": "N.A.", "death": "N.A."}, "title": "1L:  ... und das \u00fcppige Haar", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": " ... und das \u00fcppige Haar", "tokens": ["...", "und", "das", "\u00fcp\u00b7pi\u00b7ge", "Haar"], "token_info": ["punct", "word", "word", "word", "word"], "pos": ["$(", "KON", "ART", "ADJA", "NN"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.2": {"text": "und mehr denn alles deine n\u00e4chtigen Augen,", "tokens": ["und", "mehr", "denn", "al\u00b7les", "dei\u00b7ne", "n\u00e4ch\u00b7ti\u00b7gen", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "PIS", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "die saugenden Sterne ... Wahrlich Jessica,", "tokens": ["die", "sau\u00b7gen\u00b7den", "Ster\u00b7ne", "...", "Wahr\u00b7lich", "Jes\u00b7si\u00b7ca", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "ADV", "NE", "$,"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "ich bin noch nie so kindisch stolz gewesen,", "tokens": ["ich", "bin", "noch", "nie", "so", "kin\u00b7disch", "stolz", "ge\u00b7we\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "ADJD", "ADJD", "VAPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "als da ich dir, du wundervolle Hexe,", "tokens": ["als", "da", "ich", "dir", ",", "du", "wun\u00b7der\u00b7vol\u00b7le", "He\u00b7xe", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOKOM", "KOUS", "PPER", "PPER", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "als da ich deinen siegesleuchtenden Reizen", "tokens": ["als", "da", "ich", "dei\u00b7nen", "sie\u00b7ge\u00b7sleuch\u00b7ten\u00b7den", "Rei\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOKOM", "KOUS", "PPER", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "und deinen fordernden Blicken widerstanden.", "tokens": ["und", "dei\u00b7nen", "for\u00b7dern\u00b7den", "Bli\u00b7cken", "wi\u00b7der\u00b7stan\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Nun w\u00e4r ich tot f\u00fcr dich? \u2013 O k\u00f6nntest du h\u00f6ren", "tokens": ["Nun", "w\u00e4r", "ich", "tot", "f\u00fcr", "dich", "?", "\u2013", "O", "k\u00f6nn\u00b7test", "du", "h\u00f6\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADJD", "APPR", "PPER", "$.", "$(", "NE", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "das gelle Lachen, das ich einsam lache", "tokens": ["das", "gel\u00b7le", "La\u00b7chen", ",", "das", "ich", "ein\u00b7sam", "la\u00b7che"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "mir selbst zum Hohne! \u2013 Jessica! Verzeih!", "tokens": ["mir", "selbst", "zum", "Hoh\u00b7ne", "!", "\u2013", "Jes\u00b7si\u00b7ca", "!", "Ver\u00b7zeih", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "ADV", "APPRART", "NN", "$.", "$(", "NE", "$.", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Verzeih! Ich war ein Thor! Ein Narr! Ein S\u00fcnder!", "tokens": ["Ver\u00b7zeih", "!", "Ich", "war", "ein", "Thor", "!", "Ein", "Narr", "!", "Ein", "S\u00fcn\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "$.", "PPER", "VAFIN", "ART", "NN", "$.", "ART", "NN", "$.", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Verzeih! Und wenn du diese Worte liest,", "tokens": ["Ver\u00b7zeih", "!", "Und", "wenn", "du", "die\u00b7se", "Wor\u00b7te", "liest", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "KON", "KOUS", "PPER", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "so wisse, dass die Reue mich verzehrt,", "tokens": ["so", "wis\u00b7se", ",", "dass", "die", "Reu\u00b7e", "mich", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "und wisse, dass ich wiederkehren werde,", "tokens": ["und", "wis\u00b7se", ",", "dass", "ich", "wie\u00b7der\u00b7keh\u00b7ren", "wer\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "und dass ich deine Knie umklammern werde", "tokens": ["und", "dass", "ich", "dei\u00b7ne", "Knie", "um\u00b7klam\u00b7mern", "wer\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "PPO