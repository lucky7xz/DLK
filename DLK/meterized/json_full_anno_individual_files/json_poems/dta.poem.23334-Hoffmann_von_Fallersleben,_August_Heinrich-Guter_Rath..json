{"dta.poem.23334": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Fr\u00f6sche mit den Unken,", "tokens": ["Die", "Fr\u00f6\u00b7sche", "mit", "den", "Un\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie sie so schrecklich schrei'n!", "tokens": ["Wie", "sie", "so", "schreck\u00b7lich", "schrei'n", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich kann vor den Hallunken", "tokens": ["Ich", "kann", "vor", "den", "Hal\u00b7lun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "ART", "NN"], "meter": "-+---+-", "measure": "dactylic.init"}, "line.4": {"text": "Nicht ruhig schlafen ein.", "tokens": ["Nicht", "ru\u00b7hig", "schla\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Sollt' ihnen Freiheit fehlen?", "tokens": ["Sollt'", "ih\u00b7nen", "Frei\u00b7heit", "feh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ach nein, das kann nicht sein:", "tokens": ["Ach", "nein", ",", "das", "kann", "nicht", "sein", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKANT", "$,", "PDS", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wer wird ", "tokens": ["Wer", "wird"], "token_info": ["word", "word"], "pos": ["PWS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}}, "stanza.3": {"line.1": {"text": "Macht's doch, ihr Fr\u00f6sch' und Unken,", "tokens": ["Macht's", "doch", ",", "ihr", "Fr\u00f6sch'", "und", "Un\u00b7ken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wie wir, und trinket Wein!", "tokens": ["Wie", "wir", ",", "und", "trin\u00b7ket", "Wein", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "KON", "VVFIN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Denn habt ihr erst getrunken,", "tokens": ["Denn", "habt", "ihr", "erst", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So la\u00dft ihr auch das Schrei'n.", "tokens": ["So", "la\u00dft", "ihr", "auch", "das", "Schrei'", "n."], "token_info": ["word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ADV", "VVIMP", "PPER", "ADV", "ART", "NN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Ihr werdet Lieder singen", "tokens": ["Ihr", "wer\u00b7det", "Lie\u00b7der", "sin\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Vom freien deutschen Rhein,", "tokens": ["Vom", "frei\u00b7en", "deut\u00b7schen", "Rhein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "ADJA", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und dann vor allen Dingen", "tokens": ["Und", "dann", "vor", "al\u00b7len", "Din\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch ruhig schlafen ein.", "tokens": ["Auch", "ru\u00b7hig", "schla\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}