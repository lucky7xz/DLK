{"textgrid.poem.47025": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "22.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du standst in dich verh\u00fcllt gleich einem jungen", "tokens": ["Du", "standst", "in", "dich", "ver\u00b7h\u00fcllt", "gleich", "ei\u00b7nem", "jun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "VVFIN", "ADV", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Fr\u00fchlinge, der sich selbst noch nicht empfunden;", "tokens": ["Fr\u00fch\u00b7lin\u00b7ge", ",", "der", "sich", "selbst", "noch", "nicht", "emp\u00b7fun\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "ADV", "ADV", "PTKNEG", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ich kam und brachte deines Lenztums Kunden", "tokens": ["Ich", "kam", "und", "brach\u00b7te", "dei\u00b7nes", "Lenz\u00b7tums", "Kun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Dir erst durch meiner Blicke Flammenzungen.", "tokens": ["Dir", "erst", "durch", "mei\u00b7ner", "Bli\u00b7cke", "Flam\u00b7men\u00b7zun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Aufwachtest du aus deinen D\u00e4mmerungen,", "tokens": ["Auf\u00b7wach\u00b7test", "du", "aus", "dei\u00b7nen", "D\u00e4m\u00b7me\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und stehest jetzt, in freier Bl\u00fct' entbunden,", "tokens": ["Und", "ste\u00b7hest", "jetzt", ",", "in", "frei\u00b7er", "Bl\u00fct'", "ent\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$,", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Siegatmend da. Was hab' ich Lohn gefunden,", "tokens": ["Sie\u00b7gat\u00b7mend", "da", ".", "Was", "hab'", "ich", "Lohn", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$.", "PWS", "VAFIN", "PPER", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df ich zuerst den Lenz dir angesungen?", "tokens": ["Da\u00df", "ich", "zu\u00b7erst", "den", "Lenz", "dir", "an\u00b7ge\u00b7sun\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Die Lerche darf ins Saatfeld, wo sie schwirrte,", "tokens": ["Die", "Ler\u00b7che", "darf", "ins", "Saat\u00b7feld", ",", "wo", "sie", "schwirr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die Nachtigall ins Buschwerk, wo sie lockte,", "tokens": ["Die", "Nach\u00b7ti\u00b7gall", "ins", "Busc\u00b7hwerk", ",", "wo", "sie", "lock\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Schwalbe, wo sie sang, ans Dach von Moose", "tokens": ["Die", "Schwal\u00b7be", ",", "wo", "sie", "sang", ",", "ans", "Dach", "von", "Moo\u00b7se"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,", "APPRART", "NN", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ihr Nest sich baun. O du, um die ich girrte,", "tokens": ["Ihr", "Nest", "sich", "baun", ".", "O", "du", ",", "um", "die", "ich", "girr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "VVINF", "$.", "NE", "NE", "$,", "APPR", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mir Dach und Busch und Saatfeld, o verstockte,", "tokens": ["Mir", "Dach", "und", "Busch", "und", "Saat\u00b7feld", ",", "o", "ver\u00b7stock\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "KON", "NN", "$,", "FM", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wo soll ich nisten, als in deinem Scho\u00dfe?", "tokens": ["Wo", "soll", "ich", "nis\u00b7ten", ",", "als", "in", "dei\u00b7nem", "Scho\u00b7\u00dfe", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVFIN", "$,", "KOUS", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du standst in dich verh\u00fcllt gleich einem jungen", "tokens": ["Du", "standst", "in", "dich", "ver\u00b