{"textgrid.poem.25768": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wohlan! Wir sind zur Stadt hinaus,", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wohlan! Wir sind zur Stadt hinaus,", "tokens": ["Wo\u00b7hlan", "!", "Wir", "sind", "zur", "Stadt", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschallt nun, ihr Ges\u00e4nge!", "tokens": ["Er\u00b7schallt", "nun", ",", "ihr", "Ge\u00b7s\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Rechte war uns unser Haus", "tokens": ["Mit", "Rech\u00b7te", "war", "uns", "un\u00b7ser", "Haus"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VAFIN", "PPER", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu dunkel und zu enge;", "tokens": ["Zu", "dun\u00b7kel", "und", "zu", "en\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "KON", "APPR", "ADJA", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Hier lockt uns die Schalmey heraus,", "tokens": ["Hier", "lockt", "uns", "die", "Schal\u00b7mey", "he\u00b7raus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.6": {"text": "Und V\u00f6gelein die Menge.", "tokens": ["Und", "V\u00f6\u00b7ge\u00b7lein", "die", "Men\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Fort, Schwager! \u00fcber Stock und Stein", "tokens": ["Fort", ",", "Schwa\u00b7ger", "!", "\u00fc\u00b7ber", "Stock", "und", "Stein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$.", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Mit unserm leichten Wagen!", "tokens": ["Mit", "un\u00b7serm", "leich\u00b7ten", "Wa\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Mehr als Ap'thekerspezereyn", "tokens": ["Mehr", "als", "Ap'\u00b7the\u00b7ker\u00b7spe\u00b7ze\u00b7reyn"], "token_info": ["word", "word", "word"], "pos": ["PIAT", "KOKOM", "NN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.10": {"text": "Wird uns das Ding behagen;", "tokens": ["Wird", "uns", "das", "Ding", "be\u00b7ha\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Ersch\u00fcttern wird es Mark und Bein,", "tokens": ["Er\u00b7sch\u00fct\u00b7tern", "wird", "es", "Mark", "und", "Bein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Und st\u00e4rken unsern Magen.", "tokens": ["Und", "st\u00e4r\u00b7ken", "un\u00b7sern", "Ma\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Brav, Schwager! Nein! ich lobe mir", "tokens": ["Brav", ",", "Schwa\u00b7ger", "!", "Nein", "!", "ich", "lo\u00b7be", "mir"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "$,", "NN", "$.", "PTKANT", "$.", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Das Reisen auf dem Lande!", "tokens": ["Das", "Rei\u00b7sen", "auf", "dem", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Was fragen nach den Winden wir", "tokens": ["Was", "fra\u00b7gen", "nach", "den", "Win\u00b7den", "wir"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "APPR", "ART", "NN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.16": {"text": "Und der Planeten Stande?", "tokens": ["Und", "der", "Pla\u00b7ne\u00b7ten", "Stan\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Wer f\u00fcrchtet sich vor Klippen hier,", "tokens": ["Wer", "f\u00fcrch\u00b7tet", "sich", "vor", "Klip\u00b7pen", "hier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Und vor des Meeres Strande?", "tokens": ["Und", "vor", "des", "Mee\u00b7res", "Stran\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Halt, Schwager! halt! Hervor mit dir,", "tokens": ["Halt", ",", "Schwa\u00b7ger", "!", "halt", "!", "Her\u00b7vor", "mit", "dir", ","], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$.", "VVFIN", "$.", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Du guter Flaschenkeller!", "tokens": ["Du", "gu\u00b7ter", "Fla\u00b7schen\u00b7kel\u00b7ler", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.21": {"text": "Du duftest, Wein! im Freien hier", "tokens": ["Du", "duf\u00b7test", ",", "Wein", "!", "im", "Frei\u00b7en", "hier"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "NN", "$.", "APPRART", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "So lieblich, blinkst auch heller;", "tokens": ["So", "lieb\u00b7lich", ",", "blinkst", "auch", "hel\u00b7ler", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Allein hinab, hinab mit dir,", "tokens": ["Al\u00b7lein", "hin\u00b7ab", ",", "hin\u00b7ab", "mit", "dir", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "PTKVZ", "$,", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Denn du verriechst auch schneller.", "tokens": ["Denn", "du", "ver\u00b7riechst", "auch", "schnel\u00b7ler", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADV", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Nun aber, Schwager! blase du", "tokens": ["Nun", "a\u00b7ber", ",", "Schwa\u00b7ger", "!", "bla\u00b7se", "du"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "ADV", "$,", "NN", "$.", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "Was Lung' und Horn verm\u00f6gen!", "tokens": ["Was", "Lung'", "und", "Horn", "ver\u00b7m\u00f6\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Und nun, ihr Rappen, laufet zu,", "tokens": ["Und", "nun", ",", "ihr", "Rap\u00b7pen", ",", "lau\u00b7fet", "zu", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Als wenn davon wir fl\u00f6gen!", "tokens": ["Als", "wenn", "da\u00b7von", "wir", "fl\u00f6\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PAV", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "O seht! wie kommen uns im Nu", "tokens": ["O", "seht", "!", "wie", "kom\u00b7men", "uns", "im", "Nu"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "PWAV", "VVFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Die Weidenb\u00e4um' entgegen!", "tokens": ["Die", "Wei\u00b7den\u00b7b\u00e4um'", "ent\u00b7ge\u00b7gen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Lauft nur, ihr Weiden, lauft nur hin,", "tokens": ["Lauft", "nur", ",", "ihr", "Wei\u00b7den", ",", "lauft", "nur", "hin", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$,", "VVFIN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Woher wir eben kommen,", "tokens": ["Wo\u00b7her", "wir", "e\u00b7ben", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.33": {"text": "Es wird, so wahr ich fr\u00f6hlich bin,", "tokens": ["Es", "wird", ",", "so", "wahr", "ich", "fr\u00f6h\u00b7lich", "bin", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "ADV", "ADJD", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.34": {"text": "Im St\u00e4dtlein euch nicht frommen,", "tokens": ["Im", "St\u00e4dt\u00b7lein", "euch", "nicht", "from\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "PTKNEG", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.35": {"text": "Denn Luft und Sonne wird darin", "tokens": ["Denn", "Luft", "und", "Son\u00b7ne", "wird", "da\u00b7rin"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "NN", "KON", "NN", "VAFIN", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.36": {"text": "Den Menschen fast benommen.", "tokens": ["Den", "Men\u00b7schen", "fast", "be\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.37": {"text": "Mag an des St\u00e4dtleins Einerlei", "tokens": ["Mag", "an", "des", "St\u00e4dt\u00b7leins", "Ei\u00b7ner\u00b7lei"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VMFIN", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.38": {"text": "Ein Faulthier sich gew\u00f6hnen.", "tokens": ["Ein", "Fault\u00b7hier", "sich", "ge\u00b7w\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.39": {"text": "Auf jedem Schritte aber neu", "tokens": ["Auf", "je\u00b7dem", "Schrit\u00b7te", "a\u00b7ber", "neu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "ADV", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.40": {"text": "Sind hier der Landschaft Scenen.", "tokens": ["Sind", "hier", "der", "Land\u00b7schaft", "Sce\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.41": {"text": "Was kommt der Lust zu reisen bei?", "tokens": ["Was", "kommt", "der", "Lust", "zu", "rei\u00b7sen", "bei", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKZU", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.42": {"text": "La\u00dft uns ihr ewig fr\u00f6hnen.", "tokens": ["La\u00dft", "uns", "ihr", "e\u00b7wig", "fr\u00f6h\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "ADJD", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Wohlan! Wir sind zur Stadt hinaus,", "tokens": ["Wo\u00b7hlan", "!", "Wir", "sind", "zur", "Stadt", "hin\u00b7aus", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "PPER", "VAFIN", "APPRART", "NN", "APZR", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erschallt nun, ihr Ges\u00e4nge!", "tokens": ["Er\u00b7schallt", "nun", ",", "ihr", "Ge\u00b7s\u00e4n\u00b7ge", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Mit Rechte war uns unser Haus", "tokens": ["Mit", "Rech\u00b7te", "war", "uns", "un\u00b7ser", "Haus"], "token_info": ["word", "word", "word", "word", "w