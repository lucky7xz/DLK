{"dta.poem.10989": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Wort-Streit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht, etwas wirklich zu ergr\u00fcnden,", "tokens": ["Nicht", ",", "et\u00b7was", "wirk\u00b7lich", "zu", "er\u00b7gr\u00fcn\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "ADV", "ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht, da\u00df man will die Wahrheit finden;", "tokens": ["Nicht", ",", "da\u00df", "man", "will", "die", "Wahr\u00b7heit", "fin\u00b7den", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "$,", "KOUS", "PIS", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Nein; Sondern die\u00df gemeiniglich:", "tokens": ["Nein", ";", "Son\u00b7dern", "die\u00df", "ge\u00b7mei\u00b7nig\u00b7lich", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "KON", "PDS", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Zung und Lunge zu probiren,", "tokens": ["Mit", "Zung", "und", "Lun\u00b7ge", "zu", "pro\u00b7bi\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Mit lautem Schreyen, \u00fcberf\u00fchren,", "tokens": ["Mit", "lau\u00b7tem", "Schre\u00b7yen", ",", "\u00fc\u00b7berf\u00b7\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df du so klug nicht seyst, als ich,", "tokens": ["Da\u00df", "du", "so", "klug", "nicht", "seyst", ",", "als", "ich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PTKNEG", "VAFIN", "$,", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df meiner kl\u00fcger, als dein Geist,", "tokens": ["Da\u00df", "mei\u00b7ner", "kl\u00fc\u00b7ger", ",", "als", "dein", "Geist", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "ADJA", "$,", "KOUS", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und viel erhabner. Dieses hei\u00dft,", "tokens": ["Und", "viel", "er\u00b7hab\u00b7ner", ".", "Die\u00b7ses", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "$.", "PDS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mit einem Worte: ", "tokens": ["Mit", "ei\u00b7nem", "Wor\u00b7te", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}