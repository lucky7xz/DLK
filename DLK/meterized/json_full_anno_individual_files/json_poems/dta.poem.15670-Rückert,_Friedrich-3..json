{"dta.poem.15670": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195122", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn du nicht ausziehn kannst den Fehler der Natur,", "tokens": ["Wenn", "du", "nicht", "aus\u00b7ziehn", "kannst", "den", "Feh\u00b7ler", "der", "Na\u00b7tur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VVINF", "VMFIN", "ART", "NN", "ART", "NN", "$,"], "meter": "----+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "In eine Tugend such' ihn umzubilden nur.", "tokens": ["In", "ei\u00b7ne", "Tu\u00b7gend", "such'", "ihn", "um\u00b7zu\u00b7bil\u00b7den", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PPER", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Nicht mein' ich ihn mit Schein der Tugend zu bedecken,", "tokens": ["Nicht", "mein'", "ich", "ihn", "mit", "Schein", "der", "Tu\u00b7gend", "zu", "be\u00b7de\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "APPR", "NN", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "F\u00fcr Kinder h\u00e4ngt man Fr\u00fccht' an unfruchtbare Hecken;", "tokens": ["F\u00fcr", "Kin\u00b7der", "h\u00e4ngt", "man", "Fr\u00fccht'", "an", "un\u00b7frucht\u00b7ba\u00b7re", "He\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PIS", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Doch nie wird Heuchelei des G\u00e4rtners Flei\u00df geschimpft,", "tokens": ["Doch", "nie", "wird", "Heu\u00b7che\u00b7lei", "des", "G\u00e4rt\u00b7ners", "Flei\u00df", "ge\u00b7schimpft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "ART", "NN", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der edle Reiser auf unedlen Stamm geimpft.", "tokens": ["Der", "ed\u00b7le", "Rei\u00b7ser", "auf", "u\u00b7ned\u00b7len", "Stamm", "geimpft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wie man des starren Bergs rauh unfruchtbare Warten", "tokens": ["Wie", "man", "des", "star\u00b7ren", "Bergs", "rauh", "un\u00b7frucht\u00b7ba\u00b7re", "War\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "ART", "ADJA", "NN", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+---+-", "measure": "unknown.measure.penta"}, "line.2": {"text": "Zu R\u00fcckhalt w\u00e4hlt und Schirm dem angelegten Garten.", "tokens": ["Zu", "R\u00fcck\u00b7halt", "w\u00e4hlt", "und", "Schirm", "dem", "an\u00b7ge\u00b7leg\u00b7ten", "Gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "KON", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wie, wo des Stroms Gewalt Trotz bietet aller Hemmung,", "tokens": ["Wie", ",", "wo", "des", "Stroms", "Ge\u00b7walt", "Trotz", "bie\u00b7tet", "al\u00b7ler", "Hem\u00b7mung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "PWAV", "ART", "NN", "NN", "APPR", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man zur Bew\u00e4sserung benutzt die Ueberschwemmung.", "tokens": ["Man", "zur", "Be\u00b7w\u00e4s\u00b7se\u00b7rung", "be\u00b7nutzt", "die", "Ue\u00b7ber\u00b7schwem\u00b7mung", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "APPRART", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}}}}}