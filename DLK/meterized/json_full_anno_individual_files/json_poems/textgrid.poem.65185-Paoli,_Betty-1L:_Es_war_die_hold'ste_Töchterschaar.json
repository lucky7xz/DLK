{"textgrid.poem.65185": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es war die hold'ste T\u00f6chterschaar", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es war die hold'ste T\u00f6chterschaar", "tokens": ["Es", "war", "die", "hold'\u00b7ste", "T\u00f6ch\u00b7ter\u00b7schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Einsamkeit beschieden;", "tokens": ["Der", "Ein\u00b7sam\u00b7keit", "be\u00b7schie\u00b7den", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Wer ihnen sich gelobt, f\u00fcrwahr!", "tokens": ["Wer", "ih\u00b7nen", "sich", "ge\u00b7lobt", ",", "f\u00fcr\u00b7wahr", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "PRF", "VVPP", "$,", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der findet sel'gen Frieden.", "tokens": ["Der", "fin\u00b7det", "sel'\u00b7gen", "Frie\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Da ist die Ahnung, die beschwingt,", "tokens": ["Da", "ist", "die", "Ah\u00b7nung", ",", "die", "be\u00b7schwingt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Uns nicht am Staub l\u00e4\u00dft kleben;", "tokens": ["Uns", "nicht", "am", "Staub", "l\u00e4\u00dft", "kle\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKNEG", "APPRART", "NN", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Erkenntnis, die den Willen zwingt", "tokens": ["Er\u00b7kennt\u00b7nis", ",", "die", "den", "Wil\u00b7len", "zwingt"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sich selber aufzugeben;", "tokens": ["Sich", "sel\u00b7ber", "auf\u00b7zu\u00b7ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Die Sammlung, die, der Seele Licht,", "tokens": ["Die", "Samm\u00b7lung", ",", "die", ",", "der", "See\u00b7le", "Licht", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "In's Gro\u00dfe sich versenket;", "tokens": ["In's", "Gro\u00b7\u00dfe", "sich", "ver\u00b7sen\u00b7ket", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "PRF", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Die echte Freiheit, l\u00e4nger nicht", "tokens": ["Die", "ech\u00b7te", "Frei\u00b7heit", ",", "l\u00e4n\u00b7ger", "nicht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "ADJD", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Von Furcht und Wunsch beschr\u00e4nket,", "tokens": ["Von", "Furcht", "und", "Wunsch", "be\u00b7schr\u00e4n\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Die Liebe, die, nicht mehr verrannt", "tokens": ["Die", "Lie\u00b7be", ",", "die", ",", "nicht", "mehr", "ver\u00b7rannt"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "$,", "PTKNEG", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "In selbstisches Verlangen,", "tokens": ["In", "selbs\u00b7ti\u00b7sches", "Ver\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.15": {"text": "Sich von dem einzelnen gewandt", "tokens": ["Sich", "von", "dem", "ein\u00b7zel\u00b7nen", "ge\u00b7wandt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.16": {"text": "Um alle zu umfangen.", "tokens": ["Um", "al\u00b7le", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUI", "PIS", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.17": {"text": "Wer priese nicht die edle Zier", "tokens": ["Wer", "prie\u00b7se", "nicht", "die", "ed\u00b7le", "Zier"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VVFIN", "PTKNEG", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "So makelloser Lilien?", "tokens": ["So", "ma\u00b7kel\u00b7lo\u00b7ser", "Li\u00b7li\u00b7en", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.19": {"text": "Doch, schlimm genug! ergeht's auch hier", "tokens": ["Doch", ",", "schlimm", "ge\u00b7nug", "!", "er\u00b7geht's", "auch", "hier"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "$,", "ADJD", "ADV", "$.", "VVFIN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Wie manchmal in Familien:", "tokens": ["Wie", "manch\u00b7mal", "in", "Fa\u00b7mi\u00b7li\u00b7en", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "APPR", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.21": {"text": "Den T\u00f6chtern, liebevoll bestellt", "tokens": ["Den", "T\u00f6ch\u00b7tern", ",", "lie\u00b7be\u00b7voll", "be\u00b7stellt"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Uns himmelw\u00e4rts zu tragen,", "tokens": ["Uns", "him\u00b7mel\u00b7w\u00e4rts", "zu", "tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Sind leider S\u00f6hne beigesellt,", "tokens": ["Sind", "lei\u00b7der", "S\u00f6h\u00b7ne", "bei\u00b7ge\u00b7sellt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Die aus der Art geschlagen.", "tokens": ["Die", "aus", "der", "Art", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.25": {"text": "Der Eigensinn, der st\u00f6rrisch h\u00e4lt", "tokens": ["Der", "Ei\u00b7gen\u00b7sinn", ",", "der", "st\u00f6r\u00b7risch", "h\u00e4lt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.26": {"text": "An angema\u00dftem Rechte,", "tokens": ["An", "an\u00b7ge\u00b7ma\u00df\u00b7tem", "Rech\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.27": {"text": "Der Hochmut, der rings auf der Welt", "tokens": ["Der", "Hoch\u00b7mut", ",", "der", "rings", "auf", "der", "Welt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "APPR", "ART", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.28": {"text": "Nur Thoren sieht und Knechte,", "tokens": ["Nur", "Tho\u00b7ren", "sieht", "und", "Knech\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.29": {"text": "Der Mi\u00dfmut, mit sich selbst im Streit,", "tokens": ["Der", "Mi\u00df\u00b7mut", ",", "mit", "sich", "selbst", "im", "Streit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "PRF", "ADV", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Der starre Trotz nicht minder,", "tokens": ["Der", "star\u00b7re", "Trotz", "nicht", "min\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PTKNEG", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Sind allesamt der Einsamkeit", "tokens": ["Sind", "al\u00b7le\u00b7samt", "der", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.32": {"text": "Schmachvoll mi\u00dfrat'ne Kinder.", "tokens": ["Schmach\u00b7voll", "mi\u00df\u00b7rat'\u00b7ne", "Kin\u00b7der", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Drum lasse du in deiner Brust", "tokens": ["Drum", "las\u00b7se", "du", "in", "dei\u00b7ner", "Brust