{"textgrid.poem.47349": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich ging aus meinem Vaterland", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich ging aus meinem Vaterland", "tokens": ["Ich", "ging", "aus", "mei\u00b7nem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einziges Mal im Leben,", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", "Mal", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und habe, weil ich dich drau\u00dfen nicht fand,", "tokens": ["Und", "ha\u00b7be", ",", "weil", "ich", "dich", "drau\u00b7\u00dfen", "nicht", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich schleunig zur\u00fcck begeben.", "tokens": ["Mich", "schleu\u00b7nig", "zu\u00b7r\u00fcck", "be\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "PTKVZ", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Ich werde nach keinem fremden Strand", "tokens": ["Ich", "wer\u00b7de", "nach", "kei\u00b7nem", "frem\u00b7den", "Strand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "PIAT", "ADJA", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.6": {"text": "Mich jemals wieder begeben", "tokens": ["Mich", "je\u00b7mals", "wie\u00b7der", "be\u00b7ge\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.7": {"text": "Und denk' einst auch nur an deiner Hand", "tokens": ["Und", "denk'", "einst", "auch", "nur", "an", "dei\u00b7ner", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Zu reisen ins andre Leben.", "tokens": ["Zu", "rei\u00b7sen", "ins", "and\u00b7re", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Ich ging aus meinem Vaterland", "tokens": ["Ich", "ging", "aus", "mei\u00b7nem", "Va\u00b7ter\u00b7land"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ein einziges Mal im Leben,", "tokens": ["Ein", "ein\u00b7zi\u00b7ges", "Mal", "im", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und habe, weil ich dich drau\u00dfen nicht fand,", "tokens": ["Und", "ha\u00b7be", ",", "weil", "ich", "dich", "drau\u00b7\u00dfen", "nicht", "fand", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "$,", "KOUS", "PPER", "PRF", "ADV", "PTKNEG", "VVFIN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Mich schleunig zur\u00fcck begeben.", "tokens": ["Mich", "schleu\u00b7nig", "zu\u00b7r\u00fcck", "be\u00b7ge\