{"textgrid.poem.62300": {"metadata": {"author": {"name": "Kempner, Friederike", "birth": "N.A.", "death": "N.A."}, "title": "Sonett", "genre": "verse", "period": "N.A.", "pub_year": 1868, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "In Waffen steht die Welt", "tokens": ["In", "Waf\u00b7fen", "steht", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Um L\u00e4nder und um Geld,", "tokens": ["Um", "L\u00e4n\u00b7der", "und", "um", "Geld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "NN", "KON", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Aus Friedensliebe zwar,", "tokens": ["Aus", "Frie\u00b7dens\u00b7lie\u00b7be", "zwar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "So hei\u00dft es immerdar.", "tokens": ["So", "hei\u00dft", "es", "im\u00b7mer\u00b7dar", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Nur eines ruhig bleibt", "tokens": ["Nur", "ei\u00b7nes", "ru\u00b7hig", "bleibt"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJD", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Und ew'ge Bl\u00fcten treibt,", "tokens": ["Und", "ew'\u00b7ge", "Bl\u00fc\u00b7ten", "treibt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Im Reich der Poesie,", "tokens": ["Im", "Reich", "der", "Poe\u00b7sie", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Da streitet man sich nie. \u2013", "tokens": ["Da", "strei\u00b7tet", "man", "sich", "nie", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PRF", "ADV", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "In reinem Element", "tokens": ["In", "rei\u00b7nem", "E\u00b7le\u00b7ment"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Man keine Feindschaft kennt,", "tokens": ["Man", "kei\u00b7ne", "Feind\u00b7schaft", "kennt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIS", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Die Mensch vom Menschen trennt.", "tokens": ["Die", "Mensch", "vom", "Men\u00b7schen", "trennt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Ob nahe oder weit", "tokens": ["Ob", "na\u00b7he", "o\u00b7der", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "KON", "ADJD"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Man leidet mit, mit Leid \u2013", "tokens": ["Man", "lei\u00b7det", "mit", ",", "mit", "Leid", "\u2013"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PTKVZ", "$,", "APPR", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "F\u00fcr jetzt und alle Zeit.", "tokens": ["F\u00fcr", "jetzt", "und", "al\u00b7le", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "KON", "PIAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.