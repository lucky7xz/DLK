{"dta.poem.6101": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der Sonne, den Planeten,  \n und vom Firmament.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Doch halt! wir m\u00fcssen die Gedancken,", "tokens": ["Doch", "halt", "!", "wir", "m\u00fcs\u00b7sen", "die", "Ge\u00b7dan\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "PPER", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Die gar die Menschheit \u00fcbergehn,", "tokens": ["Die", "gar", "die", "Menschheit", "\u00fc\u00b7ber\u00b7gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Zur\u00fccke ziehn in unsers Wirbels Schrancken,", "tokens": ["Zu\u00b7r\u00fc\u00b7cke", "ziehn", "in", "un\u00b7sers", "Wir\u00b7bels", "Schran\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und seine Pracht und Ordnung sehn.", "tokens": ["Und", "sei\u00b7ne", "Pracht", "und", "Ord\u00b7nung", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Lasst uns allein mit unsern Blicken", "tokens": ["Lasst", "uns", "al\u00b7lein", "mit", "un\u00b7sern", "Bli\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Der Dinge Sch\u00f6nheit schaun, die unsern Himmel schm\u00fccken.", "tokens": ["Der", "Din\u00b7ge", "Sch\u00f6n\u00b7heit", "schaun", ",", "die", "un\u00b7sern", "Him\u00b7mel", "schm\u00fc\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVINF", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}