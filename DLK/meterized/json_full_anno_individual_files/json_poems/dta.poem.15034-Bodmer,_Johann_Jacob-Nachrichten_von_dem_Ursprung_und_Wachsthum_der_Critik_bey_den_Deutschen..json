{"dta.poem.15034": {"metadata": {"author": {"name": "Bodmer, Johann Jacob", "birth": "N.A.", "death": "N.A."}, "title": "Nachrichten von dem Ursprung und  \n Wachsthum der Critik bey den  \n Deutschen.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1741", "urn": "urn:nbn:de:kobv:b4-200905198526", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der von der Hungersucht beth\u00f6rten Dichterzunft,", "tokens": ["Der", "von", "der", "Hun\u00b7ger\u00b7sucht", "be\u00b7th\u00f6r\u00b7ten", "Dich\u00b7ter\u00b7zunft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die sich durch falsche Kunst auf den Parna\u00df geschlichen,", "tokens": ["Die", "sich", "durch", "fal\u00b7sche", "Kunst", "auf", "den", "Par\u00b7na\u00df", "ge\u00b7schli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "ADJA", "NN", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+--++-+-", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Von der gesezten Bahn der Alten abgewichen,", "tokens": ["Von", "der", "ge\u00b7sez\u00b7ten", "Bahn", "der", "Al\u00b7ten", "ab\u00b7ge\u00b7wi\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "\u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012 \u2012", "tokens": ["\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012", "\u2012"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["$(", "$(", "$(", "$(", "$(", "$(", "$(", "$(", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Der Griechen Z\u00e4rtlichkeit das Todesurtheil f\u00e4llet,", "tokens": ["Der", "Grie\u00b7chen", "Z\u00e4rt\u00b7lich\u00b7keit", "das", "To\u00b7des\u00b7urt\u00b7heil", "f\u00e4l\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Maro klugen Wiz in Kinderclassen weist,", "tokens": ["Des", "Ma\u00b7ro", "klu\u00b7gen", "Wiz", "in", "Kin\u00b7der\u00b7clas\u00b7sen", "weist", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "ADJA", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Horazens Dichterbuch verrauchte Grillen hei\u00dft. rc.", "tokens": ["Ho\u00b7ra\u00b7zens", "Dich\u00b7ter\u00b7buch", "ver\u00b7rauch\u00b7te", "Gril\u00b7len", "hei\u00dft", ".", "rc", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "PPER", "VVFIN", "NN", "VVFIN", "$.", "FM.la", "$."], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}}}}}