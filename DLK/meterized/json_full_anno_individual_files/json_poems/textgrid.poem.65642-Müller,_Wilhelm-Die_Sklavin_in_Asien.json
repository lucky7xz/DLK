{"textgrid.poem.65642": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Sklavin in Asien", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwestern, weint mit mir! Ich weine \u00fcber meine Ketten nicht.", "tokens": ["Schwes\u00b7tern", ",", "weint", "mit", "mir", "!", "Ich", "wei\u00b7ne", "\u00fc\u00b7ber", "mei\u00b7ne", "Ket\u00b7ten", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sollt' es mich denn gleich zerdr\u00fccken, dieses eiserne Gewicht,", "tokens": ["Sollt'", "es", "mich", "denn", "gleich", "zer\u00b7dr\u00fc\u00b7cken", ",", "die\u00b7ses", "ei\u00b7ser\u00b7ne", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Das so lange hat getragen unser edles Vaterland,", "tokens": ["Das", "so", "lan\u00b7ge", "hat", "ge\u00b7tra\u00b7gen", "un\u00b7ser", "ed\u00b7les", "Va\u00b7ter\u00b7land", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "ADV", "VAFIN", "VVPP", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und es konnt' ihm doch nicht l\u00e4hmen seine alte Heldenhand?", "tokens": ["Und", "es", "konnt'", "ihm", "doch", "nicht", "l\u00e4h\u00b7men", "sei\u00b7ne", "al\u00b7te", "Hel\u00b7den\u00b7hand", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "PPER", "ADV", "PTKNEG", "VVFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Schwestern, weint mit mir! Ich weine nicht um unsrer Arbeit Schwei\u00df.", "tokens": ["Schwes\u00b7tern", ",", "weint", "mit", "mir", "!", "Ich", "wei\u00b7ne", "nicht", "um", "uns\u00b7rer", "Ar\u00b7beit", "Schwei\u00df", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Keiner soll des Polsters pflegen, der den Leib zu r\u00fchren wei\u00df,", "tokens": ["Kei\u00b7ner", "soll", "des", "Pols\u00b7ters", "pfle\u00b7gen", ",", "der", "den", "Leib", "zu", "r\u00fch\u00b7ren", "wei\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ART", "NN", "VVINF", "$,", "PRELS", "ART", "NN", "PTKZU", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Wenn das Vaterland in N\u00f6then laut nach seinen Kindern schreit \u2013", "tokens": ["Wenn", "das", "Va\u00b7ter\u00b7land", "in", "N\u00f6\u00b7then", "laut", "nach", "sei\u00b7nen", "Kin\u00b7dern", "schreit", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "NN", "ADJD", "APPR", "PPOSAT", "NN", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Wer nicht wehren kann und st\u00fcrmen, sei zu leiden doch bereit.", "tokens": ["Wer", "nicht", "weh\u00b7ren", "kann", "und", "st\u00fcr\u00b7men", ",", "sei", "zu", "lei\u00b7den", "doch", "be\u00b7reit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKNEG", "VVINF", "VMFIN", "KON", "VVFIN", "$,", "VAFIN", "PTKZU", "VVINF", "KON", "ADJD", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "Schwestern, weint mit mir! Ich weine nicht um meiner Br\u00fcder Tod.", "tokens": ["Schwes\u00b7tern", ",", "weint", "mit", "mir", "!", "Ich", "wei\u00b7ne", "nicht", "um", "mei\u00b7ner", "Br\u00fc\u00b7der", "Tod", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "PTKNEG", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Ihre sel'gen Geister schweben oft um mich im Abendroth,", "tokens": ["Ih\u00b7re", "sel'\u00b7gen", "Geis\u00b7ter", "schwe\u00b7ben", "oft", "um", "mich", "im", "A\u00b7ben\u00b7droth", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "APPR", "PRF", "APPRART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Wehn mit ihren Siegeskr\u00e4nzen k\u00fchlen Trost von fern mir zu \u2013", "tokens": ["Wehn", "mit", "ih\u00b7ren", "Sie\u00b7ges\u00b7kr\u00e4n\u00b7zen", "k\u00fch\u00b7len", "Trost", "von", "fern", "mir", "zu", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "APPR", "ADJD", "PPER", "PTKZU", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.12": {"text": "Sollt' ich denn durch eitle Thr\u00e4nen st\u00f6ren ihre Grabesruh'?", "tokens": ["Sollt'", "ich", "denn", "durch", "eit\u00b7le", "Thr\u00e4\u00b7nen", "st\u00f6\u00b7ren", "ih\u00b7re", "Gra\u00b7bes\u00b7ruh'", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Schwestern, weint mit mir! Ich weine auch um meinen Liebling nicht.", "tokens": ["Schwes\u00b7tern", ",", "weint", "mit", "mir", "!", "Ich", "wei\u00b7ne", "auch", "um", "mei\u00b7nen", "Lieb\u00b7ling", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Lebt er, o so wei\u00df ich, da\u00df er, als ein Held \u2013 f\u00fcr mich auch ficht;", "tokens": ["Lebt", "er", ",", "o", "so", "wei\u00df", "ich", ",", "da\u00df", "er", ",", "als", "ein", "Held", "\u2013", "f\u00fcr", "mich", "auch", "ficht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "FM", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "$,", "KOUS", "ART", "NN", "$(", "APPR", "PPER", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Sank er, will ich Lorbeerb\u00e4ume pflanzen \u00fcber sein Gebein,", "tokens": ["Sank", "er", ",", "will", "ich", "Lor\u00b7beer\u00b7b\u00e4u\u00b7me", "pflan\u00b7zen", "\u00fc\u00b7ber", "sein", "Ge\u00b7bein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VMFIN", "PPER", "NN", "VVINF", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Und die St\u00e4tte wird ein Tempel f\u00fcr die freie Hellas sein.", "tokens": ["Und", "die", "St\u00e4t\u00b7te", "wird", "ein", "Tem\u00b7pel", "f\u00fcr", "die", "frei\u00b7e", "Hel\u00b7las", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Schwestern, weint mit mir! Ich weine, weine, da\u00df ich bin kein Mann \u2013", "tokens": ["Schwes\u00b7tern", ",", "weint", "mit", "mir", "!", "Ich", "wei\u00b7ne", ",", "wei\u00b7ne", ",", "da\u00df", "ich", "bin", "kein", "Mann", "\u2013"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "$,", "VVFIN", "$,", "KOUS", "PPER", "VAFIN", "PIAT", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Da\u00df ich nicht ein Ro\u00df besteigen, keine Lanze schwingen kann,", "tokens": ["Da\u00df", "ich", "nicht", "ein", "Ro\u00df", "be\u00b7stei\u00b7gen", ",", "kei\u00b7ne", "Lan\u00b7ze", "schwin\u00b7gen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "ART", "NN", "VVINF", "$,", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Da\u00df ich nicht kann Eisen sprengen, schwimmen durch die wilde Fluth \u2013", "tokens": ["Da\u00df", "ich", "nicht", "kann", "Ei\u00b7sen", "spren\u00b7gen", ",", "schwim\u00b7men", "durch", "die", "wil\u00b7de", "Fluth", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKNEG", "VMFIN", "NN", "VVFIN", "$,", "VVFIN", "APPR", "ART", "ADJA", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Dr\u00fcben in dem freien Lande frei verspritzen freies Blut!", "tokens": ["Dr\u00fc\u00b7ben", "in", "dem", "frei\u00b7en", "Lan\u00b7de", "frei", "ver\u00b7sprit\u00b7zen", "frei\u00b7es", "Blut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "ADJD", "VVINF", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "Schwestern, weint mit mir! Ich weine \u00fcber meine Ketten nicht.", "tokens": ["Schwes\u00b7tern", ",", "weint", "mit", "mir", "!", "Ich", "wei\u00b7ne", "\u00fc\u00b7ber", "mei\u00b7ne", "Ket\u00b7ten", "nicht", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "PPER", "$.", "PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Sollt' es mich denn gleich zerdr\u00fccken, dieses eiserne Gewicht,", "tokens": ["Sollt'", "es", "mich", "denn", "gleich", "zer\u00b7dr\u00fc\u00b7cken", ",", "die\u00b7ses", "ei\u00b7ser\u00b7ne", "Ge\u00b7wicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADV", "ADV", "VVINF", "$,", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Das so lange hat getragen unser edles Vaterland,", "tokens": ["Das", "so", "lan\u00b7ge", "hat", "ge\u00b7tra\u00b7gen", "un\u00b7ser", "ed\u00b7les", "Va\u00