{"dta.poem.6271": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von der H\u00e4rte und von der  \n Fl\u00fc\u00dfigkeit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das Eisen, das so hart, verdirbt und wird zerst\u00f6hret;", "tokens": ["Das", "Ei\u00b7sen", ",", "das", "so", "hart", ",", "ver\u00b7dirbt", "und", "wird", "zer\u00b7st\u00f6h\u00b7ret", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$,", "VVFIN", "KON", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dringt in dasselbige die Feuchtigkeit.", "tokens": ["Dringt", "in", "das\u00b7sel\u00b7bi\u00b7ge", "die", "Feuch\u00b7tig\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PDAT", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und durch die Hitze wird der feuchte Koht verkehret", "tokens": ["Und", "durch", "die", "Hit\u00b7ze", "wird", "der", "feuch\u00b7te", "Koht", "ver\u00b7keh\u00b7ret"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "VAFIN", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In eine feste Trockenheit.", "tokens": ["In", "ei\u00b7ne", "fes\u00b7te", "Tro\u00b7cken\u00b7heit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Denn wenn die Erde sich verdicket,", "tokens": ["Denn", "wenn", "die", "Er\u00b7de", "sich", "ver\u00b7di\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und ihre C\u00f6rper d\u00fcrr\u2019 und mehr in Ruhe seyn,", "tokens": ["Und", "ih\u00b7re", "C\u00f6r\u00b7per", "d\u00fcrr'", "und", "mehr", "in", "Ru\u00b7he", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ADJD", "KON", "ADV", "APPR", "NN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wann sie die Zeit verbindet und sie dr\u00fccket;", "tokens": ["Wann", "sie", "die", "Zeit", "ver\u00b7bin\u00b7det", "und", "sie", "dr\u00fc\u00b7cket", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "NN", "VVFIN", "KON", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Wird ihre Massa hart, ein Fels, ein Kieselstein.", "tokens": ["Wird", "ih\u00b7re", "Mas\u00b7sa", "hart", ",", "ein", "Fels", ",", "ein", "Kie\u00b7sel\u00b7stein", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJD", "$,", "ART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}