{"textgrid.poem.43956": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ach Kind, verschone mich in dir", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ach Kind, verschone mich in dir", "tokens": ["Ach", "Kind", ",", "ver\u00b7scho\u00b7ne", "mich", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und las mich unbetr\u00fcbt von hier!", "tokens": ["Und", "las", "mich", "un\u00b7be\u00b7tr\u00fcbt", "von", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was qu\u00e4lstu dich mit so viel Thr\u00e4nen?", "tokens": ["Was", "qu\u00e4l\u00b7stu", "dich", "mit", "so", "viel", "Thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es sind die Kr\u00e4fte meiner Brust.", "tokens": ["Es", "sind", "die", "Kr\u00e4f\u00b7te", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, hastu denn bey so viel Sehnen", "tokens": ["Ach", ",", "has\u00b7tu", "denn", "bey", "so", "viel", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "APPR", "ADV", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Noch gar zu meiner Ohnmacht Lust?", "tokens": ["Noch", "gar", "zu", "mei\u00b7ner", "Ohn\u00b7macht", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich bin wohl so genug geplagt,", "tokens": ["Ich", "bin", "wohl", "so", "ge\u00b7nug", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verfolgt, verleumdet und verjagt,", "tokens": ["Ver\u00b7folgt", ",", "ver\u00b7leum\u00b7det", "und", "ver\u00b7jagt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du wilst noch die Angst verst\u00e4rcken?", "tokens": ["Und", "du", "wilst", "noch", "die", "Angst", "ver\u00b7st\u00e4r\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was G\u00fcnther f\u00fchlt, das weis sein Herz,", "tokens": ["Was", "G\u00fcn\u00b7ther", "f\u00fchlt", ",", "das", "weis", "sein", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "PRELS", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich las es kaum die H\u00e4lfte mercken,", "tokens": ["Ich", "las", "es", "kaum", "die", "H\u00e4lf\u00b7te", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst macht ich dir noch sch\u00e4rfern Schmerz.", "tokens": ["Sonst", "macht", "ich", "dir", "noch", "sch\u00e4r\u00b7fern", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Du bist ja meiner Treu gewis,", "tokens": ["Du", "bist", "ja", "mei\u00b7ner", "Treu", "ge\u00b7wis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies ist ein Band vor diesen Ri\u00df,", "tokens": ["Dies", "ist", "ein", "Band", "vor", "die\u00b7sen", "Ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem die Hofnung auch schon heilet.", "tokens": ["An", "dem", "die", "Hof\u00b7nung", "auch", "schon", "hei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, mildre doch nur den Verdru\u00df,", "tokens": ["Ach", ",", "mild\u00b7re", "doch", "nur", "den", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dieweil die Zeit, so jezo theilet,", "tokens": ["Die\u00b7weil", "die", "Zeit", ",", "so", "je\u00b7zo", "thei\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns endlich wieder binden mu\u00df.", "tokens": ["Uns", "end\u00b7lich", "wie\u00b7der", "bin\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Gesezt, du w\u00fcrdest ungetreu,", "tokens": ["Ge\u00b7sezt", ",", "du", "w\u00fcr\u00b7dest", "un\u00b7ge\u00b7treu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wovor doch Gl\u00fcck und Himmel sey,", "tokens": ["Wo\u00b7vor", "doch", "Gl\u00fcck", "und", "Him\u00b7mel", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich k\u00f6nte dich unm\u00f6glich ha\u00dfen;", "tokens": ["Ich", "k\u00f6n\u00b7te", "dich", "un\u00b7m\u00f6g\u00b7lich", "ha\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir w\u00e4r es zwar die \u00e4rgste Pein.", "tokens": ["Mir", "w\u00e4r", "es", "zwar", "die", "\u00e4rgs\u00b7te", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat sie dich, d\u00e4cht ich, doch verla\u00dfen,", "tokens": ["Hat", "sie", "dich", ",", "d\u00e4cht", "ich", ",", "doch", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich um desto treuer seyn.", "tokens": ["Will", "ich", "um", "des\u00b7to", "treu\u00b7er", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ich weis, man tadelt mich darum;", "tokens": ["Ich", "weis", ",", "man", "ta\u00b7delt", "mich", "da\u00b7rum", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PIS", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schilt mich weibisch, jener tumm.", "tokens": ["Der", "schilt", "mich", "wei\u00b7bisch", ",", "je\u00b7ner", "tumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "PDAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gro\u00dfmuth adelt mein Gem\u00fcthe,", "tokens": ["Die", "Gro\u00df\u00b7muth", "a\u00b7delt", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df ich z\u00e4rtlich lieben kan,", "tokens": ["Und", "da\u00df", "ich", "z\u00e4rt\u00b7lich", "lie\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das nehm ich von des Sch\u00f6pfers G\u00fcte", "tokens": ["Das", "nehm", "ich", "von", "des", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wohl vor die gr\u00f6ste Wohlthat an.", "tokens": ["Wohl", "vor", "die", "gr\u00f6s\u00b7te", "Wohlt\u00b7hat", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Sey arm, verla\u00dfen und veracht,", "tokens": ["Sey", "arm", ",", "ver\u00b7la\u00b7\u00dfen", "und", "ver\u00b7acht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "$,", "VVINF", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verliere, was gef\u00e4llig macht,", "tokens": ["Ver\u00b7lie\u00b7re", ",", "was", "ge\u00b7f\u00e4l\u00b7lig", "macht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Las Zahn und Farb und Jugend schwinden,", "tokens": ["Las", "Zahn", "und", "Farb", "und", "Ju\u00b7gend", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Du bleibst in meinen Augen sch\u00f6n", "tokens": ["Du", "bleibst", "in", "mei\u00b7nen", "Au\u00b7gen", "sch\u00f6n"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und solt sie allemahl entz\u00fcnden,", "tokens": ["Und", "solt", "sie", "al\u00b7le\u00b7mahl", "ent\u00b7z\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So lange sie noch ofen stehn.", "tokens": ["So", "lan\u00b7ge", "sie", "noch", "o\u00b7fen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ein Augenblick der s\u00fc\u00dfen Zeit,", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "der", "s\u00fc\u00b7\u00dfen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In welchem mich dein Scherz erfreut,", "tokens": ["In", "wel\u00b7chem", "mich", "dein", "Scherz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gilt mehr als alle Freudenfeste,", "tokens": ["Gilt", "mehr", "als", "al\u00b7le", "Freu\u00b7den\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Dresden, jezt die halbe Welt,", "tokens": ["Wo", "Dres\u00b7den", ",", "jezt", "die", "hal\u00b7be", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz der hohen Hochzeitg\u00e4ste", "tokens": ["Das", "Herz", "der", "ho\u00b7hen", "Hoch\u00b7zeit\u00b7g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mit tausend Wollust unterh\u00e4lt.", "tokens": ["Mit", "tau\u00b7send", "Wol\u00b7lust", "un\u00b7ter\u00b7h\u00e4lt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Fr\u00fchling ist nun nicht mehr weit;", "tokens": ["Der", "Fr\u00fch\u00b7ling", "ist", "nun", "nicht", "mehr", "weit", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "PTKNEG", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Spazier in gr\u00fcner Einsamkeit", "tokens": ["Spa\u00b7zier", "in", "gr\u00fc\u00b7ner", "Ein\u00b7sam\u00b7keit"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In euren sch\u00f6nen Erleng\u00e4ngen", "tokens": ["In", "eu\u00b7ren", "sch\u00f6\u00b7nen", "Er\u00b7len\u00b7g\u00e4n\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und denck in allem Ungemach,", "tokens": ["Und", "denck", "in", "al\u00b7lem", "Un\u00b7ge\u00b7mach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "APPR", "PIS", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So sehr dich Neid und Freunde dr\u00e4ngen,", "tokens": ["So", "sehr", "dich", "Neid", "und", "Freun\u00b7de", "dr\u00e4n\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Den oft gegebnen Lehren nach.", "tokens": ["Den", "oft", "ge\u00b7geb\u00b7nen", "Leh\u00b7ren", "nach", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Dort soll der jungen V\u00f6gel Schreyn", "tokens": ["Dort", "soll", "der", "jun\u00b7gen", "V\u00f6\u00b7gel", "Schreyn"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Botschaft meiner Sehnsucht seyn,", "tokens": ["Die", "Bot\u00b7schaft", "mei\u00b7ner", "Sehn\u00b7sucht", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und scherzt der West mit Kleid und Wangen,", "tokens": ["Und", "scherzt", "der", "West", "mit", "Kleid", "und", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So wi\u00df und glaube sicherlich:", "tokens": ["So", "wi\u00df", "und", "glau\u00b7be", "si\u00b7cher\u00b7lich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "VVFIN", "PRF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Er meldet dir mein hei\u00df Verlangen", "tokens": ["Er", "mel\u00b7det", "dir", "mein", "hei\u00df", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und k\u00fcst dich tausendmahl vor mich.", "tokens": ["Und", "k\u00fcst", "dich", "tau\u00b7send\u00b7mahl", "vor", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Ach Kind, verschone mich in dir", "tokens": ["Ach", "Kind", ",", "ver\u00b7scho\u00b7ne", "mich", "in", "dir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ITJ", "NN", "$,", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und las mich unbetr\u00fcbt von hier!", "tokens": ["Und", "las", "mich", "un\u00b7be\u00b7tr\u00fcbt", "von", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "APPR", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Was qu\u00e4lstu dich mit so viel Thr\u00e4nen?", "tokens": ["Was", "qu\u00e4l\u00b7stu", "dich", "mit", "so", "viel", "Thr\u00e4\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "ADV", "PIAT", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Es sind die Kr\u00e4fte meiner Brust.", "tokens": ["Es", "sind", "die", "Kr\u00e4f\u00b7te", "mei\u00b7ner", "Brust", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ach, hastu denn bey so viel Sehnen", "tokens": ["Ach", ",", "has\u00b7tu", "denn", "bey", "so", "viel", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "VAFIN", "ADV", "APPR", "ADV", "PIAT", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.6": {"text": "Noch gar zu meiner Ohnmacht Lust?", "tokens": ["Noch", "gar", "zu", "mei\u00b7ner", "Ohn\u00b7macht", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Ich bin wohl so genug geplagt,", "tokens": ["Ich", "bin", "wohl", "so", "ge\u00b7nug", "ge\u00b7plagt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Verfolgt, verleumdet und verjagt,", "tokens": ["Ver\u00b7folgt", ",", "ver\u00b7leum\u00b7det", "und", "ver\u00b7jagt", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVPP", "KON", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und du wilst noch die Angst verst\u00e4rcken?", "tokens": ["Und", "du", "wilst", "noch", "die", "Angst", "ver\u00b7st\u00e4r\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VMFIN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was G\u00fcnther f\u00fchlt, das weis sein Herz,", "tokens": ["Was", "G\u00fcn\u00b7ther", "f\u00fchlt", ",", "das", "weis", "sein", "Herz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NE", "VVFIN", "$,", "PRELS", "PTKVZ", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich las es kaum die H\u00e4lfte mercken,", "tokens": ["Ich", "las", "es", "kaum", "die", "H\u00e4lf\u00b7te", "mer\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Sonst macht ich dir noch sch\u00e4rfern Schmerz.", "tokens": ["Sonst", "macht", "ich", "dir", "noch", "sch\u00e4r\u00b7fern", "Schmerz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.12": {"line.1": {"text": "Du bist ja meiner Treu gewis,", "tokens": ["Du", "bist", "ja", "mei\u00b7ner", "Treu", "ge\u00b7wis", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dies ist ein Band vor diesen Ri\u00df,", "tokens": ["Dies", "ist", "ein", "Band", "vor", "die\u00b7sen", "Ri\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "An dem die Hofnung auch schon heilet.", "tokens": ["An", "dem", "die", "Hof\u00b7nung", "auch", "schon", "hei\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "ADV", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ach, mildre doch nur den Verdru\u00df,", "tokens": ["Ach", ",", "mild\u00b7re", "doch", "nur", "den", "Ver\u00b7dru\u00df", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVFIN", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.5": {"text": "Dieweil die Zeit, so jezo theilet,", "tokens": ["Die\u00b7weil", "die", "Zeit", ",", "so", "je\u00b7zo", "thei\u00b7let", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Uns endlich wieder binden mu\u00df.", "tokens": ["Uns", "end\u00b7lich", "wie\u00b7der", "bin\u00b7den", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.13": {"line.1": {"text": "Gesezt, du w\u00fcrdest ungetreu,", "tokens": ["Ge\u00b7sezt", ",", "du", "w\u00fcr\u00b7dest", "un\u00b7ge\u00b7treu", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "PPER", "VAFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wovor doch Gl\u00fcck und Himmel sey,", "tokens": ["Wo\u00b7vor", "doch", "Gl\u00fcck", "und", "Him\u00b7mel", "sey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "NN", "KON", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich k\u00f6nte dich unm\u00f6glich ha\u00dfen;", "tokens": ["Ich", "k\u00f6n\u00b7te", "dich", "un\u00b7m\u00f6g\u00b7lich", "ha\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PRF", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mir w\u00e4r es zwar die \u00e4rgste Pein.", "tokens": ["Mir", "w\u00e4r", "es", "zwar", "die", "\u00e4rgs\u00b7te", "Pein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Hat sie dich, d\u00e4cht ich, doch verla\u00dfen,", "tokens": ["Hat", "sie", "dich", ",", "d\u00e4cht", "ich", ",", "doch", "ver\u00b7la\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "$,", "VVFIN", "PPER", "$,", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Will ich um desto treuer seyn.", "tokens": ["Will", "ich", "um", "des\u00b7to", "treu\u00b7er", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "ADV", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.14": {"line.1": {"text": "Ich weis, man tadelt mich darum;", "tokens": ["Ich", "weis", ",", "man", "ta\u00b7delt", "mich", "da\u00b7rum", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PTKVZ", "$,", "PIS", "VVFIN", "PRF", "PAV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der schilt mich weibisch, jener tumm.", "tokens": ["Der", "schilt", "mich", "wei\u00b7bisch", ",", "je\u00b7ner", "tumm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "$,", "PDAT", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Gro\u00dfmuth adelt mein Gem\u00fcthe,", "tokens": ["Die", "Gro\u00df\u00b7muth", "a\u00b7delt", "mein", "Ge\u00b7m\u00fc\u00b7the", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und da\u00df ich z\u00e4rtlich lieben kan,", "tokens": ["Und", "da\u00df", "ich", "z\u00e4rt\u00b7lich", "lie\u00b7ben", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das nehm ich von des Sch\u00f6pfers G\u00fcte", "tokens": ["Das", "nehm", "ich", "von", "des", "Sch\u00f6p\u00b7fers", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "