s": ["KON", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So lange sie noch ofen stehn.", "tokens": ["So", "lan\u00b7ge", "sie", "noch", "o\u00b7fen", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ADV", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.16": {"line.1": {"text": "Ein Augenblick der s\u00fc\u00dfen Zeit,", "tokens": ["Ein", "Au\u00b7gen\u00b7blick", "der", "s\u00fc\u00b7\u00dfen", "Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "In welchem mich dein Scherz erfreut,", "tokens": ["In", "wel\u00b7chem", "mich", "dein", "Scherz", "er\u00b7freut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gilt mehr als alle Freudenfeste,", "tokens": ["Gilt", "mehr", "als", "al\u00b7le", "Freu\u00b7den\u00b7fes\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "KOKOM", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Dresden, jezt die halbe Welt,", "tokens": ["Wo", "Dres\u00b7den", ",", "jezt", "die", "hal\u00b7be", "Welt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "$,", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das Herz der hohen Hochzeitg\u00e4ste", "tokens": ["Das", "Herz", "der", "ho\u00b7hen", "Hoch\u00b7zeit\u00b7g\u00e4s\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "