{"textgrid.poem.63330": {"metadata": {"author": {"name": "Klabund", "birth": "N.A.", "death": "N.A."}, "title": "Das Wasserm\u00e4del", "genre": "verse", "period": "N.A.", "pub_year": 1909, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich liebe ein Wasserm\u00e4del vom Caf\u00e9 Arkadia,", "tokens": ["Ich", "lie\u00b7be", "ein", "Was\u00b7ser\u00b7m\u00e4\u00b7del", "vom", "Ca\u00b7f\u00e9", "Ar\u00b7ka\u00b7dia", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NE", "NE", "$,"], "meter": "-+--+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Bin siebzehn Jahr'", "tokens": ["Bin", "sieb\u00b7zehn", "Jahr'"], "token_info": ["word", "word", "word"], "pos": ["VAFIN", "CARD", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Und erstes Semester in M\u00fcnchen.", "tokens": ["Und", "ers\u00b7tes", "Se\u00b7mes\u00b7ter", "in", "M\u00fcn\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "NE", "$."], "meter": "-+-+---+-", "measure": "unknown.measure.tri"}, "line.4": {"text": "Ich kann mein Herz nicht mit Erfahrungen \u00fcbert\u00fcnchen,", "tokens": ["Ich", "kann", "mein", "Herz", "nicht", "mit", "Er\u00b7fah\u00b7run\u00b7gen", "\u00fc\u00b7ber\u00b7t\u00fcn\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "PTKNEG", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Und immer, wenn sich eine mir freundlich zugewandt:", "tokens": ["Und", "im\u00b7mer", ",", "wenn", "sich", "ei\u00b7ne", "mir", "freund\u00b7lich", "zu\u00b7ge\u00b7wandt", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PRF", "ART", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Ein Kind vor dem Christbaum oder vor den Glaskugeln im Parke stand.", "tokens": ["Ein", "Kind", "vor", "dem", "Christ\u00b7baum", "o\u00b7der", "vor", "den", "Glas\u00b7ku\u00b7geln", "im", "Par\u00b7ke", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "APPRART", "NN", "VVFIN", "$."], "meter": "-+--+-+-+-+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.7": {"text": "Oder ich sah blaue Pferde, erstaunlichstes Getier.", "tokens": ["O\u00b7der", "ich", "sah", "blau\u00b7e", "Pfer\u00b7de", ",", "er\u00b7staun\u00b7lichs\u00b7tes", "Ge\u00b7tier", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+---+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "Eine Stute mit schlanken Fohlen sprang spielerisch zu mir.", "tokens": ["Ei\u00b7ne", "Stu\u00b7te", "mit", "schlan\u00b7ken", "Foh\u00b7len", "sprang", "spie\u00b7le\u00b7risch", "zu", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ADJA", "NN", "VVFIN", "ADJD", "APPR", "PPER", "$."], "meter": "+-+--+-+-++-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.9": {"text": "Und als das Wasserm\u00e4del schlief bei mir zur Nacht \u2013", "tokens": ["Und", "als", "das", "Was\u00b7ser\u00b7m\u00e4\u00b7del", "schlief", "bei", "mir", "zur", "Nacht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "VVFIN", "APPR", "PPER", "APPRART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "War sie Jungfrau? Oder hatte sie sich zur Jungfrau gemacht?", "tokens": ["War", "sie", "Jung\u00b7frau", "?", "O\u00b7der", "hat\u00b7te", "sie", "sich", "zur", "Jung\u00b7frau", "ge\u00b7macht", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "NN", "$.", "NE", "VAFIN", "PPER", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "--+-+-+--+--+-+", "measure": "iambic.hexa.relaxed"}, "line.11": {"text": "Sie war mir wie ein L\u00e4cheln im Dunkel zugetan...", "tokens": ["Sie", "war", "mir", "wie", "ein", "L\u00e4\u00b7cheln", "im", "Dun\u00b7kel", "zu\u00b7ge\u00b7tan", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "KOKOM", "ART", "NN", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wei\u00dfes Segelboot... S\u00fcdwind wehte um unsere Rahn...", "tokens": ["Wei\u00b7\u00dfes", "Se\u00b7gel\u00b7boot", "...", "S\u00fcd\u00b7wind", "weh\u00b7te", "um", "un\u00b7se\u00b7re", "Rahn", "..."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$(", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.13": {"text": "Die ewige F\u00f6hrde lag im Morgenscheine da...", "tokens": ["Die", "e\u00b7wi\u00b7ge", "F\u00f6hr\u00b7de", "lag", "im", "Mor\u00b7gen\u00b7schei\u00b7ne", "da", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPRART", "NN", "ADV", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "Ich liebe ein Wasserm\u00e4del vom Caf\u00e9 Arkadia.", "tokens": ["Ich", "lie\u00b7be", "ein", "Was\u00b7ser\u00b7m\u00e4\u00b7del", "vom", "Ca\u00b7f\u00