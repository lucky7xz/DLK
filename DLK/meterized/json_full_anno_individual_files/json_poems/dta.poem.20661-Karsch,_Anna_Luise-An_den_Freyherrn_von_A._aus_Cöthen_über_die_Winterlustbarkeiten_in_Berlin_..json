{"dta.poem.20661": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "An  \n  den Freyherrn von A. aus C\u00f6then  \n  \u00fcber  \n  die Winterlustbarkeiten  \n in Berlin .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du, dessen Auge nichts verr\u00e4th                 ", "tokens": ["Du", ",", "des\u00b7sen", "Au\u00b7ge", "nichts", "ver\u00b7r\u00e4\u00b7th"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELAT", "NN", "PIS", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom Stolze, den so manche Brust bewirthet,", "tokens": ["Vom", "Stol\u00b7ze", ",", "den", "so", "man\u00b7che", "Brust", "be\u00b7wirt\u00b7het", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "$,", "PRELS", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch Ordensb\u00e4nder aufgebl\u00e4ht,", "tokens": ["Durch", "Or\u00b7dens\u00b7b\u00e4n\u00b7der", "auf\u00b7ge\u00b7bl\u00e4ht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Womit sie ward umg\u00fcrtet.", "tokens": ["Wo\u00b7mit", "sie", "ward", "um\u00b7g\u00fcr\u00b7tet", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "O A*! Dein Herz verschlie\u00dft", "tokens": ["O", "A", "*", "!", "Dein", "Herz", "ver\u00b7schlie\u00dft"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NE", "XY", "XY", "$.", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Sich nimmer, wenn die Freuden Dich umgeben,", "tokens": ["Sich", "nim\u00b7mer", ",", "wenn", "die", "Freu\u00b7den", "Dich", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "$,", "KOUS", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Weise braucht sie, und vers\u00fc\u00dft", "tokens": ["Der", "Wei\u00b7se", "braucht", "sie", ",", "und", "ver\u00b7s\u00fc\u00dft"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich gern dadurch das Leben.", "tokens": ["Sich", "gern", "da\u00b7durch", "das", "Le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "PAV", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Dich reizt Dein Landgut, wenn im May", "tokens": ["Dich", "reizt", "Dein", "Land\u00b7gut", ",", "wenn", "im", "May"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die V\u00f6gel aus den schattigen Geb\u00fcschen", "tokens": ["Die", "V\u00f6\u00b7gel", "aus", "den", "schat\u00b7ti\u00b7gen", "Ge\u00b7b\u00fc\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mit eines Sch\u00e4fers Feldschalmei", "tokens": ["Mit", "ei\u00b7nes", "Sch\u00e4\u00b7fers", "Feld\u00b7schal\u00b7mei"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihr tonreich Lied vermischen.", "tokens": ["Ihr", "ton\u00b7reich", "Lied", "ver\u00b7mi\u00b7schen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Und Deine Rinderheerde satt", "tokens": ["Und", "Dei\u00b7ne", "Rin\u00b7der\u00b7heer\u00b7de", "satt"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Im Blumenthal beim Bache lieblich br\u00fcllet:", "tokens": ["Im", "Blu\u00b7ment\u00b7hal", "beim", "Ba\u00b7che", "lieb\u00b7lich", "br\u00fcl\u00b7let", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "NN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jezt aber reizt Dich Friedrichs Stadt", "tokens": ["Jezt", "a\u00b7ber", "reizt", "Dich", "Fried\u00b7richs", "Stadt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "NE", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Spiel und Tanz erf\u00fcllet.", "tokens": ["Mit", "Spiel", "und", "Tanz", "er\u00b7f\u00fcl\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Jezt ladet Dich der Singe-Saal", "tokens": ["Jezt", "la\u00b7det", "Dich", "der", "Sin\u00b7ge\u00b7Saal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des Helden ein, der \u00fcber andre gl\u00e4nzet,", "tokens": ["Des", "Hel\u00b7den", "ein", ",", "der", "\u00fc\u00b7ber", "and\u00b7re", "gl\u00e4n\u00b7zet", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKVZ", "$,", "PRELS", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Wie Ph\u00f6bus, wenn der goldne Strahl", "tokens": ["Wie", "Ph\u00f6\u00b7bus", ",", "wenn", "der", "gold\u00b7ne", "Strahl"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "$,", "KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sein lokkigt Haupt bekr\u00e4nzet", "tokens": ["Sein", "lok\u00b7kigt", "Haupt", "be\u00b7kr\u00e4n\u00b7zet"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "VVFIN", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Die Sterne ringsumher besch\u00e4mt;", "tokens": ["Die", "Ster\u00b7ne", "rings\u00b7um\u00b7her", "be\u00b7sch\u00e4mt", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hier herrschen hohe k\u00f6nigliche Freuden,", "tokens": ["Hier", "herr\u00b7schen", "ho\u00b7he", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Freu\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und selbst der B\u00fcrger, der sich gr\u00e4mt,", "tokens": ["Und", "selbst", "der", "B\u00fcr\u00b7ger", ",", "der", "sich", "gr\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "PRELS", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Verstaunt hier seine Leiden;", "tokens": ["Ver\u00b7staunt", "hier", "sei\u00b7ne", "Lei\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Vergi\u00dft den Mangel, der ihn dr\u00fcckt,", "tokens": ["Ver\u00b7gi\u00dft", "den", "Man\u00b7gel", ",", "der", "ihn", "dr\u00fcckt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und st\u00fcrzt sich mit der zahlenlosen Menge", "tokens": ["Und", "st\u00fcrzt", "sich", "mit", "der", "zah\u00b7len\u00b7lo\u00b7sen", "Men\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Ans Schauspielhaus, und wird erquickt", "tokens": ["Ans", "Schau\u00b7spiel\u00b7haus", ",", "und", "wird", "er\u00b7quickt"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "KON", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Vom Wohlklang der Ges\u00e4nge.", "tokens": ["Vom", "Wohl\u00b7klang", "der", "Ge\u00b7s\u00e4n\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}}}}}