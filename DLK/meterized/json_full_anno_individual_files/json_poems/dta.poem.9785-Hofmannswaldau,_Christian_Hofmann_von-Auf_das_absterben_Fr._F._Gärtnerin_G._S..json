{"dta.poem.9785": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Auf das absterben Fr. F. G\u00e4rtnerin/ G. S.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Frau G\u00e4rtnern/ ihrer grufft geb\u00fchrt ein blumen-garten/", "tokens": ["Frau", "G\u00e4rt\u00b7nern", "/", "ih\u00b7rer", "grufft", "ge\u00b7b\u00fchrt", "ein", "blu\u00b7men\u00b7gar\u00b7ten", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "$(", "PPOSAT", "NN", "VVFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Flora solte selbst mit ihr zu grabe gehn/", "tokens": ["Die", "Flo\u00b7ra", "sol\u00b7te", "selbst", "mit", "ihr", "zu", "gra\u00b7be", "gehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "ADV", "APPR", "PPER", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihrer bilder glantz/ die blumen bester arten/", "tokens": ["Und", "ih\u00b7rer", "bil\u00b7der", "glantz", "/", "die", "blu\u00b7men", "bes\u00b7ter", "ar\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "$(", "ART", "NN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Um ihren zarten leib in voller bl\u00fcthe stehn:", "tokens": ["Um", "ih\u00b7ren", "zar\u00b7ten", "leib", "in", "vol\u00b7ler", "bl\u00fc\u00b7the", "stehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Es solten tulipen/ violen und narcissen/", "tokens": ["Es", "sol\u00b7ten", "tu\u00b7li\u00b7pen", "/", "vi\u00b7o\u00b7len", "und", "nar\u00b7cis\u00b7sen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "VVINF", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jasmin und lilien die blassen schl\u00e4ffe schl\u00fcssen.", "tokens": ["Jas\u00b7min", "und", "li\u00b7li\u00b7en", "die", "blas\u00b7sen", "schl\u00e4f\u00b7fe", "schl\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "KON", "VVFIN", "ART", "ADJA", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil aber strenger frost die sternen unsrer auen/", "tokens": ["Weil", "a\u00b7ber", "stren\u00b7ger", "frost", "die", "ster\u00b7nen", "uns\u00b7rer", "au\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADJA", "NN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Chloris kinder itzt mit kaltem ei\u00df beschwert/", "tokens": ["Der", "Chlo\u00b7ris", "kin\u00b7der", "itzt", "mit", "kal\u00b7tem", "ei\u00df", "be\u00b7schwert", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wo der fr\u00fchling sonst lust-h\u00e4user pflegt zu bauen/", "tokens": ["Und", "wo", "der", "fr\u00fch\u00b7ling", "sonst", "lust\u00b7h\u00e4u\u00b7ser", "pflegt", "zu", "bau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "VVFIN", "ADV", "ADJD", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ein blumen-feind/ der nord/ durch unsre g\u00e4rten f\u00e4hrt;", "tokens": ["Ein", "blu\u00b7men\u00b7feind", "/", "der", "nord", "/", "durch", "uns\u00b7re", "g\u00e4r\u00b7ten", "f\u00e4hrt", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "NN", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Bem\u00fch\u2019 ich mich umsonst ihr leichen-tuch zu mahlen/", "tokens": ["Be\u00b7m\u00fch'", "ich", "mich", "um\u00b7sonst", "ihr", "lei\u00b7chen\u00b7tuch", "zu", "mah\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "PPOSAT", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und diesen letzten dienst mit blumen abzuzahlen.", "tokens": ["Und", "die\u00b7sen", "letz\u00b7ten", "dienst", "mit", "blu\u00b7men", "ab\u00b7zu\u00b7zah\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDAT", "ADJA", "NN", "APPR", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Zu dem ist meine harff und leyer gantz verstimmet/", "tokens": ["Zu", "dem", "ist", "mei\u00b7ne", "harff", "und", "le\u00b7yer", "gantz", "ver\u00b7stim\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "VAFIN", "PPOSAT", "NN", "KON", "ADJD", "ADV", "VVFIN", "$("], "meter": "-+-+-+--+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "In iede fuge f\u00fcgt sich ach und winseln ein;", "tokens": ["In", "ie\u00b7de", "fu\u00b7ge", "f\u00fcgt", "sich", "ach", "und", "win\u00b7seln", "ein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "VVFIN", "PRF", "XY", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil der betr\u00fcbte fall ihr allen klang benimmet/", "tokens": ["Weil", "der", "be\u00b7tr\u00fcb\u00b7te", "fall", "ihr", "al\u00b7len", "klang", "be\u00b7nim\u00b7met", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPER", "PIS", "VVFIN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Geschweige da\u00df ich ihr ietzt blumen solte streu\u2019n/", "tokens": ["Ge\u00b7schwei\u00b7ge", "da\u00df", "ich", "ihr", "ietzt", "blu\u00b7men", "sol\u00b7te", "streu'n", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "PPER", "PPER", "ADV", "VVINF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die denen nur/ so sich der himmel eingego\u00dfen", "tokens": ["Die", "de\u00b7nen", "nur", "/", "so", "sich", "der", "him\u00b7mel", "ein\u00b7ge\u00b7go\u00b7\u00dfen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "PRELS", "ADV", "$(", "ADV", "PRF", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und Castalis getr\u00e4nckt/ aus ihren federn spro\u00dfen.", "tokens": ["Und", "Cas\u00b7ta\u00b7lis", "ge\u00b7tr\u00e4nckt", "/", "aus", "ih\u00b7ren", "fe\u00b7dern", "spro\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "VVPP", "$(", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Doch ich erinnre mich/ du kern der klugen frauen/", "tokens": ["Doch", "ich", "e\u00b7rinn\u00b7re", "mich", "/", "du", "kern", "der", "klu\u00b7gen", "frau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "$(", "PPER", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Was vor ein kluges wort aus deinem munde ging;", "tokens": ["Was", "vor", "ein", "klu\u00b7ges", "wort", "aus", "dei\u00b7nem", "mun\u00b7de", "ging", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als das verh\u00e4ngni\u00df dir mit kranckheit fieng zu drauen/", "tokens": ["Als", "das", "ver\u00b7h\u00e4ng\u00b7ni\u00df", "dir", "mit", "kran\u00b7ck\u00b7heit", "fi\u00b7eng", "zu", "drau\u00b7en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "VVFIN", "PPER", "APPR", "NN", "VVFIN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.4": {"text": "Und sich der marter-stein an deine seiten hieng;", "tokens": ["Und", "sich", "der", "mar\u00b7ter\u00b7stein", "an", "dei\u00b7ne", "sei\u00b7ten", "hieng", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wie du im geiste schon das haus der ruh\u2019 erblicket/", "tokens": ["Wie", "du", "im", "geis\u00b7te", "schon", "das", "haus", "der", "ruh'", "er\u00b7bli\u00b7cket", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "APPRART", "ADJA", "ADV", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und diesen schwanen-thon den wolcken zugeschicket:", "tokens": ["Und", "die\u00b7sen", "schwa\u00b7nen\u00b7thon", "den", "wol\u00b7cken", "zu\u00b7ge\u00b7schi\u00b7cket", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Mein blumen-garten tr\u00e4gt nur eitel granadillen/", "tokens": ["Mein", "blu\u00b7men\u00b7gar\u00b7ten", "tr\u00e4gt", "nur", "ei\u00b7tel", "gra\u00b7na\u00b7dil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "VVFIN", "ADV", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der allerliebste freund/ mein G\u00e4rtner/ ist dahin;", "tokens": ["Der", "al\u00b7ler\u00b7liebs\u00b7te", "freund", "/", "mein", "G\u00e4rt\u00b7ner", "/", "ist", "da\u00b7hin", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$(", "PPOSAT", "NN", "$(", "VAFIN", "PAV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Es kan kein Aesculap die trauer-schmertzen stillen/", "tokens": ["Es", "kan", "kein", "A\u00b7e\u00b7scu\u00b7lap", "die", "trau\u00b7e\u00b7rschmert\u00b7zen", "stil\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PIAT", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Bi\u00df da\u00df mein monde wird zu seiner sonnen ziehn:", "tokens": ["Bi\u00df", "da\u00df", "mein", "mon\u00b7de", "wird", "zu", "sei\u00b7ner", "son\u00b7nen", "ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "VAFIN", "APPR", "PPOSAT", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df von keiner lust im marter-haus der erden/", "tokens": ["Ich", "wei\u00df", "von", "kei\u00b7ner", "lust", "im", "mar\u00b7ter\u00b7haus", "der", "er\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PIS", "VVFIN", "APPRART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Bis dieser sieche leib wird staub und asche werden.", "tokens": ["Bis", "die\u00b7ser", "sie\u00b7che", "leib", "wird", "staub", "und", "asc\u00b7he", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "VAFIN", "ADJD", "KON", "ADJA", "VAINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "So nimm nun asch und staub/ prophetin/ deiner leiche", "tokens": ["So", "nimm", "nun", "asch", "und", "staub", "/", "pro\u00b7phe\u00b7tin", "/", "dei\u00b7ner", "lei\u00b7che"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["ADV", "VVIMP", "ADV", "ADJD", "KON", "VVFIN", "$(", "NE", "$(", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von meiner schwachen hand/ statt fr\u00fchlings-blumen/ an/", "tokens": ["Von", "mei\u00b7ner", "schwa\u00b7chen", "hand", "/", "statt", "fr\u00fch\u00b7lings\u00b7blu\u00b7men", "/", "an", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$(", "APPR", "ADJA", "$(", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die ich nach landes-art mitleidend \u00fcberreiche/", "tokens": ["Die", "ich", "nach", "lan\u00b7des\u00b7art", "mit\u00b7lei\u00b7dend", "\u00fc\u00b7berr\u00b7ei\u00b7che", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NE", "VVPP", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und bahn der werthen asch aus aschen eine bahn;", "tokens": ["Und", "bahn", "der", "wert\u00b7hen", "asch", "aus", "asc\u00b7hen", "ei\u00b7ne", "bahn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "VVFIN", "ADJD", "APPR", "ADJA", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Geh\u2019 Artemisia zu deines mannes grabe/", "tokens": ["Geh'", "Ar\u00b7te\u00b7mi\u00b7sia", "zu", "dei\u00b7nes", "man\u00b7nes", "gra\u00b7be", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Damit sich deine glut in seiner aschen labe.", "tokens": ["Da\u00b7mit", "sich", "dei\u00b7ne", "glut", "in", "sei\u00b7ner", "asc\u00b7hen", "la\u00b7be", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Was uns das heydenthum von todten-asche lehret/", "tokens": ["Was", "uns", "das", "hey\u00b7den\u00b7thum", "von", "tod\u00b7ten\u00b7a\u00b7sche", "leh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "APPR", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bevor das g\u00f6ldne Rom/ die k\u00f6nigin der welt/", "tokens": ["Be\u00b7vor", "das", "g\u00f6ld\u00b7ne", "Rom", "/", "die", "k\u00f6\u00b7ni\u00b7gin", "der", "welt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NE", "$(", "ART", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So ihrer keyser asch\u2019 in golde hat geehret/", "tokens": ["So", "ih\u00b7rer", "key\u00b7ser", "asch'", "in", "gol\u00b7de", "hat", "ge\u00b7eh\u00b7ret", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "W\u00fcrd\u2019 ihrer aschen auch nicht unrecht angestellt;", "tokens": ["W\u00fcrd'", "ih\u00b7rer", "asc\u00b7hen", "auch", "nicht", "un\u00b7recht", "an\u00b7ge\u00b7stellt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "ADV", "PTKNEG", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wenn nicht des himmels braut/ die g\u00f6ttin ihrer h\u00fctten/", "tokens": ["Wenn", "nicht", "des", "him\u00b7mels", "braut", "/", "die", "g\u00f6t\u00b7tin", "ih\u00b7rer", "h\u00fct\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos"