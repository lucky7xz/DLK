, "Held", ",", "der", "zu", "jed\u00b7we\u00b7der", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "PRELS", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Starkm\u00fcthig trotzt den t\u00f6dtlichen Gewittern!", "tokens": ["Stark\u00b7m\u00fct\u00b7hig", "trotzt", "den", "t\u00f6dt\u00b7li\u00b7chen", "Ge\u00b7wit\u00b7tern", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.3": {"text": "Ein Weib nur, mu\u00df ich vor dem Worte zittern,", "tokens": ["Ein", "Weib", "nur", ",", "mu\u00df", "ich", "vor", "dem", "Wor\u00b7te", "zit\u00b7tern", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "VMFIN", "PPER", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Mit welchem die Vernichtung selbst im Bunde!", "tokens": ["Mit", "wel\u00b7chem", "die", "Ver\u00b7nich\u00b7tung", "selbst", "im", "Bun\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "ART", "NN", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Das ist's, was alle Gl\u00fcckessaaten reutet", "tokens": ["Das", "ist's", ",", "was", "al\u00b7le", "Gl\u00fc\u00b7ckes\u00b7saa\u00b7ten", "reu\u00b7tet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "$,", "PRELS", "PIAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Aus meiner Brust! das l\u00e4hmt mein bestes Streben,", "tokens": ["Aus", "mei\u00b7ner", "Brust", "!", "das", "l\u00e4hmt", "mein", 