{"textgrid.poem.25941": {"metadata": {"author": {"name": "Dauthendey, Max", "birth": "N.A.", "death": "N.A."}, "title": "Die blaue Kornblum wohnt versteckt", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die blaue Kornblum wohnt versteckt,", "tokens": ["Die", "blau\u00b7e", "Korn\u00b7blum", "wohnt", "ver\u00b7steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "So hab ich meinen Schatz entdeckt.", "tokens": ["So", "hab", "ich", "mei\u00b7nen", "Schatz", "ent\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie kann nicht meinen H\u00e4nden wehren,", "tokens": ["Sie", "kann", "nicht", "mei\u00b7nen", "H\u00e4n\u00b7den", "weh\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wiegt sie wie's Sommerfeld die \u00c4hren.", "tokens": ["Wiegt", "sie", "wie's", "Som\u00b7mer\u00b7feld", "die", "\u00c4h\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJA", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Die \u00c4hren sind jetzt k\u00f6rnerschwer,", "tokens": ["Die", "\u00c4h\u00b7ren", "sind", "jetzt", "k\u00f6r\u00b7ner\u00b7schwer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Als l\u00e4g schon Brot mannshoch umher,", "tokens": ["Als", "l\u00e4g", "schon", "Brot", "manns\u00b7hoch", "um\u00b7her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "NN", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nahrhaft wie im B\u00e4ckerhaus", "tokens": ["Und", "nahr\u00b7haft", "wie", "im", "B\u00e4\u00b7cker\u00b7haus"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KOKOM", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sieht's an der langen Landstra\u00df aus.", "tokens": ["Sieht's", "an", "der", "lan\u00b7gen", "Land\u00b7stra\u00df", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Mein Schatz die \u00c4hren streicheln tut.", "tokens": ["Mein", "Schatz", "die", "\u00c4h\u00b7ren", "strei\u00b7cheln", "tut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "\u00bbnach Leben riechen sie so gut,\u00ab", "tokens": ["\u00bb", "nach", "Le\u00b7ben", "rie\u00b7chen", "sie", "so", "gut", ",", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$,", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Sagt sie. Und schau ich roten Mohn,", "tokens": ["Sagt", "sie", ".", "Und", "schau", "ich", "ro\u00b7ten", "Mohn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KON", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12":