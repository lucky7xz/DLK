{"textgrid.poem.67793": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Liebe", "genre": "verse", "period": "N.A.", "pub_year": 1790, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "H\u00e4tt' ich Menschen-, h\u00e4tt' ich Engelzungen,", "tokens": ["H\u00e4tt'", "ich", "Men\u00b7schen", ",", "h\u00e4tt'", "ich", "En\u00b7gel\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "TRUNC", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcrde Gottes Lob von mir gesungen", "tokens": ["W\u00fcr\u00b7de", "Got\u00b7tes", "Lob", "von", "mir", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Sternen-, wie des Himmels Sang,", "tokens": ["Wie", "ein", "Ster\u00b7nen", ",", "wie", "des", "Him\u00b7mels", "Sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "TRUNC", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir fehlete die Liebe,", "tokens": ["Und", "mir", "feh\u00b7le\u00b7te", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Liebe:", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ohne Dich sind meine Lieder todter Schellenklang!", "tokens": ["Oh\u00b7ne", "Dich", "sind", "mei\u00b7ne", "Lie\u00b7der", "tod\u00b7ter", "Schel\u00b7len\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.2": {"line.1": {"text": "H\u00e4tt' ich Prophezeihung, alle Tiefen", "tokens": ["H\u00e4tt'", "ich", "Pro\u00b7phe\u00b7zei\u00b7hung", ",", "al\u00b7le", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Geheimnisse, Erkenntni\u00dftiefen,", "tokens": ["Der", "Ge\u00b7heim\u00b7nis\u00b7se", ",", "Er\u00b7kennt\u00b7ni\u00df\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Berge zu versetzen, h\u00e4tt' ich Macht,", "tokens": ["Ber\u00b7ge", "zu", "ver\u00b7set\u00b7zen", ",", "h\u00e4tt'", "ich", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir fehlete die Liebe,", "tokens": ["Und", "mir", "feh\u00b7le\u00b7te", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Liebe:", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ohne Dich w\u00e4r all mein Glaube, all mein Wissen Nacht!", "tokens": ["Oh\u00b7ne", "Dich", "w\u00e4r", "all", "mein", "Glau\u00b7be", ",", "all", "mein", "Wis\u00b7sen", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.3": {"line.1": {"text": "G\u00e4b' ich Armen alle meine Habe,", "tokens": ["G\u00e4b'", "ich", "Ar\u00b7men", "al\u00b7le", "mei\u00b7ne", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00e4be meinen Leib zur Gottesgabe", "tokens": ["G\u00e4\u00b7be", "mei\u00b7nen", "Leib", "zur", "Got\u00b7tes\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Preis dem Feuer, lachete der Gluth,", "tokens": ["Preis", "dem", "Feu\u00b7er", ",", "la\u00b7che\u00b7te", "der", "Gluth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir fehlete die Liebe,", "tokens": ["Und", "mir", "feh\u00b7le\u00b7te", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Liebe:", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ohne Dich ist Thun und Leiden leere, blinde Wuth!", "tokens": ["Oh\u00b7ne", "Dich", "ist", "Thun", "und", "Lei\u00b7den", "lee\u00b7re", ",", "blin\u00b7de", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "KON", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.4": {"line.1": {"text": "Liebe, Du bist g\u00fctig, freundlich, milde,", "tokens": ["Lie\u00b7be", ",", "Du", "bist", "g\u00fc\u00b7tig", ",", "freund\u00b7lich", ",", "mil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neidlos, eiferst nimmer toll und wilde,", "tokens": ["Neid\u00b7los", ",", "ei\u00b7ferst", "nim\u00b7mer", "toll", "und", "wil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nimmer stolz und ungeberdig nie,", "tokens": ["Nim\u00b7mer", "stolz", "und", "un\u00b7ge\u00b7ber\u00b7dig", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht argw\u00f6hnisch, suchst das Meine,", "tokens": ["Nicht", "arg\u00b7w\u00f6h\u00b7nisch", ",", "suchst", "das", "Mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht das Deine:", "tokens": ["Nicht", "das", "Dei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "PPOSAT", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nur die Wahrheit, nicht die L\u00fcge, Gutes freuet sie!", "tokens": ["Nur", "die", "Wahr\u00b7heit", ",", "nicht", "die", "L\u00fc\u00b7ge", ",", "Gu\u00b7tes", "freu\u00b7et", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.5": {"line.1": {"text": "Alles deckt sie, glaubt sie, hofft sie, duldet,", "tokens": ["Al\u00b7les", "deckt", "sie", ",", "glaubt", "sie", ",", "hofft", "sie", ",", "dul\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Duldet Alles, was sie nie verschuldet.", "tokens": ["Dul\u00b7det", "Al\u00b7les", ",", "was", "sie", "nie", "ver\u00b7schul\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Liebe, Du wirst bleiben, Du allein!", "tokens": ["Lie\u00b7be", ",", "Du", "wirst", "blei\u00b7ben", ",", "Du", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVINF", "$,", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Alle Gaben werden schwinden,", "tokens": ["Al\u00b7le", "Ga\u00b7ben", "wer\u00b7den", "schwin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAINF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sprachen schwinden,", "tokens": ["Spra\u00b7chen", "schwin\u00b7den", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVFIN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Alles St\u00fcckwerk der Erkenntni\u00df: Liebe nur wird sein!", "tokens": ["Al\u00b7les", "St\u00fcck\u00b7werk", "der", "Er\u00b7kennt\u00b7ni\u00df", ":", "Lie\u00b7be", "nur", "wird", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$.", "NN", "ADV", "VAFIN", "VAINF", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.6": {"line.1": {"text": "St\u00fcckwerk ist mein Wissen, mein Vergleichen;", "tokens": ["St\u00fcck\u00b7werk", "ist", "mein", "Wis\u00b7sen", ",", "mein", "Ver\u00b7glei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Kommt das Ganze, mu\u00df das St\u00fcckwerk weichen;", "tokens": ["Kommt", "das", "Gan\u00b7ze", ",", "mu\u00df", "das", "St\u00fcck\u00b7werk", "wei\u00b7chen", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VMFIN", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Kind ist Kind und kl\u00fcgelt wie ein Kind.", "tokens": ["Kind", "ist", "Kind", "und", "kl\u00fc\u00b7gelt", "wie", "ein", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "NN", "KON", "VVFIN", "KOKOM", "ART", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Wird ein Mann an Kindereien", "tokens": ["Wird", "ein", "Mann", "an", "Kin\u00b7de\u00b7rei\u00b7en"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Sich erfreuen?", "tokens": ["Sich", "er\u00b7freu\u00b7en", "?"], "token_info": ["word", "word", "punct"], "pos": ["PRF", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Er, ein Mann, ist m\u00e4nnlicher gesinnt.", "tokens": ["Er", ",", "ein", "Mann", ",", "ist", "m\u00e4nn\u00b7li\u00b7cher", "ge\u00b7sinnt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "NN", "$,", "VAFIN", "ADJD", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.7": {"line.1": {"text": "Jetzt im R\u00e4thsel, jetzt im dunkeln Spiegel,", "tokens": ["Jetzt", "im", "R\u00e4th\u00b7sel", ",", "jetzt", "im", "dun\u00b7keln", "Spie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Einst erscheinet uns der Wahrheit Siegel", "tokens": ["Einst", "er\u00b7schei\u00b7net", "uns", "der", "Wahr\u00b7heit", "Sie\u00b7gel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wirklich, Angesicht zu Angesicht:", "tokens": ["Wirk\u00b7lich", ",", "An\u00b7ge\u00b7sicht", "zu", "An\u00b7ge\u00b7sicht", ":"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "NN", "APPR", "NN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Glaube bleibet, Hoffnung, Liebe,", "tokens": ["Glau\u00b7be", "blei\u00b7bet", ",", "Hoff\u00b7nung", ",", "Lie\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Doch die Liebe", "tokens": ["Doch", "die", "Lie\u00b7be"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ist die gr\u00f6\u00dfte Aller, Liebe nur weicht nicht.", "tokens": ["Ist", "die", "gr\u00f6\u00df\u00b7te", "Al\u00b7ler", ",", "Lie\u00b7be", "nur", "weicht", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "$,", "NN", "ADV", "VVFIN", "PTKNEG", "$."], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}}, "stanza.8": {"line.1": {"text": "H\u00e4tt' ich Menschen-, h\u00e4tt' ich Engelzungen,", "tokens": ["H\u00e4tt'", "ich", "Men\u00b7schen", ",", "h\u00e4tt'", "ich", "En\u00b7gel\u00b7zun\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "TRUNC", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "W\u00fcrde Gottes Lob von mir gesungen", "tokens": ["W\u00fcr\u00b7de", "Got\u00b7tes", "Lob", "von", "mir", "ge\u00b7sun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "NN", "APPR", "PPER", "VVPP"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wie ein Sternen-, wie des Himmels Sang,", "tokens": ["Wie", "ein", "Ster\u00b7nen", ",", "wie", "des", "Him\u00b7mels", "Sang", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "TRUNC", "$,", "PWAV", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir fehlete die Liebe,", "tokens": ["Und", "mir", "feh\u00b7le\u00b7te", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Liebe:", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ohne Dich sind meine Lieder todter Schellenklang!", "tokens": ["Oh\u00b7ne", "Dich", "sind", "mei\u00b7ne", "Lie\u00b7der", "tod\u00b7ter", "Schel\u00b7len\u00b7klang", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.9": {"line.1": {"text": "H\u00e4tt' ich Prophezeihung, alle Tiefen", "tokens": ["H\u00e4tt'", "ich", "Pro\u00b7phe\u00b7zei\u00b7hung", ",", "al\u00b7le", "Tie\u00b7fen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "NN", "$,", "PIAT", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Der Geheimnisse, Erkenntni\u00dftiefen,", "tokens": ["Der", "Ge\u00b7heim\u00b7nis\u00b7se", ",", "Er\u00b7kennt\u00b7ni\u00df\u00b7tie\u00b7fen", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Berge zu versetzen, h\u00e4tt' ich Macht,", "tokens": ["Ber\u00b7ge", "zu", "ver\u00b7set\u00b7zen", ",", "h\u00e4tt'", "ich", "Macht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$,", "VAFIN", "PPER", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir fehlete die Liebe,", "tokens": ["Und", "mir", "feh\u00b7le\u00b7te", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Liebe:", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ohne Dich w\u00e4r all mein Glaube, all mein Wissen Nacht!", "tokens": ["Oh\u00b7ne", "Dich", "w\u00e4r", "all", "mein", "Glau\u00b7be", ",", "all", "mein", "Wis\u00b7sen", "Nacht", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "PIAT", "PPOSAT", "NN", "$,", "PIAT", "PPOSAT", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.10": {"line.1": {"text": "G\u00e4b' ich Armen alle meine Habe,", "tokens": ["G\u00e4b'", "ich", "Ar\u00b7men", "al\u00b7le", "mei\u00b7ne", "Ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "NN", "PIAT", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "G\u00e4be meinen Leib zur Gottesgabe", "tokens": ["G\u00e4\u00b7be", "mei\u00b7nen", "Leib", "zur", "Got\u00b7tes\u00b7ga\u00b7be"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPOSAT", "NN", "APPRART", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Preis dem Feuer, lachete der Gluth,", "tokens": ["Preis", "dem", "Feu\u00b7er", ",", "la\u00b7che\u00b7te", "der", "Gluth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Und mir fehlete die Liebe,", "tokens": ["Und", "mir", "feh\u00b7le\u00b7te", "die", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Liebe, Liebe:", "tokens": ["Lie\u00b7be", ",", "Lie\u00b7be", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Ohne Dich ist Thun und Leiden leere, blinde Wuth!", "tokens": ["Oh\u00b7ne", "Dich", "ist", "Thun", "und", "Lei\u00b7den", "lee\u00b7re", ",", "blin\u00b7de", "Wuth", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "VAFIN", "NN", "KON", "NN", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.11": {"line.1": {"text": "Liebe, Du bist g\u00fctig, freundlich, milde,", "tokens": ["Lie\u00b7be", ",", "Du", "bist", "g\u00fc\u00b7tig", ",", "freund\u00b7lich", ",", "mil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "ADJD", "$,", "ADJD", "$,", "ADJD", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Neidlos, eiferst nimmer toll und wilde,", "tokens": ["Neid\u00b7los", ",", "ei\u00b7ferst", "nim\u00b7mer", "toll", "und", "wil\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "ADV", "ADV", "ADJD", "KON", "ADJA", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Nimmer stolz und ungeberdig nie,", "tokens": ["Nim\u00b7mer", "stolz", "und", "un\u00b7ge\u00b7ber\u00b7dig", "nie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Nicht argw\u00f6hnisch, suchst das Meine,", "tokens": ["Nicht", "arg\u00b7w\u00f6h\u00b7nisch", ",", "suchst", "das", "Mei\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "ADJD", "$,", "VVFIN", "ART", "PPOSAT", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Nicht das Deine:", "tokens": ["Nicht", "das", "Dei\u00b7ne", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PTKNEG", "ART", "PPOSAT", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Nur die Wahrheit, nicht die L\u00fcge, Gutes freuet sie!", "tokens": ["Nur", "die", "Wahr\u00b7heit", ",", "nicht", "die", "L\u00fc\u00b7ge", ",", "Gu\u00b7tes", "freu\u00b7et", "sie", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}}, "stanza.12": {"line.1": {"text": "Alles deckt sie, glaubt sie, hofft sie, duldet,", "tokens": ["Al\u00b7les", "deckt", "sie", ",", "glaubt", "sie", ",", "hofft", "sie", ",", "dul\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "PPER", "$,", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Duldet Alles, was sie nie verschuldet.", "tokens": ["Dul\u00b7det", "Al\u00b7les", ",", "was", "sie", "nie", "ver\u00b7schul\u00b7det", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "$,", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Liebe, Du wirst bleiben, Du allein!", "tokens": ["Lie\u00b7be", ",", "Du", "wirst", "blei\u00b7ben", ",", "Du", "al\u00b7lein", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VAFIN", "VVINF", "$,", "PPER", "ADV", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4"