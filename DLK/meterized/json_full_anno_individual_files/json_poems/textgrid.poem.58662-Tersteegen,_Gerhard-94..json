{"textgrid.poem.58662": {"metadata": {"author": {"name": "Tersteegen, Gerhard", "birth": "N.A.", "death": "N.A."}, "title": "94.", "genre": "verse", "period": "N.A.", "pub_year": 1733, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn dir's Gesetz die S\u00fcnde straft, so willst du's ba\u00df erf\u00fcllen,", "tokens": ["Wenn", "dir's", "Ge\u00b7setz", "die", "S\u00fcn\u00b7de", "straft", ",", "so", "willst", "du's", "ba\u00df", "er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "NN", "ART", "NN", "ADJD", "$,", "ADV", "VMFIN", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Doch kann ", "tokens": ["Doch", "kann"], "token_info": ["word", "word"], "pos": ["KON", "VMFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Die S\u00fcnde wird nur s\u00fcndig mehr, es ist kein andrer Grund", "tokens": ["Die", "S\u00fcn\u00b7de", "wird", "nur", "s\u00fcn\u00b7dig", "mehr", ",", "es", "ist", "kein", "an\u00b7drer", "Grund"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "ADV", "$,", "PPER", "VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "Als Jesu Blut, das S\u00fcnden tilgt nach Gottes Gnadenbund.", "tokens": ["Als", "Je\u00b7su", "Blut", ",", "das", "S\u00fcn\u00b7den", "tilgt", "nach", "Got\u00b7tes", "Gna\u00b7den\u00b7bund", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NE", "NN", "$,", "ART", "NN", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.5": {"text": "Ja, Herr, ich denke noch daran und mu\u00df mich schweigend sch\u00e4men,", "tokens": ["Ja", ",", "Herr", ",", "ich", "den\u00b7ke", "noch", "da\u00b7ran", "und", "mu\u00df", "mich", "schwei\u00b7gend", "sch\u00e4\u00b7men", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "PPER", "VVFIN", "ADV", "PAV", "KON", "VMFIN", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.6": {"text": "Da\u00df du mir alle S\u00fcnd' vergibst und willst sie von mir nehmen;", "tokens": ["Da\u00df", "du", "mir", "al\u00b7le", "S\u00fcnd'", "ver\u00b7gibst", "und", "willst", "sie", "von", "mir", "neh\u00b7men", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "PIAT", "NN", "VVFIN", "KON", "VMFIN", "PPER", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.7": {"text": "Aus Gnaden ohne mein Verdienst soll ich dein Kind nun sein,", "tokens": ["Aus", "Gna\u00b7den", "oh\u00b7ne", "mein", "Ver\u00b7dienst", "soll", "ich", "dein", "Kind", "nun", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PPOSAT", "NN", "ADV", "VAINF", "$,"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "Ach, so erfahr' ich, da\u00df du Herr und Heiland bist allein.", "tokens": ["Ach", ",", "so", "er\u00b7fahr'", "ich", ",", "da\u00df", "du", "Herr", "und", "Hei\u00b7land", "bist", "al\u00b7lein", "."], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "$,", "KOUS", "PPER", "NN", "KON", "NN", "VAFIN", "ADV", "$."], "meter": "-+-+-+-+-+-+-+", "measure": "iam