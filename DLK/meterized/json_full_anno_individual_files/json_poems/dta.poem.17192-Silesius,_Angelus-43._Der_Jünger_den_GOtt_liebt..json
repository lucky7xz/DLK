{"dta.poem.17192": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "43.  Der J\u00fcnger den GOtt liebt.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "Ein Mensch der gantz un\u0304 gar sich abwendt von der Welt/", "tokens": ["Ein", "Mensch", "der", "gantz", "un\u0304", "gar", "sich", "ab\u00b7wendt", "von", "der", "Welt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADV", "ADJD", "ADV", "PRF", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Vnd seinen Leih und Seel dem HErren heilig h\u00e4lt/", "tokens": ["Vnd", "sei\u00b7nen", "Leih", "und", "Seel", "dem", "Her\u00b7ren", "hei\u00b7lig", "h\u00e4lt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "KON", "NN", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Stirbt noch vertirbet nicht/ ob man ihm gleich vergibt.", "tokens": ["Stirbt", "noch", "ver\u00b7tir\u00b7bet", "nicht", "/", "ob", "man", "ihm", "gleich", "ver\u00b7gibt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "VVFIN", "PTKNEG", "$(", "KOUS", "PIS", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Fragstu warumb? er ist der J\u00fcnger den er liebt.", "tokens": ["Frags\u00b7tu", "wa\u00b7rumb", "?", "er", "ist", "der", "J\u00fcn\u00b7ger", "den", "er", "liebt", "."], "token_info"