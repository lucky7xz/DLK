{"dta.poem.6443": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Von dem Geruch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Man kan auf den begrasten Auen,", "tokens": ["Man", "kan", "auf", "den", "be\u00b7gras\u00b7ten", "Au\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So wild, als zahme Thiere, schauen,", "tokens": ["So", "wild", ",", "als", "zah\u00b7me", "Thie\u00b7re", ",", "schau\u00b7en", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "KOUS", "ADJA", "NN", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Wie sie, durch einen Trieb, (den, als sie GOTT gemacht,", "tokens": ["Wie", "sie", ",", "durch", "ei\u00b7nen", "Trieb", ",", "(", "den", ",", "als", "sie", "GoTT", "ge\u00b7macht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPR", "ART", "NN", "$,", "$(", "ART", "$,", "KOUS", "PPER", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Er ihnen beygebracht;)", "tokens": ["Er", "ih\u00b7nen", "bey\u00b7ge\u00b7bracht", ";)"], "token_info": ["word", "word", "word", "emoticon"], "pos": ["PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "In Kr\u00e4utern, ohne da\u00df sie fehlen,", "tokens": ["In", "Kr\u00e4u\u00b7tern", ",", "oh\u00b7ne", "da\u00df", "sie", "feh\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "KOUI", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Geschickt und f\u00e4hig sind zu wehlen,", "tokens": ["Ge\u00b7schickt", "und", "f\u00e4\u00b7hig", "sind", "zu", "weh\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "VAFIN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Viel sicherer, als wir, mit unserm Witz.", "tokens": ["Viel", "si\u00b7che\u00b7rer", ",", "als", "wir", ",", "mit", "un\u00b7serm", "Witz", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "$,", "KOUS", "PPER", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}