{"dta.poem.2567": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf Hn. M. S.   J. C.   und Jungf. E. C. F.  \n Hochzeit den 1. Septembr. 1676.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Freund/ dem Minerva mich von Jugend auf verbunden/", "tokens": ["Freund", "/", "dem", "Mi\u00b7ner\u00b7va", "mich", "von", "Ju\u00b7gend", "auf", "ver\u00b7bun\u00b7den", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "ART", "NE", "PRF", "APPR", "NN", "APPR", "VVPP", "$("], "meter": "+-+--+-+-+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Es fodert meine Pflicht/", "tokens": ["Es", "fo\u00b7dert", "mei\u00b7ne", "Pflicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Durch ein beliebt Gedicht/", "tokens": ["Durch", "ein", "be\u00b7liebt", "Ge\u00b7dicht", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Zu ehren deinen Tag/ und s\u00fcsse Hochzeit-Stunden;", "tokens": ["Zu", "eh\u00b7ren", "dei\u00b7nen", "Tag", "/", "und", "s\u00fcs\u00b7se", "Hoch\u00b7zeit\u00b7Stun\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "PPOSAT", "NN", "$(", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Alleine Geist und Glut/", "tokens": ["Al\u00b7lei\u00b7ne", "Geist", "und", "Glut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Verlassen meinen Muth/", "tokens": ["Ver\u00b7las\u00b7sen", "mei\u00b7nen", "Muth", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die alte Liebligkeit ist bey mir gantz verschwunden.", "tokens": ["Die", "al\u00b7te", "Lieb\u00b7lig\u00b7keit", "ist", "bey", "mir", "gantz", "ver\u00b7schwun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Denn wer von Liebe schreibt/ dem mu\u00df die Feder gl\u00fchen/", "tokens": ["Denn", "wer", "von", "Lie\u00b7be", "schreibt", "/", "dem", "mu\u00df", "die", "Fe\u00b7der", "gl\u00fc\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "APPR", "NN", "VVFIN", "$(", "PDS", "VMFIN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der Kopf recht feurig seyn/", "tokens": ["Der", "Kopf", "recht", "feu\u00b7rig", "seyn", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "VAINF", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Sol Leben/ Licht und Schein/", "tokens": ["Sol", "Le\u00b7ben", "/", "Licht", "und", "Schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "$(", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Und aufgeweckter Schertz in seinen Reim einziehen:", "tokens": ["Und", "auf\u00b7ge\u00b7weck\u00b7ter", "Schertz", "in", "sei\u00b7nen", "Reim", "ein\u00b7zie\u00b7hen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Der Liebes Engel Chor/", "tokens": ["Der", "Lie\u00b7bes", "En\u00b7gel", "Chor", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Schenckt selbst ein Zucker-Rohr/", "tokens": ["Schenckt", "selbst", "ein", "Zu\u00b7cke\u00b7rRohr", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.14": {"text": "Dem ", "tokens": ["Dem"], "token_info": ["word"], "pos": ["ART"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "So wird der ", "tokens": ["So", "wird", "der"], "token_info": ["word", "word", "word"], "pos": ["ADV", "VAFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.16": {"text": "Wenn er vom K\u00fcssen sagt/", "tokens": ["Wenn", "er", "vom", "K\u00fcs\u00b7sen", "sagt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Und voller Eyfer klagt/", "tokens": ["Und", "vol\u00b7ler", "Ey\u00b7fer", "klagt", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.18": {"text": "Da\u00df nicht die ", "tokens": ["Da\u00df", "nicht", "die"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART"], "meter": "+--", "measure": "dactylic.init"}, "line.19": {"text": "Ja in nicht mindrer Brunst/", "tokens": ["Ja", "in", "nicht", "mind\u00b7rer", "Brunst", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "PTKNEG", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.20": {"text": "Schreibt ", "tokens": ["Schreibt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.21": {"text": "Wie man recht lieben sol/ wenn ihn ", "tokens": ["Wie", "man", "recht", "lie\u00b7ben", "sol", "/", "wenn", "ihn"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "PIS", "ADJD", "VVINF", "VMFIN", "$(", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Verge\u00df\u2019 ich den ", "tokens": ["Ver\u00b7ge\u00df'", "ich", "den"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ART"], "meter": "+-+-", "measure": "trochaic.di"}, "line.23": {"text": "Der nach der Sterne Wahl/", "tokens": ["Der", "nach", "der", "Ster\u00b7ne", "Wahl", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.24": {"text": "Inungezehlter Zahl/", "tokens": ["I\u00b7nun\u00b7ge\u00b7zehl\u00b7ter", "Zahl", "/"], "token_info": ["word", "word", "punct"], "pos": ["NN", "NN", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.25": {"text": "De\u00df K\u00fcssens s\u00fcsses Werck bey Le\u00dfbien will \u00fcben.", "tokens": ["De\u00df", "K\u00fcs\u00b7sens", "s\u00fcs\u00b7ses", "Werck", "bey", "Le\u00df\u00b7bi\u00b7en", "will", "\u00fc\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "APPR", "NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Wo bleibt ", "tokens": ["Wo", "bleibt"], "token_info": ["word", "word"], "pos": ["PWAV", "VVFIN"], "meter": "+-", "measure": "trochaic.single"}, "line.27": {"text": "Der allen Uberflu\u00df/", "tokens": ["Der", "al\u00b7len", "U\u00b7ber\u00b7flu\u00df", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.28": {"text": "Von Lieb und Buhlerey auf jedem Blat getrieben.", "tokens": ["Von", "Lieb", "und", "Buh\u00b7le\u00b7rey", "auf", "je\u00b7dem", "Blat", "ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "APPR", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Die Lehrer gelten nicht nun mit verliebten Blicken/", "tokens": ["Die", "Leh\u00b7rer", "gel\u00b7ten", "nicht", "nun", "mit", "ver\u00b7lieb\u00b7ten", "Bli\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Dir deine sch\u00f6ne Braut/", "tokens": ["Dir", "dei\u00b7ne", "sch\u00f6\u00b7ne", "Braut", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Tief in das Hertze schaut/", "tokens": ["Tief", "in", "das", "Hert\u00b7ze", "schaut", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "PDS", "VVFIN", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Uns deine ", "tokens": ["Uns", "dei\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPER", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.33": {"text": "Der F\u00fchling ihrer Zier/", "tokens": ["Der", "F\u00fch\u00b7ling", "ih\u00b7rer", "Zier", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.34": {"text": "Stellt dir mehr Anmuth f\u00fcr/", "tokens": ["Stellt", "dir", "mehr", "An\u00b7muth", "f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PIAT", "NN", "APPR", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.35": {"text": "Als jener todte Schrifft ist m\u00e4chtig zu erquicken.", "tokens": ["Als", "je\u00b7ner", "tod\u00b7te", "Schrifft", "ist", "m\u00e4ch\u00b7tig", "zu", "er\u00b7qui\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "ADJA", "NN", "VAFIN", "ADJD", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Gleicht nicht der Augen Pracht zwey hellen Liebes-Sonnen/", "tokens": ["Gleicht", "nicht", "der", "Au\u00b7gen", "Pracht", "zwey", "hel\u00b7len", "Lie\u00b7bes\u00b7Son\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "ART", "NN", "NN", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Jhr Blitz der flammt und spielt/", "tokens": ["Ihr", "Blitz", "der", "flammt", "und", "spielt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "VVFIN", "KON", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.38": {"text": "Bi\u00df er dein Hertz erziehlt/", "tokens": ["Bi\u00df", "er", "dein", "Hertz", "er\u00b7ziehlt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.39": {"text": "Das von getrener Brunst als wie ein Wachs zerronnen.", "tokens": ["Das", "von", "ge\u00b7tre\u00b7ner", "Brunst", "als", "wie", "ein", "Wachs", "zer\u00b7ron\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ADJA", "NN", "KOUS", "KOKOM", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "In der Crystallnen H\u00f6hl\u2019/", "tokens": ["In", "der", "Crys\u00b7tall\u00b7nen", "H\u00f6hl'", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.41": {"text": "Als Spiegel deiner Seel\u2019/", "tokens": ["Als", "Spie\u00b7gel", "dei\u00b7ner", "Seel'", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Hat Liebreitz anfangs bald sein Netz und Garn gesponnen.", "tokens": ["Hat", "Lieb\u00b7reitz", "an\u00b7fangs", "bald", "sein", "Netz", "und", "Garn", "ge\u00b7spon\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "ADV", "ADV", "PPOSAT", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Und siehstu wie ihr Mund in Schalen von Rubinen/", "tokens": ["Und", "sieh\u00b7stu", "wie", "ihr", "Mund", "in", "Scha\u00b7len", "von", "Ru\u00b7bi\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "PPOSAT", "NN", "APPR", "NN", "APPR", "NE", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Dir liefert ein Confect/", "tokens": ["Dir", "lie\u00b7fert", "ein", "Con\u00b7fect", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.45": {"text": "Das nach dem Himmel schmeckt/", "tokens": ["Das", "nach", "dem", "Him\u00b7mel", "schmeckt", "/"], "token_info": ["word", "word", "word", "word", "