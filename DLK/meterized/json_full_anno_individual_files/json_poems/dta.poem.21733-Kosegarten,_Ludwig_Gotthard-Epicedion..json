{"dta.poem.21733": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Epicedion.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1802", "urn": "urn:nbn:de:kobv:b4-200905193310", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Edmund und Ida sind hin. Sie haben des sch\u00f6-\nneren Lebens", "tokens": ["Ed\u00b7mund", "und", "I\u00b7da", "sind", "hin", ".", "Sie", "ha\u00b7ben", "des", "sch\u00f6", "ne\u00b7ren", "Le\u00b7bens"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "KON", "NE", "VAFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "TRUNC", "ADJA", "NN"], "meter": "+--+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Bis auf die Hefen geschl\u00fcrft. Lasset in Frie-", "tokens": ["Bis", "auf", "die", "He\u00b7fen", "ge\u00b7schl\u00fcrft", ".", "Las\u00b7set", "in", "Frie"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "VVPP", "$.", "VVFIN", "APPR", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenige bittere Hefen nur blieben im g\u00fcldenen", "tokens": ["We\u00b7ni\u00b7ge", "bit\u00b7te\u00b7re", "He\u00b7fen", "nur", "blie\u00b7ben", "im", "g\u00fcl\u00b7de\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "ADJA", "NN", "ADV", "VVFIN", "APPRART", "ADJA"], "meter": "+--+--+--+--+--", "measure": "diphilius"}, "line.4": {"text": "Becher.", "tokens": ["Be\u00b7cher", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Dem, der des Weines geschmeckt, widert der", "tokens": ["Dem", ",", "der", "des", "Wei\u00b7nes", "ge\u00b7schmeckt", ",", "wi\u00b7dert", "der"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "$,", "PRELS", "ART", "NN", "VVPP", "$,", "VVFIN", "ART"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.6": {"text": "schaalere Rest.", "tokens": ["schaa\u00b7le\u00b7re", "Rest", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Lasset hin\u00fcber sie ziehn zur Insel der Uranionen,", "tokens": ["Las\u00b7set", "hin\u00b7\u00fc\u00b7ber", "sie", "ziehn", "zur", "In\u00b7sel", "der", "U\u00b7ra\u00b7ni\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+--+--+--+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Wo um ", "tokens": ["Wo", "um"], "token_info": ["word", "word"], "pos": ["PWAV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.9": {"text": "Wo um ", "tokens": ["Wo", "um"], "token_info": ["word", "word"], "pos": ["PWAV", "APPR"], "meter": "-+", "measure": "iambic.single"}, "line.10": {"text": "verschmachtet,", "tokens": ["ver\u00b7schmach\u00b7tet", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.11": {"text": "Um ", "tokens": ["Um"], "token_info": ["word"], "pos": ["KOUI"], "meter": "+", "measure": "single.up"}, "line.12": {"text": "Achill.", "tokens": ["A\u00b7chill", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "Wo gel\u00f6st von den Banden des Stoffs, vom Stachel", "tokens": ["Wo", "ge\u00b7l\u00f6st", "von", "den", "Ban\u00b7den", "des", "Stoffs", ",", "vom", "Sta\u00b7chel"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PWAV", "VVPP", "APPR", "ART", "NN", "ART", "NN", "$,", "APPRART", "NN"], "meter": "--+--+--+-+-", "measure": "anapaest.tri.plus"}, "line.14": {"text": "des Triebes,", "tokens": ["des", "Trie\u00b7bes", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.15": {"text": "H\u00f6chste Sch\u00f6nheit, an dir sich die Verg\u00f6tter-", "tokens": ["H\u00f6chs\u00b7te", "Sch\u00f6n\u00b7heit", ",", "an", "dir", "sich", "die", "Ver\u00b7g\u00f6t\u00b7ter"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "APPR", "PPER", "PRF", "ART", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.16": {"text": "Froh des erh\u00f6heten Seyns, sich selig f\u00fchlend im", "tokens": ["Froh", "des", "er\u00b7h\u00f6\u00b7he\u00b7ten", "Seyns", ",", "sich", "se\u00b7lig", "f\u00fch\u00b7lend", "im"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJD", "ART", "ADJA", "NN", "$,", "PRF", "ADJD", "VVPP", "APPRART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.17": {"text": "Anschaun,", "tokens": ["An\u00b7schaun", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.18": {"text": "Ruhig im heitern Genuss, ledig des Sporns der", "tokens": ["Ru\u00b7hig", "im", "hei\u00b7tern", "Ge\u00b7nuss", ",", "le\u00b7dig", "des", "Sporns", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "ART"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.19": {"text": "Begier.", "tokens": ["Be\u00b7gier", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "-+", "measure": "iambic.single"}}, "stanza.2": {"line.1": {"text": "Sch\u00f6nere ", "tokens": ["Sch\u00f6\u00b7ne\u00b7re"], "token_info": ["word"], "pos": ["NN"], "meter": "+--", "measure": "dactylic.init"}, "line.2": {"text": "Insel.", "tokens": ["In\u00b7sel", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Edlere ", "tokens": ["Ed\u00b7le\u00b7re"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+--", "measure": "dactylic.init"}, "line.4": {"text": "Strom.", "tokens": ["Strom", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+", "measure": "single.up"}, "line.5": {"text": "Jede Erinn'rung verblasst des gemeineren irdischen", "tokens": ["Je\u00b7de", "Er\u00b7inn'\u00b7rung", "ver\u00b7blasst", "des", "ge\u00b7mei\u00b7ne\u00b7ren", "ir\u00b7di\u00b7schen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VVFIN", "ART", "ADJA", "NN"], "meter": "+--+--+--+--+-+", "measure": "diphilius"}, "line.6": {"text": "Lebens.", "tokens": ["Le\u00b7bens", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Alles erlischt, was uns mahnt an die Gewalt", "tokens": ["Al\u00b7les", "er\u00b7lischt", ",", "was", "uns", "mahnt", "an", "die", "Ge\u00b7walt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PIS", "VVPP", "$,", "PRELS", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+---+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "der Natur.", "tokens": ["der", "Na\u00b7tur", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "--+", "measure": "anapaest.init"}, "line.9": {"text": "Du nur schimmerst beglaubigt, o Ahnung erhabne-", "tokens": ["Du", "nur", "schim\u00b7merst", "be\u00b7glau\u00b7bigt", ",", "o", "Ah\u00b7nung", "er\u00b7hab\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "ADV", "ADV", "VVPP", "$,", "FM", "NN", "TRUNC"], "meter": "--+--++-+--+-", "measure": "anapaest.di.plus"}, "line.10": {"text": "Die uns durchblitzte, wenn uns ", "tokens": ["Die", "uns", "durch\u00b7blitz\u00b7te", ",", "wenn", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "PPER", "VVFIN", "$,", "KOUS", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}}}}