{"dta.poem.21119": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Religionseifer .  \n An Herrn G \u2012 \u2012 \u2012.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Mein G &#8210; &#8210; &#8210;, ist nicht ein frommer Eifer,", "tokens": ["Mein", "G", "&#8210;", "&#8210;", "&#8210;", ",", "ist", "nicht", "ein", "from\u00b7mer", "Ei\u00b7fer", ","], "token_info": ["word", "word", "XML_entity", "XML_entity", "XML_entity", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NE", "$(", "$(", "$(", "$,", "VAFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der mit dem Schwerd und mit der Flamme predigt,", "tokens": ["Der", "mit", "dem", "Schwerd", "und", "mit", "der", "Flam\u00b7me", "pre\u00b7digt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "KON", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mehr hassenswerth, als des Erobrers Blutdurst,", "tokens": ["Mehr", "has\u00b7sens\u00b7werth", ",", "als", "des", "E\u00b7ro\u00b7brers", "Blut\u00b7durst", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "KOUS", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Die Wuth erwacht, sie w\u00fctet in sich selber;", "tokens": ["Die", "Wuth", "er\u00b7wacht", ",", "sie", "w\u00fc\u00b7tet", "in", "sich", "sel\u00b7ber", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PPER", "VVFIN", "APPR", "PRF", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und sie vergr\u00e4bt in rauchende Ruinen", "tokens": ["Und", "sie", "ver\u00b7gr\u00e4bt", "in", "rau\u00b7chen\u00b7de", "Ru\u00b7i\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jhr Vaterland. Der Vater mordet S\u00f6hne,", "tokens": ["Ihr", "Va\u00b7ter\u00b7land", ".", "Der", "Va\u00b7ter", "mor\u00b7det", "S\u00f6h\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "ART", "NN", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wenn sie erwacht, wie sie in Frankreich flammte,", "tokens": ["Wenn", "sie", "er\u00b7wacht", ",", "wie", "sie", "in", "Fran\u00b7kreich", "flamm\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "$,", "PWAV", "PPER", "APPR", "NE", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und drei\u00dfig Jahr Germanien verheerte;", "tokens": ["Und", "drei\u00b7\u00dfig", "Jahr", "Ger\u00b7ma\u00b7ni\u00b7en", "ver\u00b7heer\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "CARD", "NN", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Warum hat sie der kriegerische Priester", "tokens": ["Wa\u00b7rum", "hat", "sie", "der", "krie\u00b7ge\u00b7ri\u00b7sche", "Pries\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "ADJA", "NN"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}}, "stanza.4": {"line.1": {"text": "Ach, da\u00df sein Herz nicht sanft und menschenliebend", "tokens": ["Ach", ",", "da\u00df", "sein", "Herz", "nicht", "sanft", "und", "men\u00b7schen\u00b7lie\u00b7bend"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "KOUS", "PPOSAT", "NN", "PTKNEG", "ADJD", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Dem stillen Geist der Duldung nachgefolget!", "tokens": ["Dem", "stil\u00b7len", "Geist", "der", "Dul\u00b7dung", "nach\u00b7ge\u00b7fol\u00b7get", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "So h\u00e4tt\u2019 er nicht von V\u00f6lkern andre V\u00f6lker", "tokens": ["So", "h\u00e4tt'", "er", "nicht", "von", "V\u00f6l\u00b7kern", "and\u00b7re", "V\u00f6l\u00b7ker"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PTKNEG", "APPR", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Sie h\u00e4tren nicht in neugebauten Tempeln", "tokens": ["Sie", "h\u00e4t\u00b7ren", "nicht", "in", "neu\u00b7ge\u00b7bau\u00b7ten", "Tem\u00b7peln"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PTKNEG", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "In fremder Luft die Thr\u00e4nen hingeweinet,", "tokens": ["In", "frem\u00b7der", "Luft", "die", "Thr\u00e4\u00b7nen", "hin\u00b7ge\u00b7wei\u00b7net", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die Rache schreyn; und ihrem Vaterlande", "tokens": ["Die", "Ra\u00b7che", "schreyn", ";", "und", "ih\u00b7rem", "Va\u00b7ter\u00b7lan\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VAINF", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Weit herrlicher r\u00fchmt Fama in die Nachwelt", "tokens": ["Weit", "herr\u00b7li\u00b7cher", "r\u00fchmt", "Fa\u00b7ma", "in", "die", "Nach\u00b7welt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADJD", "ADJA", "VVFIN", "NN", "APPR", "ART", "NN"], "meter": "-+--+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Des Batavers, und Preu\u00dfens Menschenliebe.", "tokens": ["Des", "Ba\u00b7ta\u00b7vers", ",", "und", "Preu\u00b7\u00dfens", "Men\u00b7schen\u00b7lie\u00b7be", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Sie sind belohnt. In reichen Colonien", "tokens": ["Sie", "sind", "be\u00b7lohnt", ".", "In", "rei\u00b7chen", "Co\u00b7lo\u00b7ni\u00b7en"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "VVPP", "$.", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}