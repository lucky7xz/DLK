, "measure": "amphibrach.tri.plus"}, "line.49": {"text": "Er tr\u00e4umt mit dem Auge, das er verlor.", "tokens": ["Er", "tr\u00e4umt", "mit", "dem", "Au\u00b7ge", ",", "das", "er", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.50": {"text": "Sein T\u00f6chterlein mu\u00df ihm berichten", "tokens": ["Sein", "T\u00f6ch\u00b7ter\u00b7lein", "mu\u00df", "ihm", "be\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Von den sch\u00f6nen, bunten Geschichten.", "tokens": ["Von", "den", "sch\u00f6\u00b7nen", ",", "bun\u00b7ten", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}}}}