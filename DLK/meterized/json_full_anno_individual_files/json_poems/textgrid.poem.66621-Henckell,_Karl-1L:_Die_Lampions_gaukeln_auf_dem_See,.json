{"textgrid.poem.66621": {"metadata": {"author": {"name": "Henckell, Karl", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Lampions gaukeln auf dem See,", "genre": "verse", "period": "N.A.", "pub_year": 1896, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Lampions gaukeln auf dem See,", "tokens": ["Die", "Lam\u00b7pi\u00b7ons", "gau\u00b7keln", "auf", "dem", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Laut zischen nach unten die spr\u00fchenden Garben,", "tokens": ["Laut", "zi\u00b7schen", "nach", "un\u00b7ten", "die", "spr\u00fc\u00b7hen\u00b7den", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das Gl\u00fchlicht scheint wie Mondesschnee,", "tokens": ["Das", "Gl\u00fch\u00b7licht", "scheint", "wie", "Mon\u00b7des\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hellt weit die Flut vom hohen Quai,", "tokens": ["Hellt", "weit", "die", "Flut", "vom", "ho\u00b7hen", "Qua\u00b7i", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Bengalisch leuchten die Farben,", "tokens": ["Ben\u00b7ga\u00b7lisch", "leuch\u00b7ten", "die", "Far\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Bis matt sie im Dunkel erstarben.", "tokens": ["Bis", "matt", "sie", "im", "Dun\u00b7kel", "er\u00b7star\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "PPER", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.7": {"text": "Die K\u00f6nigin der Gondeln naht", "tokens": ["Die", "K\u00f6\u00b7ni\u00b7gin", "der", "Gon\u00b7deln", "naht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Langsam auf stolzem Spiegelpfad.", "tokens": ["Lang\u00b7sam", "auf", "stol\u00b7zem", "Spie\u00b7gel\u00b7pfad", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.9": {"text": "Der Buntlaternen zauberisch Tor", "tokens": ["Der", "Bunt\u00b7la\u00b7ter\u00b7nen", "zau\u00b7be\u00b7risch", "Tor"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "ADJD", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.10": {"text": "Spitzt diademisch sich empor.", "tokens": ["Spitzt", "di\u00b7a\u00b7de\u00b7misch", "sich", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.11": {"text": "Darunter spielt die Stadtmusik", "tokens": ["Da\u00b7run\u00b7ter", "spielt", "die", "Stadt\u00b7mu\u00b7sik"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Ein s\u00fcdlich Barkarolenst\u00fcck.", "tokens": ["Ein", "s\u00fcd\u00b7lich", "Bar\u00b7ka\u00b7ro\u00b7len\u00b7st\u00fcck", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.13": {"text": "Leuchtkugeln steigen und neigen", "tokens": ["Leucht\u00b7ku\u00b7geln", "stei\u00b7gen", "und", "nei\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "VVFIN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Ihr sch\u00f6n verscheidendes Haupt,", "tokens": ["Ihr", "sch\u00f6n", "ver\u00b7schei\u00b7den\u00b7des", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.15": {"text": "Brandfr\u00f6sche knattern im Reigen,", "tokens": ["Brand\u00b7fr\u00f6\u00b7sche", "knat\u00b7tern", "im", "Rei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.16": {"text": "Das Feuerrad schwirrt und schnaubt.", "tokens": ["Das", "Feu\u00b7er\u00b7rad", "schwirrt", "und", "schnaubt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.17": {"text": "Vom \u00dctliberg aufschimmert's hell,", "tokens": ["Vom", "\u00dct\u00b7li\u00b7berg", "auf\u00b7schim\u00b7mert's", "hell", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "Mit Blenden gr\u00fc\u00dft das Kulmhotel.", "tokens": ["Mit", "Blen\u00b7den", "gr\u00fc\u00dft", "das", "Kulm\u00b7ho\u00b7tel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.19": {"text": "Mattblinkend winken Mond und Sterne", "tokens": ["Matt\u00b7blin\u00b7kend", "win\u00b7ken", "Mond", "und", "Ster\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.20": {"text": "Aus meeresdunkelblauer Ferne.", "tokens": ["Aus", "mee\u00b7res\u00b7dun\u00b7kel\u00b7blau\u00b7er", "Fer\u00b7ne", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.21": {"text": "Das ganze Ufer schwarzgedr\u00e4ngt", "tokens": ["Das", "gan\u00b7ze", "U\u00b7fer", "schwarz\u00b7ge\u00b7dr\u00e4ngt"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.22": {"text": "Staunt in das rot-blau-gr\u00fcne Spiel,", "tokens": ["Staunt", "in", "das", "rot\u00b7blau\u00b7gr\u00fc\u00b7ne", "Spiel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.23": {"text": "Die Fahne des Vergn\u00fcgens schwenkt", "tokens": ["Die", "Fah\u00b7ne", "des", "Ver\u00b7gn\u00fc\u00b7gens", "schwenkt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.24": {"text": "Frau Neugier hoch am Vorderkiel.", "tokens": ["Frau", "Neu\u00b7gier", "hoch", "am", "Vor\u00b7der\u00b7kiel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.25": {"text": "Verliebte Leute, Br\u00e4ute, Greise", "tokens": ["Ver\u00b7lieb\u00b7te", "Leu\u00b7te", ",", "Br\u00e4u\u00b7te", ",", "Grei\u00b7se"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["ADJA", "NN", "$,", "NN", "$,", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.26": {"text": "Genie\u00dfen laue Luft und Licht,", "tokens": ["Ge\u00b7nie\u00b7\u00dfen", "lau\u00b7e", "Luft", "und", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.27": {"text": "Der eine jauchzt, ", "tokens": ["Der", "ei\u00b7ne", "jauchzt", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Der Dichter schaukelt sein Gedicht.", "tokens": ["Der", "Dich\u00b7ter", "schau\u00b7kelt", "sein", "Ge\u00b7dicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.29": {"text": "Er ist so farbenlustberauscht,", "tokens": ["Er", "ist", "so", "far\u00b7ben\u00b7lust\u00b7be\u00b7rauscht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.30": {"text": "Er schaut in Wundertraum und lauscht,", "tokens": ["Er", "schaut", "in", "Wun\u00b7der\u00b7traum", "und", "lauscht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.31": {"text": "Wie all die kleinen Lichter h\u00fcpfen,", "tokens": ["Wie", "all", "die", "klei\u00b7nen", "Lich\u00b7ter", "h\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIAT", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Leuchtschlangen durcheinander schl\u00fcpfen,", "tokens": ["Leucht\u00b7schlan\u00b7gen", "durch\u00b7ein\u00b7an\u00b7der", "schl\u00fcp\u00b7fen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.33": {"text": "Wie nach dem Takt der Melodien", "tokens": ["Wie", "nach", "dem", "Takt", "der", "Me\u00b7lo\u00b7dien"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "APPR", "ART", "NN", "ART", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.34": {"text": "Sie tanzen, kreisen, suchen, fliehn.", "tokens": ["Sie", "tan\u00b7zen", ",", "krei\u00b7sen", ",", "su\u00b7chen", ",", "fliehn", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Die schwarzen Zuschauer, die flimmernden Nachen,", "tokens": ["Die", "schwar\u00b7zen", "Zu\u00b7schau\u00b7er", ",", "die", "flim\u00b7mern\u00b7den", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.36": {"text": "Er sieht sie mit Trauer, er sieht sie mit Lachen \u2013", "tokens": ["Er", "sieht", "sie", "mit", "Trau\u00b7er", ",", "er", "sieht", "sie", "mit", "La\u00b7chen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "NN", "$,", "PPER", "VVFIN", "PPER", "APPR", "NN", "$("], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.37": {"text": "Das springt empor bei japanischem Licht,", "tokens": ["Das", "springt", "em\u00b7por", "bei", "ja\u00b7pa\u00b7ni\u00b7schem", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.38": {"text": "Geistesspr\u00fchfeuer bezaubert sie nicht.", "tokens": ["Geis\u00b7tes\u00b7spr\u00fch\u00b7feu\u00b7er", "be\u00b7zau\u00b7bert", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.39": {"text": "Aber wir alle freun uns am Schein ...", "tokens": ["A\u00b7ber", "wir", "al\u00b7le", "freun", "uns", "am", "Schein", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "PIS", "VVFIN", "PPER", "APPRART", "NN", "$("], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.40": {"text": "Da schleift hochaufgedonnert pikfein", "tokens": ["Da", "schleift", "hoch\u00b7auf\u00b7ge\u00b7don\u00b7nert", "pik\u00b7fein"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADJD", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.41": {"text": "Starrblickende Dirne vor\u00fcber,", "tokens": ["Starr\u00b7bli\u00b7cken\u00b7de", "Dir\u00b7ne", "vor\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.42": {"text": "Sie lockt nicht der wellengespiegelte Schein,", "tokens": ["Sie", "lockt", "nicht", "der", "wel\u00b7len\u00b7ge\u00b7spie\u00b7gel\u00b7te", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.43": {"text": "Zwanzig Fr\u00e4nkli w\u00e4ren ihr lieber.", "tokens": ["Zwan\u00b7zig", "Fr\u00e4nk\u00b7li", "w\u00e4\u00b7ren", "ihr", "lie\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "VAFIN", "PPER", "ADV", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.44": {"text": "Ihre Hechtaugen sp\u00e4hen nach Beute,", "tokens": ["Ih\u00b7re", "Hech\u00b7tau\u00b7gen", "sp\u00e4\u00b7hen", "nach", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.45": {"text": "Ein g\u00fcnstiger Fangabend heute.", "tokens": ["Ein", "g\u00fcns\u00b7ti\u00b7ger", "Fan\u00b7ga\u00b7bend", "heu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.46": {"text": "Und hinter ihr an der Tochter Arm", "tokens": ["Und", "hin\u00b7ter", "ihr", "an", "der", "Toch\u00b7ter", "Arm"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "APPR", "ART", "NN", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.47": {"text": "Tastet ein blinder Mann durch den Schwarm.", "tokens": ["Tas\u00b7tet", "ein", "blin\u00b7der", "Mann", "durch", "den", "Schwarm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}, "line.48": {"text": "Die Raketen platzen in seinem Ohr,", "tokens": ["Die", "Ra\u00b7ke\u00b7ten", "plat\u00b7zen", "in", "sei\u00b7nem", "Ohr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.49": {"text": "Er tr\u00e4umt mit dem Auge, das er verlor.", "tokens": ["Er", "tr\u00e4umt", "mit", "dem", "Au\u00b7ge", ",", "das", "er", "ver\u00b7lor", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.50": {"text": "Sein T\u00f6chterlein mu\u00df ihm berichten", "tokens": ["Sein", "T\u00f6ch\u00b7ter\u00b7lein", "mu\u00df", "ihm", "be\u00b7rich\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.51": {"text": "Von den sch\u00f6nen, bunten Geschichten.", "tokens": ["Von", "den", "sch\u00f6\u00b7nen", ",", "bun\u00b7ten", "Ge\u00b7schich\u00b7ten", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Die Lampions gaukeln auf dem See,", "tokens": ["Die", "Lam\u00b7pi\u00b7ons", "gau\u00b7keln", "auf", "dem", "See", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Laut zischen nach unten die spr\u00fchenden Garben,", "tokens": ["Laut", "zi\u00b7schen", "nach", "un\u00b7ten", "die", "spr\u00fc\u00b7hen\u00b7den", "Gar\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "APPR", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+---+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Das Gl\u00fchlicht scheint wie Mondesschnee,", "tokens": ["Das", "Gl\u00fch\u00b7licht", "scheint", "wie", "Mon\u00b7des\u00b7schnee", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KOKOM", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hellt weit die Flut vom hohen Quai,", "tokens": ["Hellt", "weit", "die", "Flut", "vom", "ho\u00b7hen", "Qua\u00b7i", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", 