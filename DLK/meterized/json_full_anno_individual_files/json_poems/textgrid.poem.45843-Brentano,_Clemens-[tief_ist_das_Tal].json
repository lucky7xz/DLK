{"textgrid.poem.45843": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "[tief ist das Tal]", "genre": "verse", "period": "N.A.", "pub_year": 1799, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tief ist das Tal, so tief hinabgesunken,", "tokens": ["Tief", "ist", "das", "Tal", ",", "so", "tief", "hin\u00b7ab\u00b7ge\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "ART", "NN", "$,", "ADV", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So tief ihr Grab! Und in der weiten Welt", "tokens": ["So", "tief", "ihr", "Grab", "!", "Und", "in", "der", "wei\u00b7ten", "Welt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "$.", "KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Schlie\u00dft sich die Heimat zu, die Sternenfunken", "tokens": ["Schlie\u00dft", "sich", "die", "Hei\u00b7mat", "zu", ",", "die", "Ster\u00b7nen\u00b7fun\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN", "PTKVZ", "$,", "ART", "NN"], "meter": "++-+-+-+-+-", "measure": "iambic.penta.spondeus"}, "line.4": {"text": "Sind meiner Wohnung Licht, das Himmelszelt", "tokens": ["Sind", "mei\u00b7ner", "Woh\u00b7nung", "Licht", ",", "das", "Him\u00b7mels\u00b7zelt"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Mein k\u00fchles Dach. In dunkler Fern' ertrunken", "tokens": ["Mein", "k\u00fch\u00b7les", "Dach", ".", "In", "dunk\u00b7ler", "Fern'", "er\u00b7trun\u00b7ken"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "APPR", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ist alle Aussicht, und den Busen schwellt", "tokens": ["Ist", "al\u00b7le", "Aus\u00b7sicht", ",", "und", "den", "Bu\u00b7sen", "schwellt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "NN", "$,", "KON", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ein ewiges und nie erf\u00fcllt' Verlangen", "tokens": ["Ein", "e\u00b7wi\u00b7ges", "und", "nie", "er\u00b7f\u00fcllt'", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "KON", "ADV", "VVFIN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Die nimmer kehrt, die Holde, zu umfangen.", "tokens": ["Die", "nim\u00b7mer", "kehrt", ",", "die", "Hol\u00b7de", ",", "zu", "um\u00b7fan\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADV", "VVFIN", "$,", "ART", "NN", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Da sitz' ich stumm auf Berges H\u00f6h' und blicke", "tokens": ["Da", "sitz'", "ich", "stumm", "auf", "Ber\u00b7ges", "H\u00f6h'", "und", "bli\u00b7cke"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "KON", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mit Wehmut in der Kindheit Paradies,", "tokens": ["Mit", "Weh\u00b7mut", "in", "der", "Kind\u00b7heit", "Pa\u00b7ra\u00b7dies", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "ART", "NN", "NE", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der H\u00fctte Licht, es winkt mir still zur\u00fccke,", "tokens": ["Der", "H\u00fct\u00b7te", "Licht", ",", "es", "winkt", "mir", "still", "zu\u00b7r\u00fc\u00b7cke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "PPER", "VVFIN", "PPER", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ruft des Freundes Fl\u00f6te mir so s\u00fc\u00df.", "tokens": ["Es", "ruft", "des", "Freun\u00b7des", "Fl\u00f6\u00b7te", "mir", "so", "s\u00fc\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "NN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Als wolle sie dem finsteren Geschicke,", "tokens": ["Als", "wol\u00b7le", "sie", "dem", "fins\u00b7te\u00b7ren", "Ge\u00b7schi\u00b7cke", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Das mich von Haus und Hof der Sch\u00f6pfung stie\u00df,", "tokens": ["Das", "mich", "von", "Haus", "und", "Hof", "der", "Sch\u00f6p\u00b7fung", "stie\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "APPR", "NN", "KON", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Den bittern Ernst in sanften T\u00f6nen l\u00f6sen,", "tokens": ["Den", "bit\u00b7tern", "Ernst", "in", "sanf\u00b7ten", "T\u00f6\u00b7nen", "l\u00f6\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und s\u00fc\u00dfen Trost mir in den Busen fl\u00f6\u00dfen.", "tokens": ["Und", "s\u00fc\u00b7\u00dfen", "Trost", "mir", "in", "den", "Bu\u00b7sen", "fl\u00f6\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "PPER", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Es bricht der Kampf, im Herzen tief zerronnen", "tokens": ["Es", "bricht", "der", "Kampf", ",", "im", "Her\u00b7zen", "tief", "zer\u00b7ron\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "APPRART", "NN", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist meiner Jugend innerstes Gebild,", "tokens": ["Ist", "mei\u00b7ner", "Ju\u00b7gend", "in\u00b7ners\u00b7tes", "Ge\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADJA", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Aus Kindesliebe war es zart gesponnen,", "tokens": ["Aus", "Kin\u00b7des\u00b7lie\u00b7be", "war", "es", "zart", "ge\u00b7spon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPER", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In Traulichkeit und Milde eingeh\u00fcllt.", "tokens": ["In", "Trau\u00b7lich\u00b7keit", "und", "Mil\u00b7de", "ein\u00b7ge\u00b7h\u00fcllt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Des Lebens j\u00fcngster Tag war kaum begonnen,", "tokens": ["Des", "Le\u00b7bens", "j\u00fcngs\u00b7ter", "Tag", "war", "kaum", "be\u00b7gon\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ein dichtes Dunkel schon die Bahn erf\u00fcllt", "tokens": ["Ein", "dich\u00b7tes", "Dun\u00b7kel", "schon", "die", "Bahn", "er\u00b7f\u00fcllt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und f\u00fcr des Mittags, f\u00fcr des Abends Wonnen", "tokens": ["Und", "f\u00fcr", "des", "Mit\u00b7tags", ",", "f\u00fcr", "des", "A\u00b7bends", "Won\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "$,", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Ein Strom von Schmerz schon aus dem Morgen quillt.", "tokens": ["Ein", "Strom", "von", "Schmerz", "schon", "aus", "dem", "Mor\u00b7gen", "quillt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wo, dunkles Leben, f\u00fchrst du mich nun hin,", "tokens": ["Wo", ",", "dunk\u00b7les", "Le\u00b7ben", ",", "f\u00fchrst", "du", "mich", "nun", "hin", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJA", "NN", "$,", "VVFIN", "PPER", "PRF", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo lebt ein Wesen, das ich dicht umschlinge,", "tokens": ["Wo", "lebt", "ein", "We\u00b7sen", ",", "das", "ich", "dicht", "um\u00b7schlin\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "$,", "PRELS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Dem ich das Herz, und den verwaisten Sinn", "tokens": ["Dem", "ich", "das", "Herz", ",", "und", "den", "ver\u00b7wais\u00b7ten", "Sinn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ART", "NN", "$,", "KON", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Zur Pflege und zur Liebe wiederbringe;", "tokens": ["Zur", "Pfle\u00b7ge", "und", "zur", "Lie\u00b7be", "wie\u00b7der\u00b7brin\u00b7ge", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Ihm werde jede Bl\u00fcte zum Gewinne,", "tokens": ["Ihm", "wer\u00b7de", "je\u00b7de", "Bl\u00fc\u00b7te", "zum", "Ge\u00b7win\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Die aus dem armen Fr\u00fchling ich erringe.", "tokens": ["Die", "aus", "dem", "ar\u00b7men", "Fr\u00fch\u00b7ling", "ich", "er\u00b7rin\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der stumme Freund, er wird ja nimmer sprechen,", "tokens": ["Der", "stum\u00b7me", "Freund", ",", "er", "wird", "ja", "nim\u00b7mer", "spre\u00b7chen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PPER", "VAFIN", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Das dunkle Leben nie ein Wort durchbrechen.", "tokens": ["Das", "dunk\u00b7le", "Le\u00b7ben", "nie", "ein", "Wort", "durch\u00b7bre\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Da fl\u00fcstert's freundlich durch die dunkeln Eichen,", "tokens": ["Da", "fl\u00fcs\u00b7tert's", "freund\u00b7lich", "durch", "die", "dun\u00b7keln", "Ei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADJD", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Es weht wie Trost mir aus dem Tal herauf,", "tokens": ["Es", "weht", "wie", "Trost", "mir", "aus", "dem", "Tal", "her\u00b7auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KOKOM", "NN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ein heilig Wort spricht durch des Waldes Schweigen,", "tokens": ["Ein", "hei\u00b7lig", "Wort", "spricht", "durch", "des", "Wal\u00b7des", "Schwei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Es ziehet schnell der Wolken wilder Lauf,", "tokens": ["Es", "zie\u00b7het", "schnell", "der", "Wol\u00b7ken", "wil\u00b7der", "Lauf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Vor meinem Sinn die schwarzen Schatten weichen,", "tokens": ["Vor", "mei\u00b7nem", "Sinn", "die", "schwar\u00b7zen", "Schat\u00b7ten", "wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und Mondeslicht, und Sterne bl\u00fchen auf.", "tokens": ["Und", "Mon\u00b7des\u00b7licht", ",", "und", "Ster\u00b7ne", "bl\u00fc\u00b7hen", "auf", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Des stummen Freundes inniges Umfassen", "tokens": ["Des", "stum\u00b7men", "Freun\u00b7des", "in\u00b7ni\u00b7ges", "Um\u00b7fas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.8": {"text": "Spricht laut zu mir: ich will dich nie verlassen.", "tokens": ["Spricht", "laut", "zu", "mir", ":", "ich", "will", "dich", "nie", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "APPR", "PPER", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "So hell und still von zartem Licht umgossen,", "tokens": ["So", "hell", "und", "still", "von", "zar\u00b7tem", "Licht", "um\u00b7gos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "KON", "ADJD", "APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ist mir zu F\u00fc\u00dfen wie ein Bildersaal", "tokens": ["Ist", "mir", "zu", "F\u00fc\u00b7\u00dfen", "wie", "ein", "Bil\u00b7der\u00b7saal"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "APPR", "NN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mein vorig Leben r\u00fchrend aufgeschlossen,", "tokens": ["Mein", "vo\u00b7rig", "Le\u00b7ben", "r\u00fch\u00b7rend", "auf\u00b7ge\u00b7schlos\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "VVPP", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Erinnerung spielt in der sch\u00f6nen Wahl.", "tokens": ["E\u00b7rin\u00b7ne\u00b7rung", "spielt", "in", "der", "sch\u00f6\u00b7nen", "Wahl", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.5": {"text": "Ich sehe fr\u00fch die Bl\u00fcten all entsprossen,", "tokens": ["Ich", "se\u00b7he", "fr\u00fch", "die", "Bl\u00fc\u00b7ten", "all", "ent\u00b7spros\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PIAT", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Und fr\u00fch herab gewehet in das Tal,", "tokens": ["Und", "fr\u00fch", "her\u00b7ab", "ge\u00b7we\u00b7het", "in", "das", "Tal", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADV", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Da weine ich nun an dem eignen Grabe,", "tokens": ["Da", "wei\u00b7ne", "ich", "nun", "an", "dem", "eig\u00b7nen", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "