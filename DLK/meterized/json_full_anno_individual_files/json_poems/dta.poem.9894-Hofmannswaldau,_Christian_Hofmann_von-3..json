{"dta.poem.9894": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Seit da\u00df ich gebohren bin/ mu\u00df ich unaufh\u00f6rlich lauffen/", "tokens": ["Seit", "da\u00df", "ich", "ge\u00b7boh\u00b7ren", "bin", "/", "mu\u00df", "ich", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "lauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPER", "VVPP", "VAFIN", "$(", "VMFIN", "PPER", "ADJD", "VVINF", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "Alle welt die suchet mich/ kommt zu mir mit grossem hauffen/", "tokens": ["Al\u00b7le", "welt", "die", "su\u00b7chet", "mich", "/", "kommt", "zu", "mir", "mit", "gros\u00b7sem", "hauf\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "VVFIN", "PPER", "$(", "VVFIN", "APPR", "PPER", "APPR", "ADJA", "NN", "$("], "meter": "+-+-+--+-+-+-+-", "measure": "trochaic.septa.relaxed"}, "line.3": {"text": "Ohne glieder ist mein c\u00f6rper/ habe weder arm noch hand/", "tokens": ["Oh\u00b7ne", "glie\u00b7der", "ist", "mein", "c\u00f6r\u00b7per", "/", "ha\u00b7be", "we\u00b7der", "arm", "noch", "hand", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VAFIN", "PPOSAT", "NN", "$(", "VAFIN", "KON", "ADJD", "ADV", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Und dennoch thut keine st\u00e4rcke meiner st\u00e4rcke widerstand.", "tokens": ["Und", "den\u00b7noch", "thut", "kei\u00b7ne", "st\u00e4r\u00b7cke", "mei\u00b7ner", "st\u00e4r\u00b7cke", "wi\u00b7der\u00b7stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PIAT", "VVFIN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "--+-+-+-+-+-+-+", "measure": "anapaest.init"}}}}}