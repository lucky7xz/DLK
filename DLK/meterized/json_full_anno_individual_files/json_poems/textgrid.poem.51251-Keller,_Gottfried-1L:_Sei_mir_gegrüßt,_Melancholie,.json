{"textgrid.poem.51251": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "1L: Sei mir gegr\u00fc\u00dft, Melancholie,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, Melancholie,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "Me\u00b7lan\u00b7cho\u00b7lie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit dem leisen Feenschritt", "tokens": ["Die", "mit", "dem", "lei\u00b7sen", "Feen\u00b7schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Garten meiner Phantasie", "tokens": ["Im", "Gar\u00b7ten", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu rechter Zeit ans Herz mir tritt!", "tokens": ["Zu", "rech\u00b7ter", "Zeit", "ans", "Herz", "mir", "tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die mir den Mut, wie eine junge Weide,", "tokens": ["Die", "mir", "den", "Mut", ",", "wie", "ei\u00b7ne", "jun\u00b7ge", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Tief an den Rand des Lebens biegt,", "tokens": ["Tief", "an", "den", "Rand", "des", "Le\u00b7bens", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Doch dann in meinem bittern Leide", "tokens": ["Doch", "dann", "in", "mei\u00b7nem", "bit\u00b7tern", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll Treue mir zur Seite liegt!", "tokens": ["Voll", "Treu\u00b7e", "mir", "zur", "Sei\u00b7te", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Die mir der Wahrheit Spiegelschild,", "tokens": ["Die", "mir", "der", "Wahr\u00b7heit", "Spie\u00b7gel\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den unbezwungnen, h\u00e4lt empor,", "tokens": ["Den", "un\u00b7be\u00b7zwung\u00b7nen", ",", "h\u00e4lt", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df der Erkenntnis Tr\u00e4ne schwillt", "tokens": ["Da\u00df", "der", "Er\u00b7kennt\u00b7nis", "Tr\u00e4\u00b7ne", "schwillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bricht aus dunklem Aug hervor;", "tokens": ["Und", "bricht", "aus", "dunk\u00b7lem", "Aug", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie hebst das Haupt du streng und strenger immer,", "tokens": ["Wie", "hebst", "das", "Haupt", "du", "streng", "und", "stren\u00b7ger", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn ich dich mehr und mehr verga\u00df", "tokens": ["Wenn", "ich", "dich", "mehr", "und", "mehr", "ver\u00b7ga\u00df"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "KON", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ob l\u00e4rmendem Ger\u00e4usch und Flimmer,", "tokens": ["Ob", "l\u00e4r\u00b7men\u00b7dem", "Ge\u00b7r\u00e4usch", "und", "Flim\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Die doch an meiner Wiege sa\u00df!", "tokens": ["Die", "doch", "an", "mei\u00b7ner", "Wie\u00b7ge", "sa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Wie h\u00e4ngt mein Herz an eitler Lust", "tokens": ["Wie", "h\u00e4ngt", "mein", "Herz", "an", "eit\u00b7ler", "Lust"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und an der Torheit dieser Welt!", "tokens": ["Und", "an", "der", "Tor\u00b7heit", "die\u00b7ser", "Welt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Oft mehr als eines Weibes Brust", "tokens": ["Oft", "mehr", "als", "ei\u00b7nes", "Wei\u00b7bes", "Brust"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "KOKOM", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ist es von Au\u00dfenwerk umstellt,", "tokens": ["Ist", "es", "von", "Au\u00b7\u00dfen\u00b7werk", "um\u00b7stellt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und selbst den Trost, da\u00df ich aus eignem Streben,", "tokens": ["Und", "selbst", "den", "Trost", ",", "da\u00df", "ich", "aus", "eig\u00b7nem", "Stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ART", "NN", "$,", "KOUS", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Was leer und nichtig ist, erkannt,", "tokens": ["Was", "leer", "und", "nich\u00b7tig", "ist", ",", "er\u00b7kannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "ADJD", "KON", "ADJD", "VAFIN", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Nimmst du und hast mein stolz Erheben", "tokens": ["Nimmst", "du", "und", "hast", "mein", "stolz", "Er\u00b7he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "KON", "VAFIN", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Zu Boden alsobald gewandt,", "tokens": ["Zu", "Bo\u00b7den", "al\u00b7so\u00b7bald", "ge\u00b7wandt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Wenn du mir l\u00e4chelnd zeigst das Buch", "tokens": ["Wenn", "du", "mir", "l\u00e4\u00b7chelnd", "zeigst", "das", "Buch"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Des K\u00f6nigs, den ich oft verh\u00f6hnt,", "tokens": ["Des", "K\u00f6\u00b7nigs", ",", "den", "ich", "oft", "ver\u00b7h\u00f6hnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Aus dem es, wie von Erz ein Fluch,", "tokens": ["Aus", "dem", "es", ",", "wie", "von", "Erz", "ein", "Fluch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "$,", "PWAV", "APPR", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df alles eitel sei! ert\u00f6nt.", "tokens": ["Da\u00df", "al\u00b7les", "ei\u00b7tel", "sei", "!", "er\u00b7t\u00f6nt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PIS", "ADJD", "VAFIN", "$.", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und nah und ferne h\u00f6r ich dann erklingen", "tokens": ["Und", "nah", "und", "fer\u00b7ne", "h\u00f6r", "ich", "dann", "er\u00b7klin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "KON", "ADV", "VVFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gleich Narrenschellen ein Get\u00f6n \u2013", "tokens": ["Gleich", "Nar\u00b7ren\u00b7schel\u00b7len", "ein", "Ge\u00b7t\u00f6n", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "O G\u00f6ttin, la\u00df mich dich umschlingen,", "tokens": ["O", "G\u00f6t\u00b7tin", ",", "la\u00df", "mich", "dich", "um\u00b7schlin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "VVIMP", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Nur du, nur du bist wahr und sch\u00f6n! \u2013", "tokens": ["Nur", "du", ",", "nur", "du", "bist", "wahr", "und", "sch\u00f6n", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Noch f\u00fchl ich dich so edel nicht,", "tokens": ["Noch", "f\u00fchl", "ich", "dich", "so", "e\u00b7del", "nicht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie Albrecht D\u00fcrer dich geschaut:", "tokens": ["Wie", "Al\u00b7brecht", "D\u00fc\u00b7rer", "dich", "ge\u00b7schaut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "NE", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein sinnend Weib, von innerm Licht", "tokens": ["Ein", "sin\u00b7nend", "Weib", ",", "von", "in\u00b7nerm", "Licht"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJD", "NN", "$,", "APPR", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erhellt, des Flei\u00dfes sch\u00f6nste Braut,", "tokens": ["Er\u00b7hellt", ",", "des", "Flei\u00b7\u00dfes", "sch\u00f6ns\u00b7te", "Braut", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Umgeben reich von aller Werke Zeichen,", "tokens": ["Um\u00b7ge\u00b7ben", "reich", "von", "al\u00b7ler", "Wer\u00b7ke", "Zei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "PIAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit milder Trauer angetan;", "tokens": ["Mit", "mil\u00b7der", "Trau\u00b7er", "an\u00b7ge\u00b7tan", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Sie sinnt \u2013 der D\u00e4mon mu\u00df entweichen", "tokens": ["Sie", "sinnt", "\u2013", "der", "D\u00e4\u00b7mon", "mu\u00df", "ent\u00b7wei\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "ART", "NN", "VMFIN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor des Vollbringens reifem Plan.", "tokens": ["Vor", "des", "Voll\u00b7brin\u00b7gens", "rei\u00b7fem", "Plan", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.6": {"line.1": {"text": "Sei mir gegr\u00fc\u00dft, Melancholie,", "tokens": ["Sei", "mir", "ge\u00b7gr\u00fc\u00dft", ",", "Me\u00b7lan\u00b7cho\u00b7lie", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["VAFIN", "PPER", "VVPP", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die mit dem leisen Feenschritt", "tokens": ["Die", "mit", "dem", "lei\u00b7sen", "Feen\u00b7schritt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Im Garten meiner Phantasie", "tokens": ["Im", "Gar\u00b7ten", "mei\u00b7ner", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu rechter Zeit ans Herz mir tritt!", "tokens": ["Zu", "rech\u00b7ter", "Zeit", "ans", "Herz", "mir", "tritt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPRART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die mir den Mut, wie eine junge Weide,", "tokens": ["Die", "mir", "den", "Mut", ",", "wie", "ei\u00b7ne", "jun\u00b7ge", "Wei\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "$,", "PWAV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Tief an den Rand des Lebens biegt,", "tokens": ["Tief", "an", "den", "Rand", "des", "Le\u00b7bens", "biegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.7": {"text": "Doch dann in meinem bittern Leide", "tokens": ["Doch", "dann", "in", "mei\u00b7nem", "bit\u00b7tern", "Lei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Voll Treue mir zur Seite liegt!", "tokens": ["Voll", "Treu\u00b7e", "mir", "zur", "Sei\u00b7te", "liegt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "NN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Die mir der Wahrheit Spiegelschild,", "tokens": ["Die", "mir", "der", "Wahr\u00b7heit", "Spie\u00b7gel\u00b7schild", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den unbezwungnen, h\u00e4lt empor,", "tokens": ["Den", "un\u00b7be\u00b7zwung\u00b7nen", ",", "h\u00e4lt", "em\u00b7por", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df der Erkenntnis Tr\u00e4ne schwillt", "tokens": ["Da\u00df", "der", "Er\u00b7kennt\u00b7nis", "Tr\u00e4\u00b7ne", "schwillt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und bricht aus dunklem Aug hervor;", "tokens": ["Und", "bricht", "aus", "dunk\u00b7lem", "Aug", "her\u00b7vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wie hebst das Haupt du streng und strenger immer,", "tokens": ["Wie", "hebst", "das", "Haupt", "du", "streng", "und", "stren\u00b7ger", "im\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "PPER", "VVFIN", "KON", "ADJD", "ADV", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wenn ich dich mehr und mehr verga\u00df", "tokens": ["Wenn", "ich", "dich", "meh