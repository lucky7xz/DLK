{"dta.poem.9911": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "20.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ob ich schon der sch\u00f6nheit affe/ hab ich nie viel guts gemacht:", "tokens": ["Ob", "ich", "schon", "der", "sch\u00f6n\u00b7heit", "af\u00b7fe", "/", "hab", "ich", "nie", "viel", "guts", "ge\u00b7macht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "PIAT", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Habe viele\u0303 statt der sch\u00f6nheit/ marter/ schand und schimpf ge-", "tokens": ["Ha\u00b7be", "viel\u1ebd", "statt", "der", "sch\u00f6n\u00b7heit", "/", "mar\u00b7ter", "/", "schand", "und", "schimpf", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "$(", "NE", "$(", "VVFIN", "KON", "ADJD", "TRUNC"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "bracht/", "tokens": ["bracht", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+", "measure": "single.up"}, "line.4": {"text": "Straffe diese durch sich selbst/ welche meiner sich bedienet/", "tokens": ["Straf\u00b7fe", "die\u00b7se", "durch", "sich", "selbst", "/", "wel\u00b7che", "mei\u00b7ner", "sich", "be\u00b7die\u00b7net", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PDAT", "APPR", "PRF", "ADV", "$(", "PRELS", "PPOSAT", "PRF", "VVFIN", "$("], "meter": "+--+-+-+-+-+-+-", "measure": "iambic.septa.invert"}, "line.5": {"text": "Und zu trotze der natur/ sch\u00f6n zu machen sich erk\u00fchnet.", "tokens": ["Und", "zu", "trot\u00b7ze", "der", "na\u00b7tur", "/", "sch\u00f6n", "zu", "ma\u00b7chen", "sich", "er\u00b7k\u00fch\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKZU", "VVFIN", "ART", "NN", "$(", "ADJD", "PTKZU", "VVINF", "PRF", "VVFIN", "$."], "meter": "--+--+-+-+-+-+-", "measure": "anapaest.di.plus"}}}}}