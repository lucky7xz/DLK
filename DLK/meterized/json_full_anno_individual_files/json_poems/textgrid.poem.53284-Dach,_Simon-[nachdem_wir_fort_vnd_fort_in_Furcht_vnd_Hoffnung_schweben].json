word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ob nicht das Morgenroth schier komm hervorgegangen,", "tokens": ["Ob", "nicht", "das", "Mor\u00b7gen\u00b7roth", "schier", "komm", "her\u00b7vor\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJD", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er wendet jmmerzu die Augen in den Ost,", "tokens": ["Er", "wen\u00b7det", "jm\u00b7mer\u00b7zu", "die", "Au\u00b7gen", "in", "den", "Ost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vnd spricht: nun brich doch an! brich an, mein Wunsch vnd Trost,", "tokens": ["Vnd", "spricht", ":", "nun", "brich", "doch", "an", "!", "brich", "an", ",", "mein", "Wunsch", "vnd", "Trost", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "ADJD", "ADV", "PTKVZ", "$.", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du helles Tage-Liecht! Di\u00df war, O werthe Seele,", "tokens": ["Du", "hel\u00b7les", "Ta\u00b7ge\u00b7Liecht", "!", "Di\u00df", "war", ",", "O", "wert\u00b7he", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PDS", "VAFIN", "$,", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auch dein Wunsch damals, als des finstern Leibes H\u00f6le", "tokens": ["Auch", "dein", "Wunsch", "da\u00b7mals", ",", "als", "des", "fins\u00b7tern", "Lei\u00b7bes", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dich hielte noch bestrickt, in allem Creutz vnd Pein", "tokens": ["Dich", "hiel\u00b7te", "noch", "be\u00b7strickt", ",", "in", "al\u00b7lem", "Creutz", "vnd", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,", "APPR", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ermahntest du dich selbst mit diesem Trost allein:", "tokens": ["Er\u00b7mahn\u00b7test", "du", "dich", "selbst", "mit", "die\u00b7sem", "Trost", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der strenge Wittwen-Standt, die S\u00e4ufftzer vnd die Z\u00e4hren,", "tokens": ["Der", "stren\u00b7ge", "Witt\u00b7wen\u00b7Standt", ",", "die", "S\u00e4ufft\u00b7zer", "vnd", "die", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das leyden w\u00fcrde nicht, ob GOTT wil, Ewig wehren;", "tokens": ["Das", "ley\u00b7den", "w\u00fcr\u00b7de", "nicht", ",", "ob", "GoTT", "wil", ",", "E\u00b7wig", "weh\u00b7ren", ";"], "toke