{"textgrid.poem.53284": {"metadata": {"author": {"name": "Dach, Simon", "birth": "N.A.", "death": "N.A."}, "title": "[nachdem wir fort vnd fort in Furcht vnd Hoffnung schweben]", "genre": "verse", "period": "N.A.", "pub_year": 1632, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Nachdem wir fort vnd fort in Furcht vnd Hoffnung schweben,", "tokens": ["Nach\u00b7dem", "wir", "fort", "vnd", "fort", "in", "Furcht", "vnd", "Hoff\u00b7nung", "schwe\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PTKVZ", "KON", "PTKVZ", "APPR", "NN", "KON", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Empfinden Noht vnd Angst, bejammern dieses Leben,", "tokens": ["Emp\u00b7fin\u00b7den", "Noht", "vnd", "Angst", ",", "be\u00b7jam\u00b7mern", "die\u00b7ses", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,", "VVFIN", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Vnd m\u00fcssen Brandt au\u00dfstehn, auch Hagel, Frost vnd Wind", "tokens": ["Vnd", "m\u00fcs\u00b7sen", "Brandt", "au\u00df\u00b7stehn", ",", "auch", "Ha\u00b7gel", ",", "Frost", "vnd", "Wind"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VMFIN", "NN", "VVINF", "$,", "ADV", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "In dieser Pilgrimschafft, ein Ball vnd Kreisel sind", "tokens": ["In", "die\u00b7ser", "Pil\u00b7grim\u00b7schafft", ",", "ein", "Ball", "vnd", "Krei\u00b7sel", "sind"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "$,", "ART", "NN", "KON", "NN", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Des Gl\u00fcckes so vns treibt; wie wol hat GOTT geschicket,", "tokens": ["Des", "Gl\u00fc\u00b7ckes", "so", "vns", "treibt", ";", "wie", "wol", "hat", "GoTT", "ge\u00b7schi\u00b7cket", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$.", "PWAV", "ADV", "VAFIN", "NE", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Des Creutzes Schlu\u00df, den Todt, der vns von hinnen r\u00fccket", "tokens": ["Des", "Creut\u00b7zes", "Schlu\u00df", ",", "den", "Todt", ",", "der", "vns", "von", "hin\u00b7nen", "r\u00fc\u00b7cket"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "$,", "PRELS", "PPER", "APPR", "ADV", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Vnd in das Grab verbirgt, der aus dem Joch' vns spannt,", "tokens": ["Vnd", "in", "das", "Grab", "ver\u00b7birgt", ",", "der", "aus", "dem", "Joch'", "vns", "spannt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Des Wetters Vnmuht legt, vnd setzt vns an das Land", "tokens": ["Des", "Wet\u00b7ters", "Vn\u00b7muht", "legt", ",", "vnd", "setzt", "vns", "an", "das", "Land"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der wahren Ruh vnd Lust! drumb sieht man, wie mit Thr\u00e4nen", "tokens": ["Der", "wah\u00b7ren", "Ruh", "vnd", "Lust", "!", "drumb", "sieht", "man", ",", "wie", "mit", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "KON", "NN", "$.", "PAV", "VVFIN", "PIS", "$,", "PWAV", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Nach diesem Vfer sich bedr\u00fcckte Hertzen sehnen,", "tokens": ["Nach", "die\u00b7sem", "Vfer", "sich", "be\u00b7dr\u00fcck\u00b7te", "Hert\u00b7zen", "seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "NN", "PRF", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.11": {"text": "Recht als ein Wandrer sonst fast alle Tritte zehlt,", "tokens": ["Recht", "als", "ein", "Wand\u00b7rer", "sonst", "fast", "al\u00b7le", "Trit\u00b7te", "zehlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOKOM", "ART", "NN", "ADV", "ADV", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Nichts auff das Wetter giebt, vnnd vnges\u00e4umbt sich qu\u00e4lt", "tokens": ["Nichts", "auff", "das", "Wet\u00b7ter", "giebt", ",", "vnnd", "vn\u00b7ge\u00b7s\u00e4umbt", "sich", "qu\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "APPR", "ART", "NN", "VVFIN", "$,", "KON", "APPR", "PRF", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Zu kommen in die Stadt, vnd da sich zu erquicken,", "tokens": ["Zu", "kom\u00b7men", "in", "die", "Stadt", ",", "vnd", "da", "sich", "zu", "er\u00b7qui\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "APPR", "ART", "NN", "$,", "KON", "KOUS", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Wenn sonderlich ein Thurn sich schon hat lassen blicken;", "tokens": ["Wenn", "son\u00b7der\u00b7lich", "ein", "Thurn", "sich", "schon", "hat", "las\u00b7sen", "bli\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ART", "NN", "PRF", "ADV", "VAFIN", "VVINF", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Er sieht jhn sehnlich an, h\u00e4lt allen Richt-Steig wol,", "tokens": ["Er", "sieht", "jhn", "sehn\u00b7lich", "an", ",", "h\u00e4lt", "al\u00b7len", "Richt\u00b7Steig", "wol", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "PTKVZ", "$,", "VVFIN", "PIAT", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Vnd misst wol hundert mahl wie lang er gehen sol.", "tokens": ["Vnd", "misst", "wol", "hun\u00b7dert", "mahl", "wie", "lang", "er", "ge\u00b7hen", "sol", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "CARD", "ADV", "KOKOM", "ADJD", "PPER", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wie auch ein W\u00e4chter steht vnnd wartet mit Verlangen", "tokens": ["Wie", "auch", "ein", "W\u00e4ch\u00b7ter", "steht", "vnnd", "war\u00b7tet", "mit", "Ver\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "KON", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Ob nicht das Morgenroth schier komm hervorgegangen,", "tokens": ["Ob", "nicht", "das", "Mor\u00b7gen\u00b7roth", "schier", "komm", "her\u00b7vor\u00b7ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADJD", "VVFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Er wendet jmmerzu die Augen in den Ost,", "tokens": ["Er", "wen\u00b7det", "jm\u00b7mer\u00b7zu", "die", "Au\u00b7gen", "in", "den", "Ost", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Vnd spricht: nun brich doch an! brich an, mein Wunsch vnd Trost,", "tokens": ["Vnd", "spricht", ":", "nun", "brich", "doch", "an", "!", "brich", "an", ",", "mein", "Wunsch", "vnd", "Trost", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$.", "ADV", "ADJD", "ADV", "PTKVZ", "$.", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Du helles Tage-Liecht! Di\u00df war, O werthe Seele,", "tokens": ["Du", "hel\u00b7les", "Ta\u00b7ge\u00b7Liecht", "!", "Di\u00df", "war", ",", "O", "wert\u00b7he", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$.", "PDS", "VAFIN", "$,", "NE", "VVFIN", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Auch dein Wunsch damals, als des finstern Leibes H\u00f6le", "tokens": ["Auch", "dein", "Wunsch", "da\u00b7mals", ",", "als", "des", "fins\u00b7tern", "Lei\u00b7bes", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPOSAT", "NN", "ADV", "$,", "KOUS", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Dich hielte noch bestrickt, in allem Creutz vnd Pein", "tokens": ["Dich", "hiel\u00b7te", "noch", "be\u00b7strickt", ",", "in", "al\u00b7lem", "Creutz", "vnd", "Pein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "VVPP", "$,", "APPR", "PIS", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ermahntest du dich selbst mit diesem Trost allein:", "tokens": ["Er\u00b7mahn\u00b7test", "du", "dich", "selbst", "mit", "die\u00b7sem", "Trost", "al\u00b7lein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "APPR", "PDAT", "NN", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der strenge Wittwen-Standt, die S\u00e4ufftzer vnd die Z\u00e4hren,", "tokens": ["Der", "stren\u00b7ge", "Witt\u00b7wen\u00b7Standt", ",", "die", "S\u00e4ufft\u00b7zer", "vnd", "die", "Z\u00e4h\u00b7ren", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Das leyden w\u00fcrde nicht, ob GOTT wil, Ewig wehren;", "tokens": ["Das", "ley\u00b7den", "w\u00fcr\u00b7de", "nicht", ",", "ob", "GoTT", "wil", ",", "E\u00b7wig", "weh\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PDS", "VVINF", "VAFIN", "PTKNEG", "$,", "KOUS", "NE", "VMFIN", "$,", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Es k\u00e4me noch die Zeit, da\u00df alle deine Noth", "tokens": ["Es", "k\u00e4\u00b7me", "noch", "die", "Zeit", ",", "da\u00df", "al\u00b7le", "dei\u00b7ne", "Noth"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ART", "NN", "$,", "KOUS", "PIS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Gew\u00fcnscht w\u00fcrd' au\u00dfgetauscht, durch einen s\u00fcssen Todt", "tokens": ["Ge\u00b7w\u00fcnscht", "w\u00fcrd'", "au\u00df\u00b7ge\u00b7tauscht", ",", "durch", "ei\u00b7nen", "s\u00fcs\u00b7sen", "Todt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "VVPP", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Mit vnersch\u00f6pffter Lust. Es ist dir auch gelungen.", "tokens": ["Mit", "vner\u00b7sch\u00f6pff\u00b7ter", "Lust", ".", "Es", "ist", "dir", "auch", "ge\u00b7lun\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$.", "PPER", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}}, "stanza.2": {"line.1": {"text": "Du hast den Krantz erreicht, nach dem du stets gerungen,", "tokens": ["Du", "hast", "den", "Krantz", "er\u00b7reicht", ",", "nach", "dem", "du", "stets", "ge\u00b7run\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der wahren Sonnen-Liecht geht dir nun Ewig auff,", "tokens": ["Der", "wah\u00b7ren", "Son\u00b7nen\u00b7Liecht", "geht", "dir", "nun", "E\u00b7wig", "auff", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADV", "ADJD", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du kennest keine Nacht. Der Eitelheiten Lauff,", "tokens": ["Du", "ken\u00b7nest", "kei\u00b7ne", "Nacht", ".", "Der", "Ei\u00b7tel\u00b7hei\u00b7ten", "Lauff", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "NN", "$.", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die Bo\u00dfheit dieser Welt wird nimmermehr dich kr\u00e4ncken,", "tokens": ["Die", "Bo\u00df\u00b7heit", "die\u00b7ser", "Welt", "wird", "nim\u00b7mer\u00b7mehr", "dich", "kr\u00e4n\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PDAT", "NN", "VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Du h\u00f6rest vmb dich nichts von List, von b\u00f6sen Rencken,", "tokens": ["Du", "h\u00f6\u00b7rest", "vmb", "dich", "nichts", "von", "List", ",", "von", "b\u00f6\u00b7sen", "Ren\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "PIS", "APPR", "NE", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Mit welchen diese Welt vmbringt ist weit vnd breit,", "tokens": ["Mit", "wel\u00b7chen", "die\u00b7se", "Welt", "vm\u00b7bringt", "ist", "weit", "vnd", "breit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PDAT", "NN", "VVPP", "VAFIN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du kennest nichts als Ruh, als Lust vnd Ewigkeit,", "tokens": ["Du", "ken\u00b7nest", "nichts", "als", "Ruh", ",", "als", "Lust", "vnd", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "KOKOM", "NN", "$,", "KOUS", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wir aber solten noch f\u00fcr Trawren gantz verzagen,", "tokens": ["Wir", "a\u00b7ber", "sol\u00b7ten", "noch", "f\u00fcr", "Traw\u00b7ren", "gantz", "ver\u00b7za\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VMFIN", "ADV", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Vnd, wie die Heyden thun, ohn Hoffnung dich beklagen,", "tokens": ["Vnd", ",", "wie", "die", "Hey\u00b7den", "thun", ",", "ohn", "Hoff\u00b7nung", "dich", "be\u00b7kla\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PWAV", "ART", "NN", "VVINF", "$,", "KOUI", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Dich, die du numehr prangst in Ewig-klarer Zier?", "tokens": ["Dich", ",", "die", "du", "nu\u00b7mehr", "prangst", "in", "E\u00b7wig\u00b7kla\u00b7rer", "Zier", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PRELS", "PPER", "ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mit