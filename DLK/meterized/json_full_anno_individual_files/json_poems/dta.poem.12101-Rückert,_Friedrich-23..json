{"dta.poem.12101": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "23.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus Felsen springt der Quell, und Freiheit will ihm ahnen,", "tokens": ["Aus", "Fel\u00b7sen", "springt", "der", "Quell", ",", "und", "Frei\u00b7heit", "will", "ihm", "ah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "ART", "NN", "$,", "KON", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Schicksal rei\u00dft ihn schnell auf ungew\u00e4hlte Bahnen.", "tokens": ["Das", "Schick\u00b7sal", "rei\u00dft", "ihn", "schnell", "auf", "un\u00b7ge\u00b7w\u00e4hl\u00b7te", "Bah\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Er m\u00f6chte dort hinab, doch er mu\u00df da hinunter;", "tokens": ["Er", "m\u00f6ch\u00b7te", "dort", "hin\u00b7ab", ",", "doch", "er", "mu\u00df", "da", "hin\u00b7un\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "PTKVZ", "$,", "KON", "PPER", "VMFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er schlingt und schl\u00e4ngelt sich, und spielt mit Kieseln munter.", "tokens": ["Er", "schlingt", "und", "schl\u00e4n\u00b7gelt", "sich", ",", "und", "spielt", "mit", "Kie\u00b7seln", "mun\u00b7ter", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "PRF", "$,", "KON", "VVFIN", "APPR", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Er sammelt sich zum See, doch seine Lust ist kurz;", "tokens": ["Er", "sam\u00b7melt", "sich", "zum", "See", ",", "doch", "sei\u00b7ne", "Lust", "ist", "kurz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPRART", "NN", "$,", "ADV", "PPOSAT", "NN", "VAFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er mu\u00df aus weichem Bett zum j\u00e4hen Wassersturz.", "tokens": ["Er", "mu\u00df", "aus", "wei\u00b7chem", "Bett", "zum", "j\u00e4\u00b7hen", "Was\u00b7ser\u00b7sturz", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPR", "ADJA", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Da meint er zu verspr\u00fchn, doch kurz ist auch die Qual;", "tokens": ["Da", "meint", "er", "zu", "ver\u00b7spr\u00fchn", ",", "doch", "kurz", "ist", "auch", "die", "Qual", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKZU", "VVINF", "$,", "ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er schnaufet aus, und flie\u00dft ein stiller Flu\u00df im Thal.", "tokens": ["Er", "schnau\u00b7fet", "aus", ",", "und", "flie\u00dft", "ein", "stil\u00b7ler", "Flu\u00df", "im", "Thal", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$,", "KON", "VVFIN", "ART", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "O Wandersmann am Quell, so wechselt Leid und Gl\u00fcck;", "tokens": ["O", "Wan\u00b7ders\u00b7mann", "am", "Quell", ",", "so", "wech\u00b7selt", "Leid", "und", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPRART", "NN", "$,", "ADV", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das Leben rinnet schnell, und kehret nie zur\u00fcck.", "tokens": ["Das", "Le\u00b7ben", "rin\u00b7net", "schnell", ",", "und", "keh\u00b7ret", "nie", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,", "KON", "VVFIN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}