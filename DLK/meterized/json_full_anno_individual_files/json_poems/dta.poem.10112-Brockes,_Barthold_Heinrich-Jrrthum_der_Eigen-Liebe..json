"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Da man von Jhm nichts Gutes w\u00fcnschen kann,", "tokens": ["Da", "man", "von", "Jhm", "nichts", "Gu\u00b7tes", "w\u00fcn\u00b7schen", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "PIAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Ohn\u2019 da\u00df man nicht von Jhm zu gleicher Zeit auch meint,", "tokens": ["Ohn'", "da\u00df", "man", "nicht", "von", "Jhm", "zu", "glei\u00b7cher", "Zeit", "auch", "meint", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PIS", "PTKNEG", "APPR", "PPER", "APPR", "ADJA", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er habe Macht und Weisheit, iederman", "tokens": ["Er", "ha\u00b7be", "Macht", "und", "Weis\u00b7heit", ",", "ie\u00b7der\u00b7man"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "NN", "KON", "NN", "$,", "PIS"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Zu helffen, wenn es Jhm gef\u00e4llt.", "tokens": ["Zu", "helf\u00b7fen", ",", "wenn", "es", "Jhm", "ge\u00b7f\u00e4llt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "PPER", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Du f\u00e4hrst vielleicht noch fort, und sprichst, da\u00df in der", "tokens": ["Du", "f\u00e4hrst", "viel\u00b7leicht", "noch", "fort", ",", "und", "sprichst", ",", "da\u00df", "in", "der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PTKV