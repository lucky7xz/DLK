{"dta.poem.10112": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Jrrthum der Eigen-Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ach; wenn du, lieber Mensch, einst in dich gehen,", "tokens": ["Ach", ";", "wenn", "du", ",", "lie\u00b7ber", "Mensch", ",", "einst", "in", "dich", "ge\u00b7hen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "KOUS", "PPER", "$,", "ADV", "NN", "$,", "ADV", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und recht mit Ernst die eigenen Jdeen,", "tokens": ["Und", "recht", "mit", "Ernst", "die", "ei\u00b7ge\u00b7nen", "Jdeen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "NE", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.3": {"text": "Die von der Gottheit du dir machst, betrachten wolltest;", "tokens": ["Die", "von", "der", "Got\u00b7theit", "du", "dir", "machst", ",", "be\u00b7trach\u00b7ten", "woll\u00b7test", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "PPER", "PPER", "VVFIN", "$,", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Wir w\u00fcrden, wenn du sie uns recht erkl\u00e4ren solltest,", "tokens": ["Wir", "w\u00fcr\u00b7den", ",", "wenn", "du", "sie", "uns", "recht", "er\u00b7kl\u00e4\u00b7ren", "soll\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "PPER", "PPER", "PRF", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Vermuthlich anders nichts in ihnen sehen,", "tokens": ["Ver\u00b7muth\u00b7lich", "an\u00b7ders", "nichts", "in", "ih\u00b7nen", "se\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PIS", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Als ein Ehr-w\u00fcrdig Bild von einem alten Mann,", "tokens": ["Als", "ein", "Ehr\u00b7w\u00fcr\u00b7dig", "Bild", "von", "ei\u00b7nem", "al\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJD", "NN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der gro\u00df und m\u00e4chtig ist, in und nach diesem Leben,", "tokens": ["Der", "gro\u00df", "und", "m\u00e4ch\u00b7tig", "ist", ",", "in", "und", "nach", "die\u00b7sem", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "$,", "APPR", "KON", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Gl\u00fcck und den Himmel dir zu geben;", "tokens": ["Gl\u00fcck", "und", "den", "Him\u00b7mel", "dir", "zu", "ge\u00b7ben", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "ART", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "++-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "Und der dich auch verdammen kann.", "tokens": ["Und", "der", "dich", "auch", "ver\u00b7dam\u00b7men", "kann", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PPER", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Viel weiter geht man nicht. Hieraus nun folget klar:", "tokens": ["Viel", "wei\u00b7ter", "geht", "man", "nicht", ".", "Hier\u00b7aus", "nun", "fol\u00b7get", "klar", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PIS", "PTKNEG", "$.", "PAV", "ADV", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn nichts alhier von GOTT f\u00fcr dich", "tokens": ["Wenn", "nichts", "al\u00b7hier", "von", "GoTT", "f\u00fcr", "dich"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "APPR", "NE", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Zu hoffen und zu f\u00fcrchten w\u00e4re,", "tokens": ["Zu", "hof\u00b7fen", "und", "zu", "f\u00fcrch\u00b7ten", "w\u00e4\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "KON", "PTKZU", "VVINF", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Erzeigtest du wol sicherlich", "tokens": ["Er\u00b7zeig\u00b7test", "du", "wol", "si\u00b7cher\u00b7lich"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PRF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Der wahren Gottheit wenig Ehre.", "tokens": ["Der", "wah\u00b7ren", "Got\u00b7theit", "we\u00b7nig", "Eh\u00b7re", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ist also das, was dich zum GOTTES-Dienste triebe,", "tokens": ["Ist", "al\u00b7so", "das", ",", "was", "dich", "zum", "GoT\u00b7TES\u00b7Diens\u00b7te", "trie\u00b7be", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PDS", "$,", "PWS", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Wenn man es untersucht, nur Eigen-Liebe.", "tokens": ["Wenn", "man", "es", "un\u00b7ter\u00b7sucht", ",", "nur", "Ei\u00b7gen\u00b7Lie\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PIS", "PPER", "VVPP", "$,", "ADV", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Wir bitten meist, da\u00df GOTT, in dieser Zeit,", "tokens": ["Wir", "bit\u00b7ten", "meist", ",", "da\u00df", "GoTT", ",", "in", "die\u00b7ser", "Zeit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "NE", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Uns Brodt und gute Tage gebe,", "tokens": ["Uns", "Brodt", "und", "gu\u00b7te", "Ta\u00b7ge", "ge\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "KON", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df man dort in Ewigkeit,", "tokens": ["Und", "da\u00df", "man", "dort", "in", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PIS", "ADV", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Frey von der H\u00f6llen, seelig lebe.", "tokens": ["Frey", "von", "der", "H\u00f6l\u00b7len", ",", "see\u00b7lig", "le\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,", "ADJD", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Einfolglich ist es leider mehr als wahr,", "tokens": ["Ein\u00b7folg\u00b7lich", "ist", "es", "lei\u00b7der", "mehr", "als", "wahr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "ADV", "PIAT", "KOKOM", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df Eigen-Nutz und Eigen-Lieb\u2019 allein", "tokens": ["Da\u00df", "Ei\u00b7gen\u00b7Nutz", "und", "Ei\u00b7gen\u00b7Lieb'", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "KON", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Die St\u00fctzen deiner Andacht seyn.", "tokens": ["Die", "St\u00fct\u00b7zen", "dei\u00b7ner", "An\u00b7dacht", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "War\u00fcm betrachtest du das, was durch GOtt geschicht,", "tokens": ["Wa\u00b7r\u00fcm", "be\u00b7trach\u00b7test", "du", "das", ",", "was", "durch", "Gott", "ge\u00b7schicht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPER", "PDS", "$,", "PRELS", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In seinen Wundern nicht?", "tokens": ["In", "sei\u00b7nen", "Wun\u00b7dern", "nicht", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "War\u00fcm willt du in Seinen Wercken", "tokens": ["Wa\u00b7r\u00fcm", "willt", "du", "in", "Sei\u00b7nen", "Wer\u00b7cken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VMFIN", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Nicht Seine Liebe, Macht und Weisheit mercken?", "tokens": ["Nicht", "Sei\u00b7ne", "Lie\u00b7be", ",", "Macht", "und", "Weis\u00b7heit", "mer\u00b7cken", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PPOSAT", "NN", "$,", "NN", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Und war\u00fcm betest du Den, welcher alles kann,", "tokens": ["Und", "wa\u00b7r\u00fcm", "be\u00b7test", "du", "Den", ",", "wel\u00b7cher", "al\u00b7les", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "VVFIN", "PPER", "NE", "$,", "PRELS", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Den, welcher alles wirckt, Den, welcher alles liebet,", "tokens": ["Den", ",", "wel\u00b7cher", "al\u00b7les", "wirckt", ",", "Den", ",", "wel\u00b7cher", "al\u00b7les", "lie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIS", "VVFIN", "$,", "NE", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Den, welcher allen alles giebet,", "tokens": ["Den", ",", "wel\u00b7cher", "al\u00b7len", "al\u00b7les", "gie\u00b7bet", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PRELS", "PIAT", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "In heiliger Bewunderung nicht an?", "tokens": ["In", "hei\u00b7li\u00b7ger", "Be\u00b7wun\u00b7de\u00b7rung", "nicht", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Erinnre dich, wie sch\u00f6n,", "tokens": ["E\u00b7rinn\u00b7re", "dich", ",", "wie", "sch\u00f6n", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.10": {"text": "Vom Mi\u00dfbrauch GOTT zu lieben,", "tokens": ["Vom", "Mi\u00df\u00b7brauch", "GoTT", "zu", "lie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "NE", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.11": {"text": "Wie wir schon einst gezeigt, so gar ein T\u00fcrck geschrieben:", "tokens": ["Wie", "wir", "schon", "einst", "ge\u00b7zeigt", ",", "so", "gar", "ein", "T\u00fcrck", "ge\u00b7schrie\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "ADV", "VVPP", "$,", "ADV", "ADV", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Ein grosses Frauen-Mensch, die in der rechten Hand", "tokens": ["Ein", "gros\u00b7ses", "Frau\u00b7en\u00b7Mensch", ",", "die", "in", "der", "rech\u00b7ten", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Ein brennend Feuer trug, und Wasser in der andern.", "tokens": ["Ein", "bren\u00b7nend", "Feu\u00b7er", "trug", ",", "und", "Was\u00b7ser", "in", "der", "an\u00b7dern", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "VVFIN", "$,", "KON", "NN", "APPR", "ART", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Gefragt: Zu welchem Zweck? Sprach sie: der H\u00f6llen\nBrand", "tokens": ["Ge\u00b7fragt", ":", "Zu", "wel\u00b7chem", "Zweck", "?", "Sprach", "sie", ":", "der", "H\u00f6l\u00b7len", "Brand"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VVPP", "$.", "APPR", "PWAT", "NN", "$.", "NN", "PPER", "$.", "ART", "NN", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "L\u00f6sch ich mit dieser Fluth:", "tokens": ["L\u00f6sch", "ich", "mit", "die\u00b7ser", "Fluth", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.16": {"text": "Und mit des Feuers Gluht", "tokens": ["Und", "mit", "des", "Feu\u00b7ers", "Gluht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Will ich das Paradis verbrennen und verheeren,", "tokens": ["Will", "ich", "das", "Pa\u00b7ra\u00b7dis", "ver\u00b7bren\u00b7nen", "und", "ver\u00b7hee\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Da\u00df keiner GOTT aus Furcht, noch \u00fcm Belohnung eh-\nren;", "tokens": ["Da\u00df", "kei\u00b7ner", "GoTT", "aus", "Furcht", ",", "noch", "\u00fcm", "Be\u00b7loh\u00b7nung", "eh", "ren", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "APPR", "NN", "$,", "ADV", "APPRART", "NN", "TRUNC", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Nein, blo\u00df \u00fcm Seiner selbst allein", "tokens": ["Nein", ",", "blo\u00df", "\u00fcm", "Sei\u00b7ner", "selbst", "al\u00b7lein"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "ADV", "APPRART", "NN", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.20": {"text": "Jhn lieben mag, und Jhm ergeben seyn.", "tokens": ["Jhn", "lie\u00b7ben", "mag", ",", "und", "Jhm", "er\u00b7ge\u00b7ben", "seyn", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMFIN", "$,", "KON", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Du sprichst vielleicht, da\u00df ich mit Unrecht hier", "tokens": ["Du", "sprichst", "viel\u00b7leicht", ",", "da\u00df", "ich", "mit", "Un\u00b7recht", "hier"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "KOUS", "PPER", "APPR", "NN", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Eigen-Liebe Trieb verdamme;", "tokens": ["Der", "Ei\u00b7gen\u00b7Lie\u00b7be", "Trieb", "ver\u00b7dam\u00b7me", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da, aus der n\u00fctzlichen Begier,", "tokens": ["Da", ",", "aus", "der", "n\u00fctz\u00b7li\u00b7chen", "Be\u00b7gier", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Uns zu erhalten, dir und mir", "tokens": ["Uns", "zu", "er\u00b7hal\u00b7ten", ",", "dir", "und", "mir"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "PTKZU", "VVINF", "$,", "PPER", "KON", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Doch so viel n\u00fctzliches und gutes stam