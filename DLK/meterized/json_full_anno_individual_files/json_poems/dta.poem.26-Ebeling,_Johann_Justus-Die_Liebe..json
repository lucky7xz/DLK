{"dta.poem.26": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Liebe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198797", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Des Sch\u00f6pfers weiser Zwek, da er\ndie Welt gemacht,", "tokens": ["Des", "Sch\u00f6p\u00b7fers", "wei\u00b7ser", "Zwek", ",", "da", "er", "die", "Welt", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "War aller Menschen Gl\u00fck; dar-", "tokens": ["War", "al\u00b7ler", "Men\u00b7schen", "Gl\u00fck", ";", "dar"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PIAT", "NN", "NN", "$.", "TRUNC"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Weil seine ewge G\u00fct, dies", "tokens": ["Weil", "sei\u00b7ne", "ew\u00b7ge", "G\u00fct", ",", "dies"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPOSAT", "ADJA", "NN", "$,", "PDS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Da\u00df man darinnen noch viel", "tokens": ["Da\u00df", "man", "da\u00b7rin\u00b7nen", "noch", "viel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADV", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "Die Vorsicht gab die Welt ihm zum Besizze ein,", "tokens": ["Die", "Vor\u00b7sicht", "gab", "die", "Welt", "ihm", "zum", "Be\u00b7siz\u00b7ze", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df er und sein Geschlecht, bey seinen Gnaden-", "tokens": ["Da\u00df", "er", "und", "sein", "Ge\u00b7schlecht", ",", "bey", "sei\u00b7nen", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "KON", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "In unverr\u00fckter Lust, das stets geniessen solte,", "tokens": ["In", "un\u00b7ver\u00b7r\u00fck\u00b7ter", "Lust", ",", "das", "stets", "ge\u00b7nies\u00b7sen", "sol\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "PRELS", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Was er als Oberherrr nach seiner Weisheit wolte.", "tokens": ["Was", "er", "als", "O\u00b7ber\u00b7her\u00b7rr", "nach", "sei\u00b7ner", "Weis\u00b7heit", "wol\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "KOUS", "NN", "APPR", "PPOSAT", "NN", "VMFIN", "$."], "meter": "-+-+--+-+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.9": {"text": "Er dr\u00fckte in sein Herz sein g\u00f6ttlich Ebenbild,", "tokens": ["Er", "dr\u00fck\u00b7te", "in", "sein", "Herz", "sein", "g\u00f6tt\u00b7lich", "E\u00b7ben\u00b7bild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da ward der Menschen Geist mit Licht und Glanz", "tokens": ["Da", "ward", "der", "Men\u00b7schen", "Geist", "mit", "Licht", "und", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "NN", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Er fl\u00f6\u00dfte in sein Herz, des Himmels reine Triebe,", "tokens": ["Er", "fl\u00f6\u00df\u00b7te", "in", "sein", "Herz", ",", "des", "Him\u00b7mels", "rei\u00b7ne", "Trie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Worauf das Gl\u00fck beruht, ", "tokens": ["Wo\u00b7rauf", "das", "Gl\u00fck", "be\u00b7ruht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PAV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.13": {"text": "Die Liebe ist ein Band, das ", "tokens": ["Die", "Lie\u00b7be", "ist", "ein", "Band", ",", "das"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ART", "NN", "VAFIN", "ART", "NN", "$,", "PRELS"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.14": {"text": "Ein Feur das w\u00e4rmt und n\u00e4hrt, und wo dasselbe", "tokens": ["Ein", "Feur", "das", "w\u00e4rmt", "und", "n\u00e4hrt", ",", "und", "wo", "das\u00b7sel\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PDS", "VVFIN", "KON", "VVFIN", "$,", "KON", "PWAV", "PDAT"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Da strahlt mit heitren Blik in einer g\u00fcldnen Won-", "tokens": ["Da", "strahlt", "mit", "hei\u00b7tren", "Blik", "in", "ei\u00b7ner", "g\u00fcld\u00b7nen", "Won"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "APPR", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Den Menschen immer an, des Himmels Gnaden-", "tokens": ["Den", "Men\u00b7schen", "im\u00b7mer", "an", ",", "des", "Him\u00b7mels", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "PTKVZ", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.17": {"text": "Allein seid dem der Gift, der Schlangen uns be-", "tokens": ["Al\u00b7lein", "seid", "dem", "der", "Gift", ",", "der", "Schlan\u00b7gen", "uns", "be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ART", "NN", "$,", "ART", "NN", "PPER", "TRUNC"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.18": {"text": "Der Satan in uns hat die S\u00fcnde angestekt;", "tokens": ["Der", "Sa\u00b7tan", "in", "uns", "hat", "die", "S\u00fcn\u00b7de", "an\u00b7ges\u00b7tekt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "VAFIN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Da ist der arme Mensch aus seinen Gl\u00fcksstand", "tokens": ["Da", "ist", "der", "ar\u00b7me", "Mensch", "aus", "sei\u00b7nen", "Gl\u00fckss\u00b7tand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.20": {"text": "Da hat der bittre Has, das Herze eingenommen.", "tokens": ["Da", "hat", "der", "bitt\u00b7re", "Has", ",", "das", "Her\u00b7ze", "ein\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gott ist das h\u00f6chste Gut, das durch die Lieb allein,", "tokens": ["Gott", "ist", "das", "h\u00f6chs\u00b7te", "Gut", ",", "das", "durch", "die", "Lieb", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Sich uns zu eigen schenkt zu unserm gl\u00fcklich seyn:", "tokens": ["Sich", "uns", "zu", "ei\u00b7gen", "schenkt", "zu", "un\u00b7serm", "gl\u00fck\u00b7lich", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "PPER", "PTKA", "ADJD", "VVFIN", "APPR", "PPOSAT", "ADJD", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Wer durch den Has verf\u00fchrt, durch scheelen Neid", "tokens": ["Wer", "durch", "den", "Has", "ver\u00b7f\u00fchrt", ",", "durch", "schee\u00b7len", "Neid"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "APPR", "ART", "NN", "VVPP", "$,", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Der wird von dieser Quell in Jrthum abgewendet;", "tokens": ["Der", "wird", "von", "die\u00b7ser", "Quell", "in", "Jr\u00b7thum", "ab\u00b7ge\u00b7wen\u00b7det", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PDAT", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der sucht was ihm vergn\u00fcgt, und trift es doch", "tokens": ["Der", "sucht", "was", "ihm", "ver\u00b7gn\u00fcgt", ",", "und", "trift", "es", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PWS", "PPER", "VVPP", "$,", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.26": {"text": "Weil man in Pf\u00fczzen nicht, sein Labsal finden kan,", "tokens": ["Weil", "man", "in", "Pf\u00fcz\u00b7zen", "nicht", ",", "sein", "Lab\u00b7sal", "fin\u00b7den", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "NN", "PTKNEG", "$,", "PPOSAT", "NN", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Das in den Quellen fliest. Der Erden eitle G\u00fcter,", "tokens": ["Das", "in", "den", "Quel\u00b7len", "fliest", ".", "Der", "Er\u00b7den", "eit\u00b7le", "G\u00fc\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPR", "ART", "NN", "VVFIN", "$.", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Sind wenn man sie recht schmekt, nicht s\u00fcsse, son-", "tokens": ["Sind", "wenn", "man", "sie", "recht", "schmekt", ",", "nicht", "s\u00fcs\u00b7se", ",", "son"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["VAFIN", "KOUS", "PIS", "PPER", "ADJD", "VVFIN", "$,", "PTKNEG", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.29": {"text": "Sie sind ein Wermuth-Salz, mit Zukker untermischt,", "tokens": ["Sie", "sind", "ein", "Wer\u00b7muth\u00b7Sa\u00b7lz", ",", "mit", "Zuk\u00b7ker", "un\u00b7ter\u00b7mischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.30": {"text": "Ein bittres Aloe mit Candis \u00fcberwischt,", "tokens": ["Ein", "bitt\u00b7res", "A\u00b7loe", "mit", "Can\u00b7dis", "\u00fc\u00b7berw\u00b7ischt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.31": {"text": "Damit labt sich der Mensch; und wird doch nur ge-", "tokens": ["Da\u00b7mit", "labt", "sich", "der", "Mensch", ";", "und", "wird", "doch", "nur", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "PRF", "ART", "NN", "$.", "KON", "VAFIN", "ADV", "ADV", "TRUNC"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.32": {"text": "Weil ihm die wahre Lieb zum h\u00f6chsten Gute fehlet.", "tokens": ["Weil", "ihm", "die", "wah\u00b7re", "Lieb", "zum", "h\u00f6chs\u00b7ten", "Gu\u00b7te", "feh\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Wer seinen Sch\u00f6pfer ha\u00dft, das wahre Freuden-", "tokens": ["Wer", "sei\u00b7nen", "Sch\u00f6p\u00b7fer", "ha\u00dft", ",", "das", "wah\u00b7re", "Freu\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.34": {"text": "Von falschen Wahn bet\u00e4ubt, der liebt sich selber", "tokens": ["Von", "fal\u00b7schen", "Wahn", "be\u00b7t\u00e4ubt", ",", "der", "liebt", "sich", "sel\u00b7ber"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,", "PRELS", "VVFIN", "PRF", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.35": {"text": "Und wenn er Ruhe sucht, wird er in dem Gewissen,", "tokens": ["Und", "wenn", "er", "Ru\u00b7he", "sucht", ",", "wird", "er", "in", "dem", "Ge\u00b7wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "NN", "VVFIN", "$,", "VAFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}, "line.36": {"text": "Von Furcht und Angst gejagt, von Furien gebissen.", "tokens": ["Von", "Furcht", "und", "Angst", "ge\u00b7jagt", ",", "von", "Fu\u00b7ri\u00b7en", "ge\u00b7bis\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVPP", "$,", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die Furcht folgt S\u00fcndern nach, scheucht stets den", "tokens": ["Die", "Furcht", "folgt", "S\u00fcn\u00b7dern", "nach", ",", "scheucht", "stets", "den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "NN", "PTKVZ", "$,", "VVFIN", "ADV", "ART"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.38": {"text": "Der zwischen ", "tokens": ["Der", "zwi\u00b7schen"], "token_info": ["word", "word"], "pos": ["ART", "VVINF"], "meter": "-+-", "measure": "amphibrach.single"}, "line.39": {"text": "Ach! m\u00f6chte jederman der Liebe Vortheil kennen;", "tokens": ["Ach", "!", "m\u00f6ch\u00b7te", "je\u00b7der\u00b7man", "der", "Lie\u00b7be", "Vor\u00b7theil", "ken\u00b7nen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VMFIN", "PIS", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "So w\u00fcrde keiner leicht derselben Band zertrennen:", "tokens": ["So", "w\u00fcr\u00b7de", "kei\u00b7ner", "leicht", "der\u00b7sel\u00b7ben", "Band", "zer\u00b7tren\u00b7nen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIS", "ADJD", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Da ", "tokens": ["Da"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.42": {"text": "Da ist Zufriedenheit auch in der gr\u00f6\u00dften Noth;", "tokens": ["Da", "ist", "Zu\u00b7frie\u00b7den\u00b7heit", "auch", "in", "der", "gr\u00f6\u00df\u00b7ten", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "ADV", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Wo wahre Liebe fehlt, kan man auf sanften K\u00fcs-", "tokens": ["Wo", "wah\u00b7re", "Lie\u00b7be", "fehlt", ",", "kan", "man", "auf", "sanf\u00b7ten", "K\u00fcs"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "NN", "VVFIN", "$,", "VMFIN", "PIS", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Beim Schlaf der Sicherheit auch keine Ruh ge-", "tokens": ["Beim", "Schlaf", "der", "Si\u00b7cher\u00b7heit", "auch", "kei\u00b7ne", "Ruh", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN", "ADV", "PIAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Alsdenn lebt man vergn\u00fcgt, wenn uns der Sch\u00f6pfer", "tokens": ["Als\u00b7denn", "lebt", "man", "ver\u00b7gn\u00fcgt", ",", "wenn", "uns", "der", "Sch\u00f6p\u00b7fer"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PIS", "VVPP", "$,", "KOUS", "PPER"