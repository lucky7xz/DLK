{"dta.poem.1025": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Demnach ich mich newlich bedacht/", "tokens": ["Dem\u00b7nach", "ich", "mich", "new\u00b7lich", "be\u00b7dacht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "ADJD", "VVPP", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.2": {"text": "Welcher Musen es wolt geb\u00fchren/", "tokens": ["Wel\u00b7cher", "Mu\u00b7sen", "es", "wolt", "ge\u00b7b\u00fch\u00b7ren", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAT", "NN", "PPER", "VMFIN", "VVINF", "$("], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Euch dises Buch zu pr", "tokens": ["Euch", "di\u00b7ses", "Buch", "zu", "pr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "PDAT", "NN", "APPR", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Da kam ein jede bald zu Mir/", "tokens": ["Da", "kam", "ein", "je\u00b7de", "bald", "zu", "Mir", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "PIAT", "ADV", "APPR", "NE", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Hoffend solche ehr zu erlangen;", "tokens": ["Hof\u00b7fend", "sol\u00b7che", "ehr", "zu", "er\u00b7lan\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PIAT", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Allein zwo kamen nicht herf\u00fcr/", "tokens": ["Al\u00b7lein", "zwo", "ka\u00b7men", "nicht", "her\u00b7f\u00fcr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "VVFIN", "PTKNEG", "ADV", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Welche doch sunst vor andern prangen.", "tokens": ["Wel\u00b7che", "doch", "sunst", "vor", "an\u00b7dern", "pran\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "ADV", "APPR", "PIS", "VVFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.3": {"line.1": {"text": "Die R\u00f6mische versagt die ehr/", "tokens": ["Die", "R\u00f6\u00b7mi\u00b7sche", "ver\u00b7sagt", "die", "ehr", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil Jhr Euch nicht vor Rom auch naiget", "tokens": ["Weil", "Ihr", "Euch", "nicht", "vor", "Rom", "auch", "nai\u00b7get"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "PTKNEG", "APPR", "NE", "ADV", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Vnd die Spannische het vilmehr", "tokens": ["Vnd", "die", "Span\u00b7ni\u00b7sche", "het", "vil\u00b7mehr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "ADV"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.4": {"text": "Solche Ewerm Brudern erzaiget.", "tokens": ["Sol\u00b7che", "E\u00b7werm", "Bru\u00b7dern", "er\u00b7zai\u00b7get", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "PPOSAT", "NN", "VVFIN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Aber die andre all-zumahl", "tokens": ["A\u00b7ber", "die", "and\u00b7re", "all\u00b7zu\u00b7mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Wolten sich in solches ampt dringen;", "tokens": ["Wol\u00b7ten", "sich", "in", "sol\u00b7ches", "ampt", "drin\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PRF", "APPR", "PIAT", "NN", "VVINF", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Fiengen auch an vmb dise wahl", "tokens": ["Fi\u00b7en\u00b7gen", "auch", "an", "vmb", "di\u00b7se", "wahl"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "APPR", "PDAT", "NN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Jhr ansprach also f\u00fcr-zu-bringen.", "tokens": ["Ihr", "an\u00b7sprach", "al\u00b7so", "f\u00fcr\u00b7zu\u00b7brin\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}