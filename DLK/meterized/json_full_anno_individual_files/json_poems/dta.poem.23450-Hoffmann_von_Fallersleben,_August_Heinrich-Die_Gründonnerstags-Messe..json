{"dta.poem.23450": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Gr\u00fcndonnerstags-Messe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr F\u00fcrsten, die von Ketzern stammen,", "tokens": ["Ihr", "F\u00fcrs\u00b7ten", ",", "die", "von", "Ket\u00b7zern", "stam\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr wollt nicht h\u00f6ren, wollt nicht sehn \u2014", "tokens": ["Ihr", "wollt", "nicht", "h\u00f6\u00b7ren", ",", "wollt", "nicht", "sehn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "VMFIN", "PTKNEG", "VVINF", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr la\u00dft euch von ", "tokens": ["Ihr", "la\u00dft", "euch", "von"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR"], "meter": "-+-+", "measure": "iambic.di"}, "line.4": {"text": "Mit dem ihr wollt in Freundschaft stehn?", "tokens": ["Mit", "dem", "ihr", "wollt", "in", "Freund\u00b7schaft", "stehn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "VMFIN", "APPR", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und kennt ihr die verfluchten Worte", "tokens": ["Und", "kennt", "ihr", "die", "ver\u00b7fluch\u00b7ten", "Wor\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Vom gr\u00fcnen Donnerstage nicht,", "tokens": ["Vom", "gr\u00fc\u00b7nen", "Don\u00b7ners\u00b7ta\u00b7ge", "nicht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "PTKNEG", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So stellt euch an St. Peters Pforte,", "tokens": ["So", "stellt", "euch", "an", "St.", "Pe\u00b7ters", "Pfor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NE", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "H\u00f6rt, was der heil'ge Vater spricht!", "tokens": ["H\u00f6rt", ",", "was", "der", "heil'\u00b7ge", "Va\u00b7ter", "spricht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "\u201ewir thun nach altem Kirchenamte", "tokens": ["\u201e", "wir", "thun", "nach", "al\u00b7tem", "Kir\u00b7chen\u00b7am\u00b7te"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In Bann die Ketzer aller Welt,", "tokens": ["In", "Bann", "die", "Ket\u00b7zer", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und Luthers Lehre die verdammte,", "tokens": ["Und", "Lu\u00b7thers", "Leh\u00b7re", "die", "ver\u00b7damm\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und Alles was sich zu ihr h\u00e4lt.\u201c", "tokens": ["Und", "Al\u00b7les", "was", "sich", "zu", "ihr", "h\u00e4lt", ".", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PIS", "PRELS", "PRF", "APPR", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "So bannt euch an St. Peters Pforte", "tokens": ["So", "bannt", "euch", "an", "St.", "Pe\u00b7ters", "Pfor\u00b7te"], "token_info": ["word", "word", "word", "word", "abbreviation", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NE", "NE", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Der Papst in seiner Heiligkeit,", "tokens": ["Der", "Papst", "in", "sei\u00b7ner", "Hei\u00b7lig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihr aber gebt ihm gute Worte,", "tokens": ["Ihr", "a\u00b7ber", "gebt", "ihm", "gu\u00b7te", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er gemischte Eh'n verzeiht.", "tokens": ["Da\u00df", "er", "ge\u00b7mischte", "Eh'n", "ver\u00b7zeiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}