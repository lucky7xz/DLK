{"textgrid.poem.32609": {"metadata": {"author": {"name": "Pet\u00f6fi, S\u00e1ndor", "birth": "N.A.", "death": "N.A."}, "title": "Auf der Donau.", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie oft, du stolzer Strom, verwundet deine Brust", "tokens": ["Wie", "oft", ",", "du", "stol\u00b7zer", "Strom", ",", "ver\u00b7wun\u00b7det", "dei\u00b7ne", "Brust"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ADV", "$,", "PPER", "ADJA", "NN", "$,", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Bootes scharfer Kiel, des Sturmes wilde Lust!", "tokens": ["Des", "Boo\u00b7tes", "schar\u00b7fer", "Kiel", ",", "des", "Stur\u00b7mes", "wil\u00b7de", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NE", "$,", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wie ist die Wunde tief, wie tief ist da der Schmerz,", "tokens": ["Wie", "ist", "die", "Wun\u00b7de", "tief", ",", "wie", "tief", "ist", "da", "der", "Schmerz", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "ADJD", "$,", "PWAV", "ADJD", "VAFIN", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie schneidet's da so weh und grausam dir ins Herz!", "tokens": ["Wie", "schnei\u00b7det's", "da", "so", "weh", "und", "grau\u00b7sam", "dir", "ins", "Herz", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ADV", "ADV", "ADJD", "KON", "ADJD", "PPER", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und doch, \u2013 enteilt das Schiff und schweigt des Sturmes Wut,", "tokens": ["Und", "doch", ",", "\u2013", "ent\u00b7eilt", "das", "Schiff", "und", "schweigt", "des", "Stur\u00b7mes", "Wut", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "$(", "VVFIN", "ART", "NN", "KON", "VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dann ist die Wunde heil, und alles, alles gut.", "tokens": ["Dann", "ist", "die", "Wun\u00b7de", "heil", ",", "und", "al\u00b7les", ",", "al\u00b7les", "gut", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "$,", "KON", "PIS", "$,", "PIS", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "F\u00fcrs Menschenherz jedoch, ward einmal es verwundet,", "tokens": ["F\u00fcrs", "Men\u00b7schen\u00b7herz", "je\u00b7doch", ",", "ward", "ein\u00b7mal", "es", "ver\u00b7wun\u00b7det", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "$,", "VAFIN", "ADV", "PPER", "VVPP", "$,"], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Gibt's keinen Balsam mehr, durch den es je gesundet!", "tokens": ["Gibt's", "kei\u00b7nen", "Bal\u00b7sam", "mehr", ",", "durch", "den", "es", "je", "ge\u00b7sun\u00b7det", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PIAT", "NN", "ADV", "$,", "APPR", "PRELS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line