{"textgrid.poem.35438": {"metadata": {"author": {"name": "Conradi, Hermann", "birth": "N.A.", "death": "N.A."}, "title": "Trauer", "genre": "verse", "period": "N.A.", "pub_year": 1876, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Meine Seele ist traurig ...", "tokens": ["Mei\u00b7ne", "See\u00b7le", "ist", "trau\u00b7rig", "..."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADJD", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Warum bist du traurig, meine Seele?", "tokens": ["Wa\u00b7rum", "bist", "du", "trau\u00b7rig", ",", "mei\u00b7ne", "See\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADJD", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Und sie spricht zu mir:", "tokens": ["Und", "sie", "spricht", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PPER", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.2": {"text": "Vor\u00fcber ging ich mit dir", "tokens": ["Vor\u00b7\u00fc\u00b7ber", "ging", "ich", "mit", "dir"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "PPER"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "An rauschenden Wassern \u2013", "tokens": ["An", "rau\u00b7schen\u00b7den", "Was\u00b7sern", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Und die rauschenden Wasser", "tokens": ["Und", "die", "rau\u00b7schen\u00b7den", "Was\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Ums\u00e4umte die Siedlung", "tokens": ["Um\u00b7s\u00e4um\u00b7te", "die", "Sied\u00b7lung"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.6": {"text": "Tatfroher Menschen.", "tokens": ["Tat\u00b7fro\u00b7her", "Men\u00b7schen", "."], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.3": {"line.1": {"text": "Mit der Sonne Emporgl\u00fchn", "tokens": ["Mit", "der", "Son\u00b7ne", "Em\u00b7por\u00b7gl\u00fchn"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Traten hinaus sie", "tokens": ["Tra\u00b7ten", "hin\u00b7aus", "sie"], "token_info": ["word", "word", "word"], "pos": ["NN", "APZR", "PPER"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aus ihrer H\u00fctten", "tokens": ["Aus", "ih\u00b7rer", "H\u00fct\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Schmuckloser Enge \u2013", "tokens": ["Schmuck\u00b7lo\u00b7ser", "En\u00b7ge", "\u2013"], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Und tiefeinatmend", "tokens": ["Und", "tie\u00b7fei\u00b7nat\u00b7mend"], "token_info": ["word", "word"], "pos": ["KON", "ADJD"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Des Morgens S\u00e4uselwind", "tokens": ["Des", "Mor\u00b7gens", "S\u00e4u\u00b7sel\u00b7wind"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADV", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Und des Tages Lichtstrahl", "tokens": ["Und", "des", "Ta\u00b7ges", "Licht\u00b7strahl"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.8": {"text": "Mit freudvollem Blicke", "tokens": ["Mit", "freud\u00b7vol\u00b7lem", "Bli\u00b7cke"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.9": {"text": "Emsig begr\u00fc\u00dfend,", "tokens": ["Em\u00b7sig", "be\u00b7gr\u00fc\u00b7\u00dfend", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "VVPP", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.10": {"text": "Gingen sie heiter", "tokens": ["Gin\u00b7gen", "sie", "hei\u00b7ter"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADJD"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.11": {"text": "Und guter Dinge,", "tokens": ["Und", "gu\u00b7ter", "Din\u00b7ge", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.12": {"text": "Ruhvoll und kraftreich,", "tokens": ["Ruh\u00b7voll", "und", "kraft\u00b7reich", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.13": {"text": "An ihr hartes Schaffen,", "tokens": ["An", "ihr", "har\u00b7tes", "Schaf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.14": {"text": "Das Schwei\u00df und Schwielen", "tokens": ["Das", "Schwei\u00df", "und", "Schwie\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.15": {"text": "Gebiert, jedoch auch", "tokens": ["Ge\u00b7biert", ",", "je\u00b7doch", "auch"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVPP", "$,", "ADV", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.16": {"text": "Helle Gedanken", "tokens": ["Hel\u00b7le", "Ge\u00b7dan\u00b7ken"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.17": {"text": "Und die Frucht des Frohsinns,", "tokens": ["Und", "die", "Frucht", "des", "Froh\u00b7sinns", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.18": {"text": "Die unvergleichlich.", "tokens": ["Die", "un\u00b7ver\u00b7gleich\u00b7lich", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJD", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.4": {"line.1": {"text": "Und wiederum ging ich", "tokens": ["Und", "wie\u00b7de\u00b7rum", "ging", "ich"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Mit dir hinauf,", "tokens": ["Mit", "dir", "hin\u00b7auf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.3": {"text": "Sprach meine Seele,", "tokens": ["Sprach", "mei\u00b7ne", "See\u00b7le", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Zu Bergesgipfeln.", "tokens": ["Zu", "Ber\u00b7ges\u00b7gip\u00b7feln", "."], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.5": {"line.1": {"text": "Und ich ward so heiter", "tokens": ["Und", "ich", "ward", "so", "hei\u00b7ter"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "ADV", "ADJD"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Da mich der H\u00f6h'nwind", "tokens": ["Da", "mich", "der", "H\u00f6h'n\u00b7wind"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Weidlich durchl\u00fcftet!", "tokens": ["Weid\u00b7lich", "durch\u00b7l\u00fcf\u00b7tet", "!"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "VVPP", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Wie dehnt' ich mich doch", "tokens": ["Wie", "dehnt'", "ich", "mich", "doch"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "VVFIN", "PPER", "PRF", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.5": {"text": "Und reckte mich weit", "tokens": ["Und", "reck\u00b7te", "mich", "weit"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "ADJD"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "Und sog den Atem", "tokens": ["Und", "sog", "den", "A\u00b7tem"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Schrankenloser", "tokens": ["Schran\u00b7ken\u00b7lo\u00b7ser"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.8": {"text": "Unendlichkeit!", "tokens": ["Un\u00b7end\u00b7lich\u00b7keit", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.6": {"line.1": {"text": "Und allen, die mir", "tokens": ["Und", "al\u00b7len", ",", "die", "mir"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["KON", "PIS", "$,", "PRELS", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Entgegentraten,", "tokens": ["Ent\u00b7ge\u00b7gent\u00b7ra\u00b7ten", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-+--", "measure": "unknown.measure.di"}, "line.3": {"text": "Lachte das Herz", "tokens": ["Lach\u00b7te", "das", "Herz"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.4": {"text": "Aus den hellen Augen,", "tokens": ["Aus", "den", "hel\u00b7len", "Au\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.5": {"text": "Da\u00df ich ihnen", "tokens": ["Da\u00df", "ich", "ih\u00b7nen"], "token_info": ["word", "word", "word"], "pos": ["KOUS", "PPER", "PPER"], "meter": "+-+-", "measure": "trochaic.di"}, "line.6": {"text": "Sehnsuchtsbeschwingt", "tokens": ["Sehn\u00b7suchts\u00b7be\u00b7schwingt"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "-+-+", "measure": "iambic.di"}, "line.7": {"text": "Entgegenh\u00fcpfte ...", "tokens": ["Ent\u00b7ge\u00b7gen\u00b7h\u00fcpf\u00b7te", "..."], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "---+-", "measure": "unknown.measure.single"}}, "stanza.7": {"line.1": {"text": "Und sie boten", "tokens": ["Und", "sie", "bo\u00b7ten"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "--+-", "measure": "anapaest.init"}, "line.2": {"text": "Mir Gru\u00df \u2013 und einer", "tokens": ["Mir", "Gru\u00df", "\u2013", "und", "ei\u00b7ner"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["NE", "NN", "$(", "KON", "ART"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Lud mich zu rasten \u2013", "tokens": ["Lud", "mich", "zu", "ras\u00b7ten", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "$("], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Lud mich zu bleiben:", "tokens": ["Lud", "mich", "zu", "blei\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "PTKZU", "VVINF", "$."], "meter": "---+-", "measure": "unknown.measure.single"}, "line.5": {"text": "\u00bbgelt! Es w\u00e4r' sch\u00f6n doch,", "tokens": ["\u00bb", "gelt", "!", "Es", "w\u00e4r'", "sch\u00f6n", "doch", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "VVPP", "$.", "PPER", "VAFIN", "ADJD", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.6": {"text": "Blieben wir immer", "tokens": ["Blie\u00b7ben", "wir", "im\u00b7mer"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Und ewig zusammen!\u00ab", "tokens": ["Und", "e\u00b7wig", "zu\u00b7sam\u00b7men", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["KON", "ADJD", "PTKVZ", "$.", "$("], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.8": {"line.1": {"text": "Aber wieder", "tokens": ["A\u00b7ber", "wie\u00b7der"], "token_info": ["word", "word"], "pos": ["KON", "ADV"], "meter": "+-+-", "measure": "trochaic.di"}, "line.2": {"text": "Ri\u00df ich mich los", "tokens": ["Ri\u00df", "ich", "mich", "los"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PRF", "PTKVZ"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.3": {"text": "Und der Vergangenheit", "tokens": ["Und", "der", "Ver\u00b7gan\u00b7gen\u00b7heit"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.4": {"text": "Schmerzensreichem", "tokens": ["Schmer\u00b7zens\u00b7rei\u00b7chem"], "token_info": ["word"], "pos": ["NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "M\u00fchenscho\u00df,", "tokens": ["M\u00fc\u00b7hen\u00b7scho\u00df", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.6": {"text": "Der mich gewirket,", "tokens": ["Der", "mich", "ge\u00b7wir\u00b7ket", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Gab ich mich wieder.", "tokens": ["Gab", "ich", "mich", "wie\u00b7der", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.9": {"line.1": {"text": "Es fra\u00df sich in mich", "tokens": ["Es", "fra\u00df", "sich", "in", "mich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "PPER"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.2": {"text": "Und gebiert sich fort", "tokens": ["Und", "ge\u00b7biert", "sich", "fort"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PRF", "PTKVZ"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und haftet immer!", "tokens": ["Und", "haf\u00b7tet", "im\u00b7mer", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.10": {"line.1": {"text": "Nimmer! O nimmer", "tokens": ["Nim\u00b7mer", "!", "O", "nim\u00b7mer"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$.", "NE", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lehrt mich des Fischers", "tokens": ["Lehrt", "mich", "des", "Fi\u00b7schers"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "ART", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Oder des Schiffers", "tokens": ["O\u00b7der", "des", "Schif\u00b7fers"], "token_info": ["word", "word", "word"], "pos": ["KON", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Beengtes Trachten", "tokens": ["Be\u00b7eng\u00b7tes", "Trach\u00b7ten"], "token_info": ["word", "word"], "pos": ["ADJA", "NN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Grenze und Ma\u00df \u2013", "tokens": ["Gren\u00b7ze", "und", "Ma\u00df", "\u2013"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$("], "meter": "+--+", "measure": "iambic.di.chol"}, "line.6": {"text": "St\u00fcrmisch Verachten,", "tokens": ["St\u00fcr\u00b7misch", "Ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.7": {"text": "Emsig Vergessen", "tokens": ["Em\u00b7sig", "Ver\u00b7ges\u00b7sen"], "token_info": ["word", "word"], "pos": ["NE", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.8": {"text": "Alles dessen,", "tokens": ["Al\u00b7les", "des\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["PIS", "PDS", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.9": {"text": "Was ich im Grunde doch \u2013 nie besa\u00df!", "tokens": ["Was", "ich", "im", "Grun\u00b7de", "doch", "\u2013", "nie", "be\u00b7sa\u00df", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPER", "APPRART", "NN", "ADV", "$(", "ADV", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Nimmer! O nimmer", "tokens": ["Nim\u00b7mer", "!", "O", "nim\u00b7mer"], "token_info": ["word", "punct", "word", "word"], "pos": ["ADV", "$.", "NE", "ADV"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Lehrt der hell\u00e4ugige", "tokens": ["Lehrt", "der", "hel\u00b7l\u00e4u\u00b7gi\u00b7ge"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Sohn mich der Berge", "tokens": ["Sohn", "mich", "der", "Ber\u00b7ge"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ART", "NN"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.4": {"text": "Frohe Gemeinschaft,", "tokens": ["Fro\u00b7he", "Ge\u00b7mein\u00b7schaft", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+--+-"