{"textgrid.poem.33783": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Mahomed", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Unter der Platane,", "tokens": ["Un\u00b7ter", "der", "Pla\u00b7ta\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Um den Brunnen ruht", "tokens": ["Um", "den", "Brun\u00b7nen", "ruht"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUI", "ART", "NN", "VVFIN"], "meter": "--+-+", "measure": "anapaest.init"}, "line.3": {"text": "Meine Karawane", "tokens": ["Mei\u00b7ne", "Ka\u00b7ra\u00b7wa\u00b7ne"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Mit Chadidscha's Gut.", "tokens": ["Mit", "Cha\u00b7did\u00b7scha's", "Gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NE", "ADJD", "$."], "meter": "-+--+", "measure": "iambic.di.chol"}}, "stanza.2": {"line.1": {"text": "Die zum Schlafen taugen,", "tokens": ["Die", "zum", "Schla\u00b7fen", "tau\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ruhn vom Zelt bedacht,", "tokens": ["Ruhn", "vom", "Zelt", "be\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "VVPP", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Aber meine Augen", "tokens": ["A\u00b7ber", "mei\u00b7ne", "Au\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00d6ffnete die Nacht.", "tokens": ["\u00d6ff\u00b7ne\u00b7te", "die", "Nacht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.3": {"line.1": {"text": "Auf der W\u00fcste Steinen", "tokens": ["Auf", "der", "W\u00fcs\u00b7te", "Stei\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Unterm Sternenzelt", "tokens": ["Un\u00b7term", "Ster\u00b7nen\u00b7zelt"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Preis' ich dich, den einen,", "tokens": ["Preis'", "ich", "dich", ",", "den", "ei\u00b7nen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "PPER", "PRF", "$,", "PRELS", "ART", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ew'gen Geist der Welt!", "tokens": ["Ew'\u00b7gen", "Geist", "der", "Welt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.4": {"line.1": {"text": "Oft, wenn die Kamele", "tokens": ["Oft", ",", "wenn", "die", "Ka\u00b7me\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Tr\u00e4nken ging dein Knecht,", "tokens": ["Tr\u00e4n\u00b7ken", "ging", "dein", "Knecht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Hobst du meine Seele", "tokens": ["Hobst", "du", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "\u00dcber mein Geschlecht,", "tokens": ["\u00dc\u00b7ber", "mein", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.5": {"line.1": {"text": "Zeigtest mir die Bahnen,", "tokens": ["Zeig\u00b7test", "mir", "die", "Bah\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Wie den Feuern dort,", "tokens": ["Wie", "den", "Feu\u00b7ern", "dort", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und mit ernstem Mahnen", "tokens": ["Und", "mit", "erns\u00b7tem", "Mah\u00b7nen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ging an mich das Wort.", "tokens": ["Ging", "an", "mich", "das", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPER", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.6": {"line.1": {"text": "\u00bbweh! dem Tier, dem Baume", "tokens": ["\u00bb", "weh", "!", "dem", "Tier", ",", "dem", "Bau\u00b7me"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["$(", "PTKVZ", "$.", "ART", "NN", "$,", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Dient noch tr\u00e4ger Wahn.", "tokens": ["Dient", "noch", "tr\u00e4\u00b7ger", "Wahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Wecke, die im Traume", "tokens": ["We\u00b7cke", ",", "die", "im", "Trau\u00b7me"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Blinden G\u00f6tzen nahn!", "tokens": ["Blin\u00b7den", "G\u00f6t\u00b7zen", "nahn", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.7": {"line.1": {"text": "Trenn vom Pfad der S\u00fcnder", "tokens": ["Trenn", "vom", "Pfad", "der", "S\u00fcn\u00b7der"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "APPRART", "NN", "ART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Ismaels Gebet,", "tokens": ["Is\u00b7maels", "Ge\u00b7bet", ","], "token_info": ["word", "word", "punct"], "pos": ["NE", "NN", "$,"], "meter": "-+--", "measure": "dactylic.init"}, "line.3": {"text": "Werde mein Verk\u00fcnder,", "tokens": ["Wer\u00b7de", "mein", "Ver\u00b7k\u00fcn\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Werde mein Prophet!", "tokens": ["Wer\u00b7de", "mein", "Pro\u00b7phet", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.8": {"line.1": {"text": "Lies, was deinem Volke", "tokens": ["Lies", ",", "was", "dei\u00b7nem", "Vol\u00b7ke"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Gottes Finger schrieb,", "tokens": ["Got\u00b7tes", "Fin\u00b7ger", "schrieb", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Lies es in der Wolke:", "tokens": ["Lies", "es", "in", "der", "Wol\u00b7ke", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Bete! Faste! Gib!", "tokens": ["Be\u00b7te", "!", "Fas\u00b7te", "!", "Gib", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$.", "NN", "$.", "VVIMP", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.9": {"line.1": {"text": "G\u00fcrte deine Frommen", "tokens": ["G\u00fcr\u00b7te", "dei\u00b7ne", "From\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Mit gelassnem Mut;", "tokens": ["Mit", "ge\u00b7lass\u00b7nem", "Mut", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Was da mu\u00df, wird kommen,", "tokens": ["Was", "da", "mu\u00df", ",", "wird", "kom\u00b7men", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "ADV", "VMFIN", "$,", "VAFIN", "VVINF", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Was geschieht, ist gut.", "tokens": ["Was", "ge\u00b7schieht", ",", "ist", "gut", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "$,", "VAFIN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.10": {"line.1": {"text": "Wer im Schlachtget\u00fcmmel", "tokens": ["Wer", "im", "Schlacht\u00b7ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["PWS", "APPRART", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Lanze schwingt und Schwert,", "tokens": ["Lan\u00b7ze", "schwingt", "und", "Schwert", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Ist schon halb im Himmel,", "tokens": ["Ist", "schon", "halb", "im", "Him\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Ist schon Edens wert.", "tokens": ["Ist", "schon", "E\u00b7dens", "wert", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "NN", "ADJD", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.11": {"line.1": {"text": "Wer f\u00fcr seinen Glauben", "tokens": ["Wer", "f\u00fcr", "sei\u00b7nen", "Glau\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["PWS", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Fiel im Siegeslauf,", "tokens": ["Fiel", "im", "Sie\u00b7ges\u00b7lauf", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Dort in Rosenlauben", "tokens": ["Dort", "in", "Ro\u00b7sen\u00b7lau\u00b7ben"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPR", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Wacht er himmlisch auf.", "tokens": ["Wacht", "er", "himm\u00b7lisch", "auf", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADJD", "PTKVZ", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.12": {"line.1": {"text": "Eine sch\u00f6n're Sonne", "tokens": ["Ei\u00b7ne", "sch\u00f6n'\u00b7re", "Son\u00b7ne"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Strahlt dort Mann und Weib,", "tokens": ["Strahlt", "dort", "Mann", "und", "Weib", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Und in ew'ger Wonne", "tokens": ["Und", "in", "ew'\u00b7ger", "Won\u00b7ne"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "--+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Schwelgen Seel' und Leib.", "tokens": ["Schwel\u00b7gen", "Seel'", "und", "Leib", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.13": {"line.1": {"text": "Auf, Mohammed, mahne", "tokens": ["Auf", ",", "Mo\u00b7ham\u00b7med", ",", "mah\u00b7ne"], "token_info": ["word", "punct", "word", "punct", "word"], "pos": ["APPR", "$,", "NE", "$,", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Jemens Volk ins Feld;", "tokens": ["Je\u00b7mens", "Volk", "ins", "Feld", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPRART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.3": {"text": "Nimm die Halbmondfahne,", "tokens": ["Nimm", "die", "Halb\u00b7mond\u00b7fah\u00b7ne", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.4": {"text": "Priester, K\u00f6nig, Held!", "tokens": ["Pries\u00b7ter", ",", "K\u00f6\u00b7nig", ",", "Held", "!"], "token_info": ["word", "punct", "word", "punct", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}}, "stanza.14": {"line.1": {"text": "Nie dir Rast gestatten", "tokens": ["Nie", "dir", "Rast", "ge\u00b7stat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "PPER", "NN", "VVPP"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.2": {"text": "Darf der heilige Krieg,", "tokens": ["Darf", "der", "hei\u00b7li\u00b7ge", "Krieg", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Nie dein Schwert ermatten", "tokens": ["Nie",