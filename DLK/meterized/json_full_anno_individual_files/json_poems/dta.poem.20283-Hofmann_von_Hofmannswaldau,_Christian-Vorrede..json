{"dta.poem.20283": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Vorrede.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ist friede gold/ so mu\u00df der freyheit edler schatz", "tokens": ["Ist", "frie\u00b7de", "gold", "/", "so", "mu\u00df", "der", "frey\u00b7heit", "ed\u00b7ler", "schatz"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJA", "NN", "$(", "ADV", "VMFIN", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sein g\u00fcldener als gold. Die tr\u00e4gt August euch an:", "tokens": ["Sein", "g\u00fcl\u00b7de\u00b7ner", "als", "gold", ".", "Die", "tr\u00e4gt", "Au\u00b7gust", "euch", "an", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "KOUS", "NN", "$.", "ART", "VVFIN", "NN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Rom aber weigert sich zu geben raum und platz/", "tokens": ["Rom", "a\u00b7ber", "wei\u00b7gert", "sich", "zu", "ge\u00b7ben", "raum", "und", "platz", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PRF", "PTKZU", "VVINF", "NN", "KON", "VVIMP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Weil es vergn\u00fcgter ihm gehorcht/ als frey seyn kan.", "tokens": ["Weil", "es", "ver\u00b7gn\u00fcg\u00b7ter", "ihm", "ge\u00b7horcht", "/", "als", "frey", "seyn", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "PPER", "VVPP", "$(