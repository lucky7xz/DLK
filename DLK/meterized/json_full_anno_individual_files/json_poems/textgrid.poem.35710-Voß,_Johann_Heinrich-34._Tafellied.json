{"textgrid.poem.35710": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "34. Tafellied", "genre": "verse", "period": "N.A.", "pub_year": 1787, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie hehr im Glase blinket", "tokens": ["Wie", "hehr", "im", "Gla\u00b7se", "blin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der k\u00f6nigliche Wein!", "tokens": ["Der", "k\u00f6\u00b7nig\u00b7li\u00b7che", "Wein", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wie str\u00f6mt sein Duft! O trinket,", "tokens": ["Wie", "str\u00f6mt", "sein", "Duft", "!", "O", "trin\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "PPOSAT", "NN", "$.", "NE", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und la\u00dft uns fr\u00f6hlich sein!", "tokens": ["Und", "la\u00dft", "uns", "fr\u00f6h\u00b7lich", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch f\u00e4lscht ein Rebenhasser", "tokens": ["Doch", "f\u00e4lscht", "ein", "Re\u00b7ben\u00b7has\u00b7ser"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Den Feuertrank mit Wasser;", "tokens": ["Den", "Feu\u00b7er\u00b7trank", "mit", "Was\u00b7ser", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und reicht ihm klares Wasser!", "tokens": ["Und", "reicht", "ihm", "kla\u00b7res", "Was\u00b7ser", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der edle Wein erweitert", "tokens": ["Der", "ed\u00b7le", "Wein", "er\u00b7wei\u00b7tert"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Des edlen Mannes Herz,", "tokens": ["Des", "ed\u00b7len", "Man\u00b7nes", "Herz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Erhellt den Geist, und l\u00e4utert", "tokens": ["Er\u00b7hellt", "den", "Geist", ",", "und", "l\u00e4u\u00b7tert"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "VVFIN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Des Wortes Ernst und Scherz.", "tokens": ["Des", "Wor\u00b7tes", "Ernst", "und", "Scherz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Will jemand einen Sparren", "tokens": ["Will", "je\u00b7mand", "ei\u00b7nen", "Spar\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VMFIN", "PIS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Zu viel ins Dach uns narren;", "tokens": ["Zu", "viel", "ins", "Dach", "uns", "nar\u00b7ren", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPRART", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und la\u00dft ihm seinen Sparren!", "tokens": ["Und", "la\u00dft", "ihm", "sei\u00b7nen", "Spar\u00b7ren", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Es strahlt, wie Gottes Sonne,", "tokens": ["Es", "strahlt", ",", "wie", "Got\u00b7tes", "Son\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "NN", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die Wahrheit allgemein;", "tokens": ["Die", "Wahr\u00b7heit", "all\u00b7ge\u00b7mein", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nicht Kirche, Log' und Tonne", "tokens": ["Nicht", "Kir\u00b7che", ",", "Lo\u00b7g'", "und", "Ton\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["PTKNEG", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Des Denkers schlie\u00dft sie ein.", "tokens": ["Des", "Den\u00b7kers", "schlie\u00dft", "sie", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Wenn etwa Sch\u00e4lt' im Dunkeln", "tokens": ["Wenn", "et\u00b7wa", "Sch\u00e4lt'", "im", "Dun\u00b7keln"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "NN", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Von eigner Wahrheit munkeln;", "tokens": ["Von", "eig\u00b7ner", "Wahr\u00b7heit", "mun\u00b7keln", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und lacht der Sch\u00e4lk' im Dunkeln!", "tokens": ["Und", "lacht", "der", "Sch\u00e4l\u00b7k'", "im", "Dun\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}}, "stanza.4": {"line.1": {"text": "Kocht th\u00f6richt Gold im Tiegel,", "tokens": ["Kocht", "th\u00f6\u00b7richt", "Gold", "im", "Tie\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und blast den Diamant;", "tokens": ["Und", "blast", "den", "Di\u00b7a\u00b7mant", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Raubt Salomonis Siegel,", "tokens": ["Raubt", "Sa\u00b7lo\u00b7mo\u00b7nis", "Sie\u00b7gel", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Geister Graun, und bannt!", "tokens": ["Der", "Geis\u00b7ter", "Graun", ",", "und", "bannt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$,", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch wird zum Trank der Jugend", "tokens": ["Doch", "wird", "zum", "Trank", "der", "Ju\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPRART", "NN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Gebraut der Sterne Tugend;", "tokens": ["Ge\u00b7braut", "der", "Ster\u00b7ne", "Tu\u00b7gend", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Nur Wein ist Trank der Jugend!", "tokens": ["Nur", "Wein", "ist", "Trank", "der", "Ju\u00b7gend", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "VAFIN", "NN", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Wer Messe liebt zu pl\u00e4rren", "tokens": ["Wer", "Mes\u00b7se", "liebt", "zu", "pl\u00e4r\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "NN", "VVFIN", "PTKZU", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Am hellen Fronaltar,", "tokens": ["Am", "hel\u00b7len", "Fro\u00b7nal\u00b7tar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der spiel' auch Tempelherren", "tokens": ["Der", "spiel'", "auch", "Tem\u00b7pel\u00b7her\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "ADV", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "In wei\u00dfem Amtstalar!", "tokens": ["In", "wei\u00b7\u00dfem", "Amt\u00b7sta\u00b7lar", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Doch trennt man uns vom Bunde", "tokens": ["Doch", "trennt", "man", "uns", "vom", "Bun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PIS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Der feuchten Tafelrunde;", "tokens": ["Der", "feuch\u00b7ten", "Ta\u00b7fel\u00b7run\u00b7de", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.8": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.9": {"text": "Frisch!", "tokens": ["Frisch", "!"], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+", "measure": "single.up"}, "line.10": {"text": "Trommelt auf den Tisch!", "tokens": ["Trom\u00b7melt", "auf", "den", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.11": {"text": "Und fei'rt die Tafelrunde!", "tokens": ["Und", "fei'rt", "die", "Ta\u00b7fel\u00b7run\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN