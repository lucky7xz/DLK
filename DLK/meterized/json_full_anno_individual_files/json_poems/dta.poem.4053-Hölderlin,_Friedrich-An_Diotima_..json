{"dta.poem.4053": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "An Diotima .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Komm und bes\u00e4nftige mir, die du einst Elemente", "tokens": ["Komm", "und", "be\u00b7s\u00e4nf\u00b7ti\u00b7ge", "mir", ",", "die", "du", "einst", "E\u00b7le\u00b7men\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "$,", "PRELS", "PPER", "ADV", "NN"], "meter": "++-+-+-+-+-+--", "measure": "unknown.measure.septa"}, "line.2": {"text": "Wonne der himmlischen Muse, das Chaos der", "tokens": ["Won\u00b7ne", "der", "himm\u00b7li\u00b7schen", "Mu\u00b7se", ",", "das", "Chaos", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "$,", "ART", "NN", "ART"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.3": {"text": "Ordne den tobenden Kampf mit Friedenst\u00f6nen des", "tokens": ["Ord\u00b7ne", "den", "to\u00b7ben\u00b7den", "Kampf", "mit", "Frie\u00b7den\u00b7st\u00f6\u00b7nen", "des"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "ADJA", "NN", "APPR", "NN", "ART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Bis in der sterblichen Brust sich das entzweite", "tokens": ["Bis", "in", "der", "sterb\u00b7li\u00b7chen", "Brust", "sich", "das", "ent\u00b7zwei\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "PRF", "ART", "ADJA"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Bis der Menschen alte Natur, die ruhige, gro\u00dfe,", "tokens": ["Bis", "der", "Men\u00b7schen", "al\u00b7te", "Na\u00b7tur", ",", "die", "ru\u00b7hi\u00b7ge", ",", "gro\u00b7\u00dfe", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "ART", "ADJA", "$,", "ADJA", "$,"], "meter": "--+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Aus der g\u00e4hrenden Zeit m\u00e4chtig und heiter sich", "tokens": ["Aus", "der", "g\u00e4h\u00b7ren\u00b7den", "Zeit", "m\u00e4ch\u00b7tig", "und", "hei\u00b7ter", "sich"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "ADJD", "KON", "ADJD", "PRF"], "meter": "--+--++--+-+", "measure": "anapaest.di.plus"}, "line.7": {"text": "Kehr' in die d\u00fcrftigen Herzen des Volks, lebendige", "tokens": ["Kehr'", "in", "die", "d\u00fcrf\u00b7ti\u00b7gen", "Her\u00b7zen", "des", "Volks", ",", "le\u00b7ben\u00b7di\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "ART", "NN", "$,", "ADJA"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.8": {"text": "Kehr' an den gastlichen Tisch, kehr' in die Tempel", "tokens": ["Kehr'", "an", "den", "gast\u00b7li\u00b7chen", "Tisch", ",", "kehr'", "in", "die", "Tem\u00b7pel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "ADJA", "NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.9": {"text": "Denn Diotima lebt, wie die zarten Bl\u00fcthen im", "tokens": ["Denn", "Dio\u00b7ti\u00b7ma", "lebt", ",", "wie", "die", "zar\u00b7ten", "Bl\u00fc\u00b7then", "im"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "NE", "VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "APPRART"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Reich an eigenem Geist, sucht sie die Sonne", "tokens": ["Reich", "an", "ei\u00b7ge\u00b7nem", "Geist", ",", "sucht", "sie", "die", "Son\u00b7ne"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ADJA", "NN", "$,", "VVFIN", "PPER", "ART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "Aber die Sonne des Geists, die sch\u00f6nere Welt,", "tokens": ["A\u00b7ber", "die", "Son\u00b7ne", "des", "Geists", ",", "die", "sch\u00f6\u00b7ne\u00b7re", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-+--+", "measure": "dactylic.di.plus"}, "line.12": {"text": "Und in frostiger Nacht zanken Orkane sich nun.", "tokens": ["Und", "in", "fros\u00b7ti\u00b7ger", "Nacht", "zan\u00b7ken", "Or\u00b7ka\u00b7ne", "sich", "nun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "ADJA", "NN", "PRF", "ADV", "$."], "meter": "--+--++-++--+", "measure": "anapaest.di.plus"}}}}}