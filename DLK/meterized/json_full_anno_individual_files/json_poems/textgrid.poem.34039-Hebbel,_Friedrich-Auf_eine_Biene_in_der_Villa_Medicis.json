{"textgrid.poem.34039": {"metadata": {"author": {"name": "Hebbel, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Auf eine Biene in der Villa Medicis", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Holdes Bienchen, du irrst! Dort winkt dir bl\u00fchend der Lorbeer,", "tokens": ["Hol\u00b7des", "Bien\u00b7chen", ",", "du", "irrst", "!", "Dort", "winkt", "dir", "bl\u00fc\u00b7hend", "der", "Lor\u00b7beer", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "PPER", "ADV", "$.", "ADV", "VVFIN", "PPER", "VVPP", "ART", "NN", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Sprich, was umsurrst du denn mir emsig die Wang' und den Mund?", "tokens": ["Sprich", ",", "was", "um\u00b7surrst", "du", "denn", "mir", "em\u00b7sig", "die", "Wang'", "und", "den", "Mund", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "PWS", "VVFIN", "PPER", "ADV", "PPER", "ADJD", "ART", "NN", "KON", "ART", "NN", "$."], "meter": "+--+-+-+--+--+", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Honig entsaugst du mir nicht, du sei'st denn ein schelmisches M\u00e4dchen,", "tokens": ["Ho\u00b7nig", "ent\u00b7saugst", "du", "mir", "nicht", ",", "du", "sei'st", "denn", "ein", "schel\u00b7mi\u00b7sches", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "PPER", "PTKNEG", "$,", "PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "+-----+-+-+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "Das sich vermummte, und dann komm in der wahren Gestalt!", "tokens": ["Das", "sich", "ver\u00b7mumm\u00b7te", ",", "und", "dann", "komm", "in", "der", "wah\u00b7ren", "Ge\u00b7stalt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRF", "VVFIN", "$,", "KON", "ADV", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+---+--+--+", "measure": "iambic.penta.relaxed"}, "line.5": {"text": "Sinnst du mir aber ein Arges, gedenkst du, daf\u00fcr mich zu strafen,", "tokens": ["Sinnst", "du", "mir", "a\u00b7ber", "ein", "Ar\u00b7ges", ",", "ge\u00b7denkst", "du", ",", "da\u00b7f\u00fcr", "mich", "zu", "stra\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPER", "ADV", "ART", "NN", "$,", "VVFIN", "PPER", "$,", "PAV", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+--+--+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.6": {"text": "Da\u00df ich ein Mensch nur bin, nimmer die Rose des Thals,", "tokens": ["Da\u00df", "ich", "ein", "Mensch", "nur", "bin", ",", "nim\u00b7mer", "die", "Ro\u00b7se", "des", "Thals", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "VAFIN", "$,", "ADV", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.7": {"text": "Oder bin ich dir gar aus alter Zeit noch verschuldet,", "tokens": ["O\u00b7der", "bin", "ich", "dir", "gar", "aus", "al\u00b7ter", "Zeit", "noch", "ver\u00b7schul\u00b7det", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "PPER", "ADV", "APPR", "ADJA", "NN", "ADV", "VVPP", "$,"], "meter": "+-+--+-+-+--+-", "measure": "hexameter"}, "line.8": {"text": "Hab' ich als Blume vielleicht einst dir geweigert den Trunk:", "tokens": ["Hab'", "ich", "al