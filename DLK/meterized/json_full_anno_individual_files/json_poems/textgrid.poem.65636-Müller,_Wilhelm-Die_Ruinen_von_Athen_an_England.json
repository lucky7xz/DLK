{"textgrid.poem.65636": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Ruinen von Athen an England", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df dir unsern Dank gefallen, Hort der Freiheit, Engeland!", "tokens": ["La\u00df", "dir", "un\u00b7sern", "Dank", "ge\u00b7fal\u00b7len", ",", "Hort", "der", "Frei\u00b7heit", ",", "En\u00b7ge\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "$,", "NN", "ART", "NN", "$,", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hast zum Herrn der hohen Pforte einen edlen Lord gesandt,", "tokens": ["Hast", "zum", "Herrn", "der", "ho\u00b7hen", "Pfor\u00b7te", "ei\u00b7nen", "ed\u00b7len", "Lord", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df er sich f\u00fcr uns verwende; und er that es ritterlich \u2013", "tokens": ["Da\u00df", "er", "sich", "f\u00fcr", "uns", "ver\u00b7wen\u00b7de", ";", "und", "er", "that", "es", "rit\u00b7ter\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Griechen, h\u00f6rt, was er errungen hat mit scharfem Federstrich!", "tokens": ["Grie\u00b7chen", ",", "h\u00f6rt", ",", "was", "er", "er\u00b7run\u00b7gen", "hat", "mit", "schar\u00b7fem", "Fe\u00b7der\u00b7strich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wenn der jungen Freiheit Blume wird getreten in den Staub,", "tokens": ["Wenn", "der", "jun\u00b7gen", "Frei\u00b7heit", "Blu\u00b7me", "wird", "ge\u00b7tre\u00b7ten", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wenn die heil'ge Stadt Athene's wird des rohen Heiden Raub,", "tokens": ["Wenn", "die", "heil'\u00b7ge", "Stadt", "A\u00b7the\u00b7ne's", "wird", "des", "ro\u00b7hen", "Hei\u00b7den", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NE", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Dann, auch dann, \u2013 begreift es, Griechen, \u2013 sollen wir doch unversehrt", "tokens": ["Dann", ",", "auch", "dann", ",", "\u2013", "be\u00b7greift", "es", ",", "Grie\u00b7chen", ",", "\u2013", "sol\u00b7len", "wir", "doch", "un\u00b7ver\u00b7sehrt"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "NN", "$,", "$(", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Stehn, beschirmt im Sturm der Waffen durch des wilden Feindes Schwert.", "tokens": ["Stehn", ",", "be\u00b7schirmt", "im", "Sturm", "der", "Waf\u00b7fen", "durch", "des", "wil\u00b7den", "Fein\u00b7des", "Schwert", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "ART", "NN", "APPR", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.9": {"text": "La\u00df dir unsern Dank gefallen, Hort der Freiheit, Engeland!", "tokens": ["La\u00df", "dir", "un\u00b7sern", "Dank", "ge\u00b7fal\u00b7len", ",", "Hort", "der", "Frei\u00b7heit", ",", "En\u00b7ge\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "$,", "NN", "ART", "NN", "$,", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.10": {"text": "Schade, schade, hast vergebens deinen edlen Lord gesandt.", "tokens": ["Scha\u00b7de", ",", "scha\u00b7de", ",", "hast", "ver\u00b7ge\u00b7bens", "dei\u00b7nen", "ed\u00b7len", "Lord", "ge\u00b7sandt", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "VVFIN", "$,", "VAFIN", "ADV", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.11": {"text": "Keine Bittschrift kann uns retten \u2013 die Ruinen von Athen", "tokens": ["Kei\u00b7ne", "Bitt\u00b7schrift", "kann", "uns", "ret\u00b7ten", "\u2013", "die", "Ru\u00b7i\u00b7nen", "von", "A\u00b7then"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIAT", "NN", "VMFIN", "PPER", "VVINF", "$(", "ART", "NN", "APPR", "NE"], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.12": {"text": "Werden mit den freien Griechen wanken, st\u00fcrzen, untergehn.", "tokens": ["Wer\u00b7den", "mit", "den", "frei\u00b7en", "Grie\u00b7chen", "wan\u00b7ken", ",", "st\u00fcr\u00b7zen", ",", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN", "VVINF", "$,", "VVFIN", "$,", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.13": {"text": "Lange haben wir gestanden unter Schmach und Schimpf und Leid,", "tokens": ["Lan\u00b7ge", "ha\u00b7ben", "wir", "ge\u00b7stan\u00b7den", "un\u00b7ter", "Schmach", "und", "Schimpf", "und", "Leid", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVPP", "APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.14": {"text": "Mochten kaum uns aufrecht halten in der jammervollen Zeit.", "tokens": ["Moch\u00b7ten", "kaum", "uns", "auf\u00b7recht", "hal\u00b7ten", "in", "der", "jam\u00b7mer\u00b7vol\u00b7len", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPER", "ADJD", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.15": {"text": "Fremde kamen hergewandert, staunten uns verwundert an,", "tokens": ["Frem\u00b7de", "ka\u00b7men", "her\u00b7ge\u00b7wan\u00b7dert", ",", "staun\u00b7ten", "uns", "ver\u00b7wun\u00b7dert", "an", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVPP", "$,", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.16": {"text": "Und wir lie\u00dfen es geschehen, aber's lag uns wenig dran;", "tokens": ["Und", "wir", "lie\u00b7\u00dfen", "es", "ge\u00b7sche\u00b7hen", ",", "a\u00b7ber's", "lag", "uns", "we\u00b7nig", "dran", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PPER", "VVPP", "$,", "ADV", "VVFIN", "PPER", "PIS", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.17": {"text": "Lie\u00dfen messen sie und malen \u2013 Keiner malt und mi\u00dft den Geist \u2013", "tokens": ["Lie\u00b7\u00dfen", "mes\u00b7sen", "sie", "und", "ma\u00b7len", "\u2013", "Kei\u00b7ner", "malt", "und", "mi\u00dft", "den", "Geist", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "KON", "VVINF", "$(", "PIS", "VVFIN", "KON", "VVFIN", "ART", "NN", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.18": {"text": "Und sie geben sich zufrieden, wissen sie, wie Jedes hei\u00dft.", "tokens": ["Und", "sie", "ge\u00b7ben", "sich", "zu\u00b7frie\u00b7den", ",", "wis\u00b7sen", "sie", ",", "wie", "Je\u00b7des", "hei\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "PRF", "ADJD", "$,", "VVFIN", "PPER", "$,", "PWAV", "PIS", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.19": {"text": "Auch ein gro\u00dfer Lord ist kommen, hat von unserm morschen Haupt", "tokens": ["Auch", "ein", "gro\u00b7\u00dfer", "Lord", "ist", "kom\u00b7men", ",", "hat", "von", "un\u00b7serm", "mor\u00b7schen", "Haupt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "ADJA", "NN", "VAFIN", "VVINF", "$,", "VAFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.20": {"text": "Im Entz\u00fccken der Bewunderung uns der Bilder Schmuck geraubt.", "tokens": ["Im", "Ent\u00b7z\u00fc\u00b7cken", "der", "Be\u00b7wun\u00b7de\u00b7rung", "uns", "der", "Bil\u00b7der", "Schmuck", "ge\u00b7raubt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PPER", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+--+-+-+-+", "measure": "trochaic.octa.plus.relaxed"}, "line.21": {"text": "Mag er ziehen mit der Beute! \u2013 Heil uns, da\u00df wir fest noch stehn,", "tokens": ["Mag", "er", "zie\u00b7hen", "mit", "der", "Beu\u00b7te", "!", "\u2013", "Heil", "uns", ",", "da\u00df", "wir", "fest", "noch", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "VVFIN", "APPR", "ART", "NN", "$.", "$(", "NN", "PPER", "$,", "KOUS", "PPER", "ADJD", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.22": {"text": "Um der Freiheit Morgenr\u00f6the nach so langer Nacht zu sehn!", "tokens": ["Um", "der", "Frei\u00b7heit", "Mor\u00b7gen\u00b7r\u00f6\u00b7the", "nach", "so", "lan\u00b7ger", "Nacht", "zu", "sehn", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "NN", "APPR", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.23": {"text": "Statt der G\u00f6tterbilder tragen wir das Banner in die Luft,", "tokens": ["Statt", "der", "G\u00f6t\u00b7ter\u00b7bil\u00b7der", "tra\u00b7gen", "wir", "das", "Ban\u00b7ner", "in", "die", "Luft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.24": {"text": "Das zum Kampf mit den Barbaren Hellas tapfre S\u00f6hne ruft.", "tokens": ["Das", "zum", "Kampf", "mit", "den", "Bar\u00b7ba\u00b7ren", "Hel\u00b7las", "tapf\u00b7re", "S\u00f6h\u00b7ne", "ruft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "APPRART", "NN", "APPR", "ART", "NN", "NE", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+--+-+-+-+", "measure": "anapaest.tri.plus"}, "line.25": {"text": "Ach, wenn diese unterliegen, wozu sollen wir denn stehn?", "tokens": ["Ach", ",", "wenn", "die\u00b7se", "un\u00b7ter\u00b7lie\u00b7gen", ",", "wo\u00b7zu", "sol\u00b7len", "wir", "denn", "stehn", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "KOUS", "PDS", "VVINF", "$,", "PWAV", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.26": {"text": "Habt sie ja in euren B\u00fcchern, die Ruinen von Athen.", "tokens": ["Habt", "sie", "ja", "in", "eu\u00b7ren", "B\u00fc\u00b7chern", ",", "die", "Ru\u00b7i\u00b7nen", "von", "A\u00b7then", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "APPR", "PPOSAT", "NN", "$,", "ART", "NN", "APPR", "NE", "$."], "meter": "+-+-+-+-+-+--+-", "measure": "trochaic.septa.relaxed"}, "line.27": {"text": "Mit der Freiheit letztem Schlage st\u00fcrzen unsre Mauern ein,", "tokens": ["Mit", "der", "Frei\u00b7heit", "letz\u00b7tem", "Schla\u00b7ge", "st\u00fcr\u00b7zen", "uns\u00b7re", "Mau\u00b7ern", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "VVFIN", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.28": {"text": "Und auf jedes Helden H\u00fcgel werfen wir noch einen Stein.", "tokens": ["Und", "auf", "je\u00b7des", "Hel\u00b7den", "H\u00fc\u00b7gel", "wer\u00b7fen", "wir", "noch", "ei\u00b7nen", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "NN", "VVFIN", "PPER", "ADV", "ART", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}, "stanza.2": {"line.1": {"text": "La\u00df dir unsern Dank gefallen, Hort der Freiheit, Engeland!", "tokens": ["La\u00df", "dir", "un\u00b7sern", "Dank", "ge\u00b7fal\u00b7len", ",", "Hort", "der", "Frei\u00b7heit", ",", "En\u00b7ge\u00b7land", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "VVPP", "$,", "NN", "ART", "NN", "$,", "NE", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Hast zum Herrn der hohen Pforte einen edlen Lord gesandt,", "tokens": ["Hast", "zum", "Herrn", "der", "ho\u00b7hen", "Pfor\u00b7te", "ei\u00b7nen", "ed\u00b7len", "Lord", "ge\u00b7sandt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPRART", "NN", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.3": {"text": "Da\u00df er sich f\u00fcr uns verwende; und er that es ritterlich \u2013", "tokens": ["Da\u00df", "er", "sich", "f\u00fcr", "uns", "ver\u00b7wen\u00b7de", ";", "und", "er", "that", "es", "rit\u00b7ter\u00b7lich", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$.", "KON", "PPER", "VVFIN", "PPER", "ADJD", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Griechen, h\u00f6rt, was er errungen hat mit scharfem Federstrich!", "tokens": ["Grie\u00b7chen", ",", "h\u00f6rt", ",", "was", "er", "er\u00b7run\u00b7gen", "hat", "mit", "schar\u00b7fem", "Fe\u00b7der\u00b7strich", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "PWS", "PPER", "VVPP", "VAFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Wenn der jungen Freiheit Blume wird getreten in den Staub,", "tokens": ["Wenn", "der", "jun\u00b7gen", "Frei\u00b7heit", "Blu\u00b7me", "wird", "ge\u00b7tre\u00b7ten", "in", "den", "Staub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NN", "VAFIN", "VVPP", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.6": {"text": "Wenn die heil'ge Stadt Athene's wird des rohen Heiden Raub,", "tokens": ["Wenn", "die", "heil'\u00b7ge", "Stadt", "A\u00b7the\u00b7ne's", "wird", "des", "ro\u00b7hen", "Hei\u00b7den", "Raub", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "NE", "VAFIN", "ART", "ADJA", "NN", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.7": {"text": "Dann, auch dann, \u2013 begreift es, Griechen, \u2013 sollen wir doch unversehrt", "tokens": ["Dann", ",", "auch", "dann", ",", "\u2013", "be\u00b7greift", "es", ",", "Grie\u00b7chen", ",", "\u2013", "sol\u00b7len", "wir", "doch", "un\u00b7ver\u00b7sehrt"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADV", "$,", "$(", "VVFIN", "PPER", "$,", "NN", "$,", "$(", "VMFIN", "PPER", "ADV", "ADJD"], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.8": {"text": "Stehn, beschirmt im Sturm der Waff