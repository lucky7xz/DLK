{"textgrid.poem.52244": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wie unsre mut'gen Orlogsm\u00e4nner hei\u00dfen?", "tokens": ["Wie", "uns\u00b7re", "mut'\u00b7gen", "Or\u00b7logs\u00b7m\u00e4n\u00b7ner", "hei\u00b7\u00dfen", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Komm mit aufs Meer, ich will es dir verk\u00fcnden!", "tokens": ["Komm", "mit", "aufs", "Meer", ",", "ich", "will", "es", "dir", "ver\u00b7k\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "APPRART", "NN", "$,", "PPER", "VMFIN", "PPER", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da dr\u00fcben der mit sechzig Feuerschl\u00fcnden,", "tokens": ["Da", "dr\u00fc\u00b7ben", "der", "mit", "sech\u00b7zig", "Feu\u00b7er\u00b7schl\u00fcn\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Das ist \u00bbder Arndt!\u00ab Du siehst die Goldschrift glei\u00dfen!", "tokens": ["Das", "ist", "\u00bb", "der", "Arndt", "!", "\u00ab", "Du", "siehst", "die", "Gold\u00b7schrift", "glei\u00b7\u00dfen", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "$(", "ART", "NN", "$.", "$(", "PPER", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Hier die Fregatte, bauschig rings von wei\u00dfen,", "tokens": ["Hier", "die", "Fre\u00b7gat\u00b7te", ",", "bau\u00b7schig", "rings", "von", "wei\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "NN", "$,", "ADJD", "ADV", "APPR", "ADJA", "$,"], "meter": "-+++-+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Halbvollen Segeln, k\u00e4mpfend mit den Winden \u2013", "tokens": ["Halb\u00b7vol\u00b7len", "Se\u00b7geln", ",", "k\u00e4mp\u00b7fend", "mit", "den", "Win\u00b7den", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJD", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "O Gott, ihr Name mahnt an alte S\u00fcnden! \u2013", "tokens": ["O", "Gott", ",", "ihr", "Na\u00b7me", "mahnt", "an", "al\u00b7te", "S\u00fcn\u00b7den", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$,", "PPOSAT", "NN", "VVFIN", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "\u00bbdie Sieben\u00ab hei\u00dft sie! Mag kein Strick ihr rei\u00dfen!", "tokens": ["\u00bb", "die", "Sie\u00b7ben", "\u00ab", "hei\u00dft", "sie", "!", "Mag", "kein", "Strick", "ihr", "rei\u00b7\u00dfen", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$(", "VVFIN", "PPER", "$.", "VMFIN", "PIAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Dort die Korvette, segelnd wie der Blitz,", "tokens": ["Dort", "die", "Kor\u00b7vet\u00b7te", ",", "se\u00b7gelnd", "wie", "der", "Blitz", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"