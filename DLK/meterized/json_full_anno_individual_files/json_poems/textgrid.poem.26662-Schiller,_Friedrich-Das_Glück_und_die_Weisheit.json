{"textgrid.poem.26662": {"metadata": {"author": {"name": "Schiller, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Das Gl\u00fcck und die Weisheit", "genre": "verse", "period": "N.A.", "pub_year": 1782, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Entzweit mit einem Favoriten,", "tokens": ["Ent\u00b7zweit", "mit", "ei\u00b7nem", "Fa\u00b7vo\u00b7ri\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Flog einst Fortun' der Weisheit zu.", "tokens": ["Flog", "einst", "For\u00b7tun'", "der", "Weis\u00b7heit", "zu", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.3": {"text": "\u00bbich will dir meine Sch\u00e4tze bieten,", "tokens": ["\u00bb", "ich", "will", "dir", "mei\u00b7ne", "Sch\u00e4t\u00b7ze", "bie\u00b7ten", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VMFIN", "PPER", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei meine Freundin du!", "tokens": ["Sei", "mei\u00b7ne", "Freun\u00b7din", "du", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Mein F\u00fcllhorn go\u00df ich dem Verschwender", "tokens": ["Mein", "F\u00fcll\u00b7horn", "go\u00df", "ich", "dem", "Ver\u00b7schwen\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "In seinen Scho\u00df, so m\u00fctterlich!", "tokens": ["In", "sei\u00b7nen", "Scho\u00df", ",", "so", "m\u00fct\u00b7ter\u00b7lich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sieh! Er fodert drum nicht minder", "tokens": ["Und", "sieh", "!", "Er", "fo\u00b7dert", "drum", "nicht", "min\u00b7der"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "PPER", "VVFIN", "PAV", "PTKNEG", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und nennt noch geizig mich.", "tokens": ["Und", "nennt", "noch", "gei\u00b7zig", "mich", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Komm, Schwester, la\u00df uns Freundschaft schlie\u00dfen,", "tokens": ["Komm", ",", "Schwes\u00b7ter", ",", "la\u00df", "uns", "Freund\u00b7schaft", "schlie\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "NN", "$,", "VVIMP", "PPER", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du keuchst so schwer an deinem Pflug.", "tokens": ["Du", "keuchst", "so", "schwer", "an", "dei\u00b7nem", "Pflug", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In deinen Scho\u00df will ich sie gie\u00dfen,", "tokens": ["In", "dei\u00b7nen", "Scho\u00df", "will", "ich", "sie", "gie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VMFIN", "PPER", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf, folge mir! \u2013 Du hast genug.\u00ab", "tokens": ["Auf", ",", "fol\u00b7ge", "mir", "!", "\u2013", "Du", "hast", "ge\u00b7nug", ".", "\u00ab"], "token_info": ["word", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "$,", "VVFIN", "PPER", "$.", "$(", "PPER", "VAFIN", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Die Weisheit l\u00e4\u00dft die Schaufel sinken", "tokens": ["Die", "Weis\u00b7heit", "l\u00e4\u00dft", "die", "Schau\u00b7fel", "sin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und wischt den Schwei\u00df vom Angesicht.", "tokens": ["Und", "wischt", "den", "Schwei\u00df", "vom", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbdort eilt dein Freund \u2013 sich zu erhenken,", "tokens": ["\u00bb", "dort", "eilt", "dein", "Freund", "\u2013", "sich", "zu", "er\u00b7hen\u00b7ken", ","], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "PPOSAT", "NN", "$(", "PRF", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Vers\u00f6hnet euch \u2013 ich brauch dich nicht.\u00ab", "tokens": ["Ver\u00b7s\u00f6h\u00b7net", "euch", "\u2013", "ich", "brauch", "dich", "nicht", ".", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "PPER", "$(", "PPER", "VVFIN", "PPER", "PTKNEG", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Entzweit mit einem Favor