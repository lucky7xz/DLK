{"textgrid.poem.65835": {"metadata": {"author": {"name": "Shakespeare, William", "birth": "N.A.", "death": "N.A."}, "title": "109", "genre": "verse", "period": "N.A.", "pub_year": 1590, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O nimmer sprich zu mir: \u00bbTreulose Seele!\u00ab", "tokens": ["O", "nim\u00b7mer", "sprich", "zu", "mir", ":", "\u00bb", "Treu\u00b7lo\u00b7se", "See\u00b7le", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["NE", "ADV", "ADJD", "APPR", "PPER", "$.", "$(", "ADJA", "NN", "$.", "$("], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Schien Trennung gleich zu wandeln meine Glut:", "tokens": ["Schien", "Tren\u00b7nung", "gleich", "zu", "wan\u00b7deln", "mei\u00b7ne", "Glut", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "ADV", "PTKZU", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Weil ich so leicht mir selber ja mich st\u00f6hle", "tokens": ["Weil", "ich", "so", "leicht", "mir", "sel\u00b7ber", "ja", "mich", "st\u00f6h\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "ADJD", "PPER", "ADV", "ADV", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als meinem Geist, der dir im Busen ruht.", "tokens": ["Als", "mei\u00b7nem", "Geist", ",", "der", "dir", "im", "Bu\u00b7sen", "ruht", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "$,", "PRELS", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Da ist mein Freundeshaus! Schw\u00e4rmt' ich vom Ziel,", "tokens": ["Da", "ist", "mein", "Freun\u00b7des\u00b7haus", "!", "Schw\u00e4rmt'", "ich", "vom", "Ziel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "NN", "$.", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Doch kehr' ich heim von langen Wegesstrecken:", "tokens": ["Doch", "kehr'", "ich", "heim", "von", "lan\u00b7gen", "We\u00b7ges\u00b7stre\u00b7cken", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Der Zeit gehorchend, nicht ihr Launenspiel,", "tokens": ["Der", "Zeit", "ge\u00b7hor\u00b7chend", ",", "nicht", "ihr", "Lau\u00b7nen\u00b7spiel", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$,", "PTKNEG", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Bring' ich das Wasser selbst f\u00fcr meine Flecken.", "tokens": ["Bring'", "ich", "das", "Was\u00b7ser", "selbst", "f\u00fcr", "mei\u00b7ne", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "O halte nicht, und w\u00e4r es gleich bedeckt", "tokens": ["O", "hal\u00b7te", "nicht", ",", "und", "w\u00e4r", "es", "gleich", "be\u00b7deckt"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PTKNEG", "$,", "KON", "VAFIN", "PPER", "ADV", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Mit jeglichem Gebrechen jedes Blutes,", "tokens": ["Mit", "jeg\u00b7li\u00b7chem", "Ge\u00b7bre\u00b7chen", "je\u00b7des", "Bl