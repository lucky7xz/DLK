{"textgrid.poem.38521": {"metadata": {"author": {"name": "Matthisson, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "An den Tod", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kronengeber, o Tod! eile, entkette mich!", "tokens": ["Kro\u00b7nen\u00b7ge\u00b7ber", ",", "o", "Tod", "!", "ei\u00b7le", ",", "ent\u00b7ket\u00b7te", "mich", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "FM", "NN", "$.", "VVFIN", "$,", "VVFIN", "PPER", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Bring die Palme des Sieges mir!", "tokens": ["Bring", "die", "Pal\u00b7me", "des", "Sie\u00b7ges", "mir", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PPER", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Bald erscheine mir, bald! fl\u00fcgle mich himmelan", "tokens": ["Bald", "er\u00b7schei\u00b7ne", "mir", ",", "bald", "!", "fl\u00fcg\u00b7le", "mich", "him\u00b7me\u00b7lan"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADV", "$.", "VVFIN", "PPER", "ADV"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Zu den H\u00f6hen der Seligen;", "tokens": ["Zu", "den", "H\u00f6\u00b7hen", "der", "Se\u00b7li\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Da\u00df ich engelverkl\u00e4rt knie an Gottes Thron,", "tokens": ["Da\u00df", "ich", "en\u00b7gel\u00b7ver\u00b7kl\u00e4rt", "knie", "an", "Got\u00b7tes", "Thron", ","], "token_info": ["word", "word", "word", "word", "word",