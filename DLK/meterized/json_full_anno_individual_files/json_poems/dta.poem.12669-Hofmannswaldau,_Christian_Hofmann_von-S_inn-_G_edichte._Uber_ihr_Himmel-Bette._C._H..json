{"dta.poem.12669": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "S inn- G edichte.  \n Uber ihr Himmel-Bette.  \n C. H.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Hjer hat die Delie den himmel auf der erden,", "tokens": ["Hjer", "hat", "die", "De\u00b7lie", "den", "him\u00b7mel", "auf", "der", "er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "Hier ruhet die, die mir viel unruh macht:", "tokens": ["Hier", "ru\u00b7het", "die", ",", "die", "mir", "viel", "un\u00b7ruh", "macht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$,", "PRELS", "PPER", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hier schl\u00e4fet die/ um die mein geist stets wacht,", "tokens": ["Hier", "schl\u00e4\u00b7fet", "die", "/", "um", "die", "mein", "geist", "stets", "wacht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$(", "APPR", "PRELS", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und tr\u00e4umet, unbesorgt, da\u00df ich zum schatten werde;", "tokens": ["Und", "tr\u00e4u\u00b7met", ",", "un\u00b7be\u00b7sorgt", ",", "da\u00df", "ich", "zum", "schat\u00b7ten", "wer\u00b7de", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "ADJD", "$,", "KOUS", "PPER", "APPRART", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Hier lieget die, vor der mein hertze liegt,", "tokens": ["Hier", "lie\u00b7get", "die", ",", "vor", "der", "mein", "hert\u00b7ze", "liegt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "$,", "APPR", "ART", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Und athmet, da\u00df in sie ein hauffen seuffzer fliegt;", "tokens": ["Und", "ath\u00b7met", ",", "da\u00df", "in", "sie", "ein", "hauf\u00b7fen", "seuff\u00b7zer", "fliegt", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "APPR", "PPER", "ART", "NN", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ach athmete sie doch davon die meinen ein!", "tokens": ["Ach", "ath\u00b7me\u00b7te", "sie", "doch", "da\u00b7von", "die", "mei\u00b7nen", "ein", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "PAV", "ART", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So k\u00f6nt\u2019 ich sehr begl\u00fcckt durch diesen himmel seyn", "tokens": ["So", "k\u00f6nt'", "ich", "sehr", "be\u00b7gl\u00fcckt", "durch", "die\u00b7sen", "him\u00b7mel", "seyn"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "VVPP", "APPR", "PDAT", "NN", "VAINF"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Ach wer doch nur, du leichtes decke-bette!", "tokens": ["Ach", "wer", "doch", "nur", ",", "du", "leich\u00b7tes", "de\u00b7cke\u00b7bet\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "PWS", "ADV", "ADV", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ein eintzigmal dein amt hier zu verwalten h\u00e4tte!", "tokens": ["Ein", "eint\u00b7zig\u00b7mal", "dein", "amt", "hier", "zu", "ver\u00b7wal\u00b7ten", "h\u00e4t\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "PPOSAT", "NN", "ADV", "PTKZU", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Es m\u00f6chte zwar in diesem himmel ein finsternis davon entstehn/", "tokens": ["Es", "m\u00f6ch\u00b7te", "zwar", "in", "die\u00b7sem", "him\u00b7mel", "ein", "fins\u00b7ter\u00b7nis", "da\u00b7von", "ent\u00b7stehn", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PDAT", "NN", "ART", "NN", "PAV", "VVINF", "$("], "meter": "-+-+-+-+--+-+-+-+", "measure": "iambic.octa.plus.relaxed"}, "line.12": {"text": "Wenn ich, als mond, o Sonne! dir das ", "tokens": ["Wenn", "ich", ",", "als", "mond", ",", "o", "Son\u00b7ne", "!", "dir", "das"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "$,", "KOUS", "NN", "$,", "FM", "NN", "$.", "PPER", "ART"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.13": {"text": "Hingegen w\u00fcrd\u2019 es mir weit besser als tausend andern buhlern", "tokens": ["Hin\u00b7ge\u00b7gen", "w\u00fcrd'", "es", "mir", "weit", "bes\u00b7ser", "als", "tau\u00b7send", "an\u00b7dern", "buh\u00b7lern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADJD", "ADJD", "KOKOM", "CARD", "ADJA", "NN"], "meter": "+-++--+-+-+-+-+-", "measure": "trochaic.octa.plus.relaxed"}, "line.14": {"text": "gehn,", "tokens": ["gehn", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Weil ich alsdenn im himmel-bette, und auch zugleich im himmel", "tokens": ["Weil", "ich", "als\u00b7denn", "im", "him\u00b7mel\u00b7bet\u00b7te", ",", "und", "auch", "zu\u00b7gleich", "im", "him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPRART", "NN", "$,", "KON", "ADV", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+-", "measure": "iambic.septa.relaxed"}, "line.16": {"text": "steckte.", "tokens": ["steck\u00b7te", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}}}}}