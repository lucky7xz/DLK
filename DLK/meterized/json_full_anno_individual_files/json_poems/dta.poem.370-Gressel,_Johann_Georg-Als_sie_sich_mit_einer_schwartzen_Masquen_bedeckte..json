{"dta.poem.370": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sie sich mit einer schwartzen  Masquen  \n bedeckte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Wirff deine Masque weg/ zeig uns dein Angesicht/", "tokens": ["Wirff", "dei\u00b7ne", "Mas\u00b7que", "weg", "/", "zeig", "uns", "dein", "An\u00b7ge\u00b7sicht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPOSAT", "NN", "PTKVZ", "$(", "VVFIN", "PPER", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und mache dich nicht schwartz/ dein Glantz entf\u00e4rbt sich", "tokens": ["Und", "ma\u00b7che", "dich", "nicht", "schwartz", "/", "dein", "Glantz", "ent\u00b7f\u00e4rbt", "sich"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADJD", "$(", "PPOSAT", "NN", "VVFIN", "PRF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "nicht.", "tokens": ["nicht", "."], "token_info": ["word", "punct"], "pos": ["PTKNEG", "$."], "meter": "-", "measure": "single.down"}}}}}