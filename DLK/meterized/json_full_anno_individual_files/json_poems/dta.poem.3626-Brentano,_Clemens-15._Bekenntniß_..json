{"dta.poem.3626": {"metadata": {"author": {"name": "Brentano, Clemens", "birth": "N.A.", "death": "N.A."}, "title": "15.  Bekenntni\u00df .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1808", "urn": "urn:nbn:de:kobv:b4-20090519172", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unsch\u00e4tzbares Einfaltwesen!               ", "tokens": ["Un\u00b7sch\u00e4tz\u00b7ba\u00b7res", "Ein\u00b7falt\u00b7we\u00b7sen", "!"], "token_info": ["word", "word", "punct"], "pos": ["PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Perle, die ich mir erlesen;", "tokens": ["Per\u00b7le", ",", "die", "ich", "mir", "er\u00b7le\u00b7sen", ";"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Vielheit in mir ganz vernicht,", "tokens": ["Viel\u00b7heit", "in", "mir", "ganz", "ver\u00b7nicht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "PPER", "ADV", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und mein Aug auf dich nur richt.", "tokens": ["Und", "mein", "Aug", "auf", "dich", "nur", "richt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Mach mich los vom Doppeltsehen!", "tokens": ["Mach", "mich", "los", "vom", "Dop\u00b7pelt\u00b7se\u00b7hen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKVZ", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "La\u00df auf eins den Sinn nur gehen;", "tokens": ["La\u00df", "auf", "eins", "den", "Sinn", "nur", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "PIS", "ART", "NN", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "In recht unverr\u00fcckter Treu,", "tokens": ["In", "recht", "un\u00b7ver\u00b7r\u00fcck\u00b7ter", "Treu", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Und von allen T\u00fccken frey.", "tokens": ["Und", "von", "al\u00b7len", "T\u00fc\u00b7cken", "frey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PIAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ey so mach mich dann aufrichtig,", "tokens": ["Ey", "so", "mach", "mich", "dann", "auf\u00b7rich\u00b7tig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einen Leib, der ganz durchsichtig;", "tokens": ["Ei\u00b7nen", "Leib", ",", "der", "ganz", "durch\u00b7sich\u00b7tig", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Licht sey, schaff und ruf in mir", "tokens": ["Licht", "sey", ",", "schaff", "und", "ruf", "in", "mir"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "$,", "VVFIN", "KON", "NN", "APPR", "PPER"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus der Finsterni\u00df herf\u00fcr.", "tokens": ["Aus", "der", "Fins\u00b7ter\u00b7ni\u00df", "her\u00b7f\u00fcr", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Mache neu die alte Erde,", "tokens": ["Ma\u00b7che", "neu", "die", "al\u00b7te", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da\u00df sie kristallinisch werde;", "tokens": ["Da\u00df", "sie", "kris\u00b7tal\u00b7li\u00b7nisch", "wer\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VAFIN", "$."], "meter": "+-++--+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Und das Meer la\u00df seyn nicht mehr,", "tokens": ["Und", "das", "Meer", "la\u00df", "seyn", "nicht", "mehr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VAINF", "PTKNEG", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ausser nur dein gl\u00e4sern Meer.", "tokens": ["Aus\u00b7ser", "nur", "dein", "gl\u00e4\u00b7sern", "Meer", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "PPOSAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Dieses la\u00df mit Feuerg\u00fcssen", "tokens": ["Die\u00b7ses", "la\u00df", "mit", "Feu\u00b7er\u00b7g\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aus dir in mich \u00fcberflie\u00dfen:", "tokens": ["Aus", "dir", "in", "mich", "\u00fc\u00b7berf\u00b7lie\u00b7\u00dfen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "APPR", "PRF", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Komm o stark erhabne Fluth,", "tokens": ["Komm", "o", "stark", "er\u00b7hab\u00b7ne", "Fluth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "FM", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Rei\u00df mich hin ins h\u00f6chste Gut.", "tokens": ["Rei\u00df", "mich", "hin", "ins", "h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}