{"dta.poem.4074": {"metadata": {"author": {"name": "H\u00f6lderlin, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Archipelagus .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1826", "urn": "urn:nbn:de:kobv:b4-200905197104", "language": ["de:0.99"], "booktitle": "H\u00f6lderlin, Friedrich: Gedichte. Stuttgart u. a., 1826."}, "poem": {"stanza.1": {"line.1": {"text": "Kehren die Kraniche wieder zu dir? und suchen", "tokens": ["Keh\u00b7ren", "die", "Kra\u00b7ni\u00b7che", "wie\u00b7der", "zu", "dir", "?", "und", "su\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "APPR", "PPER", "$.", "KON", "VVINF"], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "zu deinen", "tokens": ["zu", "dei\u00b7nen"], "token_info": ["word", "word"], "pos": ["APPR", "PPOSAT"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Ufern wieder die Schiffe den Lauf? umathmen er-", "tokens": ["U\u00b7fern", "wie\u00b7der", "die", "Schif\u00b7fe", "den", "Lauf", "?", "um\u00b7ath\u00b7men", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "ART", "NN", "ART", "NN", "$.", "VVFIN", "TRUNC"], "meter": "+-+--+--+-+--", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "w\u00fcnschte", "tokens": ["w\u00fcnschte"], "token_info": ["word"], "pos": ["VVFIN"], "meter": "+", "measure": "single.up"}, "line.5": {"text": "L\u00fcfte dir die beruhigte Flut, und sonnet der Delphin,", "tokens": ["L\u00fcf\u00b7te", "dir", "die", "be\u00b7ru\u00b7hig\u00b7te", "Flut", ",", "und", "son\u00b7net", "der", "Del\u00b7phin", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "ADJA", "NN", "$,", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+-+---+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.6": {"text": "Aus der Tiefe gelockt, am neuen Lichte den R\u00fccken?", "tokens": ["Aus", "der", "Tie\u00b7fe", "ge\u00b7lockt", ",", "am", "neu\u00b7en", "Lich\u00b7te", "den", "R\u00fc\u00b7cken", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "APPRART", "ADJA", "NN", "ART", "NN", "$."], "meter": "--+--+-+-+--+-", "measure": "anapaest.di.plus"}, "line.7": {"text": "Bl\u00fcht Jonien? ist es die Zeit? denn immer im", "tokens": ["Bl\u00fcht", "Jo\u00b7ni\u00b7en", "?", "ist", "es", "die", "Zeit", "?", "denn", "im\u00b7mer", "im"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "NE", "$.", "VAFIN", "PPER", "ART", "NN", "$.", "ADV", "ADV", "APPRART"], "meter": "+---+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Fr\u00fchling,", "tokens": ["Fr\u00fch\u00b7ling", ","], "token_info": ["word", "punct"], "pos": ["NE", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wenn den Lebenden sich das Herz erneut und die", "tokens": ["Wenn", "den", "Le\u00b7ben\u00b7den", "sich", "das", "Herz", "er\u00b7neut", "und", "die"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "PRF", "ART", "NN", "ADJD", "KON", "ART"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.10": {"text": "erste", "tokens": ["ers\u00b7te"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Liebe den Menschen erwacht, und goldner Zeiten", "tokens": ["Lie\u00b7be", "den", "Men\u00b7schen", "er\u00b7wacht", ",", "und", "gold\u00b7ner", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "ART", "NN", "VVPP", "$,", "KON", "ADJA", "NN"], "meter": "+--+--+-+-+-", "measure": "dactylic.di.plus"}, "line.12": {"text": "Erinnrung,", "tokens": ["E\u00b7rinn\u00b7rung", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Komm' ich zu dir, und gr\u00fc\u00df' in deiner Stille dich,", "tokens": ["Komm'", "ich", "zu", "dir", ",", "und", "gr\u00fc\u00df'", "in", "dei\u00b7ner", "Stil\u00b7le", "dich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPER", "$,", "KON", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Alter!", "tokens": ["Al\u00b7ter", "!"], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.2": {"line.1": {"text": "Immer, Gewaltiger! lebst du noch und ruhest im", "tokens": ["Im\u00b7mer", ",", "Ge\u00b7wal\u00b7ti\u00b7ger", "!", "lebst", "du", "noch", "und", "ru\u00b7hest", "im"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "$,", "NN", "$.", "VVFIN", "PPER", "ADV", "KON", "VVFIN", "APPRART"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Schatten", "tokens": ["Schat\u00b7ten"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Deiner Berge, wie sonst; mit J\u00fcnglingsarmen", "tokens": ["Dei\u00b7ner", "Ber\u00b7ge", ",", "wie", "sonst", ";", "mit", "J\u00fcng\u00b7lings\u00b7ar\u00b7men"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADV", "$.", "APPR", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "umf\u00e4ngst du", "tokens": ["um\u00b7f\u00e4ngst", "du"], "token_info": ["word", "word"], "pos": ["VVFIN", "PPER"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Noch dein liebliches Land, und deiner T\u00f6chter, o", "tokens": ["Noch", "dein", "lieb\u00b7li\u00b7ches", "Land", ",", "und", "dei\u00b7ner", "T\u00f6ch\u00b7ter", ",", "o"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "$,", "KON", "PPOSAT", "NN", "$,", "FM"], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Vater,", "tokens": ["Va\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Deiner Inseln ist noch, der bl\u00fchenden, keine ver-", "tokens": ["Dei\u00b7ner", "In\u00b7seln", "ist", "noch", ",", "der", "bl\u00fc\u00b7hen\u00b7den", ",", "kei\u00b7ne", "ver"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "$,", "ART", "ADJA", "$,", "PIAT", "TRUNC"], "meter": "+-+-+--+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.8": {"text": "loren.", "tokens": ["lo\u00b7ren", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Kreta steht, und Salamis gr\u00fcnt, umd\u00e4mmert von", "tokens": ["Kre\u00b7ta", "steht", ",", "und", "Sa\u00b7la\u00b7mis", "gr\u00fcnt", ",", "um\u00b7d\u00e4m\u00b7mert", "von"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "VVFIN", "$,", "KON", "NE", "VVFIN", "$,", "VVPP", "APPR"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Lorbeern,", "tokens": ["Lor\u00b7beern", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.11": {"text": "Rings von Stralen umbl\u00fcht, erhebt zur Stunde", "tokens": ["Rings", "von", "Stra\u00b7len", "um\u00b7bl\u00fcht", ",", "er\u00b7hebt", "zur", "Stun\u00b7de"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "VVFIN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.12": {"text": "des Aufgangs", "tokens": ["des", "Auf\u00b7gangs"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.13": {"text": "Delos ihr begeistertes Haupt, und Cenos und", "tokens": ["De\u00b7los", "ihr", "be\u00b7geis\u00b7ter\u00b7tes", "Haupt", ",", "und", "Ce\u00b7nos", "und"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "PPOSAT", "ADJA", "NN", "$,", "KON", "NN", "KON"], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.14": {"text": "Chios", "tokens": ["Chios"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.15": {"text": "Haben der purpurnen Fr\u00fcchte genug, von trun-", "tokens": ["Ha\u00b7ben", "der", "pur\u00b7pur\u00b7nen", "Fr\u00fcch\u00b7te", "ge\u00b7nug", ",", "von", "trun"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "ADJA", "NN", "ADV", "$,", "APPR", "TRUNC"], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}, "line.16": {"text": "kenen H\u00fcgeln", "tokens": ["ke\u00b7nen", "H\u00fc\u00b7geln"], "token_info": ["word", "word"], "pos": ["VVFIN", "NN"], "meter": "+-+-", "measure": "trochaic.di"}, "line.17": {"text": "Quillt der Cypriertrank, und von Kalauria fallen", "tokens": ["Quillt", "der", "Cyp\u00b7rier\u00b7trank", ",", "und", "von", "Ka\u00b7lau\u00b7ria", "fal\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "$,", "KON", "APPR", "NE", "VVINF"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.18": {"text": "Silberne B\u00e4che, wie einst, in die alten Wasser", "tokens": ["Sil\u00b7ber\u00b7ne", "B\u00e4\u00b7che", ",", "wie", "einst", ",", "in", "die", "al\u00b7ten", "Was\u00b7ser"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PWAV", "ADV", "$,", "APPR", "ART", "ADJA", "NN"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.19": {"text": "des Vaters.", "tokens": ["des", "Va\u00b7ters", "."], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.20": {"text": "Alle leben sie noch, die Heroenm\u00fctter, die Inseln,", "tokens": ["Al\u00b7le", "le\u00b7ben", "sie", "noch", ",", "die", "He\u00b7ro\u00b7en\u00b7m\u00fct\u00b7ter", ",", "die", "In\u00b7seln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "PPER", "ADV", "$,", "ART", "NN", "$,", "ART", "NN", "$,"], "meter": "--+--+--+-+--+-", "measure": "anapaest.tri.plus"}, "line.21": {"text": "Bl\u00fchend von Jahr zu Jahr, und wenn zu Zeiten,", "tokens": ["Bl\u00fc\u00b7hend", "von", "Jahr", "zu", "Jahr", ",", "und", "wenn", "zu", "Zei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "NN", "APPR", "NN", "$,", "KON", "KOUS", "APPR", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.22": {"text": "vom Abgrund", "tokens": ["vom", "Ab\u00b7grund"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.23": {"text": "Losgelassen, die Flamme der Nacht, das untre", "tokens": ["Los\u00b7ge\u00b7las\u00b7sen", ",", "die", "Flam\u00b7me", "der", "Nacht", ",", "das", "un\u00b7tre"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["NE", "$,", "ART", "NN", "ART", "NN", "$,", "ART", "ADJA"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.24": {"text": "Gewitter,", "tokens": ["Ge\u00b7wit\u00b7ter", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.25": {"text": "Eine der Holden ergriff und die Sterbende dir in", "tokens": ["Ei\u00b7ne", "der", "Hol\u00b7den", "er\u00b7griff", "und", "die", "Ster\u00b7ben\u00b7de", "dir", "in"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "VVFIN", "KON", "ART", "NN", "PPER", "APPR"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}, "line.26": {"text": "den Schoo\u00df sank,", "tokens": ["den", "Schoo\u00df", "sank", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.27": {"text": "G\u00f6ttlicher! du, du dauertest aus, denn \u00fcber den", "tokens": ["G\u00f6tt\u00b7li\u00b7cher", "!", "du", ",", "du", "dau\u00b7er\u00b7test", "aus", ",", "denn", "\u00fc\u00b7ber", "den"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "PPER", "$,", "PPER", "VVFIN", "PTKVZ", "$,", "KON", "APPR", "ART"], "meter": "+--+-+--+-+--", "measure": "iambic.penta.invert"}, "line.28": {"text": "dunkeln", "tokens": ["dun\u00b7keln"], "token_info": ["word"], "pos": ["ADJA"], "meter": "+-", "measure": "trochaic.single"}, "line.29": {"text": "Tiefen ist Manches schon dir auf und unterge-", "tokens": ["Tie\u00b7fen", "ist", "Man\u00b7ches", "schon", "dir", "auf", "und", "un\u00b7ter\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PIS", "ADV", "PPER", "PTKVZ", "KON", "TRUNC"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.30": {"text": "gangen.", "tokens": ["gan\u00b7gen", "."], "token_info": ["word", "punct"], "pos": ["VVPP", "$."], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "Auch die Himmlischen, sie , die Kr\u00e4fte der H\u00f6he", "tokens": ["Auch", "die", "Himm\u00b7li\u00b7schen", ",", "sie", ",", "die", "Kr\u00e4f\u00b7te", "der", "H\u00f6\u00b7he"], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PPER", "$,", "ART", "NN", "ART", "NN"], "meter": "--+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "die stillen,", "tokens": ["die", "stil\u00b7len", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "ADJA", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Die den heiteren Tag und s\u00fc\u00dfen Schlummer und", "tokens": ["Die", "den", "hei\u00b7te\u00b7ren", "Tag", "und", "s\u00fc\u00b7\u00dfen", "Schlum\u00b7mer", "und"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ART", "ADJA", "NN", "KON", "ADJA", "NN", "KON"], "meter": "--+--+-+-+-+", "measure": "anapaest.di.plus"}, "line.4": {"text": "Ahnung", "tokens": ["Ah\u00b7nung"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.5": {"text": "Fernher bringen \u00fcber das Haupt der f\u00fchlenden", "tokens": ["Fern\u00b7her", "brin\u00b7gen", "\u00fc\u00b7ber", "das", "Haupt", "der", "f\u00fch\u00b7len\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVINF", "APPR", "ART", "NN", "ART", "ADJA"], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Menschen", "tokens": ["Men\u00b7schen"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Aus der F\u00fclle der Macht, auch sie, die alten Ge-", "tokens": ["Aus", "der", "F\u00fcl\u00b7le", "der", "Macht", ",", "auch", "sie", ",", "die", "al\u00b7ten", "Ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,", "ADV", "PPER", "$,", "ART", "ADJA", "TRUNC"], "meter": "--+--+-+-+--", "measure": "anapaest.di.plus"}, "line.8": {"text": "spielen,", "tokens": ["spie\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wohnen, wie einst, mit dir, und oft am d\u00e4m-", "tokens": ["Woh\u00b7nen", ",", "wie", "einst", ",", "mit", "dir", ",", "und", "oft", "am", "d\u00e4m"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "PWAV", "ADV", "$,", "APPR", "PPER", "$,", "KON", "ADV", "APPRART", "TRUNC"], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.10": {"text": "mernden Abend,", "tokens": ["mern\u00b7den", "A\u00b7bend", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.11": {"text": "Wenn von Asiens Bergen herein das heilige", "tokens": ["Wenn", "von", "A\u00b7siens", "Ber\u00b7gen", "her\u00b7ein", "das", "hei\u00b7li\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NE", "NN", "PTKVZ", "ART", "ADJA"], "meter": "--+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Mondlicht", "tokens": ["Mond\u00b7licht"], "token_info": ["word"], "pos": ["NN"], "meter": "+-", "measure": "trochaic.single"}, "line.13": {"text": "K\u00f6mmt und die Sterne sich in deiner Woge be-", "tokens": ["K\u00f6mmt", "und", "die", "Ster\u00b7ne", "sich", "in", "dei\u00b7ner", "Wo\u00b7ge", "be"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "KON", "ART", "NN", "PRF", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "++-+-+-+-+--", "measure": "unknown.measure.hexa"}, "line.14": {"text": "gegnen,", "tokens": ["geg\u00b7nen", ","], "token_info": ["word", "punct"], "p