{"textgrid.poem.46944": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "14.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der du noch j\u00fcngst durch deines Ruhms Posaunen", "tokens": ["Der", "du", "noch", "j\u00fcngst", "durch", "dei\u00b7nes", "Ruhms", "Po\u00b7sau\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADV", "ADV", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ausrufen lie\u00dfest vor Europas Ohre:", "tokens": ["Aus\u00b7ru\u00b7fen", "lie\u00b7\u00dfest", "vor", "Eu\u00b7ro\u00b7pas", "Oh\u00b7re", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "APPR", "NE", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "\u00bbgeh\u00f6rt nun haben Asias Felsenthore", "tokens": ["\u00bb", "ge\u00b7h\u00f6rt", "nun", "ha\u00b7ben", "A\u00b7sias", "Fel\u00b7sen\u00b7tho\u00b7re"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "VVFIN", "ADV", "VAFIN", "NE", "NE"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Meines Gesch\u00fctzes Donner auch mit Staunen!\u00ab", "tokens": ["Mei\u00b7nes", "Ge\u00b7sch\u00fct\u00b7zes", "Don\u00b7ner", "auch", "mit", "Stau\u00b7nen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "NN", "ADV", "APPR", "NN", "$.", "$("], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.5": {"text": "Nun da du dein Gesch\u00fctz mit abgehau'nen", "tokens": ["Nun", "da", "du", "dein", "Ge\u00b7sch\u00fctz", "mit", "ab\u00b7ge\u00b7hau'\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "KOUS", "PPER", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gestr\u00e4ngen l\u00e4ssest stehn in Eis und Moore,", "tokens": ["Ge\u00b7str\u00e4n\u00b7gen", "l\u00e4s\u00b7sest", "stehn", "in", "Eis", "und", "Moo\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "VVFIN", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Dein Donnerwerkzeug bricht gleich schwachem Rohre;", "tokens": ["Dein", "Don\u00b7ner\u00b7werk\u00b7zeug", "bricht", "gleich", "schwa\u00b7chem", "Roh\u00b7re", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Statt Donners blitze nun mit Augenbraunen.", "tokens": ["Statt", "Don\u00b7ners", "blit\u00b7ze", "nun", "mit", "Au\u00b7gen\u00b7brau\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "ADV", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Du hast gedacht die Erde zu ersch\u00fcttern,", "tokens": ["Du", "hast", "ge\u00b7dacht", "die", "Er\u00b7de", "zu", "er\u00b7sch\u00fct\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "VVPP", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Wie Zeus den Himmel, wenn er regt die Locken;", "tokens": ["Wie", "Zeus", "den", "Him\u00b7mel", ",", "wenn", "er", "regt", "die", "Lo\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "ART", "NN", "$,", "KOUS", "PPER", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "m