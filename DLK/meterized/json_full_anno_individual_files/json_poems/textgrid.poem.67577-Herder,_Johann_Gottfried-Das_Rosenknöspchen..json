{"textgrid.poem.67577": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Das Rosenkn\u00f6spchen.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Hier ist ein kleines s\u00fc\u00dfes M\u00e4dchen,", "tokens": ["Hier", "ist", "ein", "klei\u00b7nes", "s\u00fc\u00b7\u00dfes", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Kaum Sechzehn alt vor sieben Tagen,", "tokens": ["Kaum", "Sech\u00b7zehn", "alt", "vor", "sie\u00b7ben", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "CARD", "ADJD", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Der ganzen Gegend eine Blume,", "tokens": ["Der", "gan\u00b7zen", "Ge\u00b7gend", "ei\u00b7ne", "Blu\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In zwanzig Jahren lang kaum ", "tokens": ["In", "zwan\u00b7zig", "Jah\u00b7ren", "lang", "kaum"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "CARD", "NN", "ADJD", "ADV"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.5": {"text": "So sch\u00f6n, so s\u00fc\u00df erspro\u00dfte Blume.", "tokens": ["So", "sch\u00f6n", ",", "so", "s\u00fc\u00df", "ers\u00b7pro\u00df\u00b7te", "Blu\u00b7me", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "$,", "ADV", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich nenne sie nur Rosenkn\u00f6spchen.", "tokens": ["Ich", "nen\u00b7ne", "sie", "nur", "Ro\u00b7sen\u00b7kn\u00f6spc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur, Bube, brich das Kn\u00f6spchen nicht!", "tokens": ["Nur", ",", "Bu\u00b7be", ",", "brich", "das", "Kn\u00f6spc\u00b7hen", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "ADJD", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ein Waisenkind! Ist ohne Mutter,", "tokens": ["Ein", "Wai\u00b7sen\u00b7kind", "!", "Ist", "oh\u00b7ne", "Mut\u00b7ter", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$.", "VAFIN", "APPR", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist ihres guten Vaters Freude,", "tokens": ["Ist", "ih\u00b7res", "gu\u00b7ten", "Va\u00b7ters", "Freu\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ihm Trost allein und Bild der Mutter;", "tokens": ["Ihm", "Trost", "al\u00b7lein", "und", "Bild", "der", "Mut\u00b7ter", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ADV", "KON", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und ihre Ahn' hat's von mir weinend", "tokens": ["Und", "ih\u00b7re", "Ahn'", "hat's", "von", "mir", "wei\u00b7nend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VAFIN", "APPR", "PPER", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Erfleht, hat mich am schw\u00e4chsten Herzen", "tokens": ["Er\u00b7fleht", ",", "hat", "mich", "am", "schw\u00e4chs\u00b7ten", "Her\u00b7zen"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "$,", "VAFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Bewegt, doch nicht sie zu verf\u00fchren.", "tokens": ["Be\u00b7wegt", ",", "doch", "nicht", "sie", "zu", "ver\u00b7f\u00fch\u00b7ren", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "ADV", "PTKNEG", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Ha, Bube, thu auch Du es nicht!", "tokens": ["Ha", ",", "Bu\u00b7be", ",", "thu", "auch", "Du", "es", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "VVFIN", "ADV", "PPER", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "H\u00f6r mein Gebot! Mit H\u00f6llenreue", "tokens": ["H\u00f6r", "mein", "Ge\u00b7bot", "!", "Mit", "H\u00f6l\u00b7len\u00b7reu\u00b7e"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NE", "PPOSAT", "NN", "$.", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Hab' ich's entweihet je! Ein M\u00e4dchen,", "tokens": ["Hab'", "ich's", "ent\u00b7wei\u00b7het", "je", "!", "Ein", "M\u00e4d\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PIS", "VVFIN", "ADV", "$.", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Das nichts als Unschuld hat und Tugend", "tokens": ["Das", "nichts", "als", "Un\u00b7schuld", "hat", "und", "Tu\u00b7gend"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "PIS", "KOKOM", "NN", "VAFIN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und dann in Hohn und Elend sinket", "tokens": ["Und", "dann", "in", "Hohn", "und", "E\u00b7lend", "sin\u00b7ket"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und dann f\u00fcr Hohn und Gram und Elend", "tokens": ["Und", "dann", "f\u00fcr", "Hohn", "und", "Gram", "und", "E\u00b7lend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "NN", "KON", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Zum Giftkelch fliehn mu\u00df! Ha, wer anders", "tokens": ["Zum", "Gift\u00b7kelch", "fliehn", "mu\u00df", "!", "Ha", ",", "wer", "an\u00b7ders"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$.", "ITJ", "$,", "PWS", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Als solch ein Elend kann ihn w\u00e4hlen? \u2013", "tokens": ["Als", "solch", "ein", "E\u00b7lend", "kann", "ihn", "w\u00e4h\u00b7len", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PIAT", "ART", "NN", "VMFIN", "PPER", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Ruchloser Bube, thu es nicht!", "tokens": ["Ruch\u00b7lo\u00b7ser", "Bu\u00b7be", ",", "thu", "es", "nicht", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie wird Dir dienen, wird Dir t\u00e4ndeln,", "tokens": ["Sie", "wird", "Dir", "die\u00b7nen", ",", "wird", "Dir", "t\u00e4n\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "VVINF", "$,", "VAFIN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wird alle s\u00fc\u00dfe M\u00e4dchenseele", "tokens": ["Wird", "al\u00b7le", "s\u00fc\u00b7\u00dfe", "M\u00e4d\u00b7chen\u00b7see\u00b7le"], "token_info": ["word", "word", "word", "word"], "pos": ["VAFIN", "PIAT", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Dir schuldlos, kunstlos Dir enth\u00fcllen,", "tokens": ["Dir", "schuld\u00b7los", ",", "kunst\u00b7los", "Dir", "ent\u00b7h\u00fcl\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "$,", "ADJD", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wird Deiner List nicht trotzen, Arglist", "tokens": ["Wird", "Dei\u00b7ner", "List", "nicht", "trot\u00b7zen", ",", "Arg\u00b7list"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["VAFIN", "PPOSAT", "NN", "PTKNEG", "VVINF", "$,", "NN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Argwohnen nicht, wird nie Dich locken,", "tokens": ["Arg\u00b7woh\u00b7nen", "nicht", ",", "wird", "nie", "Dich", "lo\u00b7cken", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "$,", "VAFIN", "ADV", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Wird wie ein L\u00e4mmlein um Dich spielen.", "tokens": ["Wird", "wie", "ein", "L\u00e4mm\u00b7lein", "um", "Dich", "spie\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "KOKOM", "ART", "NN", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Nur, H\u00f6llenm\u00f6rder, w\u00fcrg es nicht!", "tokens": ["Nur", ",", "H\u00f6l\u00b7len\u00b7m\u00f6r\u00b7der", ",", "w\u00fcrg", "es", "nicht", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "NN", "$,", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Ihr kleiner Busen schl\u00e4gt schon Liebe;", "tokens": ["Ihr", "klei\u00b7ner", "Bu\u00b7sen", "schl\u00e4gt", "schon", "Lie\u00b7be", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Nur wei\u00df es noch nicht, was er schl\u00e4get,", "tokens": ["Nur", "wei\u00df", "es", "noch", "nicht", ",", "was", "er", "schl\u00e4\u00b7get", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "PTKNEG", "$,", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ist gut nur noch dem s\u00fc\u00dfen H\u00e4nschen,", "tokens": ["Ist", "gut", "nur", "noch", "dem", "s\u00fc\u00b7\u00dfen", "H\u00e4n\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADJD", "ADV", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und oft (oft hab' ich sie belauschet!)", "tokens": ["Und", "oft", "(", "oft", "hab'", "ich", "sie", "be\u00b7lau\u00b7schet", "!", ")"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "ADV", "$(", "ADV", "VAFIN", "PPER", "PPER", "VVFIN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Begegnen schon, wie Unschuldpilger,", "tokens": ["Be\u00b7geg\u00b7nen", "schon", ",", "wie", "Un\u00b7schuld\u00b7pil\u00b7ger", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "$,", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "So gern, so sanft sich ihre Blicke;", "tokens": ["So", "gern", ",", "so", "sanft", "sich", "ih\u00b7re", "Bli\u00b7cke", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "ADV", "ADJD", "PRF", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Dann h\u00fcpft sie, froh des stummen Blickes,", "tokens": ["Dann", "h\u00fcpft", "sie", ",", "froh", "des", "stum\u00b7men", "Bli\u00b7ckes", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Das Rehchen, fr\u00f6hlicher nach Hause.", "tokens": ["Das", "Reh\u00b7chen", ",", "fr\u00f6h\u00b7li\u00b7cher", "nach", "Hau\u00b7se", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "\u00bbliebst Du auch schon, mein kleines Herzchen?\u00ab", "tokens": ["\u00bb", "liebst", "Du", "auch", "schon", ",", "mein", "klei\u00b7nes", "Herz\u00b7chen", "?", "\u00ab"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "PPER", "ADV", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "\u00bbei, was ist Liebe? Meinem H\u00e4nschen", "tokens": ["\u00bb", "ei", ",", "was", "ist", "Lie\u00b7be", "?", "Mei\u00b7nem", "H\u00e4n\u00b7schen"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ITJ", "$,", "PWS", "VAFIN", "NN", "$.", "PPOSAT", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Bin ich zwar gut, auch seine Mutter", "tokens": ["Bin", "ich", "zwar", "gut", ",", "auch", "sei\u00b7ne", "Mut\u00b7ter"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "$,", "ADV", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sagt oft: ich w\u00e4re wol das M\u00e4dchen", "tokens": ["Sagt", "oft", ":", "ich", "w\u00e4\u00b7re", "wol", "das", "M\u00e4d\u00b7chen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "$.", "PPER", "VAFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "F\u00fcr H\u00e4nschen. Doch was hilft das Sagen?", "tokens": ["F\u00fcr", "H\u00e4n\u00b7schen", ".", "Doch", "was", "hilft", "das", "Sa\u00b7gen", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$.", "KON", "PWS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Ich bin arm, er ist arm! Und R\u00f6schen", "tokens": ["Ich", "bin", "arm", ",", "er", "ist", "arm", "!", "Und", "R\u00f6\u00b7schen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "ADJD", "$,", "PPER", "VAFIN", "ADJD", "$.", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Soll H\u00e4nschen nicht ungl\u00fccklich machen!\u00ab", "tokens": ["Soll", "H\u00e4n\u00b7schen", "nicht", "un\u00b7gl\u00fcck\u00b7lich", "ma\u00b7chen", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["VMFIN", "NN", "PTKNEG", "ADJD", "VVINF", "$.", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Ha, Bube, w\u00e4re meine Seele", "tokens": ["Ha", ",", "Bu\u00b7be", ",", "w\u00e4\u00b7re", "mei\u00b7ne", "See\u00b7le"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "NN", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "So gut noch wie mein Rosenkn\u00f6spchen,", "tokens": ["So", "gut", "noch", "wie", "mein", "Ro\u00b7sen\u00b7kn\u00f6spc\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "ADV", "KOKOM", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Aufrichtig, schuldlos, zart und edel!", "tokens": ["Auf\u00b7rich\u00b7tig", ",", "schuld\u00b7los", ",", "zart", "und", "e\u00b7del", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ha, meine schwarze Teufelsseele", "tokens": ["Ha", ",", "mei\u00b7ne", "schwar\u00b7ze", "Teu\u00b7fels\u00b7see\u00b7le"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["ITJ", "$,", "PPOSAT", "ADJA", "NN"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.5": {"text": "Ist, Bube, schwarz wie Du!", "tokens": ["Ist", ",", "Bu\u00b7be", ",", "schwarz", "wie", "Du", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "NN", "$,", "ADJD", "KOKOM", "PPER", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Der Augenblick Genu\u00df! Das eitle,", "tokens": ["Der", "Au\u00b7gen\u00b7blick", "Ge\u00b7nu\u00df", "!", "Das", "eit\u00b7le", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$.", "ART", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text"