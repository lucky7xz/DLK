{"dta.poem.11511": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Das 4. capitel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn ein getreuer freund schon mit dir reden will,", "tokens": ["Wenn", "ein", "ge\u00b7treu\u00b7er", "freund", "schon", "mit", "dir", "re\u00b7den", "will", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "ADV", "APPR", "PPER", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So kan es doch dein schmertz vielleichte nicht vertragen;", "tokens": ["So", "kan", "es", "doch", "dein", "schmertz", "viel\u00b7leich\u00b7te", "nicht", "ver\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPOSAT", "ADJD", "VVFIN", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Allein der ungeduld geh\u00f6ret maas und ziel:", "tokens": ["Al\u00b7lein", "der", "un\u00b7ge\u00b7duld", "ge\u00b7h\u00f6\u00b7ret", "maas", "und", "ziel", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "VVFIN", "NE", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und darum mu\u00df ich dir die trockne wahrheit sagen.", "tokens": ["Und", "da\u00b7rum", "mu\u00df", "ich", "dir", "die", "trock\u00b7ne", "wahr\u00b7heit", "sa\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PAV", "VMFIN", "PPER", "PPER", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "War auch ein weis\u2019rer mann, als Hiob, auf der welt?", "tokens": ["War", "auch", "ein", "weis'\u00b7rer", "mann", ",", "als", "Hiob", ",", "auf", "der", "welt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "$,", "KOUS", "NE", "$,", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.6": {"text": "So lange dir der HErr des hertzens wunsch erf\u00fcllet,", "tokens": ["So", "lan\u00b7ge", "dir", "der", "Herr", "des", "hert\u00b7zens", "wunsch", "er\u00b7f\u00fcl\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "ART", "NN", "ART", "ADV", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da fand des n\u00e4chsten brust, was krafft und muth erh\u00e4lt:", "tokens": ["Da", "fand", "des", "n\u00e4chs\u00b7ten", "brust", ",", "was", "krafft", "und", "muth", "er\u00b7h\u00e4lt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PWS", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und dein behertzter mund hat manche pein gestillet.", "tokens": ["Und", "dein", "be\u00b7hertz\u00b7ter", "mund", "hat", "man\u00b7che", "pein", "ge\u00b7stil\u00b7let", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "NN", "VAFIN", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Nun aber, da dich GOtt auf scharffe dornen legt,", "tokens": ["Nun", "a\u00b7ber", ",", "da", "dich", "Gott", "auf", "scharf\u00b7fe", "dor\u00b7nen", "legt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "KOUS", "PPER", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So liegt die tapfferkeit auf einmahl in dem staube:", "tokens": ["So", "liegt", "die", "tapf\u00b7fer\u00b7keit", "auf", "ein\u00b7mahl", "in", "dem", "stau\u00b7be", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ADV", "APPR", "ART", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Da wird dein weiches hertz, als wie ein rohr, bewegt,", "tokens": ["Da", "wird", "dein", "wei\u00b7ches", "hertz", ",", "als", "wie", "ein", "rohr", ",", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "VAFIN", "PPOSAT", "ADJA", "NN", "$,", "KOUS", "KOKOM", "ART", "ADJD", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und Hiobs seele girrt, als eine turtel-taube.", "tokens": ["Und", "Hiobs", "see\u00b7le", "girrt", ",", "als", "ei\u00b7ne", "tur\u00b7tel\u00b7tau\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVFIN", "$,", "KOUS", "ART", "NN", "$."], "meter": "--+-+-+-+-+-", "measure": "anapaest.init"}, "line.13": {"text": "Ist deine gottesfurcht nun nicht ein bloser traum?", "tokens": ["Ist", "dei\u00b7ne", "got\u00b7tes\u00b7furcht", "nun", "nicht", "ein", "blo\u00b7ser", "traum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Und deiner hoffnung schlo\u00df ein haus von karten-bl\u00e4ttern?", "tokens": ["Und", "dei\u00b7ner", "hoff\u00b7nung", "schlo\u00df", "ein", "haus", "von", "kar\u00b7ten\u00b7bl\u00e4t\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Bebt der erschrockne geist nicht als ein pappel-baum?", "tokens": ["Bebt", "der", "er\u00b7schrock\u00b7ne", "geist", "nicht", "als", "ein", "pap\u00b7pel\u00b7baum", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "PTKNEG", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Und l\u00e4st sich nicht dein muth, wie spr\u00f6der dohn zerschmettern?", "tokens": ["Und", "l\u00e4st", "sich", "nicht", "dein", "muth", ",", "wie", "spr\u00f6\u00b7der", "dohn", "zer\u00b7schmet\u00b7tern", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "PTKNEG", "PPOSAT", "NN", "$,", "PWAV", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Wenn hat des HErren zorn der unschuld zugesetzt?", "tokens": ["Wenn", "hat", "des", "Her\u00b7ren", "zorn", "der", "un\u00b7schuld", "zu\u00b7ge\u00b7setzt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Und wahre fr\u00f6mmigkeit mit ruthen heimgesuchet?", "tokens": ["Und", "wah\u00b7re", "fr\u00f6m\u00b7mig\u00b7keit", "mit", "ru\u00b7then", "heim\u00b7ge\u00b7su\u00b7chet", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Meinst du: Da\u00df GOtt sein schwerd auf die gerechten wetzt,", "tokens": ["Meinst", "du", ":", "Da\u00df", "Gott", "sein", "schwerd", "auf", "die", "ge\u00b7rech\u00b7ten", "wetzt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$.", "KOUS", "NN", "VAINF", "VAFIN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Und einen heiligen so, wie itzt dich, verfluchet?", "tokens": ["Und", "ei\u00b7nen", "hei\u00b7li\u00b7gen", "so", ",", "wie", "itzt", "dich", ",", "ver\u00b7flu\u00b7chet", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ART", "ADJA", "ADV", "$,", "PWAV", "ADV", "PPER", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Das hab ich wohl gesehn, da\u00df heuchler, die nur m\u00fch", "tokens": ["Das", "hab", "ich", "wohl", "ge\u00b7sehn", ",", "da\u00df", "heuch\u00b7ler", ",", "die", "nur", "m\u00fch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$,", "KOUS", "NE", "$,", "PRELS", "ADV", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Und ungl\u00fcck ausges\u00e4\u2019t, auch ungl\u00fcck einbekommen:", "tokens": ["Und", "un\u00b7gl\u00fcck", "aus\u00b7ge\u00b7s\u00e4't", ",", "auch", "un\u00b7gl\u00fcck", "ein\u00b7be\u00b7kom\u00b7men", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "$,", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Da\u00df die vergn\u00fcgte ruh verruchte s\u00fcnder flieh:", "tokens": ["Da\u00df", "die", "ver\u00b7gn\u00fcg\u00b7te", "ruh", "ver\u00b7ruch\u00b7te", "s\u00fcn\u00b7der", "flieh", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "VVFIN", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Und da\u00df des himmels grimm schon manchen hingenommen.", "tokens": ["Und", "da\u00df", "des", "him\u00b7mels", "grimm", "schon", "man\u00b7chen", "hin\u00b7ge\u00b7nom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADJD", "ADV", "PIS", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Denn wenn die l\u00f6wen gleich aus vollem rachen br\u00fclln,", "tokens": ["Denn", "wenn", "die", "l\u00f6\u00b7wen", "gleich", "aus", "vol\u00b7lem", "ra\u00b7chen", "br\u00fclln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Und mit der scharffen klau in zarten hertzen w\u00fchlen:", "tokens": ["Und", "mit", "der", "scharf\u00b7fen", "klau", "in", "zar\u00b7ten", "hert\u00b7zen", "w\u00fch\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "VVFIN", "ADJD", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "So m\u00fcssen sie zuletzt doch eine grube f\u00fclln,", "tokens": ["So", "m\u00fcs\u00b7sen", "sie", "zu\u00b7letzt", "doch", "ei\u00b7ne", "gru\u00b7be", "f\u00fclln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "ADV", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und die erhitzte wuth in eignem blute k\u00fchlen.", "tokens": ["Und", "die", "er\u00b7hitz\u00b7te", "wuth", "in", "eig\u00b7nem", "blu\u00b7te", "k\u00fch\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Doch h\u00f6re, was dir itzt ein treuer freund entdeckt,", "tokens": ["Doch", "h\u00f6\u00b7re", ",", "was", "dir", "itzt", "ein", "treu\u00b7er", "freund", "ent\u00b7deckt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PWS", "PPER", "ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Und was der himmel mir in kurtzem offenbaret:", "tokens": ["Und", "was", "der", "him\u00b7mel", "mir", "in", "kurt\u00b7zem", "of\u00b7fen\u00b7ba\u00b7ret", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "PPER", "APPR", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Ich hab ein wort geh\u00f6rt, in dem viel nachdruck steckt,", "tokens": ["Ich", "hab", "ein", "wort", "ge\u00b7h\u00f6rt", ",", "in", "dem", "viel", "nach\u00b7druck", "steckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVFIN", "$,", "APPR", "ART", "PIAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Und solches bi\u00df daher als einen schatz verwahret.", "tokens": ["Und", "sol\u00b7ches", "bi\u00df", "da\u00b7her", "als", "ei\u00b7nen", "schatz", "ver\u00b7wah\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "ADV", "PAV", "KOUS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Gleich, als der matte schlaf die sanfften glieder band,", "tokens": ["Gleich", ",", "als", "der", "mat\u00b7te", "schlaf", "die", "sanff\u00b7ten", "glie\u00b7der", "band", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "ART", "ADJA", "NN", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Und ich in tieffer nacht mit schweren tr\u00e4umen spielte:", "tokens": ["Und", "ich", "in", "tief\u00b7fer", "nacht", "mit", "schwe\u00b7ren", "tr\u00e4u\u00b7men", "spiel\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "APPR", "ADJA", "NN", "APPR", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "So satzte mich die furcht in einen bangen stand,", "tokens": ["So", "satz\u00b7te", "mich", "die", "furcht", "in", "ei\u00b7nen", "ban\u00b7gen", "stand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "In dem ich lanter angst und k\u00fchles schrecken f\u00fchlte:", "tokens": ["In", "dem", "ich", "lan\u00b7ter", "angst", "und", "k\u00fch\u00b7les", "schre\u00b7cken", "f\u00fchl\u00b7te", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ADJD", "VVPP", "KON", "ADJA", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Die haare giengen mir, so viel ich wei\u00df, berg-an,", "tokens": ["Die", "haa\u00b7re", "gien\u00b7gen", "mir", ",", "so", "viel", "ich", "wei\u00df", ",", "ber\u00b7gan", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$,", "ADV", "ADV", "PPER", "VVFIN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Indem ein schneller geist bey mir vor\u00fcber reunte.", "tokens": ["In\u00b7dem", "ein", "schnel\u00b7ler", "geist", "bey", "mir", "vor\u00b7\u00fc\u00b7ber", "re\u00b7un\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "PPER", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.39": {"text": "Gleich stund ein fremdes bild, das ich nicht nennen kan,", "tokens": ["Gleich", "stund", "ein", "frem\u00b7des", "bild", ",", "das", "ich", "nicht", "nen\u00b7nen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Nachdem ich in der angst mich selber kaum erkennte.", "tokens": ["Nach\u00b7dem", "ich", "in", "der", "angst", "mich", "sel\u00b7ber", "kaum", "er\u00b7kenn\u00b7te", "."], "token_info": ["word", "word", "word", "word", 