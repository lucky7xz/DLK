{"dta.poem.11055": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Kurze  \n Abend-Andacht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1740", "urn": "urn:nbn:de:kobv:b4-200905198572", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich kann nunmehr noch einen Tag, zu den bereits erlebten,\nf\u00fcgen.", "tokens": ["Ich", "kann", "nun\u00b7mehr", "noch", "ei\u00b7nen", "Tag", ",", "zu", "den", "be\u00b7reits", "er\u00b7leb\u00b7ten", ",", "f\u00fc\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ART", "NN", "$,", "APPR", "ART", "ADV", "VVFIN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-+-", "measure": "iambic.octa.plus"}, "line.2": {"text": "Gottlob! er ist von mir gesund, ja gar mit mancherley Ver-", "tokens": ["Gott\u00b7lob", "!", "er", "ist", "von", "mir", "ge\u00b7sund", ",", "ja", "gar", "mit", "man\u00b7cher\u00b7ley", "Ver"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$.", "PPER", "VAFIN", "APPR", "PPER", "ADJD", "$,", "ADV", "ADV", "APPR", "PIAT", "TRUNC"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Es sind im selbigen so viele Stunden,", "tokens": ["Es", "sind", "im", "sel\u00b7bi\u00b7gen", "so", "vie\u00b7le", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPRART", "ADJA", "ADV", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "So viel Minuten und Secunden,", "tokens": ["So", "viel", "Mi\u00b7nu\u00b7ten", "und", "Se\u00b7cun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nicht nur von mir, auch von den Meinen (dir Herr sey Lob", "tokens": ["Nicht", "nur", "von", "mir", ",", "auch", "von", "den", "Mei\u00b7nen", "(", "dir", "Herr", "sey", "Lob"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKNEG", "ADV", "APPR", "PPER", "$,", "ADV", "APPR", "ART", "NN", "$(", "PPER", "NN", "VAFIN", "NN"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.6": {"text": "Jetzt folgt, nach deiner weisen Ordnung, zu unsrer Ruh, die", "tokens": ["Jetzt", "folgt", ",", "nach", "dei\u00b7ner", "wei\u00b7sen", "Ord\u00b7nung", ",", "zu", "uns\u00b7rer", "Ruh", ",", "die"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "VVFIN", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,", "APPR", "PPOSAT", "NN", "$,", "PRELS"], "meter": "-+-+-+----+-+-", "measure": "unknown.measure.penta"}, "line.7": {"text": "Ach la\u00df mich, auch in selbiger, gesund, bequem und ruhig", "tokens": ["Ach", "la\u00df", "mich", ",", "auch", "in", "sel\u00b7bi\u00b7ger", ",", "ge\u00b7sund", ",", "be\u00b7quem", "und", "ru\u00b7hig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVIMP", "PPER", "$,", "ADV", "APPR", "ADJA", "$,", "ADJD", "$,", "ADJD", "KON", "ADJD"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.8": {"text": "La\u00df mich dich in der Fr\u00fche preisen: Da\u00df deine Weisheit, Lieb", "tokens": ["La\u00df", "mich", "dich", "in", "der", "Fr\u00fc\u00b7he", "prei\u00b7sen", ":", "Da\u00df", "dei\u00b7ne", "Weis\u00b7heit", ",", "Lieb"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["VVIMP", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$.", "KOUS", "PPOSAT", "NN", "$,", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}, "line.9": {"text": "Den hellen Tag, zu reger Arbeit, die stille Nacht zur Ruh", "tokens": ["Den", "hel\u00b7len", "Tag", ",", "zu", "re\u00b7ger", "Ar\u00b7beit", ",", "die", "stil\u00b7le", "Nacht", "zur", "Ruh"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+--+-+-+", "measure": "iambic.septa.relaxed"}}}}}