{"textgrid.poem.35086": {"metadata": {"author": {"name": "May, Karl", "birth": "N.A.", "death": "N.A."}, "title": "Heimkehr", "genre": "verse", "period": "N.A.", "pub_year": 1877, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kehre heim! Auch ich ging wie die Andern", "tokens": ["Ich", "keh\u00b7re", "heim", "!", "Auch", "ich", "ging", "wie", "die", "An\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ADV", "PPER", "VVFIN", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinaus ins Leben, in die weite Welt.", "tokens": ["Hin\u00b7aus", "ins", "Le\u00b7ben", ",", "in", "die", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nirgends bot sich mir bei meinem Wandern", "tokens": ["Doch", "nir\u00b7gends", "bot", "sich", "mir", "bei", "mei\u00b7nem", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die rechte Stelle f\u00fcr mein kleines Zelt.", "tokens": ["Die", "rech\u00b7te", "Stel\u00b7le", "f\u00fcr", "mein", "klei\u00b7nes", "Zelt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Es st\u00f6rte mich das Locken und das Prahlen", "tokens": ["Es", "st\u00f6r\u00b7te", "mich", "das", "Lo\u00b7cken", "und", "das", "Prah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ART", "NN", "KON", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Mit nichtgem Tand, mit eitlem Trug und Schein;", "tokens": ["Mit", "nicht\u00b7gem", "Tand", ",", "mit", "eit\u00b7lem", "Trug", "und", "Schein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich wollte nicht das Blei mit Gold bezahlen", "tokens": ["Ich", "woll\u00b7te", "nicht", "das", "Blei", "mit", "Gold", "be\u00b7zah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "PTKNEG", "ART", "NN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und nicht der Erde meinen Himmel weihn.", "tokens": ["Und", "nicht", "der", "Er\u00b7de", "mei\u00b7nen", "Him\u00b7mel", "weihn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "ART", "NN", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Ich kehre heim! Ich sehe rings ein Trachten", "tokens": ["Ich", "keh\u00b7re", "heim", "!", "Ich", "se\u00b7he", "rings", "ein", "Trach\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Zielen, die nicht meine Ziele sind.", "tokens": ["Nach", "Zie\u00b7len", ",", "die", "nicht", "mei\u00b7ne", "Zie\u00b7le", "sind", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "PTKNEG", "PPOSAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich will zur Heimath; mag man mich verachten,", "tokens": ["Ich", "will", "zur", "Hei\u00b7math", ";", "mag", "man", "mich", "ver\u00b7ach\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "NN", "$.", "VMFIN", "PIS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Da\u00df da ich sein will, wo ich war als Kind.", "tokens": ["Da\u00df", "da", "ich", "sein", "will", ",", "wo", "ich", "war", "als", "Kind", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "VAINF", "VMFIN", "$,", "PWAV", "PPER", "VAFIN", "KOKOM", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich will zur\u00fcck zu jenen selgen Tagen,", "tokens": ["Ich", "will", "zu\u00b7r\u00fcck", "zu", "je\u00b7nen", "sel\u00b7gen", "Ta\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Wo ich an dich und deiner Engel Schaar", "tokens": ["Wo", "ich", "an", "dich", "und", "dei\u00b7ner", "En\u00b7gel", "Schaar"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "APPR", "PPER", "KON", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "So innig glaubte, ohne viel zu fragen,", "tokens": ["So", "in\u00b7nig", "glaub\u00b7te", ",", "oh\u00b7ne", "viel", "zu", "fra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "$,", "KOUI", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und nur dein Kind und gar nichts Andres war.", "tokens": ["Und", "nur", "dein", "Kind", "und", "gar", "nichts", "And\u00b7res", "war", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PPOSAT", "NN", "KON", "ADV", "PIS", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Ich kehre heim! Ich bin des Hastens m\u00fcde", "tokens": ["Ich", "keh\u00b7re", "heim", "!", "Ich", "bin", "des", "Has\u00b7tens", "m\u00fc\u00b7de"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VAFIN", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Nach Flitterkram, nach glei\u00dfnerischem Ruhm.", "tokens": ["Nach", "Flit\u00b7ter\u00b7kram", ",", "nach", "glei\u00df\u00b7ne\u00b7ri\u00b7schem", "Ruhm", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sei du mein Stab; f\u00fchr mich in deiner G\u00fcte", "tokens": ["Sei", "du", "mein", "Stab", ";", "f\u00fchr", "mich", "in", "dei\u00b7ner", "G\u00fc\u00b7te"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "$.", "VVFIN", "PRF", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Zu meiner Kindheit s\u00fc\u00dfem Heiligthum!", "tokens": ["Zu", "mei\u00b7ner", "Kind\u00b7heit", "s\u00fc\u00b7\u00dfem", "Hei\u00b7lig\u00b7thum", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich wei\u00df es ja, dies Trachten und dies Dichten", "tokens": ["Ich", "wei\u00df", "es", "ja", ",", "dies", "Trach\u00b7ten", "und", "dies", "Dich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "$,", "PDS", "NN", "KON", "PDS", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Bringt nicht das wahre Heil, das wahre Gl\u00fcck;", "tokens": ["Bringt", "nicht", "das", "wah\u00b7re", "Heil", ",", "das", "wah\u00b7re", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Ich will so gern, so gern darauf verzichten", "tokens": ["Ich", "will", "so", "gern", ",", "so", "gern", "da\u00b7rauf", "ver\u00b7zich\u00b7ten"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "$,", "ADV", "ADV", "PAV", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und kehr in meine Jugendzeit zur\u00fcck.", "tokens": ["Und", "kehr", "in", "mei\u00b7ne", "Ju\u00b7gend\u00b7zeit", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Ich kehre heim! Ich sehne mich nach Ruhe,", "tokens": ["Ich", "keh\u00b7re", "heim", "!", "Ich", "seh\u00b7ne", "mich", "nach", "Ru\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "PRF", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und diese find ich nur und nur in dir,", "tokens": ["Und", "die\u00b7se", "find", "ich", "nur", "und", "nur", "in", "dir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VVFIN", "PPER", "ADV", "KON", "ADV", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn was ich f\u00fcr das Zeitliche hier thue,", "tokens": ["Denn", "was", "ich", "f\u00fcr", "das", "Zeit\u00b7li\u00b7che", "hier", "thue", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "APPR", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Das r\u00e4cht sich an dem Ewigen in mir.", "tokens": ["Das", "r\u00e4cht", "sich", "an", "dem", "E\u00b7wi\u00b7gen", "in", "mir", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PRF", "APPR", "ART", "ADJA", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Ich kehre heim. Mein himmlischer Berather,", "tokens": ["Ich", "keh\u00b7re", "heim", ".", "Mein", "himm\u00b7li\u00b7scher", "Be\u00b7ra\u00b7ther", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Ich bin so gern dein Kind, so gern noch klein;", "tokens": ["Ich", "bin", "so", "gern", "dein", "Kind", ",", "so", "gern", "noch", "klein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "PPOSAT", "NN", "$,", "ADV", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Du warst schon meiner Jugend Schirm und Vater", "tokens": ["Du", "warst", "schon", "mei\u00b7ner", "Ju\u00b7gend", "Schirm", "und", "Va\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "PPOSAT", "NN", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Und sollst es, wenn ich sterbe, auch noch sein!", "tokens": ["Und", "sollst", "es", ",", "wenn", "ich", "ster\u00b7be", ",", "auch", "noch", "sein", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "$,", "KOUS", "PPER", "VVFIN", "$,", "ADV", "ADV", "VAINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich kehre heim! Auch ich ging wie die Andern", "tokens": ["Ich", "keh\u00b7re", "heim", "!", "Auch", "ich", "ging", "wie", "die", "An\u00b7dern"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "ADV", "PPER", "VVFIN", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Hinaus ins Leben, in die weite Welt.", "tokens": ["Hin\u00b7aus", "ins", "Le\u00b7ben", ",", "in", "die", "wei\u00b7te", "Welt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPRART", "NN", "$,", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Doch nirgends bot sich mir bei meinem Wandern", "tokens": ["Doch", "nir\u00b7gends", "bot", "sich", "mir", "bei", "mei\u00b7nem", "Wan\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PRF", "PPER", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die rechte Stelle f\u00fcr mein kleines Zelt.", "tokens": ["Die", "rech\u00b7te", "Stel\u00b7le", "f\u00fcr", "mein", "klei\u00b7nes