{"textgrid.poem.34576": {"metadata": {"author": {"name": "Hoffmannswaldau, Christian Hoffmann von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Was ist dieses Rund der Erden,", "genre": "verse", "period": "N.A.", "pub_year": 1647, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was ist dieses Rund der Erden,", "tokens": ["Was", "ist", "die\u00b7ses", "Rund", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein Spielplatz voller Schein,", "tokens": ["Als", "ein", "Spiel\u00b7platz", "vol\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo wir heute Helden werden,", "tokens": ["Wo", "wir", "heu\u00b7te", "Hel\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen Schatten nur zu sein,", "tokens": ["Mor\u00b7gen", "Schat\u00b7ten", "nur", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo bei Kronen, Thron' und Siegen", "tokens": ["Wo", "bei", "Kro\u00b7nen", ",", "Thron'", "und", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fessel, Band und Ketten liegen.", "tokens": ["Fes\u00b7sel", ",", "Band", "und", "Ket\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hier will Lachen, Lust und Scherzen", "tokens": ["Hier", "will", "La\u00b7chen", ",", "Lust", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei den hei\u00dfen Thr\u00e4nen stehn,", "tokens": ["Bei", "den", "hei\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die hohen Wunderkerzen", "tokens": ["Und", "die", "ho\u00b7hen", "Wun\u00b7der\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcssen pl\u00f6tzlich untergehn.", "tokens": ["M\u00fcs\u00b7sen", "pl\u00f6tz\u00b7lich", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der die Welt vermeint zu schrecken,", "tokens": ["Der", "die", "Welt", "ver\u00b7meint", "zu", "schre\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "VVPP", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "N\u00e4chstens wird ein Grab ihn decken.", "tokens": ["N\u00e4chs\u00b7tens", "wird", "ein", "Grab", "ihn", "de\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wo die gr\u00f6\u00dften Pfeiler waren,", "tokens": ["Wo", "die", "gr\u00f6\u00df\u00b7ten", "Pfei\u00b7ler", "wa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Da liegt itzt ein wenig Graus;", "tokens": ["Da", "liegt", "itzt", "ein", "we\u00b7nig", "Graus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ART", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Bei den S\u00e4ngern schaut man Bahren,", "tokens": ["Bei", "den", "S\u00e4n\u00b7gern", "schaut", "man", "Bah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "PIS", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bei der Burg ein Todtenhaus,", "tokens": ["Bei", "der", "Burg", "ein", "Tod\u00b7ten\u00b7haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Bei den Rosen Dornenhecken,", "tokens": ["Bei", "den", "Ro\u00b7sen", "Dor\u00b7nen\u00b7he\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Auf dem Purpur schwarze Flecken.", "tokens": ["Auf", "dem", "Pur\u00b7pur", "schwar\u00b7ze", "Fle\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Dieser Platz zeigt viel Gesichte,", "tokens": ["Die\u00b7ser", "Platz", "zeigt", "viel", "Ge\u00b7sich\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die der Falschheit Maske deckt,", "tokens": ["Die", "der", "Falschheit", "Mas\u00b7ke", "deckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und bei falschem Tageslichte", "tokens": ["Und", "bei", "fal\u00b7schem", "Ta\u00b7ges\u00b7lich\u00b7te"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird viel falscher Dunst erweckt;", "tokens": ["Wird", "viel", "fal\u00b7scher", "Dunst", "er\u00b7weckt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PIAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schwur und Untreu, Ku\u00df und Wunden", "tokens": ["Schwur", "und", "Un\u00b7treu", ",", "Ku\u00df", "und", "Wun\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "KON", "ADJD", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind zusammen hier verbunden.", "tokens": ["Sind", "zu\u00b7sam\u00b7men", "hier", "ver\u00b7bun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Nichts will lang allhie verweilen;", "tokens": ["Nichts", "will", "lang", "all\u00b7hie", "ver\u00b7wei\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "ADJD", "ADV", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Jugend, Pracht und Herrlichkeit", "tokens": ["Ju\u00b7gend", ",", "Pracht", "und", "Herr\u00b7lich\u00b7keit"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hei\u00dft des Himmels Satzung eilen", "tokens": ["Hei\u00dft", "des", "Him\u00b7mels", "Sat\u00b7zung", "ei\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und verstieben vor der Zeit.", "tokens": ["Und", "ver\u00b7stie\u00b7ben", "vor", "der", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mancher Blume Haupt erbleichet,", "tokens": ["Man\u00b7cher", "Blu\u00b7me", "Haupt", "er\u00b7blei\u00b7chet", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Eh' es eine Nacht bestreichet.", "tokens": ["Eh'", "es", "ei\u00b7ne", "Nacht", "be\u00b7strei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Unsre Kindheit liebt die Wiege,", "tokens": ["Uns\u00b7re", "Kind\u00b7heit", "liebt", "die", "Wie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Unsre Jugend Gluth und Wein,", "tokens": ["Uns\u00b7re", "Ju\u00b7gend", "Gluth", "und", "Wein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Unsre Mannheit Ehr' und Kriege,", "tokens": ["Uns\u00b7re", "Mann\u00b7heit", "Ehr'", "und", "Krie\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Unser Alter Geld und Stein.", "tokens": ["Un\u00b7ser", "Al\u00b7ter", "Geld", "und", "Stein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Mancher hat in wenig Stunden", "tokens": ["Man\u00b7cher", "hat", "in", "we\u00b7nig", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VAFIN", "APPR", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Spiel, Beruf und Abschied funden.", "tokens": ["Spiel", ",", "Be\u00b7ruf", "und", "Ab\u00b7schied", "fun\u00b7den", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Wohl dem Menschen, der im Spielen", "tokens": ["Wohl", "dem", "Men\u00b7schen", ",", "der", "im", "Spie\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "$,", "PRELS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Nicht zu sehr den Spielplatz liebt", "tokens": ["Nicht", "zu", "sehr", "den", "Spiel\u00b7platz", "liebt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PTKNEG", "PTKA", "ADV", "ART", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und zum Himmel wei\u00df zu zielen,", "tokens": ["Und", "zum", "Him\u00b7mel", "wei\u00df", "zu", "zie\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPRART", "NN", "VVFIN", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weil die Welt ihm Blicke giebt;", "tokens": ["Weil", "die", "Welt", "ihm", "Bli\u00b7cke", "giebt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "PPER", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Der, als Fremder auf der Erden,", "tokens": ["Der", ",", "als", "Frem\u00b7der", "auf", "der", "Er\u00b7den", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$,", "KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Oben B\u00fcrger denkt zu werden!", "tokens": ["O\u00b7ben", "B\u00fcr\u00b7ger", "denkt", "zu", "wer\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PTKZU", "VAINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Wer so stirbt, ist nicht gestorben;", "tokens": ["Wer", "so", "stirbt", ",", "ist", "nicht", "ge\u00b7stor\u00b7ben", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADV", "VVFIN", "$,", "VAFIN", "PTKNEG", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ihn verkl\u00e4rt die Ewigkeit,", "tokens": ["Ihn", "ver\u00b7kl\u00e4rt", "die", "E\u00b7wig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Er hat einen Schatz erworben,", "tokens": ["Er", "hat", "ei\u00b7nen", "Schatz", "er\u00b7wor\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den nicht Zeit und Sturm zerstreut.", "tokens": ["Den", "nicht", "Zeit", "und", "Sturm", "zer\u00b7streut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PTKNEG", "NN", "KON", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Tugend kann den Tod verlachen", "tokens": ["Tu\u00b7gend", "kann", "den", "Tod", "ver\u00b7la\u00b7chen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VMFIN", "ART", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Und die beste Grabschrift machen.", "tokens": ["Und", "die", "bes\u00b7te", "Grab\u00b7schrift", "ma\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Was ist dieses Rund der Erden,", "tokens": ["Was", "ist", "die\u00b7ses", "Rund", "der", "Er\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PDAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als ein Spielplatz voller Schein,", "tokens": ["Als", "ein", "Spiel\u00b7platz", "vol\u00b7ler", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo wir heute Helden werden,", "tokens": ["Wo", "wir", "heu\u00b7te", "Hel\u00b7den", "wer\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Morgen Schatten nur zu sein,", "tokens": ["Mor\u00b7gen", "Schat\u00b7ten", "nur", "zu", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADV", "PTKZU", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo bei Kronen, Thron' und Siegen", "tokens": ["Wo", "bei", "Kro\u00b7nen", ",", "Thron'", "und", "Sie\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "APPR", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Fessel, Band und Ketten liegen.", "tokens": ["Fes\u00b7sel", ",", "Band", "und", "Ket\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Hier will Lachen, Lust und Scherzen", "tokens": ["Hier", "will", "La\u00b7chen", ",", "Lust", "und", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VMFIN", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bei den hei\u00dfen Thr\u00e4nen stehn,", "tokens": ["Bei", "den", "hei\u00b7\u00dfen", "Thr\u00e4\u00b7nen", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die hohen Wunderkerzen", "tokens": ["Und", "die", "ho\u00b7hen", "Wun\u00b7der\u00b7ker\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "M\u00fcssen pl\u00f6tzlich untergehn.", "tokens": ["M\u00fcs\u00b7sen", "pl\u00f6tz\u00b7lich", "un\u00b7ter\u00b7gehn", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJD", "VVINF", "$."], "meter": "+-+-+-+", "measure": 