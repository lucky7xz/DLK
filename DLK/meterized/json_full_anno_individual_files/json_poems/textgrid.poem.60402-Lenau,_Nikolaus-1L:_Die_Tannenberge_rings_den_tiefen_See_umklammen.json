{"textgrid.poem.60402": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "1L: Die Tannenberge rings den tiefen See umklammen", "genre": "verse", "period": "N.A.", "pub_year": 1838, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Tannenberge rings den tiefen See umklammen", "tokens": ["Die", "Tan\u00b7nen\u00b7ber\u00b7ge", "rings", "den", "tie\u00b7fen", "See", "um\u00b7klam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sch\u00fctten in den See die Schatten schwarz zusammen.", "tokens": ["Und", "sch\u00fct\u00b7ten", "in", "den", "See", "die", "Schat\u00b7ten", "schwarz", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Der Himmel ist bedeckt mit dunklen Wetterlasten,", "tokens": ["Der", "Him\u00b7mel", "ist", "be\u00b7deckt", "mit", "dunk\u00b7len", "Wet\u00b7ter\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ruhig starrt das Rohr, und alle L\u00fcfte rasten.", "tokens": ["Doch", "ru\u00b7hig", "starrt", "das", "Rohr", ",", "und", "al\u00b7le", "L\u00fcf\u00b7te", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Sehr ernst ist hier die Welt und stumm in sich versunken,", "tokens": ["Sehr", "ernst", "ist", "hier", "die", "Welt", "und", "stumm", "in", "sich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als w\u00e4r ihr letzter Laut im finstern See ertrunken.", "tokens": ["Als", "w\u00e4r", "ihr", "letz\u00b7ter", "Laut", "im", "fins\u00b7tern", "See", "er\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "ADJA", "APPR", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Als wie ein Scheidegru\u00df erscheint mir diese Stille,", "tokens": ["Als", "wie", "ein", "Schei\u00b7de\u00b7gru\u00df", "er\u00b7scheint", "mir", "die\u00b7se", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein stummes Lebewohl, ein d\u00fcstrer letzter Wille.", "tokens": ["Ein", "stum\u00b7mes", "Le\u00b7be\u00b7wohl", ",", "ein", "d\u00fcst\u00b7rer", "letz\u00b7ter", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Sehr ernst ist hier die Welt und mahnt, das Erdenweh,", "tokens": ["Sehr", "ernst", "ist", "hier", "die", "Welt", "und", "mahnt", ",", "das", "Er\u00b7den\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Herzens letzten Wunsch zu werfen in den See.", "tokens": ["Des", "Her\u00b7zens", "letz\u00b7ten", "Wunsch", "zu", "wer\u00b7fen", "in", "den", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "O Hoffnungen, hinab! zerri\u00dfne Traumgeflechte!", "tokens": ["O", "Hoff\u00b7nun\u00b7gen", ",", "hin\u00b7ab", "!", "zer\u00b7ri\u00df\u00b7ne", "Traum\u00b7ge\u00b7flech\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKVZ", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Liebe, s\u00fc\u00dfer Schmerz der schlummerlosen N\u00e4chte!", "tokens": ["O", "Lie\u00b7be", ",", "s\u00fc\u00b7\u00dfer", "Schmerz", "der", "schlum\u00b7mer\u00b7lo\u00b7sen", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Ihr habt mein Herz get\u00e4uscht; nicht heilen wird die Wunde,", "tokens": ["Ihr", "habt", "mein", "Herz", "ge\u00b7t\u00e4uscht", ";", "nicht", "hei\u00b7len", "wird", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$.", "PTKNEG", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hab ich noch die Kraft, zu sto\u00dfen euch zum Grunde. \u2013", "tokens": ["Doch", "hab", "ich", "noch", "die", "Kraft", ",", "zu", "sto\u00b7\u00dfen", "euch", "zum", "Grun\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PTKZU", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Der Wind wacht auf, ich seh ihn durchs Gew\u00e4sser streichen;", "tokens": ["Der", "Wind", "wacht", "auf", ",", "ich", "seh", "ihn", "durchs", "Ge\u00b7w\u00e4s\u00b7ser", "strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will denn sein Hauch das Herz mir noch einmal erweichen?", "tokens": ["Will", "denn", "sein", "Hauch", "das", "Herz", "mir", "noch", "ein\u00b7mal", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "Das Schilf am Ufer bebt und fl\u00fcstert mir so bange,", "tokens": ["Das", "Schilf", "am", "U\u00b7fer", "bebt", "und", "fl\u00fcs\u00b7tert", "mir", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Winde bebt der Wald am steilen Uferhange.", "tokens": ["Im", "Win\u00b7de", "bebt", "der", "Wald", "am", "stei\u00b7len", "U\u00b7fer\u00b7han\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Ich h\u00f6re kommen dich, Natur! dein Mantel rauscht,", "tokens": ["Ich", "h\u00f6\u00b7re", "kom\u00b7men", "dich", ",", "Na\u00b7tur", "!", "dein", "Man\u00b7tel", "rauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "$,", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie der Geliebten Kleid, wenn ich nach ihr gelauscht;", "tokens": ["Wie", "der", "Ge\u00b7lieb\u00b7ten", "Kleid", ",", "wenn", "ich", "nach", "ihr", "ge\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}}, "stanza.11": {"line.1": {"text": "Willst du denn noch einmal an meinen Hals dich h\u00e4ngen?", "tokens": ["Willst", "du", "denn", "noch", "ein\u00b7mal", "an", "mei\u00b7nen", "Hals", "dich", "h\u00e4n\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ins Elend locken mich mit schmeichelnden Ges\u00e4ngen?", "tokens": ["Ins", "E\u00b7lend", "lo\u00b7cken", "mich", "mit", "schmei\u00b7cheln\u00b7den", "Ge\u00b7s\u00e4n\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PRF", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.12": {"line.1": {"text": "Es schwillt der Wind zum Sturm, es zucken Blitze wild,", "tokens": ["Es", "schwillt", "der", "Wind", "zum", "Sturm", ",", "es", "zu\u00b7cken", "Blit\u00b7ze", "wild", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPRART", "NN", "$,", "PPER", "VVFIN", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den schwarzen See durchgl\u00fcht ihr schnell verzitternd Bild;", "tokens": ["Den", "schwar\u00b7zen", "See", "durch\u00b7gl\u00fcht", "ihr", "schnell", "ver\u00b7zit\u00b7ternd", "Bild", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "ADJD", "VVPP", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.13": {"line.1": {"text": "Sie leuchten durch den See, wie aus begl\u00fcckten Tagen", "tokens": ["Sie", "leuch\u00b7ten", "durch", "den", "See", ",", "wie", "aus", "be\u00b7gl\u00fcck\u00b7ten", "Ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PWAV", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch mein verfinstert Herz Erinnerungen jagen.", "tokens": ["Durch", "mein", "ver\u00b7fins\u00b7tert", "Herz", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "VVPP", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.14": {"line.1": {"text": "Sie rufen mir: O Tor! was hat dein Wahn beschlossen!", "tokens": ["Sie", "ru\u00b7fen", "mir", ":", "O", "Tor", "!", "was", "hat", "dein", "Wahn", "be\u00b7schlos\u00b7sen", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "$.", "NE", "NE", "$.", "PWS", "VAFIN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hoffnung kannst und sollst du in das Grab hier sto\u00dfen;", "tokens": ["Die", "Hoff\u00b7nung", "kannst", "und", "sollst", "du", "in", "das", "Grab", "hier", "sto\u00b7\u00dfen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "KON", "VMFIN", "PPER", "APPR", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.15": {"line.1": {"text": "Doch willst in diesem See die Liebe du ertr\u00e4nken,", "tokens": ["Doch", "willst", "in", "die\u00b7sem", "See", "die", "Lie\u00b7be", "du", "er\u00b7tr\u00e4n\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "APPR", "PDAT", "NN", "ART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So mu\u00dft du selber dich in seine Fluten senken!", "tokens": ["So", "mu\u00dft", "du", "sel\u00b7ber", "dich", "in", "sei\u00b7ne", "Flu\u00b7ten", "sen\u00b7ken", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "ADV", "PPER", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.16": {"line.1": {"text": "Die Tannenberge rings den tiefen See umklammen", "tokens": ["Die", "Tan\u00b7nen\u00b7ber\u00b7ge", "rings", "den", "tie\u00b7fen", "See", "um\u00b7klam\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADV", "ART", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und sch\u00fctten in den See die Schatten schwarz zusammen.", "tokens": ["Und", "sch\u00fct\u00b7ten", "in", "den", "See", "die", "Schat\u00b7ten", "schwarz", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.17": {"line.1": {"text": "Der Himmel ist bedeckt mit dunklen Wetterlasten,", "tokens": ["Der", "Him\u00b7mel", "ist", "be\u00b7deckt", "mit", "dunk\u00b7len", "Wet\u00b7ter\u00b7las\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch ruhig starrt das Rohr, und alle L\u00fcfte rasten.", "tokens": ["Doch", "ru\u00b7hig", "starrt", "das", "Rohr", ",", "und", "al\u00b7le", "L\u00fcf\u00b7te", "ras\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$,", "KON", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.18": {"line.1": {"text": "Sehr ernst ist hier die Welt und stumm in sich versunken,", "tokens": ["Sehr", "ernst", "ist", "hier", "die", "Welt", "und", "stumm", "in", "sich", "ver\u00b7sun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "ADJD", "APPR", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als w\u00e4r ihr letzter Laut im finstern See ertrunken.", "tokens": ["Als", "w\u00e4r", "ihr", "letz\u00b7ter", "Laut", "im", "fins\u00b7tern", "See", "er\u00b7trun\u00b7ken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "ADJA", "APPR", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.19": {"line.1": {"text": "Als wie ein Scheidegru\u00df erscheint mir diese Stille,", "tokens": ["Als", "wie", "ein", "Schei\u00b7de\u00b7gru\u00df", "er\u00b7scheint", "mir", "die\u00b7se", "Stil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOKOM", "ART", "NN", "VVFIN", "PPER", "PDAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein stummes Lebewohl, ein d\u00fcstrer letzter Wille.", "tokens": ["Ein", "stum\u00b7mes", "Le\u00b7be\u00b7wohl", ",", "ein", "d\u00fcst\u00b7rer", "letz\u00b7ter", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.20": {"line.1": {"text": "Sehr ernst ist hier die Welt und mahnt, das Erdenweh,", "tokens": ["Sehr", "ernst", "ist", "hier", "die", "Welt", "und", "mahnt", ",", "das", "Er\u00b7den\u00b7weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "ADV", "ART", "NN", "KON", "VVFIN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Des Herzens letzten Wunsch zu werfen in den See.", "tokens": ["Des", "Her\u00b7zens", "letz\u00b7ten", "Wunsch", "zu", "wer\u00b7fen", "in", "den", "See", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "PTKZU", "VVINF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.21": {"line.1": {"text": "O Hoffnungen, hinab! zerri\u00dfne Traumgeflechte!", "tokens": ["O", "Hoff\u00b7nun\u00b7gen", ",", "hin\u00b7ab", "!", "zer\u00b7ri\u00df\u00b7ne", "Traum\u00b7ge\u00b7flech\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PTKVZ", "$.", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "O Liebe, s\u00fc\u00dfer Schmerz der schlummerlosen N\u00e4chte!", "tokens": ["O", "Lie\u00b7be", ",", "s\u00fc\u00b7\u00dfer", "Schmerz", "der", "schlum\u00b7mer\u00b7lo\u00b7sen", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "ADJA", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.22": {"line.1": {"text": "Ihr habt mein Herz get\u00e4uscht; nicht heilen wird die Wunde,", "tokens": ["Ihr", "habt", "mein", "Herz", "ge\u00b7t\u00e4uscht", ";", "nicht", "hei\u00b7len", "wird", "die", "Wun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "VVPP", "$.", "PTKNEG", "VVINF", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Doch hab ich noch die Kraft, zu sto\u00dfen euch zum Grunde. \u2013", "tokens": ["Doch", "hab", "ich", "noch", "die", "Kraft", ",", "zu", "sto\u00b7\u00dfen", "euch", "zum", "Grun\u00b7de", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PTKZU", "VVFIN", "PPER", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.23": {"line.1": {"text": "Der Wind wacht auf, ich seh ihn durchs Gew\u00e4sser streichen;", "tokens": ["Der", "Wind", "wacht", "auf", ",", "ich", "seh", "ihn", "durchs", "Ge\u00b7w\u00e4s\u00b7ser", "strei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Will denn sein Hauch das Herz mir noch einmal erweichen?", "tokens": ["Will", "denn", "sein", "Hauch", "das", "Herz", "mir", "noch", "ein\u00b7mal", "er\u00b7wei\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PPOSAT", "NN", "ART", "NN", "PPER", "ADV", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.24": {"line.1": {"text": "Das Schilf am Ufer bebt und fl\u00fcstert mir so bange,", "tokens": ["Das", "Schilf", "am", "U\u00b7fer", "bebt", "und", "fl\u00fcs\u00b7tert", "mir", "so", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "NN", "VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Im Winde bebt der Wald am steilen Uferhange.", "tokens": ["Im", "Win\u00b7de", "bebt", "der", "Wald", "am", "stei\u00b7len", "U\u00b7fer\u00b7han\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.25": {"line.1": {"text": "Ich h\u00f6re kommen dich, Natur! dein Mantel rauscht,", "tokens": ["Ich", "h\u00f6\u00b7re", "kom\u00b7men", "dich", ",", "Na\u00b7tur", "!", "dein", "Man\u00b7tel", "rauscht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "VVFIN", "PPER", "$,", "NN", "$.", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie der Geliebten Kleid, wenn ich nach ihr gelauscht;", "tokens": ["Wie", "der", "Ge\u00b7lieb\u00b7ten", "Kleid", ",", "wenn", "ich", "nach", "ihr", "ge\u00b7lauscht", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$,", "KOUS", "PPER", "APPR", "PPER", "VVPP", "$."], "meter": "-+-+-+-++--+", "measure": "iambic.hexa.chol"}}, "stanza.26": {"line.1": {"text": "Willst du denn noch einmal an meinen Hals dich h\u00e4ngen?", "tokens": ["Willst", "du", "denn", "noch", "ein\u00b7mal", "an", "mei\u00b7nen", "Hals", "dich", "h\u00e4n\u00b7gen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "ADV", "ADV", "APPR", "PPOSAT", "NN", "PPER", "VVINF", "$."], "meter": "-+-+-----+-+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "Ins Elend locken mich mit schmeichelnden Ges\u00e4ngen?", "tokens": ["Ins", "E\u00b7lend", "lo\u00b7cken", "mich", "mit", "schmei\u00b7cheln\u00b7den", "Ge\u00b7s\u00e4n\u00b7gen", "?"], "token_info": ["w