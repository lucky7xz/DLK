{"dta.poem.23414": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Fr\u00fchling alten Stils.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wen k\u00f6nnte nicht der Fr\u00fchling freuen?", "tokens": ["Wen", "k\u00f6nn\u00b7te", "nicht", "der", "Fr\u00fch\u00b7ling", "freu\u00b7en", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PTKNEG", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wem wird das alte Herz nicht jung?", "tokens": ["Wem", "wird", "das", "al\u00b7te", "Herz", "nicht", "jung", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "ADJA", "NN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer wollte sich nicht gern erneuen", "tokens": ["Wer", "woll\u00b7te", "sich", "nicht", "gern", "er\u00b7neu\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VMFIN", "PRF", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In Freuden der Erinnerung?", "tokens": ["In", "Freu\u00b7den", "der", "E\u00b7rin\u00b7ne\u00b7rung", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.2": {"line.1": {"text": "Und dennoch stimmt es mich so eigen \u2014", "tokens": ["Und", "den\u00b7noch", "stimmt", "es", "mich", "so", "ei\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Der ganzen Welt ward Fr\u00fchlingsgl\u00fcck:", "tokens": ["Der", "gan\u00b7zen", "Welt", "ward", "Fr\u00fch\u00b7lings\u00b7gl\u00fcck", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den Polen will kein Lenz sich zeigen,", "tokens": ["Den", "Po\u00b7len", "will", "kein", "Lenz", "sich", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PIAT", "NN", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der wei\u00dfe Zar h\u00e4lt ihn zur\u00fcck.", "tokens": ["Der", "wei\u00b7\u00dfe", "Zar", "h\u00e4lt", "ihn", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}