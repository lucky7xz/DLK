{"textgrid.poem.55854": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "Einsamkeit", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die Einsamkeit ist wie ein Regen.", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "ist", "wie", "ein", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie steigt vom Meer den Abenden entgegen;", "tokens": ["Sie", "steigt", "vom", "Meer", "den", "A\u00b7ben\u00b7den", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von Ebenen, die fern sind und entlegen,", "tokens": ["von", "E\u00b7be\u00b7nen", ",", "die", "fern", "sind", "und", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "geht sie zum Himmel, der sie immer hat.", "tokens": ["geht", "sie", "zum", "Him\u00b7mel", ",", "der", "sie", "im\u00b7mer", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und erst vom Himmel f\u00e4llt sie auf die Stadt.", "tokens": ["Und", "erst", "vom", "Him\u00b7mel", "f\u00e4llt", "sie", "auf", "die", "Stadt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Regnet hernieder in den Zwitterstunden,", "tokens": ["Reg\u00b7net", "her\u00b7nie\u00b7der", "in", "den", "Zwit\u00b7ter\u00b7stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "wenn sich nach Morgen wenden alle Gassen", "tokens": ["wenn", "sich", "nach", "Mor\u00b7gen", "wen\u00b7den", "al\u00b7le", "Gas\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PRF", "APPR", "NN", "VVFIN", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "und wenn die Leiber, welche nichts gefunden,", "tokens": ["und", "wenn", "die", "Lei\u00b7ber", ",", "wel\u00b7che", "nichts", "ge\u00b7fun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "PIS", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "entt\u00e4uscht und traurig von einander lassen;", "tokens": ["ent\u00b7t\u00e4uscht", "und", "trau\u00b7rig", "von", "ein\u00b7an\u00b7der", "las\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "KON", "ADJD", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "und wenn die Menschen, die einander hassen,", "tokens": ["und", "wenn", "die", "Men\u00b7schen", ",", "die", "ein\u00b7an\u00b7der", "has\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "NN", "$,", "PRELS", "PRF", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "in ", "tokens": ["in"], "token_info": ["word"], "pos": ["APPR"], "meter": "-", "measure": "single.down"}}, "stanza.3": {"line.1": {"text": "dann geht die Einsamkeit mit den Fl\u00fcssen...", "tokens": ["dann", "geht", "die", "Ein\u00b7sam\u00b7keit", "mit", "den", "Fl\u00fcs\u00b7sen", "..."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Die Einsamkeit ist wie ein Regen.", "tokens": ["Die", "Ein\u00b7sam\u00b7keit", "ist", "wie", "ein", "Re\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "KOKOM", "ART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie steigt vom Meer den Abenden entgegen;", "tokens": ["Sie", "steigt", "vom", "Meer", "den", "A\u00b7ben\u00b7den", "ent\u00b7ge\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "von Ebenen, die fern sind und entlegen,", "tokens": ["von", "E\u00b7be\u00b7nen", ",", "die", "fern", "sind", "und", "ent\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADJD", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+---+-+-+-", "measure": "dactylic.init"}, "line.4": {"text": "geht sie zum Himmel, der sie immer hat.", "tokens": ["geht", "sie", "zum", "Him\u00b7mel", ",", "der", "sie", "im\u00b7mer", "hat", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPRART", "NN", "$,", "PRELS", "PPER", "ADV", "VAFIN", "$."], "meter": "-