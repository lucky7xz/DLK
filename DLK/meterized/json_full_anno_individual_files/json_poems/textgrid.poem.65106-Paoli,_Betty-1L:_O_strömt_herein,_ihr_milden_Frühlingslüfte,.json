{"textgrid.poem.65106": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: O str\u00f6mt herein, ihr milden Fr\u00fchlingsl\u00fcfte,", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O str\u00f6mt herein, ihr milden Fr\u00fchlingsl\u00fcfte,", "tokens": ["O", "str\u00f6mt", "her\u00b7ein", ",", "ihr", "mil\u00b7den", "Fr\u00fch\u00b7lings\u00b7l\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PTKVZ", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und tr\u00f6stet mich in meiner Krankenzelle!", "tokens": ["Und", "tr\u00f6s\u00b7tet", "mich", "in", "mei\u00b7ner", "Kran\u00b7ken\u00b7zel\u00b7le", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Herein, o West! du fl\u00fcchtiger Geselle!", "tokens": ["Her\u00b7ein", ",", "o", "West", "!", "du", "fl\u00fcch\u00b7ti\u00b7ger", "Ge\u00b7sel\u00b7le", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKVZ", "$,", "FM", "FM", "$.", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Herein ihr Strahlen und ihr Blumend\u00fcfte.", "tokens": ["Her\u00b7ein", "ihr", "Strah\u00b7len", "und", "ihr", "Blu\u00b7men\u00b7d\u00fcf\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sonst sucht' ich euch im Berg- und Felsgekl\u00fcfte,", "tokens": ["Sonst", "sucht'", "ich", "euch", "im", "Ber\u00b7g", "und", "Fels\u00b7ge\u00b7kl\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "APPRART", "TRUNC", "KON", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Im Thalesgrund und an dem Waldesquelle,", "tokens": ["Im", "Tha\u00b7les\u00b7grund", "und", "an", "dem", "Wal\u00b7des\u00b7quel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Jetzt aber fleh' ich: Kommt zu meiner Schwelle", "tokens": ["Jetzt", "a\u00b7ber", "fleh'", "ich", ":", "Kommt", "zu", "mei\u00b7ner", "Schwel\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$.", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und scheucht hinweg den dumpfen Hauch der Gr\u00fcfte", "tokens": ["Und", "scheucht", "hin\u00b7weg", "den", "dum\u00b7pfen", "Hauch", "der", "Gr\u00fcf\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "ART", "ADJA", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Soll mir des ernsten Engels Ruf erschallen,", "tokens": ["Soll", "mir", "des", "erns\u00b7ten", "En\u00b7gels", "Ruf", "er\u00b7schal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "ADJA", "NN", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So la\u00dft, eh' ich dem Todesstreich erlegen,", "tokens": ["So", "la\u00dft", ",", "eh'", "ich", "dem", "To\u00b7des\u00b7streich", "er\u00b7le\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Noch eure holden Schimmer auf mich fallen!", "tokens": ["Noch", "eu\u00b7re", "hol\u00b7den", "Schim\u00b7mer", "auf", "mich", "fal\u00b7len", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "ADJA", "NN", "APPR", "PRF", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Wenn nicht, so la\u00dft es tief ins Herz mich pr\u00e4gen,", "tokens": ["Wenn", "nicht", ",", "so", "la\u00dft", "es", "tief", "ins", "Herz", "mich", "pr\u00e4\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PTKNEG", "$,", "ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Wie es doch sch\u00f6n und herrlich hinzuwallen", "tokens": ["Wie", "es", "doch", "sch\u00f6n", "und", "herr\u00b7lich", "hin\u00b7zu\u00b7wal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "ADV", "ADJD", "KON", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Auf einer Welt voll Fr\u00fchlingsglanz und Segen!", "tokens": ["Auf", "ei\u00b7ner", "Welt", "voll", "Fr\u00fch\u00b7lings\u00b7glanz", "und", "Se\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "O str\u00f6mt herein, ihr milden Fr\u00fchlingsl\u00fcfte,", "tokens": ["O", "str\u00f6mt", "her\u00b7ein", ",", "ihr", "mil\u00b7den", "Fr\u00fch\u0