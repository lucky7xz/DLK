{"textgrid.poem.43938": {"metadata": {"author": {"name": "G\u00fcnther, Johann Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Amarillis, hat mein Sehnen", "genre": "verse", "period": "N.A.", "pub_year": 1709, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Amarillis, hat mein Sehnen", "tokens": ["A\u00b7ma\u00b7ril\u00b7lis", ",", "hat", "mein", "Seh\u00b7nen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses um mein Herz verdient,", "tokens": ["Die\u00b7ses", "um", "mein", "Herz", "ver\u00b7dient", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "APPR", "PPOSAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Da\u00df mein Fluch von deinen Thr\u00e4nen", "tokens": ["Da\u00df", "mein", "Fluch", "von", "dei\u00b7nen", "Thr\u00e4\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem feuchten Gra\u00dfe gr\u00fcnt,", "tokens": ["Mit", "dem", "feuch\u00b7ten", "Gra\u00b7\u00dfe", "gr\u00fcnt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Welchem meiner Liebe Brand", "tokens": ["Wel\u00b7chem", "mei\u00b7ner", "Lie\u00b7be", "Brand"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAT", "PPOSAT", "NN", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Saft und Wachsthum l\u00e4ngst entwand?", "tokens": ["Saft", "und", "Wach\u00b7sthum", "l\u00e4ngst", "ent\u00b7wand", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Hastu darum mich gebunden,", "tokens": ["Has\u00b7tu", "da\u00b7rum", "mich", "ge\u00b7bun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "PAV", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "War ich darum freudenvoll,", "tokens": ["War", "ich", "da\u00b7rum", "freu\u00b7den\u00b7voll", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PAV", "ADJD", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df der Ri\u00df, so schnell verwunden,", "tokens": ["Da\u00df", "der", "Ri\u00df", ",", "so", "schnell", "ver\u00b7wun\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "$,", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Desto sch\u00e4rfer schmerzen soll?", "tokens": ["Des\u00b7to", "sch\u00e4r\u00b7fer", "schmer\u00b7zen", "soll", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Rufe nur den leichten Wellen", "tokens": ["Ru\u00b7fe", "nur", "den", "leich\u00b7ten", "Wel\u00b7len"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und dem gr\u00fcnen Ufer zu,", "tokens": ["Und", "dem", "gr\u00fc\u00b7nen", "U\u00b7fer", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn bey meinen Ungl\u00fccksf\u00e4llen", "tokens": ["Denn", "bey", "mei\u00b7nen", "Un\u00b7gl\u00fccks\u00b7f\u00e4l\u00b7len"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "\u00c4ndern sie so schnell als du;", "tokens": ["\u00c4n\u00b7dern", "sie", "so", "schnell", "als", "du", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "ADJD", "KOKOM", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ja, sie \u00e4ndern Lauf und Ort,", "tokens": ["Ja", ",", "sie", "\u00e4n\u00b7dern", "Lauf", "und", "Ort", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "PPER", "VVINF", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Und du \u00e4nderst Herz und Wort.", "tokens": ["Und", "du", "\u00e4n\u00b7derst", "Herz", "und", "Wort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Seht, ihr angenehmen Wiesen,", "tokens": ["Seht", ",", "ihr", "an\u00b7ge\u00b7neh\u00b7men", "Wie\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Elidor steht jezt besch\u00e4mt,", "tokens": ["E\u00b7li\u00b7dor", "steht", "jezt", "be\u00b7sch\u00e4mt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "ADJD", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.9": {"text": "Weil er die bey euch gepriesen,", "tokens": ["Weil", "er", "die", "bey", "euch", "ge\u00b7prie\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "APPR", "PPER", "VVPP", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.10": {"text": "Die sich ihm nicht mehr bequemt.", "tokens": ["Die", "sich", "ihm", "nicht", "mehr", "be\u00b7quemt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "PPER", "PTKNEG", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Himmel, hastu einen Seegen,", "tokens": ["Him\u00b7mel", ",", "has\u00b7tu", "ei\u00b7nen", "See\u00b7gen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der auf Erden gl\u00fccklich macht,", "tokens": ["Der", "auf", "Er\u00b7den", "gl\u00fcck\u00b7lich", "macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "NN", "ADJD", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "O so sey er meinetwegen", "tokens": ["O", "so", "sey", "er", "mei\u00b7net\u00b7we\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "ADV", "VAFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Amarillen zugedacht.", "tokens": ["A\u00b7ma\u00b7ril\u00b7len", "zu\u00b7ge\u00b7dacht", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "\u00dcbersch\u00fctt ihr Haupt und Brust", "tokens": ["\u00dc\u00b7ber\u00b7sch\u00fctt", "ihr", "Haupt", "und", "Brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Mit des Paradieses Lust!", "tokens": ["Mit", "des", "Pa\u00b7ra\u00b7die\u00b7ses", "Lust", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dieses w\u00fcntsch ich, mich zu r\u00e4chen", "tokens": ["Die\u00b7ses", "w\u00fcnt\u00b7sch", "ich", ",", "mich", "zu", "r\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "$,", "PPER", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Vor den falschen Selbstbetrug;", "tokens": ["Vor", "den", "fal\u00b7schen", "Selbst\u00b7be\u00b7trug", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Denn sich meiner zu entbrechen,", "tokens": ["Denn", "sich", "mei\u00b7ner", "zu", "ent\u00b7bre\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "PPOSAT", "PTKZU", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Ist sie schon gestraft genug.", "tokens": ["Ist", "sie", "schon", "ge\u00b7straft", "ge\u00b7nug", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADJD", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber, ach, was soll die Rache?", "tokens": ["A\u00b7ber", ",", "ach", ",", "was", "soll", "die", "Ra\u00b7che", "?"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "ITJ", "$,", "PWS", "VMFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Was entbrechen? Nimmermehr.", "tokens": ["Was", "ent\u00b7bre\u00b7chen", "?", "Nim\u00b7mer\u00b7mehr", "."], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["PWS", "VVINF", "$.", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ich h\u00f6re, seh und mache,", "tokens": ["Was", "ich", "h\u00f6\u00b7re", ",", "seh", "und", "ma\u00b7che", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "R\u00fchrt mich ihrentwegen sehr;", "tokens": ["R\u00fchrt", "mich", "ih\u00b7rent\u00b7we\u00b7gen", "sehr", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Linden, Wa\u00dfer, Feld und Stein", "tokens": ["Lin\u00b7den", ",", "Wa\u00b7\u00dfer", ",", "Feld", "und", "Stein"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "NN", "$,", "NN", "KON", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Pr\u00e4gen mir ihr Bildn\u00fc\u00df ein", "tokens": ["Pr\u00e4\u00b7gen", "mir", "ihr", "Bild\u00b7n\u00fc\u00df", "ein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "PPER", "PPOSAT", "NN", "ART"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erwecken meine Liebe,", "tokens": ["Und", "er\u00b7we\u00b7cken", "mei\u00b7ne", "Lie\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die sie wider mich besch\u00fczt", "tokens": ["Die", "sie", "wi\u00b7der", "mich", "be\u00b7sch\u00fczt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und, indem ich mich betr\u00fcbe,", "tokens": ["Und", ",", "in\u00b7dem", "ich", "mich", "be\u00b7tr\u00fc\u00b7be", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "PRF", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "An der Seite weinend sizt.", "tokens": ["An", "der", "Sei\u00b7te", "wei\u00b7nend", "sizt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Amarillis, hat mein K\u00fc\u00dfen", "tokens": ["A\u00b7ma\u00b7ril\u00b7lis", ",", "hat", "mein", "K\u00fc\u00b7\u00dfen"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VAFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich nur einmahl recht vergn\u00fcgt,", "tokens": ["Dich", "nur", "ein\u00b7mahl", "recht", "ver\u00b7gn\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Kanstu Ort und Zeit noch wi\u00dfen,", "tokens": ["Kans\u00b7tu", "Ort", "und", "Zeit", "noch", "wi\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "NN", "KON", "NN", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die mein Herz an deins gef\u00fcgt,", "tokens": ["Die", "mein", "Herz", "an", "deins", "ge\u00b7f\u00fcgt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "APPR", "NE", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "O so bitt ich durch den Schwur,", "tokens": ["O", "so", "bitt", "ich", "durch", "den", "Schwur", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "ART", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der uns mit Bedacht entfuhr,", "tokens": ["Der", "uns", "mit", "Be\u00b7dacht", "ent\u00b7fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "O so bitt ich durch die Plagen,", "tokens": ["O", "so", "bitt", "ich", "durch", "die", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Die ich mir mit dir erwehlt", "tokens": ["Die", "ich", "mir", "mit", "dir", "er\u00b7wehlt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PRF", "APPR", "PPER", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Und bisher mit Lust getragen:", "tokens": ["Und", "bis\u00b7her", "mit", "Lust", "ge\u00b7tra\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPR", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Lebe doch nur ungequ\u00e4lt!", "tokens": ["Le\u00b7be", "doch", "nur", "un\u00b7ge\u00b7qu\u00e4lt", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "ADV", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Nimm den Ring, das Pfand des Eides,", "tokens": ["Nimm", "den", "Ring", ",", "das", "Pfand", "des", "Ei\u00b7des", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "ART", "NN", "$,", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und behalt den leichten Flor,", "tokens": ["Und", "be\u00b7halt", "den", "leich\u00b7ten", "Flor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn die Menge meines Leides", "tokens": ["Denn", "die", "Men\u00b7ge", "mei\u00b7nes", "Lei\u00b7des"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Stellt dir schon ein Trauren vor;", "tokens": ["Stellt", "dir", "schon", "ein", "Trau\u00b7ren", "vor", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Dein Verdacht und meine Treu", "tokens": ["Dein", "Ver\u00b7dacht", "und", "mei\u00b7ne", "Treu"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Machen schon die Seele frey", "tokens": ["Ma\u00b7chen", "schon", "die", "See\u00b7le", "frey"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ART", "NN", "ADJD"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Und erl\u00f6sen mich im Grabe,", "tokens": ["Und", "er\u00b7l\u00f6\u00b7sen", "mich", "im", "Gra\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und auf diesem soll allein,", "tokens": ["Und", "auf", "die\u00b7sem", "soll", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PDAT", "VMFIN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Da\u00df ich dich betrogen habe,", "tokens": ["Da\u00df", "ich", "dich", "be\u00b7tro\u00b7gen", "ha\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVPP", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Meine sch\u00f6nste Grabschrift seyn.", "tokens": ["Mei\u00b7ne", "sch\u00f6ns\u00b7te", "Grab\u00b7schrift", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Lebe wohl mit deinem Kummer,", "tokens": ["Le\u00b7be", "wohl", "mit", "dei\u00b7nem", "Kum\u00b7mer", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo dich der nur leben l\u00e4st,", "tokens": ["Wo", "dich", "der", "nur", "le\u00b7ben", "l\u00e4st", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ART", "ADV", "VVINF", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und verst\u00f6re meinen Schlummer", "tokens": ["Und", "ver\u00b7st\u00f6\u00b7re", "mei\u00b7nen", "Schlum\u00b7mer"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch kein na\u00dfes Trauerfest.", "tokens": ["Durch", "kein", "na\u00b7\u00dfes", "Trau\u00b7er\u00b7fest", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Hier um diesen w\u00fcsten Thal,", "tokens": ["Hier", "um", "die\u00b7sen", "w\u00fcs\u00b7ten", "Thal", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Der uns mehr als tausendmahl", "tokens": ["Der", "uns", "mehr", "als", "tau\u00b7send\u00b7mahl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PIAT", "KOKOM", "ADV"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Vor der Tadelsucht verborgen,", "tokens": ["Vor", "der", "Ta\u00b7del\u00b7sucht", "ver\u00b7bor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Schneid ich in den nechsten Baum:", "tokens": ["Schneid", "ich", "in", "den", "nechs\u00b7ten", "Baum", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Elidor und seine Sorgen", "tokens": ["E\u00b7li\u00b7dor", "und", "sei\u00b7ne", "Sor\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "KON", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Suchten hier den lezten Raum.", "tokens": ["Such\u00b7ten", "hier", "den", "lez\u00b7ten", "Raum", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Ist noch einer von den Hirten,", "tokens": ["Ist", "noch", "ei\u00b7ner", "von", "den", "Hir\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Der die rechte Liebe kennt,", "tokens": ["Der", "die", "rech\u00b7te", "Lie\u00b7be", "kennt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dem verbleibt mein Kranz 