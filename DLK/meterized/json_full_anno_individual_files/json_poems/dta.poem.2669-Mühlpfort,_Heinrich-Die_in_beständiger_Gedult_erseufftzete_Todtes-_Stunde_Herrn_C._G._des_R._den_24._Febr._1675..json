{"dta.poem.2669": {"metadata": {"author": {"name": "M\u00fchlpfort, Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die in best\u00e4ndiger Gedult erseufftzete Todtes-  \n Stunde/  \n  Herrn C. G. des R. den 24. Febr. 1675.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1686", "urn": "urn:nbn:de:kobv:b4-20414-7", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So hat nun die Gedult Herr Grundmanns \u00fcber-\nwunden/", "tokens": ["So", "hat", "nun", "die", "Ge\u00b7dult", "Herr", "Grund\u00b7manns", "\u00fc\u00b7ber", "wun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "ART", "NN", "NN", "NE", "TRUNC", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "In welcher er den Grund des Glaubens hat gelegt:", "tokens": ["In", "wel\u00b7cher", "er", "den", "Grund", "des", "Glau\u00b7bens", "hat", "ge\u00b7legt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "ART", "NN", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "So brechen nunmehr an die h\u00f6chst- erw\u00fcnschte Stun-", "tokens": ["So", "bre\u00b7chen", "nun\u00b7mehr", "an", "die", "h\u00f6chst", "er\u00b7w\u00fcnschte", "Stun"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ADV", "APPR", "ART", "TRUNC", "ADJA", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "Da\u00df man den m\u00fcden Leib zu seiner Ruhe tr\u00e4gt.", "tokens": ["Da\u00df", "man", "den", "m\u00fc\u00b7den", "Leib", "zu", "sei\u00b7ner", "Ru\u00b7he", "tr\u00e4gt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Er wuste/ da\u00df er nur im Staube war gegr\u00fcndet/", "tokens": ["Er", "wus\u00b7te", "/", "da\u00df", "er", "nur", "im", "Stau\u00b7be", "war", "ge\u00b7gr\u00fcn\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOUS", "PPER", "ADV", "APPRART", "NN", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und da\u00df sein leimen Haus nicht ewig k\u00f6nte stehn.", "tokens": ["Und", "da\u00df", "sein", "lei\u00b7men", "Haus", "nicht", "e\u00b7wig", "k\u00f6n\u00b7te", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "ADJA", "NN", "PTKNEG", "ADJD", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Drumb rieff er/ von der Glut des Himmels angez\u00fcndet:", "tokens": ["Drumb", "rieff", "er", "/", "von", "der", "Glut", "des", "Him\u00b7mels", "an\u00b7ge\u00b7z\u00fcn\u00b7det", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "$(", "APPR", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Herr/ la\u00df doch deinen Knecht aus diesem Kercker gehn!", "tokens": ["Herr", "/", "la\u00df", "doch", "dei\u00b7nen", "Knecht", "aus", "die\u00b7sem", "Ker\u00b7cker", "gehn", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$(", "VVIMP", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Je mehr der Schmertzen Qual den siechen Leib umbgeben/", "tokens": ["Je", "mehr", "der", "Schmert\u00b7zen", "Qual", "den", "sie\u00b7chen", "Leib", "umb\u00b7ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "NN", "ART", "ADJA", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Je feuriger sein Geist den Sternen sich genaht/", "tokens": ["Je", "feu\u00b7ri\u00b7ger", "sein", "Geist", "den", "Ster\u00b7nen", "sich", "ge\u00b7naht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "PPOSAT", "NN", "ART", "NN", "PRF", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und da kein Mittel mehr/ die Kranckheit zuerheben/", "tokens": ["Und", "da", "kein", "Mit\u00b7tel", "mehr", "/", "die", "Kran\u00b7ck\u00b7heit", "zu\u00b7er\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "NN", "ADV", "$(", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Floh\u2019 er zu seinem GOtt als Vater/ Artzt/ und Rath.", "tokens": ["Floh'", "er", "zu", "sei\u00b7nem", "Gott", "als", "Va\u00b7ter", "/", "Artzt", "/", "und", "Rath", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "PPOSAT", "NN", "KOUS", "NN", "$(", "NN", "$(", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Des muden Alters Last/ die abgeschw\u00e4chten Glieder/", "tokens": ["Des", "mu\u00b7den", "Al\u00b7ters", "Last", "/", "die", "ab\u00b7ge\u00b7schw\u00e4ch\u00b7ten", "Glie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NE", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Die Adern ohne Safft/ die trockene Gebein\u2019/", "tokens": ["Die", "A\u00b7dern", "oh\u00b7ne", "Safft", "/", "die", "tro\u00b7cke\u00b7ne", "Ge\u00b7bein'", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "NN", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Und gantz entkr\u00e4ffte Krafft rie\u00df nie den Glauben nieder/", "tokens": ["Und", "gantz", "ent\u00b7kr\u00e4ff\u00b7te", "Krafft", "rie\u00df", "nie", "den", "Glau\u00b7ben", "nie\u00b7der", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJA", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Der mitten in der Noth must Uberwinder seyn.", "tokens": ["Der", "mit\u00b7ten", "in", "der", "Noth", "must", "U\u00b7ber\u00b7win\u00b7der", "seyn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "APPR", "ART", "NN", "VMFIN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Die edle Palme wird von keiner B\u00fcrde sincken/", "tokens": ["Die", "ed\u00b7le", "Pal\u00b7me", "wird", "von", "kei\u00b7ner", "B\u00fcr\u00b7de", "sin\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Jhr gr\u00fcner Gipfel steigt anmutiger empor.", "tokens": ["Ihr", "gr\u00fc\u00b7ner", "Gip\u00b7fel", "steigt", "an\u00b7mu\u00b7ti\u00b7ger", "em\u00b7por", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADJD", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "Ein Fels/ und mu\u00df er gleich den Schaum de\u00df Meeres trincken/", "tokens": ["Ein", "Fels", "/", "und", "mu\u00df", "er", "gleich", "den", "Schaum", "de\u00df", "Mee\u00b7res", "trin\u00b7cken", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "KON", "VMFIN", "PPER", "ADV", "ART", "NN", "ART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Weist dennoch unbewegt sein steinern Haupt hervor.", "tokens": ["Weist", "den\u00b7noch", "un\u00b7be\u00b7wegt", "sein", "stei\u00b7nern", "Haupt", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADJD", "PPOSAT", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Mit solcher Palmen Art/ und Felsen-gleichem Muthe", "tokens": ["Mit", "sol\u00b7cher", "Pal\u00b7men", "Art", "/", "und", "Fel\u00b7sen\u00b7glei\u00b7chem", "Mu\u00b7the"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "NN", "$(", "KON", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "War in dem grimmen Schmertz ", "tokens": ["War", "in", "dem", "grim\u00b7men", "Schmertz"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.23": {"text": "Er k\u00fcste mit Gedult des H\u00f6chsten Zucht und Ruthe/", "tokens": ["Er", "k\u00fcs\u00b7te", "mit", "Ge\u00b7dult", "des", "H\u00f6chs\u00b7ten", "Zucht", "und", "Ru\u00b7the", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "KON", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Die seiner Lieb und Treu ein kundbar Zeichen ist.", "tokens": ["Die", "sei\u00b7ner", "Lieb", "und", "Treu", "ein", "kund\u00b7bar", "Zei\u00b7chen", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "KON", "NN", "ART", "ADJD", "NN", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Der vielen Jahre Lauff und Wechselgang der Zeiten", "tokens": ["Der", "vie\u00b7len", "Jah\u00b7re", "Lauff", "und", "Wech\u00b7sel\u00b7gang", "der", "Zei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "PIAT", "NN", "NN", "KON", "NN", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Hatt\u2019 ihn schon l\u00e4ngst belehrt was lange leben sey.", "tokens": ["Hatt'", "ihn", "schon", "l\u00e4ngst", "be\u00b7lehrt", "was", "lan\u00b7ge", "le\u00b7ben", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADV", "ADV", "VVPP", "PRELS", "ADV", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Nichts als ein langer Kampff/ mit Fleisch und Blut zu streiten.", "tokens": ["Nichts", "als", "ein", "lan\u00b7ger", "Kampff", "/", "mit", "Fleisch", "und", "Blut", "zu", "strei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "KOKOM", "ART", "ADJA", "NN", "$(", "APPR", "NN", "KON", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Und seine Herrligkeit ein albre Gauckeley.", "tokens": ["Und", "sei\u00b7ne", "Herr\u00b7lig\u00b7keit", "ein", "alb\u00b7re", "Gau\u00b7cke\u00b7ley", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Er sah\u2019 auch da\u00df er hier auf nichts sich konte gr\u00fcnden/", "tokens": ["Er", "sah'", "auch", "da\u00df", "er", "hier", "auf", "nichts", "sich", "kon\u00b7te", "gr\u00fcn\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "KOUS", "PPER", "ADV", "APPR", "PIS", "PRF", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Der Gliederbau zerf\u00e4llt/ der Ehre Licht verglimmt/", "tokens": ["Der", "Glie\u00b7der\u00b7bau", "zer\u00b7f\u00e4llt", "/", "der", "Eh\u00b7re", "Licht", "ver\u00b7glimmt", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "$(", "ART", "NN", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Die G\u00fcter de\u00df Gel\u00fccks sind weiter nicht als Binden/", "tokens": ["Die", "G\u00fc\u00b7ter", "de\u00df", "Ge\u00b7l\u00fccks", "sind", "wei\u00b7ter", "nicht", "als", "Bin\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VAFIN", "ADV", "PTKNEG", "KOUS", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Worinn der Geist verwirrt/ fast zu sich selbst nicht k\u00f6mmt.", "tokens": ["Wo\u00b7rinn", "der", "Geist", "ver\u00b7wirrt", "/", "fast", "zu", "sich", "selbst", "nicht", "k\u00f6mmt", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJD", "$(", "ADV", "APPR", "PRF", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Und wenn wir alles di\u00df zum Uberflu\u00df besitzen/", "tokens": ["Und", "wenn", "wir", "al\u00b7les", "di\u00df", "zum", "U\u00b7ber\u00b7flu\u00df", "be\u00b7sit\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "PIS", "PDS", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Macht uns die Menge nicht bey solchem Reichthum arm?", "tokens": ["Macht", "uns", "die", "Men\u00b7ge", "nicht", "bey", "sol\u00b7chem", "Reicht\u00b7hum", "arm", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "PTKNEG", "APPR", "PIAT", "NN", "ADJD", "$."], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.3": {"text": "Indem wir sind bem\u00fcht es offt und viel zu n\u00fctzen/", "tokens": ["In\u00b7dem", "wir", "sind", "be\u00b7m\u00fcht", "es", "offt", "und", "viel", "zu", "n\u00fct\u00b7zen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "VVFIN", "PPER", "ADV", "KON", "PIS", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Verzehrt den meisten Theil/ Zeit/ Zufall/ Furcht und Harm.", "tokens": ["Ver\u00b7zehrt", "den", "meis\u00b7ten", "Theil", "/", "Zeit", "/", "Zu\u00b7fall", "/", "Furcht", "und", "Harm", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "PIAT", "NN", "$(", "NN", "$(", "NN", "$(", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Di\u00df hat ", "tokens": ["Di\u00df", "hat"], "token_info": ["word", "word"], "pos": ["PDS", "VAFIN"], "meter": "-+", "measure": "iambic.single"}, "line.2": {"text": "Und ", "tokens": ["Und"], "token_info": ["word"], "pos": ["KON"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Der vor der Menschen Heil die Menschheit angezogen/", "tokens": ["Der", "vor", "der", "Men\u00b7schen", "Heil", "die", "Menschheit", "an\u00b7ge\u00b7zo\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "NN", "ART", "NN", "VVPP", "$("], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Und durch sein heilig Blut von S\u00fcnden uns gefegt.", "tokens": ["Und", "durch", "sein", "hei\u00b7lig", "Blut", "von", "S\u00fcn\u00b7den", "uns", "ge\u00b7fegt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "ADJD", "NN", "APPR", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Dem gab er sich anheim bey noch gesunden Tagen/", "tokens": ["Dem", "gab", "er", "sich", "an\u00b7heim", "bey", "noch", "ge\u00b7sun\u00b7den", "Ta\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PRF", "ADV", "APPR", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als Beystand in dem Creutz und Helffern in der Noth;", "tokens": ["Als", "Beys\u00b7tand", "in", "dem", "Creutz", "und", "Helf\u00b7fern", "in", "der", "Noth", ";"], "to