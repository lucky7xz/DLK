{"textgrid.poem.56120": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "5.", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Errichtet keinen Denkstein. La\u00dft die Rose", "tokens": ["Er\u00b7rich\u00b7tet", "kei\u00b7nen", "Denk\u00b7stein", ".", "La\u00dft", "die", "Ro\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "$.", "VVIMP", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "nur jedes Jahr zu seinen Gunsten bl\u00fchn.", "tokens": ["nur", "je\u00b7des", "Jahr", "zu", "sei\u00b7nen", "Guns\u00b7ten", "bl\u00fchn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Denn Orpheus ists. Seine Metamorphose", "tokens": ["Denn", "Or\u00b7pheus", "ists", ".", "Sei\u00b7ne", "Me\u00b7ta\u00b7mor\u00b7pho\u00b7se"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["KON", "NE", "VAFIN", "$.", "PPOSAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "in dem und dem. Wir sollen uns nicht m\u00fchn", "tokens": ["in", "dem", "und", "dem", ".", "Wir", "sol\u00b7len", "uns", "nicht", "m\u00fchn"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "KON", "ART", "$.", "PPER", "VMFIN", "PPER", "PTKNEG", "VVINF"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "um andre Namen. Ein f\u00fcr alle Male", "tokens": ["um", "and\u00b7re", "Na\u00b7men", ".", "Ein", "f\u00fcr", "al\u00b7le", "Ma\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$.", "ART", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "ists Orpheus, wenn es singt. Er kommt und geht.", "tokens": ["ists", "Or\u00b7pheus", ",", "wenn", "es", "singt", ".", "Er", "kommt", "und", "geht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NE", "$,", "KOUS", "PPER", "VVFIN", "$.", "PPER", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ists nicht schon viel, wenn er die Rosenschale", "tokens": ["Ists", "nicht", "schon", "viel", ",", "wenn", "er", "die", "Ro\u00b7sen\u00b7scha\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "ADV", "ADV", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "um ein paar Tage manchmal \u00fcbersteht?", "tokens": ["um", "ein", "paar", "Ta\u00b7ge", "manch\u00b7mal", "\u00fc\u00b7bers\u00b7teht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "PIAT", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "O wie er schwinden mu\u00df, da\u00df ihrs begrifft!", "tokens": ["O", "wie", "er", "schwin\u00b7den", "mu\u00df", ",", "da\u00df", "ihrs", "be\u00b7grifft", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PWAV", "PPER", "VVINF", "VMFIN", "$,", "KOUS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und wenn ihm selbst auch bangte, da\u00df er schw\u00e4nde.", "tokens": ["Und", "wenn", "ihm", "selbst", "auch", "bang\u00b7te", ",", "da\u00df", "er", "schw\u00e4n\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "