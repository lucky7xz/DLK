{"textgrid.poem.59390": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Abend", "genre": "verse", "period": "N.A.", "pub_year": 1815, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gest\u00fcrzt sind die goldnen Br\u00fccken", "tokens": ["Ge\u00b7st\u00fcrzt", "sind", "die", "gold\u00b7nen", "Br\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VAFIN", "ART", "ADJA", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und unten und oben so still!", "tokens": ["Und", "un\u00b7ten", "und", "o\u00b7ben", "so", "still", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "KON", "ADV", "ADV", "ADJD", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Es will mir nichts mehr gl\u00fccken,", "tokens": ["Es", "will", "mir", "nichts", "mehr", "gl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich wei\u00df nicht mehr, was ich will.", "tokens": ["Ich", "wei\u00df", "nicht", "mehr", ",", "was", "ich", "will", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADV", "$,", "PWS", "PPER", "VMFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.2": {"line.1": {"text": "Von \u00fcppig bl\u00fchenden Schmerzen", "tokens": ["Von", "\u00fcp\u00b7pig", "bl\u00fc\u00b7hen\u00b7den", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Rauscht eine Wildnis im Grund,", "tokens": ["Rauscht", "ei\u00b7ne", "Wild\u00b7nis", "im", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Da spielt wie in wahnsinnigen Scherzen", "tokens": ["Da", "spielt", "wie", "in", "wahn\u00b7sin\u00b7ni\u00b7gen", "Scher\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "APPR", "ADJA", "NN"], "meter": "-+--++--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das Herz an dem schwindligen Schlund. \u2013", "tokens": ["Das", "Herz", "an", "dem", "schwind\u00b7li\u00b7gen", "Schlund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.3": {"line.1": {"text": "Die Felsen m\u00f6chte ich packen", "tokens": ["Die", "Fel\u00b7sen", "m\u00f6ch\u00b7te", "ich", "pa\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Vor Zorn und Wehe und Lust,", "tokens": ["Vor", "Zorn", "und", "We\u00b7he", "und", "Lust", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Und unter den brechenden Zacken", "tokens": ["Und", "un\u00b7ter", "den", "bre\u00b7chen\u00b7den", "Za\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.4": {"text": "Begraben die wilde Brust.", "tokens": ["Be\u00b7gra\u00b7ben", "die", "wil\u00b7de", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+