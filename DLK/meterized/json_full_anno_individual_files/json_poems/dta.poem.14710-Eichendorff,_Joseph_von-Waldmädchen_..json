{"dta.poem.14710": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Waldm\u00e4dchen .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Bin ein Feuer hell, das lodert", "tokens": ["Bin", "ein", "Feu\u00b7er", "hell", ",", "das", "lo\u00b7dert"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "ART", "NN", "ADJD", "$,", "PDS", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Von dem gr\u00fcnen Felsenkranz,", "tokens": ["Von", "dem", "gr\u00fc\u00b7nen", "Fel\u00b7sen\u00b7kranz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seewind ist mein Buhl' und fodert", "tokens": ["See\u00b7wind", "ist", "mein", "Buhl'", "und", "fo\u00b7dert"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VAFIN", "PPOSAT", "NN", "KON", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich zum lust'gen Wirbeltanz,", "tokens": ["Mich", "zum", "lust'\u00b7gen", "Wir\u00b7bel\u00b7tanz", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Kommt und wechselt unbest\u00e4ndig.", "tokens": ["Kommt", "und", "wech\u00b7selt", "un\u00b7be\u00b7st\u00e4n\u00b7dig", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ADJD", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Steigend wild\u201a", "tokens": ["Stei\u00b7gend", "wild", "\u201a"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.7": {"text": "Neigend mild,", "tokens": ["Nei\u00b7gend", "mild", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADJD", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.8": {"text": "Meine schlanken Lohen wend' ich:", "tokens": ["Mei\u00b7ne", "schlan\u00b7ken", "Lo\u00b7hen", "wend'", "ich", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Komm nicht nach mir, ich verbrenn' Dich!", "tokens": ["Komm", "nicht", "nach", "mir", ",", "ich", "ver\u00b7brenn'", "Dich", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "APPR", "PPER", "$,", "PPER", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}