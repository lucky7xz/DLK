{"dta.poem.10063": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Stille.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1735", "urn": "urn:nbn:de:kobv:b4-20086-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nach einem heitern Tag, und k\u00fchler Abend-Zeit,", "tokens": ["Nach", "ei\u00b7nem", "hei\u00b7tern", "Tag", ",", "und", "k\u00fch\u00b7ler", "A\u00b7ben\u00b7dZeit", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Brach, voller reinen Heiterkeit,", "tokens": ["Brach", ",", "vol\u00b7ler", "rei\u00b7nen", "Hei\u00b7ter\u00b7keit", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "$,", "ADJA", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Auch eine k\u00fchl\u2019 und heitre Nacht,", "tokens": ["Auch", "ei\u00b7ne", "k\u00fchl'", "und", "heit\u00b7re", "Nacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Erleuchtet durch die mannichfache Pracht,", "tokens": ["Er\u00b7leuch\u00b7tet", "durch", "die", "man\u00b7nich\u00b7fa\u00b7che", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Und durch der hellen Sternen Schein,", "tokens": ["Und", "durch", "der", "hel\u00b7len", "Ster\u00b7nen", "Schein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Mit Schatten, die dennoch voll Licht, gemach herein.", "tokens": ["Mit", "Schat\u00b7ten", ",", "die", "den\u00b7noch", "voll", "Licht", ",", "ge\u00b7mach", "her\u00b7ein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "NN", "$,", "PRELS", "ADV", "ADJD", "NN", "$,", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ich stand, und lenckte Blick und Hertz,", "tokens": ["Ich", "stand", ",", "und", "lenck\u00b7te", "Blick", "und", "Hertz", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "KON", "ADJA", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit stillem dencken, Himmel-w\u00e4rts;", "tokens": ["Mit", "stil\u00b7lem", "den\u00b7cken", ",", "Him\u00b7mel\u00b7w\u00e4rts", ";"], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PIS", "VVINF", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich senckte mich ins tieffe Meer", "tokens": ["Ich", "senck\u00b7te", "mich", "ins", "tief\u00b7fe", "Meer"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des tieffen Raums, doch ohn\u2019 Gefahr, hinein;", "tokens": ["Des", "tief\u00b7fen", "Raums", ",", "doch", "ohn'", "Ge\u00b7fahr", ",", "hin\u00b7ein", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ADV", "APPR", "NN", "$,", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Weil nicht ein Pol-Stern nur, ein gantzes Heer", "tokens": ["Weil", "nicht", "ein", "Pol\u00b7S\u00b7tern", "nur", ",", "ein", "gant\u00b7zes", "Heer"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PTKNEG", "ART", "NN", "ADV", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Von Pol-Gestirnen mir, mit ihrem hellen Schein,", "tokens": ["Von", "Pol\u00b7Ge\u00b7stir\u00b7nen", "mir", ",", "mit", "ih\u00b7rem", "hel\u00b7len", "Schein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "PPER", "$,", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "So mancher Leit-Stern war.", "tokens": ["So", "man\u00b7cher", "Leit\u00b7S\u00b7tern", "war", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.8": {"text": "Es war die Lufft so heiter und so rein,", "tokens": ["Es", "war", "die", "Lufft", "so", "hei\u00b7ter", "und", "so", "rein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADJD", "KON", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Da\u00df aller Sternen glimme Schaar", "tokens": ["Da\u00df", "al\u00b7ler", "Ster\u00b7nen", "glim\u00b7me", "Schaar"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Nicht fest, wie sonst an der sapphirnen B\u00fchne,", "tokens": ["Nicht", "fest", ",", "wie", "sonst", "an", "der", "sap\u00b7phir\u00b7nen", "B\u00fch\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "PTKVZ", "$,", "PWAV", "ADV", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Rein, frey, und fast zu schweben schiene.", "tokens": ["Rein", ",", "frey", ",", "und", "fast", "zu", "schwe\u00b7ben", "schie\u00b7ne", "."], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "$,", "KON", "ADV", "PTKZU", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich sahe, einzeln bald, bald \u00fcberhaupt sie an,", "tokens": ["Ich", "sa\u00b7he", ",", "ein\u00b7zeln", "bald", ",", "bald", "\u00fc\u00b7ber\u00b7haupt", "sie", "an", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "ADJD", "ADV", "$,", "ADV", "ADV", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und ward zwar ebenfalls von ihrem Glantz und Strahl,", "tokens": ["Und", "ward", "zwar", "e\u00b7ben\u00b7falls", "von", "ih\u00b7rem", "Glantz", "und", "Strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jedoch f\u00fcr dieses mahl", "tokens": ["Je\u00b7doch", "f\u00fcr", "die\u00b7ses", "mahl"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "PDAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Durch die so stille Majest\u00e4t", "tokens": ["Durch", "die", "so", "stil\u00b7le", "Ma\u00b7jes\u00b7t\u00e4t"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Am innigsten ger\u00fchrt.", "tokens": ["Am", "in\u00b7nigs\u00b7ten", "ge\u00b7r\u00fchrt", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "VVPP", "$."], "meter": "+-+--+", "measure": "iambic.tri.chol"}}, "stanza.4": {"line.1": {"text": "O GOTT! rieff ich, Dem ewig Ruhm geb\u00fchrt,", "tokens": ["O", "GoTT", "!", "rieff", "ich", ",", "Dem", "e\u00b7wig", "Ruhm", "ge\u00b7b\u00fchrt", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "VVFIN", "PPER", "$,", "ART", "ADJD", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Wo etwas Deine Macht erh\u00f6ht,", "tokens": ["Wo", "et\u00b7was", "Dei\u00b7ne", "Macht", "er\u00b7h\u00f6ht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "So ist es dieser Zug von Deinen Schaaren.", "tokens": ["So", "ist", "es", "die\u00b7ser", "Zug", "von", "Dei\u00b7nen", "Schaa\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PDAT", "NN", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Nunmehr, seit so viel tausend Jahren,", "tokens": ["Nun\u00b7mehr", ",", "seit", "so", "viel", "tau\u00b7send", "Jah\u00b7ren", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "APPR", "ADV", "ADV", "CARD", "NN", "$,"], "meter": "-++--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Ist Dein allgegenw\u00e4rtger Wille,", "tokens": ["Ist", "Dein", "all\u00b7ge\u00b7gen\u00b7w\u00e4rt\u00b7ger", "Wil\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Dein Wincken, ihr Gesetz. Ein ehrerbietigs schweigen", "tokens": ["Dein", "Win\u00b7cken", ",", "ihr", "Ge\u00b7setz", ".", "Ein", "ehr\u00b7er\u00b7bie\u00b7tigs", "schwei\u00b7gen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PPOSAT", "NN", "$.", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Pflegt hier, bey K\u00f6nigen, die Demuth anzuzeigen:", "tokens": ["Pflegt", "hier", ",", "bey", "K\u00f6\u00b7ni\u00b7gen", ",", "die", "De\u00b7muth", "an\u00b7zu\u00b7zei\u00b7gen", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "$,", "APPR", "NN", "$,", "ART", "NN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "So deucht mich, da\u00df auch hier der Himmel tieffe Stille", "tokens": ["So", "deucht", "mich", ",", "da\u00df", "auch", "hier", "der", "Him\u00b7mel", "tief\u00b7fe", "Stil\u00b7le"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ADV", "ADV", "ART", "NN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Die Gegenwart des Sch\u00f6pfers aller Welt,", "tokens": ["Die", "Ge\u00b7gen\u00b7wart", "des", "Sch\u00f6p\u00b7fers", "al\u00b7ler", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Nebst ihrer Ehr-Furcht, uns vor Augen stellt.", "tokens": ["Nebst", "ih\u00b7rer", "Ehr\u00b7Furcht", ",", "uns", "vor", "Au\u00b7gen", "stellt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "PPER", "APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Ich blieb, bey dieser Still\u2019 in tieffer Stille stehn,", "tokens": ["Ich", "blieb", ",", "bey", "die\u00b7ser", "Still'", "in", "tief\u00b7fer", "Stil\u00b7le", "stehn", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "APPR", "PDAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und dachte der so schnell- und dennoch sanften Weise,", "tokens": ["Und", "dach\u00b7te", "der", "so", "schnell", "und", "den\u00b7noch", "sanf\u00b7ten", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADV", "TRUNC", "KON", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "In welcher sich so ungeme\u00dfne Kreise,", "tokens": ["In", "wel\u00b7cher", "sich", "so", "un\u00b7ge\u00b7me\u00df\u00b7ne", "Krei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PRF", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja gar die Himmel selber drehn,", "tokens": ["Ja", "gar", "die", "Him\u00b7mel", "sel\u00b7ber", "drehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mit stiller Seele nach:", "tokens": ["Mit", "stil\u00b7ler", "See\u00b7le", "nach", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Bis unverhofft ein l\u00e4rmendes Gew\u00e4sche,", "tokens": ["Bis", "un\u00b7ver\u00b7hofft", "ein", "l\u00e4r\u00b7men\u00b7des", "Ge\u00b7w\u00e4\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Ein wild verwirrt Gequack der Fr\u00f6sche,", "tokens": ["Ein", "wild", "ver\u00b7wirrt", "Ge\u00b7quack", "der", "Fr\u00f6\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "ADJD", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Mein stilles dencken unterbrach.", "tokens": ["Mein", "stil\u00b7les", "den\u00b7cken", "un\u00b7ter\u00b7brach", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Ja, dacht ich, hiedurch kann der Erden", "tokens": ["Ja", ",", "dacht", "ich", ",", "hie\u00b7durch", "kann", "der", "Er\u00b7den"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$,", "VVFIN", "PPER", "$,", "PAV", "VMFIN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Unn\u00fctzes eiteles Get\u00fcmmel", "tokens": ["Un\u00b7n\u00fct\u00b7zes", "ei\u00b7te\u00b7les", "Ge\u00b7t\u00fcm\u00b7mel"], "token_info": ["word", "word", "word"], "pos": ["ADJA", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mit jener sanften Still\u2019 am Himmel", "tokens": ["Mit", "je\u00b7ner", "sanf\u00b7ten", "Still'", "am", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "ADJA", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "In einen Gegen-Satz gesetzet werden.", "tokens": ["In", "ei\u00b7nen", "Ge\u00b7gen\u00b7Satz", "ge\u00b7set\u00b7zet", "wer\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Man handelt hier, man wandelt, rennt und l\u00e4ufft,", "tokens": ["Man", "han\u00b7delt", "hier", ",", "man", "wan\u00b7delt", ",", "rennt", "und", "l\u00e4ufft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ADV", "$,", "PIS", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Man lacht, man weint, man liebt, man keifft,", "tokens": ["Man", "lacht", ",", "man", "weint", ",", "man", "liebt", ",", "man", "keifft", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Man siehet ein verwirrtes schw\u00e4rmen,", "tokens": ["Man", "sie\u00b7het", "ein", "ver\u00b7wirr\u00b7tes", "schw\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Man h\u00f6rt ein unn\u00fctz m\u00fchsam l\u00e4rmen,", "tokens": ["Man", "h\u00f6rt", "ein", "un\u00b7n\u00fctz", "m\u00fch\u00b7sam", "l\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "ART", "ADJD", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Und alles ist dennoch, wie ein Geschrey,", "tokens": ["Und", "al\u00b7les", "ist", "den\u00b7noch", ",", "wie", "ein", "Ge\u00b7schrey", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punc