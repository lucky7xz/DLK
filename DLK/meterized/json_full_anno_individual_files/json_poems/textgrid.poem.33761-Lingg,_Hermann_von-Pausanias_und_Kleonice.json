{"textgrid.poem.33761": {"metadata": {"author": {"name": "Lingg, Hermann von", "birth": "N.A.", "death": "N.A."}, "title": "Pausanias und Kleonice", "genre": "verse", "period": "N.A.", "pub_year": 1862, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Kalt war die Nacht, Schneeregen fiel,", "tokens": ["Kalt", "war", "die", "Nacht", ",", "Schnee\u00b7re\u00b7gen", "fiel", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$,", "NN", "VVFIN", "$,"], "meter": "-+-++--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Er sa\u00df am Kolcherstrande,", "tokens": ["Er", "sa\u00df", "am", "Kol\u00b7cher\u00b7stran\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Da kamen zu ihm die M\u00e4nner vom Nil,", "tokens": ["Da", "ka\u00b7men", "zu", "ihm", "die", "M\u00e4n\u00b7ner", "vom", "Nil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "PPER", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+----+--+", "measure": "iambic.tri.chol"}, "line.4": {"text": "Theb\u00e4er im dunklen Gewande;", "tokens": ["The\u00b7b\u00e4\u00b7er", "im", "dunk\u00b7len", "Ge\u00b7wan\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Sie warfen in rauchende Pfannen das Kraut", "tokens": ["Sie", "war\u00b7fen", "in", "rau\u00b7chen\u00b7de", "Pfan\u00b7nen", "das", "Kraut"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN", "ART", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Vom Lorbeer zu Schlangen- und Drachenhaut.", "tokens": ["Vom", "Lor\u00b7beer", "zu", "Schlan\u00b7gen", "und", "Dra\u00b7chen\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "TRUNC", "KON", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}}, "stanza.2": {"line.1": {"text": "Der Rauch stieg mit dem Meeresdunst", "tokens": ["Der", "Rauch", "stieg", "mit", "dem", "Mee\u00b7res\u00b7dunst"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Vermischt zum Mond hin\u00fcber,", "tokens": ["Ver\u00b7mischt", "zum", "Mond", "hin\u00b7\u00fc\u00b7ber", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ADV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der wie durch eine Feuersbrunst", "tokens": ["Der", "wie", "durch", "ei\u00b7ne", "Feu\u00b7ers\u00b7brunst"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "KOKOM", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Herabsah tr\u00fcb und tr\u00fcber.", "tokens": ["Her\u00b7ab\u00b7sah", "tr\u00fcb", "und", "tr\u00fc\u00b7ber", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Abstreiften die Priester ihr faltig Gewand,", "tokens": ["Ab\u00b7streif\u00b7ten", "die", "Pries\u00b7ter", "ihr", "fal\u00b7tig", "Ge\u00b7wand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "PPER", "ADJD", "NN", "$,"], "meter": "++--+--+--+", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Entbl\u00f6\u00dft im Rauch der Feldherr stand.", "tokens": ["Ent\u00b7bl\u00f6\u00dft", "im", "Rauch", "der", "Feld\u00b7herr", "stand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Er sprach: \u00bbDie ihr den Tod beschw\u00f6rt,", "tokens": ["Er", "sprach", ":", "\u00bb", "Die", "ihr", "den", "Tod", "be\u00b7schw\u00f6rt", ","], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "$(", "ART", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Beschw\u00f6rt mir den Schemen des Leibes,", "tokens": ["Be\u00b7schw\u00f6rt", "mir", "den", "Sche\u00b7men", "des", "Lei\u00b7bes", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Den ich hei\u00df geliebt und den ich zerst\u00f6rt;", "tokens": ["Den", "ich", "hei\u00df", "ge\u00b7liebt", "und", "den", "ich", "zer\u00b7st\u00f6rt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "ADJD", "VVPP", "KON", "ART", "PPER", "VVPP", "$."], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.4": {"text": "O lasset noch einmal des Weibes", "tokens": ["O", "las\u00b7set", "noch", "ein\u00b7mal", "des", "Wei\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "ADV", "ADV", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vers\u00f6hnende Stimme mich h\u00f6ren und dann", "tokens": ["Ver\u00b7s\u00f6h\u00b7nen\u00b7de", "Stim\u00b7me", "mich", "h\u00f6\u00b7ren", "und", "dann"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "PPER", "VVINF", "KON", "ADV"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Verschlie\u00dfet die Erde, vollendet den Bann!\u00ab", "tokens": ["Ver\u00b7schlie\u00b7\u00dfet", "die", "Er\u00b7de", ",", "voll\u00b7en\u00b7det", "den", "Bann", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["VVFIN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$.", "$("], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.4": {"line.1": {"text": "Pausanias sprach's, der \u00c4gypter nahm", "tokens": ["Pau\u00b7sa\u00b7ni\u00b7as", "sprach's", ",", "der", "\u00c4\u00b7gyp\u00b7ter", "nahm"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["NE", "NE", "$,", "ART", "NN", "VVFIN"], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Und schlug metallene Platten.", "tokens": ["Und", "schlug", "me\u00b7tal\u00b7le\u00b7ne", "Plat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Allm\u00e4hlich erschien's, und n\u00e4her kam", "tokens": ["All\u00b7m\u00e4h\u00b7lich", "er\u00b7schien's", ",", "und", "n\u00e4\u00b7her", "kam"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PIS", "$,", "KON", "ADJD", "VVFIN"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ein bleicher verwundeter Schatten", "tokens": ["Ein", "blei\u00b7cher", "ver\u00b7wun\u00b7de\u00b7ter", "Schat\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Und stand mit geschlossenem Augenlicht,", "tokens": ["Und", "stand", "mit", "ge\u00b7schlos\u00b7se\u00b7nem", "Au\u00b7gen\u00b7licht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$,"], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.6": {"text": "Mit r\u00fcckgebogenem Angesicht.", "tokens": ["Mit", "r\u00fcck\u00b7ge\u00b7bo\u00b7ge\u00b7nem", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Wie Rosenbl\u00fcten im Mondenglanz", "tokens": ["Wie", "Ro\u00b7sen\u00b7bl\u00fc\u00b7ten", "im", "Mon\u00b7den\u00b7glanz"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "NN", "APPRART", "NN"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Sanft schienen die Wangen ger\u00f6tet,", "tokens": ["Sanft", "schie\u00b7nen", "die", "Wan\u00b7gen", "ge\u00b7r\u00f6\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.3": {"text": "Ihr Haupt umgab ein Myrtenkranz;", "tokens": ["Ihr", "Haupt", "um\u00b7gab", "ein", "Myr\u00b7ten\u00b7kranz", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "F\u00fcr den, der sie get\u00f6tet,", "tokens": ["F\u00fcr", "den", ",", "der", "sie", "ge\u00b7t\u00f6\u00b7tet", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "$,", "PRELS", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "War noch wie einst ihr Haupt geschm\u00fcckt,", "tokens": ["War", "noch", "wie", "einst", "ihr", "Haupt", "ge\u00b7schm\u00fcckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "KOKOM", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Von scheuem Sehnen der Mund umz\u00fcckt.", "tokens": ["Von", "scheu\u00b7em", "Seh\u00b7nen", "der", "Mund", "um\u00b7z\u00fcckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.6": {"line.1": {"text": "Der Grieche rief: \u00bbMein armes Reh! \u2013", "tokens": ["Der", "Grie\u00b7che", "rief", ":", "\u00bb", "Mein", "ar\u00b7mes", "Reh", "!", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sank zu ihren F\u00fc\u00dfen \u2013", "tokens": ["Und", "sank", "zu", "ih\u00b7ren", "F\u00fc\u00b7\u00dfen", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "PPOSAT", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "O nenne der Strafen gr\u00f6\u00dftes Weh,", "tokens": ["O", "nen\u00b7ne", "der", "Stra\u00b7fen", "gr\u00f6\u00df\u00b7tes", "Weh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+---+-+-+", "measure": "dactylic.init"}, "line.4": {"text": "O lasse die Schuld mich b\u00fc\u00dfen!", "tokens": ["O", "las\u00b7se", "die", "Schuld", "mich", "b\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "PPER", "VVINF", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Sprich, k\u00fcnde mir, wo ich und wann,", "tokens": ["Sprich", ",", "k\u00fcn\u00b7de", "mir", ",", "wo", "ich", "und", "wann", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$,", "VVFIN", "PPER", "$,", "PWAV", "PPER", "KON", "PWAV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Erz\u00fcrnte, dich vers\u00f6hnen kann!\u00ab", "tokens": ["Er\u00b7z\u00fcrn\u00b7te", ",", "dich", "ver\u00b7s\u00f6h\u00b7nen", "kann", "!", "\u00ab"], "token_info": ["word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NN", "$,", "PRF", "VVINF", "VMFIN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Er rief's, und sie erhob die Hand", "tokens": ["Er", "rie\u00b7f's", ",", "und", "sie", "er\u00b7hob", "die", "Hand"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KON", "PPER", "VVFIN", "ART", "NN"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.2": {"text": "Und sprach in sanften Worten:", "tokens": ["Und", "sprach", "in", "sanf\u00b7ten", "Wor\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbpausanias, kehre zum Vaterland!", "tokens": ["\u00bb", "pau\u00b7sa\u00b7ni\u00b7as", ",", "keh\u00b7re", "zum", "Va\u00b7ter\u00b7land", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "FM.la", "$,", "VVFIN", "APPRART", "NN", "$."], "meter": "+---+--+-+", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "In Sparta vor den Pforten", "tokens": ["In", "Spar\u00b7ta", "vor", "den", "Pfor\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Des Pallastempels, dort allein", "tokens": ["Des", "Pal\u00b7las\u00b7tem\u00b7pels", ",", "dort", "al\u00b7lein"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "$,", "ADV", "ADV"], "meter": "--+-+--+", "measure": "iambic.tri.chol"}, "line.6": {"text": "Wird deine Seele der Blutschuld rein.", "tokens": ["Wird", "dei\u00b7ne", "See\u00b7le", "der", "Blut\u00b7schuld", "rein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.8": {"line.1": {"text": "Im Hades steht ein Lagerpf\u00fchl", "tokens": ["Im", "Ha\u00b7des", "steht", "ein", "La\u00b7ger\u00b7pf\u00fchl"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "F\u00fcr dich und mich gebettet,", "tokens": ["F\u00fcr", "dich", "und", "mich", "ge\u00b7bet\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "KON", "PPER", "VVPP", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Pfosten sind mit Asphodil", "tokens": ["Die", "Pfos\u00b7ten", "sind", "mit", "As\u00b7pho\u00b7dil"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und Amaranth umkettet.", "tokens": ["Und", "A\u00b7ma\u00b7ran\u00b7th", "um\u00b7ket\u00b7tet", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["KON", "NE", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "Dort kr\u00e4nz' ich mich zu deinem Empfang;", "tokens": ["Dort", "kr\u00e4nz'", "ich", "mich", "zu", "dei\u00b7nem", "Emp\u00b7fang", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Die Parzen singen den Brautgesang.\u00ab", "tokens": ["Die", "Par\u00b7zen", "sin\u00b7gen", "den", "Braut\u00b7ge\u