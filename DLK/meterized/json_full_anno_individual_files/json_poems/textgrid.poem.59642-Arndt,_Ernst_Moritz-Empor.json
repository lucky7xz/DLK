punct"], "pos": ["ADV", "PWS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist des Menschen Freude?", "tokens": ["Was", "ist", "des", "Men\u00b7schen", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Hui, ein Nu, ein Augenblick,", "tokens": ["Ein", "Hui", ",", "ein", "Nu", ",", "ein", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "FM", "$,", "ART", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wechsels leichte Beute,", "tokens": ["Des", "Wech\u00b7sels", "leich\u00b7te", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Wasser, das von Bergen rinnt,", "tokens": ["Ein", "Was\u00b7ser", ",", "das", "von", "Ber\u00b7gen", "rinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schnee, ein Nebel, Schaum und Wind:", "tokens": ["Ein", "Schnee", ",", "ein", "Ne\u00b7bel", ",", "Schaum", "und", "Wind", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Erden mag nichts bleiben.", "tokens": ["Auf", "Er\u00b7den", "mag", "nichts", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure"