{"textgrid.poem.59642": {"metadata": {"author": {"name": "Arndt, Ernst Moritz", "birth": "N.A.", "death": "N.A."}, "title": "Empor", "genre": "verse", "period": "N.A.", "pub_year": 1814, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf! Auf, mein Geist, und schwinge dich", "tokens": ["Auf", "!", "Auf", ",", "mein", "Geist", ",", "und", "schwin\u00b7ge", "dich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "APPR", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empor vom Erdenstaube!", "tokens": ["Em\u00b7por", "vom", "Er\u00b7dens\u00b7tau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Flieg, fliege, fliege wonniglich,", "tokens": ["Flieg", ",", "flie\u00b7ge", ",", "flie\u00b7ge", "won\u00b7nig\u00b7lich", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "$,", "VVFIN", "ADJD", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.4": {"text": "Du schnelle Himmelstaube!", "tokens": ["Du", "schnel\u00b7le", "Him\u00b7mel\u00b7stau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Empor vom dunklen Erdental!", "tokens": ["Em\u00b7por", "vom", "dunk\u00b7len", "Er\u00b7den\u00b7tal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Empor zum lichten Sternensaal!", "tokens": ["Em\u00b7por", "zum", "lich\u00b7ten", "Ster\u00b7nen\u00b7saal", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Empor zum Christ, dem Heiland!", "tokens": ["Em\u00b7por", "zum", "Christ", ",", "dem", "Hei\u00b7land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Empor! Empor aus finstrer Nacht!", "tokens": ["Em\u00b7por", "!", "Em\u00b7por", "aus", "finst\u00b7rer", "Nacht", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "NN", "APPR", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Aus Staub und Schmach und Banden,", "tokens": ["Aus", "Staub", "und", "Schmach", "und", "Ban\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Aus Sklaverei und Bann und Acht", "tokens": ["Aus", "Skla\u00b7ve\u00b7rei", "und", "Bann", "und", "Acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "KON", "NN", "KON", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Zu jenen freien Landen,", "tokens": ["Zu", "je\u00b7nen", "frei\u00b7en", "Lan\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Wo Lug und Trug und Wahn verweht,", "tokens": ["Wo", "Lug", "und", "Trug", "und", "Wahn", "ver\u00b7weht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "KON", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wo nie die Sonne untergeht,", "tokens": ["Wo", "nie", "die", "Son\u00b7ne", "un\u00b7ter\u00b7geht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Worin die Frommen bl\u00fchen.", "tokens": ["Wo\u00b7rin", "die", "From\u00b7men", "bl\u00fc\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Hienieden ist nur M\u00fch' und Not,", "tokens": ["Hien\u00b7ie\u00b7den", "ist", "nur", "M\u00fch'", "und", "Not", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nur eitel Eitelkeiten;", "tokens": ["Nur", "ei\u00b7tel", "Ei\u00b7tel\u00b7kei\u00b7ten", ";"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Der arme Mensch mu\u00df bis zum Tod", "tokens": ["Der", "ar\u00b7me", "Mensch", "mu\u00df", "bis", "zum", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit Trug und Schatten streiten:", "tokens": ["Mit", "Trug", "und", "Schat\u00b7ten", "strei\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Dem bald man mit drei Ellen mi\u00dft", "tokens": ["Dem", "bald", "man", "mit", "drei", "El\u00b7len", "mi\u00dft"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "ADV", "PIS", "APPR", "CARD", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Den Raum, wo's still vom Kriegen ist,", "tokens": ["Den", "Raum", ",", "wo's", "still", "vom", "Krie\u00b7gen", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "ADJD", "APPRART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wie viel' sind seiner Plagen!", "tokens": ["Wie", "viel'", "sind", "sei\u00b7ner", "Pla\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Hienieden was ist Lust und Gl\u00fcck?", "tokens": ["Hien\u00b7ie\u00b7den", "was", "ist", "Lust", "und", "Gl\u00fcck", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PWS", "VAFIN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Was ist des Menschen Freude?", "tokens": ["Was", "ist", "des", "Men\u00b7schen", "Freu\u00b7de", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Ein Hui, ein Nu, ein Augenblick,", "tokens": ["Ein", "Hui", ",", "ein", "Nu", ",", "ein", "Au\u00b7gen\u00b7blick", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "FM", "$,", "ART", "ADV", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Des Wechsels leichte Beute,", "tokens": ["Des", "Wech\u00b7sels", "leich\u00b7te", "Beu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Ein Wasser, das von Bergen rinnt,", "tokens": ["Ein", "Was\u00b7ser", ",", "das", "von", "Ber\u00b7gen", "rinnt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein Schnee, ein Nebel, Schaum und Wind:", "tokens": ["Ein", "Schnee", ",", "ein", "Ne\u00b7bel", ",", "Schaum", "und", "Wind", ":"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Auf Erden mag nichts bleiben.", "tokens": ["Auf", "Er\u00b7den", "mag", "nichts", "blei\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PIS", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum auf, mein Geist, und schwinge dich", "tokens": ["Drum", "auf", ",", "mein", "Geist", ",", "und", "schwin\u00b7ge", "dich"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["PAV", "PTKVZ", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die hellen Sternenstra\u00dfen!", "tokens": ["Die", "hel\u00b7len", "Ster\u00b7nen\u00b7stra\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Was irdisch ist, wirf hinter dich!", "tokens": ["Was", "ir\u00b7disch", "ist", ",", "wirf", "hin\u00b7ter", "dich", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "ADJD", "VAFIN", "$,", "VVIMP", "APPR", "PPER", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Du mu\u00dft es doch verlassen.", "tokens": ["Du", "mu\u00dft", "es", "doch", "ver\u00b7las\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Das Unten mu\u00df f\u00fcr andre sein,", "tokens": ["Das", "Un\u00b7ten", "mu\u00df", "f\u00fcr", "and\u00b7re", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "APPR", "PIS", "VAINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das Droben bleibet ewig dein \u2013", "tokens": ["Das", "Dro\u00b7ben", "blei\u00b7bet", "e\u00b7wig", "dein", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "PPOSAT", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Zur Heimat wolln wir fliegen.", "tokens": ["Zur", "Hei\u00b7mat", "wolln", "wir", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Drum auf! Mein Geist! Mein froher Geist!", "tokens": ["Drum", "auf", "!", "Mein", "Geist", "!", "Mein", "fro\u00b7her", "Geist", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PAV", "PTKVZ", "$.", "PPOSAT", "NN", "$.", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Zur Heimat wolln wir fliegen;", "tokens": ["Zur", "Hei\u00b7mat", "wolln", "wir", "flie\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Die Erde und was irdisch hei\u00dft,", "tokens": ["Die", "Er\u00b7de", "und", "was", "ir\u00b7disch", "hei\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PWS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das lassen wir unten liegen.", "tokens": ["Das", "las\u00b7sen", "wir", "un\u00b7ten", "lie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.5": {"text": "O du, der unser Helfer ist,", "tokens": ["O", "du", ",", "der", "un\u00b7ser", "Hel\u00b7fer", "ist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$,", "PRELS", "PPOSAT", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das hilf du uns, Herr Jesu Christ,", "tokens": ["Das", "hilf", "du", "uns", ",", "Herr", "Je\u00b7su", "Christ", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "PPER", "$,", "NN", "NE", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df wir's mit dir gewinnen!", "tokens": ["Da\u00df", "wir's", "mit", "dir", "ge\u00b7win\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "Auf! Auf, mein Geist, und schwinge dich", "tokens": ["Auf", "!", "Auf", ",", "mein", "Geist", ",", "und", "schwin\u00b7ge", "dich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "$.", "APPR", "$,", "PPOSAT", "NN", "$,", "KON", "VVFIN", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Empor vom Erdenstaube!", "tokens": ["Em\u00b7por", "vom", "Er\u00b7dens\u00b7tau\u00b7be", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$."]