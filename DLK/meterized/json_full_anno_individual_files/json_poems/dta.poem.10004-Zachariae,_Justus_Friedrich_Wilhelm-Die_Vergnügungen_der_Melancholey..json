{"dta.poem.10004": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Vergn\u00fcgungen  \n  der Melancholey.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20674-3", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Unter jener verfallnen Abtey bemoo\u00dften Gew\u00f6lben,", "tokens": ["Un\u00b7ter", "je\u00b7ner", "ver\u00b7fall\u00b7nen", "Ab\u00b7tey", "be\u00b7moo\u00df\u00b7ten", "Ge\u00b7w\u00f6l\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJA", "NN", "ADJA", "NN", "$,"], "meter": "+-+--+-+--+--+-", "measure": "hexameter"}, "line.2": {"text": "Will ich oft sitzen, allein, in jenen d\u00e4mmernden Stunden,", "tokens": ["Will", "ich", "oft", "sit\u00b7zen", ",", "al\u00b7lein", ",", "in", "je\u00b7nen", "d\u00e4m\u00b7mern\u00b7den", "Stun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "VVINF", "$,", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN", "$,"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Wenn der traurige Mond in den s\u00fcrchterlicheinsamen", "tokens": ["Wenn", "der", "trau\u00b7ri\u00b7ge", "Mond", "in", "den", "s\u00fcrch\u00b7ter\u00b7lich\u00b7ein\u00b7sa\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN", "APPR", "ART", "ADJA"], "meter": "--+--+--+-+-+-", "measure": "anapaest.tri.plus"}, "line.4": {"text": "Einen flimmernden Stral von str\u00f6menden Lichte hinein-", "tokens": ["Ei\u00b7nen", "flim\u00b7mern\u00b7den", "Stral", "von", "str\u00f6\u00b7men\u00b7den", "Lich\u00b7te", "hin\u00b7ein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "ADJA", "NN", "TRUNC"], "meter": "+-+--+-+--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Und ein tiefes heiliges Schweigen auf allem umher", "tokens": ["Und", "ein", "tie\u00b7fes", "hei\u00b7li\u00b7ges", "Schwei\u00b7gen", "auf", "al\u00b7lem", "um\u00b7her"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "ADJA", "NN", "APPR", "PIS", "PTKVZ"], "meter": "--+-+--+--+--+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Au\u00dfer der Eule klagendem Lied, die, unter dem Schutte", "tokens": ["Au\u00b7\u00dfer", "der", "Eu\u00b7le", "kla\u00b7gen\u00b7dem", "Lied", ",", "die", ",", "un\u00b7ter", "dem", "Schut\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,", "PRELS", "$,", "APPR", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.7": {"text": "Dumpfigter H\u00f6len verscheucht, ihr \u00f6des Wohnhaus er-", "tokens": ["Dump\u00b7fig\u00b7ter", "H\u00f6\u00b7len", "ver\u00b7scheucht", ",", "ihr", "\u00f6\u00b7des", "Wohn\u00b7haus", "er"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "VVPP", "$,", "PPOSAT", "ADJA", "NN", "TRUNC"], "meter": "+--+--+-+-+--", "measure": "dactylic.di.plus"}, "line.8": {"text": "Oder der ruhig s\u00e4uselnden Luft, die zwischen dem Laube", "tokens": ["O\u00b7der", "der", "ru\u00b7hig", "s\u00e4u\u00b7seln\u00b7den", "Luft", ",", "die", "zwi\u00b7schen", "dem", "Lau\u00b7be"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "NN"], "meter": "+--+-+--+-+--+-", "measure": "hexameter"}, "line.9": {"text": "Des breitbl\u00e4ttrichten Epheu rauscht, der an den Ge-", "tokens": ["Des", "breit\u00b7bl\u00e4tt\u00b7rich\u00b7ten", "E\u00b7pheu", "rauscht", ",", "der", "an", "den", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,", "PRELS", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.10": {"text": "Eines hangenden Thurms sich an den W\u00e4nden hinauf-", "tokens": ["Ei\u00b7nes", "han\u00b7gen\u00b7den", "Thurms", "sich", "an", "den", "W\u00e4n\u00b7den", "hin\u00b7auf"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "PRF", "APPR", "ART", "NN", "TRUNC"], "meter": "+-+--+-+-+--+", "measure": "trochaic.hexa.relaxed"}, "line.11": {"text": "Oder la\u00df mich auch oft den nahen Tannengang irren,", "tokens": ["O\u00b7der", "la\u00df", "mich", "auch", "oft", "den", "na\u00b7hen", "Tan\u00b7nen\u00b7gang", "ir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADV", "ADV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "Wo die M\u00f6nche vordem in frommen Tiefsinn gewandelt.", "tokens": ["Wo", "die", "M\u00f6n\u00b7che", "vor\u00b7dem", "in", "from\u00b7men", "Tief\u00b7sinn", "ge\u00b7wan\u00b7delt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADV", "APPR", "ADJA", "NN", "VVPP", "$."], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.13": {"text": "Wie ich, im unabsehlichen Leeren der hohen Gew\u00f6lbe,", "tokens": ["Wie", "ich", ",", "im", "un\u00b7ab\u00b7seh\u00b7li\u00b7chen", "Lee\u00b7ren", "der", "ho\u00b7hen", "Ge\u00b7w\u00f6l\u00b7be", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "$,", "APPRART", "ADJA", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+--+--+--+-", "measure": "iambic.hexa.relaxed"}, "line.14": {"text": "K\u00fchn einhergeh; fasset mich schnell im innersten Dunkel", "tokens": ["K\u00fchn", "ein\u00b7her\u00b7geh", ";", "fas\u00b7set", "mich", "schnell", "im", "in\u00b7ners\u00b7ten", "Dun\u00b7kel"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "$.", "VVFIN", "PPER", "ADJD", "APPRART", "ADJA", "NN"], "meter": "+-+-+--+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.15": {"text": "Heiliger Schauder, und h\u00fcllet mein Herz in traurige", "tokens": ["Hei\u00b7li\u00b7ger", "Schau\u00b7der", ",", "und", "h\u00fcl\u00b7let", "mein", "Herz", "in", "trau\u00b7ri\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "KON", "VVFIN", "PPOSAT", "NN", "APPR", "ADJA"], "meter": "+--+--+--+-+-+", "measure": "dactylic.tri.plus"}}}}}