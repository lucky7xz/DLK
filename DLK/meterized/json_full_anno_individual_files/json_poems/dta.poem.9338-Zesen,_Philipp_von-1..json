{"dta.poem.9338": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "1.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der abend bricht herrein/ die k\u00fchle nacht entspringet/", "tokens": ["Der", "a\u00b7bend", "bricht", "her\u00b7rein", "/", "die", "k\u00fch\u00b7le", "nacht", "ent\u00b7sprin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "VVFIN", "PTKVZ", "$(", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "da man der Liebe lied mit vollen fre\u00fcden singet/", "tokens": ["da", "man", "der", "Lie\u00b7be", "lied", "mit", "vol\u00b7len", "fre\u00fc\u00b7den", "sin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "die Kinder k\u00fchler lufft seyn auch in stoltzer ruh/", "tokens": ["die", "Kin\u00b7der", "k\u00fch\u00b7ler", "lufft", "seyn", "auch", "in", "stolt\u00b7zer", "ruh", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAINF", "ADV", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "nur Echo wachet noch und rufft mir jmmerzu", "tokens": ["nur", "E\u00b7cho", "wa\u00b7chet", "noch", "und", "rufft", "mir", "jm\u00b7mer\u00b7zu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "NN", "VVFIN", "ADV", "KON", "VVFIN", "PPER", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "die letzten sylben nach: Ich sehe wie den sternen", "tokens": ["die", "letz\u00b7ten", "syl\u00b7ben", "nach", ":", "Ich", "se\u00b7he", "wie", "den", "ster\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVFIN", "PTKVZ", "$.", "PPER", "VVFIN", "KOKOM", "ART", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der fast erblasste mond zu wincken kann von fernen;", "tokens": ["der", "fast", "er\u00b7blass\u00b7te", "mond", "zu", "win\u00b7cken", "kann", "von", "fer\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJA", "NN", "PTKZU", "VVINF", "VMFIN", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur ich bin ohne trost und wache gantz allein/", "tokens": ["Nur", "ich", "bin", "oh\u00b7ne", "trost", "und", "wa\u00b7che", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN", "KON", "ADJA", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und seuftze fort f\u00fcr fort/ wenn andre schlaffen ein.", "tokens": ["und", "seuft\u00b7ze", "fort", "f\u00fcr", "fort", "/", "wenn", "and\u00b7re", "schlaf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PTKVZ", "$(", "KOUS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Du ruheft/ Hedewig/ und liegst in stoltzem frieden/", "tokens": ["Du", "ru\u00b7heft", "/", "He\u00b7de\u00b7wig", "/", "und", "liegst", "in", "stolt\u00b7zem", "frie\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "NE", "$(", "KON", "VVFIN", "APPR", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "in deinem federzelt/ ich mu\u00df mich noch erm\u00fcden", "tokens": ["in", "dei\u00b7nem", "fe\u00b7der\u00b7zelt", "/", "ich", "mu\u00df", "mich", "noch", "er\u00b7m\u00fc\u00b7den"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "PPER", "VMFIN", "PPER", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und schlafflo\u00df bringen zu die gantze liebe nacht/", "tokens": ["und", "schlaf\u00b7flo\u00df", "brin\u00b7gen", "zu", "die", "gant\u00b7ze", "lie\u00b7be", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVINF", "APPR", "ART", "ADJA", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "da ich allein auff dich ", "tokens": ["da", "ich", "al\u00b7lein", "auff", "dich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "APPR", "PPER"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Des hertzens vorhoff ligt dein rosen-Mund gestillet/", "tokens": ["Des", "hert\u00b7zens", "vor\u00b7hoff", "ligt", "dein", "ro\u00b7sen\u00b7Mund", "ge\u00b7stil\u00b7let", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "PPOSAT", "NN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "dein edles Heupt ist auch in k\u00fcssen eingeh\u00fcllet;", "tokens": ["dein", "ed\u00b7les", "Heupt", "ist", "auch", "in", "k\u00fcs\u00b7sen", "ein\u00b7ge\u00b7h\u00fcl\u00b7let", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "ADV", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Nur ich bin ohne Trost und wache gantz allein/", "tokens": ["Nur", "ich", "bin", "oh\u00b7ne", "Trost", "und", "wa\u00b7che", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN", "KON", "ADJA", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und seuftze fort f\u00fcr fort/ wenn andre schlaffen ein.", "tokens": ["und", "seuft\u00b7ze", "fort", "f\u00fcr", "fort", "/", "wenn", "and\u00b7re", "schlaf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PTKVZ", "$(", "KOUS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es kann ein Wandersmann mit lust zu bette gehen/", "tokens": ["Es", "kann", "ein", "Wan\u00b7ders\u00b7mann", "mit", "lust", "zu", "bet\u00b7te", "ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "APPR", "NN", "PTKZU", "VVFIN", "VVINF", "$("], "meter": "-+-+--+--+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "wenn auff der himmelsburg die g\u00fcldnen sterne stehen/", "tokens": ["wenn", "auff", "der", "him\u00b7mels\u00b7burg", "die", "g\u00fcld\u00b7nen", "ster\u00b7ne", "ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn er den m\u00fcden tag mit reisen durch gebracht/", "tokens": ["wenn", "er", "den", "m\u00fc\u00b7den", "tag", "mit", "rei\u00b7sen", "durch", "ge\u00b7bracht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "APPR", "VVFIN", "APPR", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "so ruht er sorgen-frey und schl\u00e4fft die gantze nacht/", "tokens": ["so", "ruht", "er", "sor\u00b7gen\u00b7frey", "und", "schl\u00e4fft", "die", "gant\u00b7ze", "nacht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "KON", "VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "und wann der fr\u00fche tag die Sonne wiederbringet/", "tokens": ["und", "wann", "der", "fr\u00fc\u00b7he", "tag", "die", "Son\u00b7ne", "wie\u00b7der\u00b7brin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so steht er freudig auff und seinen Gott ansinget/", "tokens": ["so", "steht", "er", "freu\u00b7dig", "auff", "und", "sei\u00b7nen", "Gott", "an\u00b7sin\u00b7get", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "PTKVZ", "KON", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "nur ich bin ohne trost und wache gantz allein/", "tokens": ["nur", "ich", "bin", "oh\u00b7ne", "trost", "und", "wa\u00b7che", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN", "KON", "ADJA", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und seuftze fort f\u00fcr fort/ wenn andre schlaffen ein.", "tokens": ["und", "seuft\u00b7ze", "fort", "f\u00fcr", "fort", "/", "wenn", "and\u00b7re", "schlaf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PTKVZ", "$(", "KOUS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Ich bin in angst un\u0303 furcht/ die Eul erb\u00e4rmlich schreyet/", "tokens": ["Ich", "bin", "in", "angst", "u\u00f1", "furcht", "/", "die", "Eul", "er\u00b7b\u00e4rm\u00b7lich", "schre\u00b7yet", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "NN", "KON", "NN", "$(", "ART", "NN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.2": {"text": "Ich h\u00f6re/ wie sie mier den bittern todt schon dreuet.", "tokens": ["Ich", "h\u00f6\u00b7re", "/", "wie", "sie", "mier", "den", "bit\u00b7tern", "todt", "schon", "dreu\u00b7et", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWAV", "PPER", "PPER", "ART", "ADJA", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach schmertz ach weh ach leid! ich \u00e4chtze durch die nacht", "tokens": ["Ach", "schmertz", "ach", "weh", "ach", "leid", "!", "ich", "\u00e4cht\u00b7ze", "durch", "die", "nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "ITJ", "ITJ", "ADV", "ADV", "ADJD", "$.", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "und liege schlafflo\u00df da/ bi\u00df Cynthius erwacht/", "tokens": ["und", "lie\u00b7ge", "schlaf\u00b7flo\u00df", "da", "/", "bi\u00df", "Cyn\u00b7thi\u00b7us", "er\u00b7wacht", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ADV", "$(", "APPR", "NE", "VVPP", "$("], "meter": "-+-+---+-+-+", "measure": "unknown.measure.penta"}, "line.5": {"text": "es schl\u00e4fft der Arbeitsmann/ der Drescherruhet s\u00fc\u00dfe/", "tokens": ["es", "schl\u00e4fft", "der", "Ar\u00b7beits\u00b7mann", "/", "der", "Dre\u00b7scher\u00b7ru\u00b7het", "s\u00fc\u00b7\u00dfe", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$(", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "das stoltze pferd ruht aus/ f\u00fchlt nicht die m\u00fcden f\u00fc\u00dfe/", "tokens": ["das", "stolt\u00b7ze", "pferd", "ruht", "aus", "/", "f\u00fchlt", "nicht", "die", "m\u00fc\u00b7den", "f\u00fc\u00b7\u00dfe", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PTKVZ", "$(", "VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "nur ich bin ohne trost und wacht gantz allein/", "tokens": ["nur", "ich", "bin", "oh\u00b7ne", "trost", "und", "wacht", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "APPR", "NN", "KON", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.8": {"text": "und seuftze fort f\u00fcr fort/ wenn andre schlaffen ein.", "tokens": ["und", "seuft\u00b7ze", "fort", "f\u00fcr", "fort", "/", "wenn", "and\u00b7re", "schlaf\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PTKVZ", "$(", "KOUS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Ey nun gehab dich wohl/ Ade du stoltze Dirne/", "tokens": ["Ey", "nun", "ge\u00b7hab", "dich", "wohl", "/", "A\u00b7de", "du", "stolt\u00b7ze", "Dir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VVFIN", "PPER", "ADV", "$(", "NN", "PPER", "ADJA", "NN", "$("], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}, "line.2": {"text": "Ich ruffe noch dazu zu zengen das gestirne/", "tokens": ["Ich", "ruf\u00b7fe", "noch", "da\u00b7zu", "zu", "zen\u00b7gen", "das", "ge\u00b7stir\u00b7ne", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PAV", "PTKZU", "VVFIN", "ART", "ADJA", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Euch ruff ich zeugen an/ Jhr th\u00e4ler meine zier/", "tokens": ["Euch", "ruff", "ich", "zeu\u00b7gen", "an", "/", "Ihr", "th\u00e4\u00b7ler", "mei\u00b7ne", "zier", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "PTKVZ", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "jhr wiesen/ berg\u2019/ und b\u00fcsch\u2019 und das auch wohnet hier/", "tokens": ["jhr", "wie\u00b7sen", "/", "ber\u00b7g'", "/", "und", "b\u00fcsch'", "und", "das", "auch", "woh\u00b7net", "hier", "/"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "VVFIN", "$(", "KON", "VVFIN", "KON", "PDS", "ADV", "VVFIN", "ADV", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "das Lust-kind ruff ich an und kan es nicht mehr zeugen/", "tokens": ["das", "Lust\u00b7kind", "ruff", "ich", "an", "und", "kan", "es", "nicht", "mehr", "zeu\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "PTKVZ", "KON", "VMFIN", "PPER", "PTKNEG", "ADV", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "so wird es dennoch nicht mein letztes wort verschweigen;", "tokens": ["so", "wird", "es", "den\u00b7noch", "nicht", "mein", "letz\u00b7tes", "wort", "ver\u00b7schwei\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "PTKNEG", "PPOSAT", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Da\u00df ich war ohne trost und wachte gantz allein/", "tokens": ["Da\u00df", "ich", "war", "oh\u00b7ne", "trost", "und", "wach\u00b7te", "gantz", "al\u00b7lein", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VAFIN", "APPR", "NN", "KON", "VVFIN", "ADV", "ADV", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "und seuftzte fort f\u00fcr fort/ wenn andre schlieffen ein.", "tokens": ["und", "seuftz\u00b7te", "fort", "f\u00fcr", "fort", "/", "wenn", "and\u00b7re", "schlief\u00b7fen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PTKVZ", "APPR", "PTKVZ", "$(", "KOUS", "PIS", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}