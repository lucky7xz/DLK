{"dta.poem.10791": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "N.A.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198774", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "denn so nenn ich dich; weil in der\nJugend-Zeit,", "tokens": ["denn", "so", "nenn", "ich", "dich", ";", "weil", "in", "der", "Ju\u00b7gend\u00b7Zeit", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "$.", "KOUS", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.2": {"text": "Die Liebe uns verkn\u00fcpft, die jezt den Bund", "tokens": ["Die", "Lie\u00b7be", "uns", "ver\u00b7kn\u00fcpft", ",", "die", "jezt", "den", "Bund"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "VVPP", "$,", "PRELS", "ADV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Da uns die Vorsehung, so wunderbahr re-", "tokens": ["Da", "uns", "die", "Vor\u00b7se\u00b7hung", ",", "so", "wun\u00b7der\u00b7bahr", "re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "$,", "ADV", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "In eine Stadt vereint, so nah zusammen", "tokens": ["In", "ei\u00b7ne", "Stadt", "ver\u00b7eint", ",", "so", "nah", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "VVPP", "$,", "ADV", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Freund! denk mit mir zur\u00fck, wer h\u00e4tte\ndas gedacht,", "tokens": ["Freund", "!", "denk", "mit", "mir", "zu\u00b7r\u00fck", ",", "wer", "h\u00e4t\u00b7te", "das", "ge\u00b7dacht", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "VVFIN", "APPR", "PPER", "PTKVZ", "$,", "PWS", "VAFIN", "PDS", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Als in uns Lust und Flei\u00df, die Triebe ange-", "tokens": ["Als", "in", "uns", "Lust", "und", "Flei\u00df", ",", "die", "Trie\u00b7be", "an\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "APPR", "PPER", "NN", "KON", "NN", "$,", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Das Ziel, was wir erlangt, mit M\u00fche zu", "tokens": ["Das", "Ziel", ",", "was", "wir", "er\u00b7langt", ",", "mit", "M\u00fc\u00b7he", "zu"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "PRELS", "PPER", "VVPP", "$,", "APPR", "NN", "PTKZU"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Da\u00df uns der Himmel w\u00fcrd noch so zusam-", "tokens": ["Da\u00df", "uns", "der", "Him\u00b7mel", "w\u00fcrd", "noch", "so", "zu\u00b7sam"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VAFIN", "ADV", "ADV", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wir hoften damahls nicht, dies k\u00fcnfftige", "tokens": ["Wir", "hof\u00b7ten", "da\u00b7mahls", "nicht", ",", "dies", "k\u00fcnff\u00b7ti\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "PTKNEG", "$,", "PDS", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Es war was uns erg\u00f6zt, noch ein verborg-", "tokens": ["Es", "war", "was", "uns", "er\u00b7g\u00f6zt", ",", "noch", "ein", "ver\u00b7bor\u00b7g"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PWS", "PPER", "VVFIN", "$,", "ADV", "ART", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Die Fr\u00fcchte des Bem\u00fchn, die nunmehr", "tokens": ["Die", "Fr\u00fcch\u00b7te", "des", "Be\u00b7m\u00fchn", ",", "die", "nun\u00b7mehr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.12": {"text": "Die waren zu der Zeit, in Knospen noch ge-", "tokens": ["Die", "wa\u00b7ren", "zu", "der", "Zeit", ",", "in", "Knos\u00b7pen", "noch", "ge"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PDS", "VAFIN", "APPR", "ART", "NN", "$,", "APPR", "NN", "ADV", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.13": {"text": "Der Vorsicht Auge wacht, regieret wun-", "tokens": ["Der", "Vor\u00b7sicht", "Au\u00b7ge", "wacht", ",", "re\u00b7gie\u00b7ret", "wun"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "VVFIN", "$,", "VVFIN", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.14": {"text": "Das macht dein Beispiel hell, und mein", "tokens": ["Das", "macht", "dein", "Bei\u00b7spiel", "hell", ",", "und", "mein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["PDS", "VVFIN", "PPOSAT", "NN", "ADJD", "$,", "KON", "PPOSAT"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Und unser Lebenslauf zeigt, da\u00df sein Auge", "tokens": ["Und", "un\u00b7ser", "Le\u00b7bens\u00b7lauf", "zeigt", ",", "da\u00df", "sein", "Au\u00b7ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "$,", "KOUS", "PPOSAT", "NN"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.16": {"text": "Mit Gnadenblikken sieht auf die, so auf", "tokens": ["Mit", "Gna\u00b7den\u00b7blik\u00b7ken", "sieht", "auf", "die", ",", "so", "auf"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "NN", "VVFIN", "APPR", "ART", "$,", "ADV", "APPR"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.17": {"text": "Wir sehen beiderseits, was ihre G\u00fcte kan,", "tokens": ["Wir", "se\u00b7hen", "bei\u00b7der\u00b7seits", ",", "was", "ih\u00b7re", "G\u00fc\u00b7te", "kan", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "$,", "PRELS", "PPOSAT", "NN", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Was ihre Treue thut, in uns, als Spiegeln,", "tokens": ["Was", "ih\u00b7re", "Treu\u00b7e", "thut", ",", "in", "uns", ",", "als", "Spie\u00b7geln", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWS", "PPOSAT", "NN", "VVFIN", "$,", "APPR", "PPER", "$,", "KOUS", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wer ists der Dir und mir die Lust zum Wis-", "tokens": ["Wer", "ists", "der", "Dir", "und", "mir", "die", "Lust", "zum", "Wis"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "PPER", "KON", "PPER", "ART", "NN", "APPRART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.20": {"text": "Die Triebe eingepr\u00e4gt, die noch fest in uns", "tokens": ["Die", "Trie\u00b7be", "ein\u00b7ge\u00b7pr\u00e4gt", ",", "die", "noch", "fest", "in", "uns"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "ADJD", "APPR", "PPER"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Hat uns die Muttermilch dieselbigen ver-", "tokens": ["Hat", "uns", "die", "Mut\u00b7ter\u00b7milch", "die\u00b7sel\u00b7bi\u00b7gen", "ver"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ART", "NN", "PDAT", "TRUNC"], "meter": "-+-+-+-----", "measure": "unknown.measure.tri"}, "line.22": {"text": "Die Triebe angeflammt zum feurigen Be-", "tokens": ["Die", "Trie\u00b7be", "an\u00b7ge\u00b7flammt", "zum", "feu\u00b7ri\u00b7gen", "Be"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVPP", "APPRART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.23": {"text": "Mit solcher Kost ern\u00e4hrt, woraus ein Blut", "tokens": ["Mit", "sol\u00b7cher", "Kost", "er\u00b7n\u00e4hrt", ",", "wo\u00b7raus", "ein", "Blut"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIAT", "NN", "VVPP", "$,", "PWAV", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.24": {"text": "Das uns durch seine Kraft ger\u00fchrt, bewegt,", "tokens": ["Das", "uns", "durch", "sei\u00b7ne", "Kraft", "ge\u00b7r\u00fchrt", ",", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "PPOSAT", "NN", "VVPP", "$,", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.25": {"text": "Mit Eiffer nachzugehn der tief verdekten", "tokens": ["Mit", "Eif\u00b7fer", "nach\u00b7zu\u00b7gehn", "der", "tief", "ver\u00b7dek\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "NN", "APPR", "ART", "ADJD", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.26": {"text": "Wo ein nie m\u00fcder Fleis, die Perlen finden", "tokens": ["Wo", "ein", "nie", "m\u00fc\u00b7der", "Fleis", ",", "die", "Per\u00b7len", "fin\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "ART", "ADV", "ADJA", "NN", "$,", "ART", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.27": {"text": "Die Weisheits Sch\u00e4zze sind? O! Nein,", "tokens": ["Die", "Weis\u00b7heits", "Sch\u00e4z\u00b7ze", "sind", "?", "O", "!", "Nein", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "NN", "VAFIN", "$.", "NE", "$.", "PTKANT", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.28": {"text": "Hat sie uns eingedr\u00fckt, und bis daher er-", "tokens": ["Hat", "sie", "uns", "ein\u00b7ge\u00b7dr\u00fckt", ",", "und", "bis", "da\u00b7her", "er"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PPER", "VVPP", "$,", "KON", "ADV", "PAV", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.29": {"text": "Wir w\u00fcnschten beiderseits, auf Zions heilgen", "tokens": ["Wir", "w\u00fcnschten", "bei\u00b7der\u00b7seits", ",", "auf", "Zi\u00b7ons", "heil\u00b7gen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADV", "$,", "APPR", "NE", "VVINF"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.30": {"text": "Als ", "tokens": ["Als"], "token_info": ["word"], "pos": ["KOUS"], "meter": "+", "measure": "single.up"}, "line.31": {"text": "Der Wunsch ist uns gew\u00e4hrt; wir haben", "tokens": ["Der", "Wunsch", "ist", "uns", "ge\u00b7w\u00e4hrt", ";", "wir", "ha\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "VVPP", "$.", "PPER", "VAFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.32": {"text": "Wie ", "tokens": ["Wie"], "token_info": ["word"], "pos": ["PWAV"], "meter": "+", "measure": "single.up"}, "line.33": {"text": "Das was ich damahls schon, verwundrungs-", "tokens": ["Das", "was", "ich", "da\u00b7mahls", "schon", ",", "ver\u00b7wun\u00b7drungs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PDS", "PRELS", "PPER", "ADV", "ADV", "$,", "TRUNC"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.34": {"text": "Was ich von Dir gedacht, und was die Zeit", "tokens": ["Was", "ich", "von", "Dir", "ge\u00b7dacht", ",", "und", "was", "die", "Zeit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPR", "PPER", "VVPP", "$,", "KON", "PWS", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.35": {"text": "Was deine Vaterstadt durch ein erw\u00fcnsch-", "tokens": ["Was", "dei\u00b7ne", "Va\u00b7ter\u00b7stadt", "durch", "ein", "er\u00b7w\u00fcn\u00b7sch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWS", "PPOSAT", "NN", "APPR", "ART", "TRUNC"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.36": {"text": "Von Dir vorher gesehn, ist richtig eingetrof-", "tokens": ["Von", "Dir", "vor\u00b7her", "ge\u00b7sehn", ",", "ist", "rich\u00b7tig", "ein\u00b7ge\u00b7trof"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPER", "ADV", "VVPP", "$,", "VAFIN", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Der Tempel, da du bist den Christen beige-", "tokens": ["Der", "Tem\u00b7pel", ",", "da", "du", "bist", "den", "Chris\u00b7ten", "bei\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "VAFIN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.38": {"text": "Durchs Wasserbad im Wort, hat dich auch", "tokens": ["Durchs", "Was\u00b7ser\u00b7bad", "im", "Wort", ",", "hat", "dich", "auch"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPRART", "NN", "$,", "VAFIN", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.39": {"text": "Der Schautisch, da du dich dem Heiland", "tokens": ["Der", "Schau\u00b7tisch", ",", "da", "du", "dich", "dem", "Hei\u00b7land"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KOUS", "PPER", "PRF", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.40": {"text": "Ist Dir selbst anvertraut, wo deine Hand", "tokens": ["Ist", "Dir", "selbst", "an\u00b7ver\u00b7traut", ",", "wo", "dei\u00b7ne", "Hand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "VVPP", "$,", "PWAV", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.41": {"text": "Die Mittel dadurch der, der unsre Seele", "tokens": ["Die", "Mit\u00b7tel", "da\u00b7durch", "der", ",", "der", "uns\u00b7re", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "PAV", "ART", "$,", "PRELS", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.42": {"text": "Sich Seelen ganz und gar zu eigen \u00fcber-", "tokens": ["Sich", "See\u00b7len", "ganz", "und", "gar", "zu", "ei\u00b7gen", "\u00fc\u00b7ber"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PRF", "NN", "ADV", "KON", "ADV", "PTKA", "ADJD", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.43": {"text": "Die Glieder der Gemein, worin du bist ge-", "tokens": ["Die", "Glie\u00b7der", "der", "Ge\u00b7mein", ",", "wo\u00b7rin", "du", "bist", "ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "NN", "$,", "PWAV", "PPER", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.44": {"text": "Die haben dich mit Lust, zum Hirten auser-", "tokens": ["Die", "ha\u00b7ben", "dich", "mit", "Lust", ",", "zum", "Hir\u00b7ten", "aus\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VAFIN", "PPER", "APPR", "NN", "$,", "APPRART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.45": {"text": "Du weidest sie getreu; es schallt dein s\u00fcsser", "tokens": ["Du", "wei\u00b7dest", "sie", "ge\u00b7treu", ";", "es", "schallt", "dein", "s\u00fcs\u00b7ser"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$.", "PPER", "VVFIN", "PPOSAT", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.46": {"text": "Und deine W\u00e4chterstimm macht ihnen deut-", "tokens": ["Und", "dei\u00b7ne", "W\u00e4ch\u00b7ter\u00b7stimm", "macht", "ih\u00b7nen", "deut"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "PPER", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.47": {"text": "Wie man sich h\u00fcten sol vor derer Feinde", "tokens": ["Wie", "man", "sich", "h\u00fc\u00b7ten", "sol", "vor", "de\u00b7rer", "Fein\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PIS", "PRF", "VVINF", "VMFIN", "APPR", "PDS", "NN"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.48": {"text": "Die schleichend sich bem\u00fchn, die Seelen zu be-", "tokens": ["Die", "schlei\u00b7chend", "sich", "be\u00b7m\u00fchn", ",", "die", "See\u00b7len", "zu", "be"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADJD", "PRF", "VVINF", "$,", "ART", "NN", "APPR", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.49": {"text": "Dein Herz ist Tag und Nacht, mit Sorgfalt", "