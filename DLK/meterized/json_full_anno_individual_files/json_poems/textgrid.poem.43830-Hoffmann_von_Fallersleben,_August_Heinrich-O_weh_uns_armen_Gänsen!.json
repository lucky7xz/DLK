{"textgrid.poem.43830": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "O weh uns armen G\u00e4nsen!", "genre": "verse", "period": "N.A.", "pub_year": 1836, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wann wird es, wann wird es doch Fr\u00fchling einmal?", "tokens": ["Wann", "wird", "es", ",", "wann", "wird", "es", "doch", "Fr\u00fch\u00b7ling", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PWAV", "VAFIN", "PPER", "ADV", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wann rauschet das B\u00e4chlein doch wieder in's Thal?", "tokens": ["Wann", "rau\u00b7schet", "das", "B\u00e4ch\u00b7lein", "doch", "wie\u00b7der", "in's", "Thal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Wann k\u00f6nnen wir G\u00e4nse spazieren im Frei'n,", "tokens": ["Wann", "k\u00f6n\u00b7nen", "wir", "G\u00e4n\u00b7se", "spa\u00b7zie\u00b7ren", "im", "Frei'n", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "NN", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Im Wasser uns baden beim Sonnenschein?", "tokens": ["Im", "Was\u00b7ser", "uns", "ba\u00b7den", "beim", "Son\u00b7nen\u00b7schein", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPER", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Erst gestern ist uns eine Wake gemacht,", "tokens": ["Erst", "ge\u00b7stern", "ist", "uns", "ei\u00b7ne", "Wa\u00b7ke", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$,"], "meter": "+-+--+-+--+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Doch fror sie uns leider schon zu \u00fcber Nacht.", "tokens": ["Doch", "fror", "sie", "uns", "lei\u00b7der", "schon", "zu", "\u00fc\u00b7ber", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "APPR", "NN", "$."], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Nun sitzen wir traurig am Ufer und schau'n", "tokens": ["Nun", "sit\u00b7zen", "wir", "trau\u00b7rig", "am", "U\u00b7fer", "und", "schau'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "APPRART", "NN", "KON", "VVINF"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "Gen Himmel und fragen: wann wird es doch thau'n?", "tokens": ["Gen", "Him\u00b7mel", "und", "fra\u00b7gen", ":", "wann", "wird", "es", "doch", "thau'n", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "KON", "VVINF", "$.", "PWAV", "VAFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn's lange noch bleibet so eisig und kalt,", "tokens": ["Wenn's", "lan\u00b7ge", "noch", "blei\u00b7bet", "so", "ei\u00b7sig", "und", "kalt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "ADV", "VVFIN", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Verlernen wir G\u00e4nse das Schwimmen auch bald.", "tokens": ["Ver\u00b7ler\u00b7nen", "wir", "G\u00e4n\u00b7se", "das", "Schwim\u00b7men", "auch", "bald", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "ART", "NN", "ADV", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.3": {"text": "Stellt drum sich der Fr\u00fchling nicht ehestens ein,", "tokens": ["Stellt", "drum", "sich", "der", "Fr\u00fch\u00b7ling", "nicht", "e\u00b7hes\u00b7tens", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PAV", "PRF", "ART", "NN", "PTKNEG", "ADV", "PTKVZ", "$,"], "meter": "+-+-+-+-+-+", "measure": "trochaic.hexa"}, "line.4": {"text": "So lohnt es sich kaum, ein Gans noch zu sein.", "tokens": ["So", "lohnt", "es", "sich", "kaum", ",", "ein", "Gans", "noch", "zu", "sein", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PRF", "ADV", "$,", "ART", "NN", "ADV", "PTKZU", "VAINF", "$."], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}}, "stanza.4": {"line.1": {"text": "Wann wird es, wann wird es doch Fr\u00fchling einmal?", "tokens": ["Wann", "wird", "es", ",", "wann", "wird", "es", "doch", "Fr\u00fch\u00b7ling", "ein\u00b7mal", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "$,", "PWAV", "VAFIN", "PPER", "ADV", "NN", "ADV", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.2": {"text": "Wann rauschet das B\u00e4chlein doch wieder in's Thal?", "tokens": ["Wann", "rau\u00b7schet", "das", "B\u00e4ch\u00b7lein", "doch", "wie\u00b7der", "in's", "Thal", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VVFIN", "ART", "NN", "ADV", "ADV", "APPRART", "NN", "$."], "meter": "-+--+--