{"textgrid.poem.35902": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: O, wie bitter ist das Wandern,", "genre": "verse", "period": "N.A.", "pub_year": 1856, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O, wie bitter ist das Wandern,", "tokens": ["O", ",", "wie", "bit\u00b7ter", "ist", "das", "Wan\u00b7dern", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ADJD", "VAFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wenn die Seele r\u00fcckw\u00e4rts zieht,", "tokens": ["Wenn", "die", "See\u00b7le", "r\u00fcck\u00b7w\u00e4rts", "zieht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein liebes Auge lange", "tokens": ["Und", "ein", "lie\u00b7bes", "Au\u00b7ge", "lan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "ADJA", "NN", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weinend noch her\u00fcber sieht!", "tokens": ["Wei\u00b7nend", "noch", "her\u00b7\u00fc\u00b7ber", "sieht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "ADV", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Und ein T\u00fcchlein flattert ferne,", "tokens": ["Und", "ein", "T\u00fcch\u00b7lein", "flat\u00b7tert", "fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Bis dich birgt des Waldes Saum;", "tokens": ["Bis", "dich", "birgt", "des", "Wal\u00b7des", "Saum", ";"], "token_info": ["word", "word", "w