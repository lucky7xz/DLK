{"dta.poem.8133": {"metadata": {"author": {"name": "Klopstock, Friedrich Gottlieb", "birth": "N.A.", "death": "N.A."}, "title": "10.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1773", "urn": "urn:nbn:de:kobv:b4-200905199504", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Am Thron rollt die Heerschaar, als g\u00f6\u00df sie ein Meer weit aus,", "tokens": ["Am", "Thron", "rollt", "die", "Heer\u00b7schaar", ",", "als", "g\u00f6\u00df", "sie", "ein", "Meer", "weit", "aus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN", "$,", "KOUS", "NN", "PPER", "ART", "NN", "ADJD", "PTKVZ", "$,"], "meter": "-+--+--+--+-+", "measure": "amphibrach.tetra.plus"}, "line.2": {"text": "Des Gerichts B\u00fccher voll Ernst auf; und die Glanzschrift er-", "tokens": ["Des", "Ge\u00b7richts", "B\u00fc\u00b7cher", "voll", "Ernst", "auf", ";", "und", "die", "Glanz\u00b7schrift", "er"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "ADJD", "NN", "PTKVZ", "$.", "KON", "ART", "NN", "TRUNC"], "meter": "---+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "schreckt fernher!", "tokens": ["schreckt", "fern\u00b7her", "!"], "token_info": ["word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "$."], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Eilet empor, Erstlinge, schwebt den Triumphflug, kommt,", "tokens": ["Ei\u00b7let", "em\u00b7por", ",", "Erst\u00b7lin\u00b7ge", ",", "schwebt", "den", "Tri\u00b7umph\u00b7flug", ",", "kommt", ","], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "$,", "VVFIN", "ART", "NN", "$,", "VVFIN", "$,"], "meter": "+--+-+-+-+--+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Richtet mit dem, welchem sich die H\u00f6h, und das Gebeinthal b\u00fcckt!", "tokens": ["Rich\u00b7tet", "mit", "dem", ",", "wel\u00b7chem", "sich", "die", "H\u00f6h", ",", "und", "das", "Ge\u00b7beint\u00b7hal", "b\u00fcckt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "$,", "PRELS", "PRF", "ART", "NN", "$,", "KON", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}