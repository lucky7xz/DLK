{"dta.poem.19240": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "87.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195090", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Die Bienen wollen auch wie gute Nachbarsleute", "tokens": ["Die", "Bie\u00b7nen", "wol\u00b7len", "auch", "wie", "gu\u00b7te", "Nach\u00b7bars\u00b7leu\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "ADV", "KOKOM", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Behandelt seyn, um gern mit dir zu theilen Beute.", "tokens": ["Be\u00b7han\u00b7delt", "seyn", ",", "um", "gern", "mit", "dir", "zu", "thei\u00b7len", "Beu\u00b7te", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "VAINF", "$,", "KOUI", "ADV", "APPR", "PPER", "PTKZU", "VVINF", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Wenn jemand stirbt im Haus, must du's nicht nur ansagen", "tokens": ["Wenn", "je\u00b7mand", "stirbt", "im", "Haus", ",", "must", "du's", "nicht", "nur", "an\u00b7sa\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "VVFIN", "APPRART", "NN", "$,", "VMFIN", "PIS", "PTKNEG", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Nachbarn, sondern auch dem Bienenstocke klagen.", "tokens": ["Den", "Nach\u00b7barn", ",", "son\u00b7dern", "auch", "dem", "Bie\u00b7nen\u00b7sto\u00b7cke", "kla\u00b7gen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KON", "ADV", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Begr\u00fc\u00dfen must du ihn mit einem frommen Spruch,", "tokens": ["Be\u00b7gr\u00fc\u00b7\u00dfen", "must", "du", "ihn", "mit", "ei\u00b7nem", "from\u00b7men", "Spruch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "PPER", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und breiten dr\u00fcberhin zugleich ein Trauertuch;", "tokens": ["Und", "brei\u00b7ten", "dr\u00fc\u00b7berh\u00b7in", "zu\u00b7gleich", "ein", "Trau\u00b7er\u00b7tuch", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Damit sie merken, da\u00df nicht ihnen Heg und Pfleg", "tokens": ["Da\u00b7mit", "sie", "mer\u00b7ken", ",", "da\u00df", "nicht", "ih\u00b7nen", "Heg", "und", "Pfleg"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "VVINF", "$,", "KOUS", "PTKNEG", "PPER", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Entzogen sei; wonicht, so fliegen sie dir weg. \u2014", "tokens": ["Ent\u00b7zo\u00b7gen", "sei", ";", "wo\u00b7nicht", ",", "so", "flie\u00b7gen", "sie", "dir", "weg", "."], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NN", "VAFIN", "$.", "ADJD", "$,", "ADV", "VVFIN", "PPER", "PPER", "PTKVZ", "$.", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Du sprichst: Und lehrest du mich solchen Aberglauben?", "tokens": ["Du", "sprichst", ":", "Und", "leh\u00b7rest", "du", "mich", "sol\u00b7chen", "A\u00b7berg\u00b7lau\u00b7ben", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$.", "KON", "VVFIN", "PPER", "PRF", "PIAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das nicht; doch will ich dir unschuldigen erlauben.", "tokens": ["Das", "nicht", ";", "doch", "will", "ich", "dir", "un\u00b7schul\u00b7di\u00b7gen", "er\u00b7lau\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PTKNEG", "$.", "ADV", "VMFIN", "PPER", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+--+-++-+-", "measure": "iambic.hexa.relaxed"}}}}}