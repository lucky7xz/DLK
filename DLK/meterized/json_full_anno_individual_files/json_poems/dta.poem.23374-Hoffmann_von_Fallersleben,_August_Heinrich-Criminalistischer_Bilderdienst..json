{"dta.poem.23374": {"metadata": {"author": {"name": "Hoffmann von Fallersleben, August Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Criminalistischer Bilderdienst.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192634", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Nicht an Hellas d\u00fcrft ihr denken,", "tokens": ["Nicht", "an", "Hel\u00b7las", "d\u00fcrft", "ihr", "den\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sucht ihr, K\u00fcnstler, Ruhm und Preis:", "tokens": ["Sucht", "ihr", ",", "K\u00fcnst\u00b7ler", ",", "Ruhm", "und", "Preis", ":"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Auch bei uns in Flachsenfingen", "tokens": ["Auch", "bei", "uns", "in", "Flach\u00b7sen\u00b7fin\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "APPR", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sprie\u00dft der Kunst ein Lorbeerreis.", "tokens": ["Sprie\u00dft", "der", "Kunst", "ein", "Lor\u00b7beer\u00b7reis", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Seht! vor eurem schlechtsten Machwerk", "tokens": ["Seht", "!", "vor", "eu\u00b7rem", "schlechts\u00b7ten", "Mach\u00b7werk"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "$.", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "M\u00fcss