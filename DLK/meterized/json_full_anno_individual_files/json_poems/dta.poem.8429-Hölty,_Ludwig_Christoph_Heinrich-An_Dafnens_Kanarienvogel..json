{"dta.poem.8429": {"metadata": {"author": {"name": "H\u00f6lty, Ludwig Christoph Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "An Dafnens Kanarienvogel.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1783", "urn": "urn:nbn:de:kobv:b4-200905192568", "language": ["de:0.99"], "booktitle": "H\u00f6lty, Ludwig Christoph Heinrich: Gedichte. Hamburg, 1783."}, "poem": {"stanza.1": {"line.1": {"text": "Dein gesunkenes K\u00f6pflein unterm Fittig;", "tokens": ["Dein", "ge\u00b7sun\u00b7ke\u00b7nes", "K\u00f6\u00b7pflein", "un\u00b7term", "Fit\u00b7tig", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPRART", "NN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Tr\u00e4umst Ges\u00e4nge des Tages, pickst aus Dafnens", "tokens": ["Tr\u00e4umst", "Ge\u00b7s\u00e4n\u00b7ge", "des", "Ta\u00b7ges", ",", "pickst", "aus", "Daf\u00b7nens"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NN", "NN", "ART", "NN", "$,", "VVFIN", "APPR", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Sch\u00f6nen H\u00e4nden ein St\u00fccklein Zucker, oder", "tokens": ["Sch\u00f6\u00b7nen", "H\u00e4n\u00b7den", "ein", "St\u00fcck\u00b7lein", "Zu\u00b7cker", ",", "o\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADJA", "NN", "ART", "NN", "NN", "$,", "KON"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Was vor herliche Tr\u00e4ume dich umgaukeln!", "tokens": ["Was", "vor", "her\u00b7li\u00b7che", "Tr\u00e4u\u00b7me", "dich", "um\u00b7gau\u00b7keln", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "APPR", "ADJA", "NN", "PPER", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.5": {"text": "Neidenswehrter, ach! zehnmal neidenswehrter", "tokens": ["Nei\u00b7dens\u00b7wehr\u00b7ter", ",", "ach", "!", "zehn\u00b7mal", "nei\u00b7dens\u00b7wehr\u00b7ter"], "token_info": ["word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "$,", "ITJ", "$.", "ADV", "ADJA"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.6": {"text": "Ist, o Vogel, dein Schicksal, als das meine!", "tokens": ["Ist", ",", "o", "Vo\u00b7gel", ",", "dein", "Schick\u00b7sal", ",", "als", "das", "mei\u00b7ne", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "$,", "FM", "NE", "$,", "PPOSAT", "NN", "$,", "KOUS", "PDS", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Nie umflattert des Schlummers Rosensittig", "tokens": ["Nie", "um\u00b7flat\u00b7tert", "des", "Schlum\u00b7mers", "Ro\u00b7sen\u00b7sit\u00b7tig"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "NE"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.8": {"text": "Diese weinenden Augen! Dafne klopfet", "tokens": ["Die\u00b7se", "wei\u00b7nen\u00b7den", "Au\u00b7gen", "!", "Daf\u00b7ne", "klop\u00b7fet"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PDAT", "ADJA", "NN", "$.", "NE", "VVFIN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Mir in jeglichem Puls; und fern ist Dafne!", "tokens": ["Mir", "in", "jeg\u00b7li\u00b7chem", "Puls", ";", "und", "fern", "ist", "Daf\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "PIAT", "NN", "$.", "KON", "ADJD", "VAFIN", "NE", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.10": {"text": "O verwandelten mich die guten G\u00f6tter", "tokens": ["O", "ver\u00b7wan\u00b7del\u00b7ten", "mich", "die", "gu\u00b7ten", "G\u00f6t\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPER", "ART", "ADJA", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.11": {"text": "In dies V\u00f6gelein! O wie wollt' ich zwitschernd", "tokens": ["In", "dies", "V\u00f6\u00b7ge\u00b7lein", "!", "O", "wie", "wollt'", "ich", "zwit\u00b7schernd"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PDS", "NN", "$.", "NE", "PWAV", "VMFIN", "PPER", "ADJD"], "meter": "+-+-+-++--+", "measure": "iambic.hexa.chol"}, "line.12": {"text": "Dafnens wallender Brust entgegenflattern,", "tokens": ["Daf\u00b7nens", "wal\u00b7len\u00b7der", "Brust", "ent\u00b7ge\u00b7gen\u00b7flat\u00b7tern", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.13": {"text": "Auf dem Strausse mich wiegen, und vom Kranze", "tokens": ["Auf", "dem", "Straus\u00b7se", "mich", "wie\u00b7gen", ",", "und", "vom", "Kran\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "PPER", "VVFIN", "$,", "KON", "APPRART", "NN"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.14": {"text": "Ihrer Locken ein Minneliedchen fl\u00f6ten!", "tokens": ["Ih\u00b7rer", "Lo\u00b7cken", "ein", "Min\u00b7ne\u00b7lied\u00b7chen", "fl\u00f6\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ART", "NN", "VVFIN", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.15": {"text": "In die Saiten des Fl\u00fcgels wollt' ich girren,", "tokens": ["In", "die", "Sai\u00b7ten", "des", "Fl\u00fc\u00b7gels", "wollt'", "ich", "gir\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.16": {"text": "Wann ihr fliegender kleiner Finger spielte,", "tokens": ["Wann", "ihr", "flie\u00b7gen\u00b7der", "klei\u00b7ner", "Fin\u00b7ger", "spiel\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADJA", "ADJA", "NN", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.17": {"text": "Bis ihr M\u00fcndlein mit einem Kuss mir dankte!", "tokens": ["Bis", "ihr", "M\u00fcnd\u00b7lein", "mit", "ei\u00b7nem", "Kuss", "mir", "dank\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-++--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.18": {"text": "Dann, dann w\u00fcrd' ich mit keinem Sultan tauschen,", "tokens": ["Dann", ",", "dann", "w\u00fcrd'", "ich", "mit", "kei\u00b7nem", "Sul\u00b7tan", "tau\u00b7schen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "ADV", "VAFIN", "PPER", "APPR", "PIAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Wenn auch hundert der sch\u00f6nsten Landesjungfraun", "tokens": ["Wenn", "auch", "hun\u00b7dert", "der", "sch\u00f6ns\u00b7ten", "Lan\u00b7des\u00b7jung\u00b7fraun"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "CARD", "ART", "ADJA", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.20": {"text": "Um die Ehre des seidnen Schnupftuchs buhlten!", "tokens": ["Um", "die", "Eh\u00b7re", "des", "seid\u00b7nen", "Schnupf\u00b7tuchs", "buhl\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.21": {"text": "Traun, dann w\u00fcrden die G\u00f6tter samt und sonders", "tokens": ["Traun", ",", "dann", "w\u00fcr\u00b7den", "die", "G\u00f6t\u00b7ter", "samt", "und", "son\u00b7ders"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "ADV", "VAFIN", "ART", "NN", "ADJD", "KON", "XY"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.22": {"text": "Mich im hohen Olimp ein wenig neiden!", "tokens": ["Mich", "im", "ho\u00b7hen", "O\u00b7limp", "ein", "we\u00b7nig", "nei\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPRART", "ADJA", "NN", "ART", "PIS", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}}}}}