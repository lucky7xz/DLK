{"dta.poem.14419": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Jugendandacht .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.71", "af:0.28"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Was wollen mir vertrau'n die blauen Weiten,", "tokens": ["Was", "wol\u00b7len", "mir", "ver\u00b7trau'n", "die", "blau\u00b7en", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Des Landes Glanz, die Wirrung s\u00fc\u00dfer Lieder,", "tokens": ["Des", "Lan\u00b7des", "Glanz", ",", "die", "Wir\u00b7rung", "s\u00fc\u00b7\u00dfer", "Lie\u00b7der", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Mir ist so wohl, so bang! Seid ihr es wieder", "tokens": ["Mir", "ist", "so", "wohl", ",", "so", "bang", "!", "Seid", "ihr", "es", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ADV", "$,", "ADV", "ADJD", "$.", "VAIMP", "PPER", "PPER", "ADV"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Der frommen Kindheit stille Blumenzeiten?", "tokens": ["Der", "from\u00b7men", "Kind\u00b7heit", "stil\u00b7le", "Blu\u00b7men\u00b7zei\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}