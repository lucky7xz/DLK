{"dta.poem.1113": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "1. Das sch\u00f6ne Kind.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Ihr Nimphen/ (und die ihr das Frauenzimmer ehrt) ", "tokens": ["Ihr", "Nim\u00b7phen", "/", "(", "und", "die", "ihr", "das", "Frau\u00b7en\u00b7zim\u00b7mer", "ehrt", ")"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "$(", "KON", "PRELS", "PPER", "ART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Last ein geneigtes Aug auff di\u00df Gedichte fliessen/", "tokens": ["Last", "ein", "ge\u00b7neig\u00b7tes", "Aug", "auff", "di\u00df", "Ge\u00b7dich\u00b7te", "flies\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "APPR", "PDS", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das euer hohes Lob auff neue Weise mehrt.", "tokens": ["Das", "eu\u00b7er", "ho\u00b7hes", "Lob", "auff", "neu\u00b7e", "Wei\u00b7se", "mehrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Schaut einen Blumen-M\u00e4y im fr\u00fchsten Lentz entsprissen.", "tokens": ["Schaut", "ei\u00b7nen", "Blu\u00b7men\u00b7M\u00e4y", "im", "fr\u00fchs\u00b7ten", "Lentz", "ent\u00b7spris\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPRART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da kaum die Morgenr\u00f6th uns zeigt der Sonnen Licht/", "tokens": ["Da", "kaum", "die", "Mor\u00b7gen\u00b7r\u00f6th", "uns", "zeigt", "der", "Son\u00b7nen", "Licht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "ART", "NN", "PPER", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wir schon ihre Glutt eh f\u00fchln als kennen m\u00fcssen.", "tokens": ["Und", "wir", "schon", "ih\u00b7re", "Glutt", "eh", "f\u00fchln", "als", "ken\u00b7nen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADV", "PPOSAT", "NN", "KOUS", "VVINF", "KOKOM", "VVINF", "VMINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Es schw\u00e4cht der kleine Leib den Ruff der Sch\u00f6nheit nicht:", "tokens": ["Es", "schw\u00e4cht", "der", "klei\u00b7ne", "Leib", "den", "Ruff", "der", "Sch\u00f6n\u00b7heit", "nicht", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "ART", "NN", "ART", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Den Demant seht ihr ja f\u00fcr grossen Steinen prangen", "tokens": ["Den", "De\u00b7mant", "seht", "ihr", "ja", "f\u00fcr", "gros\u00b7sen", "Stei\u00b7nen", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "PPER", "ADV", "APPR", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und wie die zarte Perl aus hellem Gold absticht.", "tokens": ["Und", "wie", "die", "zar\u00b7te", "Perl", "aus", "hel\u00b7lem", "Gold", "ab\u00b7sticht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Scheut ihr den Blatter-Schmuck der \u00fcberstickten Wangen/", "tokens": ["Scheut", "ihr", "den", "Blat\u00b7ter\u00b7Schmuck", "der", "\u00fc\u00b7bers\u00b7tick\u00b7ten", "Wan\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die Hitz und Feuchte kocht? So pflegt ein neues Kleid", "tokens": ["Die", "Hitz", "und", "Feuch\u00b7te", "kocht", "?", "So", "pflegt", "ein", "neu\u00b7es", "Kleid"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "$.", "ADV", "VVFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Durch hei\u00dfe Sonn und Bad der Adler zu erlangen.", "tokens": ["Durch", "hei\u00b7\u00dfe", "Sonn", "und", "Bad", "der", "Ad\u00b7ler", "zu", "er\u00b7lan\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "NE", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Klagt ihr das liebe Kind/ dem eine Last bereit", "tokens": ["Klagt", "ihr", "das", "lie\u00b7be", "Kind", "/", "dem", "ei\u00b7ne", "Last", "be\u00b7reit"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "$(", "ART", "ART", "NN", "ADJD"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Von seiner Buhler Zahl die m\u00fcden Schultern dr\u00fccket?", "tokens": ["Von", "sei\u00b7ner", "Buh\u00b7ler", "Zahl", "die", "m\u00fc\u00b7den", "Schul\u00b7tern", "dr\u00fc\u00b7cket", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Ach/ nennet sie vielmehr den Atlas dieser Zeit.", "tokens": ["Ach", "/", "nen\u00b7net", "sie", "viel\u00b7mehr", "den", "At\u00b7las", "die\u00b7ser", "Zeit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$(", "VVFIN", "PPER", "ADV", "ART", "NN", "PDAT", "NN", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}}, "stanza.6": {"line.1": {"text": "Die/ deren lahmer Fu\u00df sich nicht zum Lauffen schicket/", "tokens": ["Die", "/", "de\u00b7ren", "lah\u00b7mer", "Fu\u00df", "sich", "nicht", "zum", "Lauf\u00b7fen", "schi\u00b7cket", "/"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "$(", "PRELAT", "ADJA", "NN", "PRF", "PTKNEG", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird billig auch gelobt/ weil sie nicht so mit Eil", "tokens": ["Wird", "bil\u00b7lig", "auch", "ge\u00b7lobt", "/", "weil", "sie", "nicht", "so", "mit", "Eil"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "ADJD", "ADV", "VVPP", "$(", "KOUS", "PPER", "PTKNEG", "ADV", "APPR", "NN"], "meter": "-+-+-+--++-+", "measure": "iambic.hexa.relaxed"}, "line.3": {"text": "Ihr angenehmes Bild von unserm Aug entr\u00fccket.", "tokens": ["Ihr", "an\u00b7ge\u00b7neh\u00b7mes", "Bild", "von", "un\u00b7serm", "Aug", "ent\u00b7r\u00fc\u00b7cket", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Hat Flei\u00df und Zierligkeit bey Jener keinen Theil/", "tokens": ["Hat", "Flei\u00df", "und", "Zier\u00b7lig\u00b7keit", "bey", "Je\u00b7ner", "kei\u00b7nen", "Theil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "NN", "KON", "NN", "APPR", "PDAT", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Verwendet sie auff Putz und Ordnung wenig Sorgen/", "tokens": ["Ver\u00b7wen\u00b7det", "sie", "auff", "Putz", "und", "Ord\u00b7nung", "we\u00b7nig", "Sor\u00b7gen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "NN", "KON", "NN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sie beut/ was die Natur geschm\u00fcckt/ mit Kunst nicht feil.", "tokens": ["Sie", "beut", "/", "was", "die", "Na\u00b7tur", "ge\u00b7schm\u00fcckt", "/", "mit", "Kunst", "nicht", "feil", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "ART", "NN", "VVPP", "$(", "APPR", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "Ein gelbes Wange will der Sonnen Farbe borgen:", "tokens": ["Ein", "gel\u00b7bes", "Wan\u00b7ge", "will", "der", "Son\u00b7nen", "Far\u00b7be", "bor\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Manch Brunnenquell mu\u00df auch zur Zeit verstopffet seyn;", "tokens": ["Manch", "Brun\u00b7nen\u00b7quell", "mu\u00df", "auch", "zur", "Zeit", "ver\u00b7stopf\u00b7fet", "seyn", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VMFIN", "ADV", "APPRART", "NN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Man holet Ros\u2019 und Gold/ so gelbe/ weit von Morgen.", "tokens": ["Man", "ho\u00b7let", "Ros'", "und", "Gold", "/", "so", "gel\u00b7be", "/", "weit", "von", "Mor\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "NE", "KON", "NN", "$(", "ADV", "ADJA", "$(", "ADJD", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.9": {"line.1": {"text": "(ein rothes Auge will des Feuers Farbe borgen/", "tokens": ["(", "ein", "ro\u00b7thes", "Au\u00b7ge", "will", "des", "Feu\u00b7ers", "Far\u00b7be", "bor\u00b7gen", "/"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "ADJA", "NN", "VMFIN", "ART", "NN", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Durch neuer Flammen Glutt der M\u00e4nner Hencker seyn:", "tokens": ["Durch", "neu\u00b7er", "Flam\u00b7men", "Glutt", "der", "M\u00e4n\u00b7ner", "Hen\u00b7cker", "seyn", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "ART", "NN", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Matuta zeigt sich so in k\u00fchl-bethautem Morgen.)", "tokens": ["Ma\u00b7tu\u00b7ta", "zeigt", "sich", "so", "in", "k\u00fchl\u00b7be\u00b7thau\u00b7tem", "Mor\u00b7gen", ".", ")"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Die Nimphe/ die mit Flei\u00df zertheilt der Augen Schein/", "tokens": ["Die", "Nim\u00b7phe", "/", "die", "mit", "Flei\u00df", "zer\u00b7theilt", "der", "Au\u00b7gen", "Schein", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ART", "APPR", "NN", "VVFIN", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wei\u00df mit mehr Kunst und Gunst die Hertzen zu entz\u00fccken/", "tokens": ["Wei\u00df", "mit", "mehr", "Kunst", "und", "Gunst", "die", "Hert\u00b7zen", "zu", "ent\u00b7z\u00fc\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PIAT", "NN", "KON", "NN", "ART", "NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als wenn sie auff einmahl uns wolten \u00e4schern ein.", "tokens": ["Als", "wenn", "sie", "auff", "ein\u00b7mahl", "uns", "wol\u00b7ten", "\u00e4sc\u00b7hern", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "KOUS", "PPER", "APPR", "ADV", "PPER", "VMFIN", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}