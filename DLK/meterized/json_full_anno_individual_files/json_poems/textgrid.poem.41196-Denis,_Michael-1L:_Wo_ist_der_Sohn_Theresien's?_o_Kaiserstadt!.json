{"textgrid.poem.41196": {"metadata": {"author": {"name": "Denis, Michael", "birth": "N.A.", "death": "N.A."}, "title": "1L: Wo ist der Sohn Theresien's? o Kaiserstadt!", "genre": "verse", "period": "N.A.", "pub_year": 1764, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wo ist der Sohn Theresien's? o Kaiserstadt!", "tokens": ["Wo", "ist", "der", "Sohn", "The\u00b7re\u00b7si\u00b7en's", "?", "o", "Kai\u00b7ser\u00b7stadt", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "ART", "NN", "NE", "$.", "FM", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wo ist dein Herrscher? W\u00f6lke dein th\u00fcrmend Haupt,", "tokens": ["Wo", "ist", "dein", "Herr\u00b7scher", "?", "W\u00f6l\u00b7ke", "dein", "th\u00fcr\u00b7mend", "Haupt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPOSAT", "NN", "$.", "NN", "PPOSAT", "ADJD", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Aus deinen blauen D\u00fcften h\u00f6re,", "tokens": ["Aus", "dei\u00b7nen", "blau\u00b7en", "D\u00fcf\u00b7ten", "h\u00f6\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Was dir vom heiligen Eichenhaine", "tokens": ["Was", "dir", "vom", "hei\u00b7li\u00b7gen", "Ei\u00b7chen\u00b7hai\u00b7ne"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PWS", "PPER", "APPRART", "ADJA", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.2": {"line.1": {"text": "Der Barde Joseph's (Wag' ich den herrlichsten", "tokens": ["Der", "Bar\u00b7de", "Jo\u00b7se\u00b7ph's", "(", "Wag'", "ich", "den", "herr\u00b7lichs\u00b7ten"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$(", "VVIMP", "PPER", "ART", "ADJA"], "meter": "-+-+--+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der Namen unter Barden? Gef\u00e4hrlich ist", "tokens": ["Der", "Na\u00b7men", "un\u00b7ter", "Bar\u00b7den", "?", "Ge\u00b7f\u00e4hr\u00b7lich", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "APPR", "NN", "$.", "ADJD", "VAFIN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Der Reiz dem Einzigen zu folgen!", "tokens": ["Der", "Reiz", "dem", "Ein\u00b7zi\u00b7gen", "zu", "fol\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Aber zu m\u00e4chtig! \u2013 Er sei gewaget!)", "tokens": ["A\u00b7ber", "zu", "m\u00e4ch\u00b7tig", "!", "\u2013", "Er", "sei", "ge\u00b7wa\u00b7get", "!", ")"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["KON", "PTKA", "ADJD", "$.", "$(", "PPER", "VAFIN", "VVPP", "$.", "$("], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.3": {"line.1": {"text": "Der Barde Joseph's t\u00f6net: Hier oben ist", "tokens": ["Der", "Bar\u00b7de", "Jo\u00b7se\u00b7ph's", "t\u00f6\u00b7net", ":", "Hier", "o\u00b7ben", "ist"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "VVFIN", "$.", "ADV", "ADV", "VAFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Der Thaten Joseph's un\u00fcbersehliche,", "tokens": ["Der", "Tha\u00b7ten", "Jo\u00b7se\u00b7ph's", "un\u00b7\u00fc\u00b7ber\u00b7seh\u00b7li\u00b7che", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "ADJA", "$,"], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Wie Sonnen, helle Bahn gezeichnet.", "tokens": ["Wie", "Son\u00b7nen", ",", "hel\u00b7le", "Bahn", "ge\u00b7zeich\u00b7net", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "$,", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fr\u00fche begann er die Bahn zu wandeln,", "tokens": ["Fr\u00fc\u00b7he", "be\u00b7gann", "er", "die", "Bahn", "zu", "wan\u00b7deln", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKZU", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.4": {"line.1": {"text": "Je That auf That erhab'ner. Italien", "tokens": ["Je", "That", "auf", "That", "er\u00b7hab'\u00b7ner", ".", "I\u00b7ta\u00b7li\u00b7en"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["ADV", "NN", "APPR", "NN", "ADJD", "$.", "NE"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Liegt noch im s\u00fc\u00dfen Taumel. Es k\u00fcsset noch", "tokens": ["Liegt", "noch", "im", "s\u00fc\u00b7\u00dfen", "Tau\u00b7mel", ".", "Es", "k\u00fcs\u00b7set", "noch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPRART", "ADJA", "NN", "$.", "PPER", "VVFIN", "ADV"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des g\u00f6ttergleichen F\u00fcrsten Spuren,", "tokens": ["Des", "g\u00f6t\u00b7ter\u00b7glei\u00b7chen", "F\u00fcrs\u00b7ten", "Spu\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und schon erschallen der Markomannen", "tokens": ["Und", "schon", "er\u00b7schal\u00b7len", "der", "Mar\u00b7ko\u00b7man\u00b7nen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ART", "NN"], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.5": {"line.1": {"text": "Und Quaden", "tokens": ["Und", "Qua\u00b7den"], "token_info": ["word", "word"], "pos": ["KON", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.2": {"text": "Den Ahnen Joseph's, von der Begeisterung", "tokens": ["Den", "Ah\u00b7nen", "Jo\u00b7se\u00b7ph's", ",", "von", "der", "Be\u00b7geis\u00b7te\u00b7rung"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "NE", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Des tiefgereihten Brennenheeres,", "tokens": ["Des", "tief\u00b7ge\u00b7reih\u00b7ten", "Bren\u00b7nen\u00b7hee\u00b7res", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Welches den kommenden Herrscher gr\u00fc\u00dfet.", "tokens": ["Wel\u00b7ches", "den", "kom\u00b7men\u00b7den", "Herr\u00b7scher", "gr\u00fc\u00b7\u00dfet", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.6": {"line.1": {"text": "Er, jeder gro\u00dfen Gabe Bewunderer,", "tokens": ["Er", ",", "je\u00b7der", "gro\u00b7\u00dfen", "Ga\u00b7be", "Be\u00b7wun\u00b7de\u00b7rer", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PIAT", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Er hatte schon den weisen Gebieter", "tokens": ["Er", "hat\u00b7te", "schon", "den", "wei\u00b7sen", "Ge\u00b7bie\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Am Apennin die V\u00f6lker weidend", "tokens": ["Am", "A\u00b7pen\u00b7nin", "die", "V\u00f6l\u00b7ker", "wei\u00b7dend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NE", "ART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Friedsam und furchtbar ist, aufgesuchet.", "tokens": ["Fried\u00b7sam", "und", "furcht\u00b7bar", "ist", ",", "auf\u00b7ge\u00b7su\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NE", "KON", "ADJD", "VAFIN", "$,", "VVFIN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.7": {"line.1": {"text": "Nun eilt er auch den W\u00fcnschen des m\u00e4chtigen,", "tokens": ["Nun", "eilt", "er", "auch", "den", "W\u00fcn\u00b7schen", "des", "m\u00e4ch\u00b7ti\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ART", "NN", "ART", "ADJA", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Des unbezwung'nen Helden", "tokens": ["Des", "un\u00b7be\u00b7zwung'\u00b7nen", "Hel\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["ART", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Von seinen Starken, an der Spree", "tokens": ["Von", "sei\u00b7nen", "Star\u00b7ken", ",", "an", "der", "Spree"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$,", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "In dem Gewande der Ehre stralet,", "tokens": ["In", "dem", "Ge\u00b7wan\u00b7de", "der", "Eh\u00b7re", "stra\u00b7let", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.8": {"line.1": {"text": "Erk\u00e4mpft in rothen Feldern, ein Bardenfreund,", "tokens": ["Er\u00b7k\u00e4mpft", "in", "ro\u00b7then", "Fel\u00b7dern", ",", "ein", "Bar\u00b7den\u00b7freund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ADJA", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Barde selber", "tokens": ["Und", "Bar\u00b7de", "sel\u00b7ber"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "ADV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Ges\u00e4ngen holder! \u2013 und des Kieles", "tokens": ["Ge\u00b7s\u00e4n\u00b7gen", "hol\u00b7der", "!", "\u2013", "und", "des", "Kie\u00b7les"], "token_info": ["word", "word", "punct", "punct", "word", "word", "word"], "pos": ["NN", "PTKVZ", "$.", "$(", "KON", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "So, wie der Klinge gew\u00f6hnt, entgegen.", "tokens": ["So", ",", "wie", "der", "Klin\u00b7ge", "ge\u00b7w\u00f6hnt", ",", "ent\u00b7ge\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADV", "$,", "PWAV", "ART", "NN", "VVPP", "$,", "PTKVZ", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Zween Kriege, leichentr\u00e4chtig, verderbenvoll, \u2013", "tokens": ["Zween", "Krie\u00b7ge", ",", "lei\u00b7chen\u00b7tr\u00e4ch\u00b7tig", ",", "ver\u00b7der\u00b7ben\u00b7voll", ",", "\u2013"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct", "punct"], "pos": ["NN", "NN", "$,", "ADJD", "$,", "ADJD", "$,", "$("], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Wir M\u00e4nner denken's! \u2013 kriegete Friederich", "tokens": ["Wir", "M\u00e4n\u00b7ner", "den\u00b7ken's", "!", "\u2013", "krie\u00b7ge\u00b7te", "Frie\u00b7de\u00b7rich"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PPER", "NN", "NE", "$.", "$(", "VVFIN", "NE"], "meter": "-+--+---+-+", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Mit Josephs Mutter (denn er hatte,", "tokens": ["Mit", "Jo\u00b7se\u00b7phs", "Mut\u00b7ter", "(", "denn", "er", "hat\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "NN", "$(", "KON", "PPER", "VAFIN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Nie sie gesehen) und Heldenbr\u00e4ute", "tokens": ["Nie", "sie", "ge\u00b7se\u00b7hen", ")", "und", "Hel\u00b7den\u00b7br\u00e4u\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ADV", "PPER", "VVPP", "$(", "KON", "NN"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.10": {"line.1": {"text": "Vergossen zweimal Thr\u00e4nen, und J\u00fcnglinge", "tokens": ["Ver\u00b7gos\u00b7sen", "zwei\u00b7mal", "Thr\u00e4\u00b7nen", ",", "und", "J\u00fcng\u00b7lin\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["NN", "ADV", "NN", "$,", "KON", "NN"], "meter": "-+-+-+--++-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Beschwuren zweimal \u00fcber der V\u00e4ter Grab", "tokens": ["Be\u00b7schwu\u00b7ren", "zwei\u00b7mal", "\u00fc\u00b7ber", "der", "V\u00e4\u00b7ter", "Grab"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "ADV", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Des Todes Rache, deutsche Fl\u00fc\u00dfe", "tokens": ["Des", "To\u00b7des", "Ra\u00b7che", ",", "deut\u00b7sche", "Fl\u00fc\u00b7\u00dfe"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "NN", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Tr\u00fcbten sich zweimal in deutschem Blute.", "tokens": ["Tr\u00fcb\u00b7ten", "sich", "zwei\u00b7mal", "in", "deut\u00b7schem", "Blu\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ADV", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.11": {"line.1": {"text": "Nun wirft die Gro\u00dfmuth auf das Vergangene", "tokens": ["Nun", "wirft", "die", "Gro\u00df\u00b7muth", "auf", "das", "Ver\u00b7gan\u00b7ge\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Den himmelreinen Schleier. Die F\u00fcrsten stehn,", "tokens": ["Den", "him\u00b7mel\u00b7rei\u00b7nen", "Schlei\u00b7er", ".", "Die", "F\u00fcrs\u00b7ten", "stehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Zwo Sonnen, die der Mittag", "tokens": ["Zwo", "Son\u00b7nen", ",", "die", "der", "Mit\u00b7tag"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["CARD", "NN", "$,", "PRELS", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Sehen sich Ewigkeit an der Stirne;", "tokens": ["Se\u00b7hen", "sich", "E\u00b7wig\u00b7keit", "an", "der", "Stir\u00b7ne", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "APPR", "ART", "NN", "$."], "meter": "+--+-+--+-", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Und Einer ehret, was ihn verewiget,", "tokens": ["Und", "Ei\u00b7ner", "eh\u00b7ret", ",", "was", "ihn", "ve\u00b7re\u00b7wi\u00b7get", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "VVFIN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Am And'ren, einer schlie\u00dfet dem Anderen", "tokens": ["Am", "An\u00b7d'\u00b7ren", ",", "ei\u00b7ner", "schlie\u00b7\u00dfet", "dem", "An\u00b7de\u00b7ren"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "$,", "PIS", "VVFIN", "ART", "ADJA"], "meter": "--+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Sein gro\u00dfes Herz auf. Freundschaft str\u00f6met", "tokens": ["Sein", "gro\u00b7\u00dfes", "Herz", "auf", ".", "Freund\u00b7schaft", "str\u00f6\u00b7met"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKVZ", "$.", "NN", "VVFIN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Von der Gebieter erhitzten Lippen.", "tokens": ["Von", "der", "Ge\u00b7bie\u00b7ter", "er\u00b7hitz\u00b7ten", "Lip\u00b7pen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}}, "stanza.13": {"line.1": {"text": "So stand vor Siegmar'n Herrmann", "tokens": ["So", "stand", "vor", "Sieg\u00b7ma\u00b7r'n", "Herr\u00b7mann"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "APPR", "NE", "NE"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Verrieth dem grauen Helden den k\u00fcnftigen", "tokens": ["Ver\u00b7rieth", "dem", "grau\u00b7en", "Hel\u00b7den", "den", "k\u00fcnf\u00b7ti\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "ADJA", "NN", "ART", "ADJA"], "meter": "-+-+-+--+--", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Vernichter stolzer Legionen,", "tokens": ["Ver\u00b7nich\u00b7ter", "stol\u00b7zer", "Le\u00b7gi\u00b7o\u00b7nen", ","], "token_info": ["word", "word", "wor