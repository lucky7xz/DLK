c.tri"}, "line.2": {"text": "Versinkt des Mondes Kahn,", "tokens": ["Ver\u00b7sinkt", "des", "Mon\u00b7des", "Kahn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und kr\u00e4hend weckt die Lande", "tokens": ["Und", "kr\u00e4\u00b7hend", "weckt", "die", "Lan\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Morgengru\u00df der Hahn.", "tokens": ["Mit", "Mor\u00b7gen\u00b7gru\u00df", "der", "Hahn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Die Elfen fliehn erschrocken", "tokens": ["Die", "El\u00b7fen", "fliehn", "er\u00b7schro\u00b7cken"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "In Erd' und Felsenspalt \u2013", "tokens": ["In", "Erd'", "und", "Fel\u00b7sen\u00b7spalt", "\u2013"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Nebel sinkt in Flocken", "tokens": ["Der", "Ne\u00b7bel", "sinkt", "in", "Flo\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auf ihr Versteck im Wald. \u2013", "tokens": ["Auf", "ihr", "Ver\u00b7steck", "im", "Wald", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPRART", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.19": {"line.1": {"text": "Das ist dein Los ja heute", "tokens": ["Das", "