{"textgrid.poem.38885": {"metadata": {"author": {"name": "Tieck, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "1L: Tr\u00e4uft vom Himmel der k\u00fchle Thau,", "genre": "verse", "period": "N.A.", "pub_year": 1813, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Tr\u00e4uft vom Himmel der k\u00fchle Thau,", "tokens": ["Tr\u00e4uft", "vom", "Him\u00b7mel", "der", "k\u00fch\u00b7le", "Thau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPRART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Thun die Blumen die Kelche zu,", "tokens": ["Thun", "die", "Blu\u00b7men", "die", "Kel\u00b7che", "zu", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.3": {"text": "Sp\u00e4troth sieht scheidend nach der Au,", "tokens": ["Sp\u00e4t\u00b7roth", "sieht", "schei\u00b7dend", "nach", "der", "Au", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Fl\u00fcstern die Pappeln, sinkt nieder die n\u00e4cht'ge Ruh.", "tokens": ["Fl\u00fcs\u00b7tern", "die", "Pap\u00b7peln", ",", "sinkt", "nie\u00b7der", "die", "n\u00e4cht'\u00b7ge", "Ruh", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "VVFIN", "PTKVZ", "ART", "ADJA", "NN", "$."], "meter": "+--+--+--+-+", "measure": "dactylic.tri.plus"}}, "stanza.2": {"line.1": {"text": "Kommen und gehn die Schatten,", "tokens": ["Kom\u00b7men", "und", "gehn", "die", "Schat\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.2": {"text": "Wolken bleiben noch sp\u00e4t auf", "tokens": ["Wol\u00b7ken", "blei\u00b7ben", "noch", "sp\u00e4t", "auf"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ADV", "ADJD", "APPR"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ziehn mit schwerem, unbeholfnem Lauf", "tokens": ["Und", "ziehn", "mit", "schwe\u00b7rem", ",", "un\u00b7be\u00b7holf\u00b7nem", "Lauf"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "$,", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ueber die erfrischten Matten.", "tokens": ["Ue\u00b7ber", "die", "er\u00b7frischten", "Mat\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.3": {"line.1": {"text": "Kommen die Sterne und schwinden wieder,", "tokens": ["Kom\u00b7men", "die", "Ster\u00b7ne", "und", "schwin\u00b7den", "wie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "KON", "VVFIN", "ADV", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.2": {"text": "Blicken winkend und fl\u00fcchtig nieder,", "tokens": ["Bli\u00b7cken", "win\u00b7kend", "und", "fl\u00fcch\u00b7tig", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVPP", "KON", "ADJD", "PTKVZ", "$,"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "Wohnt im Wald die Dunkelheit,", "tokens": ["Wohnt", "im", "Wald", "die", "Dun\u00b7kel\u00b7heit", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Dehnt sich Finster weit und breit.", "tokens": ["Dehnt", "sich", "Fins\u00b7ter", "weit", "und", "breit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "NN", "ADJD", "KON", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Hinter'm Wasser wie flimmende Flammen,", "tokens": ["Hin\u00b7ter'm", "Was\u00b7ser", "wie", "flim\u00b7men\u00b7de", "Flam\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+--+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Berggipfel oben mit Gold beschienen,", "tokens": ["Berg\u00b7gip\u00b7fel", "o\u00b7ben", "mit", "Gold", "be\u00b7schie\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.3": {"text": "Neigen rauschend und ernst die gr\u00fcnen", "tokens": ["Nei\u00b7gen", "rau\u00b7schend", "und", "ernst", "die", "gr\u00fc\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVPP", "KON", "ADJD", "ART", "ADJA"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.4": {"text": "Geb\u00fcsche die blinkenden H\u00e4upter zusammen.", "tokens": ["Ge\u00b7b\u00fc\u00b7sche", "die", "blin\u00b7ken\u00b7den", "H\u00e4up\u00b7ter", "zu\u00b7sam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}}, "stanza.5": {"line.1": {"text": "Welle, rollst du herauf den Schein,", "tokens": ["Wel\u00b7le", ",", "rollst", "du", "her\u00b7auf", "den", "Schein", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "PPER", "ADV", "ART", "NN", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Des Mondes rund freundlich Angesicht?", "tokens": ["Des", "Mon\u00b7des", "rund", "freund\u00b7lich", "An\u00b7ge\u00b7sicht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "ADJD", "NN", "$."], "meter": "-+-++-+-+", "measure": "unknown.measure.penta"}, "line.3": {"text": "Er merkt's und freudig bewegt sich der Hain,", "tokens": ["Er", "merkt's", "und", "freu\u00b7dig", "be\u00b7wegt", "sich", "der", "Hain", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "ADJD", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Streckt die Zweig' entgegen dem Zauberlicht.", "tokens": ["Streckt", "die", "Zweig'", "ent\u00b7ge\u00b7gen", "dem", "Zau\u00b7ber\u00b7licht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+--+-+", "measure": "trochaic.penta.relaxed"}}, "stanza.6": {"line.1": {"text": "Fangen die Geister auf den Fluthen zu springen,", "tokens": ["Fan\u00b7gen", "die", "Geis\u00b7ter", "auf", "den", "Flut\u00b7hen", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPR", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "+--+-+-+--+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Thun sich die Nachtblumen auf mit Klingen,", "tokens": ["Thun", "sich", "die", "Nacht\u00b7blu\u00b7men", "auf", "mit", "Klin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "ART", "NN", "APPR", "APPR", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Wacht die Nachtigall im dicksten Baum,", "tokens": ["Wacht", "die", "Nach\u00b7ti\u00b7gall", "im", "dicks\u00b7ten", "Baum", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Verk\u00fcndet dichterisch ihren Traum,", "tokens": ["Ver\u00b7k\u00fcn\u00b7det", "dich\u00b7te\u00b7risch", "ih\u00b7ren", "Traum", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.5": {"text": "Wie helle, blendende Strahlen die T\u00f6ne niederflie\u00dfen", "tokens": ["Wie", "hel\u00b7le", ",", "blen\u00b7den\u00b7de", "Strah\u00b7len", "die", "T\u00f6\u00b7ne", "nie\u00b7der\u00b7flie\u00b7\u00dfen"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJA", "$,", "ADJA", "NN", "ART", "NN", "VVINF"], "meter": "-+-+--+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "Am Bergeshang den Wiederhall zu gr\u00fcssen.", "tokens": ["Am", "Ber\u00b7ges\u00b7hang", "den", "Wie\u00b7der\u00b7hall", "zu", "gr\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Flimmern die Wellen,", "tokens": ["Flim\u00b7mern", "die", "Wel\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,"], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}, "line.2": {"text": "Funkeln die wandernden Quellen,", "tokens": ["Fun\u00b7keln", "die", "wan\u00b7dern\u00b7den", "Quel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.3": {"text": "Streifen durch's Gestr\u00e4uch", "tokens": ["Strei\u00b7fen", "durch's", "Ge\u00b7str\u00e4uch"], "token_info": ["word", "word", "word"], "pos": ["NN", "APPRART", "NN"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.4": {"text": "Die Feuerw\u00fcrmchen bleich. \u2013", "tokens": ["Die", "Feu\u00b7er\u00b7w\u00fcrm\u00b7chen", "bleich", ".", "\u2013"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ADJD", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "Wie die Wolken wandelt mein Sehnen,", "tokens": ["Wie", "die", "Wol\u00b7ken", "wan\u00b7delt", "mein", "Seh\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "--+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Mein Gedanke, bald dunkel, bald hell,", "tokens": ["Mein", "Ge\u00b7dan\u00b7ke", ",", "bald", "dun\u00b7kel", ",", "bald", "hell", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$,"], "meter": "--+--+--+", "measure": "anapaest.tri.plus"}, "line.3": {"text": "H\u00fcpfen W\u00fcnsche um mich wie der Quell,", "tokens": ["H\u00fcp\u00b7fen", "W\u00fcn\u00b7sche", "um", "mich", "wie", "der", "Quell", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "APPR", "PPER", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.4": {"text": "Kenne nicht die brennenden Thr\u00e4nen.", "tokens": ["Ken\u00b7ne", "nicht", "die", "bren\u00b7nen\u00b7den", "Thr\u00e4\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}}, "stanza.9": {"line.1": {"text": "Bist du nah, bist du weit,", "tokens": ["Bist", "du", "nah", ",", "bist", "du", "weit", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ADJD", "$,", "VAFIN", "PPER", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Gl\u00fcck das nur f\u00fcr mich erbl\u00fchte?", "tokens": ["Gl\u00fcck", "das", "nur", "f\u00fcr", "mich", "er\u00b7bl\u00fch\u00b7te", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADV", "APPR", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Ach! da\u00df es die H\u00e4nde biete", "tokens": ["Ach", "!", "da\u00df", "es", "die", "H\u00e4n\u00b7de", "bie\u00b7te"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$.", "KOUS", "PPER", "ART", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In des Mondes Einsamkeit.", "tokens": ["In", "des", "Mon\u00b7des", "Ein\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "K\u00f6mmt's aus dem Walde? schleicht's vom Thal?", "tokens": ["K\u00f6mmt's", "aus", "dem", "Wal\u00b7de", "?", "schleicht's", "vom", "Thal", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$.", "VVFIN", "APPRART", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Steigt es den Berg vielleicht hernieder?", "tokens": ["Steigt", "es", "den", "Berg", "viel\u00b7leicht", "her\u00b7nie\u00b7der", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.3": {"text": "Kommen alte Schmerzen wieder?", "tokens": ["Kom\u00b7men", "al\u00b7te", "Schmer\u00b7zen", "wie\u00b7der", "?"], "