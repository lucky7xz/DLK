{"dta.poem.7461": {"metadata": {"author": {"name": "Herwegh, Georg", "birth": "N.A.", "death": "N.A."}, "title": "XvIII.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1841", "urn": "urn:nbn:de:kobv:b4-200905192430", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Der Tod, ihr Freunde, ja, der Tod soll leben!", "tokens": ["Der", "Tod", ",", "ihr", "Freun\u00b7de", ",", "ja", ",", "der", "Tod", "soll", "le\u00b7ben", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPOSAT", "NN", "$,", "PTKANT", "$,", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ich hab' ein gl\u00fchend Lied in tiefster Nacht", "tokens": ["Ich", "hab'", "ein", "gl\u00fc\u00b7hend", "Lied", "in", "tiefs\u00b7ter", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJD", "NN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dem treusten Freund der Erde angefacht;", "tokens": ["Dem", "treus\u00b7ten", "Freund", "der", "Er\u00b7de", "an\u00b7ge\u00b7facht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Die Toten will ich und den Tod erheben!", "tokens": ["Die", "To\u00b7ten", "will", "ich", "und", "den", "Tod", "er\u00b7he\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VMFIN", "PPER", "KON", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Wir sind nur Kinder, die mit Widerstreben,", "tokens": ["Wir", "sind", "nur", "Kin\u00b7der", ",", "die", "mit", "Wi\u00b7der\u00b7stre\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "NN", "$,", "PRELS", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gleich Tropfen von dem Meer, sich losgemacht,", "tokens": ["Gleich", "Trop\u00b7fen", "von", "dem", "Meer", ",", "sich", "los\u00b7ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "$,", "PRF", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und die vom Tode werden heimgebracht", "tokens": ["Und", "die", "vom", "To\u00b7de", "wer\u00b7den", "heim\u00b7ge\u00b7bracht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "APPRART", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Und liebend an das All zur\u00fcckgegeben.", "tokens": ["Und", "lie\u00b7bend", "an", "das", "All", "zu\u00b7r\u00fcck\u00b7ge\u00b7ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Vernichtung d\u00fcnkt Euch eine herbe Pille?", "tokens": ["Ver\u00b7nich\u00b7tung", "d\u00fcnkt", "Euch", "ei\u00b7ne", "her\u00b7be", "Pil\u00b7le", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch \u2014 heischt' das Element nicht diesen Zoll,", "tokens": ["Doch", "heischt'", "das", "E\u00b7le\u00b7ment", "nicht", "die\u00b7sen", "Zoll", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$(", "VVFIN", "ART", "NN", "PTKNEG", "PDAT", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.3": {"text": "Das Sterben w\u00fcrde unser eigner Wille.", "tokens": ["Das", "Ster\u00b7ben", "w\u00fcr\u00b7de", "un\u00b7ser", "eig\u00b7ner", "Wil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Das Sterben macht das Leben ganz und voll;", "tokens": ["Das", "Ster\u00b7ben", "macht", "das", "Le\u00b7ben", "ganz", "und", "voll", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "ADV", "KON", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erst sei das Herz in ", "tokens": ["Erst", "sei", "das", "Herz", "in"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPR"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Wenn's in der Brust der Menschheit schlagen soll.", "tokens": ["Wenn's", "in", "der", "Brust", "der", "Menschheit", "schla\u00b7gen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "+--+-+--+", "measure": "iambic.tetra.invert"}}}}}