{"textgrid.poem.35657": {"metadata": {"author": {"name": "Vo\u00df, Johann Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "20. Ahndung", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Freundlicher Mond, du gie\u00dfest milden Schimmer", "tokens": ["Freund\u00b7li\u00b7cher", "Mond", ",", "du", "gie\u00b7\u00dfest", "mil\u00b7den", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf mein goldnes Klavier, und winkest l\u00e4chelnd,", "tokens": ["Auf", "mein", "gold\u00b7nes", "Kla\u00b7vier", ",", "und", "win\u00b7kest", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Mit des seelenschmelzenden Gluck: Willkommen!", "tokens": ["Mit", "des", "see\u00b7len\u00b7schmel\u00b7zen\u00b7den", "Gluck", ":", "Will\u00b7kom\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$.", "NN", "$."], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.4": {"text": "Dich zu begr\u00fc\u00dfen.", "tokens": ["Dich", "zu", "be\u00b7gr\u00fc\u00b7\u00dfen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "PTKZU", "VVINF", "$."], "meter": "+--+-", "measure": "amphibrach.di.relaxed"}}, "stanza.2": {"line.1": {"text": "Aber mir sagt der tiefe bange Seufzer,", "tokens": ["A\u00b7ber", "mir", "sagt", "der", "tie\u00b7fe", "ban\u00b7ge", "Seuf\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "ADJA", "NN", "$,"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Da\u00df mit Thr\u00e4nen der Sehnsucht meine Selma", "tokens": ["Da\u00df", "mit", "Thr\u00e4\u00b7nen", "der", "Sehn\u00b7sucht", "mei\u00b7ne", "Sel\u00b7ma"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "--+--+-+--+", "measure": "anapaest.di.plus"}, "line.3": {"text": "Jetzt dich anblickt: freundlicher Mond, ich kann dich", "tokens": ["Jetzt", "dich", "an\u00b7blickt", ":", "freund\u00b7li\u00b7cher", "Mond", ",", "ich", "kann", "dich"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "PPER", "VVFIN", "$.", "ADJA", "NN", "$,", "PPER", "VMFIN", "PRF"], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Jetzt nicht begr\u00fc\u00dfen!", "tokens": ["Jetzt", "nicht", "be\u00b7gr\u00fc\u00b7\u00dfen", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "Freundlicher Mond, du gie\u00dfest milden Schimmer", "tokens": ["Freund\u00b7li\u00b7cher", "Mond", ",", "du", "gie\u00b7\u00dfest", "mil\u00b7den", "Schim\u00b7mer"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "PPER", "VVFIN", "ADJA", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Auf mein goldnes Klavier, und winkest l\u00e4chelnd,", "tokens": ["Auf", "mein", "gold\u00b7nes", "Kla\u00b7vier", ",", "und", "win\u00b7kest", "l\u00e4\u00b7chelnd", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "ADJD", "$,"], "meter": "+-+-+--+-+-", "measure": "sapphicusminor"}, "line.3": {"text": "Mit des