{"dta.poem.13185": {"metadata": {"author": {"name": "Scheyb, Franz Christoph von", "birth": "N.A.", "death": "N.A."}, "title": "Z ehndes  B uch.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1746", "urn": "urn:nbn:de:kobv:b4-20535-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So wurden Ohr und Aug in Fr\u00f6hlichkeit gebracht,", "tokens": ["So", "wur\u00b7den", "Ohr", "und", "Aug", "in", "Fr\u00f6h\u00b7lich\u00b7keit", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "KON", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Indem das Eine nur auf diese Stimmen dacht,", "tokens": ["In\u00b7dem", "das", "Ei\u00b7ne", "nur", "auf", "die\u00b7se", "Stim\u00b7men", "dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ART", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die\u00df aber seinen Blick nach jener Gegend wande,", "tokens": ["Die\u00df", "a\u00b7ber", "sei\u00b7nen", "Blick", "nach", "je\u00b7ner", "Ge\u00b7gend", "wan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ADV", "PPOSAT", "NN", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "20Wo sich Theresia mit ihrem Staat befande.", "tokens": ["sich", "The\u00b7re\u00b7sia", "mit", "ih\u00b7rem", "Staat", "be\u00b7fan\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "NE", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was Freude, da man sie den Thron besteigen sah!", "tokens": ["Was", "Freu\u00b7de", ",", "da", "man", "sie", "den", "Thron", "be\u00b7stei\u00b7gen", "sah", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "NN", "$,", "KOUS", "PIS", "PPER", "ART", "NN", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Weil es mit h\u00f6chster Pracht und Herrlichkeit geschah.", "tokens": ["Weil", "es", "mit", "h\u00f6chs\u00b7ter", "Pracht", "und", "Herr\u00b7lich\u00b7keit", "ge\u00b7schah", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Die Kayserinn trat vor, begab sich in die Mitte,", "tokens": ["Die", "Kay\u00b7se\u00b7rinn", "trat", "vor", ",", "be\u00b7gab", "sich", "in", "die", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PTKVZ", "$,", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Indem das K\u00f6nigs-Paar auf beyde Seiten schritte.", "tokens": ["In\u00b7dem", "das", "K\u00f6\u00b7nigs\u00b7Paar", "auf", "bey\u00b7de", "Sei\u00b7ten", "schrit\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "APPR", "PIAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}