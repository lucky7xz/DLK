{"dta.poem.12904": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Aus dem frantz\u00f6sischen des   Passeratii.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1710", "urn": "urn:nbn:de:kobv:b4-20284-0", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Du angenehmes heer, das in den l\u00fcfften schwirrt,", "tokens": ["Du", "an\u00b7ge\u00b7neh\u00b7mes", "heer", ",", "das", "in", "den", "l\u00fcff\u00b7ten", "schwirrt", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$,", "PRELS", "APPR", "ART", "ADJA", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und das des voglers hand mit s\u00fc\u00dfen k\u00f6rnern kirrt,", "tokens": ["Und", "das", "des", "vog\u00b7lers", "hand", "mit", "s\u00fc\u00b7\u00dfen", "k\u00f6r\u00b7nern", "kirrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "ADJA", "NN", "APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jhr vogel, die ihr fast nicht sicher singen k\u00f6nnet!", "tokens": ["Ihr", "vo\u00b7gel", ",", "die", "ihr", "fast", "nicht", "si\u00b7cher", "sin\u00b7gen", "k\u00f6n\u00b7net", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$,", "PRELS", "PPER", "ADV", "PTKNEG", "ADJD", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach springt mir doch zu lieb\u2019 in meinem vorsatz bey,", "tokens": ["Ach", "springt", "mir", "doch", "zu", "lieb'", "in", "mei\u00b7nem", "vor\u00b7satz", "bey", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PPER", "ADV", "PTKA", "ADJD", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Da\u00df mir, so wol als euch, einmal geholffen sey!", "tokens": ["Da\u00df", "mir", ",", "so", "wol", "als", "euch", ",", "ein\u00b7mal", "ge\u00b7holf\u00b7fen", "sey", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ADV", "ADV", "KOUS", "PPER", "$,", "ADV", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Jhr kennt den vogel wohl, den man die liebe nennet;", "tokens": ["Ihr", "kennt", "den", "vo\u00b7gel", "wohl", ",", "den", "man", "die", "lie\u00b7be", "nen\u00b7net", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "ADV", "$,", "PRELS", "PIS", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Der ist es, der uns nicht zu ruh und frieden l\u00e4st,", "tokens": ["Der", "ist", "es", ",", "der", "uns", "nicht", "zu", "ruh", "und", "frie\u00b7den", "l\u00e4st", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "$,", "PRELS", "PPER", "PTKNEG", "APPR", "NN", "KON", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Der so viel klagen uns aus hertz und munde prest,", "tokens": ["Der", "so", "viel", "kla\u00b7gen", "uns", "aus", "hertz", "und", "mun\u00b7de", "prest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADV", "VVFIN", "PPER", "APPR", "NN", "KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Dem passet flei\u00dfig auf und last euch nicht betr\u00fcgen:", "tokens": ["Dem", "pas\u00b7set", "flei\u00b7\u00dfig", "auf", "und", "last", "euch", "nicht", "be\u00b7tr\u00fc\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADJD", "PTKVZ", "KON", "VVFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn ihr ihn nun ertappt, so gebt ihm zwar qvartier;", "tokens": ["Wenn", "ihr", "ihn", "nun", "er\u00b7tappt", ",", "so", "gebt", "ihm", "zwar", "qvar\u00b7tier", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ADV", "VVPP", "$,", "ADV", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Doch bringt ihn wohl gefa\u00dft und abgepfl\u00fcckt zu mir:", "tokens": ["Doch", "bringt", "ihn", "wohl", "ge\u00b7fa\u00dft", "und", "ab\u00b7ge\u00b7pfl\u00fcckt", "zu", "mir", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADV", "VVPP", "KON", "VVPP", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "So soll der vogel uns nicht mehr ins hertze fliegen.", "tokens": ["So", "soll", "der", "vo\u00b7gel", "uns", "nicht", "mehr", "ins", "hert\u00b7ze", "flie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PPER", "PTKNEG", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}