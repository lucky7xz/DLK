{"dta.poem.9924": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "33.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-200905199377", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Durch mich unter gantze\u0303 v\u00f6lckern wird der unterscheid gemacht/", "tokens": ["Durch", "mich", "un\u00b7ter", "gant\u00b7z\u1ebd", "v\u00f6l\u00b7ckern", "wird", "der", "un\u00b7ter\u00b7scheid", "ge\u00b7macht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRF", "APPR", "ADV", "VVINF", "VAFIN", "ART", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Da ich nur bin eine blume/ die man meistens bricht bey nacht.", "tokens": ["Da", "ich", "nur", "bin", "ei\u00b7ne", "blu\u00b7me", "/", "die", "man", "meis\u00b7tens", "bricht", "bey", "nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "ART", "NN", "$(", "PRELS", "PIS", "ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+-+-+-+-+", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Ohne scham werd ich genennet/ niemahls aber sonder ehr;", "tokens": ["Oh\u00b7ne", "scham", "werd", "ich", "ge\u00b7nen\u00b7net", "/", "nie\u00b7mahls", "a\u00b7ber", "son\u00b7der", "ehr", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "VAFIN", "PPER", "VVPP", "$(", "ADV", "ADV", "ADJA", "NN", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.4": {"text": "Wenn ich einmal bin verlohren/ so gelt ich hernach nicht mehr.", "tokens": ["Wenn", "ich", "ein\u00b7mal", "bin", "ver\u00b7loh\u00b7ren", "/", "so", "gelt", "ich", "her\u00b7nach", "nicht", "mehr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "VAFIN", "VVPP", "$(", "ADV", "ADJD", "PPER", "ADV", "PTKNEG", "ADV", "$."], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}}}}}