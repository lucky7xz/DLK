{"dta.poem.21137": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Die Seuche .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Eine gef\u00fcrchtete Zeit! Mit pestilenzischem Fittig", "tokens": ["Ei\u00b7ne", "ge\u00b7f\u00fcrch\u00b7te\u00b7te", "Zeit", "!", "Mit", "pes\u00b7ti\u00b7len\u00b7zi\u00b7schem", "Fit\u00b7tig"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "$.", "APPR", "ADJA", "NN"], "meter": "+--+--+-+-+--+-", "measure": "hexameter"}, "line.2": {"text": "Wallet auf Nebeln die Seuche daher.", "tokens": ["Wal\u00b7let", "auf", "Ne\u00b7beln", "die", "Seu\u00b7che", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Furchtbar verjaget ihr Arm den harten m\u00e4nnlichen", "tokens": ["Furcht\u00b7bar", "ver\u00b7ja\u00b7get", "ihr", "Arm", "den", "har\u00b7ten", "m\u00e4nn\u00b7li\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "ART", "ADJA", "ADJA"], "meter": "+--+--+-+-+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Ueber Gefilde voll Regen und Sumpf.", "tokens": ["Ue\u00b7ber", "Ge\u00b7fil\u00b7de", "voll", "Re\u00b7gen", "und", "Sumpf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "ADJD", "NN", "KON", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.2": {"line.1": {"text": "Hat sie nicht Monathe schon des Nordpols Pforte ge-", "tokens": ["Hat", "sie", "nicht", "Mo\u00b7na\u00b7the", "schon", "des", "Nord\u00b7pols", "Pfor\u00b7te", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "PPER", "PTKNEG", "NN", "ADV", "ART", "NN", "NN", "TRUNC"], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.2": {"text": "Und die Pall\u00e4ste der K\u00e4lte gesperrt?", "tokens": ["Und", "die", "Pal\u00b7l\u00e4s\u00b7te", "der", "K\u00e4l\u00b7te", "ge\u00b7sperrt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVPP", "$."], "meter": "---+--+--+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Noch hat erfrischender Schnee nicht \u00fcber Berge gest\u00f6-", "tokens": ["Noch", "hat", "er\u00b7fri\u00b7schen\u00b7der", "Schnee", "nicht", "\u00fc\u00b7ber", "Ber\u00b7ge", "ge\u00b7st\u00f6"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADJA", "NN", "PTKNEG", "APPR", "NN", "TRUNC"], "meter": "-+-+--+-+-+--+", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Oder der Bach sich mit Eise bedeckt.", "tokens": ["O\u00b7der", "der", "Bach", "sich", "mit", "Ei\u00b7se", "be\u00b7deckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PRF", "APPR", "NN", "VVPP", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}, "stanza.3": {"line.1": {"text": "Aber auf s\u00fcdlichem Sturm braust die verderbende G\u00f6t-", "tokens": ["A\u00b7ber", "auf", "s\u00fcd\u00b7li\u00b7chem", "Sturm", "braust", "die", "ver\u00b7der\u00b7ben\u00b7de", "G\u00f6t"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ADJA", "NN", "VVFIN", "ART", "ADJA", "TRUNC"], "meter": "+--+--+-+-+--+", "measure": "dactylic.di.plus"}, "line.2": {"text": "Ueber die faulenden Wasser daher.", "tokens": ["Ue\u00b7ber", "die", "fau\u00b7len\u00b7den", "Was\u00b7ser", "da\u00b7her", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "PAV", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Gegenden trinken das Gift aus manchem unzeitigen Re-", "tokens": ["Ge\u00b7gen\u00b7den", "trin\u00b7ken", "das", "Gift", "aus", "man\u00b7chem", "un\u00b7zei\u00b7ti\u00b7gen", "Re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "ART", "NN", "APPR", "PIAT", "ADJA", "TRUNC"], "meter": "+--+--+-+-++--+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Lau, wie der Regen im fruchtbaren Lenz.", "tokens": ["Lau", ",", "wie", "der", "Re\u00b7gen", "im", "frucht\u00b7ba\u00b7ren", "Lenz", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}}}}}