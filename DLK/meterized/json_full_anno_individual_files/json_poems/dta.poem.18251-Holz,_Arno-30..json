{"dta.poem.18251": {"metadata": {"author": {"name": "Holz, Arno", "birth": "N.A.", "death": "N.A."}, "title": "30.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1886", "urn": "urn:nbn:de:kobv:b4-200905192665", "language": ["de:0.99"], "booktitle": "Holz, Arno: Das Buch der Zeit. Lieder eines Modernen. Z\u00fcrich, 1886."}, "poem": {"stanza.1": {"line.1": {"text": "Und helf ihm mit, die alte Zeit zerh\u00e4mmern,", "tokens": ["Und", "helf", "ihm", "mit", ",", "die", "al\u00b7te", "Zeit", "zer\u00b7h\u00e4m\u00b7mern", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Doch soll noch manchmal mich umd\u00e4mmern", "tokens": ["Doch", "soll", "noch", "manch\u00b7mal", "mich", "um\u00b7d\u00e4m\u00b7mern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "ADV", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Die alte, goldne Heidenwelt!", "tokens": ["Die", "al\u00b7te", ",", "gold\u00b7ne", "Hei\u00b7den\u00b7welt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn stets beleidigt meine Phantasie", "tokens": ["Denn", "stets", "be\u00b7lei\u00b7digt", "mei\u00b7ne", "Phan\u00b7ta\u00b7sie"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Ein Marmorchristus mit verrenkten Knochen,", "tokens": ["Ein", "Mar\u00b7mor\u00b7chris\u00b7tus", "mit", "ver\u00b7renk\u00b7ten", "Kno\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch oft hat mir ins Herz gesprochen", "tokens": ["Doch", "oft", "hat", "mir", "ins", "Herz", "ge\u00b7spro\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VAFIN", "PPER", "APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ein Jupiter Otricoli!", "tokens": ["Ein", "Ju\u00b7pi\u00b7ter", "O\u00b7tri\u00b7co\u00b7li", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}}, "stanza.3": {"line.1": {"text": "O sch\u00f6ne Zeit, als am Hymettoshang", "tokens": ["O", "sch\u00f6\u00b7ne", "Zeit", ",", "als", "am", "Hy\u00b7met\u00b7to\u00b7shang"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "ADJA", "NN", "$,", "KOUS", "APPRART", "NN"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ein heilig Volk sein heilig Feuer sch\u00fcrte,", "tokens": ["Ein", "hei\u00b7lig", "Volk", "sein", "hei\u00b7lig", "Feu\u00b7er", "sch\u00fcr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PPOSAT", "ADJD", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Als Phidias seinen Mei\u00dfel f\u00fchrte", "tokens": ["Als", "Phi\u00b7di\u00b7as", "sei\u00b7nen", "Mei\u00b7\u00dfel", "f\u00fchr\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NE", "PPOSAT", "NN", "VVFIN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Und Pindar seine Hymnen sang!", "tokens": ["Und", "Pin\u00b7dar", "sei\u00b7ne", "Hym\u00b7nen", "sang", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihr Wallfahrtsweltort hie\u00df Olympia", "tokens": ["Ihr", "Wall\u00b7fahrts\u00b7welt\u00b7ort", "hie\u00df", "O\u00b7lym\u00b7pia"], "token_info": ["word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "NE"], "meter": "-+--+-+-+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Und nicht von Holz war'n ihre Rosenkr\u00e4nze,", "tokens": ["Und", "nicht", "von", "Holz", "wa\u00b7r'n", "ih\u00b7re", "Ro\u00b7sen\u00b7kr\u00e4n\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PTKNEG", "APPR", "NN", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Wenn sie die priesterlichen T\u00e4nze", "tokens": ["Wenn", "sie", "die", "pries\u00b7ter\u00b7li\u00b7chen", "T\u00e4n\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sich seelenvoll verschlingen sah!", "tokens": ["Sich", "see\u00b7len\u00b7voll", "ver\u00b7schlin\u00b7gen", "sah", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "ADJD", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Erde, nicht der Himmel, war ihr Traum,", "tokens": ["Die", "Er\u00b7de", ",", "nicht", "der", "Him\u00b7mel", ",", "war", "ihr", "Traum", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PTKNEG", "ART", "NN", "$,", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Erst sp\u00e4ter lernte sie das dumme Knieen;", "tokens": ["Erst", "sp\u00e4\u00b7ter", "lern\u00b7te", "sie", "das", "dum\u00b7me", "Kni\u00b7e\u00b7en", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VVFIN", "PPER", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+--", "measure": "unknown.measure.penta"}, "line.3": {"text": "Sie spann nicht graue Theorieen,", "tokens": ["Sie", "spann", "nicht", "grau\u00b7e", "The\u00b7o\u00b7ri\u00b7een", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ADJA", "NN", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihr Leben war ein gr\u00fcner Baum.", "tokens": ["Ihr", "Le\u00b7ben", "war", "ein", "gr\u00fc\u00b7ner", "Baum", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Doch das ist lange, o schon lange her,", "tokens": ["Doch", "das", "ist", "lan\u00b7ge", ",", "o", "schon", "lan\u00b7ge", "her", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "ADV", "$,", "FM", "ADV", "ADV", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Die Opferschalen fielen und zerklirrten,", "tokens": ["Die", "Op\u00b7fer\u00b7scha\u00b7len", "fie\u00b7len", "und", "zer\u00b7klirr\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Und heut t\u00f6nt nur das Lied der Hirten", "tokens": ["Und", "heut", "t\u00f6nt", "nur", "das", "Lied", "der", "Hir\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "ADV", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Noch n\u00e4chtlich \u00fcbers Mittelmeer.", "tokens": ["Noch", "n\u00e4cht\u00b7lich", "\u00fc\u00b7bers", "Mit\u00b7tel\u00b7meer", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Das Volk des Perikles gab sich den Rest,", "tokens": ["Das", "Volk", "des", "Pe\u00b7rik\u00b7les", "gab", "sich", "den", "Rest", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "VVFIN", "PRF", "ART", "NN", "$,"], "meter": "-+--+-+--+", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Doch w\u00e4chst und bl\u00fcht der Stammbaum des Eum\u00e4us \u2014", "tokens": ["Doch", "w\u00e4chst", "und", "bl\u00fcht", "der", "Stamm\u00b7baum", "des", "Eu\u00b7m\u00e4us"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KON", "VVFIN", "ART", "NN", "ART", "NE", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Heut ist die Weltstadt am Pyr\u00e4us", "tokens": ["Heut", "ist", "die", "Welt\u00b7stadt", "am", "Py\u00b7r\u00e4us"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "NN", "APPRART", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ein elendes Barackennest!", "tokens": ["Ein", "e\u00b7len\u00b7des", "Ba\u00b7ra\u00b7cken\u00b7nest", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}}, "stanza.8": {"line.1": {"text": "Zwar ist der Himmel noch wie ehmals blau,", "tokens": ["Zwar", "ist", "der", "Him\u00b7mel", "noch", "wie", "eh\u00b7mals", "blau", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADV", "KOKOM", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der Urwald harft noch und das Weltmeer psaltert,", "tokens": ["Der", "Ur\u00b7wald", "harft", "noch", "und", "das", "Welt\u00b7meer", "psal\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADV", "KON", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Doch ach, die Menschheit hat gealtert", "tokens": ["Doch", "ach", ",", "die", "Menschheit", "hat", "ge\u00b7al\u00b7tert"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "XY", "$,", "ART", "NN", "VAFIN", "VVPP"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und pinselt nur noch grau in grau!", "tokens": ["Und", "pin\u00b7selt", "nur", "noch", "grau", "in", "grau", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADJD", "APPR", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Der Sch\u00f6nheit goldner Springquell ist versiegt,", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "gold\u00b7ner", "Spring\u00b7quell", "ist", "ver\u00b7siegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fcrwahr, wir leben in der Zeit des Spottes,", "tokens": ["F\u00fcr\u00b7wahr", ",", "wir", "le\u00b7ben", "in", "der", "Zeit", "des", "Spot\u00b7tes", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Da selbst die heilge Mutter Gottes", "tokens": ["Da", "selbst", "die", "heil\u00b7ge", "Mut\u00b7ter", "Got\u00b7tes"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Auf Pflaumenb\u00e4ume kriecht!", "tokens": ["Auf", "Pflau\u00b7men\u00b7b\u00e4u\u00b7me", "kriecht", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "Drum zupft den Dichter nicht an seinem Kranz", "tokens": ["Drum", "zupft", "den", "Dich\u00b7ter", "nicht", "an", "sei\u00b7nem", "Kranz"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PAV", "VVFIN", "ART", "NN", "PTKNEG", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und titulirt ihn nicht gleich einen Narren,", "tokens": ["Und", "ti\u00b7tu\u00b7lirt", "ihn", "nicht", "gleich", "ei\u00b7nen", "Nar\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKNEG", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Denkt er umqualmt mal von Cigarren", "tokens": ["Denkt", "er", "um\u00b7qualmt", "mal", "von", "Ci\u00b7gar\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Der G\u00f6tterwelt Altgriechenlands.", "tokens": ["Der", "G\u00f6t\u00b7ter\u00b7welt", "Alt\u00b7grie\u00b7chen\u00b7lands", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}