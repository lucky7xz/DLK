{"dta.poem.7542": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "41.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1836", "urn": "urn:nbn:de:kobv:b4-200905195073", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ein rechter Mann hat zwei Gesichter, die er h\u00e4lt", "tokens": ["Ein", "rech\u00b7ter", "Mann", "hat", "zwei", "Ge\u00b7sich\u00b7ter", ",", "die", "er", "h\u00e4lt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "CARD", "NN", "$,", "PRELS", "PPER", "VVFIN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das eine auf sein Haus, das andre auf die Welt.", "tokens": ["Das", "ei\u00b7ne", "auf", "sein", "Haus", ",", "das", "and\u00b7re", "auf", "die", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "ART", "APPR", "PPOSAT", "NN", "$,", "PRELS", "PIS", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Das freundliche Gesicht, das wendet er in's Haus,", "tokens": ["Das", "freund\u00b7li\u00b7che", "Ge\u00b7sicht", ",", "das", "wen\u00b7det", "er", "in's", "Haus", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "PDS", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Das ernste aber kehrt er in die Welt hinaus.", "tokens": ["Das", "erns\u00b7te", "a\u00b7ber", "kehrt", "er", "in", "die", "Welt", "hin\u00b7aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "PTKVZ", "$."], "meter": "