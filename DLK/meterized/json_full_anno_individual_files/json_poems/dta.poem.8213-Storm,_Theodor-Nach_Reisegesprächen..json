{"dta.poem.8213": {"metadata": {"author": {"name": "Storm, Theodor", "birth": "N.A.", "death": "N.A."}, "title": "Nach Reisegespr\u00e4chen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1852", "urn": "urn:nbn:de:kobv:b4-200905196098", "language": ["de:0.99"], "booktitle": "Storm, Theodor: Gedichte. Kiel, 1852."}, "poem": {"stanza.1": {"line.1": {"text": "Durch die deutschen Nebelschichten,", "tokens": ["Durch", "die", "deut\u00b7schen", "Ne\u00b7bel\u00b7schich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Als auf alten Tr\u00e4umen reiten", "tokens": ["Als", "auf", "al\u00b7ten", "Tr\u00e4u\u00b7men", "rei\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "APPR", "ADJA", "NN", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und auf r\u00f6mischen Berichten!", "tokens": ["Und", "auf", "r\u00f6\u00b7mi\u00b7schen", "Be\u00b7rich\u00b7ten", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ADJA", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.4": {"text": "Denn mir ist, als s\u00e4h' ich endlich", "tokens": ["Denn", "mir", "ist", ",", "als", "s\u00e4h'", "ich", "end\u00b7lich"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VAFIN", "$,", "KOUS", "VVFIN", "PPER", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unter uns ein Bild entfalten;", "tokens": ["Un\u00b7ter", "uns", "ein", "Bild", "ent\u00b7fal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Dunkel erst, doch bald verst\u00e4ndlich", "tokens": ["Dun\u00b7kel", "erst", ",", "doch", "bald", "ver\u00b7st\u00e4nd\u00b7lich"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ADJD", "ADV", "$,", "ADV", "ADV", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Sich erheben die Gestalten;", "tokens": ["Sich", "er\u00b7he\u00b7ben", "die", "Ge\u00b7stal\u00b7ten", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Hauf' an Haufen im Get\u00fcmmel,", "tokens": ["Hauf'", "an", "Hau\u00b7fen", "im", "Ge\u00b7t\u00fcm\u00b7mel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Nun zerrissen, nun zusammen;", "tokens": ["Nun", "zer\u00b7ris\u00b7sen", ",", "nun", "zu\u00b7sam\u00b7men", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVPP", "$,", "ADV", "PTKVZ", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "An dem grau verhangnen Himmel", "tokens": ["An", "dem", "grau", "ver\u00b7hang\u00b7nen", "Him\u00b7mel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Zuckt es wie von tausend Flammen.", "tokens": ["Zuckt", "es", "wie", "von", "tau\u00b7send", "Flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOKOM", "APPR", "CARD", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "H\u00f6rt ihr, wie die B\u00fcchsen knallen?", "tokens": ["H\u00f6rt", "ihr", ",", "wie", "die", "B\u00fcch\u00b7sen", "knal\u00b7len", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "$,", "PWAV", "ART", "NN", "VVINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.13": {"text": "Wuthgeschrei durchfegt die L\u00fcfte;", "tokens": ["Wuth\u00b7ge\u00b7schrei", "durch\u00b7fegt", "die", "L\u00fcf\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.14": {"text": "Und die wei\u00dfen Nebel wallen,", "tokens": ["Und", "die", "wei\u00b7\u00dfen", "Ne\u00b7bel", "wal\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Und die Br\u00fcder stehn und fallen \u2014", "tokens": ["Und", "die", "Br\u00fc\u00b7der", "stehn", "und", "fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVINF", "KON", "VVINF", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Hoher Tag und tiefe Gr\u00fcfte!", "tokens": ["Ho\u00b7her", "Tag", "und", "tie\u00b7fe", "Gr\u00fcf\u00b7te", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}