{"textgrid.poem.47727": {"metadata": {"author": {"name": "Daumer, Georg Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "[ein Dirnchen ist so hochgemuth]", "genre": "verse", "period": "N.A.", "pub_year": 1837, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Dirnchen ist so hochgemuth,", "tokens": ["Ein", "Dirn\u00b7chen", "ist", "so", "hoch\u00b7ge\u00b7muth", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als sollte sie der ganzen Welt", "tokens": ["Als", "soll\u00b7te", "sie", "der", "gan\u00b7zen", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "PPER", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Obherrschen und gebieten.", "tokens": ["Ob\u00b7herr\u00b7schen", "und", "ge\u00b7bie\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Zu vornehm, um zu singen,", "tokens": ["Zu", "vor\u00b7nehm", ",", "um", "zu", "sin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Zu adelig, um zu springen,", "tokens": ["Zu", "a\u00b7de\u00b7lig", ",", "um", "zu", "sprin\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKA", "ADJD", "$,", "KOUI", "PTKZU", "VVINF", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Ja um zu sprechen, viel zu gut,", "tokens": ["Ja", "um", "zu", "spre\u00b7chen", ",", "viel", "zu", "gut", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "wor