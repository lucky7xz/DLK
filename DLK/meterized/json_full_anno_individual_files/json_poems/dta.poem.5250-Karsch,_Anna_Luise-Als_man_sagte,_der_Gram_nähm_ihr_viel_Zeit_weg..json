{"dta.poem.5250": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Als man sagte, der Gram n\u00e4hm  \n ihr viel Zeit weg.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-200905197778", "language": ["de:0.99"], "booktitle": "Karsch, Anna Luise: Auserlesene Gedichte. Berlin, 1764."}, "poem": {"stanza.1": {"line.1": {"text": "Gram! wilst du mir die Stunden nehmen?", "tokens": ["Gram", "!", "wilst", "du", "mir", "die", "Stun\u00b7den", "neh\u00b7men", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$.", "VMFIN", "PPER", "PPER", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sie sind mir k\u00e4rglich zugez\u00e4hlt:", "tokens": ["Sie", "sind", "mir", "k\u00e4rg\u00b7lich", "zu\u00b7ge\u00b7z\u00e4hlt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh Gram! du solltest dich doch sch\u00e4men!", "tokens": ["Geh", "Gram", "!", "du", "soll\u00b7test", "dich", "doch", "sch\u00e4\u00b7men", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Komm wieder, wenn mein Freund mir fehlt.", "tokens": ["Komm", "wie\u00b7der", ",", "wenn", "mein", "Freund", "mir", "fehlt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "$,", "KOUS", "PPOSAT", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}