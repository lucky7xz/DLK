{"dta.poem.8491": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Stille Gr\u00f6\u00dfe.", "genre": "Lyrik; Prosa", "period": "N.A.", "pub_year": "1860", "urn": "urn:nbn:de:kobv:b4-200905191007", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich klage nicht den Mann, der f&#228;llt", "tokens": ["Ich", "kla\u00b7ge", "nicht", "den", "Mann", ",", "der", "f", "&#228;", "llt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "ART", "NN", "$(", "VVFIN"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "Ein Markstein dem erk\u00e4mpften Land,", "tokens": ["Ein", "Mark\u00b7stein", "dem", "er\u00b7k\u00e4mpf\u00b7ten", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der seines Schicksals Becher h\u00e4lt,", "tokens": ["Der", "sei\u00b7nes", "Schick\u00b7sals", "Be\u00b7cher", "h\u00e4lt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ihn mischend mit entschlossner Hand,", "tokens": ["Ihn", "mi\u00b7schend", "mit", "ent\u00b7schloss\u00b7ner", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn, der entgegentritt dem Sturm", "tokens": ["Ihn", ",", "der", "ent\u00b7ge\u00b7gen\u00b7tritt", "dem", "Sturm"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "VVFIN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und wei\u00df, da\u00df er die Eiche bricht;", "tokens": ["Und", "wei\u00df", ",", "da\u00df", "er", "die", "Ei\u00b7che", "bricht", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "KOUS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Wer war so reich wie G\u00f6tz im Thurm,", "tokens": ["Wer", "war", "so", "reich", "wie", "G\u00f6tz", "im", "Thurm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADV", "ADJD", "KOKOM", "NE", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Morus vor dem Blutgericht?", "tokens": ["Wie", "Mo\u00b7rus", "vor", "dem", "Blut\u00b7ge\u00b7richt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NE", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich klage nicht den Mann, der stirbt,", "tokens": ["Ich", "kla\u00b7ge", "nicht", "den", "Mann", ",", "der", "stirbt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "ART", "NN", "$,", "PRELS", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Von Welt und eigner Glut verzehrt,", "tokens": ["Von", "Welt", "und", "eig\u00b7ner", "Glut", "ver\u00b7zehrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ihn, dem des Halmes Frucht verdirbt", "tokens": ["Ihn", ",", "dem", "des", "Hal\u00b7mes", "Frucht", "ver\u00b7dirbt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und den des Himmels Manna n\u00e4hrt;", "tokens": ["Und", "den", "des", "Him\u00b7mels", "Man\u00b7na", "n\u00e4hrt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "NE", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Correggio nicht, der siech und falb", "tokens": ["Cor\u00b7reg\u00b7gio", "nicht", ",", "der", "siech", "und", "falb"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "PTKNEG", "$,", "PRELS", "PPER", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Die Kupferheller heimgebracht,", "tokens": ["Die", "Kup\u00b7fer\u00b7hel\u00b7ler", "heim\u00b7ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Cervantes, der verhungert halb", "tokens": ["Cer\u00b7van\u00b7tes", ",", "der", "ver\u00b7hun\u00b7gert", "halb"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "VVFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ob seines Pansa noch gelacht.", "tokens": ["Ob", "sei\u00b7nes", "Pan\u00b7sa", "noch", "ge\u00b7lacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Sie sind des Ungl\u00fccks F\u00fcrsten, sind", "tokens": ["Sie", "sind", "des", "Un\u00b7gl\u00fccks", "F\u00fcrs\u00b7ten", ",", "sind"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die M\u00e4chtigen im weiten Blau,", "tokens": ["Die", "M\u00e4ch\u00b7ti\u00b7gen", "im", "wei\u00b7ten", "Blau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie f\u00fchlen, da\u00df ihr Odem rinnt", "tokens": ["Sie", "f\u00fch\u00b7len", ",", "da\u00df", "ihr", "O\u00b7dem", "rinnt"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "KOUS", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Entz\u00fcndend um der Erde Bau,", "tokens": ["Ent\u00b7z\u00fcn\u00b7dend", "um", "der", "Er\u00b7de", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df nur aus dunkler Scholle gern", "tokens": ["Da\u00df", "nur", "aus", "dunk\u00b7ler", "Schol\u00b7le", "gern"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und freudig schie\u00dft der Erndte Kraft,", "tokens": ["Und", "freu\u00b7dig", "schie\u00dft", "der", "Ernd\u00b7te", "Kraft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und da\u00df zerfallen mu\u00df der Kern,", "tokens": ["Und", "da\u00df", "zer\u00b7fal\u00b7len", "mu\u00df", "der", "Kern", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "VVINF", "VMFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Soll strecken sich der Palme Schaft.", "tokens": ["Soll", "stre\u00b7cken", "sich", "der", "Pal\u00b7me", "Schaft", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "VVFIN", "PRF", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Ihn klag\u2019 ich, dessen Liebe gro\u00df", "tokens": ["Ihn", "klag'", "ich", ",", "des\u00b7sen", "Lie\u00b7be", "gro\u00df"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "$,", "PRELAT", "NN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und dessen Gabe arm und klein,", "tokens": ["Und", "des\u00b7sen", "Ga\u00b7be", "arm", "und", "klein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRELAT", "NN", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Den, wie die Glut das d\u00fcrre Moos,", "tokens": ["Den", ",", "wie", "die", "Glut", "das", "d\u00fcr\u00b7re", "Moos", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PWAV", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sengt jener Strahlen Wiederschein;", "tokens": ["Sengt", "je\u00b7ner", "Strah\u00b7len", "Wie\u00b7der\u00b7schein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihn, der des Funkens Irren f\u00fchlt", "tokens": ["Ihn", ",", "der", "des", "Fun\u00b7kens", "Ir\u00b7ren", "f\u00fchlt"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Verzehrend in der Adern Bau,", "tokens": ["Ver\u00b7zeh\u00b7rend", "in", "der", "A\u00b7dern", "Bau", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und den die Welle dann versp\u00fchlt,", "tokens": ["Und", "den", "die", "Wel\u00b7le", "dann", "ver\u00b7sp\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Ein Aschenh\u00e4uflein, karg und grau.", "tokens": ["Ein", "A\u00b7schen\u00b7h\u00e4uf\u00b7lein", ",", "karg", "und", "grau", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "O, eure Zahl ist Legion!", "tokens": ["O", ",", "eu\u00b7re", "Zahl", "ist", "Le\u00b7gi\u00b7on", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "VAFIN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ihr Halbgesegneten, wo scheu", "tokens": ["Ihr", "Halb\u00b7ge\u00b7se\u00b7gne\u00b7ten", ",", "wo", "scheu"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$,", "PWAV", "ADJD"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "In\u2019s Herz der Genius gefloh\u2019n,", "tokens": ["In's", "Herz", "der", "Ge\u00b7nius", "ge\u00b7floh'n", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Und \u00f6de lie\u00df die Phantasei;", "tokens": ["Und", "\u00f6\u00b7de", "lie\u00df", "die", "Phan\u00b7ta\u00b7sei", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ihr, die ihr m\u00f6chtet fl\u00fcgellos", "tokens": ["Ihr", ",", "die", "ihr", "m\u00f6ch\u00b7tet", "fl\u00fc\u00b7gel\u00b7los"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "$,", "PRELS", "PPER", "VMFIN", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Euch schwingen mit des Sehnens Hauch,", "tokens": ["Euch", "schwin\u00b7gen", "mit", "des", "Seh\u00b7nens", "Hauch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und nieder an der Erde Schoo\u00df", "tokens": ["Und", "nie\u00b7der", "an", "der", "Er\u00b7de", "Schoo\u00df"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PTKVZ", "APPR", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sinkt, wie ein kranker Nebelrauch.", "tokens": ["Sinkt", ",", "wie", "ein", "kran\u00b7ker", "Ne\u00b7bel\u00b7rauch", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Nicht klag\u2019 ich euch, weil ihr gering,", "tokens": ["Nicht", "klag'", "ich", "euch", ",", "weil", "ihr", "ge\u00b7ring", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "PPER", "PPER", "$,", "KOUS", "PPER", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nicht weil ihr \u00e4rmlich und versiegt;", "tokens": ["Nicht", "weil", "ihr", "\u00e4rm\u00b7lich", "und", "ver\u00b7siegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKNEG", "KOUS", "PPER", "ADJD", "KON", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich wei\u00df es, da\u