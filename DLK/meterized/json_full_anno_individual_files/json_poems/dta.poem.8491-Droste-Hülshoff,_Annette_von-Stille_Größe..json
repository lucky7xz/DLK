 "stanza.7": {"line.1": {"text": "Wenn ihr am leeren Pulte neigt", "tokens": ["Wenn", "ihr", "am", "lee\u00b7ren", "Pul\u00b7te", "neigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und an der \u00f6den Staffelei,", "tokens": ["Und", "an", "der", "\u00f6\u00b7den", "Staf\u00b7fe\u00b7lei", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Um euch des Himmels Odem steigt", "tokens": ["Um", "euch", "des", "Him\u00b7mels", "O\u00b7dem", "steigt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUI", "PPER", "ART", "NN", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und in euch der Beklemmung Schrei;", "tokens": ["Und", "in", "euch", "der", "Be\u00b7klem\u00b7mung", "Schrei", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}, "line.5": {"text": "Wenn zitternd nach dem Ideal", "tokens": ["Wenn", "zit\u00b7ternd", "nach", "dem", "I\u00b7deal"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Ihr eure hei\u00dfen Arme streckt,", "tokens": ["Ihr", "eu\u00b7re", "hei\u00b7\u00dfen", "Ar\u00b7me", "streckt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PPOSAT", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und kaum f\u00fcr\u2019s n\u00e4chste Kummermahl", "tokens": ["Und", "kaum", "f\u00fcr's", "n\u00e4chs\u00b7te", "Kum\u00b7mer\u00b7mahl"], "token_info": ["word", "word", "wor