{"dta.poem.970": {"metadata": {"author": {"name": "George, Stefan", "birth": "N.A.", "death": "N.A."}, "title": "Reifefreuden", "genre": "Lyrik", "period": "N.A.", "pub_year": "1897", "urn": "urn:nbn:de:kobv:b4-200905191530", "language": ["de:0.99"], "booktitle": "George, Stefan: Das Jahr der Seele. Berlin, 1897."}, "poem": {"stanza.1": {"line.1": {"text": "Ein stolzes beben und ein reiches schallen                 ", "tokens": ["Ein", "stol\u00b7zes", "be\u00b7ben", "und", "ein", "rei\u00b7ches", "schal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "VVINF", "KON", "ART", "ADJA", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Durch sp&#228;ter erde schwere f&#252;lle strich &#183; &#183;                 ", "tokens": ["Durch", "sp", "&#228;", "ter", "er\u00b7de", "schwe\u00b7re", "f", "&#252;", "lle", "strich", "&#183;", "&#183;"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "XML_entity", "word", "word", "XML_entity", "XML_entity"], "pos": ["APPR", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "FM", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die kurzen worte waren kaum gefallen                 ", "tokens": ["Die", "kur\u00b7zen", "wor\u00b7te", "wa\u00b7ren", "kaum", "ge\u00b7fal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADV", "VVPP"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Als tiefer r&#252;hrung ruhe uns beschlich               ", "tokens": ["Als", "tie\u00b7fer", "r", "&#252;", "hrung", "ru\u00b7he", "uns", "be\u00b7schlich"], "token_info": ["word", "word", "word", "XML_entity", "word", "word", "word", "word"], "pos": ["KOUS", "ADJA", "NN", "$(", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Sie sanken hin wo sich am fruchtgel&#228;nder                 ", "tokens": ["Sie", "san\u00b7ken", "hin", "wo", "sich", "am", "frucht\u00b7gel", "&#228;", "nder"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "ADV", "PWAV", "PRF", "APPRART", "NN", "$(", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Der purpurschein im gelben schmelz verlor                 ", "tokens": ["Der", "pur\u00b7pur\u00b7schein", "im", "gel\u00b7ben", "schmelz", "ver\u00b7lor"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "APPRART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sie stiegen auf zum schmuck der h&#252;gelr&#228;nder                 ", "tokens": ["Sie", "stie\u00b7gen", "auf", "zum", "schmuck", "der", "h", "&#252;", "gelr", "&#228;", "nder"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "XML_entity", "word", "XML_entity", "word"], "pos": ["PPER", "VVFIN", "APPR", "APPRART", "NN", "ART", "FM", "FM", "FM", "FM", "FM"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Wo f&#252;r die dunkle lust die traube gor               ", "tokens": ["Wo", "f", "&#252;", "r", "die", "dunk\u00b7le", "lust", "die", "trau\u00b7be", "gor"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NE", "$(", "VVFIN", "ART", "ADJA", "NN", "ART", "ADJA", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Ich wagte dir nicht du nicht mir zu nahen                 ", "tokens": ["Ich", "wag\u00b7te", "dir", "nicht", "du", "nicht", "mir", "zu", "na\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "PTKNEG", "PPER", "PTKNEG", "PPER", "APPR", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Als schr&#228;ger strahl um unsre h&#228;upter schoss                 ", "tokens": ["Als", "schr", "&#228;", "ger", "strahl", "um", "uns\u00b7re", "h", "&#228;", "up\u00b7ter", "schoss"], "token_info": ["word", "word", "XML_entity", "word", "word", "word", "word", "word", "XML_entity", "word", "word"], "pos": ["KOUS", "ADJD", "$(", "ADJA", "NN", "APPR", "PPOSAT", "FM", "FM", "FM", "FM"], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Noch gar mit rede st\u00f6rend zu bejahen                 ", "tokens": ["Noch", "gar", "mit", "re\u00b7de", "st\u00f6\u00b7rend", "zu", "be\u00b7ja\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "APPR", "VVFIN", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Was jezt uns band was jedes stumm genoss               ", "tokens": ["Was", "jezt", "uns", "band", "was", "je\u00b7des", "stumm", "ge\u00b7noss"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "ADV", "PPER", "VVFIN", "PIS", "PIAT", "ADJD", "ADJA"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Und was in uns bei jenes tages r&#252;ste                 ", "tokens": ["Und", "was", "in", "uns", "bei", "je\u00b7nes", "ta\u00b7ges", "r", "&#252;", "ste"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "XML_entity", "word"], "pos": ["KON", "PWS", "APPR", "PPER", "APPR", "PDAT", "ADJA", "NN", "$(", "ADJA"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "Auf zu den veilchenfarbnen wolken klomm                 ", "tokens": ["Auf", "zu", "den", "veil\u00b7chen\u00b7farb\u00b7nen", "wol\u00b7ken", "klomm"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Was mehr als unsre tr&#228;ume und gel&#252;ste                 ", "tokens": ["Was", "mehr", "als", "uns\u00b7re", "tr", "&#228;", "u\u00b7me", "und", "gel", "&#252;", "ste"], "token_info": ["word", "word", "word", "word", "word", "XML_entity", "word", "word", "word", "XML_entity", "word"], "pos": ["PWS", "PIS", "KOKOM", "PPOSAT", "NN", "$(", "VVFIN", "KON", "NN", "$(", "ADJA"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "An diesem gluten-abend zart erglomm.               ", "tokens": ["An", "die\u00b7sem", "glu\u00b7ten\u00b7a\u00b7bend", "zart", "er\u00b7glomm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "ADJD", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}