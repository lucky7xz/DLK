{"textgrid.poem.49717": {"metadata": {"author": {"name": "Thoma, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Patriotismus", "genre": "verse", "period": "N.A.", "pub_year": 1894, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn der Abend sinkt nach hei\u00dfem Tage", "tokens": ["Wenn", "der", "A\u00b7bend", "sinkt", "nach", "hei\u00b7\u00dfem", "Ta\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "Und der B\u00fcrger das Gesch\u00e4ft beschlie\u00dft,", "tokens": ["Und", "der", "B\u00fcr\u00b7ger", "das", "Ge\u00b7sch\u00e4ft", "be\u00b7schlie\u00dft", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ART", "NN", "VVFIN", "$,"], "meter": "--+-+-+-+", "measure": "anapaest.init"}, "line.3": {"text": "\u00dcberlegt er sich, wo von der Plage", "tokens": ["\u00dc\u00b7ber\u00b7legt", "er", "sich", ",", "wo", "von", "der", "Pla\u00b7ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "PRF", "$,", "PWAV", "APPR", "ART", "NN"], "meter": "+-+-+++-+-", "measure": "unknown.measure.hexa"}, "line.4": {"text": "Ruhe und Erholung er genie\u00dft.", "tokens": ["Ru\u00b7he", "und", "Er\u00b7ho\u00b7lung", "er", "ge\u00b7nie\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "Er begibt sich in den Wirtschaftsgarten,", "tokens": ["Er", "be\u00b7gibt", "sich", "in", "den", "Wirt\u00b7schafts\u00b7gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "$,"], "meter": "+-+-+