{"textgrid.poem.24254": {"metadata": {"author": {"name": "Bierbaum, Otto Julius", "birth": "N.A.", "death": "N.A."}, "title": "Freundesbrief an einen Melancholischen", "genre": "verse", "period": "N.A.", "pub_year": 1887, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du klagst, mein Freund, und jammerst sehr,", "tokens": ["Du", "klagst", ",", "mein", "Freund", ",", "und", "jam\u00b7merst", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie elend dieses Leben w\u00e4r;", "tokens": ["Wie", "e\u00b7lend", "die\u00b7ses", "Le\u00b7ben", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sei nicht auszuhalten. \u2013", "tokens": ["Es", "sei", "nicht", "aus\u00b7zu\u00b7hal\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVIZU", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was klagst du denn? Es ist dein Recht,", "tokens": ["Was", "klagst", "du", "denn", "?", "Es", "ist", "dein", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bist du ein m\u00fcd und fauler Knecht,", "tokens": ["Bist", "du", "ein", "m\u00fcd", "und", "fau\u00b7ler", "Knecht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Dich g\u00e4nzlich auszuschalten.", "tokens": ["Dich", "g\u00e4nz\u00b7lich", "aus\u00b7zu\u00b7schal\u00b7ten", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "VVIZU", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Kauf dir, o Freund, ein Pistolet", "tokens": ["Kauf", "dir", ",", "o", "Freund", ",", "ein", "Pis\u00b7to\u00b7let"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["NN", "PPER", "$,", "FM", "NN", "$,", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schie\u00df dich tot, \u2013 hurra, juchhe!", "tokens": ["Und", "schie\u00df", "dich", "tot", ",", "\u2013", "hur\u00b7ra", ",", "juch\u00b7he", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "ADJD", "$,", "$(", "ITJ", "$,", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.9": {"text": "Dann bist du gleich gestorben.", "tokens": ["Dann", "bist", "du", "gleich", "ge\u00b7stor\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Doch macht des Pulvers Knallget\u00f6s", "tokens": ["Doch", "macht", "des", "Pul\u00b7vers", "Knall\u00b7ge\u00b7t\u00f6s"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ART", "NN", "NE"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Dich, weil nerv\u00f6s du bist, nerv\u00f6s,", "tokens": ["Dich", ",", "weil", "ner\u00b7v\u00f6s", "du", "bist", ",", "ner\u00b7v\u00f6s", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "$,", "KOUS", "NE", "PPER", "VAFIN", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Brauchst du nicht zu verzagen.", "tokens": ["Brauchst", "du", "nicht", "zu", "ver\u00b7za\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Ich wei\u00df ein Mittel ohne Knall,", "tokens": ["Ich", "wei\u00df", "ein", "Mit\u00b7tel", "oh\u00b7ne", "Knall", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ger\u00e4uschlos, prompt; f\u00fcr jeden Fall", "tokens": ["Ge\u00b7r\u00e4uschlos", ",", "prompt", ";", "f\u00fcr", "je\u00b7den", "Fall"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word"], "pos": ["NE", "$,", "VVFIN", "$.", "APPR", "PIAT", "NN"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.6": {"text": "Will ich dirs hiermit sagen:", "tokens": ["Will", "ich", "dirs", "hier\u00b7mit", "sa\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PIS", "PAV", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "O speise, Freundchen, Strychenin!", "tokens": ["O", "spei\u00b7se", ",", "Freund\u00b7chen", ",", "Stry\u00b7che\u00b7nin", "!"], "token_info": ["word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["NE", "VVFIN", "$,", "NN", "$,", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Das wird dich in den Himmel ziehn.", "tokens": ["Das", "wird", "dich", "in", "den", "Him\u00b7mel", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PRF", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Dort geigst du mit den Engeln.", "tokens": ["Dort", "geigst", "du", "mit", "den", "En\u00b7geln", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Falls aber, weil du heikel bist,", "tokens": ["Falls", "a\u00b7ber", ",", "weil", "du", "hei\u00b7kel", "bist", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "$,", "KOUS", "PPER", "ADJD", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Strychnin dir unsympathisch ist", "tokens": ["Strych\u00b7nin", "dir", "un\u00b7sym\u00b7pa\u00b7thisch", "ist"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "ADJD", "VAFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "(es schmeckt ein bi\u00dfchen fade),", "tokens": ["(", "es", "schmeckt", "ein", "bi\u00df\u00b7chen", "fa\u00b7de", ")", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "ART", "PIS", "VVFIN", "$(", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So brauchst du nicht gleich b\u00f6s zu sein;", "tokens": ["So", "brauchst", "du", "nicht", "gleich", "b\u00f6s", "zu", "sein", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "ADJD", "PTKZU", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Mir f\u00e4llt schon etwas andres ein:", "tokens": ["Mir", "f\u00e4llt", "schon", "et\u00b7was", "and\u00b7res", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "PIS", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Geh auf die Promenade", "tokens": ["Geh", "auf", "die", "Pro\u00b7me\u00b7na\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN"], "meter": "++-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Und h\u00e4nge dich an einen Ast.", "tokens": ["Und", "h\u00e4n\u00b7ge", "dich", "an", "ei\u00b7nen", "Ast", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Sobald du ausgezappelt hast,", "tokens": ["So\u00b7bald", "du", "aus\u00b7ge\u00b7zap\u00b7pelt", "hast", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVPP", "VAFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "H\u00e4ngst du f\u00fcr ewig stille.", "tokens": ["H\u00e4ngst", "du", "f\u00fcr", "e\u00b7wig", "stil\u00b7le", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJD", "VVFIN", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Wie? Kitzlig bist du an dem Hals?", "tokens": ["Wie", "?", "Kitz\u00b7lig", "bist", "du", "an", "dem", "Hals", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$.", "ADJD", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wohl, mein Geliebter! Diesesfalls", "tokens": ["Wohl", ",", "mein", "Ge\u00b7lieb\u00b7ter", "!", "Die\u00b7ses\u00b7falls"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "PPOSAT", "NN", "$.", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Gilts anderes Gebaren:", "tokens": ["Gilts", "an\u00b7de\u00b7res", "Ge\u00b7ba\u00b7ren", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "ADJA", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Spring in den Flu\u00df, st\u00fcrz dich vom Turm,", "tokens": ["Spring", "in", "den", "Flu\u00df", ",", "st\u00fcrz", "dich", "vom", "Turm", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "NN", "$,", "VVFIN", "PPER", "APPRART", "NN", "$,"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.5": {"text": "La\u00df dich gleich einem Regenwurm", "tokens": ["La\u00df", "dich", "gleich", "ei\u00b7nem", "Re\u00b7gen\u00b7wurm"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Elektrisch \u00fcberfahren.", "tokens": ["E\u00b7lekt\u00b7risch", "\u00fc\u00b7berf\u00b7ah\u00b7ren", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auch ist ein ziemlich sichrer Tod", "tokens": ["Auch", "ist", "ein", "ziem\u00b7lich", "sich\u00b7rer", "Tod"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Der durch komplette Atemnot", "tokens": ["Der", "durch", "kom\u00b7plet\u00b7te", "A\u00b7tem\u00b7not"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Infolge Ofengasen.", "tokens": ["In\u00b7fol\u00b7ge", "O\u00b7fen\u00b7ga\u00b7sen", "."], "token_info": ["word", "word", "punct"], "pos": ["FM.la", "FM.la", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Du sch\u00fcttelst immer noch den Kopf?", "tokens": ["Du", "sch\u00fct\u00b7telst", "im\u00b7mer", "noch", "den", "Kopf", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Ei, du verruchter Sauertopf,", "tokens": ["Ei", ",", "du", "ver\u00b7ruch\u00b7ter", "Sau\u00b7er\u00b7topf", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Geh hin, dich zu purgieren!", "tokens": ["Geh", "hin", ",", "dich", "zu", "pur\u00b7gie\u00b7ren", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$,", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mach dir Bewegung, fauler Bauch,", "tokens": ["Mach", "dir", "Be\u00b7we\u00b7gung", ",", "fau\u00b7ler", "Bauch", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "NN", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "So wird die liebe Seele auch", "tokens": ["So", "wird", "die", "lie\u00b7be", "See\u00b7le", "auch"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Vergn\u00fcgt im Sein spazieren.", "tokens": ["Ver\u00b7gn\u00fcgt", "im", "Sein", "spa\u00b7zie\u00b7ren", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Ein wackres Wort hei\u00dft: resolut!", "tokens": ["Ein", "wack\u00b7res", "Wort", "hei\u00dft", ":", "re\u00b7so\u00b7lut", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$.", "ADJD", "$."], "meter": "-+-+---+", "measure": "unknown.measure.tri"}, "line.8": {"text": "Hast du zum Sterben nicht den Mut,", "tokens": ["Hast", "du", "zum", "Ster\u00b7ben", "nicht", "den", "Mut", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPRART", "NN", "PTKNEG", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "So lebe mit Courage!", "tokens": ["So", "le\u00b7be", "mit", "Cou\u00b7ra\u00b7ge", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.6": {"line.1": {"text": "Du klagst, mein Freund, und jammerst sehr,", "tokens": ["Du", "klagst", ",", "mein", "Freund", ",", "und", "jam\u00b7merst", "sehr", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPOSAT", "NN", "$,", "KON", "ADV", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wie elend dieses Leben w\u00e4r;", "tokens": ["Wie", "e\u00b7lend", "die\u00b7ses", "Le\u00b7ben", "w\u00e4r", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PDAT", "NN", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es sei nicht auszuhalten. \u2013", "tokens": ["Es", "sei", "nicht", "aus\u00b7zu\u00b7hal\u00b7ten", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "PTKNEG", "VVIZU", "$.", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Was klagst du denn? Es ist dein Recht,", "tokens": ["Was", "klagst", "du", "denn", "?", "Es", "ist", "dein", "Recht", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "$.", "PPER", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Bist du ein m\u00fcd und fauler Knecht,", "tokens": ["Bist", "d