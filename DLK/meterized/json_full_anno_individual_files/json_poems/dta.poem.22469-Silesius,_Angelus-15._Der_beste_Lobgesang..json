{"dta.poem.22469": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "15. Der beste Lobgesang.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1657", "urn": "urn:nbn:de:kobv:b4-200905198292", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Geistreiche Sinn- vnd Schlussrime. Wien, 1657."}, "poem": {"stanza.1": {"line.1": {"text": "Singt singt jhr Engel singt: Mit hundert tausend", "tokens": ["Singt", "singt", "jhr", "En\u00b7gel", "singt", ":", "Mit", "hun\u00b7dert", "tau\u00b7send"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "VVFIN", "$.", "APPR", "CARD", "CARD"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Zungen/", "tokens": ["Zun\u00b7gen", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Wird dieses wehrte Kind nicht w\u00fcrdiglich besungen.", "tokens": ["Wird", "die\u00b7ses", "wehr\u00b7te", "Kind", "nicht", "w\u00fcr\u00b7dig\u00b7lich", "be\u00b7sun\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PDAT", "ADJA", "NN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Ach m\u00f6cht\u2019 ich ohne Zung/ und ohne Stimme sein!", "tokens": ["Ach", "m\u00f6cht'", "ich", "oh\u00b7ne", "Zung", "/", "und", "oh\u00b7ne", "Stim\u00b7me", "sein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VMFIN", "PPER", "APPR", "NN", "$(", "KON", "APPR", "NN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ich wei\u00df ich s\u00e4ng\u2019 jhm straks da\u00df liebste Liedelein.", "tokens": ["Ich", "wei\u00df", "ich", "s\u00e4ng'", "jhm", "straks", "da\u00df", "liebs\u