{"dta.poem.11530": {"metadata": {"author": {"name": "M\u00f6rike, Eduard", "birth": "N.A.", "death": "N.A."}, "title": "Liebesvorzeichen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1838", "urn": "urn:nbn:de:kobv:b4-200905193969", "language": ["de:0.99"], "booktitle": "M\u00f6rike, Eduard: Gedichte. Stuttgart, 1838."}, "poem": {"stanza.1": {"line.1": {"text": "Vor dem Granatbaum sinnend still;", "tokens": ["Vor", "dem", "Gra\u00b7nat\u00b7baum", "sin\u00b7nend", "still", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJD", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Mir war, als m\u00fc\u00dft' ich gleich erwarten,", "tokens": ["Mir", "war", ",", "als", "m\u00fc\u00dft'", "ich", "gleich", "er\u00b7war\u00b7ten", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOUS", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ob er die Knospe sprengen will.", "tokens": ["Ob", "er", "die", "Knos\u00b7pe", "spren\u00b7gen", "will", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Sie aber schien es nicht zu wissen,", "tokens": ["Sie", "a\u00b7ber", "schien", "es", "nicht", "zu", "wis\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie m\u00e4chtig ihr die F\u00fclle schwoll,", "tokens": ["Wie", "m\u00e4ch\u00b7tig", "ihr", "die", "F\u00fcl\u00b7le", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df sie in den Feuerk\u00fcssen", "tokens": ["Und", "da\u00df", "sie", "in", "den", "Feu\u00b7er\u00b7k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des goldnen Tages brennen soll.", "tokens": ["Des", "gold\u00b7nen", "Ta\u00b7ges", "bren\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Und dort am Rasen lag Jorinde;", "tokens": ["Und", "dort", "am", "Ra\u00b7sen", "lag", "Jo\u00b7rin\u00b7de", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "APPRART", "NN", "VVFIN", "NE", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie schnell bin ich zum Gru\u00df bereit,", "tokens": ["Wie", "schnell", "bin", "ich", "zum", "Gru\u00df", "be\u00b7reit", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "VAFIN", "PPER", "APPRART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Inde\u00df sie sich nur erst geschwinde", "tokens": ["In\u00b7de\u00df", "sie", "sich", "nur", "erst", "ge\u00b7schwin\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "ADV", "ADV", "ADJA"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Den Schlummer aus den Augen streut!", "tokens": ["Den", "Schlum\u00b7mer", "aus", "den", "Au\u00b7gen", "streut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Dann leuchtet dieser Augen Schw\u00e4rze", "tokens": ["Dann", "leuch\u00b7tet", "die\u00b7ser", "Au\u00b7gen", "Schw\u00e4r\u00b7ze"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PDAT", "NN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Mich an in Lieb' und guter Ruh,", "tokens": ["Mich", "an", "in", "Lieb'", "und", "gu\u00b7ter", "Ruh", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "APPR", "APPR", "NN", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie h\u00f6rt dem Muthwill meiner Scherze", "tokens": ["Sie", "h\u00f6rt", "dem", "Mut\u00b7hwill", "mei\u00b7ner", "Scher\u00b7ze"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Mit kindischem Verwundern zu.", "tokens": ["Mit", "kin\u00b7di\u00b7schem", "Ver\u00b7wun\u00b7dern", "zu", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dazwischen dacht' ich wohl im Stillen:", "tokens": ["Da\u00b7zwi\u00b7schen", "dacht'", "ich", "wohl", "im", "Stil\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "ADV", "APPRART", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Du gut und unerfahren Kind!", "tokens": ["Du", "gut", "und", "un\u00b7er\u00b7fah\u00b7ren", "Kind", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJD", "KON", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Lippen, die von Reife quillen,", "tokens": ["Die", "Lip\u00b7pen", ",", "die", "von", "Rei\u00b7fe", "quil\u00b7len", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie bl\u00f6de noch und fromm gesinnt!", "tokens": ["Wie", "bl\u00f6\u00b7de", "noch", "und", "fromm", "ge\u00b7sinnt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJA", "ADV", "KON", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "F\u00fcrwahr, sie schien es nicht zu wissen,", "tokens": ["F\u00fcr\u00b7wahr", ",", "sie", "schien", "es", "nicht", "zu", "wis\u00b7sen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wie m\u00e4chtig ihr die F\u00fclle schwoll,", "tokens": ["Wie", "m\u00e4ch\u00b7tig", "ihr", "die", "F\u00fcl\u00b7le", "schwoll", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PPER", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und da\u00df sie in den Feuerk\u00fcssen", "tokens": ["Und", "da\u00df", "sie", "in", "den", "Feu\u00b7er\u00b7k\u00fcs\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "KOUS", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Des wildsten Knaben brennen soll.", "tokens": ["Des", "wilds\u00b7ten", "Kna\u00b7ben", "bren\u00b7nen", "soll", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Still \u00fcberlegt' ich auf und nieder,", "tokens": ["Still", "\u00fc\u00b7ber\u00b7legt'", "ich", "auf", "und", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "PPER", "PTKVZ", "KON", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und ging so meiner Wege fort,", "tokens": ["Und", "ging", "so", "mei\u00b7ner", "We\u00b7ge", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Doch schon der n\u00e4chste Morgen wieder", "tokens": ["Doch", "schon", "der", "n\u00e4chs\u00b7te", "Mor\u00b7gen", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ART", "ADJA", "NN", "ADV"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Fand mich an dem Granatbaum dort.", "tokens": ["Fand", "mich", "an", "dem", "Gra\u00b7nat\u00b7baum", "dort", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "ART", "NN", "ADV", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}}, "stanza.8": {"line.1": {"text": "Wer hat dem Baum in wenig Stunden", "tokens": ["Wer", "hat", "dem", "Baum", "in", "we\u00b7nig", "Stun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "ART", "NN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein solches Wunder angethan?", "tokens": ["Ein", "sol\u00b7ches", "Wun\u00b7der", "an\u00b7ge\u00b7than", "?"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Die Flammenkrone aufgebunden?", "tokens": ["Die", "Flam\u00b7men\u00b7kro\u00b7ne", "auf\u00b7ge\u00b7bun\u00b7den", "?"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und was sagt mir dies Zeichen an?", "tokens": ["Und", "was", "sagt", "mir", "dies", "Zei\u00b7chen", "an", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "VVFIN", "PPER", "PDS", "NN", "PTKVZ", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.9": {"line.1": {"text": "Ich eile rasch den Gang hinunter,", "tokens": ["Ich", "ei\u00b7le", "rasch", "den", "Gang", "hin\u00b7un\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Dort geht das Kind im Morgenstrahl,", "tokens": ["Dort", "geht", "das", "Kind", "im", "Mor\u00b7gen\u00b7strahl", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und bald, o Wunder \u00fcber Wunder!", "tokens": ["Und", "bald", ",", "o", "Wun\u00b7der", "\u00fc\u00b7ber", "Wun\u00b7der", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "FM", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wir k\u00fc\u00dften uns zum ersten Mal!", "tokens": ["Wir", "k\u00fc\u00df\u00b7ten", "uns", "zum", "ers\u00b7ten", "Mal", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Nun trieb der Baum wohl Bl\u00fcth' auf Bl\u00fcthe", "tokens": ["Nun", "trieb", "der", "Baum", "wohl", "Bl\u00fcth'", "auf", "Bl\u00fc\u00b7the"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "NN", "APPR", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Frisch in die blaue Luft hinaus,", "tokens": ["Frisch", "in", "die", "blau\u00b7e", "Luft", "hin\u00b7aus", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "ADJA", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und noch, seitdem er lang vergl\u00fchte,", "tokens": ["Und", "noch", ",", "seit\u00b7dem", "er", "lang", "ver\u00b7gl\u00fch\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "KOUS", "PPER", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Ging uns das K\u00fcssen nimmer aus.", "tokens": ["Ging", "uns", "das", "K\u00fcs\u00b7sen", "nim\u00b7mer", "aus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}