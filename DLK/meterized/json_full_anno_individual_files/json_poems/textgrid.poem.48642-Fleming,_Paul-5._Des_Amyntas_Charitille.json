{"textgrid.poem.48642": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "5. Des Amyntas Charitille", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mu\u00df ", "tokens": ["Mu\u00df"], "token_info": ["word"], "pos": ["VMFIN"], "meter": "+", "measure": "single.up"}, "line.2": {"text": "Und hat kein Gott k\u00f6nnen wehren", "tokens": ["Und", "hat", "kein", "Gott", "k\u00f6n\u00b7nen", "weh\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "PIAT", "NN", "VMFIN", "VVINF"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "dieser dein- und unsrer Not?", "tokens": ["die\u00b7ser", "dein", "und", "uns\u00b7rer", "Not", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "TRUNC", "KON", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Ach, Verh\u00e4ngn\u00fc\u00df, was du bist!", "tokens": ["Ach", ",", "Ver\u00b7h\u00e4ng\u00b7n\u00fc\u00df", ",", "was", "du", "bist", "!"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "NN", "$,", "PWS", "PPER", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Was reizt dich zu solcher List?", "tokens": ["Was", "reizt", "dich", "zu", "sol\u00b7cher", "List", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PRF", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "t\u00f6net diese Seufzer an,", "tokens": ["t\u00f6\u00b7net", "die\u00b7se", "Seuf\u00b7zer", "an", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDAT", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "die ich ihr zur letzte stelle,", "tokens": ["die", "ich", "ihr", "zur", "letz\u00b7te", "stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PRELS", "PPER", "PPER", "APPRART", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "weil ich selbst nicht da sein kan,", "tokens": ["weil", "ich", "selbst", "nicht", "da", "sein", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "PTKNEG", "ADV", "VAINF", "VMFIN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "und auch, o ", "tokens": ["und", "auch", ",", "o"], "token_info": ["word", "word", "punct", "word"], "pos": ["KON", "ADV", "$,", "FM"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "gieb dein Beileid auch darzu!", "tokens": ["gieb", "dein", "Bei\u00b7leid", "auch", "dar\u00b7zu", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPOSAT", "NN", "ADV", "PAV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich, wie weit ich von euch stehe,", "tokens": ["Ich", ",", "wie", "weit", "ich", "von", "euch", "ste\u00b7he", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PWAV", "ADJD", "PPER", "APPR", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "bin doch euren Schmerzen nah.", "tokens": ["bin", "doch", "eu\u00b7ren", "Schmer\u00b7zen", "nah", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo ich sitze, wo ich gehe,", "tokens": ["Wo", "ich", "sit\u00b7ze", ",", "wo", "ich", "ge\u00b7he", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "da ist stets di\u00df Herzleid da.", "tokens": ["da", "ist", "stets", "di\u00df", "Herz\u00b7leid", "da", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ADV", "PDS", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Wo ich bin und werde sein,", "tokens": ["Wo", "ich", "bin", "und", "wer\u00b7de", "sein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VAFIN", "KON", "VAFIN", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "da will ich mit stimmen ein.", "tokens": ["da", "will", "ich", "mit", "stim\u00b7men", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "APPR", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Ach! Die Sch\u00f6nste von den Sch\u00f6nen,", "tokens": ["Ach", "!", "Die", "Sch\u00f6ns\u00b7te", "von", "den", "Sch\u00f6\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "was gebiert sie uns f\u00fcr Sehnen?", "tokens": ["was", "ge\u00b7biert", "sie", "uns", "f\u00fcr", "Seh\u00b7nen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "PRF", "APPR", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Was ist \u00fcbrig mehr als Leid?", "tokens": ["Was", "ist", "\u00fcb\u00b7rig", "mehr", "als", "Leid", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "ADJD", "ADV", "KOUS", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.4": {"text": "Die so lieblich singen kunt',", "tokens": ["Die", "so", "lieb\u00b7lich", "sin\u00b7gen", "kunt'", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ADJD", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "hat verschlossen ihren Mund.", "tokens": ["hat", "ver\u00b7schlos\u00b7sen", "ih\u00b7ren", "Mund", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "VVPP", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Solte sie denn uns nicht tauren,", "tokens": ["Sol\u00b7te", "sie", "denn", "uns", "nicht", "tau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ADV", "PPER", "PTKNEG", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "sie, der sch\u00f6nen W\u00e4lder Geist?", "tokens": ["sie", ",", "der", "sch\u00f6\u00b7nen", "W\u00e4l\u00b7der", "Geist", "?"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "ART", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Alle Felder sehn wir trauren,", "tokens": ["Al\u00b7le", "Fel\u00b7der", "sehn", "wir", "trau\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "der Gep\u00fcsche Pracht verschlei\u00dft,", "tokens": ["der", "Ge\u00b7p\u00fcsc\u00b7he", "Pracht", "ver\u00b7schlei\u00dft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "das verlebte Jahr wird alt:", "tokens": ["das", "ver\u00b7leb\u00b7te", "Jahr", "wird", "alt", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "sie, sein Feuer, die ist kalt.", "tokens": ["sie", ",", "sein", "Feu\u00b7er", ",", "die", "ist", "kalt", "."], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "PRELS", "VAFIN", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ists nicht so, du Gott der Schafe?", "tokens": ["Ists", "nicht", "so", ",", "du", "Gott", "der", "Scha\u00b7fe", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKNEG", "ADV", "$,", "PPER", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fr\u00fch' hab' ich dirs gleich erz\u00e4hlt,", "tokens": ["Fr\u00fch'", "hab'", "ich", "dirs", "gleich", "er\u00b7z\u00e4hlt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "PIS", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da\u00df ich sie sah' in dem Schlafe", "tokens": ["da\u00df", "ich", "sie", "sah'", "in", "dem", "Schla\u00b7fe"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN", "APPR", "ART", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.4": {"text": "und den Man, den sie erw\u00e4hlt.", "tokens": ["und", "den", "Man", ",", "den", "sie", "er\u00b7w\u00e4hlt", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "PIS", "$,", "PRELS", "PPER", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Und wir Andern, wie wir sein,", "tokens": ["Und", "wir", "An\u00b7dern", ",", "wie", "wir", "sein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ADJA", "$,", "PWAV", "PPER", "VAINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "stimmten ihr ein Brautlied ein.", "tokens": ["stimm\u00b7ten", "ihr", "ein", "Braut\u00b7lied", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "O ihr ganz verlognen Tr\u00e4ume,", "tokens": ["O", "ihr", "ganz", "ver\u00b7log\u00b7nen", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPOSAT", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "ist euch unser Leid denn Lust?", "tokens": ["ist", "euch", "un\u00b7ser", "Leid", "denn", "Lust", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PPOSAT", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Euch auch, ihr bewegten B\u00e4ume,", "tokens": ["Euch", "auch", ",", "ihr", "be\u00b7weg\u00b7ten", "B\u00e4u\u00b7me", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "soll di\u00df \u00dcbel sein bewu\u00dft.", "tokens": ["soll", "di\u00df", "\u00dc\u00b7bel", "sein", "be\u00b7wu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PDS", "NN", "PPOSAT", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Schreit mir nach, ihr T\u00e4ler ihr:", "tokens": ["Schreit", "mir", "nach", ",", "ihr", "T\u00e4\u00b7ler", "ihr", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PTKVZ", "$,", "PPOSAT", "NN", "PPOSAT", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.6": {"text": "Sie ist weg, der Menschen Zier!", "tokens": ["Sie", "ist", "weg", ",", "der", "Men\u00b7schen", "Zier", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "ART", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.8": {"line.1": {"text": "Ihr, ihr \u00fcbrigen drei Lieben,", "tokens": ["Ihr", ",", "ihr", "\u00fcb\u00b7ri\u00b7gen", "drei", "Lie\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "ADJA", "CARD", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "weint! doch weinet, wie ihr solt!", "tokens": ["weint", "!", "doch", "wei\u00b7net", ",", "wie", "ihr", "solt", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$.", "ADV", "VVFIN", "$,", "PWAV", "PPER", "VMFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sie bleibt, ist sie einmal blieben.", "tokens": ["Sie", "bleibt", ",", "ist", "sie", "ein\u00b7mal", "blie\u00b7ben", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VAFIN", "PPER", "ADV", "VVFIN", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Folgt, seid ihr ", "tokens": ["Folgt", ",", "seid", "ihr"], "token_info": ["word", "punct", "word", "word"], "pos": ["VVFIN", "$,", "VAFIN", "PPER"], "meter": "++-", "measure": "unknown.measure.di"}, "line.5": {"text": "da\u00df er an der M\u00fcnde Stat", "tokens": ["da\u00df", "er", "an", "der", "M\u00fcn\u00b7de", "Stat"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER",