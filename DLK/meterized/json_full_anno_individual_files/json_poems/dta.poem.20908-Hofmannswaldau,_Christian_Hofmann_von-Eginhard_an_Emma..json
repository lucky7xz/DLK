{"dta.poem.20908": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Eginhard an Emma.", "genre": "Lyrik; Prosa; Drama", "period": "N.A.", "pub_year": "1679", "urn": "urn:nbn:de:kobv:b4-20289-1", "language": ["de:0.85", "nl:0.14"], "booktitle": "Hofmann von Hofmannswaldau, Christian: Deutsche Ubersetzungen und Gedichte. Breslau, 1679."}, "poem": {"stanza.1": {"line.1": {"text": "Des grossen Carles Knecht ist die Gedult ent-", "tokens": ["Des", "gros\u00b7sen", "Car\u00b7les", "Knecht", "ist", "die", "Ge\u00b7dult", "ent"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NE", "NE", "VAFIN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "rissen/", "tokens": ["ris\u00b7sen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Ich schreibe was vielleicht mein Leben kosten", "tokens": ["Ich", "schrei\u00b7be", "was", "viel\u00b7leicht", "mein", "Le\u00b7ben", "kos\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PIS", "ADV", "PPOSAT", "NN", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "kan/", "tokens": ["kan", "/"], "token_info": ["word", "punct"], "pos": ["VMFIN", "$("], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Doch darf ich nur einmahl dein sch\u00f6nes Auge k\u00fcssen/", "tokens": ["Doch", "darf", "ich", "nur", "ein\u00b7mahl", "dein", "sch\u00f6\u00b7nes", "Au\u00b7ge", "k\u00fcs\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VMFIN", "PPER", "ADV", "ADV", "PPOSAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So trett ich wohl vergn\u00fcgt hernach die Marter an.", "tokens": ["So", "trett", "ich", "wohl", "ver\u00b7gn\u00fcgt", "her\u00b7nach", "die", "Mar\u00b7ter", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Dein hoher Purpur l\u00e4st mich nicht vom Tode dencken/", "tokens": ["Dein", "ho\u00b7her", "Pur\u00b7pur", "l\u00e4st", "mich", "nicht", "vom", "To\u00b7de", "den\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "PPER", "PTKNEG", "APPRART", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die steiffe Zuversicht streicht allen Kummer hin:", "tokens": ["Die", "steif\u00b7fe", "Zu\u00b7ver\u00b7sicht", "streicht", "al\u00b7len", "Kum\u00b7mer", "hin", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "PIAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Beliebt dir einen Blick auf meinen Brief zulencken/", "tokens": ["Be\u00b7liebt", "dir", "ei\u00b7nen", "Blick", "auf", "mei\u00b7nen", "Brief", "zu\u00b7len\u00b7cken", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "So mein ich/ da\u00df ich schon der Sonne gleiche bin.", "tokens": ["So", "mein", "ich", "/", "da\u00df", "ich", "schon", "der", "Son\u00b7ne", "glei\u00b7che", "bin", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "PPER", "$(", "KOUS", "PPER", "ADV", "ART", "NN", "ADJA", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Mein Fr\u00e4ulein straffe nicht mein eyfriges Beginnen/", "tokens": ["Mein", "Fr\u00e4u\u00b7lein", "straf\u00b7fe", "nicht", "mein", "ey\u00b7fri\u00b7ges", "Be\u00b7gin\u00b7nen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PTKNEG", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und rei\u00df das treue Blat nicht vor der Zeit entzwey/", "tokens": ["Und", "rei\u00df", "das", "treu\u00b7e", "Blat", "nicht", "vor", "der", "Zeit", "ent\u00b7zwey", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "APPR", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Erwege vor die Noth und Schwachheit meiner Sin-", "tokens": ["Er\u00b7we\u00b7ge", "vor", "die", "Noth", "und", "Schwach\u00b7heit", "mei\u00b7ner", "Sin"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN", "KON", "NN", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "nen/", "tokens": ["nen", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.15": {"text": "Hernach mach einen Spruch/ ob ich zutadlen sey.", "tokens": ["Her\u00b7nach", "mach", "ei\u00b7nen", "Spruch", "/", "ob", "ich", "zu\u00b7tad\u00b7len", "sey", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$(", "KOUS", "PPER", "VVINF", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Ich wei\u00df/ das meine Glutt sich denckt zu hoch zuheben/", "tokens": ["Ich", "wei\u00df", "/", "das", "mei\u00b7ne", "Glutt", "sich", "denckt", "zu", "hoch", "zu\u00b7he\u00b7ben", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PDS", "PPOSAT", "NN", "PRF", "VVFIN", "PTKA", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und da\u00df mein Kieselstein zu Diamanten wil/", "tokens": ["Und", "da\u00df", "mein", "Kie\u00b7sel\u00b7stein", "zu", "Di\u00b7a\u00b7man\u00b7ten", "wil", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPOSAT", "NN", "APPR", "NN", "VMFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Doch die Erfahrung wird vor mich die Antwort ge-", "tokens": ["Doch", "die", "Er\u00b7fah\u00b7rung", "wird", "vor", "mich", "die", "Ant\u00b7wort", "ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "VAFIN", "APPR", "PPER", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.19": {"text": "ben/", "tokens": ["ben", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.20": {"text": "Der St\u00e4nde gleichheit ist der Liebe Possenspiel;", "tokens": ["Der", "St\u00e4n\u00b7de", "gleich\u00b7heit", "ist", "der", "Lie\u00b7be", "Pos\u00b7sen\u00b7spiel", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "VAFIN", "ART", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Sie bindet Gold an Stahl und Garn zu weisser Sey-", "tokens": ["Sie", "bin\u00b7det", "Gold", "an", "Stahl", "und", "Garn", "zu", "weis\u00b7ser", "Sey"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NN", "APPR", "NN", "KON", "NN", "APPR", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "de/", "tokens": ["de", "/"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "-", "measure": "single.down"}, "line.23": {"text": "Macht da\u00df ein Nesselstrauch die edle Rose sucht/", "tokens": ["Macht", "da\u00df", "ein", "Nes\u00b7sel\u00b7strauch", "die", "ed\u00b7le", "Ro\u00b7se", "sucht", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KOUS", "ART", "NN", "ART", "ADJA", "NN", "VVFIN", "$("], "meter": "++-+-+-+-+-+", "measure": "unknown.measure.septa"}, "line.24": {"text": "Zu Perlen legt sie Graus/ zu Kohlen legt sie Kreyde/", "tokens": ["Zu", "Per\u00b7len", "legt", "sie", "Graus", "/", "zu", "Koh\u00b7len", "legt", "sie", "Krey\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "NN", "$(", "APPR", "NN", "VVFIN", "PPER", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Und pfropft auf wilden Baum offt eine s\u00fcsse Frucht.", "tokens": ["Und", "pfropft", "auf", "wil\u00b7den", "Baum", "offt", "ei\u00b7ne", "s\u00fcs\u00b7se", "Frucht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "ADV", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Sie lachet/ was die Welt von Blutverwandn\u00fc\u00df sa-", "tokens": ["Sie", "la\u00b7chet", "/", "was", "die", "Welt", "von", "Blut\u00b7ver\u00b7wand\u00b7n\u00fc\u00df", "sa"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$(", "PWS", "ART", "NN", "APPR", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "get/", "tokens": ["get", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.28": {"text": "Di\u00df was man Ehlich heist/ hemmt ihre Pfeile nicht/", "tokens": ["Di\u00df", "was", "man", "Eh\u00b7lich", "heist", "/", "hemmt", "ih\u00b7re", "Pfei\u00b7le", "nicht", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PIS", "ADJD", "VAFIN", "$(", "VVFIN", "PPOSAT", "NN", "PTKNEG", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "Der Keyser wird ihr Knecht/ der J\u00e4ger wird erja-", "tokens": ["Der", "Key\u00b7ser", "wird", "ihr", "Knecht", "/", "der", "J\u00e4\u00b7ger", "wird", "er\u00b7ja"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "VAFIN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "get/", "tokens": ["get", "/"], "token_info": ["word", "punct"], "pos": ["VVPP", "$("], "meter": "-", "measure": "single.down"}, "line.31": {"text": "Man sp\u00fchrt wie ihre Macht/ in Stock und Closter", "tokens": ["Man", "sp\u00fchrt", "wie", "ih\u00b7re", "Macht", "/", "in", "Stock", "und", "Clos\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "KOKOM", "PPOSAT", "NN", "$(", "APPR", "NN", "KON", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.32": {"text": "bricht;", "tokens": ["bricht", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}, "line.33": {"text": "Ich schreibe/ was ich mu\u00df/ ich steh itzund gebunden/", "tokens": ["Ich", "schrei\u00b7be", "/", "was", "ich", "mu\u00df", "/", "ich", "steh", "it\u00b7zund", "ge\u00b7bun\u00b7den", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "PWS", "PPER", "VMFIN", "$(", "PPER", "VVFIN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Die Zeile/ so du siehst/ will selbst nicht meine seyn/", "tokens": ["Die", "Zei\u00b7le", "/", "so", "du", "siehst", "/", "will", "selbst", "nicht", "mei\u00b7ne", "seyn", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "ADV", "PPER", "VVFIN", "$(", "VMFIN", "ADV", "PTKNEG", "PPOSAT", "VAINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Der Gott der alles kan/ der hat sie auch erfunden/", "tokens": ["Der", "Gott", "der", "al\u00b7les", "kan", "/", "der", "hat", "sie", "auch", "er\u00b7fun\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "PIS", "VMFIN", "$(", "ART", "VAFIN", "PPER", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Ich aber liefre sie dir nur gezwungen ein.", "tokens": ["Ich", "a\u00b7ber", "lief\u00b7re", "sie", "dir", "nur", "ge\u00b7zwun\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVFIN", "PPER", "PPER", "ADV", "VVPP", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Di\u00df was ich hier gesagt/ ist kein so frembdes Wesen/", "tokens": ["Di\u00df", "was", "ich", "hier", "ge\u00b7sagt", "/", "ist", "kein", "so", "fremb\u00b7des", "We\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PRELS", "PPER", "ADV", "VVPP", "$(", "VAFIN", "PIAT", "ADV", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Das Fieber so mich plagt/ ist dir genug bekant/", "tokens": ["Das", "Fie\u00b7ber", "so", "mich", "plagt", "/", "ist", "dir", "ge\u00b7nug", "be\u00b7kant", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PPER", "VVFIN", "$(", "VAFIN", "PPER", "ADV", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Au\u00df meinem Auge hat dein Auge schon gelesen/", "tokens": ["Au\u00df", "mei\u00b7nem", "Au\u00b7ge", "hat", "dein", "Au\u00b7ge", "schon", "ge\u00b7le\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "PPOSAT", "NN", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.40": {"text": "Wa\u00df sich vor Liebes Schrifft hier eingepr\u00e4gt befandt/", "tokens": ["Wa\u00df", "sich", "vor", "Lie\u00b7bes", "Schrifft", "hier", "ein\u00b7ge\u00b7pr\u00e4gt", "be\u00b7fandt", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "APPR", "ADJA", "NN", "ADV", "VVFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Vor Seufftzen kont ich offt kein rechtes Wort begin\u0303en/", "tokens": ["Vor", "Seufft\u00b7zen", "kont", "ich", "offt", "kein", "rech\u00b7tes", "Wort", "be\u00b7gi\u00f1en", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "PPER", "ADV", "PIAT", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.42": {"text": "Die Augen branten mir/ das Hertze ward mir kalt/", "tokens": ["Die", "Au\u00b7gen", "bran\u00b7ten", "mir", "/", "das", "Hert\u00b7ze", "ward", "mir", "kalt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "$(", "PDS", "VVFIN", "VAFIN", "PPER", "ADJD", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.43": {"text": "Die H\u00e4nde b\u00f6beten/ es irrten alle Sinnen/", "tokens": ["Die", "H\u00e4n\u00b7de", "b\u00f6\u00b7be\u00b7ten", "/", "es", "irr\u00b7ten", "al\u00b7le", "Sin\u00b7nen", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$(", "PPER", "VVFIN", "PIAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Ich war ein rechtes nichts/ an Farb und an Gestalt/", "tokens": ["Ich", "war", "ein", "rech\u00b7tes", "nichts", "/", "an", "Farb", "und", "an", "Ge\u00b7stalt", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "PIS", "$(", "APPR", "NN", "KON", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Du weist wie offtmahls ich der Zeilen Reyh verloh-", "tokens": ["Du", "weist", "wie", "offt\u00b7mahls", "ich", "der", "Zei\u00b7len", "Reyh", "ver\u00b7loh"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "KOKOM", "ADV", "PPER", "ART", "NN", "NN", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "ren/", "tokens": ["ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "-", "measure": "single.down"}, "line.47": {"text": "Wann ich dem grossen Carl ge