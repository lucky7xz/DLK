{"textgrid.poem.36533": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "5. Der Greis und der Tod", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Greis von achtundachtzig Jahren,", "tokens": ["Ein", "Greis", "von", "acht\u00b7un\u00b7dacht\u00b7zig", "Jah\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "CARD", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ein armer, abgelebter Greis,", "tokens": ["Ein", "ar\u00b7mer", ",", "ab\u00b7ge\u00b7leb\u00b7ter", "Greis", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mit wenigen schneewei\u00dfen Haaren,", "tokens": ["Mit", "we\u00b7ni\u00b7gen", "schnee\u00b7wei\u00b7\u00dfen", "Haa\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Kam aus dem Walde, trug auf seinem krummen R\u00fccken,", "tokens": ["Kam", "aus", "dem", "Wal\u00b7de", ",", "trug", "auf", "sei\u00b7nem", "krum\u00b7men", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "ART", "NN", "$,", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Ein B\u00fcndel Reis.", "tokens": ["Ein", "B\u00fcn\u00b7del", "Reis", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "Ach Gott! der arme Greis!", "tokens": ["Ach", "Gott", "!", "der", "ar\u00b7me", "Greis", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "NN", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Er mu\u00dfte wohl sehr oft sich b\u00fccken,", "tokens": ["Er", "mu\u00df\u00b7te", "wohl", "sehr", "oft", "sich", "b\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADV", "ADV", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Eh' er's zusammen las;", "tokens": ["Eh'", "er's", "zu\u00b7sam\u00b7men", "las", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ADV", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.4": {"text": "Er hatte keinen Sohn, sonst h\u00e4tte der's gethan!", "tokens": ["Er", "hat\u00b7te", "kei\u00b7nen", "Sohn", ",", "sonst", "h\u00e4t\u00b7te", "der's", "ge\u00b7than", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "ADV", "VAFIN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Und weil vor Mattigkeit er nun nicht weiter kann,", "tokens": ["Und", "weil", "vor", "Mat\u00b7tig\u00b7keit", "er", "nun", "nicht", "wei\u00b7ter", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "APPR", "NN", "PPER", "ADV", "PTKNEG", "ADV", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So setzt er's ab; und als er nun da sa\u00df,", "tokens": ["So", "setzt", "er's", "ab", ";", "und", "als", "er", "nun", "da", "sa\u00df", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIS", "PTKVZ", "$.", "KON", "KOUS", "PPER", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Bei seinem B\u00fcndel, und bedachte,", "tokens": ["Bei", "sei\u00b7nem", "B\u00fcn\u00b7del", ",", "und", "be\u00b7dach\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$,", "KON", "ADJA", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Wie viel Beschwerde, M\u00fch und Not", "tokens": ["Wie", "viel", "Be\u00b7schwer\u00b7de", ",", "M\u00fch", "und", "Not"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PWAV", "PIAT", "NN", "$,", "NN", "KON", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das B\u00fcndel Reis ihm machte,", "tokens": ["Das", "B\u00fcn\u00b7del", "Reis", "ihm", "mach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.6": {"text": "Wie viel sein bi\u00dfchen t\u00e4glich Brot;", "tokens": ["Wie", "viel", "sein", "bi\u00df\u00b7chen", "t\u00e4g\u00b7lich", "Brot", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VAINF", "PIS", "ADJD", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da seufzt' er, Lebens satt, und weint, und ruft den Tod;", "tokens": ["Da", "seufzt'", "er", ",", "Le\u00b7bens", "satt", ",", "und", "weint", ",", "und", "ruft", "den", "Tod", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "NN", "ADJD", "$,", "KON", "VVFIN", "$,", "KON", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.