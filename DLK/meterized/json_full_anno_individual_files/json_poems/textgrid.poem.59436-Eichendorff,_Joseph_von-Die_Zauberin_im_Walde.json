{"textgrid.poem.59436": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Die Zauberin im Walde", "genre": "verse", "period": "N.A.", "pub_year": 1808, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbschon vor vielen, vielen Jahren", "tokens": ["\u00bb", "schon", "vor", "vie\u00b7len", ",", "vie\u00b7len", "Jah\u00b7ren"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word"], "pos": ["$(", "ADV", "APPR", "PIAT", "$,", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sa\u00df ich dr\u00fcben an dem Ufer,", "tokens": ["Sa\u00df", "ich", "dr\u00fc\u00b7ben", "an", "dem", "U\u00b7fer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sah manch Schiff vor\u00fcberfahren", "tokens": ["Sah", "manch", "Schiff", "vor\u00b7\u00fc\u00b7berf\u00b7ah\u00b7ren"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Weit hinein ins Waldesdunkel.", "tokens": ["Weit", "hin\u00b7ein", "ins", "Wal\u00b7des\u00b7dun\u00b7kel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "ADV", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Denn ein Vogel jeden Fr\u00fchling", "tokens": ["Denn", "ein", "Vo\u00b7gel", "je\u00b7den", "Fr\u00fch\u00b7ling"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ART", "NN", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "An dem gr\u00fcnen Waldessaume", "tokens": ["An", "dem", "gr\u00fc\u00b7nen", "Wal\u00b7des\u00b7sau\u00b7me"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sang mit wunderbarem Schalle,", "tokens": ["Sang", "mit", "wun\u00b7der\u00b7ba\u00b7rem", "Schal\u00b7le", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ein Waldhorn klang's im Traume.", "tokens": ["Wie", "ein", "Wald\u00b7horn", "klang's", "im", "Trau\u00b7me", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVFIN", "APPRART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Und gar seltsam hohe Blumen", "tokens": ["Und", "gar", "selt\u00b7sam", "ho\u00b7he", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "ADJD", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Standen an dem Rand der Schl\u00fcnde,", "tokens": ["Stan\u00b7den", "an", "dem", "Rand", "der", "Schl\u00fcn\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sprach der Strom so dunkle Worte,", "tokens": ["Sprach", "der", "Strom", "so", "dunk\u00b7le", "Wor\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADV", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "'s war, als ob ich sie verst\u00fcnde.", "tokens": ["'s", "war", ",", "als", "ob", "ich", "sie", "ver\u00b7st\u00fcn\u00b7de", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "KOKOM", "KOUS", "PPER", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Und wie ich so sinnend atme", "tokens": ["Und", "wie", "ich", "so", "sin\u00b7nend", "at\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PWAV", "PPER", "ADV", "ADJD", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Stromesk\u00fchl und Waldesd\u00fcfte,", "tokens": ["Stro\u00b7mes\u00b7k\u00fchl", "und", "Wal\u00b7des\u00b7d\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ein wundersam Gel\u00fcsten", "tokens": ["Und", "ein", "wun\u00b7der\u00b7sam", "Ge\u00b7l\u00fcs\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["KON", "ART", "ADJD", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich hinabzog nach den Kl\u00fcften:", "tokens": ["Mich", "hin\u00b7ab\u00b7zog", "nach", "den", "Kl\u00fcf\u00b7ten", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Sah ich auf kristallnem Nachen,", "tokens": ["Sah", "ich", "auf", "kris\u00b7tall\u00b7nem", "Na\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Tief im Herzensgrund erschrocken,", "tokens": ["Tief", "im", "Her\u00b7zens\u00b7grund", "er\u00b7schro\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPRART", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Eine wundersch\u00f6ne Fraue,", "tokens": ["Ei\u00b7ne", "wun\u00b7der\u00b7sch\u00f6\u00b7ne", "Frau\u00b7e", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ganz umwallt von goldnen Locken.", "tokens": ["Ganz", "um\u00b7wallt", "von", "gold\u00b7nen", "Lo\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Und von ihrem Hals behende", "tokens": ["Und", "von", "ih\u00b7rem", "Hals", "be\u00b7hen\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJA"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4t sie l\u00f6sen eine Kette,", "tokens": ["T\u00e4t", "sie", "l\u00f6\u00b7sen", "ei\u00b7ne", "Ket\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Reicht' mit ihren wei\u00dfen H\u00e4nden", "tokens": ["Reicht'", "mit", "ih\u00b7ren", "wei\u00b7\u00dfen", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mir die allersch\u00f6nste Perle.", "tokens": ["Mir", "die", "al\u00b7ler\u00b7sch\u00f6ns\u00b7te", "Per\u00b7le", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Nur ein Wort von fremdem Klange", "tokens": ["Nur", "ein", "Wort", "von", "frem\u00b7dem", "Klan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Sprach sie da mit rotem Munde,", "tokens": ["Sprach", "sie", "da", "mit", "ro\u00b7tem", "Mun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Doch im Herzen ewig stehen", "tokens": ["Doch", "im", "Her\u00b7zen", "e\u00b7wig", "ste\u00b7hen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPRART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wird des Worts geheime Kunde.", "tokens": ["Wird", "des", "Worts", "ge\u00b7hei\u00b7me", "Kun\u00b7de", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Seitdem sa\u00df ich wie gebannt dort,", "tokens": ["Seit\u00b7dem", "sa\u00df", "ich", "wie", "ge\u00b7bannt", "dort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "PPER", "KOKOM", "ADJD", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und wenn neu der Lenz erwachte,", "tokens": ["Und", "wenn", "neu", "der", "Lenz", "er\u00b7wach\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADJD", "ART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Immer von dem Halsgeschmeide", "tokens": ["Im\u00b7mer", "von", "dem", "Hals\u00b7ge\u00b7schmei\u00b7de"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Eine Perle sie mir brachte.", "tokens": ["Ei\u00b7ne", "Per\u00b7le", "sie", "mir", "brach\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Ich barg all' im Waldesgrunde,", "tokens": ["Ich", "barg", "all'", "im", "Wal\u00b7des\u00b7grun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPRART", "NN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Und aus jeder Perl der Fraue", "tokens": ["Und", "aus", "je\u00b7der", "Perl", "der", "Frau\u00b7e"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PIAT", "NN", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Spro\u00dfte eine Blum zur Stunde,", "tokens": ["Spro\u00df\u00b7te", "ei\u00b7ne", "Blum", "zur", "Stun\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NE", "APPRART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie ihr Auge anzuschauen.", "tokens": ["Wie", "ihr", "Au\u00b7ge", "an\u00b7zu\u00b7schau\u00b7en", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "NN", "VVIZU", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.10": {"line.1": {"text": "Und so bin ich aufgewachsen,", "tokens": ["Und", "so", "bin", "ich", "auf\u00b7ge\u00b7wach\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "T\u00e4t der Blumen treulich warten,", "tokens": ["T\u00e4t", "der", "Blu\u00b7men", "treu\u00b7lich", "war\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "ADJD", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schlummert oft und tr\u00e4umte golden", "tokens": ["Schlum\u00b7mert", "oft", "und", "tr\u00e4um\u00b7te", "gol\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "KON", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "In dem schw\u00fclen Waldesgarten.", "tokens": ["In", "dem", "schw\u00fc\u00b7len", "Wal\u00b7des\u00b7gar\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Fortgesp\u00fclt ist nun der Garten", "tokens": ["Fort\u00b7ge\u00b7sp\u00fclt", "ist", "nun", "der", "Gar\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "ADV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und die Blumen all' verschwunden,", "tokens": ["Und", "die", "Blu\u00b7men", "all'", "ver\u00b7schwun\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PIAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Und die Gegend, wo sie standen,", "tokens": ["Und", "die", "Ge\u00b7gend", ",", "wo", "sie", "stan\u00b7den", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "$,", "PWAV", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Hab ich nimmermehr gefunden.", "tokens": ["Hab", "ich", "nim\u00b7mer\u00b7mehr", "ge\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ADV", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "In der Fern liegt jetzt mein Leben,", "tokens": ["In", "der", "Fern", "liegt", "jetzt", "mein", "Le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Breitend sich wie junge Tr\u00e4ume,", "tokens": ["Brei\u00b7tend", "sich", "wie", "jun\u00b7ge", "Tr\u00e4u\u00b7me", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PRF", "KOKOM", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Schimmert stets so seltsam lockend", "tokens": ["Schim\u00b7mert", "stets", "so", "selt\u00b7sam", "lo\u00b7ckend"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Durch die alten, dunklen B\u00e4ume.", "tokens": ["Durch", "die", "al\u00b7ten", ",", "dunk\u00b7len", "B\u00e4u\u00b7me", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Jetzt erst wei\u00df ich, was der Vogel", "tokens": ["Jetzt", "erst", "wei\u00df", "ich", ",", "was", "der", "Vo\u00b7gel"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "ADV", "VVFIN", "PPER", "$,", "PRELS", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Ewig ruft so bange, bange,", "tokens": ["E\u00b7wig", "ruft", "so", "ban\u00b7ge", ",", "ban\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["ADJD", "VVFIN", "ADV", "ADJD", "$,", "ADV", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Unbekannt zieht ew'ge Treue", "tokens": ["Un\u00b7be\u00b7kannt", "zieht", "ew'\u00b7ge", "Treu\u00b7e"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mich hinunter zu dem Sange.", "tokens": ["Mich", "hin\u00b7un\u00b7ter", "zu", "dem", "San\u00b7ge", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Wie die W\u00e4lder k\u00fchle rauschen,", "tokens": ["Wie", "die", "W\u00e4l\u00b7der", "k\u00fch\u00b7le", "rau\u00b7schen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "ADJA", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Zwischendurch das alte Rufen,", "tokens": ["Zwi\u00b7schen\u00b7durch", "das", "al\u00b7te", "Ru\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Wo bin ich so lang gewesen? \u2013", "tokens": ["Wo", "bin", "ich", "so", "lang", "ge\u00b7we\u00b7sen", "?", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "ADJD", "VAPP", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O ich mu\u00df hinab zur Ruhe!\u00ab", "tokens": ["O", "ich", "mu\u00df", "hin\u00b7ab", "zur", "Ru\u00b7he", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["NE", "PPER", "VMFIN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Und es stieg vom Schlo\u00df hinunter", "tokens": ["Und", "es", "stieg", "vom", "Schlo\u00df", "hin\u00b7un\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VVFIN", "APPRART", "NN", "APPO"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Schnell der s\u00fc\u00dfe Florimunde,", "tokens": ["Schnell", "der", "s\u00fc\u00b7\u00dfe", "Flo\u00b7ri\u00b7mun\u00b7de", ","], "token_inf