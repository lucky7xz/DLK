{"dta.poem.4170": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die  \n  sch\u00f6ne Gegend zu Ritzeb\u00fcttel,  \n nebst  \n  einigen Betrachtungen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1743", "urn": "urn:nbn:de:kobv:b4-20083-6", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Fast ausser mir, ob aller Sch\u00f6nheit, ob aller Zier und", "tokens": ["Fast", "aus\u00b7ser", "mir", ",", "ob", "al\u00b7ler", "Sch\u00f6n\u00b7heit", ",", "ob", "al\u00b7ler", "Zier", "und"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPER", "$,", "KOUS", "PIAT", "NN", "$,", "KOUS", "PIAT", "NN", "KON"], "meter": "-+-+-+-+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Herrlichkeit,", "tokens": ["Herr\u00b7lich\u00b7keit", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "So, in der weit- und sch\u00f6nen Gegend, uns die Natur, auf", "tokens": ["So", ",", "in", "der", "weit", "und", "sch\u00f6\u00b7nen", "Ge\u00b7gend", ",", "uns", "die", "Na\u00b7tur", ",", "auf"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word"], "pos": ["ADV", "$,", "APPR", "ART", "TRUNC", "KON", "ADJA", "NN", "$,", "PPER", "ART", "NN", "$,", "APPR"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.4": {"text": "einmahl, beut,", "tokens": ["ein\u00b7mahl", ",", "beut", ","], "token_info": ["word", "punct", "word", "punct"], "pos": ["ADV", "$,", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Wo Himmel, Erd und Wasser sich in solcher Pracht und", "tokens": ["Wo", "Him\u00b7mel", ",", "Erd", "und", "Was\u00b7ser", "sich", "in", "sol\u00b7cher", "Pracht", "und"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "NN", "$,", "NN", "KON", "NN", "PRF", "APPR", "PIAT", "NN", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Gr\u00f6sse weisen,", "tokens": ["Gr\u00f6s\u00b7se", "wei\u00b7sen", ","], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Fing ich mit froher Seelen an, den Sch\u00f6pfer im Gesch\u00f6pf", "tokens": ["Fing", "ich", "mit", "fro\u00b7her", "See\u00b7len", "an", ",", "den", "Sch\u00f6p\u00b7fer", "im", "Ge\u00b7sch\u00f6pf"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "APPR", "ADJA", "NN", "PTKVZ", "$,", "ART", "NN", "APPRART", "NN"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.8": {"text": "zu preisen:", "tokens": ["zu", "prei\u00b7sen", ":"], "token_info": ["word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}}, "stanza.2": {"line.1": {"text": "Kein Ort ist leichtlich auf der Welt,", "tokens": ["Kein", "Ort", "ist", "leicht\u00b7lich", "auf", "der", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VAFIN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wo uns solch eine Himmels-Weite,", "tokens": ["Wo", "uns", "solch", "ei\u00b7ne", "Him\u00b7mels\u00b7Wei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PIAT", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Auch ein nicht abzusehend Feld,", "tokens": ["Auch", "ein", "nicht", "ab\u00b7zu\u00b7se\u00b7hend", "Feld", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "PTKNEG", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Nebst einer gr\u00f6ssern Meeres-Breite,", "tokens": ["Nebst", "ei\u00b7ner", "gr\u00f6s\u00b7sern", "Mee\u00b7res\u00b7Brei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Auf einmahl in die Augen f\u00e4llt.", "tokens": ["Auf", "ein\u00b7mahl", "in", "die", "Au\u00b7gen", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ach la\u00df, die ausgespannte Schranken", "tokens": ["Ach", "la\u00df", ",", "die", "aus\u00b7ge\u00b7spann\u00b7te", "Schran\u00b7ken"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["ITJ", "VVFIN", "$,", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Der Luft, der Erden und der Fluht,", "tokens": ["Der", "Luft", ",", "der", "Er\u00b7den", "und", "der", "Fluht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Samt der sie f\u00fcll\u2019nden Sonnen-Gluht,", "tokens": ["Samt", "der", "sie", "f\u00fcll'n\u00b7den", "Son\u00b7nen\u00b7Gluht", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "PPER", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Nicht ohn Ergetzen, sonder Danken,", "tokens": ["Nicht", "ohn", "Er\u00b7get\u00b7zen", ",", "son\u00b7der", "Dan\u00b7ken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PTKNEG", "APPR", "NN", "$,", "KON", "NN", "$,"], "meter": "+----+-+-", "measure": "dactylic.init"}, "line.10": {"text": "Von mir, o GOTT, gesehen seyn!", "tokens": ["Von", "mir", ",", "o", "GoTT", ",", "ge\u00b7se\u00b7hen", "seyn", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "PPER", "$,", "FM", "NE", "$,", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "La\u00df mich in allen, blo\u00df allein", "tokens": ["La\u00df", "mich", "in", "al\u00b7len", ",", "blo\u00df", "al\u00b7lein"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVIMP", "PPER", "APPR", "PIAT", "$,", "ADV", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Auf Dich, als ihren Sch\u00f6pfer, schauen.", "tokens": ["Auf", "Dich", ",", "als", "ih\u00b7ren", "Sch\u00f6p\u00b7fer", ",", "schau\u00b7en", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPER", "$,", "KOUS", "PPOSAT", "NN", "$,", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "La\u00df an der Luft, an Erd und Meer,", "tokens": ["La\u00df", "an", "der", "Luft", ",", "an", "Erd", "und", "Meer", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "APPR", "ART", "NN", "$,", "APPR", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Sie wohl betrachtend, Dir zur Ehr,", "tokens": ["Sie", "wohl", "be\u00b7trach\u00b7tend", ",", "Dir", "zur", "Ehr", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "VVPP", "$,", "PPER", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.15": {"text": "Mich, mich ergetzen und erbauen.", "tokens": ["Mich", ",", "mich", "er\u00b7get\u00b7zen", "und", "er\u00b7bau\u00b7en", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPER", "VVINF", "KON", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ach la\u00df durch diese Pracht in allen,", "tokens": ["Ach", "la\u00df", "durch", "die\u00b7se", "Pracht", "in", "al\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "APPR", "PDAT", "NN", "APPR", "PIAT", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie sich in die Seele senkt,", "tokens": ["Wenn", "sie", "sich", "in", "die", "See\u00b7le", "senkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sie als Sch\u00f6pfer Dein gedenkt,", "tokens": ["Und", "sie", "als", "Sch\u00f6p\u00b7fer", "Dein", "ge\u00b7denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "KOUS", "NN", "PPOSAT", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sie Dir, dadurch geschm\u00fcckt, gefallen.", "tokens": ["Sie", "Dir", ",", "da\u00b7durch", "ge\u00b7schm\u00fcckt", ",", "ge\u00b7fal\u00b7len", "."], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "PPER", "$,", "PAV", "VVPP", "$,", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Ach la\u00df mich\u2019s, andern auch zu zeigen,", "tokens": ["Ach", "la\u00df", "mich's", ",", "an\u00b7dern", "auch", "zu", "zei\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "VVFIN", "PIS", "$,", "PIS", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Mich doch mit Sanftmuht oft bem\u00fchn,", "tokens": ["Mich", "doch", "mit", "Sanft\u00b7muht", "oft", "be\u00b7m\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und, da\u00df ich auch dem N\u00e4chsten dien,", "tokens": ["Und", ",", "da\u00df", "ich", "auch", "dem", "N\u00e4chs\u00b7ten", "di\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Von Deinem Lobe nimmer schweigen!", "tokens": ["Von", "Dei\u00b7nem", "Lo\u00b7be", "nim\u00b7mer", "schwei\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}