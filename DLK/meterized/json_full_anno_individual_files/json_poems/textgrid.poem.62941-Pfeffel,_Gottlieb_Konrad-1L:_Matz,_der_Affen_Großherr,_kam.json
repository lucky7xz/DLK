{"textgrid.poem.62941": {"metadata": {"author": {"name": "Pfeffel, Gottlieb Konrad", "birth": "N.A.", "death": "N.A."}, "title": "1L: Matz, der Affen Gro\u00dfherr, kam", "genre": "verse", "period": "N.A.", "pub_year": 1777, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Matz, der Affen Gro\u00dfherr, kam", "tokens": ["Matz", ",", "der", "Af\u00b7fen", "Gro\u00df\u00b7herr", ",", "kam"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Schlag um alle Kr\u00e4fte,", "tokens": ["Durch", "den", "Schlag", "um", "al\u00b7le", "Kr\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Sein Gehirn verlohr die S\u00e4fte,", "tokens": ["Sein", "Ge\u00b7hirn", "ver\u00b7lohr", "die", "S\u00e4f\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Arm und Beine wurden lahm.", "tokens": ["Arm", "und", "Bei\u00b7ne", "wur\u00b7den", "lahm", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VAFIN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Arzt und Henker pfuschten zwar,", "tokens": ["Arzt", "und", "Hen\u00b7ker", "pfuschten", "zwar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVFIN", "ADV", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.6": {"text": "Doch umsonst war Kunst und Sorgen;", "tokens": ["Doch", "um\u00b7sonst", "war", "Kunst", "und", "Sor\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "NN", "KON", "NN", "$."], "meter": "--+-+-+-", "measure": "anapaest.init"}, "line.7": {"text": "Die Gefahr wuchs jeden Morgen,", "tokens": ["Die", "Ge\u00b7fahr", "wuchs", "je\u00b7den", "Mor\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Weil der Reichstag nahe war.", "tokens": ["Weil", "der", "Reichs\u00b7tag", "na\u00b7he", "war", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "VAFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.9": {"text": "Man besorgt aus gutem Grund", "tokens": ["Man", "be\u00b7sorgt", "aus", "gu\u00b7tem", "Grund"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PIS", "VVFIN", "APPR", "ADJA", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.10": {"text": "Einen Aufruhr in dem Staate,", "tokens": ["Ei\u00b7nen", "Auf\u00b7ruhr", "in", "dem", "Staa\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Weil schon lang der Potentate", "tokens": ["Weil", "schon", "lang", "der", "Po\u00b7ten\u00b7ta\u00b7te"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.12": {"text": "Uebel mit dem Volke stund.", "tokens": ["Ue\u00b7bel", "mit", "dem", "Vol\u00b7ke", "stund", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.13": {"text": "Es war wider die Natur", "tokens": ["Es", "war", "wi\u00b7der", "die", "Na\u00b7tur"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "ART", "NN"], "meter": "--+-+-+", "measure": "anapaest.init"}, "line.14": {"text": "Morgenl\u00e4ndscher Etikette,", "tokens": ["Mor\u00b7gen\u00b7l\u00e4nd\u00b7scher", "E\u00b7ti\u00b7ket\u00b7te", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.15": {"text": "Da\u00df der F\u00fcrst gesprochen h\u00e4tte;", "tokens": ["Da\u00df", "der", "F\u00fcrst", "ge\u00b7spro\u00b7chen", "h\u00e4t\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVPP", "VAFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.16": {"text": "Die\u00df geschah durch Zeichen nur.", "tokens": ["Die\u00df", "ge\u00b7schah", "durch", "Zei\u00b7chen", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "APPR", "NN", "ADV", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.17": {"text": "Eben das vermehrt die Noth;", "tokens": ["E\u00b7ben", "das", "ver\u00b7mehrt", "die", "Noth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJD", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.18": {"text": "W\u00e4r er blos ein Narr, wir f\u00e4nden", "tokens": ["W\u00e4r", "er", "blos", "ein", "Narr", ",", "wir", "f\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VAFIN", "PPER", "ADV", "ART", "NN", "$,", "PPER", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.19": {"text": "Leichter Rath; doch lahm an H\u00e4nden,", "tokens": ["Leich\u00b7ter", "Rath", ";", "doch", "lahm", "an", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "VVFIN", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.20": {"text": "Hie\u00df es, ist so gut als todt.", "tokens": ["Hie\u00df", "es", ",", "ist", "so", "gut", "als", "todt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "VAFIN", "ADV", "ADJD", "KOKOM", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.21": {"text": "Schweigt und stellt das Jammern ein,", "tokens": ["Schweigt", "und", "stellt", "das", "Jam\u00b7mern", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KON", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.22": {"text": "Rief ein Ausbund schlauer Affen,", "tokens": ["Rief", "ein", "Aus\u00b7bund", "schlau\u00b7er", "Af\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.23": {"text": "Ich, ihr Herrn, will H\u00fclfe schaffen,", "tokens": ["Ich", ",", "ihr", "Herrn", ",", "will", "H\u00fcl\u00b7fe", "schaf\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "PPOSAT", "NN", "$,", "VMFIN", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.24": {"text": "Oder gleich gehangen seyn.", "tokens": ["O\u00b7der", "gleich", "ge\u00b7han\u00b7gen", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VAINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.25": {"text": "Als der Reichstag nun begann,", "tokens": ["Als", "der", "Reichs\u00b7tag", "nun", "be\u00b7gann", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.26": {"text": "Wurde Matz auf einem Schragen", "tokens": ["Wur\u00b7de", "Matz", "auf", "ei\u00b7nem", "Schra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VAFIN", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.27": {"text": "Heimlich auf den Thron getragen", "tokens": ["Heim\u00b7lich", "auf", "den", "Thron", "ge\u00b7tra\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "ART", "NN", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.28": {"text": "Und mit Purpur angethan.", "tokens": ["Und", "mit", "Pur\u00b7pur", "an\u00b7ge\u00b7than", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Unter diesem Mantel stand", "tokens": ["Un\u00b7ter", "die\u00b7sem", "Man\u00b7tel", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Meister Gaudieb. Seine Pfoten", "tokens": ["Meis\u00b7ter", "Gau\u00b7di\u00b7eb", ".", "Sei\u00b7ne", "Pfo\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Declamierten, wie nach Noten,", "tokens": ["De\u00b7cla\u00b7mier\u00b7ten", ",", "wie", "nach", "No\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Viel von Pflicht und Vaterland.", "tokens": ["Viel", "von", "Pflicht", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Alles Volk schwur hoch erfreut:", "tokens": ["Al\u00b7les", "Volk", "schwur", "hoch", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Nein, seitdem wir F\u00fcrsten haben,", "tokens": ["Nein", ",", "seit\u00b7dem", "wir", "F\u00fcrs\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Zeigte keiner solche Gaben", "tokens": ["Zeig\u00b7te", "kei\u00b7ner", "sol\u00b7che", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "In der Staatsberedsamkeit.", "tokens": ["In", "der", "Staats\u00b7be\u00b7red\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Doch da sich der Schwarm verlor,", "tokens": ["Doch", "da", "sich", "der", "Schwarm", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Kroch der Schalk aus seiner H\u00f6le", "tokens": ["Kroch", "der", "Schalk", "aus", "sei\u00b7ner", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Und mit ihm des F\u00fcrsten Seele", "tokens": ["Und", "mit", "ihm", "des", "F\u00fcrs\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "In der Toris Kreis hervor.", "tokens": ["In", "der", "To\u00b7ris", "Kreis", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Bravo! rief ein Ordensstern;", "tokens": ["Bra\u00b7vo", "!", "rief", "ein", "Or\u00b7denss\u00b7tern", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.42": {"text": "Aber sag uns unverholen,", "tokens": ["A\u00b7ber", "sag", "uns", "un\u00b7ver\u00b7ho\u00b7len", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVIMP", "PPER", "ADJD", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.43": {"text": "Wo hast du die Kunst gestohlen?", "tokens": ["Wo", "hast", "du", "die", "Kunst", "ge\u00b7stoh\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "VAFIN", "PPER", "ART", "NN", "VVPP", "$."], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.44": {"text": "\u00bbin Europa, meine Herrn.\u00ab", "tokens": ["\u00bb", "in", "Eu\u00b7ro\u00b7pa", ",", "mei\u00b7ne", "Herrn", ".", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["$(", "APPR", "NE", "$,", "PPOSAT", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Matz, der Affen Gro\u00dfherr, kam", "tokens": ["Matz", ",", "der", "Af\u00b7fen", "Gro\u00df\u00b7herr", ",", "kam"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word"], "pos": ["NE", "$,", "ART", "NN", "NN", "$,", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den Schlag um alle Kr\u00e4fte,", "tokens": ["D