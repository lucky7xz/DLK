, "pos": ["KON", "APPR", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.29": {"text": "Unter diesem Mantel stand", "tokens": ["Un\u00b7ter", "die\u00b7sem", "Man\u00b7tel", "stand"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "VVFIN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.30": {"text": "Meister Gaudieb. Seine Pfoten", "tokens": ["Meis\u00b7ter", "Gau\u00b7di\u00b7eb", ".", "Sei\u00b7ne", "Pfo\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["PPOSAT", "NN", "$.", "PPOSAT", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.31": {"text": "Declamierten, wie nach Noten,", "tokens": ["De\u00b7cla\u00b7mier\u00b7ten", ",", "wie", "nach", "No\u00b7ten", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "APPR", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.32": {"text": "Viel von Pflicht und Vaterland.", "tokens": ["Viel", "von", "Pflicht", "und", "Va\u00b7ter\u00b7land", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.33": {"text": "Alles Volk schwur hoch erfreut:", "tokens": ["Al\u00b7les", "Volk", "schwur", "hoch", "er\u00b7freut", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVFIN", "ADJD", "ADJD", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.34": {"text": "Nein, seitdem wir F\u00fcrsten haben,", "tokens": ["Nein", ",", "seit\u00b7dem", "wir", "F\u00fcrs\u00b7ten", "ha\u00b7ben", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.35": {"text": "Zeigte keiner solche Gaben", "tokens": ["Zeig\u00b7te", "kei\u00b7ner", "sol\u00b7che", "Ga\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PIS", "PIAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "In der Staatsberedsamkeit.", "tokens": ["In", "der", "Staats\u00b7be\u00b7red\u00b7sam\u00b7keit", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.37": {"text": "Doch da sich der Schwarm verlor,", "tokens": ["Doch", "da", "sich", "der", "Schwarm", "ver\u00b7lor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PRF", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.38": {"text": "Kroch der Schalk aus seiner H\u00f6le", "tokens": ["Kroch", "der", "Schalk", "aus", "sei\u00b7ner", "H\u00f6\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NE", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.39": {"text": "Und mit ihm des F\u00fcrsten Seele", "tokens": ["Und", "mit", "ihm", "des", "F\u00fcrs\u00b7ten", "See\u00b7le"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPER", "ART", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.40": {"text": "In der Toris Kreis hervor.", "tokens": ["In", "der", "To\u00b7ris", "Kreis", "her\u00b7vor", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NE", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.41": {"text": "Bravo! rief ein Ordensstern;", "tokens": ["Bra\u00b7vo", "!", "rief", "ein", "Or\u00b7denss\u00b7tern", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "VVFIN", "ART", "NN", "$."], "meter": "+-+-+--", "measure": "unknown.measure.tri"}, "line.42": {"text": "Aber sag uns unverholen,", "tokens": ["A\u00b7ber"