{"textgrid.poem.50019": {"metadata": {"author": {"name": "Wernicke, Christian", "birth": "N.A.", "death": "N.A."}, "title": "(51). An die Chloris", "genre": "verse", "period": "N.A.", "pub_year": 1693, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es n\u00e4hrt Sicilien ein solches Feuer nicht", "tokens": ["Es", "n\u00e4hrt", "Si\u00b7ci\u00b7li\u00b7en", "ein", "sol\u00b7ches", "Feu\u00b7er", "nicht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "ART", "PIAT", "NN", "PTKNEG"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als das aus deinen Augen bricht;", "tokens": ["Als", "das", "aus", "dei\u00b7nen", "Au\u00b7gen", "bricht", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDS", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es weicht Egyptens Strom, wenn deiner sich ergiesset,", "tokens": ["Es", "weicht", "E\u00b7gyp\u00b7tens", "Strom", ",", "wenn", "dei\u00b7ner", "sich", "er\u00b7gies\u00b7set", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NE", "NN", "$,", "KOUS", "PPOSAT", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Und \u00fcber deine Wangen fliesset:", "tokens": ["Und", "\u00fc\u00b7ber", "dei\u00b7ne", "Wan\u00b7gen", "flies\u00b7set", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Nachdem die Reitzung ist, so wechselt sich ihr Spiel,", "tokens": ["Nach\u00b7dem", "die", "Reit\u00b7zung", "ist", ",", "so", "wech\u00b7selt", "sich", "ihr", "Spiel", ","], "token_info": ["word", "word", "word