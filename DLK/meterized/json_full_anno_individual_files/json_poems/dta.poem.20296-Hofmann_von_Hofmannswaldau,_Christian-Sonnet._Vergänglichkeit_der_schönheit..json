{"dta.poem.20296": {"metadata": {"author": {"name": "Hofmann von Hofmannswaldau, Christian", "birth": "N.A.", "death": "N.A."}, "title": "Sonnet.  \n Verg\u00e4nglichkeit der sch\u00f6nheit.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1695", "urn": "urn:nbn:de:kobv:b4-200905197751", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es wird der bleiche tod mit seiner kalten hand", "tokens": ["Es", "wird", "der", "blei\u00b7che", "tod", "mit", "sei\u00b7ner", "kal\u00b7ten", "hand"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dir endlich mit der zeit um deine br\u00fcste streichen/", "tokens": ["Dir", "end\u00b7lich", "mit", "der", "zeit", "um", "dei\u00b7ne", "br\u00fcs\u00b7te", "strei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "APPR", "ART", "NN", "APPR", "PPOSAT", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der liebliche corall der lippen wird verbleichen;", "tokens": ["Der", "lieb\u00b7li\u00b7che", "co\u00b7rall", "der", "lip\u00b7pen", "wird", "ver\u00b7blei\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der schultern warmer schnee wird werden kalter sand/", "tokens": ["Der", "schul\u00b7tern", "war\u00b7mer", "schnee", "wird", "wer\u00b7den", "kal\u00b7ter", "sand", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "VAFIN", "VAFIN", "ADJD", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Der augen s\u00fcsser blitz/ die kr\u00e4ffte deiner hand/", "tokens": ["Der", "au\u00b7gen", "s\u00fcs\u00b7ser", "blitz", "/", "die", "kr\u00e4ff\u00b7te", "dei\u00b7ner", "hand", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$(", "ART", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "F\u00fcr welchen solches f\u00e4llt/ die werden zeitlich weichen/", "tokens": ["F\u00fcr", "wel\u00b7chen", "sol\u00b7ches", "f\u00e4llt", "/", "die", "wer\u00b7den", "zeit\u00b7lich", "wei\u00b7chen", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "PIS", "VVFIN", "$(", "PDS", "VAFIN", "ADJD", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Das haar/ das itzund kan des goldes glantz erreichen/", "tokens": ["Das", "haar", "/", "das", "it\u00b7zund", "kan", "des", "gol\u00b7des", "glantz", "er\u00b7rei\u00b7chen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "PDS", "ADV", "VMFIN", "ART", "ADJA", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Tilgt endlich tag und jahr als ein gemeines band.", "tokens": ["Tilgt", "end\u00b7lich", "tag", "und", "jahr", "als", "ein", "ge\u00b7mei\u00b7nes", "band", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "NN", "KON", "ADJD", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Der wohlgesetzte fu\u00df/ die lieblichen geb\u00e4rden/", "tokens": ["Der", "wohl\u00b7ge\u00b7setz\u00b7te", "fu\u00df", "/", "die", "lieb\u00b7li\u00b7chen", "ge\u00b7b\u00e4r\u00b7den", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PTKVZ", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Die werden theils zu staub/ theils nichts und nichtig werden/", "tokens": ["Die", "wer\u00b7den", "theils", "zu", "staub", "/", "theils", "nichts", "und", "nich\u00b7tig", "wer\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ADV", "PTKA", "ADJD", "$(", "ADV", "PIS", "KON", "ADJD", "VAINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Denn opffert keiner mehr der gottheit deiner pracht.", "tokens": ["Denn", "opf\u00b7fert", "kei\u00b7ner", "mehr", "der", "got\u00b7theit", "dei\u00b7ner", "pracht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PIS", "ADV", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Di\u00df und noch mehr als di\u00df mu\u00df endlich untergehen/", "tokens": ["Di\u00df", "und", "noch", "mehr", "als", "di\u00df", "mu\u00df", "end\u00b7lich", "un\u00b7ter\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "KON", "ADV", "PIAT", "KOKOM", "PDS", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Dein hertze kan allein zu aller zeit bestehen/", "tokens": ["Dein", "hert\u00b7ze", "kan", "al\u00b7lein", "zu", "al\u00b7ler", "zeit", "be\u00b7ste\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VMFIN", "ADV", "APPR", "PIAT", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.14": {"text": "Dieweil es die natur aus diamant gemacht.", "tokens": ["Die\u00b7weil", "es", "die", "na\u00b7tur", "aus", "di\u00b7a\u00b7mant", "ge\u00b7macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "APPR", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}