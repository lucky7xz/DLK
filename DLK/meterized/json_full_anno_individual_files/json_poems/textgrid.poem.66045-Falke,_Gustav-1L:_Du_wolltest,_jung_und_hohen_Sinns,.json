{"textgrid.poem.66045": {"metadata": {"author": {"name": "Falke, Gustav", "birth": "N.A.", "death": "N.A."}, "title": "1L: Du wolltest, jung und hohen Sinns,", "genre": "verse", "period": "N.A.", "pub_year": 1884, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Du wolltest, jung und hohen Sinns,", "tokens": ["Du", "woll\u00b7test", ",", "jung", "und", "ho\u00b7hen", "Sinns", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "$,", "ADJD", "KON", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Pal\u00e4ste baun und Tempel,", "tokens": ["Pa\u00b7l\u00e4s\u00b7te", "baun", "und", "Tem\u00b7pel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "VVINF", "KON", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und sehntest dich, ein Haus zu sehn", "tokens": ["Und", "sehn\u00b7test", "dich", ",", "ein", "Haus", "zu", "sehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "$,", "ART", "NN", "PTKZU", "VVINF"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mit deines Geistes Stempel.", "tokens": ["Mit", "dei\u00b7nes", "Geis\u00b7tes", "Stem\u00b7pel", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.5": {"text": "Was dir der Gott an Sch\u00f6nheit gab,", "tokens": ["Was", "dir", "der", "Gott", "an", "Sch\u00f6n\u00b7heit", "gab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ART", "NN", "APPR", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Das liegt nun all im dunklen Grab.", "tokens": ["Das", "liegt", "nun", "all", "im", "dunk\u00b7len", "Grab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ADV", "PIAT", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der Tod, der Neidgeselle,", "tokens": ["Der", "Tod", ",", "der", "Neid\u00b7ge\u00b7sel\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Nahm dir zu fr\u00fch die Kelle,", "tokens": ["Nahm", "dir", "zu", "fr\u00fch", "die", "Kel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PTKA", "ADJD", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Das Richtma\u00df und den Zirkel ab.", "tokens": ["Das", "Richt\u00b7ma\u00df", "und", "den", "Zir\u00b7kel", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich aber lebe noch im Licht", "tokens": ["Ich", "a\u00b7ber", "le\u00b7be", "noch", "im", "Licht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "ADV", "VVFIN", "ADV", "APPRART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und bau auf meine Weise,", "tokens": ["Und", "bau", "auf", "mei\u00b7ne", "Wei\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Und bau an einem Tempel fromm,", "tokens": ["Und", "bau", "an", "ei\u00b7nem", "Tem\u00b7pel", "fromm", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Darin ich bet und preise.", "tokens": ["Da\u00b7rin", "ich", "bet", "und", "prei\u00b7se", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "VVFIN", "KON", "NN", "$."], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.5": {"text": "Aus Liedern soll ein Haus erstehn,", "tokens": ["Aus", "Lie\u00b7dern", "soll", "ein", "Haus", "er\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VMFIN", "ART", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Draus meine Augen fr\u00f6hlich sehn,", "tokens": ["Draus", "mei\u00b7ne", "Au\u00b7gen", "fr\u00f6h\u00b7lich", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Darin vor allen W\u00e4nden", "tokens": ["Da\u00b7rin", "vor", "al\u00b7len", "W\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word"], "pos": ["PAV", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "In stillen Opferbr\u00e4nden", "tokens": ["In", "stil\u00b7len", "Op\u00b7fer\u00b7br\u00e4n\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.9": {"text": "Der Sch\u00f6nheit ewige Flammen wehn.", "tokens": ["Der", "Sch\u00f6n\u00b7heit", "e\u00b7wi\u00b7ge", "Flam\u00b7men", "wehn", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}}, "stanza.3": {"line.1": {"text": "Und eine Tafel bring ich an,", "tokens": ["Und", "ei\u00b7ne", "Ta\u00b7fel", "bring", "ich", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN",