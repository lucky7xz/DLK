{"dta.poem.16022": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "76.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Bei seinem Vater hat das Kind nicht lernen wollen,", "tokens": ["Bei", "sei\u00b7nem", "Va\u00b7ter", "hat", "das", "Kind", "nicht", "ler\u00b7nen", "wol\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VAFIN", "ART", "NN", "PTKNEG", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in die Schule schickt' er es mit Liebesgrollen.", "tokens": ["Und", "in", "die", "Schu\u00b7le", "schickt'", "er", "es", "mit", "Lie\u00b7bes\u00b7grol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "PPER", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Da schnarchte streng es an der Lehrer, der es lehrte,", "tokens": ["Da", "schnarch\u00b7te", "streng", "es", "an", "der", "Leh\u00b7rer", ",", "der", "es", "lehr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "VVFIN", "PPER", "APPR", "ART", "NN", "$,", "PRELS", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df zu des Vaters Lehr' es bald zur\u00fcck begehrte.", "tokens": ["Da\u00df", "zu", "des", "Va\u00b7ters", "Lehr'", "es", "bald", "zu\u00b7r\u00fcck", "be\u00b7gehr\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "APPR", "ART", "NN", "NN", "PPER", "ADV", "ADV", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "In seine Lehre nahm der Vater es zur\u00fcck,", "tokens": ["In", "sei\u00b7ne", "Leh\u00b7re", "nahm", "der", "Va\u00b7ter", "es", "zu\u00b7r\u00fcck", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "ART", "NN", "PPER", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und nun gewitzigt lernt es flei\u00dfig und mit Gl\u00fcck.", "tokens": ["Und", "nun", "ge\u00b7wit\u00b7zigt", "lernt", "es", "flei\u00b7\u00dfig", "und", "mit", "Gl\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "VVFIN", "PPER", "ADJD", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}