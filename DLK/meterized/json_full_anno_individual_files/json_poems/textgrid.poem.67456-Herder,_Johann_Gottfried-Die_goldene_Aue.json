{"textgrid.poem.67456": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Die goldene Aue", "genre": "verse", "period": "N.A.", "pub_year": 1793, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Reichen, goldenes Thal! Mit ", "tokens": ["Rei\u00b7chen", ",", "gol\u00b7de\u00b7nes", "Thal", "!", "Mit"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "APPR"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wald und Felder und Strom, H\u00e4user und Heerden und Au'n.", "tokens": ["Wald", "und", "Fel\u00b7der", "und", "Strom", ",", "H\u00e4u\u00b7ser", "und", "Heer\u00b7den", "und", "Au'", "n."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "NE"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Langsam kr\u00fcmmet Dein Pfad sich zu Dir nieder, in jedem", "tokens": ["Lang\u00b7sam", "kr\u00fcm\u00b7met", "Dein", "Pfad", "sich", "zu", "Dir", "nie\u00b7der", ",", "in", "je\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "PPER", "PTKVZ", "$,", "APPR", "PIAT"], "meter": "+-+--+---+--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Tritte giebt er das Thal weiter und weiter zu schaun.", "tokens": ["Trit\u00b7te", "giebt", "er", "das", "Thal", "wei\u00b7ter", "und", "wei\u00b7ter", "zu", "schaun", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "PPER", "ART", "NN", "PTKVZ", "KON", "ADV", "PTKZU", "VVINF", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.5": {"text": "Also steiget die Jugend hinab ins Leben; es lockt sie", "tokens": ["Al\u00b7so", "stei\u00b7get", "die", "Ju\u00b7gend", "hin\u00b7ab", "ins", "Le\u00b7ben", ";", "es", "lockt", "sie"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "APPRART", "NN", "$.", "PPER", "VVFIN", "PPER"], "meter": "+-+--+--+-+--+-", "measure": "hexameter"}, "line.6": {"text": "Ungesehener Lust reicherer Busen hinab,", "tokens": ["Un\u00b7ge\u00b7se\u00b7he\u00b7ner", "Lust", "rei\u00b7che\u00b7rer", "Bu\u00b7sen", "hin\u00b7ab", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "ADJA", "NN", "PTKVZ", "$,"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.7": {"text": "Bis sie r\u00fcckw\u00e4rts klimmet, das Thal verlassend; sie blicket", "tokens": ["Bis", "sie", "r\u00fcck\u00b7w\u00e4rts", "klim\u00b7met", ",", "das", "Thal", "ver\u00b7las\u00b7send", ";", "sie", "bli\u00b7cket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "ADV", "VVFIN", "$,", "ART", "NN", "VVPP", "$.", "PPER", "VVFIN"], "meter": "--+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.8": {"text": "M\u00fchsam nieder; es wird enger und enger das Thal,", "tokens": ["M\u00fch\u00b7sam", "nie\u00b7der", ";", "es", "wird", "en\u00b7ger", "und", "en\u00b7ger", "das", "Thal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "PTKVZ", "$.", "PPER", "VAFIN", "ADJD", "KON", "ADJD", "ART", "NN", "$,"], "meter": "+-+---+--+--+", "measure": "trochaic.penta.relaxed"}, "line.9": {"text": "Bis es im letzten Blick wie ein Traum verschwindet. So lebet", "tokens": ["Bis", "es", "im", "letz\u00b7ten", "Blick", "wie", "ein", "Traum", "ver\u00b7schwin\u00b7det", ".", "So", "le\u00b7bet"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "PPER", "APPRART", "ADJA", "NN", "KOKOM", "ART", "NN", "VVPP", "$.", "ADV", "VVFIN"], "meter": "-+-+-+--+-+----", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Wohl denn, Beides ein Traum, Leben und g\u00fcldene Au'!", "tokens": ["Wohl", "denn", ",", "Bei\u00b7des", "ein", "Traum", ",", "Le\u00b7ben", "und", "g\u00fcl\u00b7de\u00b7ne", "Au'", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PIS", "ART", "NN", "$,", "NN", "KON", "ADJA", "NN", "$."], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}}, "stanza.2": {"line.1": {"text": "Reichen, goldenes Thal! Mit ", "tokens": ["Rei\u00b7chen", ",", "gol\u00b7de\u00b7nes", "Thal", "!", "Mit"], "token_info": ["word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADJA", "NN", "$.", "APPR"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Wald und Felder und Strom, H\u00e4user und Heerden und Au'n.", "tokens": ["Wald", "und", "Fel\u00b7der", "und", "Strom", ",", "H\u00e4u\u00b7ser", "und", "Heer\u00b7den", "und", "Au'", "n."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["NN", "KON", "NN", "KON", "NN", "$,", "NN", "KON", "NN", "KON", "NN", "NE"], "meter": "+-+--++--+--+", "measure": "trochaic.hexa.relaxed"}, "line.3": {"text": "Langsam kr\u00fcmmet Dein Pfad sich zu Dir nieder, in jedem", "tokens": ["Lang\u00b7sam", "kr\u00fcm\u00b7met", "Dein", "Pfad", "sich", "zu", "Dir", "nie\u00b7der", ",", "in", "je\u00b7dem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ADJD", "VVFIN", "PPOSAT", "NN", "PRF", "APPR", "PPER", "PTKVZ", "$,", "APPR", "PIAT"], "meter": "+-+--+---+--+-", "measure": "troch