{"dta.poem.3980": {"metadata": {"author": {"name": "Spindler, Christian Gotthold", "birth": "N.A.", "death": "N.A."}, "title": "Xl.", "genre": "Lyrik, Drama", "period": "N.A.", "pub_year": "1745", "urn": "urn:nbn:de:kobv:b4-20581-9", "language": ["de:0.99"], "booktitle": "Spindler, Christian Gotthold: Unschuldige Jugend-Fr\u00fcchte. Leipzig, 1745."}, "poem": {"stanza.1": {"line.1": {"text": "Hier hast du, sch\u00f6nes Kind, das nechst begehrte", "tokens": ["Hier", "hast", "du", ",", "sch\u00f6\u00b7nes", "Kind", ",", "das", "nechst", "be\u00b7gehr\u00b7te"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "$,", "ADJA", "NN", "$,", "PRELS", "ADV", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Lied,", "tokens": ["Lied", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Vielleicht, wen\u0303 solches so, wie ich, verwirrt aussieht,", "tokens": ["Viel\u00b7leicht", ",", "we\u00f1", "sol\u00b7ches", "so", ",", "wie", "ich", ",", "ver\u00b7wirrt", "aus\u00b7sieht", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "$,", "KOUS", "PIS", "ADV", "$,", "PWAV", "PPER", "$,", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erbrich es nur getrost, di\u00df Opffer reinster Pflichten,", "tokens": ["Er\u00b7brich", "es", "nur", "ge\u00b7trost", ",", "di\u00df", "Opf\u00b7fer", "reins\u00b7ter", "Pflich\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "PPER", "ADV", "VVPP", "$,", "PDS", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "So bald ich aufger\u00e4umt, will ich ein be\u00dfres dichten.", "tokens": ["So", "bald", "ich", "auf\u00b7ge\u00b7r\u00e4umt", ",", "will", "ich", "ein", "be\u00df\u00b7res", "dich\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "VVPP", "$,", "VMFIN", "PPER", "ART", "ADJA", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}