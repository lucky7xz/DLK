{"dta.poem.3373": {"metadata": {"author": {"name": "Kosegarten, Ludwig Gotthard", "birth": "N.A.", "death": "N.A."}, "title": "Dem Unbekannten.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1798", "urn": "urn:nbn:de:kobv:b4-200905193292", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ruhst und rastest du denn nimmer, erhabene", "tokens": ["Ruhst", "und", "ras\u00b7test", "du", "denn", "nim\u00b7mer", ",", "er\u00b7ha\u00b7be\u00b7ne"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["VVFIN", "KON", "VVFIN", "PPER", "ADV", "ADV", "$,", "ADJA"], "meter": "+-+-+-+--+--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Grosse Mutter Natur? Schlummert die Schnell-", "tokens": ["Gros\u00b7se", "Mut\u00b7ter", "Na\u00b7tur", "?", "Schlum\u00b7mert", "die", "Schnell"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADJA", "NN", "NN", "$.", "VVFIN", "ART", "NN"], "meter": "+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.3": {"text": "kraft nie,", "tokens": ["kraft", "nie", ","], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.4": {"text": "Die den Schooss dir befruchtet,", "tokens": ["Die", "den", "Scho\u00b7oss", "dir", "be\u00b7fruch\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Die die s\u00e4ugende Brust dir schwellt?", "tokens": ["Die", "die", "s\u00e4u\u00b7gen\u00b7de", "Brust", "dir", "schwellt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "ADJA", "NN", "PPER", "VVFIN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.2": {"line.1": {"text": "Von dem mattesten Strahl, welcher den Morgen", "tokens": ["Von", "dem", "mat\u00b7tes\u00b7ten", "Strahl", ",", "wel\u00b7cher", "den", "Mor\u00b7gen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN", "$,", "PRELS", "ART", "NN"], "meter": "--+--+---+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "f\u00e4rbt,", "tokens": ["f\u00e4rbt", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Regt die R\u00fcstige sich, schafft und zerst\u00f6rt und wirkt,", "tokens": ["Regt", "die", "R\u00fcs\u00b7ti\u00b7ge", "sich", ",", "schafft", "und", "zer\u00b7st\u00f6rt", "und", "wirkt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "PRF", "$,", "VVFIN", "KON", "VVPP", "KON", "VVFIN", "$,"], "meter": "+-+---+--+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Bis die blasseste Rose", "tokens": ["Bis", "die", "blas\u00b7ses\u00b7te", "Ro\u00b7se"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "In den Locken des Abends welkt \u2014", "tokens": ["In", "den", "Lo\u00b7cken", "des", "A\u00b7bends", "welkt"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "$("], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.3": {"line.1": {"text": "Auf thauduftender Flur schlummert die Mitternacht.", "tokens": ["Auf", "thaud\u00b7uf\u00b7ten\u00b7der", "Flur", "schlum\u00b7mert", "die", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Seine wolkige Bahn wandelt der m\u00fcde Mond.", "tokens": ["Sei\u00b7ne", "wol\u00b7ki\u00b7ge", "Bahn", "wan\u00b7delt", "der", "m\u00fc\u00b7de", "Mond", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.3": {"text": "Ringsum g\u00e4hnet die Sch\u00f6pfung;", "tokens": ["Ring\u00b7sum", "g\u00e4h\u00b7net", "die", "Sch\u00f6p\u00b7fung", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Rastlos waltet die Sch\u00f6pferin,", "tokens": ["Rast\u00b7los", "wal\u00b7tet", "die", "Sch\u00f6p\u00b7fe\u00b7rin", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.4": {"line.1": {"text": "Schwirrt im flisternden Schilf, pl\u00e4tschert im Rohr", "tokens": ["Schwirrt", "im", "flis\u00b7tern\u00b7den", "Schilf", ",", "pl\u00e4t\u00b7schert", "im", "Rohr"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$,", "VVFIN", "APPRART", "NN"], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "des Sumpfs,", "tokens": ["des", "Sumpfs", ","], "token_info": ["word", "word", "punct"], "pos": ["ART", "NN", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Tr\u00e4nkt die Saaten mit Thau, duftet im Fliederbusch,", "tokens": ["Tr\u00e4nkt", "die", "Saa\u00b7ten", "mit", "Thau", ",", "duf\u00b7tet", "im", "Flie\u00b7der\u00b7busch", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Gurgelt heiser im Frosche,", "tokens": ["Gur\u00b7gelt", "hei\u00b7ser", "im", "Fro\u00b7sche", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Fl\u00f6tet gellend im Wachtelschlag;", "tokens": ["Fl\u00f6\u00b7tet", "gel\u00b7lend", "im", "Wach\u00b7tel\u00b7schlag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "VVPP", "APPRART", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.5": {"line.1": {"text": "Summt im bl\u00fchenden Baum aus den Zehntausenden", "tokens": ["Summt", "im", "bl\u00fc\u00b7hen\u00b7den", "Baum", "aus", "den", "Zehn\u00b7tau\u00b7sen\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "APPR", "ART", "NN"], "meter": "+-+--+--++--", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "Goldner K\u00e4fer, beseelt V\u00f6lker von gaukelnden", "tokens": ["Gold\u00b7ner", "K\u00e4\u00b7fer", ",", "be\u00b7seelt", "V\u00f6l\u00b7ker", "von", "gau\u00b7keln\u00b7den"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADJA", "NN", "$,", "VVFIN", "NN", "APPR", "ADJA"], "meter": "+-+---+--+--", "measure": "trochaic.tetra.relaxed"}, "line.3": {"text": "M\u00fccken, schrillt in der Grille", "tokens": ["M\u00fc\u00b7cken", ",", "schrillt", "in", "der", "Gril\u00b7le"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "Fl\u00fcgel, donnert im Wasserfall,", "tokens": ["Fl\u00fc\u00b7gel", ",", "don\u00b7nert", "im", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.6": {"line.1": {"text": "Th\u00fcrmt am Saume des S\u00fcd Wolken wie Berg'", "tokens": ["Th\u00fcrmt", "am", "Sau\u00b7me", "des", "S\u00fcd", "Wol\u00b7ken", "wie", "Ber\u00b7g'"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "APPRART", "NN", "ART", "NN", "NN", "KOKOM", "NN"], "meter": "+-+-+-+--+-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "empor,", "tokens": ["em\u00b7por", ","], "token_info": ["word", "punct"], "pos": ["PTKVZ", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "W\u00e4lzt die Berge daher, prasselt aus k\u00e4mpfenden", "tokens": ["W\u00e4lzt", "die", "Ber\u00b7ge", "da\u00b7her", ",", "pras\u00b7selt", "aus", "k\u00e4mp\u00b7fen\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PAV", "$,", "VVFIN", "APPR", "ADJA"], "meter": "+-+-+-+-+---", "measure": "unknown.measure.penta"}, "line.4": {"text": "Wolken, zuckt in der Leuchtung,", "tokens": ["Wol\u00b7ken", ",", "zuckt", "in", "der", "Leuch\u00b7tung", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "St\u00fcrmt im brausenden Wirbelwind.", "tokens": ["St\u00fcrmt", "im", "brau\u00b7sen\u00b7den", "Wir\u00b7bel\u00b7wind", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPRART", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.7": {"line.1": {"text": "Die du, heilige Kraft, br\u00fcnstig das All um-", "tokens": ["Die", "du", ",", "hei\u00b7li\u00b7ge", "Kraft", ",", "br\u00fcns\u00b7tig", "das", "All", "um"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "$,", "ADJA", "NN", "$,", "ADJD", "ART", "NN", "TRUNC"], "meter": "+-+--+--++-", "measure": "trochaic.penta.relaxed"}, "line.2": {"text": "schlingst,", "tokens": ["schlingst", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "-", "measure": "single.down"}, "line.3": {"text": "Alles Leben gebierst, alles Geborne n\u00e4hrst,", "tokens": ["Al\u00b7les", "Le\u00b7ben", "ge\u00b7bierst", ",", "al\u00b7les", "Ge\u00b7bor\u00b7ne", "n\u00e4hrst", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "VVPP", "$,", "PIAT", "NN", "VVFIN", "$,"], "meter": "+-+--+---+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Unbekannte, wer bist du?", "tokens": ["Un\u00b7be\u00b7kann\u00b7te", ",", "wer", "bist", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Nieerlauschte, wo wirkest du?", "tokens": ["Nie\u00b7er\u00b7lauschte", ",", "wo", "wir\u00b7kest", "du", "?"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "PWAV", "VAFIN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Durch die Adern des All spritzest du flammend", "tokens": ["Durch", "die", "A\u00b7dern", "des", "All", "sprit\u00b7zest", "du", "flam\u00b7mend"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN", "PPER", "VVPP"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Blut,", "tokens": ["Blut", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Kochst in Schachten das Gold, r\u00fcttelst den Ocean,", "tokens": ["Kochst", "in", "Schach\u00b7ten", "das", "Gold", ",", "r\u00fct\u00b7telst", "den", "O\u00b7cean", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NE", "APPR", "NN", "ART", "NN", "$,", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "W\u00f6lbst Basalte zu Domen,", "tokens": ["W\u00f6lbst", "Ba\u00b7sal\u00b7te", "zu", "Do\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "NE", "APPR", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "H\u00f6hlst kristallene Grotten aus.", "tokens": ["H\u00f6hlst", "kris\u00b7tal\u00b7le\u00b7ne", "Grot\u00b7ten", "aus", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ADJA", "NN", "PTKVZ", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}}, "stanza.9": {"line.1": {"text": "Aus dem Staube herauf rufst du die Pflanzen-", "tokens": ["Aus", "dem", "Stau\u00b7be", "her\u00b7auf", "rufst", "du", "die", "Pflan\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ADV", "VVFIN", "PPER", "ART", "TRUNC"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "welt.", "tokens": ["welt", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+", "measure": "single.up"}, "line.3": {"text": "S\u00e4uselnd wallet die Saat, s\u00e4uselnd der Eichenwald.", "tokens": ["S\u00e4u\u00b7selnd", "wal\u00b7let", "die", "Saat", ",", "s\u00e4u\u00b7selnd", "der", "Ei\u00b7chen\u00b7wald", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$,", "ADJD", "ART", "NN", "$."], "meter": "+-+--+-+-+-+", "measure": "trochaic.hexa.relaxed"}, "line.4": {"text": "Sonnan rauschet die Zeder,", "tokens": ["Son\u00b7nan", "rau\u00b7schet", "die", "Ze\u00b7der", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "W\u00fcrzig duftet das Veilchenthal.", "tokens": ["W\u00fcr\u00b7zig", "duf\u00b7tet", "das", "Veil\u00b7chen\u00b7thal", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ART", "NN", "$."], "meter": "--+--+-+", "measure": "anapaest.di.plus"}}, "stanza.10": {"line.1": {"text": "Stoffen gibst du Gestalt, gibst dem Atom Ge-", "tokens": ["Stof\u00b7fen", "gibst", "du", "Ge\u00b7stalt", ",", "gibst", "dem", "A\u00b7tom", "Ge"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPER", "NN", "$,", "VVFIN", "ART", "NN", "TRUNC"], "meter": "+-+--+--+--", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "f\u00fchl,", "tokens": ["f\u00fchl", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Jubel f\u00fcllen den Busch, Jubel die blaue Luft.", "tokens": ["Ju\u00b7bel", "f\u00fcl\u00b7len", "den", "Busch", ",", "Ju\u00b7bel", "die", "blau\u00b7e", "Luft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "NN", "$,", "NN", "ART", "ADJA", "NN", "$."], "meter": "+-+--++--+-+", "measure": "asklepiade"}, "line.4": {"text": "Schau, es wimmelt im Tropfen,", "tokens": ["Schau", ",", "es", "wim\u00b7melt", "im", "Trop\u00b7fen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.5": {"text": "Schau, das Sandkorn bev\u00f6lkert sich.", "tokens": ["Schau", ",", "das", "Sand\u00b7korn", "be\u00b7v\u00f6l\u00b7kert", "sich", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ART", "NN", "VVFIN", "PRF", "$."], "meter": "+-+--++-", "measure": "trochaic.tetra.relaxed"}}, "stanza.11": {"line.1": {"text": "Leben, nimmer gez\u00e4hlt, preisen dich, K\u00fcnst", "tokens": ["Le\u00b7ben", ",", "nim\u00b7mer", "ge\u00b7z\u00e4hlt", ",", "prei\u00b7sen", "dich", ",", "K\u00fcnst"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word"], "pos": ["NN", "$,", "ADV", "VVPP", "$,", "VVFIN", "PPER", "$,", "NN"], "meter": "+-+---+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "lerin,", "tokens": ["le\u00b7rin", ","], "token_info": ["word", "punct"], "pos": ["ADV", "$,"], "meter": "-+", "measure": "iambic.single"}, "line.3": {"text": "Leben jeglicher Art, Kondor und Colibri,", "tokens": ["Le\u00b7ben", "jeg\u00b7li\u00b7cher", "Art", ",", "Kon\u00b7dor", "und", "Co\u00b7lib\u00b7ri", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PIAT", "NN", "$,", "NE", "KON", "NE", "$,"], "meter": "+-+--+--+-+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Strausspolype und Flusspferd,", "tokens": ["Straus\u00b7spo\u00b7ly\u00b7pe", "und", "Fluss\u00b7pferd", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$,"], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.5": {"text": "Riesenmuschel und R\u00e4derthier.", "tokens": ["Rie\u00b7sen\u00b7mu\u00b7schel", "und", "R\u00e4\u00b7der\u00b7thier", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "KON", "NN", "$."], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}}, "stanza.12": {"line.1": {"text": "Aber lauter, denn sie, preist dich des Menschen", "tokens": ["A\u00b7ber", "lau\u00b7ter", ",", "denn", "sie", ",", "preist", "dich", "des", "Men\u00b7schen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KON", "ADJD", "$,", "KON", "PPER", "$,", "VVFIN", "PRF", "ART", "NN"], "meter": "+-+---+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Geist,", "tokens": ["Geist", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+", "measure": "single.up"}, "line.3": {"text": "Dich der Kante Vernunft, dich der Gesang Homers,", "tokens": ["Dich", "der", "Kan\u00