{"textgrid.poem.66146": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An meinen Sohn am Ludwigstage", "genre": "verse", "period": "N.A.", "pub_year": 1784, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ludwig, du Sohn meines Herzens!", "tokens": ["Lud\u00b7wig", ",", "du", "Sohn", "mei\u00b7nes", "Her\u00b7zens", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "PPER", "NN", "PPOSAT", "NN", "$."], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.2": {"text": "Als dein Tag r\u00f6thlich heraufstieg,", "tokens": ["Als", "dein", "Tag", "r\u00f6th\u00b7lich", "her\u00b7auf\u00b7stieg", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADJD", "VVFIN", "$,"], "meter": "---+-+-+", "measure": "unknown.measure.tri"}, "line.3": {"text": "Und die St\u00e4be meines Gitters k\u00fc\u00dfte,", "tokens": ["Und", "die", "St\u00e4\u00b7be", "mei\u00b7nes", "Git\u00b7ters", "k\u00fc\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "PPOSAT", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "Da weint' ich gen Himmel: o Vater,", "tokens": ["Da", "weint'", "ich", "gen", "Him\u00b7mel", ":", "o", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "APPR", "NN", "$.", "FM", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.5": {"text": "Ueber alles, was Kinder hei\u00dft", "tokens": ["Ue\u00b7ber", "al\u00b7les", ",", "was", "Kin\u00b7der", "hei\u00dft"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "PIS", "$,", "PWS", "NN", "VVFIN"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.6": {"text": "Im Himmel und auf Erden!", "tokens": ["Im", "Him\u00b7mel", "und", "auf", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "KON", "APPR", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.7": {"text": "Auch ich bin Vater,", "tokens": ["Auch", "ich", "bin", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Hab' einen bl\u00fchenden Sohn!", "tokens": ["Hab'", "ei\u00b7nen", "bl\u00fc\u00b7hen\u00b7den", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.9": {"text": "Hab' eine bl\u00fchende Tochter!", "tokens": ["Hab'", "ei\u00b7ne", "bl\u00fc\u00b7hen\u00b7de", "Toch\u00b7ter", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.10": {"text": "Ach, ein armer Vater bin ich!", "tokens": ["Ach", ",", "ein", "ar\u00b7mer", "Va\u00b7ter", "bin", "ich", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "ART", "ADJA", "NN", "VAFIN", "PPER", "$."], "meter": "+-+-+-++", "measure": "unknown.measure.penta"}, "line.11": {"text": "Denn ferne thatest du mich von meinen Lieben!", "tokens": ["Denn", "fer\u00b7ne", "tha\u00b7test", "du", "mich", "von", "mei\u00b7nen", "Lie\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "PRF", "APPR", "PPOSAT", "ADJA", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.12": {"text": "Du winktest mit eisernem Arme", "tokens": ["Du", "wink\u00b7test", "mit", "ei\u00b7ser\u00b7nem", "Ar\u00b7me"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.13": {"text": "Mir ins Gef\u00e4ngnis; ich folgte,", "tokens": ["Mir", "ins", "Ge\u00b7f\u00e4ng\u00b7nis", ";", "ich", "folg\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "APPRART", "NN", "$.", "PPER", "VVFIN", "$,"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.14": {"text": "Ohne mit der Z\u00e4hre des Abschieds", "tokens": ["Oh\u00b7ne", "mit", "der", "Z\u00e4h\u00b7re", "des", "Ab\u00b7schieds"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "APPR", "ART", "NN", "ART", "NN"], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.15": {"text": "Zu netzen die Wange der Kinder!", "tokens": ["Zu", "net\u00b7zen", "die", "Wan\u00b7ge", "der", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.16": {"text": "Zu netzen die bleichere Wange", "tokens": ["Zu", "net\u00b7zen", "die", "blei\u00b7che\u00b7re", "Wan\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PTKZU", "VVINF", "ART", "ADJA", "NN"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.17": {"text": "Der Mutter meiner Kinder!", "tokens": ["Der", "Mut\u00b7ter", "mei\u00b7ner", "Kin\u00b7der", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.18": {"text": "Ach, nun sind schon viele, viel Jammermonde", "tokens": ["Ach", ",", "nun", "sind", "schon", "vie\u00b7le", ",", "viel", "Jam\u00b7mer\u00b7mon\u00b7de"], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VAFIN", "ADV", "PIS", "$,", "PIAT", "NN"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Am rostzerfre\u00dfnen Gitter meines Kerkers", "tokens": ["Am", "rost\u00b7zer\u00b7fre\u00df\u00b7nen", "Git\u00b7ter", "mei\u00b7nes", "Ker\u00b7kers"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.20": {"text": "Mit schwerem, n\u00e4chtlichem Fluge vor\u00fcbergeflogen,", "tokens": ["Mit", "schwe\u00b7rem", ",", "n\u00e4cht\u00b7li\u00b7chem", "Flu\u00b7ge", "vor\u00b7\u00fc\u00b7ber\u00b7ge\u00b7flo\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "$,", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+--+--+--+-", "measure": "iambic.penta.relaxed"}, "line.21": {"text": "Und noch streck' ich die Vaterarme", "tokens": ["Und", "noch", "streck'", "ich", "die", "Va\u00b7te\u00b7rar\u00b7me"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "ART", "NN"], "meter": "--+--+-+-", "measure": "anapaest.di.plus"}, "line.22": {"text": "Vergeblich aus nach dem Sohn meines Herzens,", "tokens": ["Ver\u00b7geb\u00b7lich", "aus", "nach", "dem", "Sohn", "mei\u00b7nes", "Her\u00b7zens", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "APPR", "ART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+--+-", "measure": "iambic.tetra.relaxed"}, "line.23": {"text": "Vergeblich nach der Tochter meines Herzens.", "tokens": ["Ver\u00b7geb\u00b7lich", "nach", "der", "Toch\u00b7ter", "mei\u00b7nes", "Her\u00b7zens", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "Im Kleide des Waisenknaben", "tokens": ["Im", "Klei\u00b7de", "des", "Wai\u00b7sen\u00b7kna\u00b7ben"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "NN", "ART", "NN"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.25": {"text": "Steht mein Sohn vor mir, im Schleier", "tokens": ["Steht", "mein", "Sohn", "vor", "mir", ",", "im", "Schlei\u00b7er"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPOSAT", "NN", "APPR", "PPER", "$,", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.26": {"text": "Des verwaisten M\u00e4dchens meine Tochter \u2013", "tokens": ["Des", "ver\u00b7wais\u00b7ten", "M\u00e4d\u00b7chens", "mei\u00b7ne", "Toch\u00b7ter", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "PPOSAT", "NN", "$("], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.27": {"text": "Zwei Bilder aus Duft gewebt,", "tokens": ["Zwei", "Bil\u00b7der", "aus", "Duft", "ge\u00b7webt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["CARD", "NN", "APPR", "NN", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.28": {"text": "Die sich bewegen im Hauche meiner Seufzer,", "tokens": ["Die", "sich", "be\u00b7we\u00b7gen", "im", "Hau\u00b7che", "mei\u00b7ner", "Seuf\u00b7zer", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "VVINF", "APPRART", "NN", "PPOSAT", "NN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.29": {"text": "Und zerflie\u00dfen vor dem ausgebreiteten Arme!", "tokens": ["Und", "zer\u00b7flie\u00b7\u00dfen", "vor", "dem", "aus\u00b7ge\u00b7brei\u00b7te\u00b7ten", "Ar\u00b7me", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.30": {"text": "Ach, ich mu\u00df sein, wie einer,", "tokens": ["Ach", ",", "ich", "mu\u00df", "sein", ",", "wie", "ei\u00b7ner", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPER", "VMFIN", "VAINF", "$,", "PWAV", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.31": {"text": "Der seiner Kinder beraubt ist.", "tokens": ["Der", "sei\u00b7ner", "Kin\u00b7der", "be\u00b7raubt", "ist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.32": {"text": "Ich werde mit Herzleid fahren", "tokens": ["Ich", "wer\u00b7de", "mit", "Herz\u00b7leid", "fah\u00b7ren"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "APPR", "NN", "VVINF"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.33": {"text": "Hinunter in die Grube,", "tokens": ["Hin\u00b7un\u00b7ter", "in", "die", "Gru\u00b7be", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.34": {"text": "Eh' ich seh' Ludwig, meinen Sohn!", "tokens": ["Eh'", "ich", "seh'", "Lud\u00b7wig", ",", "mei\u00b7nen", "Sohn", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KOUS", "PPER", "VVFIN", "NE", "$,", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.35": {"text": "Juliana, meine Tochter!", "tokens": ["Ju\u00b7li\u00b7a\u00b7na", ",", "mei\u00b7ne", "Toch\u00b7ter", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.36": {"text": "Vergieb mir's, o du aller V\u00e4terlichkeit,", "tokens": ["Ver\u00b7gieb", "mir's", ",", "o", "du", "al\u00b7ler", "V\u00e4\u00b7ter\u00b7lich\u00b7keit", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "NE", "$,", "FM", "PPER", "PIAT", "NN", "$,"], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.37": {"text": "Aller M\u00fctterlichkeit Urquell,", "tokens": ["Al\u00b7ler", "M\u00fct\u00b7ter\u00b7lich\u00b7keit", "Ur\u00b7quell", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "NE", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.38": {"text": "Wenn ich in der Nacht meines Kerkergew\u00f6lbes", "tokens": ["Wenn", "ich", "in", "der", "Nacht", "mei\u00b7nes", "Ker\u00b7ker\u00b7ge\u00b7w\u00f6l\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.39": {"text": "Einsam steh' und weine!", "tokens": ["Ein\u00b7sam", "steh'", "und", "wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.40": {"text": "Auch du bist Vater,", "tokens": ["Auch", "du", "bist", "Va\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPER", "VAFIN", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.41": {"text": "Und lie\u00dfest fallen eine Z\u00e4hre,", "tokens": ["Und", "lie\u00b7\u00dfest", "fal\u00b7len", "ei\u00b7ne", "Z\u00e4h\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.42": {"text": "Da\u00df die Sonne erlosch,", "tokens": ["Da\u00df", "die", "Son\u00b7ne", "er\u00b7losch", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ADJD", "$,"], "meter": "--+--+", "measure": "anapaest.di.plus"}, "line.43": {"text": "Als dein Sohn Jesus", "tokens": ["Als", "dein", "Sohn", "Je\u00b7sus"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPOSAT", "NN", "NE"], "meter": "+-+-+", "measure": "trochaic.tri"}, "line.44": {"text": "Herunterhing am blutigen Kreuze!", "tokens": ["Her\u00b7un\u00b7ter\u00b7hing", "am", "blu\u00b7ti\u00b7gen", "Kreu\u00b7ze", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-", "measure": "iambic.tetra.relaxed"}, "line.45": {"text": "Ach, drum vergieb mir, du Bilder", "tokens": ["Ach", ",", "drum", "ver\u00b7gieb", "mir", ",", "du", "Bil\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["ITJ", "$,", "PAV", "VVFIN", "PPER", "$,", "PPER", "NN"], "meter": "+--+--+-", "measure": "dactylic.tri"}, "line.46": {"text": "Des Vaterherzens \u2013 o du!", "tokens": ["Des", "Va\u00b7ter\u00b7her\u00b7zens", "\u2013", "o", "du", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "$(", "FM", "PPER", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.47": {"text": "Der den Silberquell der Mutterbrust", "tokens": ["Der", "den", "Sil\u00b7ber\u00b7quell", "der", "Mut\u00b7ter\u00b7brust"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "ART", "NN", "ART", "NN"], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.48": {"text": "Str\u00f6men hie\u00df! Vergieb mir,", "tokens": ["Str\u00f6\u00b7men", "hie\u00df", "!", "Ver\u00b7gieb", "mir", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$.", "VVIMP", "PPER", "$,"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.49": {"text": "Wenn ich in der Nacht meines Kerkergew\u00f6lbes", "tokens": ["Wenn", "ich", "in", "der", "Nacht", "mei\u00b7nes", "Ker\u00b7ker\u00b7ge\u00b7w\u00f6l\u00b7bes"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "NN", "PPOSAT", "NN"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.50": {"text": "Einsam steh' und weine!", "tokens": ["Ein\u00b7sam", "steh'", "und", "wei\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "KON", "VVFIN", "$."], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.51": {"text": "Ach, la\u00df mich dir danken mit Thr\u00e4nen,", "tokens": ["Ach", ",", "la\u00df", "mich", "dir", "dan\u00b7ken", "mit", "Thr\u00e4\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "VVIMP", "PPER", "PPER", "VVFIN", "APPR", "NN", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.52": {"text": "Da\u00df du mir einen Sohn gabst,", "tokens": ["Da\u00df", "du", "mir", "ei\u00b7nen", "Sohn", "gabst", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPER", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.53": {"text": "Da\u00df du ihn betr\u00e4uftest", "tokens": ["Da\u00df", "du", "ihn", "be\u00b7tr\u00e4uf\u00b7test"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "PPER", "PPER", "VVFIN"], "meter": "+-+-+-", "measure": "trochaic.tri"}, "line.54": {"text": "Mit des wiedergeb\u00e4renden Bades", "tokens": ["Mit", "des", "wie\u00b7der\u00b7ge\u00b7b\u00e4\u00b7ren\u00b7den", "Ba\u00b7des"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "ART", "ADJA", "NN"], "meter": "--+--+--+-", "measure": "anapaest.tri.plus"}, "line.55": {"text": "Heiligem Wasser; da\u00df du ihn sch\u00fctztest,", "tokens": ["Hei\u00b7li\u00b7gem", "Was\u00b7ser", ";", "da\u00df", "du", "ihn", "sch\u00fctz\u00b7test", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "KOUS", "PPER", "PPER", "VVFIN", "$,"], "meter": "+--+--+-+-", "measure": "dactylic.di.plus"}, "line.56": {"text": "Als der nahe Tod gift