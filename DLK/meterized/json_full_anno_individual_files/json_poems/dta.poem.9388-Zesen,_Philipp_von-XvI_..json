{"dta.poem.9388": {"metadata": {"author": {"name": "Zesen, Philipp von", "birth": "N.A.", "death": "N.A."}, "title": "XvI .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1641", "urn": "urn:nbn:de:kobv:b4-20684-8", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es besch\u00e4men jhre Wangen", "tokens": ["Es", "be\u00b7sch\u00e4\u00b7men", "jhre", "Wan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN"], "meter": "--+--+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "das bebl\u00fcmte garten-beet/", "tokens": ["das", "be\u00b7bl\u00fcm\u00b7te", "gar\u00b7ten\u00b7beet", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "da die tausentsch\u00f6nen prangen", "tokens": ["da", "die", "tau\u00b7sent\u00b7sch\u00f6\u00b7nen", "pran\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "und die weisse Rose steht;", "tokens": ["und", "die", "weis\u00b7se", "Ro\u00b7se", "steht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Ich kann sagen/ da\u00df Narcissen", "tokens": ["Ich", "kann", "sa\u00b7gen", "/", "da\u00df", "Nar\u00b7cis\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VMFIN", "VVINF", "$(", "KOUS", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Jhrer Stirne weichen m\u00fcssen.", "tokens": ["Ih\u00b7rer", "Stir\u00b7ne", "wei\u00b7chen", "m\u00fcs\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVINF", "VMINF", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}}}}