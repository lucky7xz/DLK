{"dta.poem.1182": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die Ku\u00df-Scheue.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du stellest dich so wilde ", "tokens": ["Du", "stel\u00b7lest", "dich", "so", "wil\u00b7de"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "ADV", "ADJA"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Wenn ich dich k\u00fcssen will:", "tokens": ["Wenn", "ich", "dich", "k\u00fcs\u00b7sen", "will", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "VVINF", "VMFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Wilt du dich nennen milde/", "tokens": ["Wilt", "du", "dich", "nen\u00b7nen", "mil\u00b7de", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "VVINF", "ADJD", "$("], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.4": {"text": "So weigre dich nicht viel.", "tokens": ["So", "weig\u00b7re", "dich", "nicht", "viel", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKNEG", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Allmosen bald empfangen", "tokens": ["All\u00b7mo\u00b7sen", "bald", "emp\u00b7fan\u00b7gen"], "token_info": ["word", "word", "word"], "pos": ["ADV", "ADV", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ist einstens noch so lieb/", "tokens": ["Ist", "eins\u00b7tens", "noch", "so", "lieb", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ADV", "ADV", "ADJD", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Als was man mu\u00df erlangen", "tokens": ["Als", "was", "man", "mu\u00df", "er\u00b7lan\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PWS", "PIS", "VMFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Durch langen Bittens-Trieb.", "tokens": ["Durch", "lan\u00b7gen", "Bit\u00b7tens\u00b7Trieb", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Verziehestu zu geben/", "tokens": ["Ver\u00b7zie\u00b7hes\u00b7tu", "zu", "ge\u00b7ben", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "PTKZU", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Was du doch lo\u00df wilt seyn?", "tokens": ["Was", "du", "doch", "lo\u00df", "wilt", "seyn", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADV", "VMFIN", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich wills mit Wucher heben", "tokens": ["Ich", "wills", "mit", "Wu\u00b7cher", "he\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "APPR", "NN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und doppelt bringen ein.", "tokens": ["Und", "dop\u00b7pelt", "brin\u00b7gen", "ein", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVINF", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.4": {"line.1": {"text": "Du w\u00fcrdest meiner spotten", "tokens": ["Du", "w\u00fcr\u00b7dest", "mei\u00b7ner", "spot\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Lie\u00df ich dich gehn vorbey/", "tokens": ["Lie\u00df", "ich", "dich", "gehn", "vor\u00b7bey", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "PRF", "VVFIN", "PTKVZ", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sagen/ da\u00df zum Gutten", "tokens": ["Und", "sa\u00b7gen", "/", "da\u00df", "zum", "Gut\u00b7ten"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVINF", "$(", "KOUS", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Ich viel zu furchtsam sey.", "tokens": ["Ich", "viel", "zu", "furcht\u00b7sam", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADV", "PTKA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "Drum Cloris la\u00df dich k\u00fcssen;", "tokens": ["Drum", "Clo\u00b7ris", "la\u00df", "dich", "k\u00fcs\u00b7sen", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NE", "VVIMP", "PPER", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und soltest du zum Schein", "tokens": ["Und", "sol\u00b7test", "du", "zum", "Schein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VMFIN", "PPER", "APPRART", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dich widersetzen m\u00fcssen:", "tokens": ["Dich", "wi\u00b7der\u00b7set\u00b7zen", "m\u00fcs\u00b7sen", ":"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "VMINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Es mu\u00df gek\u00fcsset seyn.", "tokens": ["Es", "mu\u00df", "ge\u00b7k\u00fcs\u00b7set", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}}}}