{"dta.poem.21123": {"metadata": {"author": {"name": "Zachariae, Justus Friedrich Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Phantasie .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1764", "urn": "urn:nbn:de:kobv:b4-20676-2", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Okehre wieder zur\u00fcck, schwarzer Gedanke,", "tokens": ["Ok\u00b7eh\u00b7re", "wie\u00b7der", "zu\u00b7r\u00fcck", ",", "schwar\u00b7zer", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Zum Throne der Melancholey!", "tokens": ["Zum", "Thro\u00b7ne", "der", "Me\u00b7lan\u00b7cho\u00b7ley", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "In mir erbebend, sah ich, G\u00f6ttin der Schwermuth,", "tokens": ["In", "mir", "er\u00b7be\u00b7bend", ",", "sah", "ich", ",", "G\u00f6t\u00b7tin", "der", "Schwer\u00b7muth", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "VVPP", "$,", "VVFIN", "PPER", "$,", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+--++", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "Gesandten deines finstern Hoss.", "tokens": ["Ge\u00b7sand\u00b7ten", "dei\u00b7nes", "fins\u00b7tern", "Hoss", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Schon \u00fcberschatteten mich gr\u00e4\u00dfliche Fl\u00fcgel", "tokens": ["Schon", "\u00fc\u00b7bersc\u00b7hat\u00b7te\u00b7ten", "mich", "gr\u00e4\u00df\u00b7li\u00b7che", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJA", "NN"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Der schreckensvollen Einbildung.", "tokens": ["Der", "schre\u00b7ckens\u00b7vol\u00b7len", "Ein\u00b7bil\u00b7dung", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Es schw\u00e4rmten um mich herum schwarze Phantomen,", "tokens": ["Es", "schw\u00e4rm\u00b7ten", "um", "mich", "he\u00b7rum", "schwar\u00b7ze", "Phan\u00b7to\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PRF", "APZR", "ADJA", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "Die in dem schweren Blut entstehn.", "tokens": ["Die", "in", "dem", "schwe\u00b7ren", "Blut", "ent\u00b7stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Ich gieng in Gr\u00e4bern herum unter den Todten,", "tokens": ["Ich", "gieng", "in", "Gr\u00e4\u00b7bern", "he\u00b7rum", "un\u00b7ter", "den", "Tod\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "APZR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und Geister kamen um mich her.", "tokens": ["Und", "Geis\u00b7ter", "ka\u00b7men", "um", "mich", "her", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "VVFIN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Seline selbst trat daher himmlisch gestaltet,", "tokens": ["Se\u00b7li\u00b7ne", "selbst", "trat", "da\u00b7her", "himm\u00b7lisch", "ge\u00b7stal\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADV", "VVFIN", "PAV", "ADJD", "VVPP", "$,"], "meter": "+---+-++--+-", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "Mit einem Blumenkranz gekr\u00e4nzt.", "tokens": ["Mit", "ei\u00b7nem", "Blu\u00b7men\u00b7kranz", "ge\u00b7kr\u00e4nzt", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie setzte sich an den Fu\u00df einer Cypresse,", "tokens": ["Sie", "setz\u00b7te", "sich", "an", "den", "Fu\u00df", "ei\u00b7ner", "Cyp\u00b7res\u00b7se", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "APPR", "ART", "NN", "ART", "NN", "$,"], "meter": "-+--+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Die rauschend aus dem Grabe wuchs;", "tokens": ["Die", "rau\u00b7schend", "aus", "dem", "Gra\u00b7be", "wuchs", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Sie lachte mich an, doch die Augen erstarben,", "tokens": ["Sie", "lach\u00b7te", "mich", "an", ",", "doch", "die", "Au\u00b7gen", "er\u00b7star\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PTKVZ", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.4": {"text": "In denen ich den Himmel sah;", "tokens": ["In", "de\u00b7nen", "ich", "den", "Him\u00b7mel", "sah", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Und es ward Schrecken und Nacht, da sie erbla\u00dfte,", "tokens": ["Und", "es", "ward", "Schre\u00b7cken", "und", "Nacht", ",", "da", "sie", "er\u00b7bla\u00df\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VAFIN", "NN", "KON", "NN", "$,", "KOUS", "PPER", "VVFIN", "$,"], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Und mein Geschrey durchdrang die Luft \u2014", "tokens": ["Und", "mein", "Ge\u00b7schrey", "durch\u00b7drang", "die", "Luft"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "VVFIN", "ART", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "O kehre wieder zur\u00fcck, schwarzer Gedanke,", "tokens": ["O", "keh\u00b7re", "wie\u00b7der", "zu\u00b7r\u00fcck", ",", "schwar\u00b7zer", "Ge\u00b7dan\u00b7ke", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "VVFIN", "ADV", "PTKVZ", "$,", "ADJA", "NN", "$,"], "meter": "-+-+---+--+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Zum Throne der Melancholey!", "tokens": ["Zum", "Thro\u00b7ne", "der", "Me\u00b7lan\u00b7cho\u00b7ley", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}