{"dta.poem.1410": {"metadata": {"author": {"name": "Abschatz, Hans Assmann von", "birth": "N.A.", "death": "N.A."}, "title": "Tauff- und Pathen-W\u00fcnsche.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1704", "urn": "urn:nbn:de:kobv:b4-200905199889", "language": ["de:0.99"], "booktitle": "Abschatz, Hans Assmann von: Poetische Ubersetzungen und Gedichte. Leipzig, 1704."}, "poem": {"stanza.1": {"line.1": {"text": "Du begr\u00fcssest diese Welt in der Erb-Schuld schwartzem", "tokens": ["Du", "be\u00b7gr\u00fcs\u00b7sest", "die\u00b7se", "Welt", "in", "der", "Er\u00b7bSchuld", "schwart\u00b7zem"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Kleide/", "tokens": ["Klei\u00b7de", "/"], "token_info": ["word", "punct"], "pos": ["NN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.3": {"text": "Da das ewig-reine Kind unsre Menschheit ziehet an.", "tokens": ["Da", "das", "e\u00b7wig\u00b7rei\u00b7ne", "Kind", "uns\u00b7re", "Menschheit", "zie\u00b7het", "an", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "PPOSAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "+-+-+-+-+-+-+", "measure": "trochaic.septa"}, "line.4": {"text": "Dein Erl\u00f6ser/ der die Tage ward mit Purpur angethan/", "tokens": ["Dein", "Er\u00b7l\u00f6\u00b7ser", "/", "der", "die", "Ta\u00b7ge", "ward", "mit", "Pur\u00b7pur", "an\u00b7ge\u00b7than", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$(", "ART", "ART", "NN", "VAFIN", "APPR", "NN", "VVPP", "$("], "meter": "+-+-+-+-+-+-+-+", "measure": "trochaic.octa.plus"}, "line.5": {"text": "Kleidet dich mit Bad und Blutt/ mit der Unschuld weisser", "tokens": ["Klei\u00b7det", "dich", "mit", "Bad", "und", "Blutt", "/", "mit", "der", "Un\u00b7schuld", "weis\u00b7ser"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PRF", "APPR", "NE", "KON", "ADJD", "$(", "APPR", "ART", "NN", "ADJA"], "meter": "+-+-+-+--+-+-", "measure": "trochaic.hexa.relaxed"}, "line.6": {"text": "Seide.", "tokens": ["Sei\u00b7de", "."], "token_info": ["word", "punct"], "pos": ["NE", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.7": {"text": "Tugend-Purpur/ Ehren-Sammt/ Gl\u00fcck und Alter soll dich", "tokens": ["Tu\u00b7gen\u00b7dPur\u00b7pur", "/", "Eh\u00b7ren\u00b7Sammt", "/", "Gl\u00fcck", "und", "Al\u00b7ter", "soll", "dich"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "$(", "NN", "$(", "NN", "KON", "NN", "VMFIN", "PPER"], "meter": "+-+-+-++-+--+", "measure": "iambic.septa.chol"}, "line.8": {"text": "zieren/", "tokens": ["zie\u00b7ren", "/"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$("], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Bi\u00df du ewger Herrligkeit sch\u00f6nen Oster-Schmuck wirst f\u00fch-", "tokens": ["Bi\u00df", "du", "ew\u00b7ger", "Herr\u00b7lig\u00b7keit", "sch\u00f6\u00b7nen", "Os\u00b7ter\u00b7Schmuck", "wirst", "f\u00fch"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ADJA", "NN", "ADJA", "NN", "VAFIN", "TRUNC"], "meter": "+-+-+--+-+-+-+", "measure": "trochaic.septa.relaxed"}, "line.10": {"text": "ren.", "tokens": ["ren", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "-", "measure": "single.down"}}}}}