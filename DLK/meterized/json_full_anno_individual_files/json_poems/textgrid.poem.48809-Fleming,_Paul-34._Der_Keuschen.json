{"textgrid.poem.48809": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "34. Der Keuschen", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Wenn sich die G\u00f6tter auch befreien gleich als wir,", "tokens": ["Wenn", "sich", "die", "G\u00f6t\u00b7ter", "auch", "be\u00b7frei\u00b7en", "gleich", "als", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVFIN", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so n\u00e4hme dich der Schmuck auch selbst zu seinem Weibe,", "tokens": ["so", "n\u00e4h\u00b7me", "dich", "der", "Schmuck", "auch", "selbst", "zu", "sei\u00b7nem", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dieweil die Keuschheit wohnt in einem solchen Leibe,", "tokens": ["die\u00b7weil", "die", "Keuschheit", "wohnt", "in", "ei\u00b7nem", "sol\u00b7chen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "VVFIN", "APPR", "ART", "PIAT", "NN", "$,"], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.4": {"text": "in welchem sind gleich hoch die Tugend und die Zier,", "tokens": ["in", "wel\u00b7chem", "sind", "gleich", "hoch", "die", "Tu\u00b7gend", "und", "die", "Zier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "VAFIN", "ADV", "ADJD", "ART", "NN", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "der Geist und die Gestalt. Wie seltsam ist di\u00df hier!", "tokens": ["der", "Geist", "und", "die", "Ge\u00b7stalt", ".", "Wie", "selt\u00b7sam", "ist", "di\u00df", "hier", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "ART", "NN", "$.", "PWAV", "ADJD", "VAFIN", "PDS", "ADV", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Denk, Jungfrau, da\u00df ich nicht was Ungemeintes schreibe", "tokens": ["Denk", ",", "Jung\u00b7frau", ",", "da\u00df", "ich", "nicht", "was", "Un\u00b7ge\u00b7mein\u00b7tes", "schrei\u00b7be"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "$,", "NN", "$,", "KOUS", "PPER", "PTKNEG", "PWS", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "und dich berede des, was ich mir selbst nicht gl\u00e4ube.", "tokens": ["und", "dich", "be\u00b7re\u00b7de", "des", ",", "was", "ich", "mir", "selbst", "nicht", "gl\u00e4u\u00b7be", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "$,", "PWS", "PPER", "PPER", "ADV", "PTKNEG", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Dein Zeugn\u00fc\u00df bist selbst du. Du sprichst es selbst von dir.", "tokens": ["Dein", "Zeug\u00b7n\u00fc\u00df", "bist", "selbst", "du", ".", "Du", "sprichst", "es", "selbst", "von", "dir", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "ADV", "PPER", "$.", "PPER", "VVFIN", "PPER", "ADV", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Die sch\u00f6nste Sch\u00f6nheit ist ein z\u00fcchtiges Gem\u00fcte;", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Sch\u00f6n\u00b7heit", "ist", "ein", "z\u00fcch\u00b7ti\u00b7ges", "Ge\u00b7m\u00fc\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "was eine Jungfer ziert, das wohnet im Gebl\u00fcte.", "tokens": ["was", "ei\u00b7ne", "Jung\u00b7fer", "ziert", ",", "das", "woh\u00b7net", "im", "Ge\u00b7bl\u00fc\u00b7te", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PDS", "VVFIN", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Das Ander, was das Volk f\u00fcr sch\u00f6ne h\u00e4lt und hei\u00dft,", "tokens": ["Das", "An\u00b7der", ",", "was", "das", "Volk", "f\u00fcr", "sch\u00f6\u00b7ne", "h\u00e4lt", "und", "hei\u00dft", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ART", "NN", "APPR", "ADJA", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "der Seelen \u00dcberzug, der Leib pflegt oft zu triegen.", "tokens": ["der", "See\u00b7len", "\u00dc\u00b7ber\u00b7zug", ",", "der", "Leib", "pflegt", "oft", "zu", "trie\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "$,", "ART", "NN", "VVFIN", "ADV", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da ist ein sch\u00f6ner Leib, da ist ein sch\u00f6ner Geist,", "tokens": ["Da", "ist", "ein", "sch\u00f6\u00b7ner", "Leib", ",", "da", "ist", "ein", "sch\u00f6\u00b7ner", "Geist", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "$,", "ADV", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wenn sie als hier den Glanz von wahrer Sch\u00f6nheit kriegen.", "tokens": ["wenn", "sie", "als", "hier", "den", "Glanz", "von", "wah\u00b7rer", "Sch\u00f6n\u00b7heit", "krie\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOKOM", "ADV", "ART", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Wenn sich die G\u00f6tter auch befreien gleich als wir,", "tokens": ["Wenn", "sich", "die", "G\u00f6t\u00b7ter", "auch", "be\u00b7frei\u00b7en", "gleich", "als", "wir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "ART", "NN", "ADV", "VVFIN", "ADV", "KOUS", "PPER", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "so n\u00e4hme dich der Schmuck auch selbst zu seinem Weibe,", "tokens": ["so", "n\u00e4h\u00b7me", "dich", "der", "Schmuck", "auch", "selbst", "zu", "sei\u00b7nem", "Wei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "dieweil die Keuschheit wohnt in einem solchen Leibe,", "tokens": ["die\u00b7weil", "die", "Keuschheit", "wohnt", "in", "ei\u00b7nem", "sol\u00b7chen", "Lei\u00b7be", ","], "token_info": ["word", "word", "word", "word", "word", "wor