{"textgrid.poem.25516": {"metadata": {"author": {"name": "Goeckingk, Leopold Friedrich G\u00fcnther von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Gleich ihr, so prunklos, so bescheiden!", "genre": "verse", "period": "N.A.", "pub_year": 1788, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Gleich ihr, so prunklos, so bescheiden!", "tokens": ["Gleich", "ihr", ",", "so", "prun\u00b7klos", ",", "so", "be\u00b7schei\u00b7den", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PPER", "$,", "ADV", "ADJD", "$,", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Und doch \u2013 um Goldstoff tauscht' ich's nicht.", "tokens": ["Und", "doch", "\u2013", "um", "Gold\u00b7stoff", "tauscht'", "ich's", "nicht", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$(", "APPR", "NN", "VVFIN", "PIS", "PTKNEG", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Es wei\u00df allein um ihre Leiden,", "tokens": ["Es", "wei\u00df", "al\u00b7lein", "um", "ih\u00b7re", "Lei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Und sah allein bei Mondenlicht", "tokens": ["Und", "sah", "al\u00b7lein", "bei", "Mon\u00b7den\u00b7licht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "ADV", "APPR", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Des M\u00e4dchens stille Thr\u00e4ne rinnen,", "tokens": ["Des", "M\u00e4d\u00b7chens", "stil\u00b7le", "Thr\u00e4\u00b7ne", "rin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.6": {"text": "Und fing sie im Verborgnen auf,", "tokens": ["Und", "fing", "sie", "im", "Ver\u00b7borg\u00b7nen", "auf", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "APPRART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und h\u00f6rt' allein ihr Seufzen: Weg von hinnen!", "tokens": ["Und", "h\u00f6rt'", "al\u00b7lein", "ihr", "Seuf\u00b7zen", ":", "Weg", "von", "hin\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PPOSAT", "NN", "$.", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Hinauf! zu Gott hinauf!", "tokens": ["Hin\u00b7auf", "!", "zu", "Gott", "hin\u00b7auf", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "APPR", "NN", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.9": {"text": "Auch meine Thr\u00e4nen hat's getrunken,", "tokens": ["Auch", "mei\u00b7ne", "Thr\u00e4\u00b7nen", "hat's", "ge\u00b7trun\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VAFIN", "VVPP", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Als ich \u2013 o Nacht des neunten M\u00e4rz! \u2013", "tokens": ["Als", "ich", "\u2013", "o", "Nacht", "des", "neun\u00b7ten", "M\u00e4rz", "!", "\u2013"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KOUS", "PPER", "$(", "FM", "NN", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "An ihrem Busen lag, versunken", "tokens": ["An", "ih\u00b7rem", "Bu\u00b7sen", "lag", ",", "ver\u00b7sun\u00b7ken"], "token_info": ["word", "word", "word", "word", "punct", "word"], "