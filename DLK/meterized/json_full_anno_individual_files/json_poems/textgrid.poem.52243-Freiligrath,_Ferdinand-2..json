{"textgrid.poem.52243": {"metadata": {"author": {"name": "Freiligrath, Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "2.", "genre": "verse", "period": "N.A.", "pub_year": 1843, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schwarz, Rot und Gold! Frei weht ihr auf den Stangen", "tokens": ["Schwarz", ",", "Rot", "und", "Gold", "!", "Frei", "weht", "ihr", "auf", "den", "Stan\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NE", "$,", "NN", "KON", "NN", "$.", "NN", "VVFIN", "PPER", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und Masten jetzo, g\u00fcrtend rings das Land!", "tokens": ["Und", "Mas\u00b7ten", "jet\u00b7zo", ",", "g\u00fcr\u00b7tend", "rings", "das", "Land", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "ADV", "$,", "ADJD", "ADV", "ART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "In tausend Wimpeln, einst verp\u00f6ntes Band,", "tokens": ["In", "tau\u00b7send", "Wim\u00b7peln", ",", "einst", "ver\u00b7p\u00f6n\u00b7tes", "Band", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "CARD", "NN", "$,", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Hat dich der Ozean selber umgehangen!", "tokens": ["Hat", "dich", "der", "O\u00b7ze\u00b7an", "sel\u00b7ber", "um\u00b7ge\u00b7han\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "ADV", "VVPP", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "O, st\u00e4nden jetzt, die Anno Neunzehn sangen,", "tokens": ["O", ",", "st\u00e4n\u00b7den", "jetzt", ",", "die", "An\