{"textgrid.poem.65284": {"metadata": {"author": {"name": "M\u00fcller, Wilhelm", "birth": "N.A.", "death": "N.A."}, "title": "Der Apfelbaum", "genre": "verse", "period": "N.A.", "pub_year": 1810, "urn": "N.A.", "language": ["de:0.85", "fr:0.14"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was dr\u00fcckst du so tief in die Stirn den Hut?", "tokens": ["Was", "dr\u00fcckst", "du", "so", "tief", "in", "die", "Stirn", "den", "Hut", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "ADV", "ADJD", "APPR", "ART", "NN", "ART", "NN", "$."], "meter": "-+--+--+-+", "measure": "amphibrach.tri.plus"}, "line.2": {"text": "Wohin so fr\u00fch, du junges Blut?", "tokens": ["Wo\u00b7hin", "so", "fr\u00fch", ",", "du", "jun\u00b7ges", "Blut", "?"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "ADJD", "$,", "PPER", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "\u00bbherr Th\u00fcrmer, schlie\u00dft nur auf das Thor!", "tokens": ["\u00bb", "herr", "Th\u00fcr\u00b7mer", ",", "schlie\u00dft", "nur", "auf", "das", "Thor", "!"], "token_info": ["punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "NN", "NN", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Ich hab' eine lange Reise vor.\u00ab", "tokens": ["Ich", "hab'", "ei\u00b7ne", "lan\u00b7ge", "Rei\u00b7se", "vor", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "PTKVZ", "$.", "$("], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.5": {"text": "Und also ging's zur Stadt hinaus,", "tokens": ["Und", "al\u00b7so", "ging's", "zur", "Stadt", "hin\u00b7aus", ","], "token_info": ["word", "word", "wo