{"dta.poem.1046": {"metadata": {"author": {"name": "Weckherlin, Georg Rodolf", "birth": "N.A.", "death": "N.A."}, "title": "Die 4. Strophe.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1618", "urn": "urn:nbn:de:kobv:b4-20628-9", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Gleich wie/ wan der winter sich endet/", "tokens": ["Gleich", "wie", "/", "wan", "der", "win\u00b7ter", "sich", "en\u00b7det", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOKOM", "$(", "PWAV", "ART", "ADJA", "PRF", "VVFIN", "$("], "meter": "+-+-+--+-", "measure": "trochaic.tetra.relaxed"}, "line.2": {"text": "Apollons fruchtbares gesicht", "tokens": ["A\u00b7pol\u00b7lons", "frucht\u00b7ba\u00b7res", "ge\u00b7sicht"], "token_info": ["word", "word", "word"], "pos": ["NE", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Der erden hartes hertz zubricht", "tokens": ["Der", "er\u00b7den", "har\u00b7tes", "hertz", "zu\u00b7bricht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Alsbald er sich gegen jhr wendet/", "tokens": ["Als\u00b7bald", "er", "sich", "ge\u00b7gen", "jhr", "wen\u00b7det", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PRF", "APPR", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "Vnd durch seinen glantz allgemein", "tokens": ["Vnd", "durch", "sei\u00b7nen", "glantz", "all\u00b7ge\u00b7mein"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "PPOSAT", "NN", "ADJD"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "Zumahl von vnsern sehl vnd tagen", "tokens": ["Zu\u00b7mahl", "von", "vn\u00b7sern", "sehl", "vnd", "ta\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "APPR", "PPOSAT", "NN", "KON", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.7": {"text": "Pfleget die dunckelheit zu jagen", "tokens": ["Pfle\u00b7get", "die", "dun\u00b7ckel\u00b7heit", "zu", "ja\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "PTKZU", "VVINF"], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}, "line.8": {"text": "Mit dem doppelt w\u00fcrckenden schein:", "tokens": ["Mit", "dem", "dop\u00b7pelt", "w\u00fcr\u00b7cken\u00b7den", "schein", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "VAFIN", "ADJD", "$."], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.9": {"text": "Also/ G\u00f6ttin/ in dem wir sehen", "tokens": ["Al\u00b7so", "/", "G\u00f6t\u00b7tin", "/", "in", "dem", "wir", "se\u00b7hen"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$(", "NE", "$(", "APPR", "PRELS", "PPER", "VVINF"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.10": {"text": "Alhie deine Sonnen aufgehen/", "tokens": ["Al\u00b7hie", "dei\u00b7ne", "Son\u00b7nen", "auf\u00b7ge\u00b7hen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PPOSAT", "NN", "VVINF", "$("], "meter": "+-+-+-+--", "measure": "unknown.measure.tetra"}, "line.11": {"text": "Empfindet vnser schwache brust", "tokens": ["Emp\u00b7fin\u00b7det", "vn\u00b7ser", "schwa\u00b7che", "brust"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.12": {"text": "Eines fr\u00fclings trost-reichen lust;", "tokens": ["Ei\u00b7nes", "fr\u00fc\u00b7lings", "trost\u00b7rei\u00b7chen", "lust", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADJA", "NN", "$."], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.13": {"text": "Vnd wir/ so gn\u00e4diglich gewehret", "tokens": ["Vnd", "wir", "/", "so", "gn\u00e4\u00b7di\u00b7glich", "ge\u00b7weh\u00b7ret"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PPER", "$(", "ADV", "ADJD", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.14": {"text": "Deiner gegenwertigen gunst/", "tokens": ["Dei\u00b7ner", "ge\u00b7gen\u00b7wer\u00b7ti\u00b7gen", "gunst", "/"], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$("], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.15": {"text": "Sehen vnsere k\u00e4lt verkehret", "tokens": ["Se\u00b7hen", "vn\u00b7se\u00b7re", "k\u00e4lt", "ver\u00b7keh\u00b7ret"], "token_info": ["word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "VVFIN", "VVFIN"], "meter": "+-+--+-+-", "measure": "trochaic.tetra.relaxed"}, "line.16": {"text": "In ein klar angenehme brunst.", "tokens": ["In", "ein", "klar", "an\u00b7ge\u00b7neh\u00b7me", "brunst", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}