{"dta.poem.2179": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Meynung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Sehr wenig Menschen sind geschickt, ob sie sich selbst", "tokens": ["Sehr", "we\u00b7nig", "Men\u00b7schen", "sind", "ge\u00b7schickt", ",", "ob", "sie", "sich", "selbst"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "PIAT", "NN", "VAFIN", "VVPP", "$,", "KOUS", "PPER", "PRF", "ADV"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "gleich nicht so scheinen,", "tokens": ["gleich", "nicht", "so", "schei\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ADV", "VVFIN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Jhr eigne Meynungen zu haben. Nein, alles andern", "tokens": ["Ihr", "eig\u00b7ne", "Mey\u00b7nun\u00b7gen", "zu", "ha\u00b7ben", ".", "Nein", ",", "al\u00b7les", "an\u00b7dern"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "PTKZU", "VAINF", "$.", "PTKANT", "$,", "PIAT", "ADJA"], "meter": "-+-++--+--+-+-", "measure": "iambic.hexa.relaxed"}, "line.4": {"text": "nach zu meynen.", "tokens": ["nach", "zu", "mey\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "PTKZU", "VVINF", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}