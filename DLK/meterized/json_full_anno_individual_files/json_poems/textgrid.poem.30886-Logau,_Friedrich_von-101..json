{"textgrid.poem.30886": {"metadata": {"author": {"name": "Logau, Friedrich von", "birth": "N.A.", "death": "N.A."}, "title": "101.", "genre": "verse", "period": "N.A.", "pub_year": 1630, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "K\u00f6nig der Tage, du herrliches Licht!", "tokens": ["K\u00f6\u00b7nig", "der", "Ta\u00b7ge", ",", "du", "herr\u00b7li\u00b7ches", "Licht", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "PPER", "ADJA", "NN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.2": {"text": "Drinnen man jauchzet, sich muntert und spricht:", "tokens": ["Drin\u00b7nen", "man", "jauch\u00b7zet", ",", "sich", "mun\u00b7tert", "und", "spricht", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "$,", "PRF", "VVFIN", "KON", "VVFIN", "$."], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.3": {"text": "Briegische Cedern verneuen das steigen,", "tokens": ["Brie\u00b7gi\u00b7sche", "Ce\u00b7dern", "ver\u00b7neu\u00b7en", "das", "stei\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVFIN", "PDS", "VVINF", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.4": {"text": "Steigen gen Himmel mit j\u00fcngeren Zweigen.", "tokens": ["Stei\u00b7gen", "gen", "Him\u00b7mel", "mit", "j\u00fcn\u00b7ge\u00b7ren", "Zwei\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "APPR", "ADJA", "NN", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.5": {"text": "Es wachse die Pflantze, das f\u00fcrstliche Blut;", "tokens": ["Es", "wach\u00b7se", "die", "Pflant\u00b7ze", ",", "das", "f\u00fcrst\u00b7li\u00b7che", "Blut", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.6": {"text": "Sie ziere mit Glantze den f\u00fcrstlichen Hut!", "tokens": ["Sie", "zie\u00b7re", "mit", "Glant\u00b7ze", "den", "f\u00fcrst\u00b7li\u00b7chen", "Hut", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.7": {"text": "St\u00fctze de\u00df Hauses, Piastisches Kind,", "tokens": ["St\u00fct\u00b7ze", "de\u00df", "Hau\u00b7ses", ",", "Pi\u00b7as\u00b7ti\u00b7sches", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.8": {"text": "Deme gewierig und pflichtbar wir sind,", "tokens": ["De\u00b7me", "ge\u00b7wie\u00b7rig", "und", "pflicht\u00b7bar", "wir", "sind", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "PPER", "VAFIN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.9": {"text": "Bessert von neuem die schutzbaren Zinnen,", "tokens": ["Bes\u00b7sert", "von", "neu\u00b7em", "die", "schutz\u00b7ba\u00b7ren", "Zin\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "ADJA", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-++--+-", "measure": "iambic.penta.relaxed"}, "line.10": {"text": "Drunter wir Segen und Ruhe gewinnen.", "tokens": ["Drun\u00b7ter", "wir", "Se\u00b7gen", "und", "Ru\u00b7he", "ge\u00b7win\u00b7nen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "NN", "KON", "NN", "VVINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.11": {"text": "Es stehe die Mauer, drauff vieles sich st\u00fctzt,", "tokens": ["Es", "ste\u00b7he", "die", "Mau\u00b7er", ",", "drauff", "vie\u00b7les", "sich", "st\u00fctzt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PAV", "PIS", "PRF", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.12": {"text": "Das l\u00e4nger so tauer, was vielen so n\u00fctzt!", "tokens": ["Das", "l\u00e4n\u00b7ger", "so", "tau\u00b7er", ",", "was", "vie\u00b7len", "so", "n\u00fctzt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "ADV", "ADJD", "$,", "PRELS", "PIS", "ADV", "ADJD", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.13": {"text": "Zucker der Zeiten, die liebliche Frucht,", "tokens": ["Zu\u00b7cker", "der", "Zei\u00b7ten", ",", "die", "lieb\u00b7li\u00b7che", "Frucht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.14": {"text": "Die wir mit sehnen und seuffzen gesucht,", "tokens": ["Die", "wir", "mit", "seh\u00b7nen", "und", "seuff\u00b7zen", "ge\u00b7sucht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "VVINF", "KON", "NN", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.15": {"text": "S\u00fcsset die Galle der Sch\u00e4den und Plagen,", "tokens": ["S\u00fcs\u00b7set", "die", "Gal\u00b7le", "der", "Sch\u00e4\u00b7den", "und", "Pla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "ART", "NN", "KON", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.16": {"text": "Die wir au\u00df Frevel de\u00df Krieges ertragen.", "tokens": ["Die", "wir", "au\u00df", "Fre\u00b7vel", "de\u00df", "Krie\u00b7ges", "er\u00b7tra\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "APPR", "NN", "ART", "NN", "VVINF", "$."], "meter": "++-+--+--+-", "measure": "trochaic.penta.relaxed"}, "line.17": {"text": "Es bleibe die Freude, die alles erfrischt,", "tokens": ["Es", "blei\u00b7be", "die", "Freu\u00b7de", ",", "die", "al\u00b7les", "er\u00b7frischt", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "PRELS", "PIS", "VVFIN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.18": {"text": "Die mancherley Leide, die Threnen abwischt!", "tokens": ["Die", "man\u00b7cher\u00b7ley", "Lei\u00b7de", ",", "die", "Thre\u00b7nen", "ab\u00b7wischt", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "$,", "ART", "NN", "VVFIN", "$."], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.19": {"text": "Segen de\u00df Himmels, das fr\u00f6mste Geschlecht,", "tokens": ["Se\u00b7gen", "de\u00df", "Him\u00b7mels", ",", "das", "fr\u00f6ms\u00b7te", "Ge\u00b7schlecht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "ART", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "+--+--+--+", "measure": "dactylic.tetra"}, "line.20": {"text": "Dem es an G\u00fcte nie mangelt und Recht,", "tokens": ["Dem", "es", "an", "G\u00fc\u00b7te", "nie", "man\u00b7gelt", "und", "Recht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPER", "APPR", "NN", "ADV", "VVFIN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.21": {"text": "G\u00fcnnet uns, ferner so heilsame Gaben,", "tokens": ["G\u00fcn\u00b7net", "uns", ",", "fer\u00b7ner", "so", "heil\u00b7sa\u00b7me", "Ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "PPER", "$,", "ADV", "ADV", "ADJA", "NN", "$,"], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.22": {"text": "H\u00fclffe, Schutz, Ehre, Vergn\u00fcgen zu haben.", "tokens": ["H\u00fclf\u00b7fe", ",", "Schutz", ",", "Eh\u00b7re", ",", "Ver\u00b7gn\u00fc\u00b7gen", "zu", "ha\u00b7ben", "."], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "$,", "NN", "PTKZU", "VAINF", "$."], "meter": "+--+--+--+-", "measure": "dactylic.tetra"}, "line.23": {"text": "Es gr\u00fcnde sich feste f\u00fcr T\u00fccken und Neid", "tokens": ["Es", "gr\u00fcn\u00b7de", "sich", "fes\u00b7te", "f\u00fcr", "T\u00fc\u00b7cken", "und", "Neid"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PRF", "ADJA", "APPR", "NN", "KON", "NN"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.24": {"text": "Die Hoffnung, das beste der k\u00fcnfftigen Zeit!", "tokens": ["Die", "Hoff\u00b7nung", ",", "das", "bes\u00b7te", "der", "k\u00fcnff\u00b7ti\u00b7gen", "Zeit", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "ART", "ADJA", "ART", "ADJA", "NN", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.25": {"text": "Gib wachsen, gib bleiben, gib stehen, gib Grund,", "tokens": ["Gib", "wach\u00b7sen", ",", "gib", "blei\u00b7ben", ",", "gib", "ste\u00b7hen", ",", "gib", "Grund", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVIMP", "VVINF", "$,", "VVIMP", "VVINF", "$,", "VVIMP", "VVFIN", "$,", "VVIMP", "NN", "$,"], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.26": {"text": "Herr, wie wir es w\u00fcntschen von Hertzen in Mund!", "tokens": ["Herr", ",", "wie", "wir", "es", "w\u00fcnt\u00b7schen", "von", "Hert\u00b7zen", "in", "Mund", "!"], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PWAV", "PPER", "PPER", "VVFIN", "APPR", "NN", "APPR", "NN", "$."], "meter": "+-+-+--+--+", "measure": "trochaic.penta.relaxed"}, "line.27": {"text": "Mehr Zweige, mehr St\u00fctzen, mehr Zucker, mehr Segen,", "tokens": ["Mehr", "Zwei\u00b7ge", ",", "mehr", "St\u00fct\u00b7zen", ",", "mehr", "Zu\u00b7cker", ",", "mehr", "Se\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,", "PIAT", "NN", "$,"], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.28": {"text": "Dran Alten und Jungen ein grosses gelegen!", "tokens": ["Dran", "Al\u00b7ten", "und", "Jun\u00b7gen", "ein", "gros\u00b7ses", "ge\u00b7le\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "NN", "KON", "NN", "ART", "ADJA", "VVPP", "$."], "meter": "-+--+--+--+-", "measure": "amphibrach.tetra"}, "line.29": {"text": "Es lebe der Erbe, den Gott uns geschenckt!", "tokens": ["Es", "le\u00b7be", "der", "Er\u00b7be", ",", "den", "Gott", "uns", "ge\u00b7schenckt", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.30": {"text": "Der B\u00f6se, der sterbe, der b\u00f6ses gedenckt!", "tokens": ["Der", "B\u00f6\u00b7se", ",", "der", "ster\u00b7be", ",", "der", "b\u00f6\u00b7ses", "ge\u00b7denckt", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "VVFIN", "$,", "ART", "ADJA", "VVPP", "$."