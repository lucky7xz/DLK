{"dta.poem.11335": {"metadata": {"author": {"name": "Hofmannswaldau, Christian Hofmann von", "birth": "N.A.", "death": "N.A."}, "title": "Die bey eines baders hochzeit vorgestellt;  \n bad-stube der liebe.  \n  E. G.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1709", "urn": "urn:nbn:de:kobv:b4-20283-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht, wo man sich doch nur verstecken soll,", "tokens": ["Ich", "wei\u00df", "nicht", ",", "wo", "man", "sich", "doch", "nur", "ver\u00b7ste\u00b7cken", "soll", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "PIS", "PRF", "ADV", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wann man der liebe will aus aug und h\u00e4nden gehen;", "tokens": ["Wann", "man", "der", "lie\u00b7be", "will", "aus", "aug", "und", "h\u00e4n\u00b7den", "ge\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "ART", "ADJA", "VMFIN", "APPR", "NE", "KON", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und liefe schon ein Mohr bi\u00df an den norden-pol,", "tokens": ["Und", "lie\u00b7fe", "schon", "ein", "Mohr", "bi\u00df", "an", "den", "nor\u00b7den\u00b7pol", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ART", "NN", "APPR", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "So w\u00fcrde sie ihm doch wohl auf dem nacken stehen.", "tokens": ["So", "w\u00fcr\u00b7de", "sie", "ihm", "doch", "wohl", "auf", "dem", "na\u00b7cken", "ste\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "PPER", "ADV", "ADV", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Denn wenn gleich alle welt die finstre nacht bedeckt,", "tokens": ["Denn", "wenn", "gleich", "al\u00b7le", "welt", "die", "finst\u00b7re", "nacht", "be\u00b7deckt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ADV", "PIAT", "NN", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wird doch der liebe glantz am meisten angesteckt.", "tokens": ["Wird", "doch", "der", "lie\u00b7be", "glantz", "am", "meis\u00b7ten", "an\u00b7ge\u00b7steckt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "ART", "ADJA", "NN", "APPRART", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Es macht kein d\u00fcstrer wald von ihrem lichte frey:", "tokens": ["Es", "macht", "kein", "d\u00fcst\u00b7rer", "wald", "von", "ih\u00b7rem", "lich\u00b7te", "frey", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIAT", "ADJA", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wie offters pflegt sie nicht die h\u00f6len zu durchkriechen?", "tokens": ["Wie", "off\u00b7ters", "pflegt", "sie", "nicht", "die", "h\u00f6\u00b7len", "zu", "durch\u00b7krie\u00b7chen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADV", "VVFIN", "PPER", "PTKNEG", "ART", "VVINF", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Denckt nicht, da\u00df man vor ihr im wasser sicher sey?", "tokens": ["Denckt", "nicht", ",", "da\u00df", "man", "vor", "ihr", "im", "was\u00b7ser", "si\u00b7cher", "sey", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKNEG", "$,", "KOUS", "PIS", "APPR", "PPOSAT", "APPRART", "ADJA", "ADJD", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Vor diesem lie\u00df sie sich einmahl ins wein-fa\u00df pichen;", "tokens": ["Vor", "die\u00b7sem", "lie\u00df", "sie", "sich", "ein\u00b7mahl", "ins", "wein\u00b7fa\u00df", "pi\u00b7chen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PDAT", "VVFIN", "PPER", "PRF", "ADV", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Doch dieses h\u00e4tt ich mir wohl nimmermehr gedacht,", "tokens": ["Doch", "die\u00b7ses", "h\u00e4tt", "ich", "mir", "wohl", "nim\u00b7mer\u00b7mehr", "ge\u00b7dacht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PDS", "VAFIN", "PPER", "PPER", "ADV", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df sie so offt quartier in badereyen macht.", "tokens": ["Da\u00df", "sie", "so", "offt", "quar\u00b7tier", "in", "ba\u00b7de\u00b7re\u00b7yen", "macht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "ADV", "APPR", "VVINF", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Das wasser lescht ja sonst der hitze flammen aus:", "tokens": ["Das", "was\u00b7ser", "lescht", "ja", "sonst", "der", "hit\u00b7ze", "flam\u00b7men", "aus", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VVFIN", "ADV", "ADV", "ART", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wo die krancken sich im schmertz und blute baden,", "tokens": ["Und", "wo", "die", "kran\u00b7cken", "sich", "im", "schmertz", "und", "blu\u00b7te", "ba\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWAV", "ART", "NN", "PRF", "APPRART", "NN", "KON", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da bl\u00fcht wohl ordentlich kein s\u00fcsser wollust-strau\u00df:", "tokens": ["Da", "bl\u00fcht", "wohl", "or\u00b7dent\u00b7lich", "kein", "s\u00fcs\u00b7ser", "wol\u00b7lust\u00b7strau\u00df", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "ADJD", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Es schickt sich rauch und dampff nicht in vergn\u00fcgungs-laden;", "tokens": ["Es", "schickt", "sich", "rauch", "und", "dampff", "nicht", "in", "ver\u00b7gn\u00fc\u00b7gungs\u00b7la\u00b7den", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PRF", "ADJD", "KON", "PAV", "PTKNEG", "APPR", "ADJA", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wo feuer wider wunsch den matten leib erhitzt,", "tokens": ["Wo", "feu\u00b7er", "wi\u00b7der", "wunsch", "den", "mat\u00b7ten", "leib", "er\u00b7hitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "APPR", "ADJD", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da wird der liebe krafft gar leichtlich ausgeschwitzt.", "tokens": ["Da", "wird", "der", "lie\u00b7be", "krafft", "gar", "leicht\u00b7lich", "aus\u00b7ge\u00b7schwitzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Jedoch di\u00df alles rei\u00dft den ersten schlu\u00df nicht ein:", "tokens": ["Je\u00b7doch", "di\u00df", "al\u00b7les", "rei\u00dft", "den", "ers\u00b7ten", "schlu\u00df", "nicht", "ein", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PDS", "PIS", "VVFIN", "ART", "ADJA", "NN", "PTKNEG", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Man kan sie allerdings in krancken-stuben finden,", "tokens": ["Man", "kan", "sie", "al\u00b7ler\u00b7dings", "in", "kran\u00b7cken\u00b7stu\u00b7ben", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VMFIN", "PPER", "ADV", "APPR", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nachdem verliebte ja stets patienten seyn,", "tokens": ["Nach\u00b7dem", "ver\u00b7lieb\u00b7te", "ja", "stets", "pa\u00b7ti\u00b7en\u00b7ten", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VVFIN", "ADV", "ADV", "VVFIN", "VAINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Die sie als \u00e4rtztin mu\u00df mit heil und trost verbinden.", "tokens": ["Die", "sie", "als", "\u00e4rtz\u00b7tin", "mu\u00df", "mit", "heil", "und", "trost", "ver\u00b7bin\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "KOUS", "NE", "VMFIN", "APPR", "ADJD", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Schl\u00e4gt dieser hauffe nun wo einen wohn-platz auf;", "tokens": ["Schl\u00e4gt", "die\u00b7ser", "hauf\u00b7fe", "nun", "wo", "ei\u00b7nen", "wohn\u00b7platz", "auf", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PDS", "VVFIN", "ADV", "PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So bringt sie warlich auch da ihren kram zu kauf.", "tokens": ["So", "bringt", "sie", "war\u00b7lich", "auch", "da", "ih\u00b7ren", "kram", "zu", "kauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "ADV", "ADV", "PPOSAT", "NN", "PTKZU", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Jhr krancken! die ihr euch den magen wo verderbt,", "tokens": ["Ihr", "kran\u00b7cken", "!", "die", "ihr", "euch", "den", "ma\u00b7gen", "wo", "ver\u00b7derbt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVINF", "$.", "PRELS", "PPER", "PRF", "ART", "ADJA", "PWAV", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Da\u00df hitz und frost vermengt viel tremulanten schlagen,", "tokens": ["Da\u00df", "hitz", "und", "frost", "ver\u00b7mengt", "viel", "tre\u00b7mu\u00b7lan\u00b7ten", "schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "KON", "ADJD", "VVFIN", "PIAT", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sich das angesicht bald roth bald anders f\u00e4rbt,", "tokens": ["Und", "sich", "das", "an\u00b7ge\u00b7sicht", "bald", "roth", "bald", "an\u00b7ders", "f\u00e4rbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PRF", "ART", "NN", "ADV", "ADJD", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Jhr d\u00fcrfft mich nur hieher zu unsrer \u00e4rtztin tragen;", "tokens": ["Ihr", "d\u00fcrfft", "mich", "nur", "hie\u00b7her", "zu", "uns\u00b7rer", "\u00e4rtz\u00b7tin", "tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "PAV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Die tr\u00e4gt euch einen schatz von fieber-pillen an,", "tokens": ["Die", "tr\u00e4gt", "euch", "ei\u00b7nen", "schatz", "von", "fie\u00b7ber\u00b7pil\u00b7len", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ART", "NN", "APPR", "ADJA", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die kein Hippocrates so gut verschreiben kan.", "tokens": ["Die", "kein", "Hip\u00b7po\u00b7cra\u00b7tes", "so", "gut", "ver\u00b7schrei\u00b7ben", "kan", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "ADV", "ADJD", "VVINF", "VMFIN", "$."], "meter": "+-+-+--+-+-+", "measure": "trochaic.hexa.relaxed"}}, "stanza.6": {"line.1": {"text": "Weil aber alles sich nicht gleich vor alle schickt,", "tokens": ["Weil", "a\u00b7ber", "al\u00b7les", "sich", "nicht", "gleich", "vor", "al\u00b7le", "schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "PIS", "PRF", "PTKNEG", "ADV", "APPR", "PIS", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und die purgantzen offt die kranckheit mehr erregen,", "tokens": ["Und", "die", "pur\u00b7gant\u00b7zen", "offt", "die", "kran\u00b7ck\u00b7heit", "mehr", "er\u00b7re\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "ADV", "ART", "NN", "ADV", "VVINF", "$,"], "meter": "-+++-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Zumahl wenn ohnmacht euch der seiten krafft entz\u00fcckt;", "tokens": ["Zu\u00b7mahl", "wenn", "ohn\u00b7macht", "euch", "der", "sei\u00b7ten", "krafft", "ent\u00b7z\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "KOUS", "ADJD", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+--+--+-+-+", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "So mu\u00df der patient sich auch aufs schwitzen legen.", "tokens": ["So", "mu\u00df", "der", "pa\u00b7ti\u00b7ent", "sich", "auch", "aufs", "schwit\u00b7zen", "le\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "ART", "NN", "PRF", "ADV", "APPRART", "ADJA", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Dann giebt sie perlen-tr\u00e4nck und st\u00e4rckungs-tropffen ein,", "tokens": ["Dann", "giebt", "sie", "per\u00b7len\u00b7tr\u00e4nck", "und", "st\u00e4r\u00b7ckungs\u00b7tropf\u00b7fen", "ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Die k\u00fcnstlicher als zimmt, als mosch und ambra seyn.", "tokens": ["Die", "k\u00fcnst\u00b7li\u00b7cher", "als", "zimmt", ",", "als", "mosch", "und", "am\u00b7bra", "seyn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "KOKOM", "VVFIN", "$,", "KOUS", "ADJD", "KON", "NE", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.7": {"line.1": {"text": "Wen ein erz\u00f6rnter strahl der sonnen blind gemacht,", "tokens": ["Wen", "ein", "er\u00b7z\u00f6rn\u00b7ter", "strahl", "der", "son\u00b7nen", "blind", "ge\u00b7macht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ART", "ADJA", "NN", "ART", "ADJA", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und wen ein fenster-blitz der augen schein genommen,", "tokens": ["Und", "wen", "ein", "fens\u00b7ter\u00b7blitz", "der", "au\u00b7gen", "schein", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "ART", "NN", "ART", "NN", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Als ihn ein mocken-geist zu nah ans licht gebracht,", "tokens": ["Als", "ihn", "ein", "mo\u00b7cken\u00b7geist", "zu", "nah", "ans", "licht", "ge\u00b7bracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "PTKA", "ADJD", "APPRART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Der wird durch ihre cur verneuten geist bekommen.", "tokens": ["Der", "wird", "durch", "ih\u00b7re", "cur", "ver\u00b7neu\u00b7ten", "geist", "be\u00b7kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "APPR", "PPOSAT", "NN", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Jhr blinde! tragt den star zu dieser \u00e4rtztin hin!", "tokens": ["Ihr", "blin\u00b7de", "!", "tragt", "den", "star", "zu", "die\u00b7ser", "\u00e4rtz\u00b7tin", "hin", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$.", "VVFIN", "ART", "NN", "APPR", "PDAT", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sie wird das fell gar bald von euren augen ziehn.", "tokens": ["Sie", "wird", "das", "fell", "gar", "bald", "von", "eu\u00b7ren", "au\u00b7gen", "ziehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "ADV", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.8": {"line.1": {"text": "In br\u00fcchen ist sie schon von alters her bekannt:", "tokens": ["In", "br\u00fc\u00b7chen", "ist", "sie", "schon", "von", "al\u00b7ters", "her", "be\u00b7kannt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "VAFIN", "PPER", "ADV", "APPR", "ADV", "ADV", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie schneidet wurm und stein: sie heilt die tieffsten wunden", "tokens": ["Sie", "schnei\u00b7det", "wurm", "und", "stein", ":", "sie", "heilt", "die", "tieffs\u00b7ten", "wun\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "KON", "ADJD", "$.", "PPER", "VVFIN", "ART", "ADJA", "ADJA"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Mit bisam-pflaster zu: ja selbst der kalte brand", "tokens": ["Mit", "bi\u00b7sam\u00b7pflas\u00b7ter", "zu", ":", "ja", "selbst", "der", "kal\u00b7te", "brand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "PTKVZ", "$.", "ADV", "ADV", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Hat \u00f6ffters ihre krafft und widerstand empfunden.", "tokens": ["Hat", "\u00f6ff\u00b7ters", "ih\u00b7re", "krafft", "und", "wi\u00b7der\u00b7stand", "emp\u00b7fun\u00b7den", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ADV", "PPOSAT", "NN", "KON", "VVFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Und draut ja etwas noch den sterblichen das grab,", "tokens": ["Und", "draut", "ja", "et\u00b7was", "noch", "den", "sterb\u00b7li\u00b7chen", "das", "grab", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADV", "ADV", "ART", "ADJA", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Dem hilfft ihr schrepffe-kopf und aderlassen ab.", "tokens": ["Dem", "hilfft", "ihr", "schre\u00b7pf\u00b7fe\u00b7kopf", "und", "a\u00b7der\u00b7las\u00b7sen", "ab", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJD", "KON", "VVFIN", "PTKVZ", "$."], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}}, "stanza.9": {"line.1": {"text": "Ja was noch dr\u00fcber ist: Sie macht gesichter neu:", "tokens": ["Ja", "was", "noch", "dr\u00fc\u00b7ber", "ist", ":", "Sie", "macht", "ge\u00b7sich\u00b7ter", "neu", ":"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "PWS", "ADV", "PAV", "VAFIN", "$.", "PPER", "VVFIN", "ADJD", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Sie