rd", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "ADV", "ADV", "ADV", "APPRART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.10": {"line.1": {"text": "Jhr guten herren wi\u00dft offt nicht, wer vor euch sitzt,", "tokens": ["Ihr", "gu\u00b7ten", "her\u00b7ren", "wi\u00dft", "offt", "nicht", ",", "wer", "vor", "euch", "sitzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VVFIN", "ADV", "PTKNEG", "$,", "PWS", "APPR", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wenn jungfern purpur-safft aus ihren adern z\u00e4pffen,", "tokens": ["Wenn", "jung\u00b7fern", "pur\u00b7pur\u00b7\u00b7safft", "aus", "ih\u00b7ren", "a\u00b7dern", "z\u00e4pf\u00b7fen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und ihr der glieder-schnee voll blum- und nahmen ritzt,", "tokens": ["Und", "ihr", "der", "glie\u00b7der\u00b7schnee", "voll", "blum", "und", "nah\u00b7men", "ritzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "ART", "NN", "ADJD", "TRUNC", "KON", "VVFIN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Da\u00df alle tropffen nichts als treu und liebe schrepffen.", "tokens": ["Da\u00df", "al\u00b7le", "tropf\u00b7fen", "nichts", "als", "treu", "und", "lie\u00b7be", "schre\u00b7pf\u00b7fen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "VVFIN", "PIS", "KOKOM", "ADJD", "KON", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.5": {"text": "Da wird die flitte dann so wunderlich ger\u00fchrt,", "tokens": ["Da", "wird", "die", "flit\u00b7te", "dann", "so", "wun\u00b7der\u00b7lich", "ge\u00b7r\u00fchrt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "VVFIN", "ADV", "ADV", "ADJD", "VVPP", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Da\u00df man den bad er selbst ins krancken-bette f\u00fchrt.", "tokens": ["Da\u00df", "man", "den", "bad", "er", "selbst", "ins", "kran\u00b7cken\u00b7bet\u00b7te", "f\u00fchrt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIS", "ART", "NN", "PPER", "ADV", "APPRART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.11": {"line.1": {"text": "Komm, angenehmes paar! du solt mein zeuge seyn!", "tokens": ["Komm", ",", "an\u00b7ge\u00b7neh\u00b7mes", "paar", "!", "du", "solt", "mein", "zeu\u00b7ge", "seyn", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "$,", "ADJA", "NN", "$.", "PPER", "VMFIN", "PPOSAT", "VVFIN", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dein beyspiel wird uns di\u00df gantz klar beweisen k\u00f6nnen,", "tokens": ["Dein", "bey\u00b7spiel", "wird", "uns", "di\u00df", "gantz", "klar", "be\u00b7wei\u00b7sen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PPER", "PDS", "ADV", "ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die liebe zieht davor in deinem zimmer ein,", "tokens": ["Die", "lie\u00b7be", "zieht", "da\u0