{"textgrid.poem.56214": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "14", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Mein g\u00fct'ger Herr, du willst herab dich lassen", "tokens": ["Mein", "g\u00fct'\u00b7ger", "Herr", ",", "du", "willst", "her\u00b7ab", "dich", "las\u00b7sen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "$,", "PPER", "VMFIN", "ADV", "PPER", "VVINF"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Beseligend zu deiner armen Magd!", "tokens": ["Be\u00b7se\u00b7li\u00b7gend", "zu", "dei\u00b7ner", "ar\u00b7men", "Magd", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Mir hat die Sonne deiner Huld getagt!", "tokens": ["Mir", "hat", "die", "Son\u00b7ne", "dei\u00b7ner", "Huld", "ge\u00b7tagt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich kann es nicht ermessen, nicht erfassen.", "tokens": ["Ich", "kann", "es", "nicht", "er\u00b7mes\u00b7sen", ",", "nicht", "er\u00b7fas\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Du sollst nicht wirre Tr\u00e4ume neu beleben,", "tokens": ["Du", "sollst", "nicht", "wir\u00b7re", "Tr\u00e4u\u00b7me", "neu", "be\u00b7le\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADJA", "NN", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Mein innres Herz nicht rufen an das Licht,", "tokens": ["Mein", "inn\u00b7res", "Herz", "nicht", "ru\u00b7fen", "an", "das", "Licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "PTKNEG", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "La\u00df ab, du t\u00e4uschest dich, du kennst mich nicht,", "tokens": ["La\u00df", "ab", ",", "du", "t\u00e4u\u00b7schest", "dich", ",", "du", "kennst", "mich", "nicht", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "VVFIN", "PPER", "$,", "PPER", "VVFIN", "PPER", "PTKNEG", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Ich habe nichts als Liebe dir zu geben.", "tokens": ["Ich", "ha\u00b7be", "nichts", "als", "Lie\u00b7be", "dir", "zu", "ge\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIS", "KOKOM", "NN", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "La\u00df ab, du Vielgeliebter, von der Armen,", "tokens": ["La\u00df", "ab", ",", "du", "Viel\u00b7ge\u00b7lieb\u00b7ter", ",", "von", "der", "Ar\u00b7men", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PTKVZ", "$,", "PPER", "NN", "$,", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Die schon der Liebe Schmerz um dich begl\u00fcckt;", "tokens": ["Die", "schon", "der", "Lie\u00b7be", "Schmerz", "um", "dich", "be\u00b7gl\u00fcckt", ";"], "token