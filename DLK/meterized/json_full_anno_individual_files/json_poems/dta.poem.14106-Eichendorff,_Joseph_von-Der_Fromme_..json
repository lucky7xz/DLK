{"dta.poem.14106": {"metadata": {"author": {"name": "Eichendorff, Joseph von", "birth": "N.A.", "death": "N.A."}, "title": "Der Fromme .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905191113", "language": ["de:0.99"], "booktitle": "Eichendorff, Joseph von: Gedichte. Berlin, 1837."}, "poem": {"stanza.1": {"line.1": {"text": "Blick' auf! Schon schweifen Paradieses-V\u00f6gel,", "tokens": ["Blick'", "auf", "!", "Schon", "schwei\u00b7fen", "Pa\u00b7ra\u00b7die\u00b7ses\u00b7V\u00f6\u00b7gel", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$.", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Schon wehen Wunderkl\u00e4nge aus der Ferne,", "tokens": ["Schon", "we\u00b7hen", "Wun\u00b7der\u00b7kl\u00e4n\u00b7ge", "aus", "der", "Fer\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJA", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Der Garten Gottes steigt aus Morgenflammen.", "tokens": ["Der", "Gar\u00b7ten", "Got\u00b7tes", "steigt", "aus", "Mor\u00b7gen\u00b7flam\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}}}}