{"dta.poem.499": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Auf die Br\u00fcste seiner geliebten  Sideminda.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Du Wunder-Glantz der Alabaster Br\u00fcste/", "tokens": ["Du", "Wun\u00b7der\u00b7Glantz", "der", "A\u00b7la\u00b7bas\u00b7ter", "Br\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Du kleines Rund/ doch Wohn-Platz grosser L\u00fcste/", "tokens": ["Du", "klei\u00b7nes", "Rund", "/", "doch", "Wohn\u00b7Platz", "gros\u00b7ser", "L\u00fcs\u00b7te", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "ADV", "NN", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Du Schnee-Geb\u00fcrg wo ", "tokens": ["Du", "Schnee\u00b7Ge\u00b7b\u00fcrg", "wo"], "token_info": ["word", "word", "word"], "pos": ["PPER", "NN", "PWAV"], "meter": "-+-+-", "measure": "iambic.di"}, "line.4": {"text": "Du ", "tokens": ["Du"], "token_info": ["word"], "pos": ["PPER"], "meter": "-", "measure": "single.down"}, "line.5": {"text": "Wenn wird mein Geist die frohe Stunde sehen?", "tokens": ["Wenn", "wird", "mein", "Geist", "die", "fro\u00b7he", "Stun\u00b7de", "se\u00b7hen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPOSAT", "NN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Da\u00df er bey euch vor Wollust wird vergehen.", "tokens": ["Da\u00df", "er", "bey", "euch", "vor", "Wol\u00b7lust", "wird", "ver\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "APPR", "PPER", "APPR", "NN", "VAFIN", "VVINF", "$."], "meter": "+-+--+-+-+-", "measure": "trochaic.penta.relaxed"}, "line.7": {"text": "Wenn wird das Safft/ das eure ", "tokens": ["Wenn", "wird", "das", "Safft", "/", "das", "eu\u00b7re"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["KOUS", "VAFIN", "ART", "NN", "$(", "ART", "PPOSAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.8": {"text": "Mit vollem Strohm auf meinen Geist geschickt?", "tokens": ["Mit", "vol\u00b7lem", "Strohm", "auf", "mei\u00b7nen", "Geist", "ge\u00b7schickt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "APPR", "PPOSAT", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.9": {"text": "Wenn soll die Hand bey denen Liebes-Ballen", "tokens": ["Wenn", "soll", "die", "Hand", "bey", "de\u00b7nen", "Lie\u00b7bes\u00b7Bal\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "VMFIN", "ART", "NN", "APPR", "PRELS", "NN"], "meter": "-+-+---+-+-", "measure": "unknown.measure.tetra"}, "line.10": {"text": "Nach ihrem Wunsch in heissen Eyse wallen?", "tokens": ["Nach", "ih\u00b7rem", "Wunsch", "in", "heis\u00b7sen", "Ey\u00b7se", "wal\u00b7len", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "APPR", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Wenn wird der Schnee der Br\u00fcste mir gereicht?", "tokens": ["Wenn", "wird", "der", "Schnee", "der", "Br\u00fcs\u00b7te", "mir", "ge\u00b7reicht", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "ART", "NN", "ART", "NN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.12": {"text": "Aus dem die Gluth/ als einen ", "tokens": ["Aus", "dem", "die", "Gluth", "/", "als", "ei\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["APPR", "ART", "ART", "NN", "$(", "KOUS", "ART"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.13": {"text": "Allein umsonst/ ich mu\u00df vergebens hoffen/", "tokens": ["Al\u00b7lein", "um\u00b7sonst", "/", "ich", "mu\u00df", "ver\u00b7ge\u00b7bens", "hof\u00b7fen", "/"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$(", "PPER", "VMFIN", "ADV", "VVINF", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.14": {"text": "Des Busens Pracht steht zwar den Augen offen/", "tokens": ["Des", "Bu\u00b7sens", "Pracht", "steht", "zwar", "den", "Au\u00b7gen", "of\u00b7fen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "ADV", "ART", "NN", "PTKVZ", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.15": {"text": "Doch Mund und Hand greifft da vergebens zu/", "tokens": ["Doch", "Mund", "und", "Hand", "greifft", "da", "ver\u00b7ge\u00b7bens", "zu", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "VVFIN", "ADV", "ADV", "PTKZU", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.16": {"text": "Jhr Eigensinn verhindert meine Ruh.", "tokens": ["Ihr", "Ei\u00b7gen\u00b7sinn", "ver\u00b7hin\u00b7dert", "mei\u00b7ne", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}}}}