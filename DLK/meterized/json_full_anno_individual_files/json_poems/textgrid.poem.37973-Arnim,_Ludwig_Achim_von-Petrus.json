{"textgrid.poem.37973": {"metadata": {"author": {"name": "Arnim, Ludwig Achim von", "birth": "N.A.", "death": "N.A."}, "title": "Petrus", "genre": "verse", "period": "N.A.", "pub_year": 1806, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Herr der stellt ein Gastmahl an,", "tokens": ["Der", "Herr", "der", "stellt", "ein", "Gast\u00b7mahl", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit seinen J\u00fcngern alln,", "tokens": ["Mit", "sei\u00b7nen", "J\u00fcn\u00b7gern", "alln", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Sie gingen in ein Garten,", "tokens": ["Sie", "gin\u00b7gen", "in", "ein", "Gar\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Wo lustig jedermann.", "tokens": ["Wo", "lus\u00b7tig", "je\u00b7der\u00b7mann", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "PIS", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Als die Juden den Herrn gefangen nahmen,", "tokens": ["Als", "die", "Ju\u00b7den", "den", "Herrn", "ge\u00b7fan\u00b7gen", "nah\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "NN", "ART", "NN", "ADJD", "VVFIN", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.2": {"text": "Da laufen die J\u00fcnger davon,", "tokens": ["Da", "lau\u00b7fen", "die", "J\u00fcn\u00b7ger", "da\u00b7von", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "PAV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Den Petrus hat einer am Mantel ertapt:", "tokens": ["Den", "Pet\u00b7rus", "hat", "ei\u00b7ner", "am", "Man\u00b7tel", "er\u00b7tapt", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "VAFIN", "ART", "APPRART", "NN", "VVPP", "$."], "meter": "-+--+--+--+", "measure": "amphibrach.tetra"}, "line.4": {"text": "\u00bbglatzkopf, jezt hab ich dich schon.\u00ab", "tokens": ["\u00bb", "glatz\u00b7kopf", ",", "jezt", "hab", "ich", "dich", "schon", ".", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "VVFIN", "$,", "ADV", "VAFIN", "PPER", "PRF", "ADV", "$.", "$("], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Der Petrus zieht sein S\u00e4bel,", "tokens": ["Der", "Pet\u00b7rus", "zieht", "sein", "S\u00e4\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er wollte sie hauen allhie,", "tokens": ["Er", "woll\u00b7te", "sie", "hau\u00b7en", "all\u00b7hie", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "VVFIN", "ADV", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Er haut ganz miserabel,", "tokens": ["Er", "haut", "ganz", "mi\u00b7se\u00b7ra\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die mehrst Hieb gehn darneben.", "tokens": ["Die", "mehrst", "Hieb", "gehn", "dar\u00b7ne\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "NN", "VVFIN", "PAV", "$."], "meter": "--+-+--", "measure": "anapaest.init"}}, "stanza.4": {"line.1": {"text": "Der Herr gab ihm ein Deuter:", "tokens": ["Der", "Herr", "gab", "ihm", "ein", "Deu\u00b7ter", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+---+-", "measure": "dactylic.init"}, "line.2": {"text": "\u00bbach Petrus steck ein dein Schwerdt,", "tokens": ["\u00bb", "ach", "Pet\u00b7rus", "steck", "ein", "dein", "Schwerdt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "XY", "FM", "FM", "ART", "PPOSAT", "NN", "$,"], "meter": "+--+--+", "measure": "dactylic.tri"}, "line.3": {"text": "Du bist ein Erzb\u00e4rnh\u00e4uter,", "tokens": ["Du", "bist", "ein", "Erz\u00b7b\u00e4rn\u00b7h\u00e4u\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dein Schneid ist kein Teufel werth.\u00ab", "tokens": ["Dein", "Schneid", "ist", "kein", "Teu\u00b7fel", "werth", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "NN", "VAFIN", "PIAT", "NN", "ADJD", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.5": {"line.1": {"text": "Das wollte den Petrus verdrie\u00dfen,", "tokens": ["Das", "woll\u00b7te", "den", "Pet\u00b7rus", "ver\u00b7drie\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "ART", "FM", "VVINF", "$,"], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Da\u00df er erst der Niemand sollt seyn,", "tokens": ["Da\u00df", "er", "erst", "der", "Nie\u00b7mand", "sollt", "seyn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ART", "PIS", "VMFIN", "VAINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Er zog heraus sein Sabel,", "tokens": ["Er", "zog", "he\u00b7raus", "sein", "Sa\u00b7bel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADV", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und hieb ganz sakerisch drein.", "tokens": ["Und", "hieb", "ganz", "sa\u00b7ke\u00b7risch", "drein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "ADJD", "PTKVZ", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.6": {"line.1": {"text": "Der Malchus stund darneben,", "tokens": ["Der", "Mal\u00b7chus", "stund", "dar\u00b7ne\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "PAV", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Und hat sich nicht umgeschaut,", "tokens": ["Und", "hat", "sich", "nicht", "um\u00b7ge\u00b7schaut", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PRF", "PTKNEG", "VVPP", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Dem hat er \u00e4 T\u00e4scherl aufs Dach auf geben,", "tokens": ["Dem", "hat", "er", "\u00e4", "T\u00e4\u00b7scherl", "aufs", "Dach", "auf", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADJA", "NN", "APPRART", "NN", "APPR", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und Ohr-Watschl putz weggehaut.", "tokens": ["Und", "Ohr\u00b7Watschl", "putz", "weg\u00b7ge\u00b7haut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NE", "NE", "VVPP", "$."], "meter": "-++-+-+", "measure": "unknown.measure.tetra"}}, "stanza.7": {"line.1": {"text": "Der Malchus f\u00e4ngt protz und zu weinen an,", "tokens": ["Der", "Mal\u00b7chus", "f\u00e4ngt", "protz", "und", "zu", "wei\u00b7nen", "an", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "APPR", "KON", "PTKZU", "VVINF", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Und schrie da \u00fcberlaut:", "tokens": ["Und", "schrie", "da", "\u00fc\u00b7berl\u00b7aut", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "VVPP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbherr, heil mir doch mein Ohr wieder an,", "tokens": ["\u00bb", "herr", ",", "heil", "mir", "doch", "mein", "Ohr", "wie\u00b7der", "an", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PTKVZ", "$,", "KOUS", "PPER", "ADV", "PPOSAT", "NN", "ADV", "PTKVZ", "$,"], "meter": "+-+---+-+", "measure": "unknown.measure.tetra"}, "line.4": {"text": "Der Glatzkopf hat mirs weggehaut.\u00ab", "tokens": ["Der", "Glatz\u00b7kopf", "hat", "mirs", "weg\u00b7ge\u00b7haut", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "VAFIN", "NE", "VVPP", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Der Herr der nahm des Malchus Ohr", "tokens": ["Der", "Herr", "der", "nahm", "des", "Mal\u00b7chus", "Ohr"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "VVFIN", "ART", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und wollts gleich wieder kuriren,", "tokens": ["Und", "wollts", "gleich", "wie\u00b7der", "ku\u00b7ri\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADV", "ADV", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Auf einmal sprang der Petrus hervor,", "tokens": ["Auf", "ein\u00b7mal", "sprang", "der", "Pet\u00b7rus", "her\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "VVFIN", "ART", "FM", "PTKVZ", "$,"], "meter": "-+-+----+", "measure": "unknown.measure.tri"}, "line.4": {"text": "F\u00e4ngt an zu raisoniren:", "tokens": ["F\u00e4ngt", "an", "zu", "rai\u00b7so\u00b7ni\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PTKVZ", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "\u00bbwas hat mich denn mein Haun genuzt,", "tokens": ["\u00bb", "was", "hat", "mich", "denn", "mein", "Haun", "ge\u00b7nuzt", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "ADV", "PPOSAT", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Da w\u00e4r ich ja ein Hans,", "tokens": ["Da", "w\u00e4r", "ich", "ja", "ein", "Hans", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ART", "NE", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Was ich so sakrisch hab zammen gepuzt,", "tokens": ["Was", "ich", "so", "sak\u00b7risch", "hab", "zam\u00b7men", "ge\u00b7puzt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PPER", "ADV", "ADJD", "VAFIN", "ADJD", "VVPP", "$,"], "meter": "-+-+--+--+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Das machst du gleich wieder ganz.\u00ab", "tokens": ["Das", "machst", "du", "gleich", "wie\u00b7der", "ganz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "ADV", "ADV", "$.", "$("], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.10": {"line.1": {"text": "Er ging bey des Kaisers Kohlenfeuer,", "tokens": ["Er", "ging", "bey", "des", "Kai\u00b7sers", "Koh\u00b7len\u00b7feu\u00b7er", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "NN", "$,"], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Da sassen die Juden dick,", "tokens": ["Da", "sas\u00b7sen", "die", "Ju\u00b7den", "dick", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADJD", "$,"], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Da f\u00fchrt der Teufel die Dienstmagd her,", "tokens": ["Da", "f\u00fchrt", "der", "Teu\u00b7fel", "die", "Dienst\u00b7magd", "her", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN", "PTKVZ", "$,"], "meter": "-+-+--+-+", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Der Petrus kennet sie nicht.", "tokens": ["Der", "Pet\u00b7rus", "ken\u00b7net", "sie", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "FM", "VVFIN", "PPER", "PTKNEG", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.11": {"line.1": {"text": "\u00bbaha, du bist auch einer,", "tokens": ["\u00bb", "a\u00b7ha", ",", "du", "bist", "auch", "ei\u00b7ner", ","], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ITJ", "$,", "PPER", "VAFIN", "ADV", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der mit im Garten war!\u00ab", "tokens": ["Der", "mit", "im", "Gar\u00b7ten", "war", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "APPR", "APPRART", "NN", "VAFIN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Petrus l\u00fcgt wie Stahl und Band,", "tokens": ["Der", "Pet\u