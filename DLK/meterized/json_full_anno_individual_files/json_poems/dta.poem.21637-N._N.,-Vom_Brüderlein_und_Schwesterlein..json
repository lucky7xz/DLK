{"dta.poem.21637": {"metadata": {"author": {"name": "N. N., ", "birth": "N.A.", "death": "N.A."}, "title": "Vom Br\u00fcderlein und Schwesterlein.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1885", "urn": "urn:nbn:de:kobv:b4-200905196929", "language": ["de:0.99"], "booktitle": "Arent, Wilhelm (Hrsg.): Moderne Dichter-Charaktere. Leipzig, [1885]."}, "poem": {"stanza.1": {"line.1": {"text": "Wer heim das blaue Bl\u00fcmlein bringt,", "tokens": ["Wer", "heim", "das", "blau\u00b7e", "Bl\u00fcm\u00b7lein", "bringt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "PTKVZ", "ART", "ADJA", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der ist der K\u00f6nig im Lande!", "tokens": ["Der", "ist", "der", "K\u00f6\u00b7nig", "im", "Lan\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "APPRART", "NN", "$."], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.3": {"text": "Die Krone ihm vom Haupte blinkt,", "tokens": ["Die", "Kro\u00b7ne", "ihm", "vom", "Haup\u00b7te", "blinkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Er wandelt im Purpurgewande!\u201c", "tokens": ["Er", "wan\u00b7delt", "im", "Pur\u00b7pur\u00b7ge\u00b7wan\u00b7de", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "$.", "$("], "meter": "-+--+--+-", "measure": "amphibrach.tri"}}}}}