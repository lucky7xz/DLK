{"textgrid.poem.51194": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Frau R\u00f6sel", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Frau R\u00f6sel ist eine gute Frau, wie liebt sie ihren K\u00f6nig,", "tokens": ["Frau", "R\u00f6\u00b7sel", "ist", "ei\u00b7ne", "gu\u00b7te", "Frau", ",", "wie", "liebt", "sie", "ih\u00b7ren", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VAFIN", "ART", "ADJA", "NN", "$,", "PWAV", "VVFIN", "PPER", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-+-+-+-+-", "measure": "trochaic.octa.plus"}, "line.2": {"text": "Den K\u00f6nig und sein ganzes Haus, und i\u00dft und trinkt so wenig!", "tokens": ["Den", "K\u00f6\u00b7nig", "und", "sein", "gan\u00b7zes", "Haus", ",", "und", "i\u00dft", "und", "trinkt", "so", "we\u00b7nig", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "KON", "VVFIN", "ADV", "PIS", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Und als es hie\u00df, der junge Prinz wird seine Braut heimf\u00fchren,", "tokens": ["Und", "als", "es", "hie\u00df", ",", "der", "jun\u00b7ge", "Prinz", "wird", "sei\u00b7ne", "Braut", "heim\u00b7f\u00fch\u00b7ren", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PPER", "VVFIN", "$,", "ART", "ADJA", "NN", "VAFIN", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Da sprach der Vogt: \u00bbAuf, gute Frau! Ihr m\u00fc\u00dft das Haus verzieren!\u00ab", "tokens": ["Da", "sprach", "der", "Vogt", ":", "\u00bb", "Auf", ",", "gu\u00b7te", "Frau", "!", "Ihr", "m\u00fc\u00dft", "das", "Haus", "ver\u00b7zie\u00b7ren", "!", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "$.", "$(", "APPR", "$,", "ADJA", "NN", "$.", "PPER", "VMFIN", "ART", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Nun hat Frau R\u00f6sel dick zu tun, wie trippelt sie und wie lauft sie!", "tokens": ["Nun", "hat", "Frau", "R\u00f6\u00b7sel", "dick", "zu", "tun", ",", "wie", "trip\u00b7pelt", "sie", "und", "wie", "lauft", "sie", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "NN", "NN", "ADJD", "PTKZU", "VVINF", "$,", "PWAV", "VVFIN", "PPER", "KON", "PWAV", "VVFIN", "PPER", "$."], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Ein Dutzend F\u00e4hnchen und Goldpapier und junge Birken kauft sie,", "tokens": ["Ein", "Dut\u00b7zend", "F\u00e4hn\u00b7chen", "und", "Gold\u00b7pa\u00b7pier", "und", "jun\u00b7ge", "Bir\u00b7ken", "kauft", "sie", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "KON", "NN", "KON", "ADJA", "NN", "VVFIN", "PPER", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Sie geht zu Wald und sammelt Moos, beim Nachbar bettelt sie Schn\u00fcre", "tokens": ["Sie", "geht", "zu", "Wald", "und", "sam\u00b7melt", "Moos", ",", "beim", "Nach\u00b7bar", "bet\u00b7telt", "sie", "Schn\u00fc\u00b7re"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "NN", "KON", "VVFIN", "NN", "$,", "APPRART", "NN", "VVFIN", "PPER", "NN"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und alte N\u00e4gel und derlei Zeug, beim Schuster Kleister und Schmiere,", "tokens": ["Und", "al\u00b7te", "N\u00e4\u00b7gel", "und", "der\u00b7lei", "Zeug", ",", "beim", "Schus\u00b7ter", "Kleis\u00b7ter", "und", "Schmie\u00b7re", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "KON", "PIAT", "NN", "$,", "APPRART", "NN", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+--+-+--+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Dann schafft und keucht sie den ganzen Tag und sinnt und klopft und klittert,", "tokens": ["Dann", "schafft", "und", "keucht", "sie", "den", "gan\u00b7zen", "Tag", "und", "sinnt", "und", "klopft", "und", "klit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "KON", "VVFIN", "PPER", "ART", "ADJA", "NN", "KON", "VVFIN", "KON", "VVFIN", "KON", "VVFIN", "$,"], "meter": "-+-+--+-+-+-+-+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Bis da\u00df ihr H\u00e4uslein um und um behangen ist und beflittert,", "tokens": ["Bis", "da\u00df", "ihr", "H\u00e4us\u00b7lein", "um", "und", "um", "be\u00b7han\u00b7gen", "ist", "und", "be\u00b7flit\u00b7tert", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "KOUS", "PPOSAT", "NN", "APPR", "KON", "APPR", "VVPP", "VAFIN", "KON", "VVFIN", "$,"], "meter": "-+-+-+-+-+-++-+-", "measure": "unknown.measure.octa.plus"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Herr Bunzelmann, der alles kann, hilft ihr studieren und kleben,", "tokens": ["Herr", "Bun\u00b7zel\u00b7mann", ",", "der", "al\u00b7les", "kann", ",", "hilft", "ihr", "stu\u00b7die\u00b7ren", "und", "kle\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NE", "$,", "PRELS", "PIS", "VMFIN", "$,", "VVFIN", "PPER", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Macht Wappen und Kron und Namensz\u00fcg, trinkt zw\u00f6lf Ma\u00df Bier daneben", "tokens": ["Macht", "Wap\u00b7pen", "und", "Kron", "und", "Na\u00b7mens\u00b7z\u00fcg", ",", "trinkt", "zw\u00f6lf", "Ma\u00df", "Bier", "da\u00b7ne\u00b7ben"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["NN", "NN", "KON", "NN", "KON", "NN", "$,", "VVFIN", "CARD", "NN", "NN", "PAV"], "meter": "-+--+-+--+-++-+-", "measure": "iambic.septa.relaxed"}, "line.3": {"text": "Der guten, armen Frau R\u00f6sel.", "tokens": ["Der", "gu\u00b7ten", ",", "ar\u00b7men", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Und aus dem letzten Groschen kauft sie Brot und frische Butter", "tokens": ["Und", "aus", "dem", "letz\u00b7ten", "Gro\u00b7schen", "kauft", "sie", "Brot", "und", "fri\u00b7sche", "But\u00b7ter"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "PPER", "NN", "KON", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Und sitzt vergn\u00fcgt vor ihrem Haus und harrt der Landesmutter,", "tokens": ["Und", "sitzt", "ver\u00b7gn\u00fcgt", "vor", "ih\u00b7rem", "Haus", "und", "harrt", "der", "Lan\u00b7des\u00b7mut\u00b7ter", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPR", "PPOSAT", "NN", "KON", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Doch ist sie m\u00fcd, sie sitzt und schl\u00e4ft, h\u00f6rt nicht das Schie\u00dfen und L\u00e4rmen,", "tokens": ["Doch", "ist", "sie", "m\u00fcd", ",", "sie", "sitzt", "und", "schl\u00e4ft", ",", "h\u00f6rt", "nicht", "das", "Schie\u00b7\u00dfen", "und", "L\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PPER", "ADJD", "$,", "PPER", "VVFIN", "KON", "VVFIN", "$,", "VVFIN", "PTKNEG", "ART", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+-+-+--+-", "measure": "iambic.septa.relaxed"}, "line.2": {"text": "Und sie entschl\u00e4ft f\u00fcr alle Zeit, es kann sie nichts mehr h\u00e4rmen,", "tokens": ["Und", "sie", "ent\u00b7schl\u00e4ft", "f\u00fcr", "al\u00b7le", "Zeit", ",", "es", "kann", "sie", "nichts", "mehr", "h\u00e4r\u00b7men", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "PIAT", "NN", "$,", "PPER", "VMFIN", "PPER", "PIS", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Sie sieht nicht, wie vor\u00fcberrollt, als von der Luft getragen,", "tokens": ["Sie", "sieht", "nicht", ",", "wie", "vor\u00b7\u00fc\u00b7berr\u00b7ollt", ",", "als", "von", "der", "Luft", "ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWAV", "VVPP", "$,", "KOUS", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Im Sonnenschein der Freudenzug der k\u00f6niglichen Wagen,", "tokens": ["Im", "Son\u00b7nen\u00b7schein", "der", "Freu\u00b7den\u00b7zug", "der", "k\u00f6\u00b7nig\u00b7li\u00b7chen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute, stille Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "stil\u00b7le", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "Denn hinten auf dem hintersten im goldbetre\u00dften Kleide", "tokens": ["Denn", "hin\u00b7ten", "auf", "dem", "hin\u00b7ters\u00b7ten", "im", "gold\u00b7be\u00b7tre\u00df\u00b7ten", "Klei\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "ADV", "APPR", "ART", "VVFIN", "APPRART", "ADJA", "NN"], "meter": "-+-+-+---+-+-+-", "measure": "unknown.measure.hexa"}, "line.2": {"text": "Ein J\u00e4ger stand, der hie\u00df der Tod, und l\u00f6st sie von dem Leide,", "tokens": ["Ein", "J\u00e4\u00b7ger", "stand", ",", "der", "hie\u00df", "der", "Tod", ",", "und", "l\u00f6st", "sie", "von", "dem", "Lei\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "NN", "$,", "KON", "VVFIN", "PPER", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute, arme Frau R\u00f6sel.", "tokens": ["Die", "gu\u00b7te", ",", "ar\u00b7me", "Frau", "R\u00f6\u00b7sel", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.11": {"line.1": {"text": "Heut kommt der Vogt herbeigerannt und kratzt sich an den Ohren:", "tokens": ["Heut", "kommt", "der", "Vogt", "her\u00b7bei\u00b7ge\u00b7rannt", "und", "kratzt", "sich", "an", "den", "Oh\u00b7ren", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "VVPP", "KON", "VVFIN", "PRF", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Nun hab die letzte Steuer ich aus eigner Schuld verloren", "tokens": ["Nun", "hab", "die", "letz\u00b7te", "Steu\u00b7er", "ich", "aus", "eig\u00b7ner", "Schuld", "ver\u00b7lo\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ART", "ADJA", "NN", "PPER", "APPR", "ADJA", "NN", "VVPP"], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Am alten Weib, der R\u00f6sel!", "tokens": ["Am", "al\u00b7ten", "Weib", ",", "der", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Was soll ich denn dem toten Weib, dem hinterlist'gen, pf\u00e4nden?", "tokens": ["Was", "soll", "ich", "denn", "dem", "to\u00b7ten", "Weib", ",", "dem", "hin\u00b7ter\u00b7list'\u00b7gen", ",", "pf\u00e4n\u00b7den", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["PWS", "VMFIN", "PPER", "ADV", "ART", "ADJA", "NN", "$,", "PRELS", "VVPP", "$,", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Es bleibt mir nichts als Flitterkram und welkes Laub in H\u00e4nden!", "tokens": ["Es", "bleibt", "mir", "nichts", "als", "Flit\u00b7ter\u00b7kram", "und", "wel\u00b7kes", "Laub", "in", "H\u00e4n\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "PIS", "KOKOM", "NE", "KON", "PWAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Das schlechte Weib, die R\u00f6sel!", "tokens": ["Das", "schlech\u00b7te", "Weib", ",", "die", "R\u00f6\u00b7sel", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der K\u00fcnstler auch, Herr Bunzelmann, er kam herbeigehunken:", "tokens": ["Der", "K\u00fcnst\u00b7ler", "auch", ",", "Herr", "Bun\u00b7zel\u00b7mann", ",", "er", "kam", "her\u00b7bei\u00b7ge\u00b7hun\u00b7ken", ":"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "$,", "NN", "NE", "$,", "PPER", "VVFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.2": {"text": "Gut ist es, da\u00df mein Honorar ich auf der Stell getrunken!", "tokens": ["Gut", "ist", "es", ",", "da\u00df", "mein", "Ho\u00b7no\u00b7rar", "ich", "auf", "der", "Stell", "ge\u00b7trun\u00b7ken", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "VAFIN", "PPER", "$,", "KOUS", "PPOSAT", "NN", "PPER", "APPR", "ART", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-+-", "measure": "iambic.septa"}, "line.3": {"text": "Die gute arme Frau R\u00f6sel.",