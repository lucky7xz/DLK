{"textgrid.poem.31552": {"metadata": {"author": {"name": "Greiffenberg, Catharina Regina von", "birth": "N.A.", "death": "N.A."}, "title": "1L: Trotz Noht und Tod! trotz allem Vngel\u00fccke!", "genre": "verse", "period": "N.A.", "pub_year": 1663, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Trotz Noht und Tod! trotz allem Vngel\u00fccke!", "tokens": ["Trotz", "Noht", "und", "Tod", "!", "trotz", "al\u00b7lem", "Vn\u00b7ge\u00b7l\u00fc\u00b7cke", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "$.", "APPR", "PIS", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Trutz sey dem Belial! und aller seiner Schaar!", "tokens": ["Trutz", "sey", "dem", "Be\u00b7li\u00b7al", "!", "und", "al\u00b7ler", "sei\u00b7ner", "Schaar", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "ART", "NN", "$.", "KON", "PIAT", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "ich f\u00fcrcht vor ihrer Macht mich jetzo nicht ein Haar!", "tokens": ["ich", "f\u00fcrcht", "vor", "ih\u00b7rer", "Macht", "mich", "jet\u00b7zo", "nicht", "ein", "Haar", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPOSAT", "NN", "PPER", "ADV", "PTKNEG", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "sie schtessen wider mich / auch ganze Str\u00f6m voll T\u00fccke.", "tokens": ["sie", "schtes\u00b7sen", "wi\u00b7der", "mich", "/", "auch", "gan\u00b7ze", "Str\u00f6m", "voll", "T\u00fc\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "PPER", "$(", "ADV", "ADJA", "NN", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Wann ich nur Herz getrost nach meinem Helffer blicke /", "tokens": ["Wann", "ich", "nur", "Herz", "ge\u00b7trost", "nach", "mei\u00b7nem", "Helf\u00b7fer", "bli\u00b7cke", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "ADV", "NN", "VVPP", "APPR", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "der zu der Rechten sitzt / verlach' ich die Gefahr.", "tokens": ["der", "zu", "der", "Rech\u00b7ten", "sitzt", "/", "ver\u00b7lach'", "ich", "die", "Ge\u00b7fahr", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "ART", "NN", "VVFIN", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ihm ist mein Herz / die Noht / und Mittel offenbar.", "tokens": ["Ihm", "ist", "mein", "Herz", "/", "die", "Noht", "/", "und", "Mit\u00b7tel", "of\u00b7fen\u00b7bar", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$(", "ART", "NN", "$(", "KON", "NN", "ADJD", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "nach dem die Seuffzer ich / als H\u00fclff-erbitter / schicke.", "tokens": ["nach", "dem", "die", "Seuff\u00b7zer", "ich", "/", "als", "H\u00fclff\u00b7er\u00b7bit\u00b7ter", "/", "schi\u00b7cke", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "ART", "ART", "NN", "PPER", "$(", "KOUS", "NN", "$(", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Er l\u00e4nket / wie die B\u00e4ch / der Herzen Anschl\u00e4g' all.", "tokens": ["Er", "l\u00e4n\u00b7ket", "/", "wie", "die", "B\u00e4ch", "/", "der", "Her\u00b7zen", "An\u00b7schl\u00e4g'", "all", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "KOKOM", "ART", "NN", "$(", "ART", "NN", "NN", "PIAT", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Mit einen Wort er kan die starken Wellen hemmen.", "tokens": ["Mit", "ei\u00b7nen", "Wort", "er", "kan", "die", "star\u00b7ken", "Wel\u00b7len", "hem\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "PPER", "VMFIN", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Er l\u00e4st / so viel es taugt / dem Wasser seinen fall:", "tokens": ["Er", "l\u00e4st", "/", "so", "viel", "es", "taugt", "/", "dem", "Was\u00b7ser", "sei\u00b7nen", "fall", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$(", "ADV", "ADV", "PPER", "VVFIN", "$(", "ART", "NN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Gibt keines wegs doch zu / die Ebne zuverschw\u00e4mmen.", "tokens": ["Gibt", "kei\u00b7nes", "wegs", "doch", "zu", "/", "die", "Eb\u00b7ne", "zu\u00b7ver\u00b7schw\u00e4m\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIAT", "NN", "ADV", "PTKZU", "$(", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Er macht das tr\u00fcbe klar / aus Salz-Meer s\u00fcssen Safft.", "tokens": ["Er", "macht", "das", "tr\u00fc\u00b7be", "klar", "/", "aus", "Sa\u00b7l