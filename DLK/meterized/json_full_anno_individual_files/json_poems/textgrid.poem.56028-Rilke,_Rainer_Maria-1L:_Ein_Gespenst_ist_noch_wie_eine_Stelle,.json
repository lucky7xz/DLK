{"textgrid.poem.56028": {"metadata": {"author": {"name": "Rilke, Rainer Maria", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ein Gespenst ist noch wie eine Stelle,", "genre": "verse", "period": "N.A.", "pub_year": 1900, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Gespenst ist noch wie eine Stelle,", "tokens": ["Ein", "Ge\u00b7spenst", "ist", "noch", "wie", "ei\u00b7ne", "Stel\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADV", "KOKOM", "ART", "NN", "$,"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.2": {"text": "dran dein Blick mit einem Klange st\u00f6\u00dft;", "tokens": ["dran", "dein", "Blick", "mit", "ei\u00b7nem", "Klan\u00b7ge", "st\u00f6\u00dft", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPOSAT", "NN", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "aber da, an diesem schwarzen Felle", "tokens": ["a\u00b7ber", "da", ",", "an", "die\u00b7sem", "schwar\u00b7zen", "Fel\u00b7le"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "$,", "APPR", "PDAT", "ADJA", "NN"], "meter": "+-+-+-+-+-", "measure": "trochaic.penta"}, "line.4": {"text": "wird dein st\u00e4rkstes Schauen aufgel\u00f6st:", "tokens": ["wird", "dein", "st\u00e4rks\u00b7tes", "Schau\u00b7en", "auf\u00b7ge\u00b7l\u00f6st", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}}, "stanza.2": {"line.1": {"text": "wie ein Tobender, wenn er in vollster", "tokens": ["wie", "ein", "To\u00b7ben\u00b7der", ",", "wenn", "er", "in", "volls\u00b7ter"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ART