{"textgrid.poem.60502": {"metadata": {"author": {"name": "Lenau, Nikolaus", "birth": "N.A.", "death": "N.A."}, "title": "Der Laudachsee", "genre": "verse", "period": "N.A.", "pub_year": 1834, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "La\u00df meiner Einsamkeit das Angedenken", "tokens": ["La\u00df", "mei\u00b7ner", "Ein\u00b7sam\u00b7keit", "das", "An\u00b7ge\u00b7den\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVIMP", "PPOSAT", "NN", "ART", "NN"], "meter": "+--+-+-+-+-", "measure": "iambic.penta.invert"}, "line.2": {"text": "Der sch\u00f6nsten Stunden jetzt vor\u00fcberziehn,", "tokens": ["Der", "sch\u00f6ns\u00b7ten", "Stun\u00b7den", "jetzt", "vor\u00b7\u00fc\u00b7ber\u00b7ziehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Die mir das Erdenleben durfte schenken,", "tokens": ["Die", "mir", "das", "Er\u00b7den\u00b7le\u00b7ben", "durf\u00b7te", "schen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Geist der Natur! der \u00f6den Gegenwart entfliehn", "tokens": ["Geist", "der", "Na\u00b7tur", "!", "der", "\u00f6\u00b7den", "Ge\u00b7gen\u00b7wart", "ent\u00b7fliehn"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "$.", "ART", "ADJA", "NN", "VVINF"], "meter": "+--+-+-+-+-+", "measure": "iambic.hexa.invert"}, "line.5": {"text": "Und in Erinnerungen mich versenken,", "tokens": ["Und", "in", "E\u00b7rin\u00b7ne\u00b7run\u00b7gen", "mich", "ver\u00b7sen\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Zeig mir den See im stillen Felsentale,", "tokens": ["Zeig", "mir", "den", "See", "im", "stil\u00b7len", "Fel\u00b7sen\u00b7ta\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "ART", "NN", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Von Schilf und Wald die Ufer rings ums\u00e4umt,", "tokens": ["Von", "Schilf", "und", "Wald", "die", "U\u00b7fer", "rings", "um\u00b7s\u00e4umt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "KON", "NN", "ART", "NN", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Der Felsenh\u00e4upter Gl\u00fchn im Abendstrahle,", "tokens": ["Der", "Fel\u00b7sen\u00b7h\u00e4up\u00b7ter", "Gl\u00fchn", "im", "A\u00b7bend\u00b7strah\u00b7le", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Den Rasensitz, wo ich so s\u00fc\u00df getr\u00e4umt.", "tokens": ["Den", "Ra\u00b7sen\u00b7sitz", ",", "wo", "ich", "so", "s\u00fc\u00df", "ge\u00b7tr\u00e4umt", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PWAV", "PPER", "ADV", "ADJD", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.10": {"text": "Komm du auch, meine Freundin Phantasie,", "tokens": ["Komm", "du", "auch", ",", "mei\u00b7ne", "Freun\u00b7din", "Phan\u00b7ta\u00b7sie", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "PPOSAT", "NN", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.11": {"text": "Erweck mir Echos Geisterch\u00f6re", "tokens": ["Er\u