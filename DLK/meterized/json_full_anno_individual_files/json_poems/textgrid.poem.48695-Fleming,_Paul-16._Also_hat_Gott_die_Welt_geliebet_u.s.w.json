{"textgrid.poem.48695": {"metadata": {"author": {"name": "Fleming, Paul", "birth": "N.A.", "death": "N.A."}, "title": "16. Also hat Gott die Welt geliebet u.s.w", "genre": "verse", "period": "N.A.", "pub_year": 1624, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ists m\u00fcglich, da\u00df der Ha\u00df auch kan geliebet sein?", "tokens": ["Ists", "m\u00fcg\u00b7lich", ",", "da\u00df", "der", "Ha\u00df", "auch", "kan", "ge\u00b7lie\u00b7bet", "sein", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "ADJD", "$,", "KOUS", "ART", "NN", "ADV", "VMFIN", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ja, Liebe, sonst war Nichts, an dem du k\u00fcntest weisen,", "tokens": ["Ja", ",", "Lie\u00b7be", ",", "sonst", "war", "Nichts", ",", "an", "dem", "du", "k\u00fcn\u00b7test", "wei\u00b7sen", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "NN", "$,", "ADV", "VAFIN", "PIS", "$,", "APPR", "PRELS", "PPER", "VVFIN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wie stark dein Feuer sei, als an dem kalten Eisen", "tokens": ["wie", "stark", "dein", "Feu\u00b7er", "sei", ",", "als", "an", "dem", "kal\u00b7ten", "Ei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADJD", "PPOSAT", "NN", "VAFIN", "$,", "KOUS", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "der ausgest\u00e4lten Welt. Du, h\u00f6chster Sonnenschein,", "tokens": ["der", "aus\u00b7ge\u00b7st\u00e4l\u00b7ten", "Welt", ".", "Du", ",", "h\u00f6chs\u00b7ter", "Son\u00b7nen\u00b7schein", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$.", "PPER", "$,", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "wirfst deiner Stralen Glut in unser Eis herein,", "tokens": ["wirfst", "dei\u00b7ner", "Stra\u00b7len", "Glut", "in", "un\u00b7ser", "Eis", "her\u00b7ein", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "machst Tag aus unsrer Nacht. Und was noch mehr zu preisen,", "tokens": ["machst", "Tag", "aus", "uns\u00b7rer", "Nacht", ".", "Und", "was", "noch", "mehr", "zu", "prei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "PPOSAT", "NN", "$.", "KON", "PWS", "ADV", "ADV", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "du wirst des Armuts Schatz, des Hungers s\u00fc\u00dfe Speisen,", "tokens": ["du", "wirst", "des", "Ar\u00b7muts", "Schatz", ",", "des", "Hun\u00b7gers", "s\u00fc\u00b7\u00dfe", "Spei\u00b7sen", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "NN", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "giebst Himmel f\u00fcr die Welt. O Pein der H\u00f6llenpein,", "tokens": ["giebst", "Him\u00b7mel", "f\u00fcr", "die", "Welt", ".", "O", "Pein", "der", "H\u00f6l\u00b7len\u00b7pein", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "APPR", "ART", "NN", "$.", "NE", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "o Todesgift und Tod, o wahrer Freund der Feinde,", "tokens": ["o", "To\u00b7des\u00b7gift", "und", "Tod", ",", "o", "wah\u00b7rer", "Freund", "der", "Fein\u00b7de", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "KON", "NN", "$,", "FM", "ADJA", "NN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "o Meister, der du auch dein Werk dir machst zum Freunde,", "tokens": ["o", "Meis\u00b7ter", ",", "der", "du", "auch", "dein", "Werk", "dir", "machst", "zum", "Freun\u00b7de", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["FM", "NN", "$,", "PRELS", "PPER", "ADV", "PPOSAT", "NN", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "wirst deiner Diener Knecht, wirst deiner Tochter Kind.", "tokens": ["wirst", "dei\u00b7ner", "Die\u00b7ner", "Knecht", ",", "wirst", "dei\u00b7ner", "Toch\u00b7ter", "Kind", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "NE", "$,", "VAFIN", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.4": {"line.1": {"text": "Was tu ich, da\u00df ich doch den Abgrund will ergr\u00fcnden!", "tokens": ["Was", "tu", "ich", ",", "da\u00df", "ich", "doch", "den", "Ab\u00b7grund", "will", "er\u00b7gr\u00fcn\u00b7den", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "KOUS", "PPER", "ADV", "ART", "NN", "VMFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": 