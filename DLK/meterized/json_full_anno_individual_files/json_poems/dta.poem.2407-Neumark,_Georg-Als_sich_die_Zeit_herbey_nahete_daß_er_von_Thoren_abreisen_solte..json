{"dta.poem.2407": {"metadata": {"author": {"name": "Neumark, Georg", "birth": "N.A.", "death": "N.A."}, "title": "Als sich die Zeit herbey nahete da\u00df er von  \n Thoren abreisen solte.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1652", "urn": "urn:nbn:de:kobv:b4-20428-0", "language": ["de:0.99"], "booktitle": "Neumark, Georg: Poetisch- und Musikalisches Lustw\u00e4ldchen. Hamburg, 1652."}, "poem": {"stanza.1": {"line.1": {"text": "So ist es denn an demda\u00df ich von dir soll scheiden", "tokens": ["So", "ist", "es", "denn", "an", "dem\u00b7da\u00df", "ich", "von", "dir", "soll", "schei\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "APPR", "PRELS", "PPER", "APPR", "PPER", "VMFIN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Du edles Thoren du/ du andre Vaterstadt/", "tokens": ["Du", "ed\u00b7les", "Tho\u00b7ren", "du", "/", "du", "and\u00b7re", "Va\u00b7ter\u00b7stadt", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "PPER", "$(", "PPER", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Du Wohnhau\u00df meiner Lust und zweybejahrter Freuden/", "tokens": ["Du", "Wohn\u00b7hau\u00df", "mei\u00b7ner", "Lust", "und", "zwey\u00b7be\u00b7jahr\u00b7ter", "Freu\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "PPOSAT", "NN", "KON", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Von dir/ da mich so offt dein Tuhn ergetzet hat?", "tokens": ["Von", "dir", "/", "da", "mich", "so", "offt", "dein", "Tuhn", "er\u00b7get\u00b7zet", "hat", "?"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPER", "$(", "KOUS", "PPER", "ADV", "ADV", "PPOSAT", "NN", "VVPP", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Soll ich mich sch\u00f6ner Ohrt anietzo von dir reissen?", "tokens": ["Soll", "ich", "mich", "sch\u00f6\u00b7ner", "Ohrt", "an\u00b7iet\u00b7zo", "von", "dir", "reis\u00b7sen", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "PRF", "ADJA", "NN", "ADV", "APPR", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Sol ich ins k\u00fcnfftige dich nimmer wieder sehen", "tokens": ["Sol", "ich", "ins", "k\u00fcnff\u00b7ti\u00b7ge", "dich", "nim\u00b7mer", "wie\u00b7der", "se\u00b7hen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PPER", "APPRART", "ADJA", "PPER", "ADV", "ADV", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Du wehrte Stadt die du den Schl\u00fcssel von dem Preussen/", "tokens": ["Du", "wehr\u00b7te", "Stadt", "die", "du", "den", "Schl\u00fcs\u00b7sel", "von", "dem", "Preus\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "NN", "ART", "PPER", "ART", "NN", "APPR", "ART", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "An deiner Seiten tr\u00e4gst? Ach schwerlich wirds geschehn.", "tokens": ["An", "dei\u00b7ner", "Sei\u00b7ten", "tr\u00e4gst", "?", "Ach", "schwer\u00b7lich", "wirds", "ge\u00b7schehn", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "VVFIN", "$.", "NN", "ADJD", "VAFIN", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "So hab denn gute Nacht zu hundert tausend malen", "tokens": ["So", "hab", "denn", "gu\u00b7te", "Nacht", "zu", "hun\u00b7dert", "tau\u00b7send", "ma\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VAFIN", "ADV", "ADJA", "NN", "APPR", "CARD", "CARD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Sey ewig unbetr\u00fcbt du hochbeliebte du", "tokens": ["Sey", "e\u00b7wig", "un\u00b7be\u00b7tr\u00fcbt", "du", "hoch\u00b7be\u00b7lieb\u00b7te", "du"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VAIMP", "ADJD", "ADJD", "PPER", "VVFIN", "PPER"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Gott lasse seine Gunst dir unaufh\u00f6rlich strahlen", "tokens": ["Gott", "las\u00b7se", "sei\u00b7ne", "Gunst", "dir", "un\u00b7auf\u00b7h\u00f6r\u00b7lich", "strah\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVFIN", "PPOSAT", "NN", "PPER", "ADJD", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Leb allezeit vergn\u00fcgt in stottzem Fried\u2019 und Ruh.", "tokens": ["Leb", "al\u00b7le\u00b7zeit", "ver\u00b7gn\u00fcgt", "in", "stott\u00b7zem", "Fried'", "und", "Ruh", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "VVPP", "APPR", "ADJA", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.13": {"text": "Habt tausend gute Nacht ihr vielgeehrte Freunde", "tokens": ["Habt", "tau\u00b7send", "gu\u00b7te", "Nacht", "ihr", "viel\u00b7geehr\u00b7te", "Freun\u00b7de"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["VAFIN", "CARD", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+--+-+-", "measure": "iambic.penta.relaxed"}, "line.14": {"text": "Du grosser Daffnis du/ und du auch Floridan/", "tokens": ["Du", "gros\u00b7ser", "Daff\u00b7nis", "du", "/", "und", "du", "auch", "Flo\u00b7ri\u00b7dan", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NE", "NE", "$(", "KON", "PPER", "ADV", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Du \u00e4dler Tytirus/ du Schrekken meiner Feinde/", "tokens": ["Du", "\u00e4d\u00b7ler", "Ty\u00b7ti\u00b7rus", "/", "du", "Schrek\u00b7ken", "mei\u00b7ner", "Fein\u00b7de", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ADJA", "NN", "$(", "PPER", "NN", "PPOSAT", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Du wehrter Filidor du tieffgelehrter Mann/", "tokens": ["Du", "wehr\u00b7ter", "Fi\u00b7li\u00b7dor", "du", "tieff\u00b7ge\u00b7lehr\u00b7ter", "Mann", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "NE", "NE", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.17": {"text": "Und wer ihr ferner seyd/ seit allezeit gepriesen/", "tokens": ["Und", "wer", "ihr", "fer\u00b7ner", "seyd", "/", "seit", "al\u00b7le\u00b7zeit", "ge\u00b7prie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "PWS", "PPER", "ADV", "VAFIN", "$(", "APPR", "ADV", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Von meiner Poesie/ vor eure sch\u00f6ne Gunst", "tokens": ["Von", "mei\u00b7ner", "Poe\u00b7sie", "/", "vor", "eu\u00b7re", "sch\u00f6\u00b7ne", "Gunst"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["APPR", "PPOSAT", "NN", "$(", "APPR", "PPOSAT", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}, "line.19": {"text": "Die treu und hertzlich war/ die ihr mir habt erwiesen/", "tokens": ["Die", "treu", "und", "hertz\u00b7lich", "war", "/", "die", "ihr", "mir", "habt", "er\u00b7wie\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "ADJD", "VAFIN", "$(", "PRELS", "PPER", "PPER", "VAFIN", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.20": {"text": "Zum theil bald br\u00fcderlich. Ich wil die beste Kunst", "tokens": ["Zum", "theil", "bald", "br\u00fc\u00b7der\u00b7lich", ".", "Ich", "wil", "die", "bes\u00b7te", "Kunst"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "ADV", "ADJD", "$.", "PPER", "VMFIN", "ART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Gebrauchen die ich kan \u00fcm euch nur hoch zu preisen", "tokens": ["Ge\u00b7brau\u00b7chen", "die", "ich", "kan", "\u00fcm", "euch", "nur", "hoch", "zu", "prei\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "ART", "PPER", "VMFIN", "VVFIN", "PPER", "ADV", "ADJD", "PTKZU", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "Die ihr es w\u00fcrdig seit. Der ist ein B\u00f6sewicht", "tokens": ["Die", "ihr", "es", "w\u00fcr\u00b7dig", "seit", ".", "Der", "ist", "ein", "B\u00f6\u00b7se\u00b7wicht"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "PPER", "PPER", "ADJD", "NN", "$.", "PDS", "VAFIN", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Der sich vor Guttaht kan nicht dankbarlich erweisen.", "tokens": ["Der", "sich", "vor", "Gut\u00b7taht", "kan", "nicht", "dank\u00b7bar\u00b7lich", "er\u00b7wei\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PRF", "APPR", "NN", "VMFIN", "PTKNEG", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.24": {"text": "Ein b\u00f6ser Unmensch wei\u00df von keinem Danke nicht.", "tokens": ["Ein", "b\u00f6\u00b7ser", "Un\u00b7mensch", "wei\u00df", "von", "kei\u00b7nem", "Dan\u00b7ke", "nicht", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PIAT", "NN", "PTKNEG", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.25": {"text": "Auch ihr habt hertzlich Dank ihr \u00e4dlen Wei\u00dfe linnen", "tokens": ["Auch", "ihr", "habt", "hertz\u00b7lich", "Dank", "ihr", "\u00e4d\u00b7len", "Wei\u00b7\u00dfe", "lin\u00b7nen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADJD", "APPR", "PPOSAT", "ADJA", "NN", "VVINF"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Jhr honigs\u00fcsses Volk die ihr an H\u00f6fligkeit", "tokens": ["Ihr", "ho\u00b7ni\u00b7gs\u00fcs\u00b7ses", "Volk", "die", "ihr", "an", "H\u00f6f\u00b7lig\u00b7keit"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJA", "NN", "ART", "PPER", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.27": {"text": "Geht vielen vielen vor/ habt dank ibr Venusinnen", "tokens": ["Geht", "vie\u00b7len", "vie\u00b7len", "vor", "/", "habt", "dank", "ibr", "Ve\u00b7nu\u00b7sin\u00b7nen"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["VVFIN", "PIAT", "PIS", "PTKVZ", "$(", "VAFIN", "APPR", "NE", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Die ihr so manchesmal/ die liebgew\u00fcnschte Zeit/", "tokens": ["Die", "ihr", "so", "man\u00b7ches\u00b7mal", "/", "die", "lieb\u00b7ge\u00b7w\u00fcnschte", "Zeit", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ADV", "ADV", "$(", "ART", "ADJA", "NN", "$("], "meter": "-+-+---+--+", "measure": "iambic.tetra.chol"}, "line.29": {"text": "Auf eure Polnisch\u2019 Ahrt/ mir habet kurtz gemachet/", "tokens": ["Auf", "eu\u00b7re", "Pol\u00b7nisch'", "Ahrt", "/", "mir", "ha\u00b7bet", "kurtz", "ge\u00b7ma\u00b7chet", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "NN", "$(", "PPER", "VAFIN", "ADJD", "VVPP", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.30": {"text": "Lebt allzeit so erfreut/ ihr \u00e4dle Tugendschaar/", "tokens": ["Lebt", "all\u00b7zeit", "so", "er\u00b7freut", "/", "ihr", "\u00e4d\u00b7le", "Tu\u00b7gend\u00b7schaar", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ADJD", "$(", "PPOSAT", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Der Himmel/ der allzeit f\u00fcr eure Wohlfahrt wachet", "tokens": ["Der", "Him\u00b7mel", "/", "der", "all\u00b7zeit", "f\u00fcr", "eu\u00b7re", "Wohl\u00b7fahrt", "wa\u00b7chet"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$(", "ART", "ADV", "APPR", "PPOSAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Der geb euch zartes Volk viel hundert gute Jahr\u2019/", "tokens": ["Der", "geb", "euch", "zar\u00b7tes", "Volk", "viel", "hun\u00b7dert", "gu\u00b7te", "Jahr'", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "PPER", "ADJA", "NN", "ADV", "CARD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "Auch ihr habt gute Nacht ihr wundersch\u00f6nen G\u00e4rten", "tokens": ["Auch", "ihr", "habt", "gu\u00b7te", "Nacht", "ihr", "wun\u00b7der\u00b7sch\u00f6\u00b7nen", "G\u00e4r\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ADV", "PPER", "VAFIN", "ADJA", "NN", "PPOSAT", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "Jhr Oerter meiner Freud\u2019/ ihr Pl\u00e4tze meiner Lust/", "tokens": ["Ihr", "O\u00b7er\u00b7ter", "mei\u00b7ner", "Freud'", "/", "ihr", "Pl\u00e4t\u00b7ze", "mei\u00b7ner", "Lust", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "PPOSAT", "NN", "$(", "PPOSAT", "NN", "PPOSAT", "NN", "$("], "meter": "-+--+-+-+-+-+", "measure": "iambic.hexa.relaxed"}, "line.35": {"text": "Mein Hertz erstarret fast/ die Zunge wil verh\u00e4rten/", "tokens": ["Mein", "Hertz", "er\u00b7star\u00b7ret", "fast", "/", "die", "Zun\u00b7ge", "wil", "ver\u00b7h\u00e4r\u00b7ten", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "ADV", "$(", "ART", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.36": {"text": "Wenn ich den \u00f6fftern Schertz/ der/ Himmel dir bewust/", "tokens": ["Wenn", "ich", "den", "\u00f6ff\u00b7tern", "Schertz", "/", "der", "/", "Him\u00b7mel", "dir", "be\u00b7wust", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "$(", "ART", "$(", "NN", "PPER", "VVFIN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.37": {"text": "Bey mir betr\u00fcbt erweg\u2019/ ich bin vor grossen Leiden", "tokens": ["Bey", "mir", "be\u00b7tr\u00fcbt", "er\u00b7weg'", "/", "ich", "bin", "vor", "gros\u00b7sen", "Lei\u00b7den"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "PPER", "VVPP", "VVFIN", "$(", "PPER", "VAFIN", "APPR", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Jetzund fast nicht mein selbst/ da\u00df ich/ ihr sch\u00f6nen Pl\u00e4tz?", "tokens": ["Je\u00b7tzund", "fast", "nicht", "mein", "selbst", "/", "da\u00df", "ich", "/", "ihr", "sch\u00f6\u00b7nen", "Pl\u00e4tz", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PTKNEG", "PPOSAT", "ADV", "$(", "KOUS", "PPER", "$(", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.39": {"text": "Auch Przischek und Slavkow\u2019 hinf\u00fcro so mu\u00df meiden/", "tokens": ["Auch", "Pr\u00b7zi\u00b7schek", "und", "Slav\u00b7kow'", "hin\u00b7f\u00fc\u00b7ro", "so", "mu\u00df", "mei\u00b7den", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NE", "ADV", "ADV", "VMFIN", "VVINF", "$("], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.40": {"text": "Die ich was Lust bebtrifft/ fast gleich dem Tempe sch\u00e4tz\u2019.", "tokens": ["Die", "ich", "was", "Lust", "beb\u00b7trifft", "/", "fast", "gleich", "dem", "Tem\u00b7pe", "sch\u00e4tz'", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "PIS", "NN", "VVFIN", "$(", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Und du mein Str\u00f6mchen du fliess\u2019 ewig unbetrubet/", "tokens": ["Und", "du", "mein", "Str\u00f6m\u00b7chen", "du", "fliess'", "e\u00b7wig", "un\u