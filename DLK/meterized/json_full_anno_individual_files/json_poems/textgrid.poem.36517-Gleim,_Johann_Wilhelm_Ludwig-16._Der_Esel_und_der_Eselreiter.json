{"textgrid.poem.36517": {"metadata": {"author": {"name": "Gleim, Johann Wilhelm Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "16. Der Esel und der Eselreiter", "genre": "verse", "period": "N.A.", "pub_year": 1761, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was kann denn ich davor,", "tokens": ["Was", "kann", "denn", "ich", "da\u00b7vor", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "KON", "PPER", "PAV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.2": {"text": "Da\u00df du mit langem Ohr", "tokens": ["Da\u00df", "du", "mit", "lan\u00b7gem", "Ohr"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Geboren bist, Herr Esel? \u2013 Lang-Ohr nennen", "tokens": ["Ge\u00b7bo\u00b7ren", "bist", ",", "Herr", "E\u00b7sel", "?", "\u2013", "Lang\u00b7Ohr", "nen\u00b7nen"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct", "word", "word"], "pos": ["VVPP", "VAFIN", "$,", "NN", "NN", "$.", "$(", "NN", "VVINF"], "meter": "-+--+-+-+--", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Wir andern dich mit Recht!", "tokens": ["Wir", "an\u00b7dern", "dich", "mit", "Recht", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PIS", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Denn sieh! vom ganzen Tiergeschlecht", "tokens": ["Denn", "sieh", "!", "vom", "gan\u00b7zen", "Tier\u00b7ge\u00b7schlecht"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVIMP", "$.", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Hast du das l\u00e4ngste, du! das mu\u00dft du doch bekennen!", "tokens": ["Hast", "du", "das", "l\u00e4ngs\u00b7te", ",", "du", "!", "das", "mu\u00dft", "du", "doch", "be\u00b7ken\u00b7nen", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "$,", "PPER", "$.", "PDS", "VMFIN", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Ja! das bekenn' ich, sprach der Esel zu dem Mann,", "tokens": ["Ja", "!", "das", "be\u00b7kenn'", "ich", ",", "sprach", "der", "E\u00b7sel", "zu", "dem", "Mann", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$.", "PDS", "VVFIN", "PPER", "$,", "VVFIN", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+--+--+", "measure": "iambic.penta.relaxed"}, "line.2": {"text": "Der auf ihm sa\u00df, und seine Klagen:", "tokens": ["Der", "auf", "ihm", "sa\u00df", ",", "und", "sei\u00b7ne", "Kla\u00b7gen", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PPER", "VVFIN", "$,", "KON", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df er die schwerste Last getragen;", "tokens": ["Da\u00df", "er", "die", "schwers\u00b7te", "Last", "ge\u00b7tra\u00b7gen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Da\u00df er, was er gekonnt, gethan;", "tokens": ["Da\u00df", "er", ",", "was", "er", "ge\u00b7konnt", ",", "ge\u00b7than", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "PWS", "PPER", "ADJD", "$,", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df unbarmherzig doch der Treiber ihn geschlagen,", "tokens": ["Da\u00df", "un\u00b7barm\u00b7her\u00b7zig", "doch", "der", "Trei\u00b7ber", "ihn", "ge\u00b7schla\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "ADV", "ART", "NN", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Anh\u00f6rt, und Spott, f\u00fcr Trost,", "tokens": ["An\u00b7h\u00f6rt", ",", "und", "Spott", ",", "f\u00fcr", "Trost", ","], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "$,", "KON", "NN", "$,", "APPR", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Dem guten Esel konnte sagen,", "tokens": ["Dem", "gu\u00b7ten", "E\u00b7sel", "konn\u00b7te", "sa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VMFIN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.8": {"text": "Fast ungeduldig, und erbost", "tokens": ["Fast", "un\u00b7ge\u00b7dul\u00b7dig", ",", "und", "er\u00b7bost"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["ADV", "ADJD", "$,", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Allein, mein edler Herr, das werden Sie bekennen,", "tokens": ["Al\u00b7lein", ",", "mein", "ed\u00b7ler", "Herr", ",", "das", "wer\u00b7den", "Sie", "be\u00b7ken\u00b7nen", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "$,", "PPOSAT", "ADJA", "NN", "$,", "PDS", "VAFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Da\u00df einen schlechten Mann,", "tokens": ["Da\u00df", "ei\u00b7nen", "schlech\u00b7ten", "Mann", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KOUS", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"