{"textgrid.poem.35785": {"metadata": {"author": {"name": "Hensel, Luise", "birth": "N.A.", "death": "N.A."}, "title": "1L: Es warten dein zwei Freier;", "genre": "verse", "period": "N.A.", "pub_year": 1816, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Es warten dein zwei Freier;", "tokens": ["Es", "war\u00b7ten", "dein", "zwei", "Frei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schau her und w\u00e4hle, Kind!", "tokens": ["Schau", "her", "und", "w\u00e4h\u00b7le", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm, den dein Herz getreuer", "tokens": ["Nimm", ",", "den", "dein", "Herz", "ge\u00b7treu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sch\u00f6ner, reicher find't.", "tokens": ["Und", "sch\u00f6\u00b7ner", ",", "rei\u00b7cher", "find'", "t."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Der Erste ist ein K\u00f6nig,", "tokens": ["Der", "Ers\u00b7te", "ist", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein F\u00fcrst von dieser Welt.", "tokens": ["Ein", "F\u00fcrst", "von", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bber bietet mir zu wenig,", "tokens": ["\u00bb", "er", "bie\u00b7tet", "mir", "zu", "we\u00b7nig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKA", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur eitel Pracht und Geld.\u00ab", "tokens": ["Nur", "ei\u00b7tel", "Pracht", "und", "Geld", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.3": {"line.1": {"text": "Der Andre hat dort dr\u00fcben", "tokens": ["Der", "And\u00b7re", "hat", "dort", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein ewig K\u00f6nigreich.", "tokens": ["Sein", "e\u00b7wig", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbja, diesen will ich lieben", "tokens": ["\u00bb", "ja", ",", "die\u00b7sen", "will", "ich", "lie\u00b7ben"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "PDS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mit ihm ziehen gleich.\u00ab", "tokens": ["Und", "mit", "ihm", "zie\u00b7hen", "gleich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.4": {"line.1": {"text": "Der Erste will dir schenken", "tokens": ["Der", "Ers\u00b7te", "will", "dir", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Ehre, Schmuck und Reiz;", "tokens": ["Viel", "Eh\u00b7re", ",", "Schmuck", "und", "Reiz", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Noch kannst du dich bedenken:", "tokens": ["Noch", "kannst", "du", "dich", "be\u00b7den\u00b7ken", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "VVINF", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Andre tr\u00e4gt ein Kreuz.", "tokens": ["Der", "And\u00b7re", "tr\u00e4gt", "ein", "Kreuz", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.5": {"line.1": {"text": "\u00bbden Ersten la\u00df ich stehen,", "tokens": ["\u00bb", "den", "Ers\u00b7ten", "la\u00df", "ich", "ste\u00b7hen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVIMP", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er sucht und liebt nur sich;", "tokens": ["Er", "sucht", "und", "liebt", "nur", "sich", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "KON", "VVFIN", "ADV", "PRF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich will zum Andern gehen,", "tokens": ["Ich", "will", "zum", "An\u00b7dern", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "APPRART", "ADJA", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Er tr\u00e4gt das Kreuz um mich.\u00ab", "tokens": ["Er", "tr\u00e4gt", "das", "Kreuz", "um", "mich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.6": {"line.1": {"text": "Halt! sieh erst noch die Krone,", "tokens": ["Halt", "!", "sieh", "erst", "noch", "die", "Kro\u00b7ne", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "$.", "VVIMP", "ADV", "ADV", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die dir der Erste reicht;", "tokens": ["Die", "dir", "der", "Ers\u00b7te", "reicht", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "ART", "ADJA", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Dann sieh, was dir zum Lohne", "tokens": ["Dann", "sieh", ",", "was", "dir", "zum", "Loh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "$,", "PWS", "PPER", "APPRART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Der Andre giebt vielleicht.", "tokens": ["Der", "And\u00b7re", "giebt", "viel\u00b7leicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.7": {"line.1": {"text": "\u00bbdie Krone seh' ich prangen,", "tokens": ["\u00bb", "die", "Kro\u00b7ne", "seh'", "ich", "pran\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "VVFIN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Doch ist es Feuerglanz.\u00ab", "tokens": ["Doch", "ist", "es", "Feu\u00b7er\u00b7glanz", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "VAFIN", "PPER", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Der Andre kommt gegangen", "tokens": ["Der", "And\u00b7re", "kommt", "ge\u00b7gan\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "PIS", "VVFIN", "VVPP"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit einem Dornenkranz.", "tokens": ["Mit", "ei\u00b7nem", "Dor\u00b7nen\u00b7kranz", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.8": {"line.1": {"text": "\u00bbzum Kranz die Hand ich neige,", "tokens": ["\u00bb", "zum", "Kranz", "die", "Hand", "ich", "nei\u00b7ge", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "APPRART", "NN", "ART", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er soll mein Haupt umziehn:", "tokens": ["Er", "soll", "mein", "Haupt", "um\u00b7ziehn", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPOSAT", "NN", "VVIZU", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich seh' aus jedem Zweige", "tokens": ["Ich", "seh'", "aus", "je\u00b7dem", "Zwei\u00b7ge"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "PIAT", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Die sch\u00f6nste Ros' erbl\u00fchn.\u00ab", "tokens": ["Die", "sch\u00f6ns\u00b7te", "Ros'", "er\u00b7bl\u00fchn", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "ADJA", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.9": {"line.1": {"text": "Noch sieh, was in den Kelchen,", "tokens": ["Noch", "sieh", ",", "was", "in", "den", "Kel\u00b7chen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "$,", "PRELS", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Die lockend vor dir stehn,", "tokens": ["Die", "lo\u00b7ckend", "vor", "dir", "stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "PPER", "VVINF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Und sage mir dann, welchen", "tokens": ["Und", "sa\u00b7ge", "mir", "dann", ",", "wel\u00b7chen"], "token_info": ["word", "word", "word", "word", "punct", "word"], "pos": ["KON", "VVFIN", "PPER", "ADV", "$,", "PWAT"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du dir hast ausersehn.", "tokens": ["Du", "dir", "hast", "au\u00b7ser\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "PPER", "VAFIN", "VVINF", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.10": {"line.1": {"text": "\u00bbhier funkeln bunte L\u00fcgen,", "tokens": ["\u00bb", "hier", "fun\u00b7keln", "bun\u00b7te", "L\u00fc\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADV", "VVFIN", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Tr\u00fcb sch\u00e4umt der L\u00fcste Fluth;", "tokens": ["Tr\u00fcb", "sch\u00e4umt", "der", "L\u00fcs\u00b7te", "Fluth", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.3": {"text": "Im andern ruht verschwiegen", "tokens": ["Im", "an\u00b7dern", "ruht", "ver\u00b7schwie\u00b7gen"], "token_info": ["word", "word", "word", "word"], "pos": ["APPRART", "ADJA", "VVFIN", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Das allerh\u00f6chste Gut.", "tokens": ["Das", "al\u00b7ler\u00b7h\u00f6chs\u00b7te", "Gut", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.11": {"line.1": {"text": "Der erste mag wohl blinken,", "tokens": ["Der", "ers\u00b7te", "mag", "wohl", "blin\u00b7ken", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VMFIN", "ADV", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Mir ist er nicht gesund;", "tokens": ["Mir", "ist", "er", "nicht", "ge\u00b7sund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPER", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Den andern will ich trinken", "tokens": ["Den", "an\u00b7dern", "will", "ich", "trin\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Bis auf den tiefsten Grund.\u00ab", "tokens": ["Bis", "auf", "den", "tiefs\u00b7ten", "Grund", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPR", "APPR", "ART", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.12": {"line.1": {"text": "Zuvor schau auf die Wege,", "tokens": ["Zu\u00b7vor", "schau", "auf", "die", "We\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Noch winket dir das Gl\u00fcck;", "tokens": ["Noch", "win\u00b7ket", "dir", "das", "Gl\u00fcck", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Bedenke, \u00fcberlege:", "tokens": ["Be\u00b7den\u00b7ke", ",", "\u00fc\u00b7berl\u00b7e\u00b7ge", ":"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Du kannst nicht mehr zur\u00fcck.", "tokens": ["Du", "kannst", "nicht", "mehr", "zu\u00b7r\u00fcck", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "ADV", "PTKVZ", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.13": {"line.1": {"text": "Der erste breit und linde,", "tokens": ["Der", "ers\u00b7te", "breit", "und", "lin\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "KON", "ADJA", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Der andre rauh und steil.", "tokens": ["Der", "and\u00b7re", "rauh", "und", "steil", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "ADJD", "KON", "ADJD", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbade denn, Welt, geschwinde!", "tokens": ["\u00bb", "a\u00b7de", "denn", ",", "Welt", ",", "ge\u00b7schwin\u00b7de", "!"], "token_info": ["punct", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["$(", "ADV", "ADV", "$,", "NN", "$,", "ADJA", "$."], "meter": "+--+-+-", "measure": "iambic.tri.invert"}, "line.4": {"text": "Nun hab' ich freilich Eil'.\u00ab", "tokens": ["Nun", "hab'", "ich", "frei\u00b7lich", "Eil'", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.14": {"line.1": {"text": "\u00bbich seh' am Kreuz Ihn hangen,", "tokens": ["\u00bb", "ich", "seh'", "am", "Kreuz", "Ihn", "han\u00b7gen", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "APPRART", "NN", "PPER", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Er streckt die Arme Sein;", "tokens": ["Er", "streckt", "die", "Ar\u00b7me", "Sein", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Ich eil', Ihn zu umfangen,", "tokens": ["Ich", "eil'", ",", "Ihn", "zu", "um\u00b7fan\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Mit Schmerzen harrt Er mein.", "tokens": ["Mit", "Schmer\u00b7zen", "harrt", "Er", "mein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "VVFIN", "PPER", "PPOSAT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.15": {"line.1": {"text": "Willkomm, m\u00fchselig Ringen!", "tokens": ["Will\u00b7komm", ",", "m\u00fch\u00b7se\u00b7lig", "Rin\u00b7gen", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ADJD", "NN", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.2": {"text": "Du Pfad, so steil und schmal,", "tokens": ["Du", "Pfad", ",", "so", "steil", "und", "schmal", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "$,", "ADV", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Willst du zu Ihm mich bringen,", "tokens": ["Willst", "du", "zu", "Ihm", "mich", "brin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "APPR", "PPER", "PRF", "VVINF", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Dann Amen! Tausend Mal!\u00ab", "tokens": ["Dann", "A\u00b7men", "!", "Tau\u00b7send", "Mal", "!", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "NN", "$.", "CARD", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.16": {"line.1": {"text": "Es warten dein zwei Freier;", "tokens": ["Es", "war\u00b7ten", "dein", "zwei", "Frei\u00b7er", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "CARD", "NN", "$."], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Schau her und w\u00e4hle, Kind!", "tokens": ["Schau", "her", "und", "w\u00e4h\u00b7le", ",", "Kind", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["NN", "PTKVZ", "KON", "VVFIN", "$,", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "Nimm, den dein Herz getreuer", "tokens": ["Nimm", ",", "den", "dein", "Herz", "ge\u00b7treu\u00b7er"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["NE", "$,", "PRELS", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und sch\u00f6ner, reicher find't.", "tokens": ["Und", "sch\u00f6\u00b7ner", ",", "rei\u00b7cher", "find'", "t."], "token_info": ["word", "word", "punct", "word", "word", "abbreviation"], "pos": ["KON", "ADJD", "$,", "ADJD", "VVFIN", "NE"], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.17": {"line.1": {"text": "Der Erste ist ein K\u00f6nig,", "tokens": ["Der", "Ers\u00b7te", "ist", "ein", "K\u00f6\u00b7nig", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "VAFIN", "ART", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Ein F\u00fcrst von dieser Welt.", "tokens": ["Ein", "F\u00fcrst", "von", "die\u00b7ser", "Welt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PDAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bber bietet mir zu wenig,", "tokens": ["\u00bb", "er", "bie\u00b7tet", "mir", "zu", "we\u00b7nig", ","], "token_info": ["punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "VVFIN", "PPER", "PTKA", "PIS", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Nur eitel Pracht und Geld.\u00ab", "tokens": ["Nur", "ei\u00b7tel", "Pracht", "und", "Geld", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ADV", "ADJD", "NN", "KON", "NN", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.18": {"line.1": {"text": "Der Andre hat dort dr\u00fcben", "tokens": ["Der", "And\u00b7re", "hat", "dort", "dr\u00fc\u00b7ben"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "PIS", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Sein ewig K\u00f6nigreich.", "tokens": ["Sein", "e\u00b7wig", "K\u00f6\u00b7nig\u00b7reich", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJD", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "\u00bbja, diesen will ich lieben", "tokens": ["\u00bb", "ja", ",", "die\u00b7sen", "will", "ich", "lie\u00b7ben"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word"], "pos": ["$(", "PTKANT", "$,", "PDS", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Und mit ihm ziehen gleich.\u00ab", "tokens": ["Und", "mit", "ihm", "zie\u00b7hen", "gleich", ".", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "APPR", "PPER", "VVFIN", "ADV", "$.", "$("], "meter": "+--+-+", "measure": "iambic.tri.invert"}}, "stanza.19": {"line.1": {"text": "Der Erste will dir schenken", "tokens": ["Der", "Ers\u00b7te", "will", "dir", "schen\u00b7ken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VMFIN", "PPER", "VVINF"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "Viel Ehre, Schmuck und Reiz;", "tokens": ["Viel", "Eh\u00b7re", ",", "Schmuck", "und", "Reiz", ";"], "toke