{"textgrid.poem.32743": {"metadata": {"author": {"name": "Miller, Johann Martin", "birth": "N.A.", "death": "N.A."}, "title": "Das Grab", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Rings umher von Nacht umgeben,", "tokens": ["Rings", "um\u00b7her", "von", "Nacht", "um\u00b7ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "APPR", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Denk' ich deiner, o mein Grab!", "tokens": ["Denk'", "ich", "dei\u00b7ner", ",", "o", "mein", "Grab", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "$,", "FM", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Sonder Angst, und sonder Beben,", "tokens": ["Son\u00b7der", "Angst", ",", "und", "son\u00b7der", "Be\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "NN", "$,", "KON", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Schau' ich deine Kluft hinab.", "tokens": ["Schau'", "ich", "dei\u00b7ne", "Kluft", "hin\u00b7ab", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Also hier, in dieser Stille,", "tokens": ["Al\u00b7so", "hier", ",", "in", "die\u00b7ser", "Stil\u00b7le", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Soll einst dies Gebein vergehn?", "tokens": ["Soll", "einst", "dies", "Ge\u00b7bein", "ver\u00b7gehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "ADV", "PDS", "NN", "VVINF", "$."], "meter": "+---+-+", "measure": "dactylic.init"}, "line.7": {"text": "Hier soll dieses Geistes H\u00fclle", "tokens": ["Hier", "soll", "die\u00b7ses", "Geis\u00b7tes", "H\u00fcl\u00b7le"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "VMFIN", "PDAT", "NN", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Mit der Winde Hauch verwehn?", "tokens": ["Mit", "der", "Win\u00b7de", "Hauch", "ver\u00b7wehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "NN", "VVINF", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "O erheb auf ihrem Fl\u00fcgel", "tokens": ["O", "er\u00b7heb", "auf", "ih\u00b7rem", "Fl\u00fc\u00b7gel"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NE", "VVFIN", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dich vom Staub empor, mein Geist!", "tokens": ["Dich", "vom", "Staub", "em\u00b7por", ",", "mein", "Geist", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "APPRART", "NN", "PTKVZ", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Schwebe friedlich um den H\u00fcgel,", "tokens": ["Schwe\u00b7be", "fried\u00b7lich", "um", "den", "H\u00fc\u00b7gel", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Den der Tugend Ruh umfleu\u00dft.", "tokens": ["Den", "der", "Tu\u00b7gend", "Ruh", "um\u00b7fleu\u00dft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Keiner Wit