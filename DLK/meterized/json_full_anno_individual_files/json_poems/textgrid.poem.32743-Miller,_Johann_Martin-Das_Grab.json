text": "Nur f\u00fcr Vaterland und Pflicht.", "tokens": ["Nur", "f\u00fcr", "Va\u00b7ter\u00b7land", "und", "Pflicht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Fehler, die sich dir entschlichen,", "tokens": ["Feh\u00b7ler", ",", "die", "sich", "dir", "ent\u00b7schli\u00b7chen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "PRF", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Sind durch Reuethr\u00e4nen schon,", "tokens": ["Sind", "durch", "Reu\u00b7et\u00b7hr\u00e4\u00b7nen", "schon", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NN", "ADV", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Sind durch Jesus Blut erblichen,", "tokens": ["Sind", "durch", "Je\u00b7sus", "Blut", "er\u00b7bli\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "APPR", "NE", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Klagen nicht am Richterthron.", "tokens": ["Kla\u00b7gen", "nicht", "am", "Rich\u00b7ter\u00b7thron", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PTKNEG", "APPRART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "Aber eingeschleiert kommen", "tokens": ["A\u00b7ber", "ein\u00b7ge\u00b7schlei\u00b7ert", "kom\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Keusche M\u00e4dchen an die Gruft;", "tokens": ["Keu\u00b7sche", "M\u00e4d\u00b7chen", "an", "die", "Gruft", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Segensw\u00fcnsche f\u00fcr den Frommen", "tokens": ["Se\u00b7gens\u00b7w\u00fcn\u00b7sche", "f\u00fcr", "den", "From\u00b7men"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Beben heilig durch die Luft.", "tokens": ["Be\u00b7ben", "hei\u00b7lig", "durch", "die", "Luft", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Seelen, gut durch deine Lieder,", "tokens": ["See\u00b7len", ",", "gut", "durch", "dei\u00b7ne", "Lie\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "ADJD", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Bringen Blumenopfer dar,", "tokens": ["Brin\u00b7gen", "Blu\u00b7me\u00b7nop\u00b7fer", "dar", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "PTKVZ", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Dankesthr\u00e4nen fallen nieder,", "tokens": ["Dan\u00b7kest\u00b7hr\u00e4\u00b7nen", "fal\u00b7len", "nie\u00b7der", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PTKVZ", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Und der H\u00fcgel wird Altar.", "tokens": ["Und", "der", "H\u00fc\u00b7gel", "wird", "Al\u00b7tar", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.5": {"line.1": {"text": "Horch! Bekr\u00e4nzte Greise wallen", "tokens": ["Horch", "!", "Be\u00b7kr\u00e4nz\u00b7te", "Grei\u00b7se", "wal\u00b7len"], "token_info": ["word", "punct", "word", "word", "word"], "pos": ["NE", "$.", "ADJA", "NN", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Durch den d\u00fcstern Eibengang;", "tokens": ["Durch", "den", "d\u00fcs\u00b7tern", "Ei\u00b7ben\u00b7gang", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Hohe Harfenlieder schallen,", "tokens": ["Ho\u00b7he", "Har\u00b7fen\u00b7lie\u00b7der", "schal\u00b7len", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie der Engel Lobgesang. \u2013", "tokens": ["Wie", "der", "En\u00b7gel", "Lob\u00b7ge\u00b7sang", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "ART", "NN", "NN", "$.", "$("], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Gott! ach Gott! die Schar der Br\u00fcder!", "tokens": ["Gott", "!", "ach", "Gott", "!", "die", "Schar", "der", "Br\u00fc\u00b7der", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$.", "XY", "NN", "$.", "ART", "NN", "ART", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Ach, mein Herz! zu viel, mein Herz!", "tokens": ["Ach", ",", "mein", "Herz", "!", "zu", "viel", ",", "mein", "Herz", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "punct"], "pos": ["ITJ", "$,", "PPOSAT", "NN", "$.", "PTKA", "PIS", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.7": {"text": "Auf! und schwing in Thr\u00e4nen wieder", "tokens": ["Auf", "!", "und", "schwing", "in", "Thr\u00e4\u00b7nen", "wie\u00b7der"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "$.", "KON", "VVFIN", "APPR", "NN",