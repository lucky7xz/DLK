{"dta.poem.6779": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Gesicht.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wir k\u00f6nnen nimmermehr getreure H\u00fclff' erlangen,", "tokens": ["Wir", "k\u00f6n\u00b7nen", "nim\u00b7mer\u00b7mehr", "ge\u00b7treu\u00b7re", "H\u00fclff'", "er\u00b7lan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Als unsrer Augen H\u00fclff, des Firmamentes Prangen,", "tokens": ["Als", "uns\u00b7rer", "Au\u00b7gen", "H\u00fclff", ",", "des", "Fir\u00b7ma\u00b7men\u00b7tes", "Pran\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "NN", "$,", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Den wundervollen Lauff der Lichter seiner H\u00f6h\u2019n", "tokens": ["Den", "wun\u00b7der\u00b7vol\u00b7len", "Lauff", "der", "Lich\u00b7ter", "sei\u00b7ner", "H\u00f6h'n"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "ART", "NN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Zu \u00fcberlegen, zu besehn.", "tokens": ["Zu", "\u00fc\u00b7berl\u00b7e\u00b7gen", ",", "zu", "be\u00b7sehn", "."], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Allein kan man der Tieffe Gr\u00f6ss\u2019 erblicken,", "tokens": ["Al\u00b7lein", "kan", "man", "der", "Tief\u00b7fe", "Gr\u00f6ss'", "er\u00b7bli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PIS", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "--+--+-+-+-", "measure": "anapaest.di.plus"}, "line.6": {"text": "Und ihrer Kugeln Glantz und Wunderw\u00fcrdigkeit?", "tokens": ["Und", "ih\u00b7rer", "Ku\u00b7geln", "Glantz", "und", "Wun\u00b7der\u00b7w\u00fcr\u00b7dig\u00b7keit", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Sie werden das Gesicht, so sich verirrt, ber\u00fccken;", "tokens": ["Sie", "wer\u00b7den", "das", "Ge\u00b7sicht", ",", "so", "sich", "ver\u00b7irrt", ",", "be\u00b7r\u00fc\u00b7cken", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "$,", "ADV", "PRF", "VVPP", "$,", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Wo uns nicht die Vernunfft getreue Regeln beut.", "tokens": ["Wo", "uns", "nicht", "die", "Ver\u00b7nunfft", "ge\u00b7treu\u00b7e", "Re\u00b7geln", "beut", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "PTKNEG", "ART", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "Wann, fern vom Horizont, der Mond sich aufwerts f\u00fchret,", "tokens": ["Wann", ",", "fern", "vom", "Ho\u00b7ri\u00b7zont", ",", "der", "Mond", "sich", "auf\u00b7werts", "f\u00fch\u00b7ret", ","], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "$,", "ADJD", "APPRART", "NN", "$,", "ART", "NN", "PRF", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Und in den blauen Lufft-Creis steigt;", "tokens": ["Und", "in", "den", "blau\u00b7en", "Lufft\u00b7Creis", "steigt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "So scheint er, als ob er den Baum, das Haus ber\u00fchret,", "tokens": ["So", "scheint", "er", ",", "als", "ob", "er", "den", "Baum", ",", "das", "Haus", "be\u00b7r\u00fch\u00b7ret", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOKOM", "KOUS", "PPER", "ART", "NN", "$,", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Von welchem er sich gantz nicht abgesondert zeigt.", "tokens": ["Von", "wel\u00b7chem", "er", "sich", "gantz", "nicht", "ab\u00b7ge\u00b7son\u00b7dert", "zeigt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PRELS", "PPER", "PRF", "ADV", "PTKNEG", "VVPP", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}