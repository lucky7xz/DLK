{"textgrid.poem.60200": {"metadata": {"author": {"name": "Karsch, Anna Louisa", "birth": "N.A.", "death": "N.A."}, "title": "1L: L\u00e4\u00dft die Natur aus ihrer Hand", "genre": "verse", "period": "N.A.", "pub_year": 1762, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "L\u00e4\u00dft die Natur aus ihrer Hand", "tokens": ["L\u00e4\u00dft", "die", "Na\u00b7tur", "aus", "ih\u00b7rer", "Hand"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Erobrer gehn, o dann bebt schauervoll die Erde", "tokens": ["E\u00b7rob\u00b7rer", "gehn", ",", "o", "dann", "bebt", "schau\u00b7er\u00b7voll", "die", "Er\u00b7de"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "$,", "FM", "ADV", "VVFIN", "ADJD", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Erwartend, da\u00df auf manches Land", "tokens": ["Er\u00b7war\u00b7tend", ",", "da\u00df", "auf", "man\u00b7ches", "Land"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["VVPP", "$,", "KOUS", "APPR", "PIAT", "NN"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.4": {"text": "Tod und Verw\u00fcstung kommen werde!", "tokens": ["Tod", "und", "Ver\u00b7w\u00fcs\u00b7tung", "kom\u00b7men", "wer\u00b7de", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "VVINF", "VAFIN", "$."], "meter": "+--+-+-+-", "measure": "iambic.tetra.invert"}}, "stanza.2": {"line.1": {"text": "Wenn ein zuk\u00fcnftiger Tyrann", "tokens": ["Wenn", "ein", "zu\u00b7k\u00fcnf\u00b7ti\u00b7ger", "Ty\u00b7rann"], "token_info": ["word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Grimm aus dem Auge weint, das kaum sich aufgeschlossen", "tokens": ["Grimm", "aus", "dem", "Au\u00b7ge", "weint", ",", "das", "kaum", "sich", "auf\u00b7ge\u00b7schlos\u00b7sen"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "APPR", "ART", "NN", "VVFIN", "$,", "PRELS", "ADV", "PRF", "VVINF"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.3": {"text": "Dann sehen Engel weinend an", "tokens": ["Dann", "se\u00b7hen", "En\u00b7gel", "wei\u00b7nend", "an"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADJA", "NN", "VVPP", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der H\u00f6lle jungen Bundsgenossen.", "tokens": ["Der", "H\u00f6l\u00b7le", "jun\u00b7gen", "Bunds\u00b7ge\u00b7nos\u00b7sen", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Der Sonnen Antlitz wird entf\u00e4rbt", "tokens": ["Der", "Son\u00b7nen", "Ant\u00b7litz", "wird", "ent\u00b7f\u00e4rbt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "NN", "VAFIN", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Wenn sie den Heuchler sieht, dem Gift im Blute schleichet", "tokens": ["Wenn", "sie", "den", "Heuch\u00b7ler", "sieht", ",", "dem", "Gift", "im", "Blu\u00b7te", "schlei\u00b7chet"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "NN", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Der k\u00fcnftig mit dem Hauch verderbt,", "tokens": ["Der", "k\u00fcnf\u00b7tig", "mit", "dem", "Hauch", "ver\u00b7derbt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "APPR", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wenn er als Freund die H\u00e4nde reichet.", "tokens": ["Wenn", "er", "als", "Freund", "die", "H\u00e4n\u00b7de", "rei\u00b7chet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "KOUS", "NN", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Bey der Geburt des Wuchrers lacht", "tokens": ["Bey", "der", "Ge\u00b7burt", "des", "Wuc\u00b7hrers", "lacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "ART", "NN", "VVFIN"], "meter": "+--+-+-+", "measure": "iambic.tetra.invert"}, "line.2": {"text": "Der Geiz, und schreckt mit Hohn die Wollust von der Wiege", "tokens": ["Der", "Geiz", ",", "und", "schreckt", "mit", "Hohn", "die", "Wol\u00b7lust", "von", "der", "Wie\u00b7ge"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "$,", "KON", "VVFIN", "APPR", "NN", "ART", "NN", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und giebt mit schielen Blicken acht", "tokens": ["Und", "giebt", "mit", "schie\u00b7len", "Bli\u00b7cken", "acht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "APPR", "ADJA", "NN", "CARD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo Gold f\u00fcr seine H\u00e4nde liege?", "tokens": ["Wo", "Gold", "f\u00fcr", "sei\u00b7ne", "H\u00e4n\u00b7de", "lie\u00b7ge", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPR", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Den Dummkopf dr\u00fcckt die Tr\u00e4gheit an", "tokens": ["Den", "Dumm\u00b7kopf", "dr\u00fcckt", "die", "Tr\u00e4g\u00b7heit", "an"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ART", "NN", "APPR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit weichem Arm und spricht bey seiner ersten Thr\u00e4ne:", "tokens": ["Mit", "wei\u00b7chem", "Arm", "und", "spricht", "bey", "sei\u00b7ner", "ers\u00b7ten", "Thr\u00e4\u00b7ne", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "KON", "VVFIN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Sey ruhig werd ein fetter Mann,", "tokens": ["Sey", "ru\u00b7hig", "werd", "ein", "fet\u00b7ter", "Mann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und \u00fcber Gl\u00fcck und Ungl\u00fcck g\u00e4hne!", "tokens": ["Und", "\u00fc\u00b7ber", "Gl\u00fcck", "und", "Un\u00b7gl\u00fcck", "g\u00e4h\u00b7ne", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Der Neidische kommt auf die Welt", "tokens": ["Der", "Nei\u00b7di\u00b7sche", "kommt", "auf", "die", "Welt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "APPR", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Mit Blicken um sich her als wollt er trotzig wissen:", "tokens": ["Mit", "Bli\u00b7cken", "um", "sich", "her", "als", "wollt", "er", "trot\u00b7zig", "wis\u00b7sen", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "NN", "APPR", "PRF", "APZR", "KOUS", "VMFIN", "PPER", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Warums der Mutter noch gef\u00e4llt", "tokens": ["Wa\u00b7rums", "der", "Mut\u00b7ter", "noch", "ge\u00b7f\u00e4llt"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "ART", "NN", "ADV", "VVPP"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Vater mehr als ihn zu k\u00fcssen!", "tokens": ["Den", "Va\u00b7ter", "mehr", "als", "ihn", "zu", "k\u00fcs\u00b7sen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "KOKOM", "PPER", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "O Muse, frag die Gottheit nicht", "tokens": ["O", "Mu\u00b7se", ",", "frag", "die", "Got\u00b7theit", "nicht"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "NN", "$,", "VVFIN", "ART", "NN", "PTKNEG"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Warum sie alle die herab zur Erde schickte", "tokens": ["Wa\u00b7rum", "sie", "al\u00b7le", "die", "her\u00b7ab", "zur", "Er\u00b7de", "schick\u00b7te"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "PPER", "PIS", "ART", "ADV", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Nein singe nur: Wem Sonnen-Licht", "tokens": ["Nein", "sin\u00b7ge", "nur", ":", "Wem", "Son\u00b7nen\u00b7Licht"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["PTKANT", "VVFIN", "ADV", "$.", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Der Tugend, aus den Augen blickte!", "tokens": ["Der", "Tu\u00b7gend", ",", "aus", "den", "Au\u00b7gen", "blick\u00b7te", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.8": {"line.1": {"text": "Die Luft ward harten Eises Zwang,", "tokens": ["Die", "Luft", "ward", "har\u00b7ten", "Ei\u00b7ses", "Zwang", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VAFIN", "ADJA", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Der Winter schickte sich dem Fr\u00fchling auszuweichen,", "tokens": ["Der", "Win\u00b7ter", "schick\u00b7te", "sich", "dem", "Fr\u00fch\u00b7ling", "aus\u00b7zu\u00b7wei\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "PRF", "ART", "NN", "VVIZU", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Da Spiegel der Natur entdrang", "tokens": ["Da", "Spie\u00b7gel", "der", "Na\u00b7tur", "ent\u00b7drang"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "ART", "NN", "APPO"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Um ihr an G\u00fctigkeit zu gleichen!", "tokens": ["Um", "ihr", "an", "G\u00fc\u00b7tig\u00b7keit", "zu", "glei\u00b7chen", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPER", "APPR", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.9": {"line.1": {"text": "Ganz sanft war er gemacht von ihr,", "tokens": ["Ganz", "sanft", "war", "er", "ge\u00b7macht", "von", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "PPER", "VVPP", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Sein Schutzgeist l\u00e4chelte lobsprechendes Vergn\u00fcgen", "tokens": ["Sein", "Schutz\u00b7geist", "l\u00e4\u00b7chel\u00b7te", "lob\u00b7spre\u00b7chen\u00b7des", "Ver\u00b7gn\u00fc\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "NN", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Er sahe besser noch, als wir", "tokens": ["Er", "sa\u00b7he", "bes\u00b7ser", "noch", ",", "als", "wir"], "token_info": ["word", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VVFIN", "ADJD", "ADV", "$,", "KOUS", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Den Menschen-Freund in allen Z\u00fcgen!", "tokens": ["Den", "Men\u00b7schen\u00b7Freund", "in", "al\u00b7len", "Z\u00fc\u00b7gen", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}, "stanza.10": {"line.1": {"text": "So wie er ist, lie\u00df ihn hervor", "tokens": ["So", "wie", "er", "ist", ",", "lie\u00df", "ihn", "her\u00b7vor"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["ADV", "KOKOM", "PPER", "VAFIN", "$,", "VVFIN", "PPER", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die fr\u00f6liche Natur aus ihren Meister H\u00e4nden", "tokens": ["Die", "fr\u00f6\u00b7li\u00b7che", "Na\u00b7tur", "aus", "ih\u00b7ren", "Meis\u00b7ter", "H\u00e4n\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "APPR", "PPOSAT", "NN", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Und sagte: sein gef\u00e4llig Ohr", "tokens": ["Und", "sag\u00b7te", ":", "sein", "ge\u00b7f\u00e4l\u00b7lig", "Ohr"], "token_info": ["word", "word", "punct", "word", "word", "word"], "pos": ["KON", "VVFIN", "$.", "PPOSAT", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wird sich zur Freundes Muse wenden.", "tokens": ["Wird", "sich", "zur", 