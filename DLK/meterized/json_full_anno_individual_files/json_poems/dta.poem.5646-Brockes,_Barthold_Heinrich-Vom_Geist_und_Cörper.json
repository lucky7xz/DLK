{"dta.poem.5646": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Vom Geist und C\u00f6rper", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Aus dieser Gleichheit nun entsteht", "tokens": ["Aus", "die\u00b7ser", "Gleich\u00b7heit", "nun", "ent\u00b7steht"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "PDAT", "NN", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Die Meinung, welche falsch, wodurch man gr\u00f6blich irret,", "tokens": ["Die", "Mei\u00b7nung", ",", "wel\u00b7che", "falsch", ",", "wo\u00b7durch", "man", "gr\u00f6b\u00b7lich", "ir\u00b7ret", ","], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADJD", "$,", "PWAV", "PIS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Die Stoff und Geist vermischet und verwirret.", "tokens": ["Die", "Stoff", "und", "Geist", "ver\u00b7mi\u00b7schet", "und", "ver\u00b7wir\u00b7ret", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KON", "NN", "VVFIN", "KON", "VVFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Wie man sich nie erinnern kann,", "tokens": ["Wie", "man", "sich", "nie", "e\u00b7rin\u00b7nern", "kann", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "PRF", "ADV", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Da\u00df sonder C\u00f6rper je die Seele was gethan;", "tokens": ["Da\u00df", "son\u00b7der", "C\u00f6r\u00b7per", "je", "die", "See\u00b7le", "was", "ge\u00b7than", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "ADV", "ART", "NN", "PIS", "VVPP", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "So glanben wir, man k\u00f6nne sie nicht scheiden.", "tokens": ["So", "glan\u00b7ben", "wir", ",", "man", "k\u00f6n\u00b7ne", "sie", "nicht", "schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "PIS", "VMFIN", "PPER", "PTKNEG", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Es ist der Jrrthum fast nicht zu vermeiden,", "tokens": ["Es", "ist", "der", "Jrr\u00b7thum", "fast", "nicht", "zu", "ver\u00b7mei\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "ADV", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Zu glauben, da\u00df sie einerley,", "tokens": ["Zu", "glau\u00b7ben", ",", "da\u00df", "sie", "ei\u00b7ner\u00b7ley", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PTKZU", "VVINF", "$,", "KOUS", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.9": {"text": "Und eine wie die andre sey.", "tokens": ["Und", "ei\u00b7ne", "wie", "die", "and\u00b7re", "sey", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIS", "KOKOM", "ART", "PIS", "VAFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.10": {"text": "Der Geist, statt da\u00df er sich,", "tokens": ["Der", "Geist", ",", "statt", "da\u00df", "er", "sich", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "KOUI", "KOUS", "PPER", "PRF", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.11": {"text": "Wenn er recht handeln wollte,", "tokens": ["Wenn", "er", "recht", "han\u00b7deln", "woll\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADJD", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.12": {"text": "Vom C\u00f6rper unterscheiden sollte;", "tokens": ["Vom", "C\u00f6r\u00b7per", "un\u00b7ter\u00b7schei\u00b7den", "soll\u00b7te", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.13": {"text": "Macht sich selbst c\u00f6rperlich.", "tokens": ["Macht", "sich", "selbst", "c\u00f6r\u00b7per\u00b7lich", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "PRF", "ADV", "ADJD", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}, "line.14": {"text": "Elendes Vorurtheil! so wir in unsrer Wiegen,", "tokens": ["E\u00b7len\u00b7des", "Vor\u00b7urt\u00b7heil", "!", "so", "wir", "in", "uns\u00b7rer", "Wie\u00b7gen", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$.", "ADV", "PPER", "APPR", "PPOSAT", "NN", "$,"], "meter": "+--+-+-+-+-+-", "measure": "iambic.hexa.invert"}, "line.15": {"text": "Mit einem schwachen Leib von Jugend auf schon f\u00fcgen,", "tokens": ["Mit", "ei\u00b7nem", "schwa\u00b7chen", "Leib", "von", "Ju\u00b7gend", "auf", "schon", "f\u00fc\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "APPR", "NN", "APPR", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.16": {"text": "Zur steten Nahrungs-Sorg\u2019 ohn Aufschub angetrieben.", "tokens": ["Zur", "ste\u00b7ten", "Nah\u00b7rungs\u00b7Sor\u00b7g'", "ohn", "Auf\u00b7schub", "an\u00b7ge\u00b7trie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+--+-+-+-", "measure": "iambic.hexa.relaxed"}, "line.17": {"text": "Man sieht, da\u00df Seel und Stoff bey uns vermischet seyn.", "tokens": ["Man", "sieht", ",", "da\u00df", "Seel", "und", "Stoff", "bey", "uns", "ver\u00b7mi\u00b7schet", "seyn", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PIS", "VVFIN", "$,", "KOUS", "NN", "KON", "NN", "APPR", "PPER", "VVPP", "VAINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.18": {"text": "Dem C\u00f6rper, welcher doch ein Werckzeug nur allein,", "tokens": ["Dem", "C\u00f6r\u00b7per", ",", "wel\u00b7cher", "doch", "ein", "Wer\u00b7ck\u00b7zeug", "nur", "al\u00b7lein", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "ADV", "ART", "NN", "ADV", "ADV", "$,"], "meter": "-+-+-+-+-+--+", "measure": "iambic.hexa.chol.strict"}, "line.19": {"text": "Wird die Empfindungs-Krafft mit Unrecht zugeschrieben.", "tokens": ["Wird", "die", "Emp\u00b7fin\u00b7dungs\u00b7Krafft", "mit", "Un\u00b7recht", "zu\u00b7ge\u00b7schrie\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "NN", "APPR", "NN", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}