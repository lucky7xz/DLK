{"textgrid.poem.56956": {"metadata": {"author": {"name": "Morgenstern, Christian", "birth": "N.A.", "death": "N.A."}, "title": "1L: Ich m\u00f6chte eine Geige haben,", "genre": "verse", "period": "N.A.", "pub_year": 1892, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich m\u00f6chte eine Geige haben,", "tokens": ["Ich", "m\u00f6ch\u00b7te", "ei\u00b7ne", "Gei\u00b7ge", "ha\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VAFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "so ganz f\u00fcr mich allein,", "tokens": ["so", "ganz", "f\u00fcr", "mich", "al\u00b7lein", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "APPR", "PPER", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "da spielt ich all meine Schmerzen", "tokens": ["da", "spielt", "ich", "all", "mei\u00b7ne", "Schmer\u00b7zen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "PIAT", "PPOSAT", "NN"], "meter": "-+-++-+-", "measure": "unknown.measure.tetra"}, "line.4": {"text": "und all meine Lust hinein.", "tokens": ["und", "all", "mei\u00b7ne", "Lust", "hin\u00b7ein", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PIAT", "PPOSAT", "NN", "PTKVZ", "$."], "meter": "-+--+-+", "measure": "iambic.tri.relaxed"}}, "stanza.2": {"line.1": {"text": "Denn ach, ihr lieben Leute,", "tokens": ["Denn", "ach", ",", "ihr", "lie\u00b7ben", "Leu\u00b7te", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "XY", "$,", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.2": {"text": "ihr wi\u00dft nicht, was geigen hei\u00dft,", "tokens": ["ihr", "wi\u00dft", "nicht", ",", "was", "gei\u00b7gen", "hei\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$,", "PWS", "ADJA", "VVFIN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "ihr habt wohl flei\u00dfige Finger,", "tokens": ["ihr", "habt", "wohl", "flei\u00b7\u00dfi\u00b7ge", "Fin\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ADJA", "NN", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "doch nicht den heiligen Geist.", "tokens": ["doch", "nicht", "den", "hei\u00b7li\u00b7gen", "Geist", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PTKNEG", "ART", "ADJA", "NN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}}, "stanza.3": {"line.1": {"text": "Ich h\u00f6re die Welten singen,", "tokens": ["Ich", "h\u00f6\u00b7re", "die", "Wel\u00b7ten", "sin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "VVFIN", "$,"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "wenn er mein Haupt durchweht \u2013", "tokens": ["wenn", "er", "mein", "Haupt", "durch\u00b7weht", "\u2013"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PPOSAT", "NN", "VVFIN", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.3": {"text": "doch ach, ich hab keine Geige,", "tokens": ["doch", "ach", ",", "ich", "hab", "kei\u00b7ne", "Gei\u00b7ge", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "$,", "PPER", "VAFIN", "PIAT", "NN", "$,"], "meter": "+---+-+-", "measure": "dactylic.init"}, "line.4": {"text": "ich bin nur ein armer Poet.", "tokens": ["ich", "bin", "nur", "ein", "ar\u00b7mer", "Po\u00b7et", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA