{"textgrid.poem.61221": {"metadata": {"author": {"name": "Geibel, Emanuel", "birth": "N.A.", "death": "N.A."}, "title": "Sch\u00f6ne Tage", "genre": "verse", "period": "N.A.", "pub_year": 1833, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "O wie segn' ich euch, ihr Tage,", "tokens": ["O", "wie", "segn'", "ich", "euch", ",", "ihr", "Ta\u00b7ge", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PPER", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Die ihr reich und reicher bl\u00fchend", "tokens": ["Die", "ihr", "reich", "und", "rei\u00b7cher", "bl\u00fc\u00b7hend"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "PPER", "ADJD", "KON", "ADJD", "VVPP"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Still durch Hain und Garten wandelt!", "tokens": ["Still", "durch", "Hain", "und", "Gar\u00b7ten", "wan\u00b7delt", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "APPR", "NN", "KON", "NN", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "O wie segn' ich euch, ihr blauen,", "tokens": ["O", "wie", "segn'", "ich", "euch", ",", "ihr", "blau\u00b7en", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PPER", "$,", "PPOSAT", "ADJA", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Duft'gen, tiefgestirnten N\u00e4chte!", "tokens": ["Duft'\u00b7gen", ",", "tief\u00b7ges\u00b7tirn\u00b7ten", "N\u00e4ch\u00b7te", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "O wie segn' ich dich, o Erde,", "tokens": ["O", "wie", "segn'", "ich", "dich", ",", "o", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["NE", "KOKOM", "VAFIN", "PPER", "PRF", "$,", "FM", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.7": {"text": "Die zu solchem Gl\u00fcck mich n\u00e4hrte,", "tokens": ["Die", "zu", "sol\u00b7chem", "Gl\u00fcck", "mich", "n\u00e4hr\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "NN", "PPER", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Dich, o Himmel, den ich atme!", "tokens": ["Dich", ",", "o", "Him\u00b7mel", ",", "den", "ich", "at\u00b7me", "!"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "$,", "FM", "NN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Ach, schon w\u00e4hnt' ich fast erkaltet", "tokens": ["Ach", ",", "schon", "w\u00e4hnt'", "ich", "fast", "er\u00b7kal\u00b7tet"], "token_info": ["word", "punct", "word", "word", "word", "word", "word"], "pos": ["ITJ", "$,", "ADV", "VVFIN", "PPER", "ADV", "VVFIN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Dieses Herz und wollte m\u00e4nnlich", "tokens": ["Die\u00b7ses", "Herz", "und", "woll\u00b7te", "m\u00e4nn\u00b7lich"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PDAT", "NN", "KON", "VMFIN", "ADJD"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "Mit dem schwer erkauften Schatze,", "tokens": ["Mit", "dem", "schwer", "er\u00b7kauf\u00b7ten", "Schat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit der Weisheit, mich bescheiden.", "tokens": ["Mit", "der", "Weis\u00b7heit", ",", "mich", "be\u00b7schei\u00b7den", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "$,", "PPER", "VVPP", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Seht, da bringt ihr, wie des Fr\u00fchlings", "tokens": ["Seht", ",", "da", "bringt", "ihr", ",", "wie", "des", "Fr\u00fch\u00b7lings"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "$,", "ADV", "VVFIN", "PPER", "$,", "PWAV", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Milde Sonne rosig aufgl\u00fcht,", "tokens": ["Mil\u00b7de", "Son\u00b7ne", "ro\u00b7sig", "auf\u00b7gl\u00fcht", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "ADJD", "VVPP", "$,"], "meter": "+-+-+--+", "measure": "iambic.tetra.chol"}, "line.7": {"text": "Bringt noch einmal mit den Blumen", "tokens": ["Bringt", "noch", "ein\u00b7mal", "mit", "den", "Blu\u00b7men"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "ADV", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.8": {"text": "Alle F\u00fcllen der Empfindung,", "tokens": ["Al\u00b7le", "F\u00fcl\u00b7len", "der", "Emp\u00b7fin\u00b7dung", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.9": {"text": "Hei\u00dfe Tr\u00e4nen, junge Lieder;", "tokens": ["Hei\u00b7\u00dfe", "Tr\u00e4\u00b7nen", ",", "jun\u00b7ge", "Lie\u00b7der", ";"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.10": {"text": "Und, mir selbst ein selig Wunder,", "tokens": ["Und", ",", "mir", "selbst", "ein", "se\u00b7lig", "Wun\u00b7der", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "$,", "PPER", "ADV", "ART", "ADJD", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.11": {"text": "Wieder leb' ich Liebesleben.", "tokens": ["Wie\u00b7der", "leb'", "ich", "Lie\u00b7bes\u00b7le\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "NN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Wenn ich Gl\u00fccklicher nun abends", "tokens": ["Wenn", "ich", "Gl\u00fcck\u00b7li\u00b7cher", "nun", "a\u00b7bends"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "NN", "ADV", "ADV"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Arm in Arm mit der Geliebten", "tokens": ["Arm", "in", "Arm", "mit", "der", "Ge\u00b7lieb\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["NN", "APPR", "NN", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.3": {"text": "\u00dcber stille Felder schreite,", "tokens": ["\u00dc\u00b7ber", "stil\u00b7le", "Fel\u00b7der", "schrei\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Da\u00df der Halbmond hold verschlungen", "tokens": ["Da\u00df", "der", "Halb\u00b7mond", "hold", "ver\u00b7schlun\u00b7gen"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "NN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.5": {"text": "Unser Bild am Boden schattet,", "tokens": ["Un\u00b7ser", "Bild", "am", "Bo\u00b7den", "schat\u00b7tet", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPRART", "NN", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.6": {"text": "Wenn wir dann am Wald uns 