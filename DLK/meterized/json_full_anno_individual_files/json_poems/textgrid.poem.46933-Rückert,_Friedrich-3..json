{"textgrid.poem.46933": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "3.", "genre": "verse", "period": "N.A.", "pub_year": 1827, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Was schmiedst du, Schmied? \u00bbWir schmieden Ketten, Ketten!\u00ab", "tokens": ["Was", "schmiedst", "du", ",", "Schmied", "?", "\u00bb", "Wir", "schmie\u00b7den", "Ket\u00b7ten", ",", "Ket\u00b7ten", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "$(", "PPER", "ADJA", "NN", "$,", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ach, in die Ketten seid ihr selbst geschlagen.", "tokens": ["Ach", ",", "in", "die", "Ket\u00b7ten", "seid", "ihr", "selbst", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$,", "APPR", "ART", "NN", "VAFIN", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Was pfl\u00fcgst du, Bau'r? \u00bbDas Feld soll Fr\u00fcchte tragen!\u00ab", "tokens": ["Was", "pfl\u00fcgst", "du", ",", "Bau'r", "?", "\u00bb", "Das", "Feld", "soll", "Fr\u00fcch\u00b7te", "tra\u00b7gen", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "$(", "ART", "NN", "VMFIN", "NN", "VVINF", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ja f\u00fcr den Feind die Saat, f\u00fcr dich die Kletten.", "tokens": ["Ja", "f\u00fcr", "den", "Feind", "die", "Saat", ",", "f\u00fcr", "dich", "die", "Klet\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "APPR", "ART", "NN", "ART", "NN", "$,", "APPR", "PPER", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.5": {"text": "Was zielst du, Sch\u00fctze? \u00bbTod dem Hirsch, dem fetten.\u00ab", "tokens": ["Was", "zielst", "du", ",", "Sch\u00fct\u00b7ze", "?", "\u00bb", "Tod", "dem", "Hirsch", ",", "dem", "fet\u00b7ten", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "$(", "NN", "ART", "NN", "$,", "ART", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.6": {"text": "Gleich Hirsch und Reh wird man euch selber jagen.", "tokens": ["Gleich", "Hirsch", "und", "Reh", "wird", "man", "euch", "sel\u00b7ber", "ja\u00b7gen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "KON", "NN", "VAFIN", "PIS", "PPER", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.7": {"text": "Was strickst du, Fischer? \u00bbNetz dem Fisch, dem zagen.\u00ab", "tokens": ["Was", "strickst", "du", ",", "Fi\u00b7scher", "?", "\u00bb", "Netz", "dem", "Fisch", ",", "dem", "za\u00b7gen", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "punct", "punct", "word", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "NN", "$.", "$(", "NN", "ART", "NN", "$,", "ART", "ADJA", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "Aus eurem Todesnetz wer kann euch retten?", "tokens": ["Aus", "eu\u00b7rem", "To\u00b7des\u00b7netz", "wer", "kann", "euch", "ret\u00b7ten", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "PWS", "VMFIN", "PPER", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.9": {"text": "Was wiegest du, schlaflose Mutter? \u00bbKnaben.\u00ab", "tokens": ["Was", "wie\u00b7gest", "du", ",", "schlaf\u00b7lo\u00b7se", "Mut\u00b7ter", "?", "\u00bb", "Kna\u00b7ben", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "punct", "word", "punct", "punct"], "pos": ["PWS", "VVFIN", "PPER", "$,", "ADJA", "NN", "$.", "$(", "NN", "$.", "$("], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.10": {"text": "Ja, da\u00df sie wachsen und dem Vaterlande,", "tokens": ["Ja", ",", "da\u00df", "sie", "wach\u00b7sen", "und", "dem", "Va\u00b7ter\u00b7lan\u00b7de", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PTKANT", "$,", "KOUS", "PPER", "VVINF", "KON", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Im Dienst des Feindes, Wunden schlagen sollen.", "tokens": ["Im", "Dienst", "des", "Fein\u00b7des", ",", "Wun\u00b7den", "schla\u00b7gen", "sol\u00b7len", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "ART", "NN", "$,", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.12": {"text": "Was schreibest Dichter du? \u00bbIn Glutbuchstaben", "tokens": ["Was", "schrei\u00b7best", "Dich\u00b7ter", "du", "?", "\u00bb", "In", "Glut\u00b7buch\u00b7sta\u00b7ben"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word"], "pos": ["PWS", "VVFIN", "NN", "PPER", "$.", "$(", "APPR", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.13": {"text": "Einschreib' ich mein und meines Volkes Schande,", "tokens": ["Ein\u00b7schreib'", "ich", "mein", "und", "mei\u00b7nes", "Vol\u00b7kes", "Schan\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "PPOSAT", "K