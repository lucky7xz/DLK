{"dta.poem.20758": {"metadata": {"author": {"name": "Karsch, Anna Luise", "birth": "N.A.", "death": "N.A."}, "title": "Guter Rath  \n wider das Aergerni\u00df \u00fcber die Thorheit Anderer.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1792", "urn": "urn:nbn:de:kobv:b4-200905193016", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Um deinem N\u00e4chsten zu verzeihn,               ", "tokens": ["Um", "dei\u00b7nem", "N\u00e4chs\u00b7ten", "zu", "ver\u00b7zeihn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUI", "PPOSAT", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und seiner Thorheit nicht zu fluchen,", "tokens": ["Und", "sei\u00b7ner", "Thor\u00b7heit", "nicht", "zu", "flu\u00b7chen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "NN", "PTKNEG", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Mu\u00dft du die Sache selbst versuchen.", "tokens": ["Mu\u00dft", "du", "die", "Sa\u00b7che", "selbst", "ver\u00b7su\u00b7chen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VMFIN", "PPER", "ART", "NN", "ADV", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Trink einmal \u00fcberm\u00e4\u00dfig Wein,", "tokens": ["Trink", "ein\u00b7mal", "\u00fc\u00b7berm\u00b7\u00e4\u00b7\u00dfig", "Wein", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Verliebe dich einmal, spiel einmal in der Karte,", "tokens": ["Ver\u00b7lie\u00b7be", "dich", "ein\u00b7mal", ",", "spiel", "ein\u00b7mal", "in", "der", "Kar\u00b7te", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADV", "$,", "VVFIN", "ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Und dann bekenne mi\u00dfvergn\u00fcgt:", "tokens": ["Und", "dann", "be\u00b7ken\u00b7ne", "mi\u00df\u00b7ver\u00b7gn\u00fcgt", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da\u00df tief in uns verh\u00fcllt der Thorheit Saame liegt,", "tokens": ["Da\u00df", "tief", "in", "uns", "ver\u00b7h\u00fcllt", "der", "Thor\u00b7heit", "Saa\u00b7me", "liegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "APPR", "PPER", "VVFIN", "ART", "NN", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Und auf Gelegenheit nur warte.", "tokens": ["Und", "auf", "Ge\u00b7le\u00b7gen\u00b7heit", "nur", "war\u00b7te", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "NN", "ADV", "VVFIN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}}}}}