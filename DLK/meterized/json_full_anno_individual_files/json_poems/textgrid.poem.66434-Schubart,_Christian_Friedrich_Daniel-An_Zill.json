{"textgrid.poem.66434": {"metadata": {"author": {"name": "Schubart, Christian Friedrich Daniel", "birth": "N.A.", "death": "N.A."}, "title": "An Zill", "genre": "verse", "period": "N.A.", "pub_year": 1775, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Zill, der Apokalyptikus,", "tokens": ["Zill", ",", "der", "A\u00b7pok\u00b7a\u00b7lyp\u00b7ti\u00b7kus", ","], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NE", "$,", "ART", "NN", "$,"], "meter": "+-+--+-+", "measure": "glykoneus"}, "line.2": {"text": "Bewies mit einem tapfern Schlu\u00df,", "tokens": ["Be\u00b7wies", "mit", "ei\u00b7nem", "tap\u00b7fern", "Schlu\u00df", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Da\u00df einstens mit den Frommen", "tokens": ["Da\u00df", "eins\u00b7tens", "mit", "den", "From\u00b7men"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ADV", "APPR", "ART", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "Auch Thiere in den Himmel kommen.", "tokens": ["Auch", "Thie\u00b7re", "in", "den", "Him\u00b7mel", "kom\u00b7men", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "NN", "APPR", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.5": {"text": "O, schrie sein altes Weib, und freut sich inniglich,", "tokens": ["O", ",", "schrie", "sein", "al\u00b7tes", "Weib", ",", "und", "freut", "sich", "in\u00b7nig\u00b7lich", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NE", "$,", "VVFIN", "PPOSAT", "ADJA", "NN", "$,", "KON", "VVFIN", "PRF", "ADJD", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "O welch ein gro