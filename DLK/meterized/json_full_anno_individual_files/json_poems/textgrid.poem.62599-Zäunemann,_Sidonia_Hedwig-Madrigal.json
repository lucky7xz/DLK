{"textgrid.poem.62599": {"metadata": {"author": {"name": "Z\u00e4unemann, Sidonia Hedwig", "birth": "N.A.", "death": "N.A."}, "title": "Madrigal", "genre": "verse", "period": "N.A.", "pub_year": 1727, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ein Fisch, der an der Angel h\u00e4ngt,", "tokens": ["Ein", "Fisch", ",", "der", "an", "der", "An\u00b7gel", "h\u00e4ngt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Je mehr derselbige sich loszureissen denkt,", "tokens": ["Je", "mehr", "der\u00b7sel\u00b7bi\u00b7ge", "sich", "los\u00b7zu\u00b7reis\u00b7sen", "denkt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PDS", "PRF", "VVIZU", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Jemehr wird er verletzet und geritzt.", "tokens": ["Je\u00b7mehr", "wird", "er", "ver\u00b7let\u00b7zet", "und", "ge\u00b7ritzt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "VVFIN", "KON", "VVPP", "$."], "meter": "+-+--+-+-+", "measure": "trochaic.penta.relaxed"}, "line.4": {"text": "So gehts dem Menschen auch,", "tokens": ["So", "gehts", "dem", "Men\u00b7schen", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.5": {"text": "Der seine Kreuzes-Last", "tokens": ["Der", "sei\u00b7ne", "Kreu\u00b7zes\u00b7Last"], "token_info": ["word", "word", "word"], "pos": ["ART", "PPOSAT", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Mit Ungedult auf seine Schultern fa\u00dft;", "tokens": ["Mit", "Un\u00b7ge\u00b7dult", "a