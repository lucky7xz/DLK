{"textgrid.poem.67618": {"metadata": {"author": {"name": "Herder, Johann Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "39.", "genre": "verse", "period": "N.A.", "pub_year": 1773, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "\u00bbich d\u00e4cht', Herr Fuchs, wir w\u00e4ren Beide,\u00ab", "tokens": ["\u00bb", "ich", "d\u00e4cht'", ",", "Herr", "Fuchs", ",", "wir", "w\u00e4\u00b7ren", "Bei\u00b7de", ",", "\u00ab"], "token_info": ["punct", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["$(", "PPER", "VVFIN", "$,", "NN", "NE", "$,", "PPER", "VAFIN", "PIS", "$,", "$("], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Sprach K\u00f6nig Aar, \u00bbgemacht zur Nachbarschaft.", "tokens": ["Sprach", "K\u00f6\u00b7nig", "Aar", ",", "\u00bb", "ge\u00b7macht", "zur", "Nach\u00b7bar\u00b7schaft", "."], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NN", "NE", "$,", "$(", "VVPP", "APPRART", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Er hat viel List, und ich viel Kraft!", "tokens": ["Er", "hat", "viel", "List", ",", "und", "ich", "viel", "Kraft", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "KON", "PPER", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sieht Er, ich wohne", "tokens": ["Sieht", "Er", ",", "ich", "woh\u00b7ne"], "token_info": ["word", "word", "punct", "word", "word"], "pos": ["VVFIN", "PPER", "$,", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.5": {"text": "Da in der Krone,", "tokens": ["Da", "in", "der", "Kro\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "ART", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.6": {"text": "Und Er hienieden", "tokens": ["Und", "Er", "hien\u00b7ie\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["KON", "PPER", "VVFIN"], "meter": "-+-+-", "measure": "iambic.di"}, "line.7": {"text": "Im Loch in Frieden,", "tokens": ["Im", "Loch", "in", "Frie\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPR", "NN", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.8": {"text": "Und geht Er aus,", "tokens": ["Und", "geht", "Er", "aus", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "PTKVZ", "$,"], "meter": "-+-+", "measure": "iambic.di"}, "line.9": {"text": "Vertrau Er mir nur", "tokens": ["Ver\u00b7trau", "Er", "mir", "nur"], "token_info": ["word", "word", "word", "word"], "pos": ["NN", "PPER", "PPER", "ADV"], "meter": "-+--+", "measure": "iambic.di.chol"}, "line.10": {"text": "Auf meine K\u00f6nigstreu (und schw\u00f6ret K\u00f6nigsschwur)", "tokens": ["Auf", "mei\u00b7ne", "K\u00f6\u00b7nigs\u00b7treu", "(", "und", "schw\u00f6\u00b7ret", "K\u00f6\u00b7nigs\u00b7schwur", ")"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "PPOSAT", "NN", "$(", "KON", "VVFIN", "NE", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Sein kleines Haus!\u00ab", "tokens": ["Sein", "klei\u00b7nes", "Haus", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$("], "meter": "-+-+", "measure": "iambic.di"}, "line.12": {"text": "Gl\u00fcck zu, Herr Fuchs, zu hoher Nachbarschaft!", "tokens": ["Gl\u00fcck", "zu", ",", "Herr", "Fuchs", ",", "zu", "ho\u00b7her", "Nach\u00b7bar\u00b7schaft", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "PTKVZ", "$,", "NN", "NE", "$,", "APPR", "ADJA", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}, "line.13": {"text": "Er hat viel List, und Der viel Kraft.", "tokens": ["Er", "hat", "viel", "List", ",", "und", "Der", "viel", "Kraft", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "$,", "KON", "ART", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.14": {"text": "Vertrau' er nur", "tokens": ["Ver\u00b7trau'", "er", "nur"], "token_info": ["word", "word", "word"], "pos": ["VVIMP", "PPER", "ADV"], "meter": "-+-+", "measure": "iambic.di"}, "line.15": {"text": "Dem Schwur!", "tokens": ["Dem", "Schwur", "!"], "token_info": ["word", "word", "punct"], "pos": ["PDS", "VVFIN", "$."], "meter": "-+", "measure": "iambic.single"}, "line.16": {"text": "Der Fuchs ist nicht zu Haus;", "tokens": ["Der", "Fuchs", "ist", "nicht", "zu", "Haus", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VAFIN", "PTKNEG", "APPR", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.17": {"text": "Der K\u00f6nig Aar hat keinen Schmaus.", "tokens": ["Der", "K\u00f6\u00b7nig", "Aar", "hat", "kei\u00b7nen", "Schmaus", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NE", "VAFIN", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.18": {"text": "\u00bbwir sind von Gottes Gnaden", "tokens": ["\u00bb", "wir", "sind", "von", "Got\u00b7tes", "Gna\u00b7den"], "token_info": ["punct", "word", "word", "word", "word", "word"], "pos": ["$(", "PPER", "VAFIN", "APPR", "NN", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.19": {"text": "Zu Gast geladen", "tokens": ["Zu", "Gast", "ge\u00b7la\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["APPR", "NN", "VVPP"], "meter": "-+-+-", "measure": "iambic.di"}, "line.20": {"text": "In Nachbar Fuchses Haus", "tokens": ["In", "Nach\u00b7bar", "Fuch\u00b7ses", "Haus"], "token_info": ["word", "word", "word", "word"], "pos": ["APPR", "NN", "NE", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.21": {"text": "Auf junge F\u00fcchselein", "tokens": ["Auf", "jun\u00b7ge", "F\u00fcch\u00b7sel\u00b7ein"], "token_info": ["word", "word", "word"], "pos": ["APPR", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.22": {"text": "Und speisen ihm in Gnaden", "tokens": ["Und", "spei\u00b7sen", "ihm", "in", "Gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "VVFIN", "PPER", "APPR", "NN"], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.23": {"text": "Das Nest rein.\u00ab", "tokens": ["Das", "Nest", "rein", ".", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "PTKVZ", "$.", "$("], "meter": "-+-", "measure": "amphibrach.single"}, "line.24": {"text": "Der Vater kommt. \u00bbAch nein!", "tokens": ["Der", "Va\u00b7ter", "kommt", ".", "\u00bb", "Ach", "nein", "!"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "$.", "$(", "NN", "PTKANT", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.25": {"text": "Das kann nicht sein!", "tokens": ["Das", "kann", "nicht", "sein", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PDS", "VMFIN", "PTKNEG", "VAINF", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.26": {"text": "Sein hoher Schwur! \u2013 Und doch,", "tokens": ["Sein", "ho\u00b7her", "Schwur", "!", "\u2013", "Und", "doch", ","], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "$.", "$(", "KON", "ADV", "$,"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.27": {"text": "Da fri\u00dft er noch!", "tokens": ["Da", "fri\u00dft", "er", "noch", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}, "line.28": {"text": "Da liegt noch ihr Gebein!", "tokens": ["Da", "liegt", "noch", "ihr", "Ge\u00b7bein", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ADV", "PPOSAT", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.29": {"text": "O Jupiter! soll's ungerochen sein?\u00ab", "tokens": ["O", "Ju\u00b7pi\u00b7ter", "!", "soll's", "un\u00b7ge\u00b7ro\u00b7chen", "sein", "?", "\u00ab"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["NE", "NN", "$.", "VMFIN", "ADJD", "VAINF", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.30": {"text": "Verwaister, harre noch!", "tokens": ["Ver\u00b7wais\u00b7ter", ",", "har\u00b7re", "noch", "!"], "token_info": ["word", "punct", "word", "word", "punct"], "pos": ["NN", "$,", "VVFIN", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.31": {"text": "Und nun erwach und sieh!", "tokens": ["Und", "nun", "er\u00b7wach", "und", "sieh", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "KON", "VVIMP", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.32": {"text": "Da f\u00e4hrt er fr\u00fch", "tokens": ["Da", "f\u00e4hrt", "er", "fr\u00fch"], "token_info": ["word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "PPER", "ADJD"], "meter": "-+-+", "measure": "iambic.di"}, "line.33": {"text": "Schon zum Altar", "tokens": ["Schon", "zum", "Al\u00b7tar"], "token_info": ["word", "word", "word"], "pos": ["ADV", "APPRART", "NN"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.34": {"text": "Des Donnergottes selbst, raubt Flammen", "tokens": ["Des", "Don\u00b7ner\u00b7got\u00b7tes", "selbst", ",", "raubt", "Flam\u00b7men"], "token_info": ["word", "word", "word", "punct", "word", "word"], "pos": ["ART", "NN", "ADV", "$,", "VVFIN", "NN"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.35": {"text": "Und Fra\u00df zusammen", "tokens": ["Und", "Fra\u00df", "zu\u00b7sam\u00b7men"], "token_info": ["word", "word", "word"], "pos": ["KON", "NN", "VVINF"], "meter": "-+-+-", "measure": "iambic.di"}, "line.36": {"text": "Und bl\u00e4ht sich: \u00bbK\u00f6nig Aar!\u00ab", "tokens": ["Und", "bl\u00e4ht", "sich", ":", "\u00bb", "K\u00f6\u00b7nig", "Aar", "!", "\u00ab"], "token_info": ["word", "word", "word", "punct", "punct", "word", "word", "punct", "punct"], "pos": ["KON", "VVFIN", "PRF", "$.", "$(", "NN", "NE", "$.", "$("], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.37": {"text": "Da weht", "tokens": ["Da", "weht"], "token_info": ["word", "word"], "pos": ["ADV", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.38": {"text": "Ein Sturmwind hinter ihm. Sieh, Aar,", "tokens": ["Ein", "Sturm\u00b7wind", "hin\u00b7ter", "ihm", ".", "Sieh", ",", "Aar", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["ART", "NN", "APPR", "PPER", "$.", "NE", "$,", "NE", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.39": {"text": "Dein Nest in Flammen!", "tokens": ["Dein", "Nest", "in", "Flam\u00b7men", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "APPR", "NN", "$."], "meter": "-+-+-", "measure": "iambic.di"}, "line.40": {"text": "Sieh Deine Brut", "tokens": ["Sieh", "Dei\u00b7ne", "Brut"], "token_info": ["word", "word", "word"], "pos": ["NE", "PPOSAT", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.41": {"text": "Versenkt, herabgeweht!", "tokens": ["Ver\u00b7senkt", ",", "her\u00b7ab\u00b7ge\u00b7weht", "!"], "token_info": ["word", "punct", "word", "punct"], "pos": ["VVFIN", "$,", "VVFIN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.42": {"text": "Es f\u00e4ht", "tokens": ["Es", "f\u00e4ht"], "token_info": ["word", "word"], "pos": ["PPER", "VVFIN"], "meter": "-+", "measure": "iambic.single"}, "line.43": {"text": "Sie Fuchses Rachen auf und k\u00fchlet seine Gluth", "tokens": ["Sie", "Fuch\u00b7ses", "Ra\u00b7chen", "auf", "und", "k\u00fch\u00b7let", "sei\u00b7ne", "Gluth"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "NE", "NN", "PTKVZ", "KON", "VVFIN", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "In junger Adler Blut.", "tokens": ["In", "jun\u00b7ger", "Ad\u00b7ler", "Blut", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Die Fabel, grausam, falsch und schlecht", "tokens": ["Die", "Fa\u00b7bel", ",", "grau\u00b7sam", ",", "falsch", "und", "schlecht"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "$,", "ADJD", "$,", "ADJD", "KON", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und sonder Zweifel \u00fcbertrieben,", "tokens": ["Und", "son\u00b7der", "Zwei\u00b7fel", "\u00fc\u00b7bert\u00b7rie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Ich fand sie alter Hand im neuen Buch geschrieben,", "tokens": ["Ich", "fand", "sie", "al\u00b7ter", "Hand", "im", "neu\u00b7en", "Buch", "ge\u00b7schrie\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJA", "NN", "APPRART", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Das hie\u00df das ", "tokens": ["Das", "hie\u00df", "das"], "token_info": ["word", "word", "word"], "pos": ["PDS", "VVFIN", "ART"], "meter": "-+-", "measure": "amphibrach.single"}, "line.5": {"text": "Das Buch war sch\u00f6n gedruckt,", "tokens": ["Das", "Buch", "war", "sch\u00f6n", "ge\u