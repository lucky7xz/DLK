{"textgrid.poem.65177": {"metadata": {"author": {"name": "Paoli, Betty", "birth": "N.A.", "death": "N.A."}, "title": "1L: Der Dichter wandelt einsam durch das Leben!", "genre": "verse", "period": "N.A.", "pub_year": 1854, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Der Dichter wandelt einsam durch das Leben!", "tokens": ["Der", "Dich\u00b7ter", "wan\u00b7delt", "ein\u00b7sam", "durch", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "So ist es und so war's zu allen Zeiten.", "tokens": ["So", "ist", "es", "und", "so", "wa\u00b7r's", "zu", "al\u00b7len", "Zei\u00b7ten", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "KON", "ADV", "VAFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Entsagung nur darf ihm zur Seite schreiten,", "tokens": ["Ent\u00b7sa\u00b7gung", "nur", "darf", "ihm", "zur", "Sei\u00b7te", "schrei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "VMFIN", "PPER", "APPRART", "NN", "VVFIN", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.4": {"text": "Wenn holde Bande sich um and're weben!", "tokens": ["Wenn", "hol\u00b7de", "Ban\u00b7de", "sich", "um", "an\u00b7d'\u00b7re", "we\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJA", "NN", "PRF", "APPR", "PIS", "VVINF", "$."], "meter": "-+-+--+-+-+-", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Doch ein Ersatz ist ihm daf\u00fcr gegeben:", "tokens": ["Doch", "ein", "Er\u00b7satz", "ist", "ihm", "da\u00b7f\u00fcr", "ge\u00b7ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VAFIN", "PPER", "PAV", "VVPP", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Da\u00df Herzen ihm, in unbekannten Weiten,", "tokens": ["Da\u00df", "Her\u00b7zen", "ihm", ",", "in", "un\u00b7be\u00b7kann\u00b7ten", "Wei\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "PPER", "$,", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Entgegen schlagen und wie Harfensaiten", "tokens": ["Ent\u00b7ge\u00b7gen", "schla\u00b7gen", "und", "wie", "Har\u00b7fen\u00b7sai\u00b7ten"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "VVINF", "KON", "PWAV", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Vom Hauche seiner Lieder sanft erbeben.", "tokens": ["Vom", "Hau\u00b7che", "sei\u00b7ner", "Lie\u00b7der", "sanft", "er\u00b7be\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "PPOSAT", "NN", "ADJD", "VVINF", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und wurden solche Freunde dir zu teil,", "tokens": ["Und", "wur\u00b7den", "sol\u00b7che", "Freun\u00b7de", "dir", "zu", "teil", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "PIAT", "NN", "PPER", "APPR", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Betrachte sie als h\u00f6chste Schicksalsspenden,", "tokens": ["Be\u00b7trach\u00b7te", "sie", "als", "h\u00f6chs\u00b7te", "Schick\u00b7sals\u00b7spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "KOUS", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Die f\u00fcr kein fl\u00fccht'ges Gut der Erde feil!", "tokens": ["Die", "f\u00fcr", "kein", "fl\u00fccht'\u00b7ges", "Gut", "der", "Er\u00b7de", "feil", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "APPR", "PIAT", "ADJA", "NN", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.4": {"line.1": {"text": "Zweifach gesegnet ist, der sie gewann!", "tokens": ["Zwei\u00b7fach", "ge\u00b7seg\u00b7net", "ist", ",", "der", "sie", "ge\u00b7wann", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVPP", "VAFIN", "$,", "PRELS", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Denn in dem stillen Gru\u00df, den sie ihm senden,", "tokens": ["Denn", "in", "dem", "stil\u00b7len", "Gru\u00df", ",", "den", "sie", "ihm", "sen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "ADJA", "NN", "$,", "PRELS", "PPER", "PPER", "VVINF", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "F\u00e4ngt auch bereits die Nachwelt f\u00fcr ihn an!", "tokens": ["F\u00e4ngt", "auch", "be\u00b7reits", "die", "Nach\u00b7welt", "f\u00fcr", "ihn", "an", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "ADV", "ART", "NN", "APPR", "PPER", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Der Dichter wandelt einsam durch das Leben!", "tokens": ["Der", "Dich\u00b7ter", "wan\u00b7delt", "ein\u00b7sam", "durch", "das", "Le\u00b7ben", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+-+-", "measure": "iambic.penta