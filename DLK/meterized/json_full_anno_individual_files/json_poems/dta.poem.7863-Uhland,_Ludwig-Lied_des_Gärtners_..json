{"dta.poem.7863": {"metadata": {"author": {"name": "Uhland, Ludwig", "birth": "N.A.", "death": "N.A."}, "title": "Lied des G\u00e4rtners .", "genre": "Lyrik", "period": "N.A.", "pub_year": "1815", "urn": "urn:nbn:de:kobv:b4-200905196438", "language": ["de:0.99"], "booktitle": "Uhland, Ludwig: Gedichte. Stuttgart u. a., 1815."}, "poem": {"stanza.1": {"line.1": {"text": "La\u00dft euch pfl\u00fccken, la\u00dft euch pfl\u00fccken,", "tokens": ["La\u00dft", "euch", "pfl\u00fc\u00b7cken", ",", "la\u00dft", "euch", "pfl\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "VVINF", "$,", "VVIMP", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Lichte Bl\u00fcmlein, meine Lust!", "tokens": ["Lich\u00b7te", "Bl\u00fcm\u00b7lein", ",", "mei\u00b7ne", "Lust", "!"], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["NN", "NN", "$,", "PPOSAT", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Denn ihr sollet lieblich schm\u00fccken", "tokens": ["Denn", "ihr", "sol\u00b7let", "lieb\u00b7lich", "schm\u00fc\u00b7cken"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KON", "PPER", "VMFIN", "ADJD", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Meiner sch\u00f6nsten F\u00fcrstin Brust.", "tokens": ["Mei\u00b7ner", "sch\u00f6ns\u00b7ten", "F\u00fcrs\u00b7tin", "Brust", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Gl\u00fchet purpurn nach der S\u00fc\u00dfen,", "tokens": ["Gl\u00fc\u00b7het", "pur\u00b7purn", "nach", "der", "S\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "NE", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Aeugelt blau empor zu ihr!", "tokens": ["A\u00b7e\u00b7u\u00b7gelt", "blau", "em\u00b7por", "zu", "ihr", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "PTKVZ", "APPR", "PPOSAT", "$."], "meter": "+-+-+-+-+", "measure": "trochaic.penta"}, "line.3": {"text": "Ach! ihr m\u00fc\u00dft es endlich b\u00fc\u00dfen,", "tokens": ["Ach", "!", "ihr", "m\u00fc\u00dft", "es", "end\u00b7lich", "b\u00fc\u00b7\u00dfen", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ITJ", "$.", "PPER", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Sinken ohne Glanz und Zier.", "tokens": ["Sin\u00b7ken", "oh\u00b7ne", "Glanz", "und", "Zier", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "APPR", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Einst auch gl\u00fchten meine Wangen,", "tokens": ["Einst", "auch", "gl\u00fch\u00b7ten", "mei\u00b7ne", "Wan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Meine Augen hin nach ihr:", "tokens": ["Mei\u00b7ne", "Au\u00b7gen", "hin", "nach", "ihr", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "ADV", "APPR", "PPOSAT", "$."], "meter": "+-+--+-", "measure": "pherekrateus"}, "line.3": {"text": "Nun ist alles Roth vergangen,", "tokens": ["Nun", "ist", "al\u00b7les", "Roth", "ver\u00b7gan\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PIAT", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aller blaue Schimmer mir.", "tokens": ["Al\u00b7ler", "blau\u00b7e", "Schim\u00b7mer", "mir", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PIAT", "ADJA", "NN", "PPER", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}}}}