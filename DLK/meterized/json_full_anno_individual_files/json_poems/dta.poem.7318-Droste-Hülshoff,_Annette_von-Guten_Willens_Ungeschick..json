{"dta.poem.7318": {"metadata": {"author": {"name": "Droste-H\u00fclshoff, Annette von", "birth": "N.A.", "death": "N.A."}, "title": "Guten Willens Ungeschick.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1844", "urn": "urn:nbn:de:kobv:b4-20090519994", "language": ["de:0.99"], "booktitle": "Droste-H\u00fclshoff, Annette von: Gedichte. Stuttgart u. a., 1844."}, "poem": {"stanza.1": {"line.1": {"text": "Du scheuchst den frommen Freund von mir,", "tokens": ["Du", "scheuchst", "den", "from\u00b7men", "Freund", "von", "mir", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Weil krank ich sey und sehr bewegt,", "tokens": ["Weil", "krank", "ich", "sey", "und", "sehr", "be\u00b7wegt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADJD", "PPER", "VAFIN", "KON", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Mein hell und bl\u00fchend Lustrevier", "tokens": ["Mein", "hell", "und", "bl\u00fc\u00b7hend", "Lus\u00b7tre\u00b7vier"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "KON", "ADJD", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Hast du mit Dornen mir umhegt;", "tokens": ["Hast", "du", "mit", "Dor\u00b7nen", "mir", "um\u00b7hegt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "APPR", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Wohl wei\u00df ich, da\u00df der Wille rein,", "tokens": ["Wohl", "wei\u00df", "ich", ",", "da\u00df", "der", "Wil\u00b7le", "rein", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "$,", "KOUS", "ART", "NN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da\u00df eure Sorge immer wach,", "tokens": ["Da\u00df", "eu\u00b7re", "Sor\u00b7ge", "im\u00b7mer", "wach", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPOSAT", "NN", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Doch was ihn labt, was hindert, ach,", "tokens": ["Doch", "was", "ihn", "labt", ",", "was", "hin\u00b7dert", ",", "ach", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "punct"], "pos": ["KON", "PWS", "PPER", "VVFIN", "$,", "PWS", "VVFIN", "$,", "ITJ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Jeder wei\u00df es nur allein.", "tokens": ["Ein", "Je\u00b7der", "wei\u00df", "es", "nur", "al\u00b7lein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIS", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Ich denke, wie ich einstens sa\u00df", "tokens": ["Ich", "den\u00b7ke", ",", "wie", "ich", "eins\u00b7tens", "sa\u00df"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$,", "PWAV", "PPER", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "An eines H\u00fcgels schroffem Rain,", "tokens": ["An", "ei\u00b7nes", "H\u00fc\u00b7gels", "schrof\u00b7fem", "Rain", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und sah ein sch\u00f6nes Kind, das las", "tokens": ["Und", "sah", "ein", "sch\u00f6\u00b7nes", "Kind", ",", "das", "las"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word"], "pos": ["KON", "VVFIN", "ART", "ADJA", "NN", "$,", "PDS", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich Schneckenh\u00e4uschen im Gestein;", "tokens": ["Sich", "Schne\u00b7cken\u00b7h\u00e4usc\u00b7hen", "im", "Ge\u00b7stein", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PRF", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dann glitt es aus, ich sprang hinzu,", "tokens": ["Dann", "glitt", "es", "aus", ",", "ich", "sprang", "hin\u00b7zu", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PTKVZ", "$,", "PPER", "VVFIN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es hatte sich am Strauch gedr\u00fcckt;", "tokens": ["Es", "hat\u00b7te", "sich", "am", "Strauch", "ge\u00b7dr\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PRF", "APPRART", "NN", "VVPP", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich griff es an gar ungeschickt,", "tokens": ["Ich", "griff", "es", "an", "gar", "un\u00b7ge\u00b7schickt", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und abw\u00e4rts rollte es im Nu;", "tokens": ["Und", "ab\u00b7w\u00e4rts", "roll\u00b7te", "es", "im", "Nu", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VVFIN", "PPER", "APPRART", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Auf hob ich es, das weinend lag,", "tokens": ["Auf", "hob", "ich", "es", ",", "das", "wei\u00b7nend", "lag", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["APPR", "VVFIN", "PPER", "PPER", "$,", "PRELS", "ADJD", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und grimmig weinend um sich fuhr,", "tokens": ["Und", "grim\u00b7mig", "wei\u00b7nend", "um", "sich", "fuhr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "VVPP", "APPR", "PRF", "VVFIN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Und freilich, was es stie\u00df vom Haag,", "tokens": ["Und", "frei\u00b7lich", ",", "was", "es", "stie\u00df", "vom", "Haag", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "$,", "PWS", "PPER", "VVFIN", "APPRART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Mein schlimmes Helfen war es nur. \u2014", "tokens": ["Mein", "schlim\u00b7mes", "Hel\u00b7fen", "war", "es", "nur", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["PPOSAT", "ADJA", "NN", "VAFIN", "PPER", "ADV", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und an der Klippe stand ich auch,", "tokens": ["Und", "an", "der", "Klip\u00b7pe", "stand", "ich", "auch", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "VVFIN", "PPER", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Bei Vogelbrut mit Flaumenhaar,", "tokens": ["Bei", "Vo\u00b7gel\u00b7brut", "mit", "Flau\u00b7men\u00b7haar", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "NE", "APPR", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Und dr\u00fcber pfiff wie ein Corsar", "tokens": ["Und", "dr\u00fc\u00b7ber", "pfiff", "wie", "ein", "Cor\u00b7sar"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KON", "PAV", "VVFIN", "KOKOM", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Ein Weihe hoch im Nebelrauch.", "tokens": ["Ein", "Wei\u00b7he", "hoch", "im", "Ne\u00b7bel\u00b7rauch", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Nun blitzte wie ein Stral heran", "tokens": ["Nun", "blitz\u00b7te", "wie", "ein", "Stral", "he\u00b7ran"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "KOKOM", "ART", "NN", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und immer n\u00e4her scho\u00df der Weih,", "tokens": ["Und", "im\u00b7mer", "n\u00e4\u00b7her", "scho\u00df", "der", "Weih", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "ADJD", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich schwang das Tuch, den Mantel dann,", "tokens": ["Ich", "schwang", "das", "Tuch", ",", "den", "Man\u00b7tel", "dann", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "ADV", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die jungen V\u00f6gel duckten scheu;", "tokens": ["Die", "jun\u00b7gen", "V\u00f6\u00b7gel", "duck\u00b7ten", "scheu", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Und aufw\u00e4rts funkelnd, angstgepre\u00dft,", "tokens": ["Und", "auf\u00b7w\u00e4rts", "fun\u00b7kelnd", ",", "angst\u00b7ge\u00b7pre\u00dft", ","], "token_info": ["word", "word", "word", "punct", "word", "punct"], "pos": ["KON", "ADV", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Wie Marder pfiffen sie so klar;", "tokens": ["Wie", "Mar\u00b7der", "pfif\u00b7fen", "sie", "so", "klar", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "VVFIN", "PPER", "ADV", "ADJD", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Da ward mir endlich offenbar,", "tokens": ["Da", "ward", "mir", "end\u00b7lich", "of\u00b7fen\u00b7bar", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Dies sey des Weihen eignes Nest.", "tokens": ["Dies", "sey", "des", "Wei\u00b7hen", "eig\u00b7nes", "Nest", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "ART", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "So hab' ich hundertmal gef\u00fchlt,", "tokens": ["So", "hab'", "ich", "hun\u00b7dert\u00b7mal", "ge\u00b7f\u00fchlt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ADV", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und tausendmal hab' ich gesehn,", "tokens": ["Und", "tau\u00b7send\u00b7mal", "hab'", "ich", "ge\u00b7sehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "VAFIN", "PPER", "VVPP", "$,"], "meter": "-+--+--+", "measure": "prosodiakos"}, "line.3": {"text": "Da\u00df Nichts so hart am Herzen w\u00fchlt", "tokens": ["Da\u00df", "Nichts", "so", "hart", "am", "Her\u00b7zen", "w\u00fchlt"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PIS", "ADV", "ADJD", "APPRART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Wo seine tiefsten Adern gehn,", "tokens": ["Wo", "sei\u00b7ne", "tiefs\u00b7ten", "A\u00b7dern", "gehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPOSAT", "ADJA", "NN", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Als \u2014 z\u00fcrne nicht, die Lippen dr\u00fcck'", "tokens": ["Als", "z\u00fcr\u00b7ne", "nicht", ",", "die", "Lip\u00b7pen", "dr\u00fcck"], "token_info": ["word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "$(", "VVFIN", "PTKNEG", "$,", "ART", "NN", "VVFIN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.6": {"text": "Ich s\u00fchnend auf der Lippen Rand \u2014", "tokens": ["Ich", "s\u00fch\u00b7nend", "auf", "der", "Lip\u00b7pen", "Rand"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVPP", "APPR", "ART", "NN", "NN", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Als eine liebe rasche Hand", "tokens": ["Als", "ei\u00b7ne", "lie\u00b7be", "ra\u00b7sche", "Hand"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["KOUS", "ART", "ADJA", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "In guten Willens Ungeschick.", "tokens": ["In", "gu\u00b7ten", "Wil\u00b7lens", "Un\u00b7ge\u00b7schick", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}