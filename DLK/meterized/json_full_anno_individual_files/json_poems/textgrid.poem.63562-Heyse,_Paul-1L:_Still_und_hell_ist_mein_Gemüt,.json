{"textgrid.poem.63562": {"metadata": {"author": {"name": "Heyse, Paul", "birth": "N.A.", "death": "N.A."}, "title": "1L: Still und hell ist mein Gem\u00fct,", "genre": "verse", "period": "N.A.", "pub_year": 1867, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Still und hell ist mein Gem\u00fct,", "tokens": ["Still", "und", "hell", "ist", "mein", "Ge\u00b7m\u00fct", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJD", "KON", "ADJD", "VAFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.2": {"text": "Wie im Herbst ein Sonnentag,", "tokens": ["Wie", "im", "Herbst", "ein", "Son\u00b7nen\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPRART", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und doch f\u00fchl' ich, da\u00df im Innern", "tokens": ["Und", "doch", "f\u00fchl'", "ich", ",", "da\u00df", "im", "In\u00b7nern"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "ADV", "VVFIN", "PPER", "$,", "KOUS", "APPRART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie durch Lenzes Zauberschlag", "tokens": ["Wie", "durch", "Len\u00b7zes", "Zau\u00b7ber\u00b7schlag"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "APPR", "NE", "NN"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.5": {"text": "Eine junge Sch\u00f6pfung bl\u00fcht.", "tokens": ["Ei\u00b7ne", "jun\u00b7ge", "Sch\u00f6p\u00b7fung", "bl\u00fcht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Hast du noch nicht ausgegl\u00fcht,", "tokens": ["Hast", "du", "noch", "nicht", "aus\u00b7ge\u00b7gl\u00fcht", ","], "token_info": ["