{"textgrid.poem.40554": {"metadata": {"author": {"name": "Gr\u00fcn, Anastasius", "birth": "N.A.", "death": "N.A."}, "title": "1L: Im Lippenrosenbett geboren", "genre": "verse", "period": "N.A.", "pub_year": 1842, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Lippenrosenbett geboren", "tokens": ["Im", "Lip\u00b7pen\u00b7ro\u00b7sen\u00b7bett", "ge\u00b7bo\u00b7ren"], "token_info": ["word", "word", "word"], "pos": ["APPRART", "NN", "VVPP"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ward uns das freie Wort, ein Held;", "tokens": ["Ward", "uns", "das", "frei\u00b7e", "Wort", ",", "ein", "Held", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Wer sieht's dem Weichling an, erkoren", "tokens": ["Wer", "sieht's", "dem", "Weich\u00b7ling", "an", ",", "er\u00b7ko\u00b7ren"], "token_info": ["word", "word", "word", "word", "word", "punct", "word"], "pos": ["PWS", "VVFIN", "ART", "NN", "PTKVZ", "$,", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.4": {"text": "Sei er zu herrschen ob der Welt?", "tokens": ["Sei", "er", "zu", "herr\u00b7schen", "ob", "der", "Welt", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKZU", "VVINF", "KOUS", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Wie lang, da\u00df festen Tritt er lerne,", "tokens": ["Wie", "lang", ",", "da\u00df", "fes\u00b7ten", "Tritt", "er", "ler\u00b7ne", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ADJD", "$,", "KOUS", "ADJA", "NN", "PPER", "VVFIN", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.2": {"text": "Ist e