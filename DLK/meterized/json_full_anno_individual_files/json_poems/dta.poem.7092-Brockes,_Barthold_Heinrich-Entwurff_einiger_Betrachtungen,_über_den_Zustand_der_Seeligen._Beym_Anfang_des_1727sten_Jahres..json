{"dta.poem.7092": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Entwurff einiger Betrachtungen,  \n \u00fcber den  \n  Zustand der Seeligen.  \n Beym Anfang des 1727sten Jahres.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Wenn aber GOTT ihr wird das seel\u2019ge Leben g\u00f6nnen;", "tokens": ["Wenn", "a\u00b7ber", "GoTT", "ihr", "wird", "das", "seel'\u00b7ge", "Le\u00b7ben", "g\u00f6n\u00b7nen", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "ADV", "NE", "PPER", "VAFIN", "ART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Wird sie sich innerlich erkennen,", "tokens": ["Wird", "sie", "sich", "in\u00b7ner\u00b7lich", "er\u00b7ken\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PRF", "ADJD", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.3": {"text": "Sie wird sodenn, als wie ein Licht,", "tokens": ["Sie", "wird", "so\u00b7denn", ",", "als", "wie", "ein", "Licht", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "$,", "KOUS", "KOKOM", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Sich selbst durchdringen und verstehn,", "tokens": ["Sich", "selbst", "durch\u00b7drin\u00b7gen", "und", "ver\u00b7stehn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PRF", "ADV", "VVINF", "KON", "VVINF", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Zu welcher Herrlichkeit sie GOTT hat ausersehn.", "tokens": ["Zu", "wel\u00b7cher", "Herr\u00b7lich\u00b7keit", "sie", "GoTT", "hat", "au\u00b7ser\u00b7sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PWAT", "NN", "PPER", "NE", "VAFIN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Wie wird sodann zugleich", "tokens": ["Wie", "wird", "so\u00b7dann", "zu\u00b7gleich"], "token_info": ["word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "ADV", "ADV"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "Die Seele nicht an Wissenschafften reich,", "tokens": ["Die", "See\u00b7le", "nicht", "an", "Wis\u00b7sen\u00b7schaff\u00b7ten", "reich", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PTKNEG", "APPR", "NN", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Und reich an G\u00f6ttlicher Gesch\u00f6pff Erkenntni\u00df, werden", "tokens": ["Und", "reich", "an", "G\u00f6tt\u00b7li\u00b7cher", "Ge\u00b7sch\u00f6pff", "Er\u00b7kennt\u00b7ni\u00df", ",", "wer\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word"], "pos": ["KON", "ADJD", "APPR", "ADJA", "NN", "NN", "$,", "VAFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.9": {"text": "In allen Wundern aller Erden!", "tokens": ["In", "al\u00b7len", "Wun\u00b7dern", "al\u00b7ler", "Er\u00b7den", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PIAT", "NN", "$."], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Was wird sie nicht in aller Himmel Gr\u00fcnden", "tokens": ["Was", "wird", "sie", "nicht", "in", "al\u00b7ler", "Him\u00b7mel", "Gr\u00fcn\u00b7den"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWS", "VAFIN", "PPER", "PTKNEG", "APPR", "PIAT", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.11": {"text": "Vor unerforschliche Geheimni\u00df finden,", "tokens": ["Vor", "un\u00b7er\u00b7for\u00b7schli\u00b7che", "Ge\u00b7heim\u00b7ni\u00df", "fin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVINF", "$,"], "meter": "-+--+--+-+-", "measure": "amphibrach.tri.plus"}, "line.12": {"text": "Vor Machts- und Weisheits-Meere sehn?", "tokens": ["Vor", "Machts", "und", "Weis\u00b7heits\u00b7Mee\u00b7re", "sehn", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "TRUNC", "KON", "NN", "VVINF", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}