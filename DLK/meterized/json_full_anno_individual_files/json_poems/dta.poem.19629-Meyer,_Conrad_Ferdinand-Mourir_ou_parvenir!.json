{"dta.poem.19629": {"metadata": {"author": {"name": "Meyer, Conrad Ferdinand", "birth": "N.A.", "death": "N.A."}, "title": "Mourir ou parvenir!", "genre": "Lyrik", "period": "N.A.", "pub_year": "1882", "urn": "urn:nbn:de:kobv:b4-200905193933", "language": ["de:0.99"], "booktitle": "Meyer, Conrad Ferdinand: Gedichte. Leipzig, 1882."}, "poem": {"stanza.1": {"line.1": {"text": "Herr Heinrich Guise schrieb. Da rauscht' Gewand \u2014", "tokens": ["Herr", "Hein\u00b7rich", "Gui\u00b7se", "schrieb", ".", "Da", "rauscht'", "Ge\u00b7wand"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "NE", "NE", "VVFIN", "$.", "ADV", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Es war sein Lieb, das aus der Kirche kam,", "tokens": ["Es", "war", "sein", "Lieb", ",", "das", "aus", "der", "Kir\u00b7che", "kam", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PPOSAT", "NN", "$,", "PRELS", "APPR", "ART", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Sein z\u00e4rtlich Lieb, der sch\u00e4kernd aus der Hand", "tokens": ["Sein", "z\u00e4rt\u00b7lich", "Lieb", ",", "der", "sch\u00e4\u00b7kernd", "aus", "der", "Hand"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPOSAT", "ADJD", "NN", "$,", "PRELS", "ADJD", "APPR", "ART", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Er das mit Gold beschlagne Me\u00dfbuch nahm.", "tokens": ["Er", "das", "mit", "Gold", "be\u00b7schlag\u00b7ne", "Me\u00df\u00b7buch", "nahm", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "PDS", "APPR", "NN", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Er bl\u00e4ttert' drin. Hell war's von Farbenglut", "tokens": ["Er", "bl\u00e4t\u00b7tert'", "drin", ".", "Hell", "wa\u00b7r's", "von", "Far\u00b7beng\u00b7lut"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PTKVZ", "$.", "NE", "VAFIN", "APPR", "NN"], "meter": "-+--+-+-+-+", "measure": "iambic.penta.relaxed"}, "line.6": {"text": "Und keck verschlungner Arabeskenzier \u2014", "tokens": ["Und", "keck", "ver\u00b7schlung\u00b7ner", "A\u00b7ra\u00b7bes\u00b7ken\u00b7zier"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "ADJA", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "\u201egeliebter, dich verdirbt dein Uebermuth!", "tokens": ["\u201e", "ge\u00b7lieb\u00b7ter", ",", "dich", "ver\u00b7dirbt", "dein", "Ue\u00b7ber\u00b7muth", "!"], "token_info": ["punct", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "ADJA", "$,", "PPER", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Hinweg! Entflieh von hier!", "tokens": ["Hin\u00b7weg", "!", "Ent\u00b7flieh", "von", "hier", "!"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "$.", "NN", "APPR", "ADV", "$."], "meter": "-+-+-+", "measure": "iambic.tri"}}, "stanza.2": {"line.1": {"text": "Du bist zu hoch! Der K\u00f6nig, feig und schlau,", "tokens": ["Du", "bist", "zu", "hoch", "!", "Der", "K\u00f6\u00b7nig", ",", "feig", "und", "schlau", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PTKA", "ADJD", "$.", "ART", "NN", "$,", "ADJD", "KON", "ADJD", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Bebt wie ein Kind vor deinen m\u00e4cht'gen Braun!", "tokens": ["Bebt", "wie", "ein", "Kind", "vor", "dei\u00b7nen", "m\u00e4cht'\u00b7gen", "Braun", "!"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "KOKOM", "ART", "NN", "APPR", "PPOSAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Dich ha\u00dft er t\u00f6dtlich \u2014 glaub es einer Frau!", "tokens": ["Dich", "ha\u00dft", "er", "t\u00f6dt\u00b7lich", "glaub", "es", "ei\u00b7ner", "Frau", "!"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPER", "ADJD", "$(", "VVFIN", "PPER", "ART", "NN", "$."], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Ihn sah ich l\u00e4cheln j\u00fcngst \u2014 mich sch\u00fcttelt Graun!\u201c", "tokens": ["Ihn", "sah", "ich", "l\u00e4\u00b7cheln", "j\u00fcngst", "mich", "sch\u00fct\u00b7telt", "Graun", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PPER", "VVFIN", "PPER", "VVFIN", "ADV", "$(", "PPER", "VVFIN", "NN", "$.", "$("], "meter": "-+-+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.5": {"text": "Zur Feder griff er. \u201eFlora, schlanke Fei!", "tokens": ["Zur", "Fe\u00b7der", "griff", "er", ".", "\u201e", "Flo\u00b7ra", ",", "schlan\u00b7ke", "Fei", "!"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "punct", "word", "word", "punct"], "pos": ["APPRART", "NN", "VVFIN", "PPER", "$.", "$(", "NE", "$,", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "Wie k\u00f6nnt' ich leben,\u201c seufzt' er, \u201efern von dir?\u201c", "tokens": ["Wie", "k\u00f6nnt'", "ich", "le\u00b7ben", ",", "\u201c", "seufzt'", "er", ",", "\u201e", "fern", "von", "dir", "?", "\u201c"], "token_info": ["word", "word", "word", "word", "punct", "punct", "word", "word", "punct", "punct", "word", "word", "word", "punct", "punct"], "pos": ["PWAV", "VMFIN", "PPER", "VVINF", "$,", "$(", "VVFIN", "PPER", "$,", "$(", "ADJD", "APPR", "PPER", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Und schrieb ins Me\u00dfbuch, wo die Zeile frei:", "tokens": ["Und", "schrieb", "ins", "Me\u00df\u00b7buch", ",", "wo", "die", "Zei\u00b7le", "frei", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "APPRART", "NN", "$,", "PWAV", "ART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mourir \u2014", "tokens": ["Mou\u00b7rir"], "token_info": ["word", "punct"], "pos": ["NE", "$("], "meter": "+-", "measure": "trochaic.single"}}, "stanza.3": {"line.1": {"text": "\u2014 \u201eVersuche Gott nicht! Das Verderben reift!", "tokens": ["\u201e", "Ver\u00b7su\u00b7che", "Gott", "nicht", "!", "Das", "Ver\u00b7der\u00b7ben", "reift", "!"], "token_info": ["punct", "punct", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "$(", "ADJA", "NN", "PTKNEG", "$.", "ART", "NN", "VVFIN", "$."], "meter": "+-+-+---", "measure": "unknown.measure.tri"}, "line.2": {"text": "Hinweg aus Blois! Mein Alles, Schmerz und Lust!", "tokens": ["Hin\u00b7weg", "aus", "Blois", "!", "Mein", "Al\u00b7les", ",", "Schmerz", "und", "Lust", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "APPR", "NE", "$.", "PPOSAT", "PIS", "$,", "NN", "KON", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Ich wei\u00df: in diesem Augenblicke schleift", "tokens": ["Ich", "wei\u00df", ":", "in", "die\u00b7sem", "Au\u00b7gen\u00b7bli\u00b7cke", "schleift"], "token_info": ["word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "$.", "APPR", "PDAT", "NN", "VVFIN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Der Meuchelmord ein Schwert f\u00fcr deine Brust!\u201c", "tokens": ["Der", "Meu\u00b7chel\u00b7mord", "ein", "Schwert", "f\u00fcr", "dei\u00b7ne", "Brust", "!", "\u201c"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["ART", "NN", "ART", "NN", "APPR", "PPOSAT", "NN", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Der Herzog schrieb in ihrem Buche fort,", "tokens": ["Der", "Her\u00b7zog", "schrieb", "in", "ih\u00b7rem", "Bu\u00b7che", "fort", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NE", "VVFIN", "APPR", "PPOSAT", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "So viel ihm Raum gew\u00e4hrte das Papier,", "tokens": ["So", "viel", "ihm", "Raum", "ge\u00b7w\u00e4hr\u00b7te", "das", "Pa\u00b7pier", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADV", "PPER", "NN", "VVFIN", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Als w\u00e4r' es ein erbaulich Bibelwort:", "tokens": ["Als", "w\u00e4r'", "es", "ein", "er\u00b7bau\u00b7lich", "Bi\u00b7bel\u00b7wort", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "VAFIN", "PPER", "ART", "ADJD", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "\u2014 Ou parvenir!", "tokens": ["Ou", "par\u00b7ve\u00b7nir", "!"], "token_info": ["punct", "word", "word", "punct"], "pos": ["$(", "NE", "NE", "$."], "meter": "+--+", "measure": "iambic.di.chol"}}, "stanza.4": {"line.1": {"text": "\u201emich so zu qu\u00e4len! Schlimm hat mir getr\u00e4umt!", "tokens": ["\u201e", "mich", "so", "zu", "qu\u00e4\u00b7len", "!", "Schlimm", "hat", "mir", "ge\u00b7tr\u00e4umt", "!"], "token_info": ["punct", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "ADV", "PTKZU", "VVINF", "$.", "NE", "VAFIN", "PPER", "VVPP", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.2": {"text": "Mein Gott! Du wandest dich in Todesschmerz!", "tokens": ["Mein", "Gott", "!", "Du", "wan\u00b7dest", "dich", "in", "To\u00b7des\u00b7schmerz", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "$.", "PPER", "VVFIN", "PRF", "APPR", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Hinweg! Jetzt! Heute! H\u00f6rst du? Nicht ges\u00e4umt!\u201c", "tokens": ["Hin\u00b7weg", "!", "Jetzt", "!", "Heu\u00b7te", "!", "H\u00f6rst", "du", "?", "Nicht", "ge\u00b7s\u00e4umt", "!", "\u201c"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "punct", "word", "word", "punct", "punct"], "pos": ["ADV", "$.", "ADV", "$.", "ADV", "$.", "VVFIN", "PPER", "$.", "PTKNEG", "VVPP", "$.", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.4": {"text": "Sein Liebchen zog er kosend an das Herz,", "tokens": ["Sein", "Lieb\u00b7chen", "zog", "er", "ko\u00b7send", "an", "das", "Herz", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPOSAT", "NN", "VVFIN", "PPER", "ADJD", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.5": {"text": "Sie senkte des bethr\u00e4nten Auges Glanz \u2014", "tokens": ["Sie", "senk\u00b7te", "des", "be\u00b7thr\u00e4n\u00b7ten", "Au\u00b7ges", "Glanz"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ART", "ADJA", "NN", "NN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.6": {"text": "In k\u00fchnen Z\u00fcgen stand der Spruch vor ihr,", "tokens": ["In", "k\u00fch\u00b7nen", "Z\u00fc\u00b7gen", "stand", "der", "Spruch", "vor", "ihr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVFIN", "ART", "NN", "APPR", "PPER", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.7": {"text": "Umrankt von einem \u00fcpp'gen Blumenkranz:", "tokens": ["Um\u00b7rankt", "von", "ei\u00b7nem", "\u00fcpp'\u00b7gen", "Blu\u00b7men\u00b7kranz", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Mourir ou parvenir!", "tokens": ["Mou\u00b7rir", "ou", "par\u00b7ve\u00b7nir", "!"], "token_info": ["word", "word", "word", "punct"], "pos": ["FM.fr", "FM.fr", "FM.fr", "$."], "meter": "+--+-+", "measure": "iambic.tri.invert"}}}}}