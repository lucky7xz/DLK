{"textgrid.poem.56180": {"metadata": {"author": {"name": "Chamisso, Adelbert von", "birth": "N.A.", "death": "N.A."}, "title": "3", "genre": "verse", "period": "N.A.", "pub_year": 1809, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich kann's nicht fassen, nicht glauben,", "tokens": ["Ich", "kann's", "nicht", "fas\u00b7sen", ",", "nicht", "glau\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PTKNEG", "VVINF", "$,", "PTKNEG", "VVINF", "$,"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Es hat ein Traum mich ber\u00fcckt;", "tokens": ["Es", "hat", "ein", "Traum", "mich", "be\u00b7r\u00fcckt", ";"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "NN", "PPER", "VVFIN", "$."], "meter": "-+-+--+", "measure": "iambic.tri.chol"}, "line.3": {"text": "Wie h\u00e4tt er doch unter allen", "tokens": ["Wie", "h\u00e4tt", "er", "doch", "un\u00b7ter", "al\u00b7len"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "VAFIN", "PPER", "ADV", "APPR", "PIAT"], "meter": "-+--+-+-", "measure": "iambic.tri.relaxed"}, "line.4": {"text": "Mich Arme erh\u00f6ht und begl\u00fcckt?", "tokens": ["Mich", "Ar\u00b7me", "er\u00b7h\u00f6ht", "und", "be\u00b7gl\u00fcckt", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "NN", "VVPP", "KON", "VVPP", "$."], "meter": "-+--+--+", "measure": "prosodiakos"}}, "stanza.2": {"line.1": {"text": "Mir war's, er habe gesprochen:", "tokens": ["Mir", "wa\u00b7r's", ",", "er", "ha\u00b7be", "ge\u00b7spro\u00b7chen", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "$,", "PPER", "VAFIN", "VVPP", "$."], "meter": "-+--+--+-", "measure": "amphibrach.tri"}, "line.2": {"text": "Ich bin auf ewig dein \u2013", "tokens"