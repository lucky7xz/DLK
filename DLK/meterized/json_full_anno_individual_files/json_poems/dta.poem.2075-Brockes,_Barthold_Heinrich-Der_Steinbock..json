{"dta.poem.2075": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Der Steinbock.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Den Blasenstein uns zu zermalmen. Ein Theil mit sechs", "tokens": ["Den", "Bla\u00b7sen\u00b7stein", "uns", "zu", "zer\u00b7mal\u00b7men", ".", "Ein", "Theil", "mit", "sechs"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "PPER", "PTKZU", "VVINF", "$.", "ART", "NN", "APPR", "CARD"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.2": {"text": "Theil Most genommen,", "tokens": ["Theil", "Most", "ge\u00b7nom\u00b7men", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "NN", "VVPP", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.3": {"text": "Gekocht drey Tage nach einander, fr\u00fch, mittags und", "tokens": ["Ge\u00b7kocht", "drey", "Ta\u00b7ge", "nach", "ein\u00b7an\u00b7der", ",", "fr\u00fch", ",", "mit\u00b7tags", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "punct", "word", "word"], "pos": ["NN", "CARD", "NN", "APPR", "ADV", "$,", "ADJD", "$,", "ADV", "KON"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "beym Abendmal", "tokens": ["beym", "A\u00b7bend\u00b7mal"], "token_info": ["word", "word"], "pos": ["APPRART", "NN"], "meter": "-+-+", "measure": "iambic.di"}, "line.5": {"text": "Jm Bade nach und nach getrunken, soll denen trefflich", "tokens": ["Jm", "Ba\u00b7de", "nach", "und", "nach", "ge\u00b7trun\u00b7ken", ",", "soll", "de\u00b7nen", "treff\u00b7lich"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPRART", "NN", "APPR", "KON", "APPR", "VVPP", "$,", "VMFIN", "PDS", "ADJD"], "meter": "-+-+-+-+-+--+-", "measure": "iambic.hexa.relaxed"}, "line.6": {"text": "wohl bekommen,", "tokens": ["wohl", "be\u00b7kom\u00b7men", ","], "token_info": ["word", "word", "punct"], "pos": ["ADV", "VVINF", "$,"], "meter": "+-+-", "measure": "trochaic.di"}, "line.7": {"text": "Die durch den Blasenstein gemartert, da es den Ursprung", "tokens": ["Die", "durch", "den", "Bla\u00b7sen\u00b7stein", "ge\u00b7mar\u00b7tert", ",", "da", "es", "den", "Ur\u00b7sprung"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "APPR", "ART", "NN", "VVPP", "$,", "KOUS", "PPER", "ART", "NN"], "meter": "-+-+-+-+-++-+-", "measure": "unknown.measure.septa"}, "line.8": {"text": "ihrer Qual", "tokens": ["ih\u00b7rer", "Qual"], "token_info": ["word", "word"], "pos": ["PPOSAT", "NN"], "meter": "+-+", "measure": "trochaic.di"}, "line.9": {"text": "In Sand zermalmt von ihnen f\u00fchrt. Woraus wir kl\u00e4r-", "tokens": ["In", "Sand", "zer\u00b7malmt", "von", "ih\u00b7nen", "f\u00fchrt", ".", "Wo\u00b7raus", "wir", "kl\u00e4r"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "NN", "VVPP", "APPR", "PPER", "VVFIN", "$.", "PWAV", "PPER", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "lich sehen k\u00f6nnen,", "tokens": ["lich", "se\u00b7hen", "k\u00f6n\u00b7nen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "VVINF", "VMINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Wie viel auch Gott in diesem Thiere dem Menschen Gu-", "tokens": ["Wie", "viel", "auch", "Gott", "in", "die\u00b7sem", "Thie\u00b7re", "dem", "Men\u00b7schen", "Gu"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "ADV", "NN", "APPR", "PDAT", "NN", "ART", "NN", "TRUNC"], "meter": "-+-+-+-+--+-+", "measure": "iambic.hexa.relaxed"}, "line.12": {"text": "tes wollen g\u00f6nnen.", "tokens": ["tes", "wol\u00b7len", "g\u00f6n\u00b7nen", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NE", "VMFIN", "VVINF", "$."], "meter": "-+-+-", "measure": "iambic.di"}}}}}