{"dta.poem.8264": {"metadata": {"author": {"name": "Beer, Johann", "birth": "N.A.", "death": "N.A."}, "title": "f\u00fcnffter  A ufftritt.", "genre": "Lyrik, Libretto", "period": "N.A.", "pub_year": "1697", "urn": "urn:nbn:de:kobv:b4-25222-5", "language": ["de:0.99"], "booktitle": "Beer, Johann: Antonius R\u00f6mischer Triumvir. Wei\u00dfenfels, 1697."}, "poem": {"stanza.1": {"line.1": {"text": "Der schwartzen Augen Mitternacht", "tokens": ["Der", "schwart\u00b7zen", "Au\u00b7gen", "Mit\u00b7ter\u00b7nacht"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Nehrt' ein geheimes Eisen/", "tokens": ["Nehrt'", "ein", "ge\u00b7hei\u00b7mes", "Ei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "NN", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.3": {"text": "Das seinen Zug kan weisen/", "tokens": ["Das", "sei\u00b7nen", "Zug", "kan", "wei\u00b7sen", "/"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "PPOSAT", "NN", "VMFIN", "VVINF", "$("], "meter": "-+-+-+-", "measure": "iambic.tri"}, "line.4": {"text": "So offt es den Magnet", "tokens": ["So", "offt", "es", "den", "Mag\u00b7net"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADV", "ADV", "PPER", "ART", "NN"], "meter": "-+--+-", "measure": "amphibrach.di.relaxed"}, "line.5": {"text": "Verliebter Hertzen nach sich dreht:", "tokens": ["Ver\u00b7lieb\u00b7ter", "Hert\u00b7zen", "nach", "sich", "dreht", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "APPR", "PRF", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Es steckt verborgne Macht", "tokens": ["Es", "steckt", "ver\u00b7borg\u00b7ne", "Macht"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ADJA", "NN"], "meter": "-+-+-+", "measure": "iambic.tri"}, "line.7": {"text": "In schwartzer Augen Mitternacht.", "tokens": ["In", "schwart\u00b7zer", "Au\u00b7gen", "Mit\u00b7ter\u00b7nacht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}}}}