{"dta.poem.18180": {"metadata": {"author": {"name": "Ebeling, Johann Justus", "birth": "N.A.", "death": "N.A."}, "title": "Die Menschen w\u00fcnschen oft et-  \n was das sie nicht wollen.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1747", "urn": "urn:nbn:de:kobv:b4-200905198805", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Als ich mich, was der Mensch, einst in Ge-", "tokens": ["Als", "ich", "mich", ",", "was", "der", "Mensch", ",", "einst", "in", "Ge"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KOUS", "PPER", "PRF", "$,", "PRELS", "ART", "NN", "$,", "ADV", "APPR", "TRUNC"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.2": {"text": "danken frug,", "tokens": ["dan\u00b7ken", "frug", ","], "token_info": ["word", "word", "punct"], "pos": ["PAV", "VVFIN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.3": {"text": "Fiel mir die Antwort ein: Er ist ein", "tokens": ["Fiel", "mir", "die", "Ant\u00b7wort", "ein", ":", "Er", "ist", "ein"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "PTKVZ", "$.", "PPER", "VAFIN", "ART"], "meter": "+--+--+-+", "measure": "dactylic.di.plus"}, "line.4": {"text": "Wiederspruch,", "tokens": ["Wie\u00b7der\u00b7spruch", ","], "token_info": ["word", "punct"], "pos": ["NN", "$,"], "meter": "+-+", "measure": "trochaic.di"}, "line.5": {"text": "Der lebt und etwas will, das er doch", "tokens": ["Der", "lebt", "und", "et\u00b7was", "will", ",", "das", "er", "doch"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "PIS", "VMFIN", "$,", "PRELS", "PPER", "ADV"], "meter": "-+-+-+--+", "measure": "iambic.tetra.chol"}, "line.6": {"text": "nicht begehret,", "tokens": ["nicht", "be\u00b7ge\u00b7hret", ","], "token_info": ["word", "word", "punct"], "pos": ["PTKNEG", "VVFIN", "$,"], "meter": "+--+", "measure": "iambic.di.chol"}, "line.7": {"text": "Wenn ihm Gelegenheit, das was er w\u00fcnscht, be-", "tokens": ["Wenn", "ihm", "Ge\u00b7le\u00b7gen\u00b7heit", ",", "das", "was", "er", "w\u00fcnscht", ",", "be"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct", "word"], "pos": ["KOUS", "PPER", "NN", "$,", "PRELS", "PWS", "PPER", "VVFIN", "$,", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.8": {"text": "scheret.", "tokens": ["sche\u00b7ret", "."], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Wie mancher w\u00fcnschet sich, das Ende aller Noth,", "tokens": ["Wie", "man\u00b7cher", "w\u00fcn\u00b7schet", "sich", ",", "das", "En\u00b7de", "al\u00b7ler", "Noth", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "$,", "ART", "NN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "Wenn ihm ein Ungl\u00fck plagt, den f\u00fcrchterlichen Todt;", "tokens": ["Wenn", "ihm", "ein", "Un\u00b7gl\u00fck", "plagt", ",", "den", "f\u00fcrch\u00b7ter\u00b7li\u00b7chen", "Todt", ";"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ART", "NN", "VVFIN", "$,", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.11": {"text": "Und wenn derselbe komt; so will er gern entfliehen,", "tokens": ["Und", "wenn", "der\u00b7sel\u00b7be", "komt", ";", "so", "will", "er", "gern", "ent\u00b7flie\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "PDAT", "VVFIN", "$.", "ADV", "VMFIN", "PPER", "ADV", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Und sich des Schiksals Macht, fast mit Gewalt", "tokens": ["Und", "sich", "des", "Schik\u00b7sals", "Macht", ",", "fast", "mit", "Ge\u00b7walt"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["KON", "PRF", "ART", "NN", "NN", "$,", "ADV", "APPR", "NN"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.13": {"text": "entziehen.", "tokens": ["ent\u00b7zie\u00b7hen", "."], "token_info": ["word", "punct"], "pos": ["VVINF", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.14": {"text": "Wie mancher w\u00fcnschet sich die Herrlichkeit zu sehn,", "tokens": ["Wie", "man\u00b7cher", "w\u00fcn\u00b7schet", "sich", "die", "Herr\u00b7lich\u00b7keit", "zu", "sehn", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PIS", "VVFIN", "PRF", "ART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.15": {"text": "Des wesentlichen ", "tokens": ["Des", "we\u00b7sent\u00b7li\u00b7chen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+---", "measure": "dactylic.init"}, "line.16": {"text": "Den seelgen ", "tokens": ["Den", "seel\u00b7gen"], "token_info": ["word", "word"], "pos": ["ART", "NN"], "meter": "-+-", "measure": "amphibrach.single"}, "line.17": {"text": "erf\u00fcllen,", "tokens": ["er\u00b7f\u00fcl\u00b7len", ","], "token_info": ["word", "punct"], "pos": ["VVINF", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.18": {"text": "Die Sehnsucht die er zeigt, in seinen Wunsch und", "tokens": ["Die", "Sehn\u00b7sucht", "die", "er", "zeigt", ",", "in", "sei\u00b7nen", "Wunsch", "und"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "NN", "ART", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "KON"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.19": {"text": "Willen.", "tokens": ["Wil\u00b7len", "."], "token_info": ["word", "punct"], "pos": ["NN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.20": {"text": "Gott ist uns unsichtbar, das weis ein jeder woll,", "tokens": ["Gott", "ist", "uns", "un\u00b7sicht\u00b7bar", ",", "das", "weis", "ein", "je\u00b7der", "woll", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "VAFIN", "PPER", "ADJD", "$,", "PRELS", "ADV", "ART", "PIS", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.21": {"text": "Und wenn ein sterblich Aug denselben sehen soll:", "tokens": ["Und", "wenn", "ein", "sterb\u00b7lich", "Aug", "den\u00b7sel\u00b7ben", "se\u00b7hen", "soll", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "KOUS", "ART", "ADJD", "NN", "PDS", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.22": {"text": "So mu\u00df er sichtbarlich die Vollenkommenheiten,", "tokens": ["So", "mu\u00df", "er", "sicht\u00b7bar\u00b7lich", "die", "Vol\u00b7len\u00b7kom\u00b7men\u00b7hei\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VMFIN", "PPER", "PRF", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.23": {"text": "Als Strahlen seines Lichts, in seinem Werk aus-", "tokens": ["Als", "Strah\u00b7len", "sei\u00b7nes", "Lichts", ",", "in", "sei\u00b7nem", "Werk", "aus"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["KOUS", "NN", "PPOSAT", "NN", "$,", "APPR", "PPOSAT", "NN", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.24": {"text": "breiten.", "tokens": ["brei\u00b7ten", "."], "token_info": ["word", "punct"], "pos": ["ADJA", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.25": {"text": "Das hat er ja gethan: Jhm zeigt die ganze Welt,", "tokens": ["Das", "hat", "er", "ja", "ge\u00b7than", ":", "Jhm", "zeigt", "die", "gan\u00b7ze", "Welt", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VAFIN", "PPER", "ADV", "VVPP", "$.", "PPER", "VVFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.26": {"text": "Die ", "tokens": ["Die"], "token_info": ["word"], "pos": ["ART"], "meter": "-", "measure": "single.down"}, "line.27": {"text": "Wer sieht die Kreatur woran er abgedr\u00fckket,", "tokens": ["Wer", "sieht", "die", "Kre\u00b7a\u00b7tur", "wo\u00b7ran", "er", "ab\u00b7ge\u00b7dr\u00fck\u00b7ket", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VVFIN", "ART", "NN", "PWAV", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.28": {"text": "Darin man seine Gr\u00f6\u00df, als Spiegeln, klar erblikket", "tokens": ["Da\u00b7rin", "man", "sei\u00b7ne", "Gr\u00f6\u00df", ",", "als", "Spie\u00b7geln", ",", "klar", "er\u00b7blik\u00b7ket"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word"], "pos": ["PAV", "PIS", "PPOSAT", "NN", "$,", "KOUS", "NN", "$,", "ADJD", "VVFIN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.29": {"text": "In solcher Absicht an? die wenigsten thun das,", "tokens": ["In", "sol\u00b7cher", "Ab\u00b7sicht", "an", "?", "die", "we\u00b7nigs\u00b7ten", "thun", "das", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPR", "PIAT", "NN", "PTKVZ", "$.", "ART", "PIS", "VVINF", "PDS", "$,"], "meter": "-+-+-+-+--+-", "measure": "iambic.penta.relaxed"}, "line.30": {"text": "Sie leben in der Welt, sie sehn ohn Unterla\u00df", "tokens": ["Sie", "le\u00b7ben", "in", "der", "Welt", ",", "sie", "sehn", "ohn", "Un\u00b7ter\u00b7la\u00df"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "APPR", "ART", "NN", "$,", "PPER", "VVFIN", "APPR", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.31": {"text": "Derselben Sch\u00f6nheit an, und denken nur dabei,", "tokens": ["Der\u00b7sel\u00b7ben", "Sch\u00f6n\u00b7heit", "an", ",", "und", "den\u00b7ken", "nur", "da\u00b7bei", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "PTKVZ", "$,", "KON", "VVFIN", "ADV", "PAV", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.32": {"text": "Da\u00df dieses alles da, dieweil es ist so sei.", "tokens": ["Da\u00df", "die\u00b7ses", "al\u00b7les", "da", ",", "die\u00b7weil", "es", "ist", "so", "sei", "."], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PDAT", "PIS", "ADV", "$,", "KOUS", "PPER", "VAFIN", "ADV", "VAFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.33": {"text": "W\u00e4r ihr begierger Wunsch so br\u00fcnstig als man meinet:", "tokens": ["W\u00e4r", "ihr", "be\u00b7gier\u00b7ger", "Wunsch", "so", "br\u00fcns\u00b7tig", "als", "man", "mei\u00b7net", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "ADJA", "NN", "ADV", "ADJD", "KOKOM", "PIS", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.34": {"text": "So suchten sie ihn da, wo uns sein Bild erscheinet.", "tokens": ["So", "such\u00b7ten", "sie", "ihn", "da", ",", "wo", "uns", "sein", "Bild", "er\u00b7schei\u00b7net", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "PPER", "ADV", "$,", "PWAV", "PPER", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.35": {"text": "Ja! Menschen! sagt mir nicht; das ist ein falscher", "tokens": ["Ja", "!", "Men\u00b7schen", "!", "sagt", "mir", "nicht", ";", "das", "ist", "ein", "fal\u00b7scher"], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PTKANT", "$.", "NN", "$.", "VVFIN", "PPER", "PTKNEG", "$.", "PDS", "VAFIN", "ART", "ADJA"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.36": {"text": "Schlus", "tokens": ["Schlus"], "token_info": ["word"], "pos": ["NE"], "meter": "+", "measure": "single.up"}, "line.37": {"text": "Jhr wolt die Quelle sehn, ich weise euch den Flus", "tokens": ["Ihr", "wolt", "die", "Quel\u00b7le", "sehn", ",", "ich", "wei\u00b7se", "euch", "den", "Flus"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "PPER", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.38": {"text": "Der nur daraus herstammt; Ich will nicht wieder-", "tokens": ["Der", "nur", "da\u00b7raus", "her\u00b7stammt", ";", "Ich", "will", "nicht", "wie\u00b7der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ART", "ADV", "PAV", "VVFIN", "$.", "PPER", "VMFIN", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.39": {"text": "streben,", "tokens": ["stre\u00b7ben", ","], "token_info": ["word", "punct"], "pos": ["VVFIN", "$,"], "meter": "+-", "measure": "trochaic.single"}, "line.40": {"text": "Ich will euch lieber noch dies zum Beweise geben:", "tokens": ["Ich", "will", "euch", "lie\u00b7ber", "noch", "dies", "zum", "Be\u00b7wei\u00b7se", "ge\u00b7ben", ":"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "PPER", "ADV", "ADV", "PDS", "APPRART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.41": {"text": "Wer einen Endzwek w\u00fcnscht, der nimmt die Mit-", "tokens": ["Wer", "ei\u00b7nen", "End\u00b7zwek", "w\u00fcnscht", ",", "der", "nimmt", "die", "Mit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWS", "ART", "NN", "VVFIN", "$,", "PRELS", "VVFIN", "ART", "TRUNC"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.42": {"text": "tel an;", "tokens": ["tel", "an", ";"], "token_info": ["word", "word", "punct"], "pos": ["ADJD", "PTKVZ", "$."], "meter": "--", "measure": "unknown.measure.zero"}, "line.43": {"text": "Jhr wolt die Gottheit sehn, die keiner sehen kan,", "tokens": ["Ihr", "wolt", "die", "Got\u00b7theit", "sehn", ",", "die", "kei\u00b7ner", "se\u00b7hen", "kan", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ART", "NN", "VVINF", "$,", "PRELS", "PIS", "VVINF", "VMFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.44": {"text": "Als wer sich recht bestrebt ins Himmelreich zu gehen,", "tokens": ["Als", "wer", "sich", "recht", "be\u00b7strebt", "ins", "Him\u00b7mel\u00b7reich", "zu", "ge\u00b7hen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PWS", "PRF", "ADJD", "VVFIN", "APPRART", "NN", "PTKZU", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.45": {"text": "Wo mit verkl\u00e4rten Aug sein ewigs Licht zu sehen.", "tokens": ["Wo", "mit", "ver\u00b7kl\u00e4r\u00b7ten", "Aug", "sein", "e\u00b7wigs", "Licht", "zu", "se\u00b7hen", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "APPR", "ADJA", "NN", "PPOSAT", "ADJA", "NN", "PTKZU", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.46": {"text": "Ist euer Wandel stets nach diesem Ziel bem\u00fcht,", "tokens": ["Ist", "eu\u00b7er", "Wan\u00b7del", "stets", "nach", "die\u00b7sem", "Ziel", "be\u00b7m\u00fcht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPOSAT", "NN", "ADV", "APPR", "PDAT", "NN", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.47": {"text": "Dabei ihr euch der Welt, und ihrer Lust entzieht?", "tokens": ["Da\u00b7bei", "ihr", "euch", "der", "Welt", ",", "und", "ih\u00b7rer", "Lust", "ent\u00b7zieht", "?"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PPER", "PRF", "ART", "NN", "$,", "KON", "PPOSAT", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.48": {"text": "Pr\u00fcft eur Gewissen recht, und last ein Urtheil f\u00e4llen;", "tokens": ["Pr\u00fcft", "eur", "Ge\u00b7wis\u00b7sen", "recht", ",", "und", "last", "ein", "Ur\u00b7theil", "f\u00e4l\u00b7len", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPOSAT", "NN", "ADJD", "$,", "KON", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.49": {"text": "So wird die Warheit klar von diesem Saz erhellen.", "tokens": ["So", "wird", "die", "War\u00b7heit", "klar", "von", "die\u00b7sem", "Saz", "er\u00b7hel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "ART", "NN", "ADJD", "APPR", "PDAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.50": {"text": "Jhr w\u00fcnscht und wolt doch nicht; ihr geht nach", "tokens": ["Ihr", "w\u00fcnscht", "und", "wolt", "doch", "nicht", ";", "ihr", "geht", "nach