{"dta.poem.17704": {"metadata": {"author": {"name": "Silesius, Angelus", "birth": "N.A.", "death": "N.A."}, "title": "326.  Unterschiedliche Gelegenheit der Seele.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1675", "urn": "urn:nbn:de:kobv:b4-20008-3", "language": ["de:0.99"], "booktitle": "Silesius, Angelus: Cherubinischer Wandersmann oder Geist-Reiche Sinn- und Schlu\u00df-Reime. 2. Aufl. Glatz, 1675."}, "poem": {"stanza.1": {"line.1": {"text": "De\u00df S\u00fcnders Seele ligt/ de\u00df B\u00fcssers richt sich auf/", "tokens": ["De\u00df", "S\u00fcn\u00b7ders", "See\u00b7le", "ligt", "/", "de\u00df", "B\u00fcs\u00b7sers", "richt", "sich", "auf", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "NN", "VVFIN", "$(", "ART", "NN", "VVFIN", "PRF", "APPR", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und de\u00df Gerechten steht geschikt zum Tugend lauf.", "tokens": ["Und", "de\u00df", "Ge\u00b7rech\u00b7ten", "steht", "ge\u00b7schikt", "zum", "Tu\u00b7gend", "lauf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ART", "NN", "VVFIN", "VVPP", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}