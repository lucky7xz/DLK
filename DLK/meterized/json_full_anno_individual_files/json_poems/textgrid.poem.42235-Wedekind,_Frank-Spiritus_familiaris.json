{"textgrid.poem.42235": {"metadata": {"author": {"name": "Wedekind, Frank", "birth": "N.A.", "death": "N.A."}, "title": "Spiritus familiaris", "genre": "verse", "period": "N.A.", "pub_year": 1891, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Eine schwarze Katze kauert vor meiner T\u00fcr,", "tokens": ["Ei\u00b7ne", "schwar\u00b7ze", "Kat\u00b7ze", "kau\u00b7ert", "vor", "mei\u00b7ner", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eine kleine, schwarze, kurzgeschorene Katze;", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", ",", "schwar\u00b7ze", ",", "kurz\u00b7ge\u00b7scho\u00b7re\u00b7ne", "Kat\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Ich komme nach Hause, und mit einem Satze,", "tokens": ["Ich", "kom\u00b7me", "nach", "Hau\u00b7se", ",", "und", "mit", "ei\u00b7nem", "Sat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Wie ich aufschlie\u00dfe, springt sie herein zu mir.", "tokens": ["Wie", "ich", "auf\u00b7schlie\u00b7\u00dfe", ",", "springt", "sie", "her\u00b7ein", "zu", "mir", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "PPER", "VVFIN", "$,", "VVFIN", "PPER", "PTKVZ", "APPR", "PPER", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}}, "stanza.2": {"line.1": {"text": "Was will die kleine, schwarze Katze bei mir?", "tokens": ["Was", "will", "die", "klei\u00b7ne", ",", "schwar\u00b7ze", "Kat\u00b7ze", "bei", "mir", "?"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VMFIN", "ART", "ADJA", "$,", "ADJA", "NN", "APPR", "PPER", "$."], "meter": "-+-+-+-+--+", "measure": "iambic.penta.chol"}, "line.2": {"text": "W\u00e4r es ein H\u00fcndchen, ich w\u00fc\u00dfte es zu verstehen;", "tokens": ["W\u00e4r", "es", "ein", "H\u00fcnd\u00b7chen", ",", "ich", "w\u00fc\u00df\u00b7te", "es", "zu", "ver\u00b7ste\u00b7hen", ";"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "NN", "$,", "PPER", "VVFIN", "PPER", "PTKZU", "VVINF", "$."], "meter": "---+--+--+-+-", "measure": "iambic.tetra.relaxed"}, "line.3": {"text": "Ein Frauenh\u00fcndchen, ich wei\u00df damit umzugehen.", "tokens": ["Ein", "Frau\u00b7en\u00b7h\u00fcnd\u00b7chen", ",", "ich", "wei\u00df", "da\u00b7mit", "um\u00b7zu\u00b7ge\u00b7hen", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "PPER", "VVFIN", "PAV", "VVIZU", "$."], "meter": "-+-+--+-+--+-", "measure": "iambic.penta.relaxed"}, "line.4": {"text": "Die Katze ist mir ein v\u00f6llig fremdes Tier", "tokens": ["Die", "Kat\u00b7ze", "ist", "mir", "ein", "v\u00f6l\u00b7lig", "frem\u00b7des", "Tier"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN"], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.3": {"line.1": {"text": "Sie ist die Seele von meinem ", "tokens": ["Sie", "ist", "die", "See\u00b7le", "von", "mei\u00b7nem"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "ART", "NN", "APPR", "PPOSAT"], "meter": "-+-+--+-", "measure": "iambic.tri.relaxed"}, "line.2": {"text": "Die schwarze Katze kommt zu mir hereingesprungen,", "tokens": ["Die", "schwar\u00b7ze", "Kat\u00b7ze", "kommt", "zu", "mir", "her\u00b7ein\u00b7ge\u00b7sprun\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Weil sie doch irgendwo \u00fcbernachten mu\u00df.", "tokens": ["Weil", "sie", "doch", "ir\u00b7gend\u00b7wo", "\u00fc\u00b7bern\u00b7ach\u00b7ten", "mu\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "ADV", "ADV", "VVINF", "VMFIN", "$."], "meter": "-+-+--+-+-+", "measure": "iambic.penta.relaxed"}}, "stanza.4": {"line.1": {"text": "Eine schwarze Katze kauert vor meiner T\u00fcr,", "tokens": ["Ei\u00b7ne", "schwar\u00b7ze", "Kat\u00b7ze", "kau\u00b7ert", "vor", "mei\u00b7ner", "T\u00fcr", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+--+-+", "measure": "trochaic.hexa.relaxed"}, "line.2": {"text": "Eine kleine, schwarze, kurzgeschorene Katze;", "tokens": ["Ei\u00b7ne", "klei\u00b7ne", ",", "schwar\u00b7ze", ",", "kurz\u00b7ge\u00b7scho\u00b7re\u00b7ne", "Kat\u00b7ze", ";"], "token_info": ["word", "word", "punct", "word", "punct", "word", "word", "punct"], "pos": ["ART", "ADJA", "$,", "VVFIN", "$,", "ADJA", "NN", "$."], "meter": "+-+-+-+-+--+-", "measure": "hexameter"}, "line.3": {"text": "Ich komme nach Hause, und mit einem Satze,", "tokens": ["Ich", "kom\u00b7me", "nach", "Hau\u00b7se", ",", "und", "mit", "ei\u00b7nem", "Sat\u00b7ze", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPR", "NN", "$,", "KON", "APPR", "ART", "NN", "$,"], "meter": "-+--+-+-+-+-", "measure": "iambic.penta.relaxed"}, "line.4": {