{"dta.poem.16151": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "205.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1837", "urn": "urn:nbn:de:kobv:b4-200905195088", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Es ist ein alter Spruch: Rei\u00df ein dein altes Haus,", "tokens": ["Es", "ist", "ein", "al\u00b7ter", "Spruch", ":", "Rei\u00df", "ein", "dein", "al\u00b7tes", "Haus", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ART", "ADJA", "NN", "$.", "VVFIN", "ART", "PPOSAT", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "So findest du den Schatz, und baust ein neues draus.", "tokens": ["So", "fin\u00b7dest", "du", "den", "Schatz", ",", "und", "baust", "ein", "neu\u00b7es", "draus", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PPER", "ART", "NN", "$,", "KON", "VVFIN", "ART", "ADJA", "PTKVZ", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Was ist damit gemeint? die ernstliche Belehrung:", "tokens": ["Was", "ist", "da\u00b7mit", "ge\u00b7meint", "?", "die", "ernst\u00b7li\u00b7che", "Be\u00b7leh\u00b7rung", ":"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PAV", "VVPP", "$.", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Bekehrung gr\u00fcndliche, verkehrten Sinns Umkehrung.", "tokens": ["Be\u00b7keh\u00b7rung", "gr\u00fcnd\u00b7li\u00b7che", ",", "ver\u00b7kehr\u00b7ten", "Sinns", "Um\u00b7keh\u00b7rung", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "VVFIN", "$,", "ADJA", "NN", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "An alt bauf\u00e4lligem Geb\u00e4ude hilft kein Flicken,", "tokens": ["An", "alt", "bau\u00b7f\u00e4l\u00b7li\u00b7gem", "Ge\u00b7b\u00e4u\u00b7de", "hilft", "kein", "Fli\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJD", "ADJA", "NN", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Zum morschen Balken wird kein derber Stein sich schicken.", "tokens": ["Zum", "mor\u00b7schen", "Bal\u00b7ken", "wird", "kein", "der\u00b7ber", "Stein", "sich", "schi\u00b7cken", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VAFIN", "PIAT", "ADJA", "NN", "PRF", "VVINF", "$."], "meter": "+--+--+--+-+-", "measure": "dactylic.tri.plus"}}, "stanza.4": {"line.1": {"text": "Du magst hier einen Klaff, dort einen Sprung verkleben,", "tokens": ["Du", "magst", "hier", "ei\u00b7nen", "Klaff", ",", "dort", "ei\u00b7nen", "Sprung", "ver\u00b7kle\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VMFIN", "ADV", "ART", "NN", "$,", "ADV", "ART", "NN", "VVPP", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Stets wird ob deinem Haupt der Einsturz drohend schweben.", "tokens": ["Stets", "wird", "ob", "dei\u00b7nem", "Haupt", "der", "Ein\u00b7sturz", "dro\u00b7hend", "schwe\u00b7ben", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "KOUS", "PPOSAT", "NN", "ART", "NN", "VVPP", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.5": {"line.1": {"text": "Drum fa\u00df ein stark Vertraun, la\u00df dir vorm Schutt nicht graun,", "tokens": ["Drum", "fa\u00df", "ein", "stark", "Ver\u00b7traun", ",", "la\u00df", "dir", "vorm", "Schutt", "nicht", "graun", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "VVFIN", "ART", "ADJD", "VVINF", "$,", "VVIMP", "PPER", "APPRART", "NN", "PTKNEG", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und bau von Grund-auf neu, was nicht ist umzubaun.", "tokens": ["Und", "bau", "von", "Grun\u00b7dauf", "neu", ",", "was", "nicht", "ist", "um\u00b7zu\u00b7baun", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "APPR", "NN", "ADJD", "$,", "PRELS", "PTKNEG", "VAFIN", "VVIZU", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}, "stanza.6": {"line.1": {"text": "Der aber ist begl\u00fcckt, wer stets, zur rechten Zeit", "tokens": ["Der", "a\u00b7ber", "ist", "be\u00b7gl\u00fcckt", ",", "wer", "stets", ",", "zur", "rech\u00b7ten", "Zeit"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "ADV", "VAFIN", "VVPP", "$,", "PWS", "ADV", "$,", "APPRART", "ADJA", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Nachhelfend, hielt sein Haus im Stand der Baulichkeit.", "tokens": ["Nach\u00b7hel\u00b7fend", ",", "hielt", "sein", "Haus", "im", "Stand", "der", "Bau\u00b7lich\u00b7keit", "."], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVPP", "$,", "VVFIN", "PPOSAT", "NN", "APPRART", "NN", "ART", "NN", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}