{"textgrid.poem.44548": {"metadata": {"author": {"name": "Grillparzer, Franz", "birth": "N.A.", "death": "N.A."}, "title": "Der Christbaum f\u00fcr die Kinder im k.k. Wiener Milit\u00e4r-Invalidenhause 1846", "genre": "verse", "period": "N.A.", "pub_year": 1846, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Die ihr versammelt hier nach frommer Sitte,", "tokens": ["Die", "ihr", "ver\u00b7sam\u00b7melt", "hier", "nach", "from\u00b7mer", "Sit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PPER", "VVFIN", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Gar mancher nennt euch arm \u2013 ihr seid nicht reich!", "tokens": ["Gar", "man\u00b7cher", "nennt", "euch", "arm", "\u2013", "ihr", "seid", "nicht", "reich", "!"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "PIS", "VVFIN", "PPER", "ADJD", "$(", "PPER", "VAFIN", "PTKNEG", "ADJD", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und habt doch einen Christbaum in der Mitte,", "tokens": ["Und", "habt", "doch", "ei\u00b7nen", "Christ\u00b7baum", "in", "der", "Mit\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VAFIN", "ADV", "ART", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Kindern reicher Menschen heute gleich.", "tokens": ["Den", "Kin\u00b7dern", "rei\u00b7cher", "Men\u00b7schen", "heu\u00b7te", "gleich", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADJD", "NN", "ADV", "ADV", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.2": {"line.1": {"text": "Das macht: Gott gibt nicht stets mit eignen H\u00e4nden,", "tokens": ["Das", "macht", ":", "Gott", "gibt", "nicht", "stets", "mit", "eig\u00b7nen", "H\u00e4n\u00b7den", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$.", "NN", "VVFIN", "PTKNEG", "ADV", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er borgt zum Geben oft die fremde Hand,", "tokens": ["Er", "borgt", "zum", "Ge\u00b7ben", "oft", "die", "frem\u00b7de", "Hand", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "APPRART", "NN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "L\u00e4\u00dft andere verteilen seine Spenden,", "tokens": ["L\u00e4\u00dft", "an\u00b7de\u00b7re", "ver\u00b7tei\u00b7len", "sei\u00b7ne", "Spen\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Den Bruder, hinter dem der Vater stand.", "tokens": ["Den", "Bru\u00b7der", ",", "hin\u00b7ter", "dem", "der", "Va\u00b7ter", "stand", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "$,", "APPR", "ART", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.3": {"line.1": {"text": "Und schafft so nicht nur Freudige, auch Gute,", "tokens": ["Und", "schafft", "so", "nicht", "nur", "Freu\u00b7di\u00b7ge", ",", "auch", "Gu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADV", "PTKNEG", "ADV", "NN", "$,", "ADV", "NN", "$,"], "meter": "-+-+-+---+-", "measure": "unknown.measure.tetra"}, "line.2": {"text": "\u2013 Denn Zufall scheint, was frei vom Himmel sank \u2013", "tokens": ["\u2013", "Denn", "Zu\u00b7fall", "scheint", ",", "was", "frei", "vom", "Him\u00b7mel", "sank", "\u2013"], "token_info": ["punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "KON", "NN", "VVFIN", "$,", "PRELS", "ADJD", "APPRART", "NN", "VVFIN", "$("], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Macht gl\u00fccklich zwei und voll von edlem Mute,", "tokens": ["Macht", "gl\u00fcck\u00b7lich", "zwei", "und", "voll", "von", "ed\u00b7lem", "Mu\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADJD", "CARD", "KON", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Teilt das Gef\u00fchl in Wohltat und in Dank.", "tokens": ["Teilt", "das", "Ge\u00b7f\u00fchl", "in", "Wohl\u00b7tat", "und", "in", "Dank", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ART", "NN", "APPR", "NN", "KON", "APPR", "NN", "$."], "meter": "+--+-+-+-+", "measure": "iambic.penta.invert"}}, "stanza.4": {"line.1": {"text": "So hat f\u00fcr euch, die Kinder wackrer Krieger,", "tokens": ["So", "hat", "f\u00fcr", "euch", ",", "die", "Kin\u00b7der", "wack\u00b7rer", "Krie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "APPR", "PPER", "$,", "ART", "NN", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Ein Kriegersohn wie ihr, und darin gleich,", "tokens": ["Ein", "Krie\u00b7ger\u00b7sohn", "wie", "ihr", ",", "und", "da\u00b7rin", "gleich", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "NN", "KOKOM", "PPER", "$,", "KON", "PAV", "ADV", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Der Sohn des Helden, der bei Leipzig Sieger,", "tokens": ["Der", "Sohn", "des", "Hel\u00b7den", ",", "der", "bei", "Leip\u00b7zig", "Sie\u00b7ger", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ART", "NN", "$,", "PRELS", "APPR", "NE", "NN", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Die Fr\u00fcchte mancher M\u00fchn bestimmt f\u00fcr euch.", "tokens": ["Die", "Fr\u00fcch\u00b7te", "man\u00b7cher", "M\u00fchn", "be\u00b7stimmt", "f\u00fcr", "euch", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "PIAT", "NN", "VVFIN", "APPR", "PPER", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.5": {"line.1": {"text": "Was er gesehn, erstrebt, getan, gelitten,", "tokens": ["Was", "er", "ge\u00b7sehn", ",", "er\u00b7strebt", ",", "ge\u00b7tan", ",", "ge\u00b7lit\u00b7ten", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct", "word", "punct"], "pos": ["PWS", "PPER", "VVPP", "$,", "VVFIN", "$,", "VVPP", "$,", "VVPP", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Er gibts der Welt, des Volkes Neugier preis", "tokens": ["Er", "gibts", "der", "Welt", ",", "des", "Vol\u00b7kes", "Neu\u00b7gier", "preis"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "ART", "NN", "$,", "ART", "NN", "NN", "PTKVZ"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Und hat f\u00fcr sich als einzgen Lohn erstritten", "tokens": ["Und", "hat", "f\u00fcr", "sich", "als", "einz\u00b7gen", "Lohn", "er\u00b7strit\u00b7ten"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["KON", "VAFIN", "APPR", "PRF", "KOUS", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Hier diesen Christbaum, dieses Tannenreis!", "tokens": ["Hier", "die\u00b7sen", "Christ\u00b7baum", ",", "die\u00b7ses", "Tan\u00b7nen\u00b7reis", "!"], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["ADV", "PDAT", "NN", "$,", "PDAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.6": {"line.1": {"text": "Folgt ihm die Wohltat nun auf seinen Wegen", "tokens": ["Folgt", "ihm", "die", "Wohl\u00b7tat", "nun", "auf", "sei\u00b7nen", "We\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "Und st\u00e4rkt ihn, wie ein feurig edler Trank,", "tokens": ["Und", "st\u00e4rkt", "ihn", ",", "wie", "ein", "feu\u00b7rig", "ed\u00b7ler", "Trank", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "$,", "PWAV", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Nehmt ihr den zweiten Teil von Gottes Segen", "tokens": ["Nehmt", "ihr", "den", "zwei\u00b7ten", "Teil", "von", "Got\u00b7tes", "Se\u00b7gen"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ART", "ADJA", "NN", "APPR", "NN", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Und ehrt, was man euch gab, durch euern Dank.", "tokens": ["Und", "ehrt", ",", "was", "man", "euch", "gab", ",", "durch", "eu\u00b7ern", "Dank", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "$,", "PRELS", "PIS", "PPER", "VVFIN", "$,", "APPR", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}}, "stanza.7": {"line.1": {"text": "Die ihr versammelt hier nach frommer Sitte,", "tokens": ["Die", "ihr", "ver\u00b7sam\u00b7melt", "hier", "nach", "from\u00b