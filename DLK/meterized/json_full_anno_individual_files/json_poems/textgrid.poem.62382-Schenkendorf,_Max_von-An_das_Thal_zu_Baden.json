{"textgrid.poem.62382": {"metadata": {"author": {"name": "Schenkendorf, Max von", "birth": "N.A.", "death": "N.A."}, "title": "An das Thal zu Baden", "genre": "verse", "period": "N.A.", "pub_year": 1800, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Schm\u00fccke dich mit Laub und Bl\u00fcten,", "tokens": ["Schm\u00fc\u00b7cke", "dich", "mit", "Laub", "und", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein geliebtes, sch\u00f6nes Thal,", "tokens": ["Mein", "ge\u00b7lieb\u00b7tes", ",", "sch\u00f6\u00b7nes", "Thal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zartes Leben zu beh\u00fcten", "tokens": ["Zar\u00b7tes", "Le\u00b7ben", "zu", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor zu hei\u00dfem Sonnenstrahl.", "tokens": ["Vor", "zu", "hei\u00b7\u00dfem", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.2": {"line.1": {"text": "Durch den Fr\u00fchling hergetragen,", "tokens": ["Durch", "den", "Fr\u00fch\u00b7ling", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt ein liebes, frommes Kind,", "tokens": ["Kommt", "ein", "lie\u00b7bes", ",", "from\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "ART", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Engel f\u00fchren seinen Wagen,", "tokens": ["En\u00b7gel", "f\u00fch\u00b7ren", "sei\u00b7nen", "Wa\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Und es weht ein lauer Wind.", "tokens": ["Und", "es", "weht", "ein", "lau\u00b7er", "Wind", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.3": {"line.1": {"text": "Weht es schmeichelnd an, ihr L\u00fcfte,", "tokens": ["Weht", "es", "schmei\u00b7chelnd", "an", ",", "ihr", "L\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ADJD", "PTKVZ", "$,", "PPOSAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "St\u00e4rket Sinne, Geist und Muth,", "tokens": ["St\u00e4r\u00b7ket", "Sin\u00b7ne", ",", "Geist", "und", "Muth", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Ihr des Weinstocks zarte D\u00fcfte,", "tokens": ["Ihr", "des", "Wein\u00b7stocks", "zar\u00b7te", "D\u00fcf\u00b7te", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Du der Rose keusche Glut.", "tokens": ["Du", "der", "Ro\u00b7se", "keu\u00b7sche", "Glut", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.4": {"line.1": {"text": "An dem Br\u00fcnnlein, an den B\u00e4chen", "tokens": ["An", "dem", "Br\u00fcnn\u00b7lein", ",", "an", "den", "B\u00e4\u00b7chen"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["APPR", "ART", "NN", "$,", "APPR", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geht es, an dem Wasserfall,", "tokens": ["Geht", "es", ",", "an", "dem", "Was\u00b7ser\u00b7fall", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "$,", "APPR", "ART", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Mag sich oft und gern besprechen", "tokens": ["Mag", "sich", "oft", "und", "gern", "be\u00b7spre\u00b7chen"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["VMFIN", "PRF", "ADV", "KON", "ADV", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Mit dem leisen Wiederhall.", "tokens": ["Mit", "dem", "lei\u00b7sen", "Wie\u00b7der\u00b7hall", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.5": {"line.1": {"text": "Gebt ihr denn in allen T\u00f6nen,", "tokens": ["Gebt", "ihr", "denn", "in", "al\u00b7len", "T\u00f6\u00b7nen", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "PPER", "ADV", "APPR", "PIAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Geister, Segen und Geleit;", "tokens": ["Geis\u00b7ter", ",", "Se\u00b7gen", "und", "Ge\u00b7leit", ";"], "token_info": ["word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "KON", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Allem Gro\u00dfen, Guten, Sch\u00f6nen", "tokens": ["Al\u00b7lem", "Gro\u00b7\u00dfen", ",", "Gu\u00b7ten", ",", "Sch\u00f6\u00b7nen"], "token_info": ["word", "word", "punct", "word", "punct", "word"], "pos": ["PIAT", "NN", "$,", "NN", "$,", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Ist das fromme Herz geweiht.", "tokens": ["Ist", "das", "from\u00b7me", "Herz", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "ART", "ADJA", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.6": {"line.1": {"text": "Ritter, die in diesen Gauen,", "tokens": ["Rit\u00b7ter", ",", "die", "in", "die\u00b7sen", "Gau\u00b7en", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPR", "PDAT", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Einst ein treues Volk geschirmt,", "tokens": ["Einst", "ein", "treu\u00b7es", "Volk", "ge\u00b7schirmt", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ART", "ADJA", "NN", "VVPP", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Und ihr zarten, heil'gen Frauen,", "tokens": ["Und", "ihr", "zar\u00b7ten", ",", "heil'\u00b7gen", "Frau\u00b7en", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "punct"], "pos": ["KON", "PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Die der Kl\u00f6ster Bau geth\u00fcrmt.", "tokens": ["Die", "der", "Kl\u00f6s\u00b7ter", "Bau", "ge\u00b7th\u00fcrmt", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ART", "NN", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.7": {"line.1": {"text": "Sehet nun auf euren Pfaden", "tokens": ["Se\u00b7het", "nun", "auf", "eu\u00b7ren", "Pfa\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVFIN", "ADV", "APPR", "PPOSAT", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Alte Zucht und Fr\u00f6mmigkeit,", "tokens": ["Al\u00b7te", "Zucht", "und", "Fr\u00f6m\u00b7mig\u00b7keit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Rein und keusch den Himmelsgnaden", "tokens": ["Rein", "und", "keusch", "den", "Him\u00b7mels\u00b7gna\u00b7den"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["NN", "KON", "ADJD", "ART", "NN"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Wie dem Vaterland geweiht.", "tokens": ["Wie", "dem", "Va\u00b7ter\u00b7land", "ge\u00b7weiht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "NN", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.8": {"line.1": {"text": "Da\u00df sich euer Geist noch freue,", "tokens": ["Da\u00df", "sich", "eu\u00b7er", "Geist", "noch", "freu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PRF", "PPOSAT", "NN", "ADV", "VVFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Wo die ew'gen Kr\u00e4nze bl\u00fchn,", "tokens": ["Wo", "die", "ew'\u00b7gen", "Kr\u00e4n\u00b7ze", "bl\u00fchn", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PWAV", "ART", "ADJA", "NN", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seht an eurer Gruft die Treue,", "tokens": ["Seht", "an", "eu\u00b7rer", "Gruft", "die", "Treu\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "APPR", "PPOSAT", "NN", "ART", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lieb' und Demuth wieder knien!", "tokens": ["Lieb'", "und", "De\u00b7muth", "wie\u00b7der", "kni\u00b7en", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "ADV", "VVFIN", "$."], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}}, "stanza.9": {"line.1": {"text": "Warme Quellen, Wundergaben,", "tokens": ["War\u00b7me", "Quel\u00b7len", ",", "Wun\u00b7der\u00b7ga\u00b7ben", ","], "token_info": ["word", "word", "punct", "word", "punct"], "pos": ["ADJA", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Gottes reicher Segensflu\u00df,", "tokens": ["Got\u00b7tes", "rei\u00b7cher", "Se\u00b7gens\u00b7flu\u00df", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "ADJD", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Dieses Leben sollt ihr laben,", "tokens": ["Die\u00b7ses", "Le\u00b7ben", "sollt", "ihr", "la\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDAT", "NN", "VMFIN", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Bringt ihr der Gesundheit Gru\u00df.", "tokens": ["Bringt", "ihr", "der", "Ge\u00b7sund\u00b7heit", "Gru\u00df", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "NN", "$."], "meter": "--+-+-+", "measure": "anapaest.init"}}, "stanza.10": {"line.1": {"text": "S\u00fc\u00dfe Kost sollt ihr ihr geben,", "tokens": ["S\u00fc\u00b7\u00dfe", "Kost", "sollt", "ihr", "ihr", "ge\u00b7ben", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "VMFIN", "PPER", "PPER", "VVINF", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Fischlein, die im Bach sich freun,", "tokens": ["Fisc\u00b7hlein", ",", "die", "im", "Bach", "sich", "freun", ","], "token_info": ["word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "$,", "PRELS", "APPRART", "NN", "PRF", "VVINF", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Milch und Honig, Obst und Reben,", "tokens": ["Milch", "und", "Ho\u00b7nig", ",", "Obst", "und", "Re\u00b7ben", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Heil'ges Brod, und heil'gen Wein.", "tokens": ["Heil'\u00b7ges", "Brod", ",", "und", "heil'\u00b7gen", "Wein", "."], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADJA", "NN", "$,", "KON", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.11": {"line.1": {"text": "Berge, Th\u00e4ler, Wald und Aue,", "tokens": ["Ber\u00b7ge", ",", "Th\u00e4\u00b7ler", ",", "Wald", "und", "Au\u00b7e", ","], "token_info": ["word", "punct", "word", "punct", "word", "word", "word", "punct"], "pos": ["NN", "$,", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Du o s\u00fc\u00dfe Fr\u00fchlingszeit,", "tokens": ["Du", "o", "s\u00fc\u00b7\u00dfe", "Fr\u00fch\u00b7lings\u00b7zeit", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PPER", "FM", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Seid besprengt mit heil'gem Thaue,", "tokens": ["Seid", "be\u00b7sprengt", "mit", "heil'\u00b7gem", "Thau\u00b7e", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Seid gesegnet und geweiht!", "tokens": ["Seid", "ge\u00b7seg\u00b7net", "und", "ge\u00b7weiht", "!"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["VAIMP", "ADJD", "KON", "VVPP", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.12": {"line.1": {"text": "Da\u00df ihr alles Labsal werde,", "tokens": ["Da\u00df", "ihr", "al\u00b7les", "Lab\u00b7sal", "wer\u00b7de", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "PIAT", "NN", "VAFIN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Und Genesen und Gedeihn,", "tokens": ["Und", "Ge\u00b7ne\u00b7sen", "und", "Ge\u00b7deihn", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["KON", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Weih' ich Himmel, Wasser, Erde,", "tokens": ["Weih'", "ich", "Him\u00b7mel", ",", "Was\u00b7ser", ",", "Er\u00b7de", ","], "token_info": ["word", "word", "word", "punct", "word", "punct", "word", "punct"], "pos": ["KOUS", "PPER", "NN", "$,", "NN", "$,", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Lied und Wort und Schlummer ein.", "tokens": ["Lied", "und", "Wort", "und", "Schlum\u00b7mer", "ein", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "KON", "NN", "PTKVZ", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.13": {"line.1": {"text": "Auferwacht und auferstanden", "tokens": ["Au\u00b7fer\u00b7wacht", "und", "auf\u00b7er\u00b7stan\u00b7den"], "token_info": ["word", "word", "word"], "pos": ["NN", "KON", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Leib und Geist in holder Pracht,", "tokens": ["Leib", "und", "Geist", "in", "hol\u00b7der", "Pracht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "APPR", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Aus der Krankheit schweren Binden,", "tokens": ["Aus", "der", "Krank\u00b7heit", "schwe\u00b7ren", "Bin\u00b7den", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Aus des Winters langer Nacht.", "tokens": ["Aus", "des", "Win\u00b7ters", "lan\u00b7ger", "Nacht", "."], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.14": {"line.1": {"text": "Schm\u00fccke dich mit Laub und Bl\u00fcten,", "tokens": ["Schm\u00fc\u00b7cke", "dich", "mit", "Laub", "und", "Bl\u00fc\u00b7ten", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PRF", "APPR", "NN", "KON", "NN", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Mein geliebtes, sch\u00f6nes Thal,", "tokens": ["Mein", "ge\u00b7lieb\u00b7tes", ",", "sch\u00f6\u00b7nes", "Thal", ","], "token_info": ["word", "word", "punct", "word", "word", "punct"], "pos": ["PPOSAT", "ADJA", "$,", "ADJA", "NN", "$,"], "meter": "+-+-+-+", "measure": "trochaic.tetra"}, "line.3": {"text": "Zartes Leben zu beh\u00fcten", "tokens": ["Zar\u00b7tes", "Le\u00b7ben", "zu", "be\u00b7h\u00fc\u00b7ten"], "token_info": ["word", "word", "word", "word"], "pos": ["ADJA", "NN", "PTKZU", "VVINF"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.4": {"text": "Vor zu hei\u00dfem Sonnenstrahl.", "tokens": ["Vor", "zu", "hei\u00b7\u00dfem", "Son\u00b7nen\u00b7strahl", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "APPR", "ADJA", "NN", "$."], "meter": "+-+-+-+", "measure": "trochaic.tetra"}}, "stanza.15": {"line.1": {"text": "Durch den Fr\u00fchling hergetragen,", "tokens": ["Durch", "den", "Fr\u00fch\u00b7ling", "her\u00b7ge\u00b7tra\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "NN", "VVPP", "$,"], "meter": "+-+-+-+-", "measure": "trochaic.tetra"}, "line.2": {"text": "Kommt ein liebes, frommes Kind,", "tokens": ["Kommt", "ein", "lie\u00b7bes", ",", "from\u00b7mes", "Kind", ","], "token_info": ["word", "word", "word", "