{"dta.poem.377": {"metadata": {"author": {"name": "Gressel, Johann Georg", "birth": "N.A.", "death": "N.A."}, "title": "Das angenehme Rauch-Werck.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1716", "urn": "urn:nbn:de:kobv:b4-200905199041", "language": ["de:0.99"], "booktitle": "Celander [i. e. Gressel, Johann Georg]: Verliebte-Galante/ Sinn-Vermischte und Grab-Gedichte. Hamburg u. a., 1716."}, "poem": {"stanza.1": {"line.1": {"text": "Ich wei\u00df nicht/ wie es kommt/ da\u00df man das rauche liebet/", "tokens": ["Ich", "wei\u00df", "nicht", "/", "wie", "es", "kommt", "/", "da\u00df", "man", "das", "rau\u00b7che", "lie\u00b7bet", "/"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PTKNEG", "$(", "PWAV", "PPER", "VVFIN", "$(", "KOUS", "PIS", "ART", "ADJA", "VVFIN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Und in ein Haaren Loch so gern die Glieder schiebet.", "tokens": ["Und", "in", "ein", "Haa\u00b7ren", "Loch", "so", "gern", "die", "Glie\u00b7der", "schie\u00b7bet", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "APPR", "ART", "NN", "NN", "ADV", "ADV", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}