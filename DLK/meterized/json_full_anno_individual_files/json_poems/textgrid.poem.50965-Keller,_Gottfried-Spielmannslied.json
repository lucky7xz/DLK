{"textgrid.poem.50965": {"metadata": {"author": {"name": "Keller, Gottfried", "birth": "N.A.", "death": "N.A."}, "title": "Spielmannslied", "genre": "verse", "period": "N.A.", "pub_year": 1880, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Im Fr\u00fchrot stand der Morgenstern", "tokens": ["Im", "Fr\u00fch\u00b7rot", "stand", "der", "Mor\u00b7gens\u00b7tern"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPRART", "NN", "VVFIN", "ART", "NN"], "meter": "-+-+-+--", "measure": "unknown.measure.tri"}, "line.2": {"text": "Vor einem hellen Fr\u00fchlingstag,", "tokens": ["Vor", "ei\u00b7nem", "hel\u00b7len", "Fr\u00fch\u00b7lings\u00b7tag", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich, ein fl\u00fcchtig Sch\u00fclerkind,", "tokens": ["Als", "ich", ",", "ein", "fl\u00fcch\u00b7tig", "Sch\u00fc\u00b7ler\u00b7kind", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KOUS", "PPER", "$,", "ART", "ADJD", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Im silbergrauen Felde lag;", "tokens": ["Im", "sil\u00b7ber\u00b7grau\u00b7en", "Fel\u00b7de", "lag", ";"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPRART", "ADJA", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Die Wimper schwankte falterhaft,", "tokens": ["Die", "Wim\u00b7per", "schwank\u00b7te", "fal\u00b7ter\u00b7haft", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVFIN", "ADJD", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und ich entschlief an Ackers Rand.", "tokens": ["Und", "ich", "ent\u00b7schlief", "an", "A\u00b7ckers", "Rand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "PPER", "VVFIN", "APPR", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Der S\u00e4mann kam gemach daher", "tokens": ["Der", "S\u00e4\u00b7mann", "kam", "ge\u00b7mach", "da\u00b7her"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ART", "NN", "VVFIN", "ADV", "PAV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und streute K\u00f6rner aus der Hand.", "tokens": ["Und", "streu\u00b7te", "K\u00f6r\u00b7ner", "aus", "der", "Hand", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "NN", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.2": {"line.1": {"text": "Gleich einem F\u00e4cher warf er weit", "tokens": ["Gleich", "ei\u00b7nem", "F\u00e4\u00b7cher", "warf", "er", "weit"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "ART", "NN", "VVFIN", "PPER", "ADJD"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Den Samen hin im halben Rund,", "tokens": ["Den", "Sa\u00b7men", "hin", "im", "hal\u00b7ben", "Rund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "APPRART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ein kleines Tr\u00fcppchen fiel auf mich", "tokens": ["Ein", "klei\u00b7nes", "Tr\u00fcpp\u00b7chen", "fiel", "auf", "mich"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN", "APPR", "PPER"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und traf mir Augen, Stirn und Mund,", "tokens": ["Und", "traf", "mir", "Au\u00b7gen", ",", "Stirn", "und", "Mund", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "PPER", "NN", "$,", "NN", "KON", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Erwachend rafft ich mich empor", "tokens": ["Er\u00b7wa\u00b7chend", "rafft", "ich", "mich", "em\u00b7por"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["VVPP", "VVFIN", "PPER", "PRF", "PTKVZ"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und stand wie ein verbl\u00fcffter Held.", "tokens": ["Und", "stand", "wie", "ein", "ver\u00b7bl\u00fcff\u00b7ter", "Held", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "KOKOM", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Vorschreitend sprach der Bauersmann:", "tokens": ["Vor\u00b7schrei\u00b7tend", "sprach", "der", "Bau\u00b7ers\u00b7mann", ":"], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ADJD", "VVFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "\u00bbwas bist du f\u00fcr ein Ackerfeld?", "tokens": ["\u00bb", "was", "bist", "du", "f\u00fcr", "ein", "A\u00b7cker\u00b7feld", "?"], "token_info": ["punct", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["$(", "PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.3": {"line.1": {"text": "Bist du der steinig harte Grund,", "tokens": ["Bist", "du", "der", "stei\u00b7nig", "har\u00b7te", "Grund", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJD", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Darauf kein S\u00e4mlein wurzeln kann?", "tokens": ["Da\u00b7rauf", "kein", "S\u00e4m\u00b7lein", "wur\u00b7zeln", "kann", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PAV", "PIAT", "NN", "VVINF", "VMFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Bist du ein schlechtes Dorngeb\u00fcsch,", "tokens": ["Bist", "du", "ein", "schlech\u00b7tes", "Dorn\u00b7ge\u00b7b\u00fcsch", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Das keine Halme l\u00e4\u00dft hinan?", "tokens": ["Das", "kei\u00b7ne", "Hal\u00b7me", "l\u00e4\u00dft", "hi\u00b7nan", "?"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "PIAT", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Du bist wohl der gemeine Weg,", "tokens": ["Du", "bist", "wohl", "der", "ge\u00b7mei\u00b7ne", "Weg", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "ADV", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Der wilden V\u00f6gel offner Tisch!", "tokens": ["Der", "wil\u00b7den", "V\u00f6\u00b7gel", "off\u00b7ner", "Tisch", "!"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Bist du nicht dies und bist nicht das,", "tokens": ["Bist", "du", "nicht", "dies", "und", "bist", "nicht", "das", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "PDS", "KON", "VAFIN", "PTKNEG", "PDS", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Am End nicht Vogel und nicht Fisch?\u00ab", "tokens": ["Am", "End", "nicht", "Vo\u00b7gel", "und", "nicht", "Fisch", "?", "\u00ab"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["APPRART", "NN", "PTKNEG", "NE", "KON", "PTKNEG", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.4": {"line.1": {"text": "Unfreundlich schien mir der Gesell", "tokens": ["Un\u00b7freund\u00b7lich", "schien", "mir", "der", "Ge\u00b7sell"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["ADJD", "VVFIN", "PPER", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Und drohend seiner Worte Sinn;", "tokens": ["Und", "dro\u00b7hend", "sei\u00b7ner", "Wor\u00b7te", "Sinn", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJD", "PPOSAT", "NN", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich ging ihm aus den Augen sacht", "tokens": ["Ich", "ging", "ihm", "aus", "den", "Au\u00b7gen", "sacht"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PPER", "APPR", "ART", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und floh behend zur Schule hin.", "tokens": ["Und", "floh", "be\u00b7hend", "zur", "Schu\u00b7le", "hin", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["KON", "VVFIN", "ADJD", "APPRART", "NN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Dort gab der Pfarr den Unterricht", "tokens": ["Dort", "gab", "der", "Pfarr", "den", "Un\u00b7ter\u00b7richt"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "NN", "ART", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Im Bibelbuch zur fr\u00fchen Stund;", "tokens": ["Im", "Bi\u00b7bel\u00b7buch", "zur", "fr\u00fc\u00b7hen", "Stund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "APPRART", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Von Jesu Gleichnis eben sprach", "tokens": ["Von", "Je\u00b7su", "Gleich\u00b7nis", "e\u00b7ben", "sprach"], "token_info": ["word", "word", "word", "word", "word"], "pos": ["APPR", "NE", "NE", "ADV", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Erkl\u00e4rend sein beredter Mund. \u2013", "tokens": ["Er\u00b7kl\u00e4\u00b7rend", "sein", "be\u00b7red\u00b7ter", "Mund", ".", "\u2013"], "token_info": ["word", "word", "word", "word", "punct", "punct"], "pos": ["VVPP", "VAINF", "ADJA", "NN", "$.", "$("], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.5": {"line.1": {"text": "Die Jahre schwanden, und ich zog", "tokens": ["Die", "Jah\u00b7re", "schwan\u00b7den", ",", "und", "ich", "zog"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["ART", "NN", "VVINF", "$,", "KON", "PPER", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Als Zitherspieler durch das Land,", "tokens": ["Als", "Zi\u00b7ther\u00b7spie\u00b7ler", "durch", "das", "Land", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "NN", "APPR", "ART", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Als ich in einer stillen Nacht", "tokens": ["Als", "ich", "in", "ei\u00b7ner", "stil\u00b7len", "Nacht"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["KOUS", "PPER", "APPR", "ART", "ADJA", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Die alte Fabel wiederfand", "tokens": ["Die", "al\u00b7te", "Fa\u00b7bel", "wie\u00b7der\u00b7fand"], "token_info": ["word", "word", "word", "word"], "pos": ["ART", "ADJA", "NN", "VVFIN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Vom S\u00e4mann, der den Samen warf;", "tokens": ["Vom", "S\u00e4\u00b7mann", ",", "der", "den", "Sa\u00b7men", "warf", ";"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["APPRART", "NN", "$,", "PRELS", "ART", "NN", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Da ward mir ein Erinnern licht,", "tokens": ["Da", "ward", "mir", "ein", "E\u00b7rin\u00b7nern", "licht", ","], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VAFIN", "PPER", "ART", "NN", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich sp\u00fcrte jenen K\u00f6rnerwurf", "tokens": ["Ich", "sp\u00fcr\u00b7te", "je\u00b7nen", "K\u00f6r\u00b7ner\u00b7wurf"], "token_info": ["word", "word", "word", "word"], "pos": ["PPER", "VVFIN", "PDAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wie Geisterhand im Angesicht.", "tokens": ["Wie", "Geis\u00b7ter\u00b7hand", "im", "An\u00b7ge\u00b7sicht", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["PWAV", "NN", "APPRART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.6": {"line.1": {"text": "Was bist du f\u00fcr ein Ackerfeld?", "tokens": ["Was", "bist", "du", "f\u00fcr", "ein", "A\u00b7cker\u00b7feld", "?"], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["PWS", "VAFIN", "PPER", "APPR", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "H\u00f6rt wieder ich, als w\u00e4r's ein Traum;", "tokens": ["H\u00f6rt", "wie\u00b7der", "ich", ",", "als", "w\u00e4r's", "ein", "Traum", ";"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVIMP", "ADV", "PPER", "$,", "KOKOM", "VAFIN", "ART", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Ich seufzte, sann und sagte dann:", "tokens": ["Ich", "seufz\u00b7te", ",", "sann", "und", "sag\u00b7te", "dann", ":"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "VVFIN", "KON", "VVFIN", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "O Mann, ich wei\u00df es selber kaum!", "tokens": ["O", "Mann", ",", "ich", "wei\u00df", "es", "sel\u00b7ber", "kaum", "!"], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "NN", "$,", "PPER", "VVFIN", "PPER", "ADV", "ADV", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Ich bin kein Dornbusch und kein Stein", "tokens": ["Ich", "bin", "kein", "Dorn\u00b7busch", "und", "kein", "Stein"], "token_info": ["word", "word", "word", "word", "word", "word", "word"], "pos": ["PPER", "VAFIN", "PIAT", "NN", "KON", "PIAT", "NN"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Und auch kein fetter Weizengrund;", "tokens": ["Und", "auch", "kein", "fet\u00b7ter", "Wei\u00b7zen\u00b7grund", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADV", "PIAT", "ADJA", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "Ich glaub, ich bin der offne Weg,", "tokens": ["Ich", "glaub", ",", "ich", "bin", "der", "off\u00b7ne", "Weg", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "$,", "PPER", "VAFIN", "ART", "ADJA", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Wo's rauscht und fliegt zu jeder Stund.", "tokens": ["Wo's", "rauscht", "und", "fliegt", "zu", "je\u00b7der", "Stund", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NE", "VVFIN", "KON", "VVFIN", "APPR", "PIAT", "NN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}}, "stanza.7": {"line.1": {"text": "Da w\u00e4chst kein Gras, gedeiht kein Korn,", "tokens": ["Da", "w\u00e4chst", "kein", "Gras", ",", "ge\u00b7deiht", "kein", "Korn", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "PIAT", "NN", "$,", "VVFIN", "PIAT", "NN", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.2": {"text": "Statt Furchen ziehn Geleise hin,", "tokens": ["Statt", "Fur\u00b7chen", "ziehn", "Ge\u00b7lei\u00b7se", "hin", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["NN", "NN", "VVFIN", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.3": {"text": "Von harten R\u00e4dern ausgeh\u00f6hlt,", "tokens": ["Von", "har\u00b7ten", "R\u00e4\u00b7dern", "aus\u00b7ge\u00b7h\u00f6hlt", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADJA", "NN", "VVPP", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.4": {"text": "Und nackte F\u00fc\u00dfe wandern drin;", "tokens": ["Und", "nack\u00b7te", "F\u00fc\u00b7\u00dfe", "wan\u00b7dern", "drin", ";"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["KON", "ADJA", "NN", "VVFIN", "PTKVZ", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.5": {"text": "Das kommt und geht, doch f\u00e4llt einmal", "tokens": ["Das", "kommt", "und", "geht", ",", "doch", "f\u00e4llt", "ein\u00b7mal"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word"], "pos": ["PDS", "VVFIN", "KON", "VVFIN", "$,", "ADV", "VVFIN", "ADV"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.6": {"text": "Ein irrend Samenk\u00f6rnlein drauf,", "tokens": ["Ein", "ir\u00b7rend", "Sa\u00b7men\u00b7k\u00f6rn\u00b7lein", "drauf", ","], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "NN", "PTKVZ", "$,"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.7": {"text": "So fliegt ein hungrig V\u00f6glein her", "tokens": ["So", "fliegt", "ein", "hung\u00b7rig", "V\u00f6\u00b7glein", "her"], "token_info": ["word", "word", "word", "word", "word", "word"], "pos": ["ADV", "VVFIN", "ART", "ADJD", "NN", "APZR"], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.8": {"text": "Und schwingt sich mit zum Himmel auf.", "tokens": ["Und", "schwingt", "sich", "mit", "zum", "Him\u00b7mel", "auf", "."], "token_info": ["word", "word", "word", "word", "word", "word", "w