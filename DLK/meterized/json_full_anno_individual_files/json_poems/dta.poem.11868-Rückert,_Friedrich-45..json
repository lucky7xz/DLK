{"dta.poem.11868": {"metadata": {"author": {"name": "R\u00fcckert, Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "45.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1839", "urn": "urn:nbn:de:kobv:b4-200905195119", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "So sprach ein Wandersmann zu seinem Weggesellen,", "tokens": ["So", "sprach", "ein", "Wan\u00b7ders\u00b7mann", "zu", "sei\u00b7nem", "Weg\u00b7ge\u00b7sel\u00b7len", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ADV", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Dem eingebildeten die Augen aufzuhellen:", "tokens": ["Dem", "ein\u00b7ge\u00b7bil\u00b7de\u00b7ten", "die", "Au\u00b7gen", "auf\u00b7zu\u00b7hel\u00b7len", ":"], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "ART", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.2": {"line.1": {"text": "Weil jeder Wandrer tr\u00e4gt die B\u00fcrd' auf seinem R\u00fccken,", "tokens": ["Weil", "je\u00b7der", "Wand\u00b7rer", "tr\u00e4gt", "die", "B\u00fcrd'", "auf", "sei\u00b7nem", "R\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["KOUS", "PIAT", "NN", "VVFIN", "ART", "NN", "APPR", "PPOSAT", "NN", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Siehst du die Uebel leicht, die deinen Vormann dr\u00fccken,", "tokens": ["Siehst", "du", "die", "Ue\u00b7bel", "leicht", ",", "die", "dei\u00b7nen", "Vor\u00b7mann", "dr\u00fc\u00b7cken", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PPER", "ART", "NN", "ADJD", "$,", "PRELS", "PPOSAT", "NN", "VVINF", "$,"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}, "stanza.3": {"line.1": {"text": "Bedenkest aber nicht, da\u00df nach dir andre gehn,", "tokens": ["Be\u00b7den\u00b7kest", "a\u00b7ber", "nicht", ",", "da\u00df", "nach", "dir", "and\u00b7re", "gehn", ","], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "ADV", "PTKNEG", "$,", "KOUS", "APPR", "PPER", "PIS", "VVINF", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Die ebenso die Last auf deinem R\u00fccken sehn.", "tokens": ["Die", "e\u00b7ben\u00b7so", "die", "Last", "auf", "dei\u00b7nem", "R\u00fc\u00b7cken", "sehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADV", "ART", "NN", "APPR", "PPOSAT", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}}}}}