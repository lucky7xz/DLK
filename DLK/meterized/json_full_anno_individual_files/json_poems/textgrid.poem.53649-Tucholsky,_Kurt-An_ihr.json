{"textgrid.poem.53649": {"metadata": {"author": {"name": "Tucholsky, Kurt", "birth": "N.A.", "death": "N.A."}, "title": "An ihr", "genre": "verse", "period": "N.A.", "pub_year": 1912, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Auf deinen gro\u00dfen F\u00fc\u00dfen, Ernestine,", "tokens": ["Auf", "dei\u00b7nen", "gro\u00b7\u00dfen", "F\u00fc\u00b7\u00dfen", ",", "Er\u00b7nes\u00b7ti\u00b7ne", ","], "token_info": ["word", "word", "word", "word", "punct", "word", "punct"], "pos": ["APPR", "PPOSAT", "ADJA", "NN", "$,", "NE", "$,"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.2": {"text": "F\u00fchr ich dich auf den neuen Presseball.", "tokens": ["F\u00fchr", "ich", "dich", "auf", "den", "neu\u00b7en", "Pres\u00b7se\u00b7ball", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "PPER", "PRF", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.3": {"text": "Du tr\u00e4gst Chiffon. Und deine Fragemiene", "tokens": ["Du", "tr\u00e4gst", "Chif\u00b7fon", ".", "Und", "dei\u00b7ne", "Fra\u00b7ge\u00b7mie\u00b7ne"], "token_info": ["word", "word", "word", "punct", "word", "word", "word"], "pos": ["PPER", "VVFIN", "NE", "$.", "KON", "PPOSAT", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.4": {"text": "Ist \u00fcberall.", "tokens": ["Ist", "\u00fc\u00b7be\u00b7rall", "."], "token_info": ["word", "word", "punct"], "pos": ["VAFIN", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.2": {"line.1": {"text": "\u00bbder Legationsrat?\u00ab \u2013 Ja, mein Kind, das ist er!", "tokens": ["\u00bb", "der", "Le\u00b7ga\u00b7ti\u00b7ons\u00b7rat", "?", "\u00ab", "\u2013", "Ja", ",", "mein", "Kind", ",", "das", "ist", "er", "!"], "token_info": ["punct", "word", "word", "punct", "punct", "punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["$(", "ART", "NN", "$.", "$(", "$(", "PTKANT", "$,", "PPOSAT", "NN", "$,", "PDS", "VAFIN", "PPER", "$."], "meter": "+-+-+-+-+-+-", "measure": "trochaic.hexa"}, "line.2": {"text": "\u00bbdu, der da? mit dem goldenen Knopflochdings?", "tokens": ["\u00bb", "du", ",", "der", "da", "?", "mit", "dem", "gol\u00b7de\u00b7nen", "Kno\u00b7pfloch\u00b7dings", "?"], "token_info": ["punct", "word", "punct", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["$(", "PPER", "$,", "PRELS", "ADV", "$.", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+--+-+", "measure": "iambic.penta.relaxed"}, "line.3": {"text": "Und, Theo, wo \u2013 wo tanzt der Herr Minister?\u00ab", "tokens": ["Und", ",", "Theo", ",", "wo", "\u2013", "wo", "tanzt", "der", "Herr", "Mi\u00b7nis\u00b7ter", "?", "\u00ab"], "token_info": ["word", "punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["KON", "$,", "NE", "$,", "PWAV", "$(", "PWAV", "VVFIN", "ART", "NN", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.4": {"text": "Von rechts nach links.", "tokens": ["Von", "rechts", "nach", "links", "."], "token_info": ["word", "word", "word", "word", "punct"], "pos": ["APPR", "ADV", "APPR", "ADV", "$."], "meter": "-+-+", "measure": "iambic.di"}}, "stanza.3": {"line.1": {"text": "\u00bbund, Theo, ist die Presse auch am Platze?\u00ab", "tokens": ["\u00bb", "und", ",", "Theo", ",", "ist", "die", "Pres\u00b7se", "auch", "am", "Plat\u00b7ze", "?", "\u00ab"], "token_info": ["punct", "word", "punct", "word", "punct", "word", "word", "word", "word", "word", "word", "punct", "punct"], "pos": ["$(", "KON", "$,", "NE", "$,", "VAFIN", "ART", "NN", "ADV", "APPRART", "NN", "$.", "$("], "meter": "-+--+-+-+-", "measure": "iambic.tetra.relaxed"}, "line.2": {"text": "Ja, Kind \u2013 der Handelsteil steht am Balkon.", "tokens": ["Ja", ",", "Kind", "\u2013", "der", "Han\u00b7del\u00b7steil", "steht", "am", "Bal\u00b7kon", "."]