{"dta.poem.6700": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Betrachtungen \u00fcber das Geh\u00f6r.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1730", "urn": "urn:nbn:de:kobv:b4-20087-5", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "Das c\u00f6rperliche Hirn ist f\u00e4hig, da\u00df ein Schlag,", "tokens": ["Das", "c\u00f6r\u00b7per\u00b7li\u00b7che", "Hirn", "ist", "f\u00e4\u00b7hig", ",", "da\u00df", "ein", "Schlag", ","], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["ART", "ADJA", "NN", "VAFIN", "ADJD", "$,", "KOUS", "ART", "NN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Ein Sto\u00df, ein Sch\u00fcttern es sehr starck bewegen mag.", "tokens": ["Ein", "Sto\u00df", ",", "ein", "Sch\u00fct\u00b7tern", "es", "sehr", "starck", "be\u00b7we\u00b7gen", "mag."], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "word", "word", "abbreviation"], "pos": ["ART", "NN", "$,", "ART", "NN", "PPER", "ADV", "ADJD", "ADJA", "NN"], "meter": "-+-+-+-+-+-", "measure": "iambic.penta"}, "line.3": {"text": "Durch rasendes Geschrey, durch Keiffen und durch Schm\u00e4hlen", "tokens": ["Durch", "ra\u00b7sen\u00b7des", "Ge\u00b7schrey", ",", "durch", "Keif\u00b7fen", "und", "durch", "Schm\u00e4h\u00b7len"], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "word"], "pos": ["APPR", "ADJA", "NN", "$,", "APPR", "NN", "KON", "APPR", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Erregt man eben das in einer andern Seelen.", "tokens": ["Er\u00b7regt", "man", "e\u00b7ben", "das", "in", "ei\u00b7ner", "an\u00b7dern", "See\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["VVFIN", "PIS", "ADV", "ART", "APPR", "ART", "ADJA", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.5": {"text": "Allein, auch sonder Schall, und sonder hefftigs Klingen", "tokens": ["Al\u00b7lein", ",", "auch", "son\u00b7der", "Schall", ",", "und", "son\u00b7der", "heff\u00b7tigs", "Klin\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "ADV", "ADJA", "NN", "$,", "KON", "ADV", "ADJA", "NN"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Kan offt ein sanftes Wort, ein L\u00e4cheln, wenn der Geist", "tokens": ["Kan", "offt", "ein", "sanf\u00b7tes", "Wort", ",", "ein", "L\u00e4\u00b7cheln", ",", "wenn", "der", "Geist"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "punct", "word", "word", "word"], "pos": ["VMFIN", "ADV", "ART", "ADJA", "NN", "$,", "ART", "NN", "$,", "KOUS", "ART", "NN"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Was sp\u00f6ttisches, was spitzigs drinnen weis\u2019t,", "tokens": ["Was", "sp\u00f6t\u00b7ti\u00b7sches", ",", "was", "spit\u00b7zigs", "drin\u00b7nen", "weis't", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PWS", "ADJA", "$,", "PRELS", "ADV", "ADV", "VVFIN", "$,"], "meter": "-+-+-+-+-+", "measure": "iambic.penta"}, "line.8": {"text": "Ein Hertz zerreissen und durchdringen,", "tokens": ["Ein", "Hertz", "zer\u00b7reis\u00b7sen", "und", "durch\u00b7drin\u00b7gen", ","], "token_info": ["word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "VVPP", "KON", "VVINF", "$,"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.9": {"text": "So, da\u00df dasselbe zu ertragen", "tokens": ["So", ",", "da\u00df", "das\u00b7sel\u00b7be", "zu", "er\u00b7tra\u00b7gen"], "token_info": ["word", "punct", "word", "word", "word", "word"], "pos": ["ADV", "$,", "KOUS", "PDAT", "PTKZU", "VVINF"], "meter": "-+-+-+-+-", "measure": "iambic.tetra"}, "line.10": {"text": "Dem Menschen fast nicht m\u00f6glich f\u00e4llt.", "tokens": ["Dem", "Men\u00b7schen", "fast", "nicht", "m\u00f6g\u00b7lich", "f\u00e4llt", "."], "token_info": ["word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "NN", "ADV", "PTKNEG", "ADJD", "VVFIN", "$."], "meter": "-+-+-+-+", "measure": "iambic.tetra"}, "line.11": {"text": "Dies zeiget, da\u00df der Geist, der sich geschimpffet h\u00e4lt,", "tokens": ["Dies", "zei\u00b7get", ",", "da\u00df", "der", "Geist", ",", "der", "sich", "ge\u00b7schimpf\u00b7fet", "h\u00e4lt", ","], "token_info": ["word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PDS", "VVFIN", "$,", "KOUS", "ART", "NN", "$,", "PRELS", "PRF", "VVPP", "VVFIN", "$,"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Allein empfindlich sey, da\u00df er allein geschlagen.", "tokens": ["Al\u00b7lein", "emp\u00b7find\u00b7lich", "sey", ",", "da\u00df", "er", "al\u00b7lein", "ge\u00b7schla\u00b7gen", "."], "token_info": ["word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["ADV", "ADJD", "VAFIN", "$,", "KOUS", "PPER", "ADV", "VVPP", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}}}}}