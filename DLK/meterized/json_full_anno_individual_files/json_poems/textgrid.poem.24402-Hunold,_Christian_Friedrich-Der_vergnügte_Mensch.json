{"textgrid.poem.24402": {"metadata": {"author": {"name": "Hunold, Christian Friedrich", "birth": "N.A.", "death": "N.A."}, "title": "Der vergn\u00fcgte Mensch", "genre": "verse", "period": "N.A.", "pub_year": 1701, "urn": "N.A.", "language": ["de:0.99"], "booktitle": "N.A."}, "text": null, "poem": {"stanza.1": {"line.1": {"text": "Ich bin in mir vergn\u00fcgt. Ein ander mache Grillen/", "tokens": ["Ich", "bin", "in", "mir", "ver\u00b7gn\u00fcgt", ".", "Ein", "an\u00b7der", "ma\u00b7che", "Gril\u00b7len", "/"], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "APPR", "PPER", "VVPP", "$.", "ART", "ADJD", "VVFIN", "NN", "$("], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.2": {"text": "Er wird damit doch nicht den Sack noch Magen f\u00fcllen.", "tokens": ["Er", "wird", "da\u00b7mit", "doch", "nicht", "den", "Sack", "noch", "Ma\u00b7gen", "f\u00fcl\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VAFIN", "PAV", "ADV", "PTKNEG", "ART", "NN", "ADV", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.3": {"text": "Bin ich nicht reich und gro\u00df/ und klein von Herrlichkeit/", "tokens": ["Bin", "ich", "nicht", "reich", "und", "gro\u00df", "/", "und", "klein", "von", "Herr\u00b7lich\u00b7keit", "/"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "punct"], "pos": ["VAFIN", "PPER", "PTKNEG", "ADJD", "KON", "ADJD", "$(", "KON", "ADJD", "APPR", "NN", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.4": {"text": "Macht doch zufrieden seyn in mir gew\u00fcnschte Zeit.", "tokens": ["Macht", "doch", "zu\u00b7frie\u00b7den", "seyn", "in", "mir", "ge\u00b7w\u00fcnschte", "Zeit", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["NN", "ADV", "ADJD", "VAINF", "APPR", "PPER", "ADJA", "NN", "$."], "meter": "+--+-+--+-+", "measure": "iambic.penta.invert"}, "line.5": {"text": "Ich r\u00fchme nichts von mir. Ein Narr r\u00fchrt seine Schellen.", "tokens": ["Ich", "r\u00fch\u00b7me", "nichts", "von", "mir", ".", "Ein", "Narr", "r\u00fchrt", "sei\u00b7ne", "Schel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PIS", "APPR", "PPER", "$.", "ART", "NN", "VVFIN", "PPOSAT", "NN", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.6": {"text": "Ich bleibe still vor mich. Verzagte Hunde bellen.", "tokens": ["Ich", "blei\u00b7be", "still", "vor", "mich", ".", "Ver\u00b7zag\u00b7te", "Hun\u00b7de", "bel\u00b7len", "."], "token_info": ["word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "ADJD", "APPR", "PPER", "$.", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.7": {"text": "Ich warte meines Thuns/ und la\u00df auf Rosen gehn/", "tokens": ["Ich", "war\u00b7te", "mei\u00b7nes", "Thuns", "/", "und", "la\u00df", "auf", "Ro\u00b7sen", "gehn", "/"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "word", "word", "punct"], "pos": ["PPER", "VVFIN", "PPOSAT", "NN", "$(", "KON", "VVIMP", "APPR", "NN", "VVINF", "$("], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.8": {"text": "Die m\u00fc\u00dfig und dabey im gro\u00dfen Gl\u00fccke stehn.", "tokens": ["Die", "m\u00fc\u00b7\u00dfig", "und", "da\u00b7bey", "im", "gro\u00b7\u00dfen", "Gl\u00fc\u00b7cke", "stehn", "."], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "punct"], "pos": ["ART", "ADJD", "KON", "PAV", "APPRART", "ADJA", "NN", "VVINF", "$."], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.9": 