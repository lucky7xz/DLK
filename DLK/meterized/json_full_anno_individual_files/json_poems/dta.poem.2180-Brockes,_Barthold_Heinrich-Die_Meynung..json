{"dta.poem.2180": {"metadata": {"author": {"name": "Brockes, Barthold Heinrich", "birth": "N.A.", "death": "N.A."}, "title": "Die Meynung.", "genre": "Lyrik", "period": "N.A.", "pub_year": "1748", "urn": "urn:nbn:de:kobv:b4-200905198553", "language": ["de:0.99"], "booktitle": "N.A."}, "poem": {"stanza.1": {"line.1": {"text": "T. hat zu einer Zeit gemeynet, da viele ihrer Mey-", "tokens": ["T.", "hat", "zu", "ei\u00b7ner", "Zeit", "ge\u00b7mey\u00b7net", ",", "da", "vie\u00b7le", "ih\u00b7rer", "Mey"], "token_info": ["abbreviation", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["NE", "VAFIN", "APPR", "ART", "NN", "VVPP", "$,", "KOUS", "PIS", "PPOSAT", "TRUNC"], "meter": "+-+-+-+--+-+-+", "measure": "trochaic.septa.relaxed"}, "line.2": {"text": "nung m\u00fcde.", "tokens": ["nung", "m\u00fc\u00b7de", "."], "token_info": ["word", "word", "punct"], "pos": ["ADV", "ADJD", "$."], "meter": "-+-", "measure": "amphibrach.single"}, "line.3": {"text": "Hierdurch ward ", "tokens": ["Hier\u00b7durch", "ward"], "token_info": ["word", "word"], "pos": ["PAV", "VAFIN"], "meter": "+-+", "measure": "trochaic.di"}, "line.4": {"text": "Ruh und Friede.", "tokens": ["Ruh", "und", "Frie\u00b7de", "."], "token_info": ["word", "word", "word", "punct"], "pos": ["NN", "KON", "NN", "$."], "meter": "+-+-", "measure": "trochaic.di"}, "line.5": {"text": "Es w\u00e4r\u2019 ihm aber ganz gewi\u00df, was er gesucht, nicht an-", "tokens": ["Es", "w\u00e4r'", "ihm", "a\u00b7ber", "ganz", "ge\u00b7wi\u00df", ",", "was", "er", "ge\u00b7sucht", ",", "nicht", "an"], "token_info": ["word", "word", "word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word"], "pos": ["PPER", "VAFIN", "PPER", "ADV", "ADV", "ADV", "$,", "PWS", "PPER", "VVPP", "$,", "PTKNEG", "TRUNC"], "meter": "-+-+-+-+-+-+-+", "measure": "iambic.septa"}, "line.6": {"text": "gegangen,", "tokens": ["ge\u00b7gan\u00b7gen", ","], "token_info": ["word", "punct"], "pos": ["VVPP", "$,"], "meter": "-+-", "measure": "amphibrach.single"}, "line.7": {"text": "H\u00e4tt\u2019 er nur wenig Jahre fr\u00fcher also zu meynen ange-", "tokens": ["H\u00e4tt'", "er", "nur", "we\u00b7nig", "Jah\u00b7re", "fr\u00fc\u00b7her", "al\u00b7so", "zu", "mey\u00b7nen", "an\u00b7ge"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["VVFIN", "PPER", "ADV", "PIAT", "NN", "ADJD", "ADV", "APPR", "PPOSAT", "TRUNC"], "meter": "-+-+-+-+-+--+-+-", "measure": "iambic.septa.relaxed"}, "line.8": {"text": "fangen;", "tokens": ["fan\u00b7gen", ";"], "token_info": ["word", "punct"], "pos": ["VVFIN", "$."], "meter": "+-", "measure": "trochaic.single"}, "line.9": {"text": "Die\u00df giebt uns nun im rechten Ernst die gro\u00dfe Wahr-", "tokens": ["Die\u00df", "giebt", "uns", "nun", "im", "rech\u00b7ten", "Ernst", "die", "gro\u00b7\u00dfe", "Wahr"], "token_info": ["word", "word", "word", "word", "word", "word", "word", "word", "word", "word"], "pos": ["PDS", "VVFIN", "PPER", "ADV", "APPRART", "ADJA", "NN", "ART", "ADJA", "TRUNC"], "meter": "-+-+-+-+-+-+", "measure": "alexandrine.iambic.hexa"}, "line.10": {"text": "heit zu erw\u00e4gen,", "tokens": ["heit", "zu", "er\u00b7w\u00e4\u00b7gen", ","], "token_info": ["word", "word", "word", "punct"], "pos": ["ADJD", "PTKZU", "VVINF", "$,"], "meter": "-+-+-", "measure": "iambic.di"}, "line.11": {"text": "Wie viel am Umstand, an der Zeit, in allem auf der", "tokens": ["Wie", "viel", "am", "Um\u00b7stand", ",", "an", "der", "Zeit", ",", "in", "al\u00b7lem", "auf", "der"], "token_info": ["word", "word", "word", "word", "punct", "word", "word", "word", "punct", "word", "word", "word", "word"], "pos": ["PWAV", "ADV", "APPRART", "NN", "$,", "APPR", "ART", "NN", "$,", "APPR", "PIS", "APPR", "ART"], "meter": "-+-+-+-+-+-+-", "measure": "alexandrine.iambic.hexa"}, "line.12": {"text": "Welt gelegen.", "tokens": ["Welt", "ge\u00b7le\u00b7gen", "."], "token_info": ["word", "word", "punct"], "pos": ["NN", "VVPP", "$."], "meter": "+-+-", "measure": "trochaic.di"}}}}}